{"textgrid.poem.51243": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: In dem Winkel einer Schenke sa\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem Winkel einer Schenke sa\u00dfen", "tokens": ["In", "dem", "Win\u00b7kel", "ei\u00b7ner", "Schen\u00b7ke", "sa\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einstmals J\u00e4ger nach vollbrachtem Jagen.", "tokens": ["Einst\u00b7mals", "J\u00e4\u00b7ger", "nach", "voll\u00b7brach\u00b7tem", "Ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sie erz\u00e4hlten sich die feinen K\u00fcnste,", "tokens": ["Sie", "er\u00b7z\u00e4hl\u00b7ten", "sich", "die", "fei\u00b7nen", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie des Wildes Heimlichkeit zu sehen,", "tokens": ["Wie", "des", "Wil\u00b7des", "Heim\u00b7lich\u00b7keit", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Alle Kreatur sei zu beschleichen.", "tokens": ["Al\u00b7le", "Kre\u00b7a\u00b7tur", "sei", "zu", "be\u00b7schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Als sie nun nicht ihrem Witz alleine,", "tokens": ["Als", "sie", "nun", "nicht", "ih\u00b7rem", "Witz", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Sondern auch dem Gl\u00fcck erkenntlich waren,", "tokens": ["Son\u00b7dern", "auch", "dem", "Gl\u00fcck", "er\u00b7kennt\u00b7lich", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Griff ein alter Schlingel nach dem Faden", "tokens": ["Griff", "ein", "al\u00b7ter", "Schlin\u00b7gel", "nach", "dem", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Des Gespr\u00e4chs und zog ihn an sich, gleich der", "tokens": ["Des", "Ge\u00b7spr\u00e4chs", "und", "zog", "ihn", "an", "sich", ",", "gleich", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN", "PPER", "APPR", "PRF", "$,", "ADV", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Schnur, mit der ein Netz man zuzieht.", "tokens": ["Schnur", ",", "mit", "der", "ein", "Netz", "man", "zu\u00b7zieht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRELS", "ART", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Ein erlebtes Jugendabenteuer", "tokens": ["Ein", "er\u00b7leb\u00b7tes", "Ju\u00b7gen\u00b7da\u00b7bent\u00b7eu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Bracht er vor mit schlauen Blinzel\u00e4uglein,", "tokens": ["Bracht", "er", "vor", "mit", "schlau\u00b7en", "Blin\u00b7zel\u00b7\u00e4ug\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Da\u00df die K\u00f6pfe sie zusammensteckten", "tokens": ["Da\u00df", "die", "K\u00f6p\u00b7fe", "sie", "zu\u00b7sam\u00b7men\u00b7steck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Und die Pfeifen bald erkalten lie\u00dfen:", "tokens": ["Und", "die", "Pfei\u00b7fen", "bald", "er\u00b7kal\u00b7ten", "lie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbwohl, ich sa\u00df im hohen Eschenbaume,", "tokens": ["\u00bb", "wohl", ",", "ich", "sa\u00df", "im", "ho\u00b7hen", "E\u00b7schen\u00b7bau\u00b7me", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In dem Kronenbusche still verborgen;", "tokens": ["In", "dem", "Kro\u00b7nen\u00b7bu\u00b7sche", "still", "ver\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unterm Baume lag ein sch\u00f6nes Weibchen", "tokens": ["Un\u00b7term", "Bau\u00b7me", "lag", "ein", "sch\u00f6\u00b7nes", "Weib\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf dem sonnbegl\u00e4nzten Sand im Bade.", "tokens": ["Auf", "dem", "sonn\u00b7be\u00b7gl\u00e4nz\u00b7ten", "Sand", "im", "Ba\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Auf dem R\u00fccken lag sie unbeweglich,", "tokens": ["Auf", "dem", "R\u00fc\u00b7cken", "lag", "sie", "un\u00b7be\u00b7weg\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mit dem K\u00f6pfchen auf dem warmen Ufer,", "tokens": ["Mit", "dem", "K\u00f6pf\u00b7chen", "auf", "dem", "war\u00b7men", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ihre Arme reglos drum geschlungen.", "tokens": ["Ih\u00b7re", "Ar\u00b7me", "reg\u00b7los", "drum", "ge\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PAV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Doch die kleinen F\u00fc\u00dfe, sie verschwanden", "tokens": ["Doch", "die", "klei\u00b7nen", "F\u00fc\u00b7\u00dfe", ",", "sie", "ver\u00b7schwan\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "In dem blauen Purpur des Gew\u00e4ssers;", "tokens": ["In", "dem", "blau\u00b7en", "Pur\u00b7pur", "des", "Ge\u00b7w\u00e4s\u00b7sers", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Aber sichtbar wurde schon das Leuchten", "tokens": ["A\u00b7ber", "sicht\u00b7bar", "wur\u00b7de", "schon", "das", "Leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Ihrer Knie durch das bewegte Wasser,", "tokens": ["Ih\u00b7rer", "Knie", "durch", "das", "be\u00b7weg\u00b7te", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Und wie Glas auf ihrem wei\u00dfen Scho\u00dfe", "tokens": ["Und", "wie", "Glas", "auf", "ih\u00b7rem", "wei\u00b7\u00dfen", "Scho\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Unabl\u00e4ssig flo\u00df die Welle weiter,", "tokens": ["Un\u00b7ab\u00b7l\u00e4s\u00b7sig", "flo\u00df", "die", "Wel\u00b7le", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Und die Silberfischchen schwammen ruhig", "tokens": ["Und", "die", "Sil\u00b7ber\u00b7fi\u00b7schchen", "schwam\u00b7men", "ru\u00b7hig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "\u00dcber ihre H\u00fcften hin, erblinkend,", "tokens": ["\u00dc\u00b7ber", "ih\u00b7re", "H\u00fcf\u00b7ten", "hin", ",", "er\u00b7blin\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Wenn sie steuernd ihre Flossen regten.", "tokens": ["Wenn", "sie", "steu\u00b7ernd", "ih\u00b7re", "Flos\u00b7sen", "reg\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Auf des Stromes hellbegl\u00e4nzte Breite", "tokens": ["Auf", "des", "Stro\u00b7mes", "hell\u00b7be\u00b7gl\u00e4nz\u00b7te", "Brei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Sah die Sch\u00f6ne mit halboffnen Augen.", "tokens": ["Sah", "die", "Sch\u00f6\u00b7ne", "mit", "hal\u00b7boff\u00b7nen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Kahl und einsam lag das andre Ufer,", "tokens": ["Kahl", "und", "ein\u00b7sam", "lag", "das", "and\u00b7re", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Nicht ein menschlich Wesen zu ersp\u00e4hen.", "tokens": ["Nicht", "ein", "menschlich", "We\u00b7sen", "zu", "er\u00b7sp\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch auf einmal kam ein Schiff gefahren", "tokens": ["Doch", "auf", "ein\u00b7mal", "kam", "ein", "Schiff", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "VVFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mitten auf des Stromes heitrem Glanze;", "tokens": ["Mit\u00b7ten", "auf", "des", "Stro\u00b7mes", "heit\u00b7rem", "Glan\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und ich sah das Schiff und sah die Sch\u00f6ne.", "tokens": ["Und", "ich", "sah", "das", "Schiff", "und", "sah", "die", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sachte, sachte schlo\u00df sie beide Augen,", "tokens": ["Sach\u00b7te", ",", "sach\u00b7te", "schlo\u00df", "sie", "bei\u00b7de", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nicht sich regend, bis das Schiff vor\u00fcber.", "tokens": ["Nicht", "sich", "re\u00b7gend", ",", "bis", "das", "Schiff", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PRF", "ADJD", "$,", "KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die Schiffer fuhren in die Ferne,", "tokens": ["Und", "die", "Schif\u00b7fer", "fuh\u00b7ren", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Nur nach ihrem Ziel den Sinn gewendet. \u2013", "tokens": ["Nur", "nach", "ih\u00b7rem", "Ziel", "den", "Sinn", "ge\u00b7wen\u00b7det", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Triumphierend l\u00e4chelte die Holde;", "tokens": ["Tri\u00b7um\u00b7phie\u00b7rend", "l\u00e4\u00b7chel\u00b7te", "die", "Hol\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denn das \u00c4u\u00dferste zu wagen und ihm", "tokens": ["Denn", "das", "\u00c4u\u00b7\u00dfers\u00b7te", "zu", "wa\u00b7gen", "und", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "KON", "PPER"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Zu entgehen lieben oft die Frauen.", "tokens": ["Zu", "ent\u00b7ge\u00b7hen", "lie\u00b7ben", "oft", "die", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Doch sie ahnte nicht, da\u00df ihr zu H\u00e4upten", "tokens": ["Doch", "sie", "ahn\u00b7te", "nicht", ",", "da\u00df", "ihr", "zu", "H\u00e4up\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sie belauscht' ein arger Entenj\u00e4ger,", "tokens": ["Sie", "be\u00b7lauscht'", "ein", "ar\u00b7ger", "En\u00b7ten\u00b7j\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Den das Gl\u00fcck auf jenen Baum getrieben;", "tokens": ["Den", "das", "Gl\u00fcck", "auf", "je\u00b7nen", "Baum", "ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und ich mu\u00dfte mich zusammenfassen,", "tokens": ["Und", "ich", "mu\u00df\u00b7te", "mich", "zu\u00b7sam\u00b7men\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Nicht wie reife Frucht vom Baum zu fallen,", "tokens": ["Nicht", "wie", "rei\u00b7fe", "Frucht", "vom", "Baum", "zu", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "W\u00e4hrend ich in meinem Sinn erdauert',", "tokens": ["W\u00e4h\u00b7rend", "ich", "in", "mei\u00b7nem", "Sinn", "er\u00b7dau\u00b7ert'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Was zum Heil der Sch\u00f6nen zu beginnen?", "tokens": ["Was", "zum", "Heil", "der", "S