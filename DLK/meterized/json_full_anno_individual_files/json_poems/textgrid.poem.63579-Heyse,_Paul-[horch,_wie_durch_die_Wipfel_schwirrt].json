{"textgrid.poem.63579": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[horch, wie durch die Wipfel schwirrt]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch, wie durch die Wipfel schwirrt", "tokens": ["Horch", ",", "wie", "durch", "die", "Wip\u00b7fel", "schwirrt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausendstimmiger Vogelsang!", "tokens": ["Tau\u00b7sends\u00b7tim\u00b7mi\u00b7ger", "Vo\u00b7gel\u00b7sang", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Was da nur geplaudert wird,", "tokens": ["Was", "da", "nur", "ge\u00b7plau\u00b7dert", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer d\u00fcnkt die Zeit dir lang.", "tokens": ["Nim\u00b7mer", "d\u00fcnkt", "die", "Zeit", "dir", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie wenn nachts die Liebste spricht,", "tokens": ["Wie", "wenn", "nachts", "die", "Liebs\u00b7te", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4umend noch in Schlummers Hut.", "tokens": ["Tr\u00e4u\u00b7mend", "noch", "in", "Schlum\u00b7mers", "Hut", "."], "token_info": ["word", "word", "word", "word", "wo