{"dta.poem.3748": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Was haben wir dann zu essen ?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Guten Abend Aennele,               ", "tokens": ["Gu\u00b7ten", "A\u00b7bend", "A\u00b7en\u00b7ne\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Zu essen h\u00e4ben wir wenele,", "tokens": ["Zu", "es\u00b7sen", "h\u00e4\u00b7ben", "wir", "we\u00b7ne\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zu trinken h\u00e4ben wir unsern Bach,", "tokens": ["Zu", "trin\u00b7ken", "h\u00e4\u00b7ben", "wir", "un\u00b7sern", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "H\u00e4ben wir nit die beste Sach.", "tokens": ["H\u00e4\u00b7ben", "wir", "nit", "die", "bes\u00b7te", "Sach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}