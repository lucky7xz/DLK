{"textgrid.poem.53349": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: War dieses nicht mein Hoffen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War dieses nicht mein Hoffen", "tokens": ["War", "die\u00b7ses", "nicht", "mein", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd meines Glaubens St\u00fctz,", "tokens": ["Vnd", "mei\u00b7nes", "Glau\u00b7bens", "St\u00fctz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So offt mich h\u00e4tt betroffen", "tokens": ["So", "offt", "mich", "h\u00e4tt", "be\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die schwere Creutzes Hitz,", "tokens": ["Die", "schwe\u00b7re", "Creut\u00b7zes", "Hitz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das der Herr doch endlich w\u00fcrd", "tokens": ["Das", "der", "Herr", "doch", "end\u00b7lich", "w\u00fcrd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "ADV", "ADV", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich von allem B\u00f6sen,", "tokens": ["Mich", "von", "al\u00b7lem", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIS", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Von der Creutz vnd S\u00fcnden B\u0