{"textgrid.poem.35354": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schaukelt, wenn Wogen sich auf Wogen t\u00fcrmen,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaukelt, wenn Wogen sich auf Wogen t\u00fcrmen,", "tokens": ["Schau\u00b7kelt", ",", "wenn", "Wo\u00b7gen", "sich", "auf", "Wo\u00b7gen", "t\u00fcr\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "NN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Euch auf der See und nicht auf Gartenteichen;", "tokens": ["Euch", "auf", "der", "See", "und", "nicht", "auf", "Gar\u00b7ten\u00b7tei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "KON", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Typhone la\u00dft durch eure Haare streichen", "tokens": ["Ty\u00b7pho\u00b7ne", "la\u00dft", "durch", "eu\u00b7re", "Haa\u00b7re", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und k\u00fchlt die hei\u00dfe Brust an Meeresst\u00fcrmen!", "tokens": ["Und", "k\u00fchlt", "die", "hei\u00b7\u00dfe", "Brust", "an", "Mee\u00b7res\u00b7st\u00fcr\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es dringe bis in eurer Herzen Mark", "tokens": ["Es", "drin\u00b7ge", "bis", "in", "eu\u