{"textgrid.poem.24788": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eh er geb\u00e4ndigt war f\u00fcr sein Geschick,", "tokens": ["Eh", "er", "ge\u00b7b\u00e4n\u00b7digt", "war", "f\u00fcr", "sein", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Trank er viel Flut, die bitter war und schwer.", "tokens": ["Trank", "er", "viel", "Flut", ",", "die", "bit\u00b7ter", "war", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,", "PRELS", "ADJD", "VAFIN", "KON", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dann richtete er sonderbar sich auf", "tokens": ["Dann", "rich\u00b7te\u00b7te", "er", "son\u00b7der\u00b7bar", "sich", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PRF", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und stand am Ufer, seltsam leicht und leer.", "tokens": ["Und", "stand", "am", "U\u00b7fer", ",", "selt\u00b7sam", "leicht", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu seinen F\u00fc\u00dfen rollten Muscheln hin,", "tokens": ["Zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "roll\u00b7ten", "Mu\u00b7scheln", "hin", 