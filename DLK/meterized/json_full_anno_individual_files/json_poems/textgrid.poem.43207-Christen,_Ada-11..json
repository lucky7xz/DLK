{"textgrid.poem.43207": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Dogen starren aus den alten Rahmen,", "tokens": ["Die", "Do\u00b7gen", "star\u00b7ren", "aus", "den", "al\u00b7ten", "Rah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Grausame, wilde, traurige Gesichter,", "tokens": ["Grau\u00b7sa\u00b7me", ",", "wil\u00b7de", ",", "trau\u00b7ri\u00b7ge", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der Hermelin schm\u00fcckt diese ernsten Richter", "tokens": ["Der", "Her\u00b7me\u00b7lin", "schm\u00fcckt", "die\u00b7se", "erns\u00b7ten", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und stolze, gro\u00dfe, halbvergess'ne Namen.", "tokens": ["Und", "stol\u00b7ze", ",", "gro\u00b7\u00dfe", ",", "halb\u00b7ver\u00b7ge\u00b7ss'\u00b7ne", "Na\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie todte Zahlen, ohne sich zu gleichen,", "tokens": ["Wie", "tod\u00b7te", "Zah\u00b7len", ",", "oh\u00b7ne", "sich", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word",