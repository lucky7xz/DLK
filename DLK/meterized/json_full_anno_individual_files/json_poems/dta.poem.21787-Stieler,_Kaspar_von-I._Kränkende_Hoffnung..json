{"dta.poem.21787": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  Kr\u00e4nkende Hoffnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Was hilfft es uns/ da\u00df wir uns lieben/", "tokens": ["Was", "hilfft", "es", "uns", "/", "da\u00df", "wir", "uns", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$(", "KOUS", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rosille/ Sch\u00f6ne! sag es mir?", "tokens": ["Ro\u00b7sil\u00b7le", "/", "Sch\u00f6\u00b7ne", "!", "sag", "es", "mir", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$.", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df wir ein stetes seuffzen \u00fcben/", "tokens": ["da\u00df", "wir", "ein", "ste\u00b7tes", "seuff\u00b7zen", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und Schmerzen tragen f\u00fcr und f\u00fcr.", "tokens": ["und", "Schmer\u00b7zen", "tra\u00b7gen", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach Schmerzen! denen keine Wunden/", "tokens": ["Ach", "Schmer\u00b7zen", "!", "de\u00b7nen", "kei\u00b7ne", "Wun\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PRELS", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wie t\u00f6dtlich sie sind/ gleich sich funden.", "tokens": ["wie", "t\u00f6dt\u00b7lich", "sie", "sind", "/", "gleich", "sich", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$(", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So stark kan keine Wunde bluten", "tokens": ["So", "stark", "kan", "kei\u00b7ne", "Wun\u00b7de", "blu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "rizzt sie die Lebens-adern gleich/", "tokens": ["rizzt", "sie", "die", "Le\u00b7bens\u00b7a\u00b7dern", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df nicht ein Heil sey zu vermuhten.", "tokens": ["da\u00df", "nicht", "ein", "Heil", "sey", "zu", "ver\u00b7muh\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Garten ist ja noch so reich", "tokens": ["Der", "Gar\u00b7ten", "ist", "ja", "noch", "so", "reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ein edles Bl\u00fcmchen dar zustellen", "tokens": ["ein", "ed\u00b7les", "Bl\u00fcm\u00b7chen", "dar", "zu\u00b7stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "zu stopffen ihres Schweisses qwellen.", "tokens": ["zu", "stopf\u00b7fen", "ih\u00b7res", "Schweis\u00b7ses", "qwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer aber hilfft der kranken Seele", "tokens": ["Wer", "a\u00b7ber", "hilfft", "der", "kran\u00b7ken", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die bi\u00df auffs Leben steht versehrt?", "tokens": ["die", "bi\u00df", "auffs", "Le\u00b7ben", "steht", "ver\u00b7sehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "APPRART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wund\u2019 ob welcher ich mich queele", "tokens": ["Der", "Wund'", "ob", "wel\u00b7cher", "ich", "mich", "que\u00b7e\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOUS", "PRELS", "PPER", "PRF", "NE"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "wird aller Heilung Krafft verwehrt.", "tokens": ["wird", "al\u00b7ler", "Hei\u00b7lung", "Krafft", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist es/ Tod/ der mich entbindet", "tokens": ["Du", "bist", "es", "/", "Tod", "/", "der", "mich", "ent\u00b7bin\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$(", "NN", "$(", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "da\u00df/ worf\u00fcr man nicht Kr\u00e4uter findet.", "tokens": ["da\u00df", "/", "wor\u00b7f\u00fcr", "man", "nicht", "Kr\u00e4u\u00b7ter", "fin\u00b7det", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWAV", "PIS", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwar/ Zeit/ du willst mir was verheissen/", "tokens": ["Zwar", "/", "Zeit", "/", "du", "willst", "mir", "was", "ver\u00b7heis\u00b7sen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NN", "$(", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "das aber ist zu schlecht f\u00fcr mich.", "tokens": ["das", "a\u00b7ber", "ist", "zu", "schlecht", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "PTKA", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du pflegest alles hinzureissen/", "tokens": ["Du", "pfle\u00b7gest", "al\u00b7les", "hin\u00b7zu\u00b7reis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "liebst Wankelmuht. Ja wenn ich dich", "tokens": ["liebst", "Wan\u00b7kel\u00b7muht", ".", "Ja", "wenn", "ich", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$.", "PTKANT", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und deinen Flug in einer Kette", "tokens": ["und", "dei\u00b7nen", "Flug", "in", "ei\u00b7ner", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "beschlossen und umf\u00e4sselt h\u00e4tte.", "tokens": ["be\u00b7schlos\u00b7sen", "und", "um\u00b7f\u00e4s\u00b7selt", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich wolte deine F\u00f6rder-Haare", "tokens": ["Ich", "wol\u00b7te", "dei\u00b7ne", "F\u00f6r\u00b7der\u00b7Haa\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "nicht au\u00df den H\u00e4nden lassen gehn/", "tokens": ["nicht", "au\u00df", "den", "H\u00e4n\u00b7den", "las\u00b7sen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "als bi\u00df du mir so viel der Jahre", "tokens": ["als", "bi\u00df", "du", "mir", "so", "viel", "der", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "von dem Verhengn\u00fc\u00df lie\u00dft entstehn/", "tokens": ["von", "dem", "Ver\u00b7hen\u00b7gn\u00fc\u00df", "lie\u00dft", "ent\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "VVINF", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "da\u00df die Vergn\u00fcgung meiner Sinnen", "tokens": ["da\u00df", "die", "Ver\u00b7gn\u00fc\u00b7gung", "mei\u00b7ner", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "m\u00f6cht\u2019 ihren s\u00fcssen Zwekk gewinnen.", "tokens": ["m\u00f6cht'", "ih\u00b7ren", "s\u00fcs\u00b7sen", "Zwekk", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nu bistu fl\u00fcchtig/ falsch und wilde/", "tokens": ["Nu", "bis\u00b7tu", "fl\u00fcch\u00b7tig", "/", "falsch", "und", "wil\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$(", "ADJD", "KON", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "doch w\u00e4restu nur fl\u00fcchtig satt:", "tokens": ["doch", "w\u00e4\u00b7res\u00b7tu", "nur", "fl\u00fcch\u00b7tig", "satt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wie bald w\u00e4r\u2019 ach! die Wunde milde/", "tokens": ["wie", "bald", "w\u00e4r'", "ach", "!", "die", "Wun\u00b7de", "mil\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ITJ", "$.", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die mir das Leben machet matt.", "tokens": ["die", "mir", "das", "Le\u00b7ben", "ma\u00b7chet", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es w\u00fcrde noch durch etwas hoffen", "tokens": ["Es", "w\u00fcr\u00b7de", "noch", "durch", "et\u00b7was", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die Lindrung meiner Qwaal getroffen.", "tokens": ["die", "Lind\u00b7rung", "mei\u00b7ner", "Qwaal", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Verblutet euch ihr grimme Schmerzen/", "tokens": ["Ver\u00b7blu\u00b7tet", "euch", "ihr", "grim\u00b7me", "Schmer\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "verblutet Geist und Leben au\u00df.", "tokens": ["ver\u00b7blu\u00b7tet", "Geist", "und", "Le\u00b7ben", "au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gebt Sto\u00df um Sto\u00df dem treuen Herzen", "tokens": ["Gebt", "Sto\u00df", "um", "Sto\u00df", "dem", "treu\u00b7en", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "verlasst des Leibs geplagtes Hau\u00df.", "tokens": ["ver\u00b7lasst", "des", "Leibs", "ge\u00b7plag\u00b7tes", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Seele weich! es ist vergebens", "tokens": ["O", "See\u00b7le", "weich", "!", "es", "ist", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "ADJD", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ich heile nicht Zeit meines Lebens.", "tokens": ["ich", "hei\u00b7le", "nicht", "Zeit", "mei\u00b7nes", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}