{"dta.poem.3560": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Kirschen sind zeitig,               ", "tokens": ["Die", "Kir\u00b7schen", "sind", "zei\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Weichseln sind braun;", "tokens": ["Die", "Weich\u00b7seln", "sind", "braun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hat jede einen Buben,", "tokens": ["Hat", "je\u00b7de", "ei\u00b7nen", "Bu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df auch um einen scha", "tokens": ["Mu\u00df", "auch", "um", "ei\u00b7nen", "scha"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du sch\u00f6ner Kuckuk,", "tokens": ["Du", "sch\u00f6\u00b7ner", "Kuc\u00b7kuk", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wo singest denn du;", "tokens": ["Wo", "sin\u00b7gest", "denn", "du", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Du singest im Walde,", "tokens": ["Du", "sin\u00b7gest", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Verf\u00fchrest mich balde.", "tokens": ["Ver\u00b7f\u00fch\u00b7rest", "mich", "bal\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Bin ich oft mit meinem Sch\u00e4tzchen", "tokens": ["Bin", "ich", "oft", "mit", "mei\u00b7nem", "Sch\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Wald hineingegangen;", "tokens": ["In", "den", "Wald", "hin\u00b7ein\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die V\u00f6glein haben gesungen", "tokens": ["Und", "die", "V\u00f6\u00b7glein", "ha\u00b7ben", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nach meinem Verlangen.", "tokens": ["Nach", "mei\u00b7nem", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Wann ich jezt oft allein", "tokens": ["Wann", "ich", "jezt", "oft", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In den Wald hinaus geh,", "tokens": ["In", "den", "Wald", "hin\u00b7aus", "geh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APZR", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "So thuts mir im Herzen", "tokens": ["So", "thuts", "mir", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Tief drinnen so weh.", "tokens": ["Tief", "drin\u00b7nen", "so", "weh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Dort la\u00df ich mein K\u00fchlein", "tokens": ["Dort", "la\u00df", "ich", "mein", "K\u00fch\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Am liebsten fressen;", "tokens": ["Am", "liebs\u00b7ten", "fres\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wo ich oft bin des Abends", "tokens": ["Wo", "ich", "oft", "bin", "des", "A\u00b7bends"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei meinem B\u00fcbchen gesessen.", "tokens": ["Bei", "mei\u00b7nem", "B\u00fcb\u00b7chen", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ein sch\u00f6ns, ein sch\u00f6n H\u00e4uschen,", "tokens": ["Ein", "sch\u00f6ns", ",", "ein", "sch\u00f6n", "H\u00e4usc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein sch\u00f6n, ein sch\u00f6n Bett,", "tokens": ["Ein", "sch\u00f6n", ",", "ein", "sch\u00f6n", "Bett", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein sch\u00f6ns, ein sch\u00f6ns B\u00fcbchen,", "tokens": ["Ein", "sch\u00f6ns", ",", "ein", "sch\u00f6ns", "B\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sonst heirath ich nicht.", "tokens": ["Sonst", "hei\u00b7rath", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}