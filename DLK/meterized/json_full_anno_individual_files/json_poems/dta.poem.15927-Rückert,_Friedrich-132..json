{"dta.poem.15927": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "132.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sohn, der Tabakrauch auch, wozu ich dich anleiten", "tokens": ["Sohn", ",", "der", "Ta\u00b7ba\u00b7krauch", "auch", ",", "wo\u00b7zu", "ich", "dich", "an\u00b7lei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "ADV", "$,", "PWAV", "PPER", "PRF", "VVINF"], "meter": "+-+----+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Nicht will, der schlimme Brauch, hat seine guten Seiten.", "tokens": ["Nicht", "will", ",", "der", "schlim\u00b7me", "Brauch", ",", "hat", "sei\u00b7ne", "gu\u00b7ten", "Sei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "$,", "ART", "ADJA", "NN", "$,", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Leidenschaftlichkeit des Sprechens kann er d\u00e4mpfen,", "tokens": ["Die", "Lei\u00b7den\u00b7schaft\u00b7lich\u00b7keit", "des", "Spre\u00b7chens", "kann", "er", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um hingerissen nicht zu seyn von Meinungsk\u00e4mpfen:", "tokens": ["Um", "hin\u00b7ge\u00b7ris\u00b7sen", "nicht", "zu", "seyn", "von", "Mei\u00b7nungs\u00b7k\u00e4mp\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVIZU", "PTKNEG", "PTKZU", "VAINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df dir die Pfeife nicht ausgeh, die du verga\u00dfest,", "tokens": ["Da\u00df", "dir", "die", "Pfei\u00b7fe", "nicht", "aus\u00b7geh", ",", "die", "du", "ver\u00b7ga\u00b7\u00dfest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch du im Eifer mehr als recht ist Dampf ausblasest.", "tokens": ["Noch", "du", "im", "Ei\u00b7fer", "mehr", "als", "recht", "ist", "Dampf", "aus\u00b7bla\u00b7sest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "PIAT", "KOKOM", "ADJD", "VAFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}