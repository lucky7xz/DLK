{"textgrid.poem.30875": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe macht den Ehstand offt; doch macht Ehstand nicht stets Liebe;", "tokens": ["Lie\u00b7be", "macht", "den", "Eh\u00b7stand", "offt", ";", "doch", "macht", "Eh\u00b7stand", "nicht", "stets", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$.", "ADV", "VVFIN", "NN", "PTKNEG", "ADV", "NN", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Diese wil befreyet seyn, da\u00df sie stets was neues \u00fcbe.", "tokens": ["Die\u00b7se", "wil", "be\u00b7fre\u00b7yet", "seyn", ",", "da\u00df", "sie", "stets", "was", "neu\u00b7es", "\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVFIN", "VAINF", "$,", "KOUS", "PPER", "ADV", "PWS", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Magd, die stieg auffs Heu; der Knecht, der stieg ihr nach;", "tokens": ["Die", "Magd", ",", "die", "stieg", "auffs", "Heu", ";", "der", "Knecht", ",", "der", "stieg", "ihr", "nach", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$.", "ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie ward gar sehr erhitzt, zur Rache ward ihr gach,", "tokens": ["Sie", "ward", "gar", "sehr"