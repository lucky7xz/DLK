{"dta.poem.3899": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "19) Schreiben an eine verlobte Freundin.  \n Oder:  \n  Das beliebte und gelobte Schwartze.  \n Jm Nahmen eines andern.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Den Schuster f\u00e4rbt der Ru\u00df, den B\u00f6ttger", "tokens": ["Den", "Schus\u00b7ter", "f\u00e4rbt", "der", "Ru\u00df", ",", "den", "B\u00f6tt\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "schw\u00e4rtzt das pichen,", "tokens": ["schw\u00e4rtzt", "das", "pi\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Den Schmidt der Kohlen-Staub, die M\u00e4gde", "tokens": ["Den", "Schmidt", "der", "Koh\u00b7len\u00b7Staub", ",", "die", "M\u00e4g\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heerd und K\u00fcchen,", "tokens": ["Heerd", "und", "K\u00fc\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ein Curentaner hat den schwartzen Mantel um,", "tokens": ["Ein", "Cu\u00b7ren\u00b7ta\u00b7ner", "hat", "den", "schwart\u00b7zen", "Man\u00b7tel", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und schertzet Scaramin, sein Kleid l\u00e4st ihm nicht", "tokens": ["Und", "schert\u00b7zet", "Sca\u00b7ra\u00b7min", ",", "sein", "Kleid", "l\u00e4st", "ihm", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "tumm;", "tokens": ["tumm", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Du Braut, du schwartzes Kind, du schwartze Tu-", "tokens": ["Du", "Braut", ",", "du", "schwart\u00b7zes", "Kind", ",", "du", "schwart\u00b7ze", "Tu"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "gend-Perle,", "tokens": ["gen\u00b7dPer\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Du fromme Amsel du, des Br\u00e4utgams schwartze", "tokens": ["Du", "from\u00b7me", "Am\u00b7sel", "du", ",", "des", "Br\u00e4ut\u00b7gams", "schwart\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "PPER", "$,", "ART", "NE", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Schmerle,", "tokens": ["Schmer\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Schwartz ist dein Augen-Paar, die Augenbraunen", "tokens": ["Schwartz", "ist", "dein", "Au\u00b7gen\u00b7Paar", ",", "die", "Au\u00b7gen\u00b7brau\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "schwartz,", "tokens": ["schwartz", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Schwartz siehst du vorw\u00e4rts aus, schwartz siehst du", "tokens": ["Schwartz", "siehst", "du", "vor\u00b7w\u00e4rts", "aus", ",", "schwartz", "siehst", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.15": {"text": "hinterwarts.", "tokens": ["hin\u00b7ter\u00b7warts", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Jhr Weissen f\u00fchrt euch ab, hier hilfft kein Bo\u00df-", "tokens": ["Ihr", "Weis\u00b7sen", "f\u00fchrt", "euch", "ab", ",", "hier", "hilfft", "kein", "Bo\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "heits-Knirschen,", "tokens": ["heits\u00b7Knir\u00b7schen", ","], "token_info": ["word", "punct"], "pos": ["VVIZU", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Was schwartz ist, das ist sch\u00f6n, wer achtet weisse", "tokens": ["Was", "schwartz", "ist", ",", "das", "ist", "sch\u00f6n", ",", "wer", "ach\u00b7tet", "weis\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PDS", "VAFIN", "ADJD", "$,", "PWS", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Kirschen,", "tokens": ["Kir\u00b7schen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Nach schwartzen steigt man hoch, Brunetten leckt", "tokens": ["Nach", "schwart\u00b7zen", "steigt", "man", "hoch", ",", "Bru\u00b7net\u00b7ten", "leckt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "PIS", "ADJD", "$,", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "der Fuchs,", "tokens": ["der", "Fuchs", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NE", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Allons! geht packet ein, geschwinde, hurtig, flucks.", "tokens": ["Al\u00b7lons", "!", "geht", "pa\u00b7cket", "ein", ",", "ge\u00b7schwin\u00b7de", ",", "hur\u00b7tig", ",", "flucks", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "VVFIN", "PTKVZ", "$,", "ADJA", "$,", "ADJD", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Br\u00e4utgam nimmt euch nicht ins weiche Fe-", "tokens": ["Der", "Br\u00e4ut\u00b7gam", "nimmt", "euch", "nicht", "ins", "wei\u00b7che", "Fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PPER", "PTKNEG", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "der-Bette,", "tokens": ["der\u00b7Bet\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["PDS", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Und wenn sie Steine reich, und Millionen h\u00e4tte,", "tokens": ["Und", "wenn", "sie", "Stei\u00b7ne", "reich", ",", "und", "Mil\u00b7lion\u00b7en", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "ADJD", "$,", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Er liebet nur was schwartz, das ist sein fester Schlu\u00df,", "tokens": ["Er", "lie\u00b7bet", "nur", "was", "schwartz", ",", "das", "ist", "sein", "fes\u00b7ter", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "ADJD", "$,", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den schwartzen Kindern reicht er einen sanfften", "tokens": ["Den", "schwart\u00b7zen", "Kin\u00b7dern", "reicht", "er", "ei\u00b7nen", "sanff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Ku\u00df.", "tokens": ["Ku\u00df", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.29": {"text": "Was wei\u00df ist, h\u00e4ngt den Kopff, und ist nicht auf-", "tokens": ["Was", "wei\u00df", "ist", ",", "h\u00e4ngt", "den", "Kopff", ",", "und", "ist", "nicht", "auf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,", "KON", "VAFIN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "ger\u00e4umet,", "tokens": ["ge\u00b7r\u00e4u\u00b7met", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Und wenn sie sch\u00f6ne thun, so ists als wenn man", "tokens": ["Und", "wenn", "sie", "sch\u00f6\u00b7ne", "thun", ",", "so", "ists", "als", "wenn", "man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJA", "VVINF", "$,", "ADV", "VAFIN", "KOKOM", "KOUS", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "tr\u00e4umet,", "tokens": ["tr\u00e4u\u00b7met", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "Bey schwartzen Sch\u00f6nen wohnt ein m\u00e4nnliches", "tokens": ["Bey", "schwart\u00b7zen", "Sch\u00f6\u00b7nen", "wohnt", "ein", "m\u00e4nn\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Gem\u00fcth,", "tokens": ["Ge\u00b7m\u00fcth", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.35": {"text": "Das jedem Ungl\u00fccks-Sturm behertzt entgegen zieht,", "tokens": ["Das", "je\u00b7dem", "Un\u00b7gl\u00fccks\u00b7Sturm", "be\u00b7hertzt", "ent\u00b7ge\u00b7gen", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sie sincken nicht gleich hin, wenn sich ein Unfall", "tokens": ["Sie", "sin\u00b7cken", "nicht", "gleich", "hin", ",", "wenn", "sich", "ein", "Un\u00b7fall"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,", "KOUS", "PRF", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.37": {"text": "reget.", "tokens": ["re\u00b7get", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.38": {"text": "Natur hat ja in schwartz besondre Krafft geleget.", "tokens": ["Na\u00b7tur", "hat", "ja", "in", "schwartz", "be\u00b7sond\u00b7re", "Krafft", "ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Was schwartz heist, n\u00e4hrt und hegt gantz innerliche", "tokens": ["Was", "schwartz", "heist", ",", "n\u00e4hrt", "und", "hegt", "gantz", "in\u00b7ner\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Krafft,", "tokens": ["Krafft", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.41": {"text": "Was schwartz ist, das ist auch fein fest und dauerhafft.", "tokens": ["Was", "schwartz", "ist", ",", "das", "ist", "auch", "fein", "fest", "und", "dau\u00b7er\u00b7hafft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PDS", "VAFIN", "ADV", "ADJD", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.42": {"text": "Ein schwartz beliebter Schatz ist starck und sch\u00f6nster", "tokens": ["Ein", "schwartz", "be\u00b7lieb\u00b7ter", "Schatz", "ist", "starck", "und", "sch\u00f6ns\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "L\u00e4nge,", "tokens": ["L\u00e4n\u00b7ge", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.44": {"text": "Kriegt Freyer und ", "tokens": ["Kriegt", "Frey\u00b7er", "und"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NN", "KON"], "meter": "-+-+", "measure": "iambic.di"}, "line.45": {"text": "Was schwartz, ist ", "tokens": ["Was", "schwartz", ",", "ist"], "token_info": ["word", "word", "punct", "word"], "pos": ["PWS", "ADJD", "$,", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.46": {"text": "Ein schwartzes M\u00e4gdgen ist ein rechter Hertzens-", "tokens": ["Ein", "schwart\u00b7zes", "M\u00e4gd\u00b7gen", "ist", "ein", "rech\u00b7ter", "Hert\u00b7zens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Dieb.", "tokens": ["Dieb", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.48": {"text": "Was schwartz ist, kan im Schertz den Liebsten hertz-", "tokens": ["Was", "schwartz", "ist", ",", "kan", "im", "Schertz", "den", "Liebs\u00b7ten", "hertz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VMFIN", "APPRART", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "lich schmeicheln,", "tokens": ["lich", "schmei\u00b7cheln", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.50": {"text": "Und ihm mit schwartzer Hand die sch\u00f6nen Backen", "tokens": ["Und", "ihm", "mit", "schwart\u00b7zer", "Hand", "die", "sch\u00f6\u00b7nen", "Ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.51": {"text": "streicheln.", "tokens": ["strei\u00b7cheln", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.52": {"text": "So schlie\u00df ich meinen Brief, so hab ich schwartz auf", "tokens": ["So", "schlie\u00df", "ich", "mei\u00b7nen", "Brief", ",", "so", "hab", "ich", "schwartz", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.53": {"text": "wei\u00df,", "tokens": ["wei\u00df", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.54": {"text": "Die schwartze Jungfer Braut beh\u00e4lt der Tugend", "tokens": ["Die", "schwart\u00b7ze", "Jung\u00b7fer", "Braut", "be\u00b7h\u00e4lt", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.55": {"text": "Prei\u00df.", "tokens": ["Prei\u00df", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.56": {"text": "Mit schwartzen will ich nun die schwartzen Zeilen", "tokens": ["Mit", "schwart\u00b7zen", "will", "ich", "nun", "die", "schwart\u00b7zen", "Zei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVINF", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.57": {"text": "schliessen,", "tokens": ["schlies\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.58": {"text": "So mag die schwartze Braut den schwartzen Bett-", "tokens": ["So", "mag", "die", "schwart\u00b7ze", "Braut", "den", "schwart\u00b7zen", "Bet\u00b7t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.59": {"text": "Schelm k\u00fcssen.", "tokens": ["Schelm", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}