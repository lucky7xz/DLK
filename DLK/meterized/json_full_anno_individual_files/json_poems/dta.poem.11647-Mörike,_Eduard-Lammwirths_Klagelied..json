{"dta.poem.11647": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Lammwirths Klagelied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Spazier' ich auf und ab,", "tokens": ["Spa\u00b7zier'", "ich", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den ganzen lieben langen Tag,", "tokens": ["Den", "gan\u00b7zen", "lie\u00b7ben", "lan\u00b7gen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schaue die Stra\u00dfe hinab.", "tokens": ["Und", "schau\u00b7e", "die", "Stra\u00b7\u00dfe", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Es steht ein Regenbogen", "tokens": ["Es", "steht", "ein", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl \u00fcber jenem Haus,", "tokens": ["Wohl", "\u00fc\u00b7ber", "je\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Schild ist eingezogen,", "tokens": ["Mein", "Schild", "ist", "ein\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein andrer hangt heraus!", "tokens": ["Ein", "an\u00b7drer", "hangt", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Heraus hangt \u00fcber der Th\u00fcre", "tokens": ["He\u00b7raus", "hangt", "\u00fc\u00b7ber", "der", "Th\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Hahn mit rothem Kamm;", "tokens": ["Ein", "Hahn", "mit", "ro\u00b7them", "Kamm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ich die Wirthschaft f\u00fchrte,", "tokens": ["Als", "ich", "die", "Wirth\u00b7schaft", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War es ein g\u00fcldenes Lamm.", "tokens": ["War", "es", "ein", "g\u00fcl\u00b7de\u00b7nes", "Lamm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Sch\u00e4flein wohl zu scheeren,", "tokens": ["Mein", "Sch\u00e4f\u00b7lein", "wohl", "zu", "schee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich sparte keine M\u00fch',", "tokens": ["Ich", "spar\u00b7te", "kei\u00b7ne", "M\u00fch'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin herunter gekommen,", "tokens": ["Ich", "bin", "her\u00b7un\u00b7ter", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wei\u00df doch selber nicht, wie.", "tokens": ["Und", "wei\u00df", "doch", "sel\u00b7ber", "nicht", ",", "wie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKNEG", "$,", "PWAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Nun l\u00e4uft es mit K\u00f6chen und Kellnern", "tokens": ["Nun", "l\u00e4uft", "es", "mit", "K\u00f6\u00b7chen", "und", "Kell\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Im ganzen Hause so voll,", "tokens": ["Im", "gan\u00b7zen", "Hau\u00b7se", "so", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich wei\u00df nicht, wem ich von Allen", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wem", "ich", "von", "Al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "APPR", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zuerst den Hals brechen soll.", "tokens": ["Zu\u00b7erst", "den", "Hals", "bre\u00b7chen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Da kommen die Chaisen gefahren!", "tokens": ["Da", "kom\u00b7men", "die", "Chai\u00b7sen", "ge\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Hausknecht springt in die H\u00f6h'.", "tokens": ["Der", "Haus\u00b7knecht", "springt", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Vor\u00fcber, ihr R\u00f6\u00dflein, vor\u00fcber,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "ihr", "R\u00f6\u00df\u00b7lein", ",", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dem Lammwirth ist gar so weh!", "tokens": ["Dem", "Lamm\u00b7wirth", "ist", "gar", "so", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}