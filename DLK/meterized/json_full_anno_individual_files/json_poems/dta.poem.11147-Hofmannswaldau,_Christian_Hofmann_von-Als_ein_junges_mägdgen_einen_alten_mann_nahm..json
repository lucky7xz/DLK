{"dta.poem.11147": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als ein junges m\u00e4gdgen einen alten  \n mann nahm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du siehest auf das geld, nicht auf die grauen haare;", "tokens": ["Du", "sie\u00b7hest", "auf", "das", "geld", ",", "nicht", "auf", "die", "grau\u00b7en", "haa\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du gehst aufs braut-bett zu, dein mann zur todten-baare.", "tokens": ["Du", "gehst", "aufs", "braut\u00b7bett", "zu", ",", "dein", "mann", "zur", "tod\u00b7ten\u00b7baa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die freyheit ist verkaufft, was hilfft dich nun dein freyn?", "tokens": ["Die", "frey\u00b7heit", "ist", "ver\u00b7kaufft", ",", "was", "hilfft", "dich", "nun", "dein", "freyn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Am tage wirst du frau; des nachtes witwe seyn.", "tokens": ["Am", "ta\u00b7ge", "wirst", "du", "frau", ";", "des", "nach\u00b7tes", "wit\u00b7we", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}