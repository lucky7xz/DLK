{"dta.poem.322": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die artige  Balis,  da er starb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Augen/ die schon sterbend brechen/", "tokens": ["Au\u00b7gen", "/", "die", "schon", "ster\u00b7bend", "bre\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ADV", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blicken euch jetzt kl\u00e4glich an/", "tokens": ["Bli\u00b7cken", "euch", "jetzt", "kl\u00e4g\u00b7lich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lippen/ die nicht mehr zum Sprechen", "tokens": ["Lip\u00b7pen", "/", "die", "nicht", "mehr", "zum", "Spre\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "PRELS", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Recht verst\u00e4ndlich aufgethan/", "tokens": ["Recht", "ver\u00b7st\u00e4nd\u00b7lich", "auf\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fcssen euch/ mein werthes Leben/", "tokens": ["M\u00fcs\u00b7sen", "euch", "/", "mein", "wert\u00b7hes", "Le\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00f6ne ", "tokens": ["Sch\u00f6\u00b7ne"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Thr\u00e4nen statt der Worte fliessen/", "tokens": ["Thr\u00e4\u00b7nen", "statt", "der", "Wor\u00b7te", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die voll Liebes Seuffzer sind/", "tokens": ["Die", "voll", "Lie\u00b7bes", "Seuff\u00b7zer", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die falben Lippen k\u00fcssen", "tokens": ["Und", "die", "fal\u00b7ben", "Lip\u00b7pen", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch zum letzten mahl mein Kind/", "tokens": ["Euch", "zum", "letz\u00b7ten", "mahl", "mein", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich auf der Todten-Reise", "tokens": ["Da\u00df", "ich", "auf", "der", "Tod\u00b7ten\u00b7Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich mit eurer Anmuth speise.", "tokens": ["Mich", "mit", "eu\u00b7rer", "An\u00b7muth", "spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nehmet euren Geist zur\u00fccke/", "tokens": ["Neh\u00b7met", "eu\u00b7ren", "Geist", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Balis, weg aus meiner Brust/", "tokens": ["Ba\u00b7lis", ",", "weg", "aus", "mei\u00b7ner", "Brust", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein ergrimmetes Geschicke", "tokens": ["Ein", "er\u00b7grim\u00b7me\u00b7tes", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00f6hret die gehabte Lust/", "tokens": ["St\u00f6h\u00b7ret", "die", "ge\u00b7hab\u00b7te", "Lust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein Hertz mu\u00df aus eurem scheiden", "tokens": ["Mein", "Hertz", "mu\u00df", "aus", "eu\u00b7rem", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPOSAT", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und die zarte Wohnung meiden.", "tokens": ["Und", "die", "zar\u00b7te", "Woh\u00b7nung", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lebet wohl geliebte Seele/", "tokens": ["Le\u00b7bet", "wohl", "ge\u00b7lieb\u00b7te", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seel\u2019 ist ausser mir/", "tokens": ["Mei\u00b7ne", "Seel'", "ist", "aus\u00b7ser", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Grabes schwartze H\u00f6hle", "tokens": ["Und", "des", "Gra\u00b7bes", "schwart\u00b7ze", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oeffnet schon die dunckle Th\u00fcr/", "tokens": ["Oeff\u00b7net", "schon", "die", "dunck\u00b7le", "Th\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich mu\u00df euch/ mein Kind/ verlassen/", "tokens": ["Ich", "mu\u00df", "euch", "/", "mein", "Kind", "/", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und bey Geistern Wohnung fassen.", "tokens": ["Und", "bey", "Geis\u00b7tern", "Woh\u00b7nung", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}