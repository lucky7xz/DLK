{"dta.poem.5491": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nothwendiger Dienst des Sch\u00f6pfers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So weit wir des Verstandes Kr\u00e4fte mit aller F\u00e4higkeit\nerstrecken,", "tokens": ["So", "weit", "wir", "des", "Ver\u00b7stan\u00b7des", "Kr\u00e4f\u00b7te", "mit", "al\u00b7ler", "F\u00e4\u00b7hig\u00b7keit", "er\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ART", "NN", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Um in den Kr\u00e4ften unsers Geistes was GOtt anst\u00e4ndigs", "tokens": ["Um", "in", "den", "Kr\u00e4f\u00b7ten", "un\u00b7sers", "Geis\u00b7tes", "was", "Gott", "an\u00b7st\u00e4n\u00b7digs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "NN", "PPOSAT", "NN", "PWS", "NN", "NE"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So scheinet die Empfindlichkeit der Seelen, wenn wir, in", "tokens": ["So", "schei\u00b7net", "die", "Emp\u00b7find\u00b7lich\u00b7keit", "der", "See\u00b7len", ",", "wenn", "wir", ",", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "$,", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Des Sch\u00f6pfers, seine Herrlichkeit und Macht und Lieb und", "tokens": ["Des", "Sch\u00f6p\u00b7fers", ",", "sei\u00b7ne", "Herr\u00b7lich\u00b7keit", "und", "Macht", "und", "Lieb", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "KON", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sie darin mit Lust bewundern, so viel wir hier begreif-", "tokens": ["Und", "sie", "da\u00b7rin", "mit", "Lust", "be\u00b7wun\u00b7dern", ",", "so", "viel", "wir", "hier", "be\u00b7greif"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PAV", "APPR", "NN", "VVINF", "$,", "ADV", "ADV", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Das erste St\u00fcck des GOttes Diensts ja fast das eintzige", "tokens": ["Das", "ers\u00b7te", "St\u00fcck", "des", "Got\u00b7tes", "Diensts", "ja", "fast", "das", "eint\u00b7zi\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "NN", "ADV", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Erschrick, als Christ, hier\u00fcber nicht und denck\u2019, ob woll\u2019", "tokens": ["Er\u00b7schrick", ",", "als", "Christ", ",", "hier\u00b7\u00fc\u00b7ber", "nicht", "und", "denck", ",", "ob", "woll'"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "KOUS", "NN", "$,", "PAV", "PTKNEG", "KON", "VVIMP", "$,", "KOUS", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mithin dein gantzes Christenthum, durch diesen meinen", "tokens": ["Mi\u00b7thin", "dein", "gant\u00b7zes", "Chris\u00b7ten\u00b7thum", ",", "durch", "die\u00b7sen", "mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PDAT", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O nein; der bleibet Felsen-fest. La\u00df uns nur nach der", "tokens": ["O", "nein", ";", "der", "blei\u00b7bet", "Fel\u00b7sen\u00b7fest", ".", "La\u00df", "uns", "nur", "nach", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKANT", "$.", "ART", "VVFIN", "NN", "$.", "VVIMP", "PPER", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So, hoff\u2019 ich, wirft du was ich sage mir, sonder Wieder-", "tokens": ["So", ",", "hoff'", "ich", ",", "wirft", "du", "was", "ich", "sa\u00b7ge", "mir", ",", "son\u00b7der", "Wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PWS", "PPER", "VVFIN", "PPER", "$,", "KON", "TRUNC"], "meter": "-+-+---+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ist es nicht wahr? da\u00df GOtt der HErr, auch nach", "tokens": ["Ist", "es", "nicht", "wahr", "?", "da\u00df", "Gott", "der", "Herr", ",", "auch", "nach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "PTKVZ", "$.", "KOUS", "NN", "ART", "NN", "$,", "ADV", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Engeln, von den seel\u2019gen Geistern, und aller Himmel", "tokens": ["Von", "En\u00b7geln", ",", "von", "den", "seel'\u00b7gen", "Geis\u00b7tern", ",", "und", "al\u00b7ler", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Als Sch\u00f6pfer, angebetet werde? da\u00df sie, wenn sie die", "tokens": ["Als", "Sch\u00f6p\u00b7fer", ",", "an\u00b7ge\u00b7be\u00b7tet", "wer\u00b7de", "?", "da\u00df", "sie", ",", "wenn", "sie", "die"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "VVPP", "VAFIN", "$.", "KOUS", "PPER", "$,", "KOUS", "PPER", "ART"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Die er, in aller Himmel Tieffen, an Millionen Sonn-", "tokens": ["Die", "er", ",", "in", "al\u00b7ler", "Him\u00b7mel", "Tief\u00b7fen", ",", "an", "Mil\u00b7lion\u00b7en", "Sonn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "$,", "APPR", "PIAT", "NN", "NN", "$,", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Als Proben seiner weisen Macht und seiner Liebe, lassen", "tokens": ["Als", "Pro\u00b7ben", "sei\u00b7ner", "wei\u00b7sen", "Macht", "und", "sei\u00b7ner", "Lie\u00b7be", ",", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "NN", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Jhn durch ihr ", "tokens": ["Jhn", "durch", "ihr"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Da\u00df in dem seeligen Bewundern, nur ihre Pflichten blos", "tokens": ["Da\u00df", "in", "dem", "see\u00b7li\u00b7gen", "Be\u00b7wun\u00b7dern", ",", "nur", "ih\u00b7re", "Pflich\u00b7ten", "blos"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Indem sie ja nicht glauben d\u00fcrffen? ", "tokens": ["In\u00b7dem", "sie", "ja", "nicht", "glau\u00b7ben", "d\u00fcrf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch Adam in dem Paradiese an GOttes Wercken \u00fcberall", "tokens": ["Auch", "A\u00b7dam", "in", "dem", "Pa\u00b7ra\u00b7die\u00b7se", "an", "Got\u00b7tes", "Wer\u00b7cken", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "APPR", "NN", "NN", "ADV"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Sich eintzig wird besch\u00e4ftigt haben? und, wo er nicht ge-", "tokens": ["Sich", "eint\u00b7zig", "wird", "be\u00b7sch\u00e4f\u00b7tigt", "ha\u00b7ben", "?", "und", ",", "wo", "er", "nicht", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "VAFIN", "VVPP", "VAINF", "$.", "KON", "$,", "PWAV", "PPER", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Er, in dem seeligen Bewundern der Wercke GOTTES,", "tokens": ["Er", ",", "in", "dem", "see\u00b7li\u00b7gen", "Be\u00b7wun\u00b7dern", "der", "Wer\u00b7cke", "GoT\u00b7TES", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "ADJA", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Allein verherrlicht haben w\u00fcrde? Ob wir nun gleich, wie", "tokens": ["Al\u00b7lein", "ver\u00b7herr\u00b7licht", "ha\u00b7ben", "w\u00fcr\u00b7de", "?", "Ob", "wir", "nun", "gleich", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVPP", "VAINF", "VAFIN", "$.", "KOUS", "PPER", "ADV", "ADV", "$,", "PWAV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Der ew\u2019gen Liebe Wunder-Liebe in solchem hohen Grad", "tokens": ["Der", "ew'\u00b7gen", "Lie\u00b7be", "Wun\u00b7der\u00b7Lie\u00b7be", "in", "sol\u00b7chem", "ho\u00b7hen", "Grad"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Da\u00df Christus uns zum Mittler worden; und man dadurch", "tokens": ["Da\u00df", "Chris\u00b7tus", "uns", "zum", "Mitt\u00b7ler", "wor\u00b7den", ";", "und", "man", "da\u00b7durch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NE", "PPER", "APPRART", "NN", "VAPP", "$.", "KON", "PIS", "PAV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Auch als Erl\u00f6ser, GOtt zu ehren, und recht zu glauben", "tokens": ["Auch", "als", "Er\u00b7l\u00f6\u00b7ser", ",", "Gott", "zu", "eh\u00b7ren", ",", "und", "recht", "zu", "glau\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NN", "$,", "NN", "PTKZU", "VVINF", "$,", "KON", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "So ist doch unser\u2019 erste Pflicht, als Sch\u00f6pfer unsern", "tokens": ["So", "ist", "doch", "un\u00b7ser'", "ers\u00b7te", "Pflicht", ",", "als", "Sch\u00f6p\u00b7fer", "un\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und ihn in seinem Werck zu ehren, dadurch so wenig auf-", "tokens": ["Und", "ihn", "in", "sei\u00b7nem", "Werck", "zu", "eh\u00b7ren", ",", "da\u00b7durch", "so", "we\u00b7nig", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "PAV", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Da\u00df wir vielmehr nach allen Kr\u00e4ften des Sch\u00f6pfers Weis-", "tokens": ["Da\u00df", "wir", "viel\u00b7mehr", "nach", "al\u00b7len", "Kr\u00e4f\u00b7ten", "des", "Sch\u00f6p\u00b7fers", "Weis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIAT", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Wodurch Er Himmel, Erde, Geister und Menschen hat", "tokens": ["Wo\u00b7durch", "Er", "Him\u00b7mel", ",", "Er\u00b7de", ",", "Geis\u00b7ter", "und", "Men\u00b7schen", "hat"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VAFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Betrachten, und, nebst unserm Glauben, in froher Lust,", "tokens": ["Be\u00b7trach\u00b7ten", ",", "und", ",", "nebst", "un\u00b7serm", "Glau\u00b7ben", ",", "in", "fro\u00b7her", "Lust", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.16": {"text": "Wenn wir nicht GOtt, nur unserntwegen, auch seinent-", "tokens": ["Wenn", "wir", "nicht", "Gott", ",", "nur", "un\u00b7sernt\u00b7we\u00b7gen", ",", "auch", "sei\u00b7nent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "$,", "ADV", "VVINF", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}