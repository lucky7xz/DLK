{"dta.poem.2872": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dr\u00fcck manchmal zu ein Aug'! es ist nicht schwer, der Flor", "tokens": ["Dr\u00fcck", "manch\u00b7mal", "zu", "ein", "Aug'", "!", "es", "ist", "nicht", "schwer", ",", "der", "Flor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Wimper h\u00e4ngt daran, zieh ihn nur leise vor!", "tokens": ["Der", "Wim\u00b7per", "h\u00e4ngt", "da\u00b7ran", ",", "zieh", "ihn", "nur", "lei\u00b7se", "vor", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch lerne schlie\u00dfen auch, was schwerer ist, das Ohr!", "tokens": ["Doch", "ler\u00b7ne", "schlie\u00b7\u00dfen", "auch", ",", "was", "schwe\u00b7rer", "ist", ",", "das", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ADV", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von innen schlie\u00df es! denn kein Schlo\u00df ist au\u00dfen vor.", "tokens": ["Von", "in\u00b7nen", "schlie\u00df", "es", "!", "denn", "kein", "Schlo\u00df", "ist", "au\u00b7\u00dfen", "vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "$.", "ADV", "PIAT", "NN", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "La\u00df dich die Uebung in der Kunst nur nicht verdrie\u00dfen,", "tokens": ["La\u00df", "dich", "die", "Ue\u00b7bung", "in", "der", "Kunst", "nur", "nicht", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu rechter Zeit das Aug' als wie das Ohr zu schlie\u00dfen;", "tokens": ["Zu", "rech\u00b7ter", "Zeit", "das", "Aug'", "als", "wie", "das", "Ohr", "zu", "schlie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "KOUS", "KOKOM", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sonst hast du keine Ruh, weil, wie die Leute sprechen,", "tokens": ["Sonst", "hast", "du", "kei\u00b7ne", "Ruh", ",", "weil", ",", "wie", "die", "Leu\u00b7te", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$,", "KOUS", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "All wissen Kopfweh macht, all h\u00f6ren Ohrenstechen.", "tokens": ["All", "wis\u00b7sen", "Kopf\u00b7weh", "macht", ",", "all", "h\u00f6\u00b7ren", "Oh\u00b7ren\u00b7ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}