{"dta.poem.15737": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Blume hat gewis empfahn den Blumenstaub", "tokens": ["Die", "Blu\u00b7me", "hat", "ge\u00b7wis", "em\u00b7pfahn", "den", "Blu\u00b7men\u00b7staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zun\u00e4chst zu anderm Zweck als zu der Biene Raub.", "tokens": ["Zu\u00b7n\u00e4chst", "zu", "an\u00b7derm", "Zweck", "als", "zu", "der", "Bie\u00b7ne", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "NN", "KOKOM", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wir erlauben gern, da\u00df sie dazu ihn nimmt,", "tokens": ["Doch", "wir", "er\u00b7lau\u00b7ben", "gern", ",", "da\u00df", "sie", "da\u00b7zu", "ihn", "nimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nehmen selbst f\u00fcr uns, was sie f\u00fcr sich bestimmt.", "tokens": ["Und", "neh\u00b7men", "selbst", "f\u00fcr", "uns", ",", "was", "sie", "f\u00fcr", "sich", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "$,", "PRELS", "PPER", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Biene dient die Blum', es dienet mir die Biene,", "tokens": ["Der", "Bie\u00b7ne", "dient", "die", "Blum'", ",", "es", "die\u00b7net", "mir", "die", "Bie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wissens nicht, nur ich wei\u00df da\u00df ich allen diene.", "tokens": ["Sie", "wis\u00b7sens", "nicht", ",", "nur", "ich", "wei\u00df", "da\u00df", "ich", "al\u00b7len", "die\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "$,", "ADV", "PPER", "VVFIN", "KOUS", "PPER", "PIAT", "PDS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}