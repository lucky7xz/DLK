{"textgrid.poem.32878": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, reizende Selinde,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, reizende Selinde,", "tokens": ["Ja", ",", "rei\u00b7zen\u00b7de", "Se\u00b7lin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Von unsrer Z\u00e4rtlichkeit,", "tokens": ["Von", "uns\u00b7rer", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So unwahr ich es finde,", "tokens": ["So", "un\u00b7wahr", "ich", "es", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spricht man seit langer Zeit:", "tokens": ["Spricht", "man", "seit", "lan\u00b7ger", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Man saget, deine Blicke", "tokens": ["Man", "sa\u00b7get", ",", "dei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verriethen dich zu sehr;", "tokens": ["Ver\u00b7rie\u00b7then", "dich", "zu", "sehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu meinem ganzen Gl\u00fccke", "tokens": ["Zu", "mei\u00b7nem", "gan\u00b7zen", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ach! fehlte wenig mehr.", "tokens": ["Ach", "!", "fehl\u00b7te", "we\u00b7nig", "mehr", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df, dein Auge schm\u00e4hlet,", "tokens": ["Ich", "wei\u00df", ",", "dein", "Au\u00b7ge", "schm\u00e4h\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So oft man davon spricht:", "tokens": ["So", "oft", "man", "da\u00b7von", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du weist, wie viel noch fehlet,", "tokens": ["Du", "weist", ",", "wie", "viel", "noch", "feh\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich w\u00fcnscht, es fehlte nicht!", "tokens": ["Ich", "w\u00fcnscht", ",", "es", "fehl\u00b7te", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch um nicht mehr zu klagen,", "tokens": ["Doch", "um", "nicht", "mehr", "zu", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Selinde, wei\u00df ich Rath:", "tokens": ["Se\u00b7lin\u00b7de", ",", "wei\u00df", "ich", "Rath", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erf\u00fclle, was sie sagen!", "tokens": ["Er\u00b7f\u00fcl\u00b7le", ",", "was", "sie", "sa\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So schweigt die ganze Stadt.", "tokens": ["So", "schweigt", "die", "gan\u00b7ze", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, reizende Selinde,", "tokens": ["Ja", ",", "rei\u00b7zen\u00b7de", "Se\u00b7lin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Von unsrer Z\u00e4rtlichkeit,", "tokens": ["Von", "uns\u00b7rer", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So unwahr ich es finde,", "tokens": ["So", "un\u00b7wahr", "ich", "es", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spricht man seit langer Zeit:", "tokens": ["Spricht", "man", "seit", "lan\u00b7ger", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.