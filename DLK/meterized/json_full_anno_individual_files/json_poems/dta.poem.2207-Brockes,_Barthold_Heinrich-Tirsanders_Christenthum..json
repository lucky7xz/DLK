{"dta.poem.2207": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Tirsanders Christenthum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch ist eine Kreatur, die, nebst noch andrer", "tokens": ["Der", "Mensch", "ist", "ei\u00b7ne", "Kre\u00b7a\u00b7tur", ",", "die", ",", "nebst", "noch", "an\u00b7drer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "$,", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Narrethey,", "tokens": ["Nar\u00b7ret\u00b7hey", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Sich selber \u00fcberredet, schreibet, und sagt, da\u00df sie ver-", "tokens": ["Sich", "sel\u00b7ber", "\u00fc\u00b7berr\u00b7e\u00b7det", ",", "schrei\u00b7bet", ",", "und", "sagt", ",", "da\u00df", "sie", "ver"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADV", "VVPP", "$,", "VVFIN", "$,", "KON", "VVFIN", "$,", "KOUS", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "n\u00fcnftig sey:", "tokens": ["n\u00fcnf\u00b7tig", "sey", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Da doch, von allen Kreaturen, wenn sie auf sein Betra-", "tokens": ["Da", "doch", ",", "von", "al\u00b7len", "Kre\u00b7a\u00b7tu\u00b7ren", ",", "wenn", "sie", "auf", "sein", "Be\u00b7tra"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "APPR", "PIAT", "NN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "gen sehen,", "tokens": ["gen", "se\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Kein\u2019 einzig\u2019 ihm von der Vernunft die Eigenschaft wird", "tokens": ["Kein'", "ein\u00b7zig'", "ihm", "von", "der", "Ver\u00b7nunft", "die", "Ei\u00b7gen\u00b7schaft", "wird"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VAFIN"], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "zugestehen.", "tokens": ["zu\u00b7ge\u00b7ste\u00b7hen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}