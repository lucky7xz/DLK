{"textgrid.poem.26627": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dennoch, wie sehr und wie oft dich mein Auge bewundert, du sprichst mir", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dennoch, wie sehr und wie oft dich mein Auge bewundert, du sprichst mir", "tokens": ["Den\u00b7noch", ",", "wie", "sehr", "und", "wie", "oft", "dich", "mein", "Au\u00b7ge", "be\u00b7wun\u00b7dert", ",", "du", "sprichst", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ADV", "KON", "PWAV", "ADV", "PPER", "PPOSAT", "NN", "VVPP", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+--+-++-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Nicht mehr zum Herzen wie einst, weithin gebreitete Stadt;", "tokens": ["Nicht", "mehr", "zum", "Her\u00b7zen", "wie", "einst", ",", "weit\u00b7hin", "ge\u00b7brei\u00b7te\u00b7te", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "KOKOM", "ADV", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht mehr wie einst, da wallumg\u00fcrtet du noch mit den alten", "tokens": ["Nicht", "mehr", "wie", "einst", ",", "da", "wal\u00b7lum\u00b7g\u00fcr\u00b7tet", "du", "noch", "mit", "den", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "KOKOM", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Schw\u00e4rzlichen H\u00e4usern geragt \u00fcber das gr\u00fcne Glacis:", "tokens": ["Schw\u00e4rz\u00b7li\u00b7chen", "H\u00e4u\u00b7sern", "ge\u00b7ragt", "\u00fc\u00b7ber", "das", "gr\u00fc\u00b7ne", "Gla\u00b7cis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Eng und gedr\u00fcckt, voll gewundener Gassen und d\u00fcsterer Winkel \u2013", "tokens": ["Eng", "und", "ge\u00b7dr\u00fcckt", ",", "voll", "ge\u00b7wun\u00b7de\u00b7ner", "Gas\u00b7sen", "und", "d\u00fcs\u00b7te\u00b7rer", "Win\u00b7kel", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVPP", "$,", "ADJD", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Aber es wogte in dir fr\u00f6hlich ein fr\u00f6hliches Volk.", "tokens": ["A\u00b7ber", "es", "wog\u00b7te", "in", "dir", "fr\u00f6h\u00b7lich", "ein", "fr\u00f6h\u00b7li\u00b7ches", "Volk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Leicht gesinnt und bewegt, abhold den M\u00fchen des Daseins,", "tokens": ["Leicht", "ge\u00b7sinnt", "und", "be\u00b7wegt", ",", "ab\u00b7hold", "den", "M\u00fc\u00b7hen", "des", "Da\u00b7seins", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "VVFIN", "$,", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Lebt' es harmlos dahin, wie ein empf\u00e4ngliches Kind.", "tokens": ["Lebt'", "es", "harm\u00b7los", "da\u00b7hin", ",", "wie", "ein", "emp\u00b7f\u00e4ng\u00b7li\u00b7ches", 