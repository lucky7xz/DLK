, "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KOKOM", "ADV", "ADJD", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+++-+", "measure": "iambic.octa.plus.invert"}, "line.13": {"text": "Geistiger Ziele Bewu\u00dftsein, der Stolz befreiender Arbeit", "tokens": ["Geis\u00b7ti\u00b7ger", "Zie\u00b7le", "Be\u00b7wu\u00df\u00b7tsein", ",", "der", "Stolz", "be\u00b7frei\u00b7en\u00b7der", "Ar\u00b7beit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Weh'n, gleich fr\u00f6stelndem Hauch, selbst um die Reize der Frau'n.", "tokens": ["Weh'n", ",", "gleich", "fr\u00f6s\u00b7teln\u00b7dem", "Hauch", ",", "selbst", "um", "die", "Rei\u00b7ze", "der", "Frau'", "n."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "$,", "ADV", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "ART", "NN", "NE"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Reicher, beschwingter sind Handel und Wandel, doch fehlt das Behagen,", "tokens": ["Rei\u00b7cher", ",", "be\u00b7schwing\u00b7ter", "sind", "Han\u00b7del", "und", "Wan\u00b7del", ",", "doch", "fehlt", "das", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJD", "VAFIN", "NN", "KON", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Das am Erworbenen sich festen Besitzes erfreut.", "tokens": ["Das", "am", "Er\u00b7wor\u00b7be\u00b7nen", "sich", "fes\u00b7ten", "Be\u00b7sit\u00b7zes", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "PRF", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Prunkende H\u00e4user und Pl\u00e4tze gewahr' ich in stummer Ver\u00f6dung \u2013", "tokens": ["Prun\u00b7ken\u00b7de", "H\u00e4u\u00b7ser", "und", "Pl\u00e4t\u00b7ze", "ge\u00b7wahr'", "ich", "in", "stum\u00b7mer", "Ve\u00b7r\u00f6\u00b7dung", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Und kein Jubel erschallt mehr aus der menschlichen Brust ...", "tokens": ["Und", "kein", "Ju\u00b7bel", "er\u00b7schallt", "mehr", "aus", "der", "menschli\u00b7chen", "Brust", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Ja, du hast dich ver\u00e4ndert, ich f\u00fchl' es. Bist du auch sch\u00f6ner,", "tokens": ["Ja", ",", "du", "hast", "dich", "ver\u00b7\u00e4n\u00b7dert", ",", "ich", "f\u00fchl'", "es", ".", "Bist", "du", "auch", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VVFIN", "PPER", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Bist du auch gr\u00f6\u00dfer, als einst \u2013 bist du doch nicht mehr mein Wien!", "tokens": ["Bist", "du", "auch", "gr\u00f6\u00b7\u00dfer", ",", "als", "einst", "\u2013", "bist", "du", "doch", "nicht", "mehr", "mein", "Wi\u00b7en", "!"], "token_info": ["word", "word", "word", "wor