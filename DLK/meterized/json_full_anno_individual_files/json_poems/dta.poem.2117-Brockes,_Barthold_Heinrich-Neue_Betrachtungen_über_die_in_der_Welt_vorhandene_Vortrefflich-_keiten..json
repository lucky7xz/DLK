{"dta.poem.2117": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Neue Betrachtungen \u00fcber die in der  \n Welt vorhandene Vortrefflich-  \n keiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die nimmermehr gez\u00e4hlte Menge von so viel niedlichen", "tokens": ["Die", "nim\u00b7mer\u00b7mehr", "ge\u00b7z\u00e4hl\u00b7te", "Men\u00b7ge", "von", "so", "viel", "nied\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "APPR", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-++--", "measure": "unknown.measure.septa"}, "line.2": {"text": "Gerichten,", "tokens": ["Ge\u00b7rich\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Woran sie sich vergn\u00fcgen sollen, auch von den Blumen", "tokens": ["Wo\u00b7ran", "sie", "sich", "ver\u00b7gn\u00fc\u00b7gen", "sol\u00b7len", ",", "auch", "von", "den", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "vorgestellt;", "tokens": ["vor\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Nicht minder, das so mancherley zu ihrer Lust erschaffne", "tokens": ["Nicht", "min\u00b7der", ",", "das", "so", "man\u00b7cher\u00b7ley", "zu", "ih\u00b7rer", "Lust", "er\u00b7schaff\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "$,", "PRELS", "ADV", "PIS", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Vieh,", "tokens": ["Vieh", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Der s\u00fcsse Wohllaut der Musik, und sonderlich die holden", "tokens": ["Der", "s\u00fcs\u00b7se", "Wohl\u00b7laut", "der", "Mu\u00b7sik", ",", "und", "son\u00b7der\u00b7lich", "die", "hol\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "KON", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Triebe,", "tokens": ["Trie\u00b7be", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Die L\u00fcste, die nicht auszudr\u00fccken, von einer zugela\u00dfnen", "tokens": ["Die", "L\u00fcs\u00b7te", ",", "die", "nicht", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", ",", "von", "ei\u00b7ner", "zu\u00b7ge\u00b7la\u00df\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Liebe.", "tokens": ["Lie\u00b7be", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Was meynt mein Leser, w\u00fcrden sie", "tokens": ["Was", "meynt", "mein", "Le\u00b7ser", ",", "w\u00fcr\u00b7den", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$,", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nach einem solchen Aufenthalt mit vieler Sehnsucht, ohn", "tokens": ["Nach", "ei\u00b7nem", "sol\u00b7chen", "Auf\u00b7ent\u00b7halt", "mit", "vie\u00b7ler", "Sehn\u00b7sucht", ",", "ohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "PIAT", "NN", "APPR", "PIAT", "NN", "$,", "KOUI"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Verweilen,", "tokens": ["Ver\u00b7wei\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Nach aller M\u00f6glichkeit nicht eilen?", "tokens": ["Nach", "al\u00b7ler", "M\u00f6g\u00b7lich\u00b7keit", "nicht", "ei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wir aber, die wir wirklich da, und die wir alle diese", "tokens": ["Wir", "a\u00b7ber", ",", "die", "wir", "wirk\u00b7lich", "da", ",", "und", "die", "wir", "al\u00b7le", "die\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "ADJD", "PTKVZ", "$,", "KON", "PRELS", "PPER", "PIAT", "PDS"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.16": {"text": "Gaben", "tokens": ["Ga\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "In unserm wirklichen Besitz, zu unsrer Lust, unleugbar", "tokens": ["In", "un\u00b7serm", "wirk\u00b7li\u00b7chen", "Be\u00b7sitz", ",", "zu", "uns\u00b7rer", "Lust", ",", "un\u00b7leug\u00b7bar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.18": {"text": "haben,", "tokens": ["ha\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Ja, die wir glauben, da\u00df der Sch\u00f6pfer sie uns zur Lust", "tokens": ["Ja", ",", "die", "wir", "glau\u00b7ben", ",", "da\u00df", "der", "Sch\u00f6p\u00b7fer", "sie", "uns", "zur", "Lust"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "geschenkt und wolle,", "tokens": ["ge\u00b7schenkt", "und", "wol\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VMFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Da\u00df man in seinen sch\u00f6nen Werken, zu seinem Ruhm, sich", "tokens": ["Da\u00df", "man", "in", "sei\u00b7nen", "sch\u00f6\u00b7nen", "Wer\u00b7ken", ",", "zu", "sei\u00b7nem", "Ruhm", ",", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "PRF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "freuen solle,", "tokens": ["freu\u00b7en", "sol\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Seyn, so f\u00fcr sie, als ihrem Geber, so unempfindlich als", "tokens": ["Seyn", ",", "so", "f\u00fcr", "sie", ",", "als", "ih\u00b7rem", "Ge\u00b7ber", ",", "so", "un\u00b7emp\u00b7find\u00b7lich", "als"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "$,", "ADV", "APPR", "PPER", "$,", "KOUS", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KOKOM"], "meter": "+-+--+----+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "ein Stein:", "tokens": ["ein", "Stein", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Was mu\u00df daran wohl Ursach seyn?", "tokens": ["Was", "mu\u00df", "da\u00b7ran", "wohl", "Ur\u00b7sach", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PAV", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}