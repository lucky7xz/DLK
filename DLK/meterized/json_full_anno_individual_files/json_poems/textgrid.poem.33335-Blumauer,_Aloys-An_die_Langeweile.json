"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die G\u00f6ttin, die so schnell sonst eilet,", "tokens": ["Die", "G\u00f6t\u00b7tin", ",", "die", "so", "schnell", "sonst", "ei\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die gold'ne, ach! so fl\u00fccht'ge Zeit;", "tokens": ["Die", "gold'\u00b7ne", ",", "ach", "!", "so", "fl\u00fccht'\u00b7ge", "Zeit", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ITJ", "$.", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn du auch im Himmel wohnest,", "tokens": ["Und", "wenn", "du", "auch", "im", "Him\u00b7mel", "woh\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und dort die Sterblichen belohnest,", "tokens": ["Und", "dort", "die", "Sterb\u00b7li\u00b7chen", "be\u00b7loh\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie freu'n wir uns der Ewigkeit!", "tokens": ["Wie", "freu'n", "wir", "uns", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Du lehrst des Lebens uns geniessen,", "tokens": ["Du", "lehrst", "des", "Le\u00b7