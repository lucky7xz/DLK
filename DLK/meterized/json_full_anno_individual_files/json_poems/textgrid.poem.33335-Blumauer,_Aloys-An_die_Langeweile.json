{"textgrid.poem.33335": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An die Langeweile", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unsterbliche, geliebte Sch\u00f6ne,", "tokens": ["U\u00b7nsterb\u00b7li\u00b7che", ",", "ge\u00b7lieb\u00b7te", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Bei deren Lob ich jetzt schon g\u00e4hne,", "tokens": ["Bei", "de\u00b7ren", "Lob", "ich", "jetzt", "schon", "g\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dich preise heute mein Gesang:", "tokens": ["Dich", "prei\u00b7se", "heu\u00b7te", "mein", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was uns kein Aesculap kann geben,", "tokens": ["Was", "uns", "kein", "A\u00b7e\u00b7scu\u00b7lap", "kann", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gibst du uns; denn du machst das Leben", "tokens": ["Gibst", "du", "uns", ";", "denn", "du", "machst", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "$.", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Uns bis zum Ueberdrusse lang.", "tokens": ["Uns", "bis", "zum", "Ue\u00b7berd\u00b7rus\u00b7se", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In deinem Arm allein verweilet", "tokens": ["In", "dei\u00b7nem", "Arm", "al\u00b7lein", "ver\u00b7wei\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die G\u00f6ttin, die so schnell sonst eilet,", "tokens": ["Die", "G\u00f6t\u00b7tin", ",", "die", "so", "schnell", "sonst", "ei\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die gold'ne, ach! so fl\u00fccht'ge Zeit;", "tokens": ["Die", "gold'\u00b7ne", ",", "ach", "!", "so", "fl\u00fccht'\u00b7ge", "Zeit", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ITJ", "$.", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn du auch im Himmel wohnest,", "tokens": ["Und", "wenn", "du", "auch", "im", "Him\u00b7mel", "woh\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und dort die Sterblichen belohnest,", "tokens": ["Und", "dort", "die", "Sterb\u00b7li\u00b7chen", "be\u00b7loh\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie freu'n wir uns der Ewigkeit!", "tokens": ["Wie", "freu'n", "wir", "uns", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du lehrst des Lebens uns geniessen,", "tokens": ["Du", "lehrst", "des", "Le\u00b7bens", "uns", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu deinen bleibeschwerten F\u00fcssen", "tokens": ["Zu", "dei\u00b7nen", "blei\u00b7be\u00b7schwer\u00b7ten", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "G\u00e4hnt seufzend eine halbe Welt:", "tokens": ["G\u00e4hnt", "seuf\u00b7zend", "ei\u00b7ne", "hal\u00b7be", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die g\u00f6ttlichste aus allen Gaben,", "tokens": ["Die", "g\u00f6tt\u00b7lichs\u00b7te", "aus", "al\u00b7len", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Ruhe, kann Gott selbst nicht haben,", "tokens": ["Die", "Ru\u00b7he", ",", "kann", "Gott", "selbst", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "NN", "ADV", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn er sie nicht durch dich erh\u00e4lt.", "tokens": ["Wenn", "er", "sie", "nicht", "durch", "dich", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du lehrst Sultane Bilder schnitzen,", "tokens": ["Du", "lehrst", "Sul\u00b7ta\u00b7ne", "Bil\u00b7der", "schnit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Damen ihre Zungen spitzen,", "tokens": ["Die", "Da\u00b7men", "ih\u00b7re", "Zun\u00b7gen", "spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf des N\u00e4chsten Leumund schm\u00e4h'n.", "tokens": ["Und", "auf", "des", "N\u00e4chs\u00b7ten", "Leu\u00b7mund", "schm\u00e4h'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, deine sch\u00f6nen Siegstroph\u00e4en", "tokens": ["Ja", ",", "dei\u00b7ne", "sch\u00f6\u00b7nen", "Siegs\u00b7trop\u00b7h\u00e4\u00b7en"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kann man in allen Assembleen", "tokens": ["Kann", "man", "in", "al\u00b7len", "As\u00b7sem\u00b7bleen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An hundert offnen M\u00e4ulern seh'n.", "tokens": ["An", "hun\u00b7dert", "off\u00b7nen", "M\u00e4u\u00b7lern", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der M\u00f6nch auf seinem harten Brette,", "tokens": ["Der", "M\u00f6nch", "auf", "sei\u00b7nem", "har\u00b7ten", "Bret\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Abt auf seinem Flaumenbette,", "tokens": ["Der", "Abt", "auf", "sei\u00b7nem", "Flau\u00b7men\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Umarmen gleich inbr\u00fcnstig dich,", "tokens": ["Um\u00b7ar\u00b7men", "gleich", "in\u00b7br\u00fcns\u00b7tig", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und manche Prediger ereifern", "tokens": ["Und", "man\u00b7che", "Pre\u00b7di\u00b7ger", "er\u00b7ei\u00b7fern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf unsern Kanzeln bis zum Geifern", "tokens": ["Auf", "un\u00b7sern", "Kan\u00b7zeln", "bis", "zum", "Gei\u00b7fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Allein f\u00fcr deine Ehre sich.", "tokens": ["Al\u00b7lein", "f\u00fcr", "dei\u00b7ne", "Eh\u00b7re", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du thronst auf grossen Folianten:", "tokens": ["Du", "thronst", "auf", "gros\u00b7sen", "Fo\u00b7li\u00b7an\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ungeheures Heer Pedanten", "tokens": ["Ein", "un\u00b7ge\u00b7heu\u00b7res", "Heer", "Pe\u00b7dan\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Steht immerdar in deinem Sold,", "tokens": ["Steht", "im\u00b7mer\u00b7dar", "in", "dei\u00b7nem", "Sold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ach, du lieber Gott! was th\u00e4ten", "tokens": ["Und", "ach", ",", "du", "lie\u00b7ber", "Gott", "!", "was", "th\u00e4\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "XY", "$,", "PPER", "ADV", "NN", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Romanenschreiber und Poeten,", "tokens": ["Ro\u00b7ma\u00b7nen\u00b7schrei\u00b7ber", "und", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00e4rst du nicht auch den Schluckern hold?", "tokens": ["W\u00e4rst", "du", "nicht", "auch", "den", "Schlu\u00b7ckern", "hold", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du wohnst in pr\u00e4chtigen Pall\u00e4sten,", "tokens": ["Du", "wohnst", "in", "pr\u00e4ch\u00b7ti\u00b7gen", "Pal\u00b7l\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du pr\u00e4sidirst bei allen Festen,", "tokens": ["Du", "pr\u00e4\u00b7si\u00b7dirst", "bei", "al\u00b7len", "Fes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die man an F\u00fcrstenh\u00f6fen h\u00e4lt;", "tokens": ["Die", "man", "an", "F\u00fcrs\u00b7ten\u00b7h\u00f6\u00b7fen", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und o! die Grossen dieser Erde,", "tokens": ["Und", "o", "!", "die", "Gros\u00b7sen", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "$.", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was h\u00e4tten sie wohl f\u00fcr Beschwerde,", "tokens": ["Was", "h\u00e4t\u00b7ten", "sie", "wohl", "f\u00fcr", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00e4rst du's nicht, was sie manchmal qu\u00e4lt?", "tokens": ["W\u00e4rst", "du's", "nicht", ",", "was", "sie", "manch\u00b7mal", "qu\u00e4lt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Von dir begeistert, weist die Sch\u00f6ne", "tokens": ["Von", "dir", "be\u00b7geis\u00b7tert", ",", "weist", "die", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Stutzer ihre weissen Z\u00e4hne,", "tokens": ["Dem", "Stut\u00b7zer", "ih\u00b7re", "weis\u00b7sen", "Z\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und g\u00e4hnet ihn extasisch an:", "tokens": ["Und", "g\u00e4h\u00b7net", "ihn", "ex\u00b7ta\u00b7sisch", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du h\u00fcllst dich in die reichsten Kleider,", "tokens": ["Du", "h\u00fcllst", "dich", "in", "die", "reichs\u00b7ten", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und nur zu oft trifft man dich, leider!", "tokens": ["Und", "nur", "zu", "oft", "trifft", "man", "dich", ",", "lei\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADV", "VVFIN", "PIS", "PPER", "$,", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf sch\u00f6nen M\u00e4dchenlippen an.", "tokens": ["Auf", "sch\u00f6\u00b7nen", "M\u00e4d\u00b7chen\u00b7lip\u00b7pen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Um deine Freundin Zeit zu t\u00f6dten", "tokens": ["Um", "dei\u00b7ne", "Freun\u00b7din", "Zeit", "zu", "t\u00f6d\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erfand man zwar in grossen St\u00e4dten", "tokens": ["Er\u00b7fand", "man", "zwar", "in", "gros\u00b7sen", "St\u00e4d\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Spektakel, Feuerwerk und Spiel;", "tokens": ["Spek\u00b7ta\u00b7kel", ",", "Feu\u00b7er\u00b7werk", "und", "Spiel", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allein man g\u00e4hnet bei Raketten,", "tokens": ["Al\u00b7lein", "man", "g\u00e4h\u00b7net", "bei", "Ra\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bei Trauerspielen, Operetten,", "tokens": ["Bei", "Trau\u00b7er\u00b7spie\u00b7len", ",", "O\u00b7per\u00b7et\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wie bei Lomber und Quadrill,", "tokens": ["So", "wie", "bei", "Lom\u00b7ber", "und", "Quad\u00b7rill", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Ja selbst in diesem Augenblicke", "tokens": ["Ja", "selbst", "in", "die\u00b7sem", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beweist zu meiner Leier Gl\u00fccke", "tokens": ["Be\u00b7weist", "zu", "mei\u00b7ner", "Lei\u00b7er", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich deine grosse Macht an mir:", "tokens": ["Sich", "dei\u00b7ne", "gros\u00b7se", "Macht", "an", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn dieses Loblied, das ich singe,", "tokens": ["Denn", "die\u00b7ses", "Lob\u00b7lied", ",", "das", "ich", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und das ich dir zum Opfer bringe,", "tokens": ["Und", "das", "ich", "dir", "zum", "Op\u00b7fer", "brin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sing' ich aus langer Weile dir.", "tokens": ["Sing'", "ich", "aus", "lan\u00b7ger", "Wei\u00b7le", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Unsterbliche, geliebte Sch\u00f6ne,", "tokens": ["U\u00b7nsterb\u00b7li\u00b7che", ",", "ge\u00b7lieb\