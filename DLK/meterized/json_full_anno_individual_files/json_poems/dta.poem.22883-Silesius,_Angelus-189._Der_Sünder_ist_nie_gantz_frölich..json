{"dta.poem.22883": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "189. Der S\u00fcnder ist nie gantz fr\u00f6lich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Die S\u00fcnder ob sie gleich in lauter Freude leben/", "tokens": ["Die", "S\u00fcn\u00b7der", "ob", "sie", "gleich", "in", "lau\u00b7ter", "Freu\u00b7de", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "PPER", "ADV", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00df doch jhre Seel inn gr\u00f6sten Furchten schwebe\u0303.", "tokens": ["So", "mu\u00df", "doch", "jhre", "Seel", "inn", "gr\u00f6s\u00b7ten", "Furch\u00b7ten", "schwe\u00b7b\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}}}}