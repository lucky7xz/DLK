{"dta.poem.1689": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Friedlich ists am besten leben:", "tokens": ["Fried\u00b7lich", "ists", "am", "bes\u00b7ten", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKA", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ins gemein verliert nicht viel", "tokens": ["Ins", "ge\u00b7mein", "ver\u00b7liert", "nicht", "viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "VVFIN", "PTKNEG", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer mit Schl\u00e4gen handeln will/", "tokens": ["Wer", "mit", "Schl\u00e4\u00b7gen", "han\u00b7deln", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was er einfach ausgegeben/", "tokens": ["Was", "er", "ein\u00b7fach", "aus\u00b7ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt ihm \u00fcber Haubt und Glieder", "tokens": ["Kommt", "ihm", "\u00fc\u00b7ber", "Haubt", "und", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Offt mit reichem Wucher wieder.", "tokens": ["Offt", "mit", "rei\u00b7chem", "Wu\u00b7cher", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch ist Friede nur zu f\u00fchren", "tokens": ["Doch", "ist", "Frie\u00b7de", "nur", "zu", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "NN", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil der stoltze Nachbar will/", "tokens": ["Weil", "der", "stolt\u00b7ze", "Nach\u00b7bar", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ehr und Auge leidt nicht viel", "tokens": ["Ehr", "und", "Au\u00b7ge", "leidt", "nicht", "viel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJD", "PTKNEG", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und ist schwerlich zu curiren/", "tokens": ["Und", "ist", "schwer\u00b7lich", "zu", "cu\u00b7ri\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Soltu f\u00fcr die Ehre streiten", "tokens": ["Sol\u00b7tu", "f\u00fcr", "die", "Eh\u00b7re", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Thu es hurtig und bey Zeiten.", "tokens": ["Thu", "es", "hur\u00b7tig", "und", "bey", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}