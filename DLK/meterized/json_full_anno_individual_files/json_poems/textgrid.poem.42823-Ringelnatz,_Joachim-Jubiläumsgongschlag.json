{"textgrid.poem.42823": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Jubil\u00e4umsgongschlag", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es tarnt das Gong.", "tokens": ["Es", "tarnt", "das", "Gong", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Herein, ihr G\u00e4ste,", "tokens": ["Her\u00b7ein", ",", "ihr", "G\u00e4s\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "In meinen Salon", "tokens": ["In", "mei\u00b7nen", "Sa\u00b7lon"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Zu meinem Feste!", "tokens": ["Zu", "mei\u00b7nem", "Fes\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Es gibt heut das Beste,", "tokens": ["Es", "gibt", "heut", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Was ich zu erschwingen vermag,", "tokens": ["Was", "ich", "zu", "er\u00b7schwin\u00b7gen", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Gibt Weine und Sekt \u2013", "tokens": ["Gibt", "Wei\u00b7ne", "und", "Sekt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Tamtam \u2013 Schlag auf Schlag \u2013", "tokens": ["Tam\u00b7tam", "\u2013", "Schlag", "auf", "Schlag", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "APPR", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Sogar etwas, was wie Kaviar schmeckt.", "tokens": ["So\u00b7gar", "et\u00b7was", ",", "was", "wie", "Ka\u00b7vi\u00b7ar", "schmeckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es gibt heute faustgro\u00dfe", "tokens": ["Es", "gibt", "heu\u00b7te", "faust\u00b7gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJA"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.11": {"text": "Eier mit Senf sauce", "tokens": ["Ei\u00b7er", "mit", "Senf", "sau\u00b7ce"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NE"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Und s\u00fc\u00dfe \u00e0 la bonbon.", "tokens": ["Und", "s\u00fc\u00b7\u00dfe", "\u00e0", "la", "bon\u00b7bon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Es gibt sogar Schweinebraten.", "tokens": ["Es", "gibt", "so\u00b7gar", "Schwei\u00b7ne\u00b7bra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Heut geb ich gern, heut geb ich viel.", "tokens": ["Heut", "geb", "ich", "gern", ",", "heut", "geb", "ich", "viel", "."], "token_info": ["word", "word", "word", "word