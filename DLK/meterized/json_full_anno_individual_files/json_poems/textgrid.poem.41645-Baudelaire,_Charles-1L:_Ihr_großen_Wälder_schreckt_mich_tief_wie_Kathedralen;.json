{"textgrid.poem.41645": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr gro\u00dfen W\u00e4lder schreckt mich tief wie Kathedralen;", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr gro\u00dfen W\u00e4lder schreckt mich tief wie Kathedralen;", "tokens": ["Ihr", "gro\u00b7\u00dfen", "W\u00e4l\u00b7der", "schreckt", "mich", "tief", "wie", "Ka\u00b7thed\u00b7ra\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr braust wie Orgeln, und in unsren Herzen all,", "tokens": ["Ihr", "braust", "wie", "Or\u00b7geln", ",", "und", "in", "un\u00b7sren", "Her\u00b7zen", "all", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "$,", "KON", "APPR", "PPOSAT", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Grabkammern ewgen Leids, voll R\u00f6chelns alter Qualen,", "tokens": ["Grab\u00b7kam\u00b7mern", "ew\u00b7gen", "Leids", ",", "voll", "R\u00f6\u00b7chelns", "al\u00b7ter", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ADJD", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Antwortet eures De profundis Widerhall.", "tokens": ["Ant\u00b7wor\u00b7tet", "eu\u00b7res", "De", "pro\u00b7fun\u00b7dis", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "++-+