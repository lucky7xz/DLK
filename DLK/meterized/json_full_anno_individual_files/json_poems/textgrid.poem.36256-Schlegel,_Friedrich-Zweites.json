{"textgrid.poem.36256": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zweites", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die hellen Blitze h\u00e4tten uns geblendet", "tokens": ["Die", "hel\u00b7len", "Blit\u00b7ze", "h\u00e4t\u00b7ten", "uns", "ge\u00b7blen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Auges, das kein Nebel noch betrogen,", "tokens": ["Des", "Au\u00b7ges", ",", "das", "kein", "Ne\u00b7bel", "noch", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Anmut selbst den Umri\u00df nicht gezogen,", "tokens": ["Wenn", "An\u00b7mut", "selbst", "den", "Um\u00b7ri\u00df", "nicht", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und jedes L\u00e4cheln um den Mund verschwendet.", "tokens": ["Und", "je\u00b7des", "L\u00e4\u00b7cheln", "um", "den", "Mund", "ver\u00b7schwen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem Himmel scheint der Mienen Spiel entwendet,", "tokens": ["Dem", "Him\u00b7mel", "scheint", "der", "Mie\u00b7nen", "Spiel", "ent\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das, wie Musik enteilt, auf schnellen Wogen,", "tokens": ["Das", ",", "wie", "Mu\u00b7sik", "ent\u00b7eilt", ",", "auf", "schnel\u00b7len", "Wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem ird'schen Blicke oft zu rasch entflogen,", "tokens": ["Dem", "ird'\u00b7schen", "Bli\u00b7cke", "oft", "zu", "rasch", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eh' er dem Scherz die Freude nachgesendet.", "tokens": ["Eh'", "er", "dem", "Scherz", "die", "Freu\u00b7de", "nach\u00b7ge\u00b7sen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer sieht den Mund nicht leise spottend fragen?", "tokens": ["Wer", "sieht", "den", "Mund", "nicht", "lei\u00b7se", "spot\u00b7tend", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer w\u00e4hnt, da\u00df er dem Auge sich verh\u00fclle?", "tokens": ["Wer", "w\u00e4hnt", ",", "da\u00df", "er", "dem", "Au\u00b7ge", "sich", "ver\u00b7h\u00fcl\u00b7le", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer m\u00f6chte dieser Stirn nicht Kr\u00e4nze winden?", "tokens": ["Wer", "m\u00f6ch\u00b7te", "die\u00b7ser", "Stirn", "nicht", "Kr\u00e4n\u00b7ze", "win\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDAT", "NN", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ob sich nur Freude kindlich hier enth\u00fclle,", "tokens": ["Ob", "sich", "nur", "Freu\u00b7de", "kind\u00b7lich", "hier", "ent\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "NN", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob zarte Geister neckend selbst sich plagen,", "tokens": ["Ob", "zar\u00b7te", "Geis\u00b7ter", "ne\u00b7ckend", "selbst", "sich", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Darauf wird keiner wohl die Antwort finden.", "tokens": ["Da\u00b7rauf", "wird", "kei\u00b7ner", "wohl", "die", "Ant\u00b7wort", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die hellen Blitze h\u00e4tten uns geblendet", "tokens": ["Die", "hel\u00b7len", "Blit\u00b7ze", "h\u00e4t\u00b7ten", "uns", "ge\u00b7blen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP"], "meter":