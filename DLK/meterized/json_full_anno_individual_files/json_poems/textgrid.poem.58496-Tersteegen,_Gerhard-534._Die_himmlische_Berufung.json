{"textgrid.poem.58496": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "534. Die himmlische Berufung", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel ruft; weg, Fleisch, weg, Erde,", "tokens": ["Der", "Him\u00b7mel", "ruft", ";", "weg", ",", "Fleisch", ",", "weg", ",", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PTKVZ", "$,", "NN", "$,", "PTKVZ", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Ruf will, da\u00df ich himmlisch werde,", "tokens": ["Mein", "Ruf", "will", ",", "da\u00df", "ich", "himm\u00b7lisch", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Gott ist hier, ich beuge mich!", "tokens": ["Mein", "Gott", "ist", "hier", ",", "ich", "beu\u00b7ge", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will mein All's, mein Liebstes schenken;", "tokens": ["Ich", "will", "mein", "All's", ",", "mein", "Liebs\u00b7tes", "schen\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Still, eignes Wollen, Wirken, Denken,", "tokens": ["Still", ",", "eig\u00b7nes", "Wol\u00b7len", ",", "Wir\u00b7ken",