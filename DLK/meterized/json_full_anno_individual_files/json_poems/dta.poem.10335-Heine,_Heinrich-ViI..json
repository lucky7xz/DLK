{"dta.poem.10335": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Berg' und Burgen schau'n herunter", "tokens": ["Ber\u00b7g'", "und", "Bur\u00b7gen", "schau'n", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVFIN", "PTKVZ"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In den spiegelhellen Rhein,", "tokens": ["In", "den", "spie\u00b7gel\u00b7hel\u00b7len", "Rhein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Schiffchen segelt munter,", "tokens": ["Und", "mein", "Schiff\u00b7chen", "se\u00b7gelt", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rings umgl\u00e4nzt von Sonnenschein.", "tokens": ["Rings", "um\u00b7gl\u00e4nzt", "von", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ruhig seh' ich zu dem Spiele", "tokens": ["Ru\u00b7hig", "seh'", "ich", "zu", "dem", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldner Wellen, kraus bewegt;", "tokens": ["Gold\u00b7ner", "Wel\u00b7len", ",", "kraus", "be\u00b7wegt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Still erwachen die Gef\u00fchle,", "tokens": ["Still", "er\u00b7wa\u00b7chen", "die", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ich tief im Busen hegt'.", "tokens": ["Die", "ich", "tief", "im", "Bu\u00b7sen", "hegt'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freundlich gr\u00fc\u00dfend und verhei\u00dfend", "tokens": ["Freund\u00b7lich", "gr\u00fc\u00b7\u00dfend", "und", "ver\u00b7hei\u00b7\u00dfend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockt hinab des Stromes Pracht;", "tokens": ["Lockt", "hin\u00b7ab", "des", "Stro\u00b7mes", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ich kenn' ihn, oben glei\u00dfend,", "tokens": ["Doch", "ich", "kenn'", "ihn", ",", "o\u00b7ben", "glei\u00b7\u00dfend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringt sein Inn'res Tod und Nacht.", "tokens": ["Bringt", "sein", "Inn'\u00b7res", "Tod", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Oben Lust, im Busen T\u00fccken,", "tokens": ["O\u00b7ben", "Lust", ",", "im", "Bu\u00b7sen", "T\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strom, du bist der Liebsten Bild!", "tokens": ["Strom", ",", "du", "bist", "der", "Liebs\u00b7ten", "Bild", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die kann auch so freundlich nicken,", "tokens": ["Die", "kann", "auch", "so", "freund\u00b7lich", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "L\u00e4chelt auch so fromm und mild.", "tokens": ["L\u00e4\u00b7chelt", "auch", "so", "fromm", "und", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}