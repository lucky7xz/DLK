{"textgrid.poem.44013": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott, de\u00dfen Nahmen schon die F\u00fclle", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, de\u00dfen Nahmen schon die F\u00fclle", "tokens": ["Gott", ",", "de\u00b7\u00dfen", "Nah\u00b7men", "schon", "die", "F\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELAT", "NN", "ADV", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vollkommner Herrligkeit entdeckt,", "tokens": ["Voll\u00b7komm\u00b7ner", "Herr\u00b7lig\u00b7keit", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erh\u00f6r mein Herz, das in der Stille", "tokens": ["Er\u00b7h\u00f6r", "mein", "Herz", ",", "das", "in", "der", "Stil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fr\u00fcchte deiner Liebe schmeckt", "tokens": ["Die", "Fr\u00fcch\u00b7te", "dei\u00b7ner", "Lie\u00b7be", "schmeckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und das auch schon im Glauben f\u00fchlt,", "tokens": ["Und", "das", "auch", "schon", "im", "Glau\u00b7ben", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie g\u00fctig deine Vorsicht spielt.", "tokens": ["Wie", "g\u00fc\u00b7tig", "dei\u00b7ne", "Vor\u00b7sicht", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du hast und giebst den rechten Frieden;", "tokens": ["Du", "hast", "und", "giebst", "den", "rech\u00b7ten", "Frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ach gieb ihn mehr und mehr auch mir", "tokens": ["Ach", "gieb", "ihn", "mehr", "und", "mehr", "auch", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "KON", "ADV", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und zwing, wenn Geist und Fleisch erm\u00fcden,", "tokens": ["Und", "zwing", ",", "wenn", "Geist", "und", "Fleisch", "er\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welt, S\u00fcnde, Satan und Begier.", "tokens": ["Welt", ",", "S\u00fcn\u00b7de", ",", "Sa\u00b7tan", "und", "Be\u00b7gier", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du weist, wie starck die Feinde sind,", "tokens": ["Du", "weist", ",", "wie", "starck", "die", "Fein\u00b7de", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum st\u00e4rcke, bis mein Kampf gewinnt.", "tokens": ["Drum", "st\u00e4r\u00b7cke", ",", "bis", "mein", "Kampf", "ge\u00b7winnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Hofnung l\u00e4st den Muth nicht sincken,", "tokens": ["Die", "Hof\u00b7nung", "l\u00e4st", "den", "Muth", "nicht", "sin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kennt die Gr\u00f6\u00dfe deiner Treu;", "tokens": ["Sie", "kennt", "die", "Gr\u00f6\u00b7\u00dfe", "dei\u00b7ner", "Treu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du magst mich ziehn, du darfst nur wincken,", "tokens": ["Du", "magst", "mich", "ziehn", ",", "du", "darfst", "nur", "win\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So folg ich, wie und wo es sey,", "tokens": ["So", "folg", "ich", ",", "wie", "und", "wo", "es", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "KON", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nachdem du t\u00e4glich Zeugn\u00fc\u00df giebst,", "tokens": ["Nach\u00b7dem", "du", "t\u00e4g\u00b7lich", "Zeug\u00b7n\u00fc\u00df", "giebst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sehr du deine Menschen liebst.", "tokens": ["Wie", "sehr", "du", "dei\u00b7ne", "Men\u00b7schen", "liebst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du hast vor uns verirrte Schaafe", "tokens": ["Du", "hast", "vor", "uns", "ver\u00b7irr\u00b7te", "Schaa\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Hirten selbst ins Grab gesteckt", "tokens": ["Den", "Hir\u00b7ten", "selbst", "ins", "Grab", "ge\u00b7steckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und uns zugleich vom S\u00fcndenschlafe", "tokens": ["Und", "uns", "zu\u00b7gleich", "vom", "S\u00fcn\u00b7den\u00b7schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ihn von Todten auferweckt;", "tokens": ["Wie", "ihn", "von", "Tod\u00b7ten", "au\u00b7fer\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies Blut, dies theure Testament,", "tokens": ["Dies", "Blut", ",", "dies", "theu\u00b7re", "Tes\u00b7ta\u00b7ment", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Macht, da\u00df dein Kind uns Bruder nennt.", "tokens": ["Macht", ",", "da\u00df", "dein", "Kind", "uns", "Bru\u00b7der", "nennt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Welch Dancklied kan vor solche Gnade", "tokens": ["Welch", "Danck\u00b7lied", "kan", "vor", "sol\u00b7che", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein rein und w\u00fcrdig Opfer seyn?", "tokens": ["Ein", "rein", "und", "w\u00fcr\u00b7dig", "Op\u00b7fer", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir treten vor die Bundeslade", "tokens": ["Wir", "tre\u00b7ten", "vor", "die", "Bun\u00b7des\u00b7la\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wollen dir Gehorsam weihn.", "tokens": ["Und", "wol\u00b7len", "dir", "Ge\u00b7hor\u00b7sam", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Wandel, der dir wohl gef\u00e4llt,", "tokens": ["Ein", "Wan\u00b7del", ",", "der", "dir", "wohl", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist doch das reichste Wiedergelt.", "tokens": ["Ist", "doch", "das", "reichs\u00b7te", "Wie\u00b7der\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch ohne deine Kraft zu wandeln,", "tokens": ["Doch", "oh\u00b7ne", "dei\u00b7ne", "Kraft", "zu", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heist nur in Nacht und Nebel gehn;", "tokens": ["Heist", "nur", "in", "Nacht", "und", "Ne\u00b7bel", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Fertigkeit, stets recht zu handeln,", "tokens": ["Die", "Fer\u00b7tig\u00b7keit", ",", "stets", "recht", "zu", "han\u00b7deln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df blos von deiner H\u00fclf entstehn.", "tokens": ["Mu\u00df", "blos", "von", "dei\u00b7ner", "H\u00fclf", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die H\u00fclf erscheinet aufs Gebeth,", "tokens": ["Die", "H\u00fclf", "er\u00b7schei\u00b7net", "aufs", "Ge\u00b7beth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das voller Reu um Be\u00dfrung fleht.", "tokens": ["Das", "vol\u00b7ler", "Reu", "um", "Be\u00df\u00b7rung", "fleht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Verleih mir Andacht, Wiz und St\u00e4rcke,", "tokens": ["Ver\u00b7leih", "mir", "An\u00b7dacht", ",", "Wiz", "und", "St\u00e4r\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was dir gef\u00e4llt, wohl einzusehn,", "tokens": ["Was", "dir", "ge\u00b7f\u00e4llt", ",", "wohl", "ein\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und las von mir in jedem Wercke", "tokens": ["Und", "las", "von", "mir", "in", "je\u00b7dem", "Wer\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blos deinem Willen gnug geschehn;", "tokens": ["Blos", "dei\u00b7nem", "Wil\u00b7len", "gnug", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gieb, da\u00df ich sonder Heucheley", "tokens": ["Gieb", ",", "da\u00df", "ich", "son\u00b7der", "Heu\u00b7che\u00b7ley"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir und dem Nechsten dienstbahr sey.", "tokens": ["Dir", "und", "dem", "Nechs\u00b7ten", "dienst\u00b7bahr", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "PDS", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Dein Sohn, mein Jesus, hilft vollbringen,", "tokens": ["Dein", "Sohn", ",", "mein", "Je\u00b7sus", ",", "hilft", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NE", "$,", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das macht sein Amt und Priesterthum;", "tokens": ["Das", "macht", "sein", "Amt", "und", "Pries\u00b7ter\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm soll mein Hosianna klingen.", "tokens": ["Ihm", "soll", "mein", "Ho\u00b7si\u00b7an\u00b7na", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein ist der Sieg, sein ist der Ruhm,", "tokens": ["Mein", "ist", "der", "Sieg", ",", "sein", "ist", "der", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "VAINF", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier heist es Amen in der Zeit,", "tokens": ["Hier", "heist", "es", "A\u00b7men", "in", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dort Heilig in der Ewigkeit.", "tokens": ["Dort", "Hei\u00b7lig", "in", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Gott, de\u00dfen Nahmen schon die F\u00fclle", "tokens": ["Gott", ",", "de\u00b7\u00dfen", "Nah\u00b7men", "schon", "die", "F\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELAT", "NN", "ADV", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Vollkommner Herrligkeit entdeckt,", "tokens": ["Voll\u00b7komm\u00b7ner", "Herr\u00b7lig\u00b7keit", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erh\u00f6r mein Herz, das in der Stille", "tokens": ["Er\u00b7h\u00f6r", "mein", "Herz", ",", "das", "in", "der", "Stil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fr\u00fcchte deiner Liebe schmeckt", "tokens": ["Die", "Fr\u00fcch\u00b7te", "dei\u00b7ner", "Lie\u00b7be", "schmeckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und das auch schon im Glauben f\u00fchlt,", "tokens": ["Und", "das", "auch", "schon", "im", "Glau\u00b7ben", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "ia