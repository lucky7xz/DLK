 "pos": ["PIS", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singen Taboritench\u00f6re;", "tokens": ["Sin\u00b7gen", "Ta\u00b7bo\u00b7ri\u00b7ten\u00b7ch\u00f6\u00b7re", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ziska harrt des Morgengrauens", "tokens": ["Zis\u00b7ka", "harrt", "des", "Mor\u00b7gen\u00b7grau\u00b7ens"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter einer alten F\u00f6hre.", "tokens": ["Un\u00b7ter", "ei\u00b7ner", "al\u00b7ten", "F\u00f6h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}