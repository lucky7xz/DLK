{"textgrid.poem.60464": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer zum heilgen Kampf berufen,", "tokens": ["Wer", "zum", "heil\u00b7gen", "Kampf", "be\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist gl\u00fcckselig dann zu preisen,", "tokens": ["Ist", "gl\u00fcck\u00b7se\u00b7lig", "dann", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn vor sich er seinen Feind hat,", "tokens": ["Wenn", "vor", "sich", "er", "sei\u00b7nen", "Feind", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRF", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Draufzuschlagen mit dem Eisen;", "tokens": ["Drauf\u00b7zu\u00b7schla\u00b7gen", "mit", "dem", "Ei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer nicht streitet nur mit Worten,", "tokens": ["Wer", "nicht", "strei\u00b7tet", "nur", "mit", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die er zweifelnd mu\u00df vertrauen", "tokens": ["Die", "er", "zwei\u00b7felnd", "mu\u00df", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Windeslaunen, Wetterlaunen;", "tokens": ["Win\u00b7des\u00b7lau\u00b7nen", ",", "Wet\u00b7ter\u00b7lau\u00b7nen", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer da wei\u00df, wohin zu hauen.", "tokens": ["Wer", "da", "wei\u00df", ",", "wo\u00b7hin", "zu", "hau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PWAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ziska, wildbeherzter B\u00f6hme!", "tokens": ["Zis\u00b7ka", ",", "wild\u00b7be\u00b7herz\u00b7ter", "B\u00f6h\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwinge fr\u00f6hlich Lanz und Keule!", "tokens": ["Schwin\u00b7ge", "fr\u00f6h\u00b7lich", "Lanz", "und", "Keu\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00fcrgen sind dir deines Wirkens", "tokens": ["B\u00fcr\u00b7gen", "sind", "dir", "dei\u00b7nes", "Wir\u00b7kens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Str\u00f6me Bluts und Sterbgeheule. \u2013", "tokens": ["Str\u00f6\u00b7me", "Bluts", "und", "Sterb\u00b7ge\u00b7heu\u00b7le", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wieder hat er, Tod vergeudend,", "tokens": ["Wie\u00b7der", "hat", "er", ",", "Tod", "ver\u00b7geu\u00b7dend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Tag hindurch geschlagen,", "tokens": ["Ei\u00b7nen", "Tag", "hin\u00b7durch", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6chte in der Nacht und K\u00fchle", "tokens": ["M\u00f6ch\u00b7te", "in", "der", "Nacht", "und", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiter fechten mit Behagen.", "tokens": ["Wei\u00b7ter", "fech\u00b7ten", "mit", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Vorw\u00e4rts treibt er seine Scharen", "tokens": ["Vor\u00b7w\u00e4rts", "treibt", "er", "sei\u00b7ne", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den nachtverh\u00fcllten Pfaden,", "tokens": ["Auf", "den", "nacht\u00b7ver\u00b7h\u00fcll\u00b7ten", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um der Freiheit, seinem Liebchen,", "tokens": ["Um", "der", "Frei\u00b7heit", ",", "sei\u00b7nem", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufzuspielen Serenaden", "tokens": ["Auf\u00b7zu\u00b7spie\u00b7len", "Se\u00b7re\u00b7na\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit der Feldschlacht, seiner Orgel,", "tokens": ["Mit", "der", "Feld\u00b7schlacht", ",", "sei\u00b7ner", "Or\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die er wei\u00df so stark zu greifen;", "tokens": ["Die", "er", "wei\u00df", "so", "stark", "zu", "grei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfaffenvolk und F\u00fcrstenknechte", "tokens": ["Pfaf\u00b7fen\u00b7volk", "und", "F\u00fcrs\u00b7ten\u00b7knech\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind die gellen Orgelpfeifen.", "tokens": ["Sind", "die", "gel\u00b7len", "Or\u00b7gel\u00b7pfei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch es dunkelt tiefer immer", "tokens": ["Doch", "es", "dun\u00b7kelt", "tie\u00b7fer", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein Gewitter in die Schlucht,", "tokens": ["Ein", "Ge\u00b7wit\u00b7ter", "in", "die", "Schlucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur zuweilen \u00fcbers Tal weg", "tokens": ["Nur", "zu\u00b7wei\u00b7len", "\u00fc\u00b7bers", "Tal", "weg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Setzt ein Blitz in wilder Flucht.", "tokens": ["Setzt", "ein", "Blitz", "in", "wil\u00b7der", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hemmend lagert sich das Dunkel", "tokens": ["Hem\u00b7mend", "la\u00b7gert", "sich", "das", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Wagenburg, die Rosse,", "tokens": ["Um", "die", "Wa\u00b7gen\u00b7burg", ",", "die", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Geschirr' im Winde rasseln", "tokens": ["Die", "Ge\u00b7schirr'", "im", "Win\u00b7de", "ras\u00b7seln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die B\u00fcndel der Geschosse.", "tokens": ["Und", "die", "B\u00fcn\u00b7del", "der", "Ge\u00b7schos\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ziska spricht: \u00bbO wie so fl\u00fcchtig", "tokens": ["Zis\u00b7ka", "spricht", ":", "\u00bb", "O", "wie", "so", "fl\u00fcch\u00b7tig"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "$(", "NE", "KOKOM", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser sch\u00f6ne Blitz entf\u00e4hrt!", "tokens": ["Die\u00b7ser", "sch\u00f6\u00b7ne", "Blitz", "ent\u00b7f\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt ich doch hier an die Tanne", "tokens": ["K\u00f6nnt", "ich", "doch", "hier", "an", "die", "Tan\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nageln ihn mit meinem Schwert!", "tokens": ["Na\u00b7geln", "ihn", "mit", "mei\u00b7nem", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Da\u00df ich Gottes Welt befreie,", "tokens": ["Da\u00df", "ich", "Got\u00b7tes", "Welt", "be\u00b7frei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zahle heim die Racheschuld,", "tokens": ["Zah\u00b7le", "heim", "die", "Ra\u00b7ch\u00b7e\u00b7schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Br\u00fcder, k\u00f6nnt euch doch das Feuer", "tokens": ["Br\u00fc\u00b7der", ",", "k\u00f6nnt", "euch", "doch", "das", "Feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leuchten meiner Ungeduld!\u00ab \u2013", "tokens": ["Leuch\u00b7ten", "mei\u00b7ner", "Un\u00b7ge\u00b7duld", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ha! ein Blitz, ein sonnenheller!", "tokens": ["Ha", "!", "ein", "Blitz", ",", "ein", "son\u00b7nen\u00b7hel\u00b7ler", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herrlich strahlen aus der Nacht", "tokens": ["Herr\u00b7lich", "strah\u00b7len", "aus", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Hussiten Schreckgestalten,", "tokens": ["Der", "Hus\u00b7si\u00b7ten", "Schreck\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ziskas Herz in Freude lacht.", "tokens": ["Zis\u00b7kas", "Herz", "in", "Freu\u00b7de", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Donner rollen, fern verhallend,", "tokens": ["Don\u00b7ner", "rol\u00b7len", ",", "fern", "ver\u00b7hal\u00b7lend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus des Himmels tiefster Brust,", "tokens": ["Aus", "des", "Him\u00b7mels", "tiefs\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem Gewitter lauscht der Feldherr,", "tokens": ["Dem", "Ge\u00b7wit\u00b7ter", "lauscht", "der", "Feld\u00b7herr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nachtgebannt, mit Neideslust:", "tokens": ["Nacht\u00b7ge\u00b7bannt", ",", "mit", "Nei\u00b7des\u00b7lust", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "\u00bbk\u00f6nnt ich fliegen wie die Wolken,", "tokens": ["\u00bb", "k\u00f6nnt", "ich", "flie\u00b7gen", "wie", "die", "Wol\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nachts in ungehemmter Eile!", "tokens": ["Nachts", "in", "un\u00b7ge\u00b7hemm\u00b7ter", "Ei\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt ich auf verschanzte S\u00fcnder", "tokens": ["K\u00f6nnt", "ich", "auf", "ver\u00b7schanz\u00b7te", "S\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schie\u00dfen meine Todeskeile!\u00ab \u2013", "tokens": ["Schie\u00b7\u00dfen", "mei\u00b7ne", "To\u00b7des\u00b7kei\u00b7le", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Festgekoppelt stehn die Rosse,", "tokens": ["Fest\u00b7ge\u00b7kop\u00b7pelt", "stehn", "die", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stampfend im Gewitterregen,", "tokens": ["Stamp\u00b7fend", "im", "Ge\u00b7wit\u00b7ter\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Manche Streiter, schlachterm\u00fcdet,", "tokens": ["Man\u00b7che", "Strei\u00b7ter", ",", "schlach\u00b7ter\u00b7m\u00fc\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schnarchen unter ihren W\u00e4gen;", "tokens": ["Schnar\u00b7chen", "un\u00b7ter", "ih\u00b7ren", "W\u00e4\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Andre lagernd im Geb\u00fcsche", "tokens": ["And\u00b7re", "la\u00b7gernd", "im", "Ge\u00b7b\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singen Taboritench\u00f6re;", "tokens": ["Sin\u00b7gen", "Ta\u00b7bo\u00b7ri\u00b7ten\u00b7ch\u00f6\u00b7re", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ziska harrt des Morgengrauens", "tokens": ["Zis\u00b7ka", "harrt", "des", "Mor\u00b7gen\u00b7grau\u00b7ens"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter einer alten F\u00f6hre.", "tokens": ["Un\u00b7ter", "ei\u00b7ner", "al\u00b7ten", "F\u00f6h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Wer zum heilgen Kampf berufen,", "tokens": ["Wer", "zum", "heil\u00b7gen", "Kampf", "be\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist gl\u00fcckselig dann zu preisen,", "tokens": ["Ist", "gl\u00fcck\u00b7se\u00b7lig", "dann", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn vor sich er seinen Feind hat,", "tokens": ["Wenn", "vor", "sich", "er", "sei\u00b7nen", "Feind", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRF", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Draufzuschlagen mit dem Eisen;", "tokens": ["Drauf\u00b7zu\u00b7schla\u00b7gen", "mit", "dem", "Ei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Wer nicht streitet nur mit Worten,", "tokens": ["Wer", "nicht", "strei\u00b7tet", "nur", "mit", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die er zweifelnd mu\u00df vertrauen", "tokens": ["Die", "er", "zwei\u00b7felnd", "mu\u00df", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Windeslaunen, Wetterlaunen;", "tokens": ["Win\u00b7des\u00b7lau\u00b7nen", ",", "Wet\u00b7ter\u00b7lau\u00b7nen", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer da wei\u00df, wohin zu hauen.", "tokens": ["Wer", "da", "wei\u00df", ",", "wo\u00b7hin", "zu", "hau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PWAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Ziska, wildbeherzter B\u00f6hme!", "tokens": ["Zis\u00b7ka", ",", "wild\u00b7be\u00b7herz\u00b7ter", "B\u00f6h\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwinge fr\u00f6hlich Lanz und Keule!", "tokens": ["Schwin\u00b7ge", "fr\u00f6h\u00b7lich", "Lanz", "und", "Keu\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00fcrgen sind dir deines Wirkens", "tokens": ["B\u00fcr\u00b7gen", "sind", "dir", "dei\u00b7nes", "Wir\u00b7kens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Str\u00f6me Bluts und Sterbgeheule. \u2013", "tokens": ["Str\u00f6\u00b7me", "Bluts", "und", "Sterb\u00b7ge\u00b7heu\u00b7le", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Wieder hat er, Tod vergeudend,", "tokens": ["Wie\u00b7der", "hat", "er", ",", "Tod", "ver\u00b7geu\u00b7dend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Tag hindurch geschlagen,", "tokens": ["Ei\u00b7nen", "Tag", "hin\u00b7durch", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6chte in der Nacht und K\u00fchle", "tokens": ["M\u00f6ch\u00b7te", "in", "der", "Nacht", "und", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiter fechten mit Behagen.", "tokens": ["Wei\u00b7ter", "fech\u00b7ten", "mit", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Vorw\u00e4rts treibt er seine Scharen", "tokens": ["Vor\u00b7w\u00e4rts", "treibt", "er", "sei\u00b7ne", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den nachtverh\u00fcllten Pfaden,", "tokens": ["Auf", "den", "nacht\u00b7ver\u00b7h\u00fcll\u00b7ten", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um der Freiheit, seinem Liebchen,", "tokens": ["Um", "der", "Frei\u00b7heit", ",", "sei\u00b7nem", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufzuspielen Serenaden", "tokens": ["Auf\u00b7zu\u00b7spie\u00b7len", "Se\u00b7re\u00b7na\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Mit der Feldschlacht, seiner Orgel,", "tokens": ["Mit", "der", "Feld\u00b7schlacht", ",", "sei\u00b7ner", "Or\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die er wei\u00df so stark zu greifen;", "tokens": ["Die", "er", "wei\u00df", "so", "stark", "zu", "grei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfaffenvolk und F\u00fcrstenknechte", "tokens": ["Pfaf\u00b7fen\u00b7volk", "und", "F\u00fcrs\u00b7ten\u00b7knech\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind die gellen Orgelpfeifen.", "tokens": ["Sind", "die", "gel\u00b7len", "Or\u00b7gel\u00b7pfei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Doch es dunkelt tiefer immer", "tokens": ["Doch", "es", "dun\u00b7kelt", "tie\u00b7fer", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein Gewitter in die Schlucht,", "tokens": ["Ein", "Ge\u00b7wit\u00b7ter", "in", "die", "Schlucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur zuweilen \u00fcbers Tal weg", "tokens": ["Nur", "zu\u00b7wei\u00b7len", "\u00fc\u00b7bers", "Tal", "weg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Setzt ein Blitz in wilder Flucht.", "tokens": ["Setzt", "ein", "Blitz", "in", "wil\u00b7der", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Hemmend lagert sich das Dunkel", "tokens": ["Hem\u00b7mend", "la\u00b7gert", "sich", "das", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Wagenburg, die Rosse,", "tokens": ["Um", "die", "Wa\u00b7gen\u00b7burg", ",", "die", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Geschirr' im Winde rasseln", "tokens": ["Die", "Ge\u00b7schirr'", "im", "Win\u00b7de", "ras\u00b7seln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die B\u00fcndel der Geschosse.", "tokens": ["Und", "die", "B\u00fcn\u00b7del", "der", "Ge\u00b7schos\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Ziska spricht: \u00bbO wie so fl\u00fcchtig", "tokens": ["Zis\u00b7ka", "spricht", ":", "\u00bb", "O", "wie", "so", "fl\u00fcch\u00b7tig"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "$(", "NE", "KOKOM", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser sch\u00f6ne Blitz entf\u00e4hrt!", "tokens": ["Die\u00b7ser", "sch\u00f6\u00b7ne", "Blitz", "ent\u00b7f\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt ich doch hier an die Tanne", "tokens": ["K\u00f6nnt", "ich", "doch", "hier", "an", "die", "Tan\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nageln ihn mit meinem Schwert!", "tokens": ["Na\u00b7geln", "ihn", "mit", "mei\u00b7nem", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Da\u00df ich Gottes Welt befreie,", "tokens": ["Da\u00df", "ich", "Got\u00b7tes", "Welt", "be\u00b7frei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zahle heim die Racheschuld,", "tokens": ["Zah\u00b7le", "heim", "die", "Ra\u00b7ch\u00b7e\u00b7schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Br\u00fcder, k\u00f6nnt euch doch das Feuer", "tokens": ["Br\u00fc\u00b7der", ",", "k\u00f6nnt", "euch", "doch", "das", "Feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leuchten meiner Ungeduld!\u00ab \u2013", "tokens": ["Leuch\u00b7ten", "mei\u00b7ner", "Un\u00b7ge\u00b7duld", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Ha! ein Blitz, ein sonnenheller!", "tokens": ["Ha", "!", "ein", "Blitz", ",", "ein", "son\u00b7nen\u00b7hel\u00b7ler", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herrlich strahlen aus der Nacht", "tokens": ["Herr\u00b7lich", "strah\u00b7len", "aus", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Hussiten Schreckgestalten,", "tokens": ["Der", "Hus\u00b7si\u00b7ten", "Schreck\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ziskas Herz in Freude lacht.", "tokens": ["Zis\u00b7kas", "Herz", "in", "Freu\u00b7de", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "Donner rollen, fern verhallend,", "tokens": ["Don\u00b7ner", "rol\u00b7len", ",", "fern", "ver\u00b7hal\u00b7lend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus des Himmels tiefster Brust,", "tokens": ["Aus", "des", "Him\u00b7mels", "tiefs\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem Gewitter lauscht der Feldherr,", "tokens": ["Dem", "Ge\u00b7wit\u00b7ter", "lauscht", "der", "Feld\u00b7herr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nachtgebannt, mit Neideslust:", "tokens": ["Nacht\u00b7ge\u00b7bannt", ",", "mit", "Nei\u00b7des\u00b7lust", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "\u00bbk\u00f6nnt ich fliegen wie die Wolken,", "tokens": ["\u00bb", "k\u00f6nnt", "ich", "flie\u00b7gen", "wie", "die", "Wol\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nachts in ungehemmter Eile!", "tokens": ["Nachts", "in", "un\u00b7ge\u00b7hemm\u00b7ter", "Ei\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6