{"textgrid.poem.47508": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[der tadellose, gro\u00dfe Herr]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der tadellose, gro\u00dfe Herr", "tokens": ["Der", "ta\u00b7del\u00b7lo\u00b7se", ",", "gro\u00b7\u00dfe", "Herr"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des ewigen Weltenbau's,", "tokens": ["Des", "e\u00b7wi\u00b7gen", "Wel\u00b7ten\u00b7bau's", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schlo\u00df unsere Seele fest hinein", "tokens": ["Schlo\u00df", "un\u00b7se\u00b7re", "See\u00b7le", "fest", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "PTKVZ"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "In dieses ird'sche Haus.", "tokens": ["In", "die\u00b7ses", "ird'\u00b7sche", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und nimmermehr, so sehr du dich", "tokens": ["Und", "nim\u00b7mer\u00b7mehr", ",", "so", "sehr", "du", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "ADV", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entlebest und entleibst,", "tokens": ["Ent\u00b7le\u00b7best", "und", "ent\u00b7leibst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "lin