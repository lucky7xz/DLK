{"textgrid.poem.33680": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Stern und Irrlicht", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergn\u00fcglich flog ein Irrlicht durch die Moore,", "tokens": ["Ver\u00b7gn\u00fcg\u00b7lich", "flog", "ein", "Irr\u00b7licht", "durch", "die", "Moo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da rief aus Wolkenh\u00f6h'n herab ein Stern:", "tokens": ["Da", "rief", "aus", "Wol\u00b7ken\u00b7h\u00f6h'n", "her\u00b7ab", "ein", "Stern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer bist du? Welcher Raum hat dich verloren?", "tokens": ["Wer", "bist", "du", "?", "Wel\u00b7cher", "Raum", "hat", "dich", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "PWAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Komm doch zu mir herauf, ich seh' dich gern!", "tokens": ["Komm", "doch", "zu", "mir", "her\u00b7auf", ",", "ich", "seh'", "dich", "gern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Oft hab' ich deine Bahnen schon betrachtet", "tokens": ["Oft", "hab'", "ich", "dei\u00b7ne", "Bah\u00b7nen", "schon", "be\u00b7trach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und um sie zu berechnen sann ich viel.", "tokens": ["Und", "um", "sie", "zu", "be\u00b7rech\u00b7nen", "sann", "ich", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PTKZU", "VVINF", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald sah ich leuchtend dich, bald tief umnachtet \u2013", "tokens": ["Bald", "sah", "ich", "leuch\u00b7tend", "dich", ",", "bald", "tief", "um\u00b7nach\u00b7tet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "PPER", "$,", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Woraus besteht dein Kern, was ist dein Ziel?", "tokens": ["Wo\u00b7raus", "be\u00b7steht", "dein", "Kern", ",", "was", "ist", "dein", "Ziel", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "PWS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, sprach der Irrwisch, setzte sich und keuchte,", "tokens": ["Ach", ",", "sprach", "der", "Irr\u00b7wisch", ",", "setz\u00b7te", "sich", "und", "keuch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "PRF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bem\u00fchn sich Euer Gnaden nicht um mich!", "tokens": ["Be\u00b7m\u00fchn", "sich", "Eu\u00b7er", "Gna\u00b7den", "nicht", "um", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entschuldigen Sie g\u00fctigst, da\u00df ich leuchte,", "tokens": ["Ent\u00b7schul\u00b7di\u00b7gen", "Sie", "g\u00fc\u00b7tigst", ",", "da\u00df", "ich", "leuch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich freu' mich hier, es ist so sommerlich.", "tokens": ["Ich", "freu'", "mich", "hier", ",", "es", "ist", "so", "som\u00b7mer\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein Kern ist anspruchslos, das ist es eben.", "tokens": ["Mein", "Kern", "ist", "an\u00b7spruchs\u00b7los", ",", "das", "ist", "es", "e\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PDS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich kann mich aus dem niedern Aufenthalt", "tokens": ["Ich", "kann", "mich", "aus", "dem", "nie\u00b7dern", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der feuchten Atmosph\u00e4re nicht erheben;", "tokens": ["Der", "feuch\u00b7ten", "At\u00b7mo\u00b7sph\u00e4\u00b7re", "nicht", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bei Euch w\u00e4r' mir's zu regelm\u00e4\u00dfig kalt.", "tokens": ["Bei", "Euch", "w\u00e4r'", "mir's", "zu", "re\u00b7gel\u00b7m\u00e4\u00b7\u00dfig", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NE", "PTKA", "ADJD", "ADJD", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Vergn\u00fcglich flog ein Irrlicht durch die Moore,", "tokens": ["Ver\u00b7gn\u00fcg\u00b7lich", "flog", "ein", "Irr\u00b7licht", "durch", "die", "Moo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da rief aus Wolkenh\u00f6h'n herab ein Stern:", "tokens": ["Da", "rief", "aus", "Wol\u00b7ken\u00b7h\u00f6h'n", "her\u00b7ab", "ein", "Stern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer bist du? Welcher Raum hat dich verloren?", "tokens": ["Wer", "bist", "du", "?", "Wel\u00b7cher", "Raum", "hat", "dich", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "PWAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Komm doch zu mir herauf, ich seh' dich gern!", "tokens": ["Komm", "doch", "zu", "mir", "her\u00b7auf", ",", "ich", "seh'", "dich", "gern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "