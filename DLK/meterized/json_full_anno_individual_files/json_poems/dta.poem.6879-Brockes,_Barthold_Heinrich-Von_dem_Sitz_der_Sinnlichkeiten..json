{"dta.poem.6879": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So glaubet Brutus auch, bey finstrer Nacht,", "tokens": ["So", "glau\u00b7bet", "Bru\u00b7tus", "auch", ",", "bey", "finst\u00b7rer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In dem Philippischen Gefilde,", "tokens": ["In", "dem", "Phil\u00b7ip\u00b7pi\u00b7schen", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Als die gemeine Sorg\u2019 ihn voller Schwermuht macht,", "tokens": ["Als", "die", "ge\u00b7mei\u00b7ne", "Sor\u00b7g'", "ihn", "vol\u00b7ler", "Schwer\u00b7muht", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein schwartz Gespenst zu sehn,", "tokens": ["Ein", "schwartz", "Ge\u00b7spenst", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und von dem Schrecken-Bilde", "tokens": ["Und", "von", "dem", "Schre\u00b7cken\u00b7Bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sein hartes Schicksal zu verstehn.", "tokens": ["Sein", "har\u00b7tes", "Schick\u00b7sal", "zu", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}