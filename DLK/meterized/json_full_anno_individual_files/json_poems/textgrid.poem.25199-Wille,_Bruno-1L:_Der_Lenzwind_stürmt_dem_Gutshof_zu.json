unct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Lenzwind aber w\u00e4chst und heult", "tokens": ["Der", "Lenz\u00b7wind", "a\u00b7ber", "w\u00e4chst", "und", "heult"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bedrohlich in den Pappeln.", "tokens": ["Be\u00b7droh\u00b7lich", "in", "den", "Pap\u00b7peln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Lenzwind st\u00fcrmt dem Gutshof zu", "tokens": ["Der", "Lenz\u00b7wind", "st\u00fcrmt", "dem", "Guts\u00b7hof", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Zeilen schwanker Pappeln", "tokens": ["Durch", "Zei\u00b7len", "schwan\u00b7ker", "Pap\u00b7peln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00e4\u00dft auf braunem Ackerland", "tokens": ["Und", "l\u00e4\u00dft", "auf", "brau\u00b7nem", "A\u00b7cker\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Vogelscheuche zappeln.", "tokens": ["Die", "Vo\u00b7gel\u00b7scheu\u00b7che", "zap\u00b7peln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Am Pappelwege sitzt ein Strolch;", "tokens": ["Am", "Pap\u00b7pel\u00b7we\u00b7ge", "sitzt", "ein", "Strolch", ";"], "token_info": ["word", "word", "wo