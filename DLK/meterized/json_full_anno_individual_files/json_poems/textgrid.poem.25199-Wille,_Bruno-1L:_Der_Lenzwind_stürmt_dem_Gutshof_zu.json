{"textgrid.poem.25199": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Lenzwind st\u00fcrmt dem Gutshof zu", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Lenzwind st\u00fcrmt dem Gutshof zu", "tokens": ["Der", "Lenz\u00b7wind", "st\u00fcrmt", "dem", "Guts\u00b7hof", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Zeilen schwanker Pappeln", "tokens": ["Durch", "Zei\u00b7len", "schwan\u00b7ker", "Pap\u00b7peln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00e4\u00dft auf braunem Ackerland", "tokens": ["Und", "l\u00e4\u00dft", "auf", "brau\u00b7nem", "A\u00b7cker\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Vogelscheuche zappeln.", "tokens": ["Die", "Vo\u00b7gel\u00b7scheu\u00b7che", "zap\u00b7peln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Am Pappelwege sitzt ein Strolch;", "tokens": ["Am", "Pap\u00b7pel\u00b7we\u00b7ge", "sitzt", "ein", "Strolch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der knotet an einem Strick", "tokens": ["Der", "kno\u00b7tet", "an", "ei\u00b7nem", "Strick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und legt die Schlinge zur Probe", "tokens": ["Und", "legt", "die", "Schlin\u00b7ge", "zur", "Pro\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Zerrend um sein Genick.", "tokens": ["Zer\u00b7rend", "um", "sein", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "\u00bbdie h\u00e4lt! Ach wohl, nun kannst du gehn", "tokens": ["\u00bb", "die", "h\u00e4lt", "!", "Ach", "wohl", ",", "nun", "kannst", "du", "gehn"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "VVFIN", "$.", "ITJ", "ADV", "$,", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus dieser verdammten Welt.", "tokens": ["Aus", "die\u00b7ser", "ver\u00b7damm\u00b7ten", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nur schade, da\u00df hier unterm Gurt", "tokens": ["Nur", "scha\u00b7de", ",", "da\u00df", "hier", "un\u00b7term", "Gurt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch immer der Hunger bellt!", "tokens": ["Noch", "im\u00b7mer", "der", "Hun\u00b7ger", "bellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O Schande, mit Bauchweh zu verrecken!", "tokens": ["O", "Schan\u00b7de", ",", "mit", "Bauch\u00b7weh", "zu", "ver\u00b7re\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Giebt Keiner den letzten Happen? \u2013 \u2013", "tokens": ["Giebt", "Kei\u00b7ner", "den", "letz\u00b7ten", "Hap\u00b7pen", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Vielleicht ist dr\u00fcben im Hofe", "tokens": ["Viel\u00b7leicht", "ist", "dr\u00fc\u00b7ben", "im", "Ho\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Bei den Knechten was zu erschnappen.\u00ab \u2013", "tokens": ["Bei", "den", "Knech\u00b7ten", "was", "zu", "er\u00b7schnap\u00b7pen", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "PTKZU", "VVINF", "$.", "$(", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Und m\u00fcde humpelt die hungerfahle", "tokens": ["Und", "m\u00fc\u00b7de", "hum\u00b7pelt", "die", "hun\u00b7ger\u00b7fah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "D\u00fcrre Lumpengestalt zum Gutshof,", "tokens": ["D\u00fcr\u00b7re", "Lum\u00b7pen\u00b7ge\u00b7stalt", "zum", "Guts\u00b7hof", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Dr\u00e4ngt das Thor behutsam auf,", "tokens": ["Dr\u00e4ngt", "das", "Thor", "be\u00b7hut\u00b7sam", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sp\u00e4hend vorgestreckt den Kopf ...", "tokens": ["Sp\u00e4\u00b7hend", "vor\u00b7ge\u00b7streckt", "den", "Kopf", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Verdammt! Da steht der Gutsherr,", "tokens": ["Ver\u00b7dammt", "!", "Da", "steht", "der", "Guts\u00b7herr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Reitstieflig, zornrot das feiste Gesicht;", "tokens": ["Reit\u00b7stief\u00b7lig", ",", "zorn\u00b7rot", "das", "feis\u00b7te", "Ge\u00b7sicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Er pfeift dem Hunde gellend;", "tokens": ["Er", "pfeift", "dem", "Hun\u00b7de", "gel\u00b7lend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schon rennt das Vieh, die Z\u00e4hne gefletscht ...", "tokens": ["Schon", "rennt", "das", "Vieh", ",", "die", "Z\u00e4h\u00b7ne", "ge\u00b7fletscht", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Hastig zugeschlagen das Thor!", "tokens": ["Has\u00b7tig", "zu\u00b7ge\u00b7schla\u00b7gen", "das", "Thor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Fort! mit schlotternden Knieen ...", "tokens": ["Fort", "!", "mit", "schlot\u00b7tern\u00b7den", "Kni\u00b7e\u00b7en", "..."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "ADJA", "NN", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Fern h\u00e4lt der Arme zitternd, keuchend,", "tokens": ["Fern", "h\u00e4lt", "der", "Ar\u00b7me", "zit\u00b7ternd", ",", "keu\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVPP", "$,", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und sch\u00fcttelt die H\u00e4ndeknochen:", "tokens": ["Und", "sch\u00fct\u00b7telt", "die", "H\u00e4n\u00b7de\u00b7kno\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbwarte nur! Was ein Sterbender flucht,", "tokens": ["\u00bb", "war\u00b7te", "nur", "!", "Was", "ein", "Ster\u00b7ben\u00b7der", "flucht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$.", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ist nicht in den Wind gesprochen.", "tokens": ["Ist", "nicht", "in", "den", "Wind", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihr Reichen rafft uns alles weg", "tokens": ["Ihr", "Rei\u00b7chen", "rafft", "uns", "al\u00b7les", "weg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIS", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fre\u00dft es in den Magen,", "tokens": ["Und", "fre\u00dft", "es", "in", "den", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wollt uns selbst den Abfall", "tokens": ["Und", "wollt", "uns", "selbst", "den", "Ab\u00b7fall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht g\u00f6nnen zum Benagen?\u00ab", "tokens": ["Nicht", "g\u00f6n\u00b7nen", "zum", "Be\u00b7na\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "VVINF", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wutglotzend, knirschend hastet er", "tokens": ["Wut\u00b7glot\u00b7zend", ",", "knir\u00b7schend", "has\u00b7tet", "er"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf braunes Ackerland", "tokens": ["Auf", "brau\u00b7nes", "A\u00b7cker\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur Vogelscheuche und zerrei\u00dft", "tokens": ["Zur", "Vo\u00b7gel\u00b7scheu\u00b7che", "und", "zer\u00b7rei\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr zundriges Gewand;", "tokens": ["Ihr", "zund\u00b7ri\u00b7ges", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Dem Holzgerippe zieht er an", "tokens": ["Dem", "Holz\u00b7ge\u00b7rip\u00b7pe", "zieht", "er", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den eignen Lumpenrock", "tokens": ["Den", "eig\u00b7nen", "Lum\u00b7pen\u00b7rock"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und seinen sch\u00e4bigen Filzhut", "tokens": ["Und", "sei\u00b7nen", "sch\u00e4\u00b7bi\u00b7gen", "Filz\u00b7hut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.8": {"text": "St\u00fclpt er \u00fcber den Stock;", "tokens": ["St\u00fclpt", "er", "\u00fc\u00b7ber", "den", "Stock", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Und schaut sein Werk mit Grinsen an:", "tokens": ["Und", "schaut", "sein", "Werk", "mit", "Grin\u00b7sen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdu d\u00fcrres Lappenluder,", "tokens": ["\u00bb", "du", "d\u00fcr\u00b7res", "Lap\u00b7pen\u00b7lu\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du gleichst f\u00fcrwahr mir bis aufs Haar", "tokens": ["Du", "gleichst", "f\u00fcr\u00b7wahr", "mir", "bis", "aufs", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie ein Zwilligsbruder.", "tokens": ["Als", "wie", "ein", "Zwil\u00b7ligs\u00b7bru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das bin ich selbst! Nun kann ich", "tokens": ["Das", "bin", "ich", "selbst", "!", "Nun", "kann", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "$.", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem reichen Hunde trotzen", "tokens": ["Dem", "rei\u00b7chen", "Hun\u00b7de", "trot\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und, wenn mein Leib als Aas verwes't,", "tokens": ["Und", ",", "wenn", "mein", "Leib", "als", "Aas", "ver\u00b7wes't", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Satten frech beglotzen.\u00ab \u2013", "tokens": ["Die", "Sat\u00b7ten", "frech", "be\u00b7glot\u00b7zen", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Am Weg ein greiser Pappelbaum", "tokens": ["Am", "Weg", "ein", "grei\u00b7ser", "Pap\u00b7pel\u00b7baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit niedrigem Ge\u00e4st,", "tokens": ["Mit", "nied\u00b7ri\u00b7gem", "Ge\u00b7\u00e4st", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der hilft dem Strolch zu sich herauf", "tokens": ["Der", "hilft", "dem", "Strolch", "zu", "sich", "her\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "PRF", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00e4lt die Schlinge fest:", "tokens": ["Und", "h\u00e4lt", "die", "Schlin\u00b7ge", "fest", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbhinein den Hals, du Menschenkind!", "tokens": ["\u00bb", "hin\u00b7ein", "den", "Hals", ",", "du", "Men\u00b7schen\u00b7kind", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich will dich treulich henken.", "tokens": ["Ich", "will", "dich", "treu\u00b7lich", "hen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Spring ab! Nun mag der tolle Wind", "tokens": ["Spring", "ab", "!", "Nun", "mag", "der", "tol\u00b7le", "Wind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die zuckende