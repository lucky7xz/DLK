bic.tetra"}, "line.4": {"text": "Noch immer der Hunger bellt!", "tokens": ["Noch", "im\u00b7mer", "der", "Hun\u00b7ger", "bellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O Schande, mit Bauchweh zu verrecken!", "tokens": ["O", "Schan\u00b7de", ",", "mit", "Bauch\u00b7weh", "zu", "ver\u00b7re\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Giebt Keiner den letzten Happen? \u2013 \u2013", "tokens": ["Giebt", "Kei\u00b7ner", "den", "letz\u00b7ten", "Hap\u00b7pen", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Vielleicht ist dr\u00fcben im Hofe", "tokens": ["Viel\u00b7leicht", "ist", "dr\u00fc\u00b7ben", "im", "Ho\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Bei den Knechten was zu erschnappen.\u00ab \u2013", "tokens": ["Bei", "den", "Knech\u00b7ten", "was", "zu", "er\u00b7schnap\u00b7pen", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "PTKZU", "VVINF", "$.", "$(", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Und m\u00fcde humpelt die hungerfahle", "tokens": ["Und", "m\u00fc\u00b7de", "hum\u00b7pelt", "die", "hun\u00b7ger\u00b7fah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "