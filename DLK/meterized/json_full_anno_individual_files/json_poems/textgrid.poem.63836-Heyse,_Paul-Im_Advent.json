{"textgrid.poem.63836": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Im Advent", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die l\u00e4ngsten N\u00e4chte. Dr\u00fcben hinterm Vorgebirg", "tokens": ["Die", "l\u00e4ngs\u00b7ten", "N\u00e4ch\u00b7te", ".", "Dr\u00fc\u00b7ben", "hin\u00b7term", "Vor\u00b7ge\u00b7birg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von San Vigilio schl\u00e4ft noch in den Tag hinein", "tokens": ["Von", "San", "Vi\u00b7gi\u00b7lio", "schl\u00e4ft", "noch", "in", "den", "Tag", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "VVFIN", "ADV", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Frau Sonn' und kann sich nicht entschlie\u00dfen aufzustehn.", "tokens": ["Frau", "Sonn'", "und", "kann", "sich", "nicht", "ent\u00b7schlie\u00b7\u00dfen", "auf\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "VMFIN", "PRF", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zeit w\u00e4r' es l\u00e4ngst. Doch sie, wie eine K\u00f6nigin,", "tokens": ["Zeit", "w\u00e4r'", "es", "l\u00e4ngst", ".", "Doch", "sie", ",", "wie", "ei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$.", "KON", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die weich sich dehnt im seidnen Bett und g\u00e4hnend denkt,", "tokens": ["Die", "weich", "sich", "dehnt", "im", "seid\u00b7nen", "Bett", "und", "g\u00e4h\u00b7nend", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVFIN", "APPRART", "PPOSAT", "NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es eile nicht, von ihrem goldnen Thron herab", "tokens": ["Es", "ei\u00b7le", "nicht", ",", "von", "ih\u00b7rem", "gold\u00b7nen", "Thron", "her\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "APPR", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihr Weltreich zu regieren, schl\u00e4frig blinzelt sie", "tokens": ["Ihr", "Welt\u00b7reich", "zu", "re\u00b7gie\u00b7ren", ",", "schl\u00e4f\u00b7rig", "blin\u00b7zelt", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nur schwach hervor aus grauer Wimper, zieht sodann", "tokens": ["Nur", "schwach", "her\u00b7vor", "aus", "grau\u00b7er", "Wim\u00b7per", ",", "zieht", "so\u00b7dann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "PTKVZ", "APPR", "ADJA", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das Nebeldeckbett hoch sich \u00fcbers Angesicht", "tokens": ["Das", "Ne\u00b7bel\u00b7deck\u00b7bett", "hoch", "sich", "\u00fc\u00b7bers", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und schlummert weiter.", "tokens": ["Und", "schlum\u00b7mert", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Auch hernach, wenn endlich sie", "tokens": ["Auch", "her\u00b7nach", ",", "wenn", "end\u00b7lich", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Beginnt ihr Tagwerk, nur im Schlafrock schleicht sie dann", "tokens": ["Be\u00b7ginnt", "ihr", "Tag\u00b7werk", ",", "nur", "im", "Schla\u00b7frock", "schleicht", "sie", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADV", "APPRART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit ungestr\u00e4hlten Haaren, sehr unaufger\u00e4umt", "tokens": ["Mit", "un\u00b7ge\u00b7str\u00e4hl\u00b7ten", "Haa\u00b7ren", ",", "sehr", "un\u00b7auf\u00b7ge\u00b7r\u00e4umt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Des Wegs dahin, im Wolkenmantel dicht vermummt,", "tokens": ["Des", "Wegs", "da\u00b7hin", ",", "im", "Wol\u00b7ken\u00b7man\u00b7tel", "dicht", "ver\u00b7mummt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Als friere sie. Denn ach, nur eine Fabel ist", "tokens": ["Als", "frie\u00b7re", "sie", ".", "Denn", "ach", ",", "nur", "ei\u00b7ne", "Fa\u00b7bel", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "PPER", "$.", "KON", "XY", "$,", "ADV", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das ewig blaue Firmament Italias!", "tokens": ["Das", "e\u00b7wig", "blau\u00b7e", "Fir\u00b7ma\u00b7ment", "I\u00b7ta\u00b7li\u00b7as", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Auf ihrer Stirn auch spukt gar oft zur Winterszeit", "tokens": ["Auf", "ih\u00b7rer", "Stirn", "auch", "spukt", "gar", "oft", "zur", "Win\u00b7ter\u00b7szeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein Schatten grauer Schwermuth. Ihre Kinder dann", "tokens": ["Ein", "Schat\u00b7ten", "grau\u00b7er", "Schwer\u00b7muth", ".", "Ih\u00b7re", "Kin\u00b7der", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "In kellerkalten H\u00e4usern, wo kein Ofen brennt,", "tokens": ["In", "kel\u00b7ler\u00b7kal\u00b7ten", "H\u00e4u\u00b7sern", ",", "wo", "kein", "O\u00b7fen", "brennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ums Reisigfeu'r am Herd tr\u00fcbsinnig kauern sie,", "tokens": ["Ums", "Rei\u00b7sig\u00b7feu'r", "am", "Herd", "tr\u00fcb\u00b7sin\u00b7nig", "kau\u00b7ern", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "APPRART", "NN", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vor Frost erschauernd. Stumm im kleinen K\u00e4fig sitzt", "tokens": ["Vor", "Frost", "er\u00b7schau\u00b7ernd", ".", "Stumm", "im", "klei\u00b7nen", "K\u00e4\u00b7fig", "sitzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$.", "NN", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Drossel. Auch am Rocken jetzt und Webestuhl", "tokens": ["Die", "Dros\u00b7sel", ".", "Auch", "am", "Ro\u00b7cken", "jetzt", "und", "We\u00b7be\u00b7stuhl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "APPRART", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Erklingt kein Ritornell und kein Rispetto mehr,", "tokens": ["Er\u00b7klingt", "kein", "Ri\u00b7tor\u00b7nell", "und", "kein", "Ris\u00b7pet\u00b7to", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und vorwurfsvoll ert\u00f6nt nur noch des M\u00e4uschens Pfiff,", "tokens": ["Und", "vor\u00b7wurfs\u00b7voll", "er\u00b7t\u00f6nt", "nur", "noch", "des", "M\u00e4u\u00b7schens", "Pfiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da ein Polentabr\u00f6ckchen kaum ihm \u00fcbrig bleibt. \u2013", "tokens": ["Da", "ein", "Po\u00b7lent\u00b7a\u00b7br\u00f6\u00b7ck\u00b7chen", "kaum", "ihm", "\u00fcb\u00b7rig", "bleibt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PPER", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Doch nur Geduld! Nach kurzen Wochen, hingedehnt", "tokens": ["Doch", "nur", "Ge\u00b7duld", "!", "Nach", "kur\u00b7zen", "Wo\u00b7chen", ",", "hin\u00b7ge\u00b7dehnt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "NN", "$.", "APPR", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Im Nebelhalbtraum, denkt Frau Sonne wiederum", "tokens": ["Im", "Ne\u00b7bel\u00b7halb\u00b7traum", ",", "denkt", "Frau", "Son\u00b7ne", "wie\u00b7de\u00b7rum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VVFIN", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Vor Scham ergl\u00fchend ihrer alten Schuldigkeit", "tokens": ["Vor", "Scham", "er\u00b7gl\u00fc\u00b7hend", "ih\u00b7rer", "al\u00b7ten", "Schul\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und holt, was lang vers\u00e4umt ward, um so eifriger", "tokens": ["Und", "holt", ",", "was", "lang", "ver\u00b7s\u00e4umt", "ward", ",", "um", "so", "eif\u00b7ri\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADJD", "VVPP", "VAFIN", "$,", "KOUI", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Nun wieder ein. Den Reif vom Laube sch\u00fcttelt sie", "tokens": ["Nun", "wie\u00b7der", "ein", ".", "Den", "Reif", "vom", "Lau\u00b7be", "sch\u00fct\u00b7telt", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKVZ", "$.", "ART", "NN", "APPRART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Den Lorbeern und Oliven, lockt mit warmem Hauch", "tokens": ["Den", "Lor\u00b7beern", "und", "O\u00b7li\u00b7ven", ",", "lockt", "mit", "war\u00b7mem", "Hauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "Krokus und blaue Veilchen schon im Februar", "tokens": ["Kro\u00b7kus", "und", "blau\u00b7e", "Veil\u00b7chen", "schon", "im", "Feb\u00b7ru\u00b7ar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJA", "NN", "ADV", "APPRART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.33": {"text": "Hervor auf allen Wiesen. An den Reben sacht", "tokens": ["Her\u00b7vor", "auf", "al\u00b7len", "Wie\u00b7sen", ".", "An", "den", "Re\u00b7ben", "sacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Beginnt's zu knospen. Ja sogar ein V\u00f6gelchen,", "tokens": ["Be\u00b7ginnt's", "zu", "knos\u00b7pen", ".", "Ja", "so\u00b7gar", "ein", "V\u00f6\u00b7gel\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "$.", "PTKANT", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.35": {"text": "Wenn gl\u00fccklich es dem Blei des J\u00e4gers winterlang", "tokens": ["Wenn", "gl\u00fcck\u00b7lich", "es", "dem", "Blei", "des", "J\u00e4\u00b7gers", "win\u00b7ter\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Entgangen, denkt bereits an neues Nesterbau'n", "tokens": ["Ent\u00b7gan\u00b7gen", ",", "denkt", "be\u00b7reits", "an", "neu\u00b7es", "Nes\u00b7ter\u00b7bau'n"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und zirpt und wirbt um eine Braut.", "tokens": ["Und", "zirpt", "und", "wirbt", "um", "ei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Und \u00e4hnlich so", "tokens": ["Und", "\u00e4hn\u00b7lich", "so"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.39": {"text": "Ergeht's dem Dichter. Sacht in seinem Busen schon", "tokens": ["Er\u00b7geht's", "dem", "Dich\u00b7ter.", "Sacht", "in", "sei\u00b7nem", "Bu\u00b7sen", "schon"], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "R\u00fchrt sich Gesang, wenn fr\u00fch am Tag er wohlgemut", "tokens": ["R\u00fchrt", "sich", "Ge\u00b7sang", ",", "wenn", "fr\u00fch", "am", "Tag", "er", "wohl\u00b7ge\u00b7mut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "NN", "$,", "KOUS", "ADJD", "APPRART", "NN", "PPER", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.41": {"text": "Auf luft'ger H\u00f6he wandelt, nur im leichten Rock", "tokens": ["Auf", "luft'\u00b7ger", "H\u00f6\u00b7he", "wan\u00b7delt", ",", "nur", "im", "leich\u00b7ten", "Rock"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und, was das Beste \u2013 denn verha\u00dft vor allem sind", "tokens": ["Und", ",", "was", "das", "Bes\u00b7te", "\u2013", "denn", "ver\u00b7ha\u00dft", "vor", "al\u00b7lem", "sind"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PRELS", "ART", "NN", "$(", "KON", "ADJD", "APPR", "PIS", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ihm diese nordischen Gr\u00e4uel \u2013, ohne Gummischuh'!", "tokens": ["Ihm", "die\u00b7se", "nor\u00b7di\u00b7schen", "Gr\u00e4u\u00b7el", "\u2013", ",", "oh\u00b7ne", "Gum\u00b7mi\u00b7schuh'", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "PDAT", "ADJA", "NN", "$(", "$,", "KOUI", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die l\u00e4ngsten N\u00e4chte. Dr\u00fcben hinterm Vorgebirg", "tokens": ["Die", "l\u00e4ngs\u00b7ten", "N\u00e4ch\u00b7te", ".", "Dr\u00fc\u00b7ben", "hin\u00b7term", "Vor\u00b7ge\u00b7birg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "