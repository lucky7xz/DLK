{"textgrid.poem.43868": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn dieses welcke Blat, du englische Grisette,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dieses welcke Blat, du englische Grisette,", "tokens": ["Wenn", "die\u00b7ses", "wel\u00b7cke", "Blat", ",", "du", "eng\u00b7li\u00b7sche", "Gri\u00b7set\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht seine Kostbarkeit von deinem Nahmen h\u00e4tte,", "tokens": ["Nicht", "sei\u00b7ne", "Kost\u00b7bar\u00b7keit", "von", "dei\u00b7nem", "Nah\u00b7men", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So spr\u00e4ch ich allerdings, da\u00df meine Dichterey", "tokens": ["So", "spr\u00e4ch", "ich", "al\u00b7ler\u00b7dings", ",", "da\u00df", "mei\u00b7ne", "Dich\u00b7te\u00b7rey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Feuers w\u00fcrdiger als deiner Augen sey.", "tokens": ["Des", "Feu\u00b7ers", "w\u00fcr\u00b7di\u00b7ger", "als", "dei\u00b7ner", "Au\u00b7gen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KOKOM", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Ehrfurcht, so mein Geist vor deiner Gottheit heget,", "tokens": ["Die", "Ehr\u00b7furcht", ",", "so", "mein", "Geist", "vor", "dei\u00b7ner", "Got\u00b7theit", "he\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Liebe, so mein Herz zu deiner Sch\u00f6nheit tr\u00e4get,", "tokens": ["Die", "Lie\u00b7be", ",", "so", "mein", "Herz", "zu", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sind Feinde, derer Streit mich beiderseits verlezt,", "tokens": ["Sind", "Fein\u00b7de", ",", "de\u00b7rer", "Streit", "mich", "bei\u00b7der\u00b7seits", "ver\u00b7lezt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PDS", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nachdem sie meine Brust zur Wahlstatt ausgesezt,", "tokens": ["Nach\u00b7dem", "sie", "mei\u00b7ne", "Brust", "zur", "Wahl\u00b7statt", "aus\u00b7ge\u00b7sezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auf welcher sie bisher mit gleichem Gl\u00fccke kriegen;", "tokens": ["Auf", "wel\u00b7cher", "sie", "bis\u00b7her", "mit", "glei\u00b7chem", "Gl\u00fc\u00b7cke", "krie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn beide sind geschickt, einander obzusiegen.", "tokens": ["Denn", "bei\u00b7de", "sind", "ge\u00b7schickt", ",", "ein\u00b7an\u00b7der", "ob\u00b7zu\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Liebe, wie es scheint, bekommt nunmehr das Feld,", "tokens": ["Die", "Lie\u00b7be", ",", "wie", "es", "scheint", ",", "be\u00b7kommt", "nun\u00b7mehr", "das", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil dein geneigter Blick ihr Schuz und R\u00fccken h\u00e4lt;", "tokens": ["Weil", "dein", "ge\u00b7neig\u00b7ter", "Blick", "ihr", "Schuz", "und", "R\u00fc\u00b7cken", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie selbst hat mir die Hand zu dieser Schrift gef\u00fchret,", "tokens": ["Sie", "selbst", "hat", "mir", "die", "Hand", "zu", "die\u00b7ser", "Schrift", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "In welcher meine Pflicht das erste Kind gebiehret.", "tokens": ["In", "wel\u00b7cher", "mei\u00b7ne", "Pflicht", "das", "ers\u00b7te", "Kind", "ge\u00b7bieh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Morgenr\u00f6the taucht ihr Kleid in Scharlach ein", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "taucht", "ihr", "Kleid", "in", "Schar\u00b7lach", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und will, jedoch umsonst, den Rosen \u00e4hnlich seyn,", "tokens": ["Und", "will", ",", "je\u00b7doch", "um\u00b7sonst", ",", "den", "Ro\u00b7sen", "\u00e4hn\u00b7lich", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "ADV", "ADV", "$,", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Venus und ihr Sohn auf deinem Munde brechen,", "tokens": ["Die", "Ve\u00b7nus", "und", "ihr", "Sohn", "auf", "dei\u00b7nem", "Mun\u00b7de", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil deine Reizungen sie in die Augen stechen.", "tokens": ["Weil", "dei\u00b7ne", "Rei\u00b7zun\u00b7gen", "sie", "in", "die", "Au\u00b7gen", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Der Widerwillen legt der Sonne Z\u00fcgel an,", "tokens": ["Der", "Wi\u00b7der\u00b7wil\u00b7len", "legt", "der", "Son\u00b7ne", "Z\u00fc\u00b7gel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die ihres gleichen nicht auf Erden leiden kan;", "tokens": ["Die", "ih\u00b7res", "glei\u00b7chen", "nicht", "auf", "Er\u00b7den", "lei\u00b7den", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "PTKNEG", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ihr S\u00e4umn\u00fc\u00df billiget die Faulheit ihrer Pferde,", "tokens": ["Ihr", "S\u00e4um\u00b7n\u00fc\u00df", "bil\u00b7li\u00b7get", "die", "Faul\u00b7heit", "ih\u00b7rer", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Damit ihr Antliz nicht von dir besch\u00e4met werde.", "tokens": ["Da\u00b7mit", "ihr", "Ant\u00b7liz", "nicht", "von", "dir", "be\u00b7sch\u00e4\u00b7met", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PTKNEG", "APPR", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Gewis, die Lippe f\u00fchrt ein reiches Kaufmannsgut,", "tokens": ["Ge\u00b7wis", ",", "die", "Lip\u00b7pe", "f\u00fchrt", "ein", "rei\u00b7ches", "Kauf\u00b7manns\u00b7gut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und das Gesichte zeigt ein Meer voll Milch und Blut,", "tokens": ["Und", "das", "Ge\u00b7sich\u00b7te", "zeigt", "ein", "Meer", "voll", "Milch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Allwo die Gratien am Ufer deiner Wangen", "tokens": ["All\u00b7wo", "die", "Gra\u00b7ti\u00b7en", "am", "U\u00b7fer", "dei\u00b7ner", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So Perlen suchen gehn als Purpurschnecken fangen.", "tokens": ["So", "Per\u00b7len", "su\u00b7chen", "gehn", "als", "Pur\u00b7pur\u00b7schne\u00b7cken", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "VVINF", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Lilgen wuchern starck auf der . . . . . Haut,", "tokens": ["Die", "Lil\u00b7gen", "wu\u00b7chern", "starck", "auf", "der", ".", ".", ".", ".", ".", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "$.", "$.", "$.", "$.", "$.", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.28": {"text": "Der Br\u00fcste weicher Pfiehl ist vor den Schwan gebaut,", "tokens": ["Der", "Br\u00fcs\u00b7te", "wei\u00b7cher", "Pfiehl", "ist", "vor", "den", "Schwan", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und ihre Sch\u00f6nheit l\u00e4st uns aus den Schalen schlie\u00dfen,", "tokens": ["Und", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "l\u00e4st", "uns", "aus", "den", "Scha\u00b7len", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Was vor Entz\u00fcckungen im Kerne wohnen m\u00fc\u00dfen.", "tokens": ["Was", "vor", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", "im", "Ker\u00b7ne", "woh\u00b7nen", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "APPRART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ach, da\u00df der Himmel doch mit uns so hart verf\u00e4hrt,", "tokens": ["Ach", ",", "da\u00df", "der", "Him\u00b7mel", "doch", "mit", "uns", "so", "hart", "ver\u00b7f\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ART", "NN", "ADV", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da er das Sehn erlaubt und den Genu\u00df verwehrt.", "tokens": ["Da", "er", "das", "Sehn", "er\u00b7laubt", "und", "den", "Ge\u00b7nu\u00df", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "O karge Mildigkeit, was hilft es Brunnen wi\u00dfen", "tokens": ["O", "kar\u00b7ge", "Mil\u00b7dig\u00b7keit", ",", "was", "hilft", "es", "Brun\u00b7nen", "wi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und dennoch keinen Trunck vor seinen Durst genie\u00dfen!", "tokens": ["Und", "den\u00b7noch", "kei\u00b7nen", "Trunck", "vor", "sei\u00b7nen", "Durst", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Denn w\u00e4re dieses nicht, so w\u00fcrde meine Hand", "tokens": ["Denn", "w\u00e4\u00b7re", "die\u00b7ses", "nicht", ",", "so", "w\u00fcr\u00b7de", "mei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PDS", "PTKNEG", "$,", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und wohl noch etwas mehr mit deiner Schoos bekand,", "tokens": ["Und", "wohl", "noch", "et\u00b7was", "mehr", "mit", "dei\u00b7ner", "Schoos", "be\u00b7kand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich aber d\u00f6rfte nicht mit deinen Kleidern zancken,", "tokens": ["Ich", "a\u00b7ber", "d\u00f6rf\u00b7te", "nicht", "mit", "dei\u00b7nen", "Klei\u00b7dern", "zan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die mir den ebnen Weg ins Paradies verplancken.", "tokens": ["Die", "mir", "den", "eb\u00b7nen", "Weg", "ins", "Pa\u00b7ra\u00b7dies", "ver\u00b7plan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Jedoch der Umschweif hat, kaum eh ich es gedacht,", "tokens": ["Je\u00b7doch", "der", "Um\u00b7schweif", "hat", ",", "kaum", "eh", "ich", "es", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "$,", "ADV", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Den ungewi\u00dfen Fu\u00df vom Wege weggebracht.", "tokens": ["Den", "un\u00b7ge\u00b7wi\u00b7\u00dfen", "Fu\u00df", "vom", "We\u00b7ge", "weg\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Verzeih, ber\u00fchmtes Kind, die Freyheit meiner Lieder,", "tokens": ["Ver\u00b7zeih", ",", "be\u00b7r\u00fchm\u00b7tes", "Kind", ",", "die", "Frey\u00b7heit", "mei\u00b7ner", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und ist des Dichters Scherz der Tugend nicht zuwider,", "tokens": ["Und", "ist", "des", "Dich\u00b7ters", "Scherz", "der", "Tu\u00b7gend", "nicht", "zu\u00b7wi\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "So las mich diesesmahl, es kan gar leicht geschehn,", "tokens": ["So", "las", "mich", "die\u00b7ses\u00b7mahl", ",", "es", "kan", "gar", "leicht", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Die Sonne deiner Huld im ersten Zeichen sehn;", "tokens": ["Die", "Son\u00b7ne", "dei\u00b7ner", "Huld", "im", "ers\u00b7ten", "Zei\u00b7chen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Dies ist ein guter Blick, der mit geneigtem Lichte", "tokens": ["Dies", "ist", "ein", "gu\u00b7ter", "Blick", ",", "der", "mit", "ge\u00b7neig\u00b7tem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Den wohlgemeinten Wuntsch nach deiner G\u00fcte richte:", "tokens": ["Den", "wohl\u00b7ge\u00b7mein\u00b7ten", "Wunt\u00b7sch", "nach", "dei\