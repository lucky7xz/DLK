{"dta.poem.7536": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Herzens Pf\u00f6rtner ist des Mannes Angesicht,", "tokens": ["Des", "Her\u00b7zens", "Pf\u00f6rt\u00b7ner", "ist", "des", "Man\u00b7nes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der den und den Empfang beim Herren dir verspricht.", "tokens": ["Der", "den", "und", "den", "Emp\u00b7fang", "beim", "Her\u00b7ren", "dir", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "KON", "ART", "NN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein freundliches Gesicht wird Hoffnung dir erwecken,", "tokens": ["Ein", "freund\u00b7li\u00b7ches", "Ge\u00b7sicht", "wird", "Hoff\u00b7nung", "dir", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ein unfreundliches zur\u00fcck die Hoffnung schrecken.", "tokens": ["Und", "ein", "un\u00b7freund\u00b7li\u00b7ches", "zu\u00b7r\u00fcck", "die", "Hoff\u00b7nung", "schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch oft, wann du zum Herrn gelangt, hast du entdeckt,", "tokens": ["Doch", "oft", ",", "wann", "du", "zum", "Herrn", "ge\u00b7langt", ",", "hast", "du", "ent\u00b7deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "APPRART", "NN", "VVPP", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Hoffnung oder Furcht der Pf\u00f6rtner falsch erweckt.", "tokens": ["Da\u00df", "Hoff\u00b7nung", "o\u00b7der", "Furcht", "der", "Pf\u00f6rt\u00b7ner", "falsch", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein herzensholder Herr hat oft unholde Mienen", "tokens": ["Ein", "her\u00b7zens\u00b7hol\u00b7der", "Herr", "hat", "oft", "un\u00b7hol\u00b7de", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Dienern, w\u00e4hrend auch unholdem holde dienen.", "tokens": ["Zu", "Die\u00b7nern", ",", "w\u00e4h\u00b7rend", "auch", "un\u00b7hol\u00b7dem", "hol\u00b7de", "die\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum la\u00df ein L\u00e4cheln dir nicht zuviel Hoffnung wecken,", "tokens": ["Drum", "la\u00df", "ein", "L\u00e4\u00b7cheln", "dir", "nicht", "zu\u00b7viel", "Hoff\u00b7nung", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auch ein Schmollen la\u00df nicht gleich zur\u00fcck dich schrecken.", "tokens": ["Und", "auch", "ein", "Schmol\u00b7len", "la\u00df", "nicht", "gleich", "zu\u00b7r\u00fcck", "dich", "schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVIMP", "PTKNEG", "ADV", "PTKVZ", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ihr aber, Herzensherrn, sagt euern Dienern fein,", "tokens": ["Ihr", "a\u00b7ber", ",", "Her\u00b7zens\u00b7herrn", ",", "sagt", "eu\u00b7ern", "Die\u00b7nern", "fein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie nicht euerm Sinn umh\u00fcllen falschen Schein.", "tokens": ["Da\u00df", "sie", "nicht", "eu\u00b7erm", "Sinn", "um\u00b7h\u00fcl\u00b7len", "fal\u00b7schen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Warum soll Freundlichkeit vergebens Hoffnung wecken,", "tokens": ["Wa\u00b7rum", "soll", "Freund\u00b7lich\u00b7keit", "ver\u00b7ge\u00b7bens", "Hoff\u00b7nung", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gar Unfreundlichkeit zur\u00fcck die arme schrecken?", "tokens": ["Und", "gar", "Un\u00b7freund\u00b7lich\u00b7keit", "zu\u00b7r\u00fcck", "die", "ar\u00b7me", "schre\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PTKVZ", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die m\u00fcden Hoffnungen, die oft so irre gehn,", "tokens": ["Die", "m\u00fc\u00b7den", "Hoff\u00b7nun\u00b7gen", ",", "die", "oft", "so", "ir\u00b7re", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O k\u00f6nnten sie das Ziel gleich recht am Eingang sehn!", "tokens": ["O", "k\u00f6nn\u00b7ten", "sie", "das", "Ziel", "gleich", "recht", "am", "Ein\u00b7gang", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}