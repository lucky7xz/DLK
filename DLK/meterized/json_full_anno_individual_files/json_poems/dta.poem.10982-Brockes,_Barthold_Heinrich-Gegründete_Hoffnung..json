{"dta.poem.10982": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gegr\u00fcndete Hoffnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df mich, o Herr! zu deinem Preise,", "tokens": ["La\u00df", "mich", ",", "o", "Herr", "!", "zu", "dei\u00b7nem", "Prei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf meines regen Lebens Reise", "tokens": ["Auf", "mei\u00b7nes", "re\u00b7gen", "Le\u00b7bens", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mich immer dergestalt verhalten,", "tokens": ["Mich", "im\u00b7mer", "der\u00b7ge\u00b7stalt", "ver\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Damit ich, wenn ich soll erkalten,", "tokens": ["Da\u00b7mit", "ich", ",", "wenn", "ich", "soll", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich deiner Huld getr\u00f6sten m\u00f6ge.", "tokens": ["Mich", "dei\u00b7ner", "Huld", "ge\u00b7tr\u00f6s\u00b7ten", "m\u00f6\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df mich das ", "tokens": ["La\u00df", "mich", "das"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "So anzuwenden, mich bestreben,", "tokens": ["So", "an\u00b7zu\u00b7wen\u00b7den", ",", "mich", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ich, Herr! deine weise Wege", "tokens": ["Da\u00df", "ich", ",", "Herr", "!", "dei\u00b7ne", "wei\u00b7se", "We\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "In deinen Werken, die so sch\u00f6n,", "tokens": ["In", "dei\u00b7nen", "Wer\u00b7ken", ",", "die", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mag h\u00f6ren, f\u00fchlen, schmecken, sehn;", "tokens": ["Mag", "h\u00f6\u00b7ren", ",", "f\u00fch\u00b7len", ",", "schme\u00b7cken", ",", "sehn", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df, wenn ich soll von hinnen gehn,", "tokens": ["Da\u00df", ",", "wenn", "ich", "soll", "von", "hin\u00b7nen", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "VMFIN", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ich mit Vergn\u00fcgen \u00fcberlege,", "tokens": ["Ich", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "\u00fc\u00b7berl\u00b7e\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wie viel mir Guts von dir geschehn,", "tokens": ["Wie", "viel", "mir", "Guts", "von", "dir", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df ich, wie schwach ich gleich, mich freue,", "tokens": ["Da\u00df", "ich", ",", "wie", "schwach", "ich", "gleich", ",", "mich", "freu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und mir zu einem Trost gedeyhe,", "tokens": ["Und", "mir", "zu", "ei\u00b7nem", "Trost", "ge\u00b7dey\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wenn sich dazu die\u00df Denken f\u00fcgt:", "tokens": ["Wenn", "sich", "da\u00b7zu", "die\u00df", "Den\u00b7ken", "f\u00fcgt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PAV", "PDS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df ich, o Sch\u00f6pfer dieser Welt,", "tokens": ["Da\u00df", "ich", ",", "o", "Sch\u00f6p\u00b7fer", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "An vielem, was sie Guts enth\u00e4lt,", "tokens": ["An", "vie\u00b7lem", ",", "was", "sie", "Guts", "ent\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Zu deinem Ruhm, mich oft vergn\u00fcgt.", "tokens": ["Zu", "dei\u00b7nem", "Ruhm", ",", "mich", "oft", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ach! la\u00df mich anders nicht gedenken,", "tokens": ["Ach", "!", "la\u00df", "mich", "an\u00b7ders", "nicht", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Als die\u00df, aus kindlich reinem Triebe:", "tokens": ["Als", "die\u00df", ",", "aus", "kind\u00b7lich", "rei\u00b7nem", "Trie\u00b7be", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "\u201eda du, o Herr, die ewge Liebe;", "tokens": ["\u201e", "da", "du", ",", "o", "Herr", ",", "die", "ew\u00b7ge", "Lie\u00b7be", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "\u201ewirst du mich nicht in Nichts versenken,", "tokens": ["\u201e", "wirst", "du", "mich", "nicht", "in", "Nichts", "ver\u00b7sen\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "PRF", "PTKNEG", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "\u201enoch minder in der H\u00f6lle schrenken;", "tokens": ["\u201e", "noch", "min\u00b7der", "in", "der", "H\u00f6l\u00b7le", "schren\u00b7ken", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "\u201evielmehr, da du in diesem Leben", "tokens": ["\u201e", "viel\u00b7mehr", ",", "da", "du", "in", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "$,", "KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "\u201emir so viel Gutes schon gegeben,", "tokens": ["\u201e", "mir", "so", "viel", "Gu\u00b7tes", "schon", "ge\u00b7ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.27": {"text": "\u201emir dort unendlich mehr noch schenken.", "tokens": ["\u201e", "mir", "dort", "un\u00b7end\u00b7lich", "mehr", "noch", "schen\u00b7ken", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "ADJD", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}