{"dta.poem.6386": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man h\u00e4tte leicht vermocht noch mehren Unterscheid", "tokens": ["Man", "h\u00e4t\u00b7te", "leicht", "ver\u00b7mocht", "noch", "meh\u00b7ren", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADJD", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Gegenwurf des F\u00fchlens vorzustellen.", "tokens": ["Vom", "Ge\u00b7gen\u00b7wurf", "des", "F\u00fch\u00b7lens", "vor\u00b7zu\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jedoch, es wird aus dem, was schon gesagt, erhellen,", "tokens": ["Je\u00b7doch", ",", "es", "wird", "aus", "dem", ",", "was", "schon", "ge\u00b7sagt", ",", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "APPR", "ART", "$,", "PRELS", "ADV", "VVPP", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was glatt, was Festigkeit, was Trockenheit,", "tokens": ["Was", "glatt", ",", "was", "Fes\u00b7tig\u00b7keit", ",", "was", "Tro\u00b7cken\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "PWS", "NN", "$,", "PWS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was fl\u00fc\u00dfig, welches feucht,", "tokens": ["Was", "fl\u00fc\u00b7\u00dfig", ",", "wel\u00b7ches", "feucht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Was laulich ist, und was die Biegsamkeit,", "tokens": ["Was", "lau\u00b7lich", "ist", ",", "und", "was", "die", "Biegs\u00b7am\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "KON", "PWS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was das Gef\u00fchl betrifft, ist alles leicht:", "tokens": ["Was", "das", "Ge\u00b7f\u00fchl", "be\u00b7tr\u00b7ifft", ",", "ist", "al\u00b7les", "leicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VAFIN", "PIS", "ADJD", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Es ist das einfachste, das gr\u00f6bste von den Sinnen.", "tokens": ["Es", "ist", "das", "ein\u00b7fachs\u00b7te", ",", "das", "gr\u00f6bs\u00b7te", "von", "den", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Denn der Geruch so wol, als der Geschmack von innen,", "tokens": ["Denn", "der", "Ge\u00b7ruch", "so", "wol", ",", "als", "der", "Ge\u00b7schmack", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "$,", "KOUS", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "W\u00fcrckt schon mit k\u00fcnstlicher vereinter Seltenheit.", "tokens": ["W\u00fcrckt", "schon", "mit", "k\u00fcnst\u00b7li\u00b7cher", "ver\u00b7ein\u00b7ter", "Sel\u00b7ten\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}