{"textgrid.poem.39273": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[es schl\u00e4ft im Busen, in dem deutschen, treuen]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schl\u00e4ft im Busen, in dem deutschen, treuen,", "tokens": ["Es", "schl\u00e4ft", "im", "Bu\u00b7sen", ",", "in", "dem", "deut\u00b7schen", ",", "treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "APPR", "ART", "ADJA", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Wort der Wahrheit, das noch nimmer bebte;", "tokens": ["Das", "Wort", "der", "Wahr\u00b7heit", ",", "das", "noch", "nim\u00b7mer", "beb\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was in der Brust in k\u00fchner Wahrheit lebte,", "tokens": ["Was", "in", "der", "Brust", "in", "k\u00fch\u00b7ner", "Wahr\u00b7heit", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das sollst du mutig in die L\u00fcfte streuen.", "tokens": ["Das", "sollst", "du", "mu\u00b7tig", "in", "die", "L\u00fcf\u00b7te", "streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": 