{"dta.poem.9227": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n Er ist ein Narr.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr leute gebt mir doch geschrieben/", "tokens": ["Ihr", "leu\u00b7te", "gebt", "mir", "doch", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich ein ertz-fantaste bin/", "tokens": ["Da\u00df", "ich", "ein", "ertz\u00b7fan\u00b7tas\u00b7te", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und solte mir es nicht belieben/", "tokens": ["Und", "sol\u00b7te", "mir", "es", "nicht", "be\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bringt mich mit gewalt dahin/", "tokens": ["So", "bringt", "mich", "mit", "ge\u00b7walt", "da\u00b7hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich die thorheit zum beschlu\u00df", "tokens": ["Da\u00df", "ich", "die", "thor\u00b7heit", "zum", "be\u00b7schlu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor aller welt bekennen mu\u00df.", "tokens": ["Vor", "al\u00b7ler", "welt", "be\u00b7ken\u00b7nen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ich h\u00f6re nichts mit meinem ohren-", "tokens": ["Ich", "h\u00f6\u00b7re", "nichts", "mit", "mei\u00b7nem", "oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin mit sehnden augen blind/", "tokens": ["Ich", "bin", "mit", "sehn\u00b7den", "au\u00b7gen", "blind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mund hat allen schmack verlohren/", "tokens": ["Der", "mund", "hat", "al\u00b7len", "schmack", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die f\u00e4uste sind nicht/ wo sie sind/", "tokens": ["Die", "f\u00e4us\u00b7te", "sind", "nicht", "/", "wo", "sie", "sind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PTKNEG", "$(", "PWAV", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die nase reucht/ und hat gleich wohl", "tokens": ["Die", "na\u00b7se", "reucht", "/", "und", "hat", "gleich", "wohl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den schnuppen/ wann sie riechen soll-", "tokens": ["Den", "schnup\u00b7pen", "/", "wann", "sie", "rie\u00b7chen", "soll"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PWAV", "PPER", "ADJA", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Das haubt ist wie ein tauben-haus", "tokens": ["Das", "haubt", "ist", "wie", "ein", "tau\u00b7ben\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "VAFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da fliegen mir die jungen narren", "tokens": ["Da", "flie\u00b7gen", "mir", "die", "jun\u00b7gen", "nar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald fornen ein/ bald hinten naus;", "tokens": ["Bald", "for\u00b7nen", "ein", "/", "bald", "hin\u00b7ten", "naus", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "$(", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch auf den abend ziehn sie hier", "tokens": ["Doch", "auf", "den", "a\u00b7bend", "ziehn", "sie", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zusammen wieder ins quartier.", "tokens": ["Zu\u00b7sam\u00b7men", "wie\u00b7der", "ins", "quar\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "4. Wolt ihr kein cl\u00e4u\u00dfgen bauen lassen/", "tokens": ["Wolt", "ihr", "kein", "cl\u00e4u\u00df\u00b7gen", "bau\u00b7en", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Darein ich mich versperren kan/", "tokens": ["Da\u00b7rein", "ich", "mich", "ver\u00b7sper\u00b7ren", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So hetzt die kinder auf der strassen", "tokens": ["So", "hetzt", "die", "kin\u00b7der", "auf", "der", "stras\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit hund und katzen auf mich an/", "tokens": ["Mit", "hund", "und", "kat\u00b7zen", "auf", "mich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und legt mir alle nahmen zu/", "tokens": ["Und", "legt", "mir", "al\u00b7le", "nah\u00b7men", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "VVFIN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bi\u00df ich nicht mehr so n\u00e4rrisch thu.", "tokens": ["Bi\u00df", "ich", "nicht", "mehr", "so", "n\u00e4r\u00b7risch", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Verbremt mir nur den kopf mit schellen/", "tokens": ["Ver\u00b7bremt", "mir", "nur", "den", "kopf", "mit", "schel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und setzt mir einen fuchsschwantz auf/", "tokens": ["Und", "setzt", "mir", "ei\u00b7nen", "fuchs\u00b7schwantz", "auf", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wollt ihr mir einen hut bestellen/", "tokens": ["Wollt", "ihr", "mir", "ei\u00b7nen", "hut", "be\u00b7stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So flickt mir auch ein k\u00fchhorn drauf/", "tokens": ["So", "flickt", "mir", "auch", "ein", "k\u00fch\u00b7horn", "drauf", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und gebt mir an des sebels statt", "tokens": ["Und", "gebt", "mir", "an", "des", "se\u00b7bels", "statt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein holtz das keine scheide hat.", "tokens": ["Ein", "holtz", "das", "kei\u00b7ne", "schei\u00b7de", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIAT", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Besetzt mein kleid mit bunten flecken", "tokens": ["Be\u00b7setzt", "mein", "kleid", "mit", "bun\u00b7ten", "fle\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht mirs band von bohnenstroh/", "tokens": ["Und", "macht", "mirs", "band", "von", "boh\u00b7nen\u00b7stroh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "VVFIN", "APPR", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schreibt mir an auf allen ecken/", "tokens": ["Und", "schreibt", "mir", "an", "auf", "al\u00b7len", "ec\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Di\u00df ist ein narr in folio;", "tokens": ["Di\u00df", "ist", "ein", "narr", "in", "fo\u00b7lio", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "APPR", "NE", "$."], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wofern ich bey dem narren-spiel", "tokens": ["Wo\u00b7fern", "ich", "bey", "dem", "nar\u00b7ren\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht zum erk\u00e4ntni\u00df kommen will.", "tokens": ["Nicht", "zum", "er\u00b7k\u00e4nt\u00b7ni\u00df", "kom\u00b7men", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Doch nein/ ich wil nun anders werden/", "tokens": ["Doch", "nein", "/", "ich", "wil", "nun", "an\u00b7ders", "wer\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$(", "PPER", "VMFIN", "ADV", "ADV", "VAINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mag kein pickelh\u00e4ring seyn", "tokens": ["Ich", "mag", "kein", "pi\u00b7ckel\u00b7h\u00e4\u00b7ring", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich stelle mich nur an geberden/", "tokens": ["Ich", "stel\u00b7le", "mich", "nur", "an", "ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bi\u00dfweilen n\u00e4rrisch auf den schein;", "tokens": ["Bi\u00df\u00b7wei\u00b7len", "n\u00e4r\u00b7risch", "auf", "den", "schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum lieber/ was verlacht ihr mich?", "tokens": ["Drum", "lie\u00b7ber", "/", "was", "ver\u00b7lacht", "ihr", "mich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "$(", "PWS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein jeder ist ein narr vor sich.", "tokens": ["Ein", "je\u00b7der", "ist", "ein", "narr", "vor", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "ADJD", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}