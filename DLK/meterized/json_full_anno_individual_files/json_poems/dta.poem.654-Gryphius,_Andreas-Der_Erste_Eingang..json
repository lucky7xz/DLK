{"dta.poem.654": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Der Erste Eingang.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "1. Die stille lust der angenehmen Nacht", "tokens": ["Die", "stil\u00b7le", "lust", "der", "an\u00b7ge\u00b7neh\u00b7men", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der ruhe zeit/ die alles schwartz anstreicht.", "tokens": ["Der", "ru\u00b7he", "zeit", "/", "die", "al\u00b7les", "schwartz", "an\u00b7streicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kr\u00f6nt nun jhr Haupt mit schimmerend-lichter pracht", "tokens": ["Kr\u00f6nt", "nun", "jhr", "Haupt", "mit", "schim\u00b7me\u00b7ren\u00b7dlich\u00b7ter", "pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der bleiche Mond/ der Sonnenbild entweicht. ", "tokens": ["Der", "blei\u00b7che", "Mond", "/", "der", "Son\u00b7nen\u00b7bild", "ent\u00b7weicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "2. Die Erd erstarr\u2019t. der faule Morpheus lert", "tokens": ["Die", "Erd", "er\u00b7starr'", "t.", "der", "fau\u00b7le", "Mor\u00b7pheus", "lert"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein feuchtes Horn auff tausend glieder aus", "tokens": ["Sein", "feuch\u00b7tes", "Horn", "auff", "tau\u00b7send", "glie\u00b7der", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "CARD", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vnd deckt mit schlaff/ was schmertz vnd tag beschwert/", "tokens": ["Vnd", "deckt", "mit", "schlaff", "/", "was", "schmertz", "vnd", "tag", "be\u00b7schwert", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "$(", "PWS", "ADJD", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der tr\u00e4ume schar schleicht eyn in H\u00fctt vnd Hau\u00df.", "tokens": ["Der", "tr\u00e4u\u00b7me", "schar", "schleicht", "eyn", "in", "H\u00fctt", "vnd", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "VVFIN", "ART", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "3. Die kleine welt/ das grosse Bizantz liegt", "tokens": ["Die", "klei\u00b7ne", "welt", "/", "das", "gros\u00b7se", "Bi\u00b7zantz", "liegt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In stoltzer Ruh. in dem sein Keyser wacht.", "tokens": ["In", "stolt\u00b7zer", "Ruh", ".", "in", "dem", "sein", "Key\u00b7ser", "wacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der grosse Printz/ der f\u00fcr vns kriegt vnd siegt ", "tokens": ["Der", "gros\u00b7se", "Printz", "/", "der", "f\u00fcr", "vns", "kriegt", "vnd", "siegt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "APPR", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vnd gantz zubricht der harten Persen macht.", "tokens": ["Vnd", "gantz", "zu\u00b7bricht", "der", "har\u00b7ten", "Per\u00b7sen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "4. Er wacht f\u00fcr vn\u00df! da\u00df Pontus stiller fleu\u00dft", "tokens": ["Er", "wacht", "f\u00fcr", "vn\u00df", "!", "da\u00df", "Pon\u00b7tus", "stil\u00b7ler", "fleu\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "$.", "KOUS", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Nilus dient/ da\u00df Ister dich verehrt/", "tokens": ["Da\u00df", "Ni\u00b7lus", "dient", "/", "da\u00df", "Is\u00b7ter", "dich", "ver\u00b7ehrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$(", "KOUS", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Vnd da\u00df der ", "tokens": ["Vnd", "da\u00df", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Entsteht/ weil jhn nicht einer schnarchen h\u00f6rt.", "tokens": ["Ent\u00b7steht", "/", "weil", "jhn", "nicht", "ei\u00b7ner", "schnar\u00b7chen", "h\u00f6rt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "PTKNEG", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "5. Er wacht f\u00fcr vn\u00df/ vnd der wacht \u00fcber jhn", "tokens": ["Er", "wacht", "f\u00fcr", "vn\u00df", "/", "vnd", "der", "wacht", "\u00fc\u00b7ber", "jhn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$(", "KON", "ART", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der F\u00fcrsten st\u00fcel auf steiffen Demand setz't ", "tokens": ["Der", "F\u00fcrs\u00b7ten", "st\u00fcel", "auf", "steif\u00b7fen", "De\u00b7mand", "setz't"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der F\u00fcrsten t\u00e4g\u2019 hei\u00dft au\u00df metallen zihn.", "tokens": ["Der", "F\u00fcrs\u00b7ten", "t\u00e4g'", "hei\u00dft", "au\u00df", "me\u00b7tal\u00b7len", "zihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vnd jhre feind mit schnellem blitz verletz\u2019t.", "tokens": ["Vnd", "jhre", "feind", "mit", "schnel\u00b7lem", "blitz", "ver\u00b7letz'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "6. Gott helt ob den/ die er selbst G\u00f6tter nenn\u2019t/", "tokens": ["Gott", "helt", "ob", "den", "/", "die", "er", "selbst", "G\u00f6t\u00b7ter", "nenn't", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOUS", "ART", "$(", "PRELS", "PPER", "ADV", "NN", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ob schon der ", "tokens": ["Ob", "schon", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Vnd sich vor wahn vnd rasen nicht mehr kenn\u2019t/", "tokens": ["Vnd", "sich", "vor", "wahn", "vnd", "ra\u00b7sen", "nicht", "mehr", "kenn't", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "KON", "VVFIN", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vnd Berg auf Berg/ vnd fel\u00df auff klippen st\u00fctzt.", "tokens": ["Vnd", "Berg", "auf", "Berg", "/", "vnd", "fel\u00df", "auff", "klip\u00b7pen", "st\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$(", "KON", "NN", "APPR", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "7. Ob Atlas gleich schon auff dem H", "tokens": ["Ob", "At\u00b7las", "gleich", "schon", "auff", "dem", "H"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "ADV", "APPR", "ART", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd Athos reicht an das bestern\u2019te schlo\u00df.", "tokens": ["Vnd", "A\u00b7thos", "reicht", "an", "das", "bes\u00b7tern'\u00b7te", "schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob man die th\u00fcr auch in den Himmel f\u00fcnd/ ", "tokens": ["Ob", "man", "die", "th\u00fcr", "auch", "in", "den", "Him\u00b7mel", "f\u00fcnd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Rhetus noch so starck vnd noch so gro\u00df:", "tokens": ["Wenn", "Rhe\u00b7tus", "noch", "so", "starck", "vnd", "noch", "so", "gro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ADV", "ADJD", "KON", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "8. So bleibt es doch/ so bleibts vmbsonst gewagt", "tokens": ["So", "bleibt", "es", "doch", "/", "so", "bleibts", "vmbsonst", "ge\u00b7wagt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "ADV", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Was sie gew\u00fcndscht! auf einen schlag verschwind", "tokens": ["Was", "sie", "ge\u00b7w\u00fcnd\u00b7scht", "!", "auf", "ei\u00b7nen", "schlag", "ver\u00b7schwind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Das lange werck. wer Gott in streit au\u00dftag\u2019t/", "tokens": ["Das", "lan\u00b7ge", "werck", ".", "wer", "Gott", "in", "streit", "au\u00df\u00b7tag't", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PWS", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wird asch/ vnd stanb/ vnd dunst/ vnd rauch/ vnd ", "tokens": ["Wird", "asch", "/", "vnd", "stanb", "/", "vnd", "dunst", "/", "vnd", "rauch", "/", "vnd"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADJD", "$(", "KON", "VVFIN", "$(", "KON", "ADV", "$(", "KON", "ADJD", "$(", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}