{"dta.poem.11480": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von einem soldaten, der wider die Fran-  \n zosen gedient, und an der frantzosen  \n gestorben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tenellus wagt es immer hin,", "tokens": ["Te\u00b7nel\u00b7lus", "wagt", "es", "im\u00b7mer", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hatte sich sehr hoch vermessen,", "tokens": ["Und", "hat\u00b7te", "sich", "sehr", "hoch", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er wolte die Frantzosen fressen;", "tokens": ["Er", "wol\u00b7te", "die", "Frant\u00b7zo\u00b7sen", "fres\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch die frantzosen frassen ihn.", "tokens": ["Doch", "die", "frant\u00b7zo\u00b7sen", "fras\u00b7sen", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}