{"textgrid.poem.59201": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So eitel k\u00fcnstlich haben sie verwoben", "tokens": ["So", "ei\u00b7tel", "k\u00fcnst\u00b7lich", "ha\u00b7ben", "sie", "ver\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kunst, die selber sie nicht gl\u00e4ubig achten,", "tokens": ["Die", "Kunst", ",", "die", "sel\u00b7ber", "sie", "nicht", "gl\u00e4u\u00b7big", "ach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie die S\u00fcnd in diese Unschuld brachten:", "tokens": ["Da\u00df", "sie", "die", "S\u00fcnd", "in", "die\u00b7se", "Un\u00b7schuld", "brach\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer unterscheidet, was noch stammt von oben?", "tokens": ["Wer", "un\u00b7ter\u00b7schei\u00b7det", ",", "was", "noch", "stammt", "von", "o\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wer mag w\u00fcrdig jene Reinen loben,", "tokens": ["Doch", "wer", "mag", "w\u00fcr\u00b7dig", "je\u00b7ne", "Rei\u00b7nen", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "ADJD", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die in der Zeit hochm\u00fct'gem Trieb und Trachten", "tokens": ["Die", "in", "der", "Zeit", "hoch\u00b7m\u00fct'\u00b7gem", "Trieb", "und", "Trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die heil'ge Flamme treu in sich bewachten,", "tokens": ["Die", "heil'\u00b7ge", "Flam\u00b7me", "treu", "in", "sich", "be\u00b7wach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus ihr die alte Sch\u00f6nheit neu erhoben!", "tokens": ["Aus", "ihr", "die", "al\u00b7te", "Sch\u00f6n\u00b7heit", "neu", "er\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Herr! gib Demut denen, die da irren,", "tokens": ["O", "Herr", "!", "gib", "De\u00b7mut", "de\u00b7nen", ",", "die", "da", "ir\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVIMP", "NN", "PDS", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Da\u00df, wenn ihr' K\u00fcnste all zuschanden werden,", "tokens": ["Da\u00df", ",", "wenn", "ih\u00b7r'", "K\u00fcns\u00b7te", "all", "zu\u00b7schan\u00b7den", "wer\u00b7den", ","], "