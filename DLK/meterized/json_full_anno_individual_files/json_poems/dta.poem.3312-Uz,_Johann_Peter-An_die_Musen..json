{"dta.poem.3312": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An die Musen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr holden Musen! wer, an eurer Brust erzogen,", "tokens": ["Ihr", "hol\u00b7den", "Mu\u00b7sen", "!", "wer", ",", "an", "eu\u00b7rer", "Brust", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PWS", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Weg zum gr\u00fcnen Pindus weis,", "tokens": ["Den", "Weg", "zum", "gr\u00fc\u00b7nen", "Pin\u00b7dus", "weis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird nicht von Golddurst aufs erz\u00fcrnte Meer be-", "tokens": ["Wird", "nicht", "von", "Gold\u00b7durst", "aufs", "er\u00b7z\u00fcrn\u00b7te", "Meer", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPR", "NN", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "trogen,", "tokens": ["tro\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Nicht auf des Hofes tr\u00fcglich Eis.", "tokens": ["Nicht", "auf", "des", "Ho\u00b7fes", "tr\u00fcg\u00b7lich", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er, dessen Scheitel unbethr\u00e4nter Lorbeer decket,", "tokens": ["Er", ",", "des\u00b7sen", "Schei\u00b7tel", "un\u00b7be\u00b7thr\u00e4n\u00b7ter", "Lor\u00b7beer", "de\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gl\u00e4nzt in der Themis Tempel nicht,", "tokens": ["Gl\u00e4nzt", "in", "der", "The\u00b7mis", "Tem\u00b7pel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Dorngestr\u00e4uche, mit verspritztem Blut beflecket,", "tokens": ["Wo", "Dorn\u00b7ge\u00b7str\u00e4u\u00b7che", ",", "mit", "ver\u00b7spritz\u00b7tem", "Blut", "be\u00b7fle\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich um die finstern Pfade flicht.", "tokens": ["Sich", "um", "die", "fins\u00b7tern", "Pfa\u00b7de", "flicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Begl\u00fcckter Weiser, der im Stillen sich erfreuet!", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "Wei\u00b7ser", ",", "der", "im", "Stil\u00b7len", "sich", "er\u00b7freu\u00b7et", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Tage werden uns gez\u00e4hlt,", "tokens": ["Die", "Ta\u00b7ge", "wer\u00b7den", "uns", "ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns aufgerechnet, die wir kluger Lust geweihet,", "tokens": ["Uns", "auf\u00b7ge\u00b7rech\u00b7net", ",", "die", "wir", "klu\u00b7ger", "Lust", "ge\u00b7wei\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wo wir th\u00f6richt uns gequ\u00e4lt.", "tokens": ["Und", "wo", "wir", "th\u00f6\u00b7richt", "uns", "ge\u00b7qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sollt ich, wie Harpax, wund von ungeliebter B\u00fcrde,", "tokens": ["Sollt", "ich", ",", "wie", "Har\u00b7pax", ",", "wund", "von", "un\u00b7ge\u00b7lieb\u00b7ter", "B\u00fcr\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "NE", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unausgeruht im Joche ziehn,", "tokens": ["Un\u00b7aus\u00b7ge\u00b7ruht", "im", "Jo\u00b7che", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich, wie Harpax, H\u00fcter stolzer Sch\u00e4tze w\u00fcrde,", "tokens": ["Da\u00df", "ich", ",", "wie", "Har\u00b7pax", ",", "H\u00fc\u00b7ter", "stol\u00b7zer", "Sch\u00e4t\u00b7ze", "w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "NE", "$,", "NN", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die eine scheue Tugend fliehn?", "tokens": ["Die", "ei\u00b7ne", "scheu\u00b7e", "Tu\u00b7gend", "fliehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Erkargte Sch\u00e4tze, schlummert nur bey meinen Feinden!", "tokens": ["Er\u00b7karg\u00b7te", "Sch\u00e4t\u00b7ze", ",", "schlum\u00b7mert", "nur", "bey", "mei\u00b7nen", "Fein\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich w\u00fcnsche nichts, als da\u00df ich frey,", "tokens": ["Ich", "w\u00fcn\u00b7sche", "nichts", ",", "als", "da\u00df", "ich", "frey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als da\u00df ich fr\u00f6hlig unter Musen, Wein und Freunden,", "tokens": ["Als", "da\u00df", "ich", "fr\u00f6h\u00b7lig", "un\u00b7ter", "Mu\u00b7sen", ",", "Wein", "und", "Freun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nie fremder Thorheit Sklave sey!", "tokens": ["Nie", "frem\u00b7der", "Thor\u00b7heit", "Skla\u00b7ve", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}