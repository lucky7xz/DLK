{"textgrid.poem.54687": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1. Uber wahre Best\u00e4ndigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Best\u00e4ndigkeit wird stehn! Will gleich der Feind betrigen!", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "wird", "stehn", "!", "Will", "gleich", "der", "Feind", "be\u00b7tri\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVINF", "$.", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Pocht gleich der tolle Feind! Ihr wird kein Glimpff obsigen.", "tokens": ["Pocht", "gleich", "der", "tol\u00b7le", "Feind", "!", "Ihr", "wird", "kein", "Glimpff", "ob\u00b7si\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie acht kein gl\u00e4ntzend Schwerd/ sie sch\u00e4tzt kein Ehren-Kron.", "tokens": ["Sie", "acht", "kein", "gl\u00e4nt\u00b7zend", "Schwerd", "/", "sie", "sch\u00e4tzt", "kein", "Eh\u00b7ren\u00b7Kron", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "PIAT", "ADJD", "NN", "$(", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Arbeit macht sie matt/ sie fragt nach keinem Hohn.", "tokens": ["Kein", "Ar\u00b7beit", "macht", "sie", "matt", "/", "sie", "fragt", "nach", "kei\u00b7nem", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJD", "$(", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nichts gilt der Worte Pracht/ nicht wilder Lewen Rach\u1ebd:", "tokens": ["Nichts", "gilt", "der", "Wor\u00b7te", "Pracht", "/", "nicht", "wil\u00b7der", "Le\u00b7wen", "Rach\u1ebd", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "$(", "PTKNEG", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Drew ihr mit Raad und Spi\u00df/ la\u00df Glutt und Flammen krachen!", "tokens": ["Drew", "ihr", "mit", "Raad", "und", "Spi\u00df", "/", "la\u00df", "Glutt", "und", "Flam\u00b7men", "kra\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "KON", "NN", "$(", "VVIMP", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Erl\u00e4ng ihr Lebens Zill! Hei\u00df sie in Angst vergehn!", "tokens": ["Er\u00b7l\u00e4ng", "ihr", "Le\u00b7bens", "Zill", "!", "Hei\u00df", "sie", "in", "Angst", "ver\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NE", "$.", "NN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja wirff den Himmel ein! Ists sie/ so wird sie stehn.", "tokens": ["Ja", "wirff", "den", "Him\u00b7mel", "ein", "!", "Ists", "sie", "/", "so", "wird", "sie", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": [