{"textgrid.poem.34861": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast mich beschworen aus dem Grab", "tokens": ["Du", "hast", "mich", "be\u00b7schwo\u00b7ren", "aus", "dem", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Durch deinen Zauberwillen,", "tokens": ["Durch", "dei\u00b7nen", "Zau\u00b7ber\u00b7wil\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Belebtest mich mit Wollustglut \u2013", "tokens": ["Be\u00b7leb\u00b7test", "mich", "mit", "Wol\u00b7lust\u00b7glut", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt kannst du die Glut nicht stillen.", "tokens": ["Jetzt", "kannst", "du", "die", "Glut", "nicht", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Pre\u00df deinen Mund an meinen Mund,", "tokens": ["Pre\u00df", "dei\u00b7nen", "Mund", "an", "mei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Menschen Odem ist g\u00f6ttlich!", "tokens": ["Der", "Men\u00b7schen", "O\u00b7dem", "ist", "g\u00f6tt\u00b7lich", "!