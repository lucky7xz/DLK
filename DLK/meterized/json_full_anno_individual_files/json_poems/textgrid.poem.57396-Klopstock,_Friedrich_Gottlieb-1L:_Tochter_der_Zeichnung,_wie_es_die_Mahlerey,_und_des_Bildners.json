{"textgrid.poem.57396": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tochter der Zeichnung, wie es die Mahlerey, und des Bildners", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tochter der Zeichnung, wie es die Mahlerey, und des Bildners", "tokens": ["Toch\u00b7ter", "der", "Zeich\u00b7nung", ",", "wie", "es", "die", "Mah\u00b7le\u00b7rey", ",", "und", "des", "Bild\u00b7ners"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Kunst ist, \u00c4tzerin, die mit dem Stahle Gestalten in Erzt gr\u00e4bt,", "tokens": ["Kunst", "ist", ",", "\u00c4t\u00b7ze\u00b7rin", ",", "die", "mit", "dem", "Stah\u00b7le", "Ge\u00b7stal\u00b7ten", "in", "Erzt", "gr\u00e4bt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Dann auf Bl\u00e4tter sie, diese beseelend,", "tokens": ["Dann", "auf", "Bl\u00e4t\u00b7ter", "sie", ",", "die\u00b7se", "be\u00b7see\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "$,", "PDS", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Senket, und hundertf\u00e4ltig sie dann", "tokens": ["Sen\u00b7ket", ",", "und", "hun\u00b7dert\u00b7f\u00e4l\u00b7tig", "sie", "dann"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "ADJD", "PPER", "ADV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Zeiget dem Blick der Schauenden, willst du auf immer vergessen", "tokens": ["Zei\u00b7get", "dem", "Blick", "der", "Schau\u00b7en\u00b7den", ",", "willst", "du", "auf", "im\u00b7mer", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "VMFIN", "PPER", "APPR", "ADV", "VVPP"], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Deiner Mutter? auf immer der Schwestern Werke nur bilden?", "tokens": ["Dei\u00b7ner", "Mut\u00b7ter", "?", "auf", "im\u00b7mer", "der", "Schwes\u00b7tern", "Wer\u00b7ke", "nur", "bil\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "APPR", "ADV", "ART", "NN", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Nie dich erinnern, dass nicht zur Sklavin", "tokens": ["Nie", "dich", "e\u00b7rin\u00b7nern", ",", "dass", "nicht", "zur", "Skla\u00b7vin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVINF", "$,", "KOUS", "PTKNEG", "APPRART", "NN"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ihre j\u00fcngste die Mutter gebar?", "tokens": ["Ih\u00b7re", "j\u00fcngs\u00b7te", "die", "Mut\u00b7ter", "ge\u00b7bar", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ART", "NN", "ADJD", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00c4tzerin, bey dem Verdienst, das den Wissenschaften so lang schon", "tokens": ["\u00c4t\u00b7ze\u00b7rin", ",", "bey", "dem", "Ver\u00b7dienst", ",", "das", "den", "Wis\u00b7sen\u00b7schaf\u00b7ten", "so", "lang", "schon"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "ADJD", "ADV"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Dich verb\u00fcndete! hey der Unsterblichkeit deiner Gestalten!", "tokens": ["Dich", "ver\u00b7b\u00fcn\u00b7de\u00b7te", "!", "hey", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "dei\u00b7ner", "Ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+--++--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "(die des Mahlenden bleichen, verschwinden:", "tokens": ["(", "die", "des", "Mah\u00b7len\u00b7den", "blei\u00b7chen", ",", "ver\u00b7schwin\u00b7den", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "ART", "NN", "VVINF", "$,", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Selbst die marmorne sinket, Ruin.)", "tokens": ["Selbst", "die", "mar\u00b7mor\u00b7ne", "sin\u00b7ket", ",", "Ru\u00b7in", ".", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "NN", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Bey den Thr\u00e4nen des Griechen, die er in Elisium weinet,", "tokens": ["Bey", "den", "Thr\u00e4\u00b7nen", "des", "Grie\u00b7chen", ",", "die", "er", "in", "E\u00b7li\u00b7si\u00b7um", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+--+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Dass du nicht Griechin bist, und wir, die entbehrende Nachwelt,", "tokens": ["Dass", "du", "nicht", "Grie\u00b7ch\u00b7in", "bist", ",", "und", "wir", ",", "die", "ent\u00b7beh\u00b7ren\u00b7de", "Nach\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "VAFIN", "$,", "KON", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Nun die Entdeckungen nicht Theophrastens,", "tokens": ["Nun", "die", "Ent\u00b7de\u00b7ckun\u00b7gen", "nicht", "Theo\u00b7ph\u00b7ras\u00b7tens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "NE", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Nicht die Grazien Sokrates sehn!", "tokens": ["Nicht", "die", "Gra\u00b7zi\u00b7en", "Sok\u00b7ra\u00b7tes", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NE", "VVINF", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Bey dem Allen beschw\u00f6r' ich dich: Erwach', und begreife,", "tokens": ["Bey", "dem", "Al\u00b7len", "be\u00b7schw\u00f6r'", "ich", "dich", ":", "Er\u00b7wach'", ",", "und", "be\u00b7grei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "$.", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wer du bist! Erfinde dir selbst Kunstw\u00fcrdiges! fleh dann,", "tokens": ["Wer", "du", "bist", "!", "Er\u00b7fin\u00b7de", "dir", "selbst", "Kunst\u00b7w\u00fcr\u00b7di\u00b7ges", "!", "fleh", "dann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$.", "NN", "PPER", "ADV", "NN", "$.", "VVFIN", "ADV", "$,"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Dass sie die Feuerblicke des Adlers,", "tokens": ["Dass", "sie", "die", "Feu\u00b7er\u00b7bli\u00b7cke", "des", "Ad\u00b7lers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und des Falken messenden Blick,", "tokens": ["Und", "des", "Fal\u00b7ken", "mes\u00b7sen\u00b7den", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Die dir gebe! diess fleh der Mutter. Wenn du von Ernst gl\u00fchst;", "tokens": ["Die", "dir", "ge\u00b7be", "!", "diess", "fleh", "der", "Mut\u00b7ter", ".", "Wenn", "du", "von", "Ernst", "gl\u00fchst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$.", "PDS", "VVFIN", "ART", "NN", "$.", "KOUS", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "H\u00f6rt sie dich. Ruhe dann auf deinem Lorber! Die Schwestern", "tokens": ["H\u00f6rt", "sie", "dich", ".", "Ru\u00b7he", "dann", "auf", "dei\u00b7nem", "Lor\u00b7ber", "!", "Die", "Schwes\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "PRF", "$.", "NN", "ADV", "APPR", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Kommen, freuen sich deiner, die eine", "tokens": ["Kom\u00b7men", ",", "freu\u00b7en", "sich", "dei\u00b7ner", ",", "die", "ei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PRF", "PPOSAT", "$,", "PRELS", "ART"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Mahlt dann, die andere meisselt dir nach.", "tokens": ["Mahlt", "dann", ",", "die", "an\u00b7de\u00b7re", "meis\u00b7selt", "dir", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PRELS", "PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Stolz sind deine Schwestern; allein auch gerecht; denn sie wissen,", "tokens": ["Stolz", "sind", "dei\u00b7ne", "Schwes\u00b7tern", ";", "al\u00b7lein", "auch", "ge\u00b7recht", ";", "denn", "sie", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "ADV", "ADJD", "$.", "KON", "PPER", "VVINF", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Dass, wenn auf Einem Schauplatz sie dem Auge sich zeigen,", "tokens": ["Dass", ",", "wenn", "auf", "Ei\u00b7nem", "Schau\u00b7platz", "sie", "dem", "Au\u00b7ge", "sich", "zei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "APPR", "ART", "NN", "PPER", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "---+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du sie auf hunderten zeigst; ah sie wissens,", "tokens": ["Du", "sie", "auf", "hun\u00b7der\u00b7ten", "zeigst", ";", "ah", "sie", "wis\u00b7sens", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPR", "ADJA", "VVFIN", "$.", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Dass du ihnen Unsterblichkeit giebst!", "tokens": ["Dass", "du", "ih\u00b7nen", "U\u00b7nsterb\u00b7lich\u00b7keit", "giebst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Tochter der Zeichnung, wie es die Mahlerey, und des Bildners", "tokens": ["Toch\u00b7ter", "der", "Zeich\u00b7nung", ",", "wie", "es", "die", "Mah\u00b7le\u00b7rey", ",", "und", "des", "Bild\u00b7ners"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Kunst ist, \u00c4tzerin, die mit dem Stahle Gestalten in Erzt gr\u00e4bt,", "tokens": ["Kunst", "ist", ",", "\u00c4t\u00b7ze\u00b7rin", ",", "die", "mit", "dem", "Stah\u00b7le", "Ge\u00b7stal\u00b7ten", "in", "Erzt", "gr\u00e4bt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Dann auf Bl\u00e4tter sie, diese beseelend,", "tokens": ["Dann", "auf", "Bl\u00e4t\u00b7ter", "sie", ",", "die\u00b7se", "be\u00b7see\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "$,", "PDS", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Senket, und hundertf\u00e4ltig sie dann", "tokens": ["Sen\u00b7ket", ",", "und", "hun\u00b7dert\u00b7f\u00e