{"dta.poem.13546": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Begeisterung, was falsch ist, zu entwirren,", "tokens": ["Be\u00b7geis\u00b7te\u00b7rung", ",", "was", "falsch", "ist", ",", "zu", "ent\u00b7wir\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Freudigkeit, wo's \u00f6de wird auf Erden,", "tokens": ["Und", "Freu\u00b7dig\u00b7keit", ",", "wo's", "\u00f6\u00b7de", "wird", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAV", "ADJD", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verleihe denen, die dich redlich suchen!", "tokens": ["Ver\u00b7lei\u00b7he", "de\u00b7nen", ",", "die", "dich", "red\u00b7lich", "su\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}