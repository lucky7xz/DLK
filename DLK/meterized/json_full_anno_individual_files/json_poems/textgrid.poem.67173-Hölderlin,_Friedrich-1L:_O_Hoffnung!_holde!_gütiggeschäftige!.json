{"textgrid.poem.67173": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Hoffnung! holde! g\u00fctiggesch\u00e4ftige!", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Hoffnung! holde! g\u00fctiggesch\u00e4ftige!", "tokens": ["O", "Hoff\u00b7nung", "!", "hol\u00b7de", "!", "g\u00fc\u00b7tig\u00b7ge\u00b7sch\u00e4f\u00b7ti\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "$.", "ADJA", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die du das Haus der Trauernden nicht verschm\u00e4hst,", "tokens": ["Die", "du", "das", "Haus", "der", "Trau\u00b7ern\u00b7den", "nicht", "ver\u00b7schm\u00e4hst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gerne dienend, Edle! zwischen", "tokens": ["Und", "ger\u00b7ne", "die\u00b7nend", ",", "Ed\u00b7le", "!", "zwi\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "VVPP", "$,", "ADJA", "$.", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sterblichen waltest und Himmelsm\u00e4chten,", "tokens": ["Sterb\u00b7li\u00b7chen", "wal\u00b7test", "und", "Him\u00b7mels\u00b7m\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wo bist du? wenig lebt ich; doch atmet kalt", "tokens": ["Wo", "bist", "du", "?", "we\u00b7nig", "lebt", "ich", ";", "doch", "at\u00b7met", "kalt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "PIS", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Abend schon. Und stille, den Schatten gleich,", "tokens": ["Mein", "A\u00b7bend", "schon", ".", "Und", "stil\u00b7le", ",", "den", "Schat\u00b7ten", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$.", "KON", "VVFIN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bin ich schon hier; und schon gesanglos", "tokens": ["Bin", "ich", "schon", "hier", ";", "und", "schon", "ge\u00b7sang\u00b7los"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$.", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlummert das schaudernde Herz im Busen.", "tokens": ["Schlum\u00b7mert", "das", "schau\u00b7dern\u00b7de", "Herz", "im", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Im gr\u00fcnen Tale, dort, wo der frische Quell", "tokens": ["Im", "gr\u00fc\u00b7nen", "Ta\u00b7le", ",", "dort", ",", "wo", "der", "fri\u00b7sche", "Quell"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vom Berge t\u00e4glich rauscht, und die liebliche", "tokens": ["Vom", "Ber\u00b7ge", "t\u00e4g\u00b7lich", "rauscht", ",", "und", "die", "lieb\u00b7li\u00b7c