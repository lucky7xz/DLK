{"textgrid.poem.52754": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Todt!? Nein nicht todt! begraben nur,", "tokens": ["Todt", "!?", "Nein", "nicht", "todt", "!", "be\u00b7gra\u00b7ben", "nur", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PTKANT", "PTKNEG", "ADJD", "$.", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nein! nicht begraben \u2013 noch einmal geboren", "tokens": ["Nein", "!", "nicht", "be\u00b7gra\u00b7ben", "\u2013", "noch", "ein\u00b7mal", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "PTKNEG", "VVPP", "$(", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist \u2013 Utiscz: Zunge der Natur!", "tokens": ["Ist", "\u2013", "U\u00b7tiscz", ":", "Zun\u00b7ge", "der", "Na\u00b7tur", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "NE", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es hat ein Genius nur sein Joch verloren.", "tokens": ["Es", "hat", "ein", "Ge\u00b7nius", "nur", "sein", "Joch", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Doch er ist todt, ja \u2013 todt.", "tokens": ["Doch", "er", "ist", "todt", ",", "ja", "\u2013", "todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["