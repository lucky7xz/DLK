{"dta.poem.7700": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll unsre Jugend nicht durchaus den Teufel missen,", "tokens": ["Soll", "uns\u00b7re", "Ju\u00b7gend", "nicht", "durc\u00b7haus", "den", "Teu\u00b7fel", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So la\u00dft sie wenigstens von ihm was Rechtes wissen.", "tokens": ["So", "la\u00dft", "sie", "we\u00b7nigs\u00b7tens", "von", "ihm", "was", "Rech\u00b7tes", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "PPER", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie lernt, der Teufel geh' umher als wie ein Leu,", "tokens": ["Sie", "lernt", ",", "der", "Teu\u00b7fel", "geh'", "um\u00b7her", "als", "wie", "ein", "Leu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "KOKOM", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der br\u00fcll' und suche wen er einschling' ohne Scheu.", "tokens": ["Der", "br\u00fcll'", "und", "su\u00b7che", "wen", "er", "ein\u00b7schling'", "oh\u00b7ne", "Scheu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PWS", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Teufel aber geht nicht mehr auf Mord und Rauben", "tokens": ["Der", "Teu\u00b7fel", "a\u00b7ber", "geht", "nicht", "mehr", "auf", "Mord", "und", "Rau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So l\u00f6wenhaft einher mit Br\u00fcllen und mit Schnauben.", "tokens": ["So", "l\u00f6\u00b7wen\u00b7haft", "ein\u00b7her", "mit", "Br\u00fcl\u00b7len", "und", "mit", "Schnau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er schleicht noch um vielleicht mit Arglist wie ein Fuchs,", "tokens": ["Er", "schleicht", "noch", "um", "viel\u00b7leicht", "mit", "Arg\u00b7list", "wie", "ein", "Fuchs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADV", "APPR", "NN", "KOKOM", "ART", "NE", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und lauert ungesehn mit Scharfsicht wie ein Luchs.", "tokens": ["Und", "lau\u00b7ert", "un\u00b7ge\u00b7sehn", "mit", "Scharf\u00b7sicht", "wie", "ein", "Luchs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "KOKOM", "ART", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie aber soll vor ihm das junge Volk sich h\u00fcten,", "tokens": ["Wie", "a\u00b7ber", "soll", "vor", "ihm", "das", "jun\u00b7ge", "Volk", "sich", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "APPR", "PPER", "ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ihn erkennen soll am Br\u00fcllen und am W\u00fcten?", "tokens": ["Das", "ihn", "er\u00b7ken\u00b7nen", "soll", "am", "Br\u00fcl\u00b7len", "und", "am", "W\u00fc\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVINF", "VMFIN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}