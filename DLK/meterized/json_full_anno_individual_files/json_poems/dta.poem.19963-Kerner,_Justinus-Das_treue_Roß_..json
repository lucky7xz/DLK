{"dta.poem.19963": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Das treue Ro\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Graf Turneck kam nach hartem Strau\u00df", "tokens": ["Graf", "Tur\u00b7neck", "kam", "nach", "har\u00b7tem", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Nacht wohl vor ein Gotteshaus.", "tokens": ["Bei", "Nacht", "wohl", "vor", "ein", "Got\u00b7tes\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Haus, das lag im Walde tief,", "tokens": ["Das", "Haus", ",", "das", "lag", "im", "Wal\u00b7de", "tief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner Gruft ein K\u00f6nig schlief.", "tokens": ["In", "sei\u00b7ner", "Gruft", "ein", "K\u00f6\u00b7nig", "schlief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier auszuruhn gedenkt der Graf,", "tokens": ["Hier", "aus\u00b7zu\u00b7ruhn", "ge\u00b7denkt", "der", "Graf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wei\u00df nicht, da\u00df ein Pfeil ihn traf.", "tokens": ["Er", "wei\u00df", "nicht", ",", "da\u00df", "ein", "Pfeil", "ihn", "traf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Graf steigt ab vom wei\u00dfen Ro\u00df;", "tokens": ["Der", "Graf", "steigt", "ab", "vom", "wei\u00b7\u00dfen", "Ro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201egras', bis ich wieder komm', im Moos!\u201c", "tokens": ["\u201e", "gras'", ",", "bis", "ich", "wie\u00b7der", "komm'", ",", "im", "Moos", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf f\u00e4hrt das Thor mit dumpfem Schall,", "tokens": ["Auf", "f\u00e4hrt", "das", "Thor", "mit", "dum\u00b7pfem", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann schweigt es in der weiten Hall'.", "tokens": ["Dann", "schweigt", "es", "in", "der", "wei\u00b7ten", "Hall'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Graf tappt hin an kalter Wand", "tokens": ["Der", "Graf", "tappt", "hin", "an", "kal\u00b7ter", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald einen alten Sarg er fand.", "tokens": ["Bald", "ei\u00b7nen", "al\u00b7ten", "Sarg", "er", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u201eder m\u00fcde Leib soll rasten hier;", "tokens": ["\u201e", "der", "m\u00fc\u00b7de", "Leib", "soll", "ras\u00b7ten", "hier", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VMFIN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versteinert Holz! brichst nicht mit mir.\u201c", "tokens": ["Ver\u00b7stei\u00b7nert", "Holz", "!", "brichst", "nicht", "mit", "mir", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "NN", "$.", "VVFIN", "PTKNEG", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Graf sich legt, so lang er war,", "tokens": ["Der", "Graf", "sich", "legt", ",", "so", "lang", "er", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wol auf dieselbe Todtenbahr.", "tokens": ["Wol", "auf", "die\u00b7sel\u00b7be", "Tod\u00b7ten\u00b7bahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Die Sonn' kam \u00fcber Berge roth,", "tokens": ["Die", "Sonn'", "kam", "\u00fc\u00b7ber", "Ber\u00b7ge", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Graf kam nicht, der Graf war todt.", "tokens": ["Der", "Graf", "kam", "nicht", ",", "der", "Graf", "war", "todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Seitdem verstrich manch hundert Jahr,", "tokens": ["Seit\u00b7dem", "ver\u00b7strich", "manch", "hun\u00b7dert", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PIAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein harrt das Ro\u00df noch immerdar.", "tokens": ["Sein", "harrt", "das", "Ro\u00df", "noch", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Vor'm Gotteshaus steht noch ein Stein,", "tokens": ["Vor'm", "Got\u00b7tes\u00b7haus", "steht", "noch", "ein", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dran grast das Ro\u00df im Mondenschein.", "tokens": ["Dran", "grast", "das", "Ro\u00df", "im", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}