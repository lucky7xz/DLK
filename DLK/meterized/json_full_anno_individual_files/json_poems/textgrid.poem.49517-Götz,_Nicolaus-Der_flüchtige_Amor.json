{"textgrid.poem.49517": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der fl\u00fcchtige Amor", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst sah ich den Cupido", "tokens": ["J\u00fcngst", "sah", "ich", "den", "Cu\u00b7pi\u00b7do"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Am Feuer brauner Augen,", "tokens": ["Am", "Feu\u00b7er", "brau\u00b7ner", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich kleine Pfeile schmieden;", "tokens": ["Sich", "klei\u00b7ne", "Pfei\u00b7le", "schmie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da trat ich etwas n\u00e4her,", "tokens": ["Da", "trat", "ich", "et\u00b7was", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und guckte zu, und lachte.", "tokens": ["Und", "guck\u00b7te", "zu", ",", "und", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da spr\u00fcheten auf einmahl", "tokens": ["Da", "spr\u00fc\u00b7he\u00b7ten", "auf", "ein\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "So viele Feuerfunken", "tokens": ["So", "vie\u00b7le", "Feu\u00b7er\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf seine nackten Glieder,", "tokens": ["Auf", "sei\u00b7ne", "nack\u00b7ten", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da\u00df er entfliehen wollte;", "tokens": ["Da\u00df", "er", "ent\u00b7flie\u00b7hen", "woll\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Doch seiner Fl\u00fcgel Spitzen", "tokens": ["Doch", "sei\u00b7ner", "Fl\u00fc\u00b7gel", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die waren schon versenget,", "tokens": ["Die", "wa\u00b7ren", "schon", "ver\u00b7sen\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und konnt' er nicht mehr weiter", "tokens": ["Und", "konnt'", "er", "nicht", "mehr", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Als in mein Herze flattern.", "tokens": ["Als", "in", "mein", "Her\u00b7ze", "flat\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "J\u00fcngst sah ich den Cupido", "tokens": ["J\u00fcngst", "sah", "ich", "den", "Cu\u00b7p