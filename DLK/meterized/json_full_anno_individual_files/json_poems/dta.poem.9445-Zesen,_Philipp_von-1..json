{"dta.poem.9445": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du gleichest Jerusalems Th\u00fcrnen und Auen/", "tokens": ["Du", "glei\u00b7chest", "Je\u00b7ru\u00b7sa\u00b7lems", "Th\u00fcr\u00b7nen", "und", "Au\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "KON", "NN", "$("], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du liebliches Thirza/ du herrliches Schlo\u00df/", "tokens": ["Du", "lieb\u00b7li\u00b7ches", "Thir\u00b7za", "/", "du", "herr\u00b7li\u00b7ches", "Schlo\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein blicken ist schrecklich wie spitzen zu schauen/", "tokens": ["Dein", "bli\u00b7cken", "ist", "schreck\u00b7lich", "wie", "spit\u00b7zen", "zu", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ADJD", "KOKOM", "VVINF", "PTKZU", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie spitzen des Heeres/ wie feindes-gescho\u00df.", "tokens": ["Wie", "spit\u00b7zen", "des", "Hee\u00b7res", "/", "wie", "fein\u00b7des\u00b7ge\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$(", "PWAV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Ach wende die pfeile", "tokens": ["Ach", "wen\u00b7de", "die", "pfei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Augen/ ach! eyle", "tokens": ["Der", "Au\u00b7gen", "/", "ach", "!", "ey\u00b7le"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$(", "XY", "$.", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Sie t\u00f6dten mich schier;", "tokens": ["Sie", "t\u00f6d\u00b7ten", "mich", "schier", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Sie regen im Hertzen", "tokens": ["Sie", "re\u00b7gen", "im", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJA", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Mehr marter und schmertzen/", "tokens": ["Mehr", "mar\u00b7ter", "und", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "So lange dein Auge wirst strahlen nach mir.", "tokens": ["So", "lan\u00b7ge", "dein", "Au\u00b7ge", "wirst", "strah\u00b7len", "nach", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VAFIN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Wie jenseit dem Eufrat die lustigen Ziegen", "tokens": ["Wie", "jen\u00b7seit", "dem", "Euf\u00b7rat", "die", "lus\u00b7ti\u00b7gen", "Zie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf Galaad h\u00fcpfen und gleichen dem klee/", "tokens": ["Auf", "Ga\u00b7laad", "h\u00fcp\u00b7fen", "und", "glei\u00b7chen", "dem", "klee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "KON", "ADJA", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So m\u00fcssen die Haare sich schwingen und fl\u00fcgen", "tokens": ["So", "m\u00fcs\u00b7sen", "die", "Haa\u00b7re", "sich", "schwin\u00b7gen", "und", "fl\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "VVINF", "KON", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "umb deine verliebete stirne wie schnee.", "tokens": ["umb", "dei\u00b7ne", "ver\u00b7lie\u00b7be\u00b7te", "stir\u00b7ne", "wie", "schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "ADV", "KOKOM", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Den Z\u00e4hnen ingleichen", "tokens": ["Den", "Z\u00e4h\u00b7nen", "in\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Herde mu\u00df weichen", "tokens": ["Die", "Her\u00b7de", "mu\u00df", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "im wasser geschwem\u0303t/", "tokens": ["im", "was\u00b7ser", "ge\u00b7schwem\u0303t", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die allzumahl tr\u00e4get/", "tokens": ["Die", "all\u00b7zu\u00b7mahl", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Viel wollust erreget/", "tokens": ["Viel", "wol\u00b7lust", "er\u00b7re\u00b7get", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Die jmmer mit doppelten fr\u00fcchten ank\u00f6m\u0303t.", "tokens": ["Die", "jm\u00b7mer", "mit", "dop\u00b7pel\u00b7ten", "fr\u00fcch\u00b7ten", "an\u00b7k\u00f6m\u0303t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "ADJA", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Der Granat an farbe den Wangen mu\u00df weichen/", "tokens": ["Der", "Gra\u00b7nat", "an", "far\u00b7be", "den", "Wan\u00b7gen", "mu\u00df", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die zwischen den z\u00f6pfen volf\u00fcren den schein.", "tokens": ["Die", "zwi\u00b7schen", "den", "z\u00f6p\u00b7fen", "vol\u00b7f\u00fc\u00b7ren", "den", "schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Zwar sechzig der K\u00f6nigin/ Achzig ingleichen", "tokens": ["Zwar", "sech\u00b7zig", "der", "K\u00f6\u00b7ni\u00b7gin", "/", "Ach\u00b7zig", "in\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "CARD", "ART", "NN", "$(", "CARD", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Frauen/ die Zooffen unz\u00e4hlich auch seyn.", "tokens": ["Der", "Frau\u00b7en", "/", "die", "Zoo\u00b7ffen", "un\u00b7z\u00e4h\u00b7lich", "auch", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "ADJD", "ADV", "VAINF", "$."], "meter": "-+--+-+---+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Doch bleibestu Meine/", "tokens": ["Doch", "blei\u00b7be\u00b7stu", "Mei\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "O Freundin/ alleine/", "tokens": ["O", "Freun\u00b7din", "/", "al\u00b7lei\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$(", "ADV", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mein trautes Gemahl;", "tokens": ["Mein", "trau\u00b7tes", "Ge\u00b7mahl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die Mutter dich liebet/", "tokens": ["Die", "Mut\u00b7ter", "dich", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Dier alles ergiebet/", "tokens": ["Dier", "al\u00b7les", "er\u00b7gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Dich loben die T\u00f6chter mit gro\u00dfer anzahl.", "tokens": ["Dich", "lo\u00b7ben", "die", "T\u00f6ch\u00b7ter", "mit", "gro\u00b7\u00dfer", "an\u00b7zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}