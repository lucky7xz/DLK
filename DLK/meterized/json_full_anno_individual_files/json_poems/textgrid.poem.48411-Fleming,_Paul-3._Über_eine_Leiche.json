{"textgrid.poem.48411": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3. \u00dcber eine Leiche", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer jung stirbt, der stirbt wol. Wen Gott zu lieben pflegt,", "tokens": ["Wer", "jung", "stirbt", ",", "der", "stirbt", "wol", ".", "Wen", "Gott", "zu", "lie\u00b7ben", "pflegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "$.", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der wird in seiner Bl\u00fct' in frischen Sand gelegt.", "tokens": ["der", "wird", "in", "sei\u00b7ner", "Bl\u00fct'", "in", "fri\u00b7schen", "Sand", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Tod h\u00e4lt gleiches Recht. Wer hundertj\u00e4hrig stirbet,", "tokens": ["Der", "Tod", "h\u00e4lt", "glei\u00b7ches", "Recht", ".", "Wer", "hun\u00b7dert\u00b7j\u00e4h\u00b7rig", "stir\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$.", "PWS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "verweset ja so bald, als der, so jung verdirbet", "tokens": ["ver\u00b7we\u00b7set", "ja", "so", "bald", ",", "als", "der", ",", "so", "jung", "ver\u00b7dir\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "$,", "KOUS", "ART", "$,", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und besser stirbt als er. Ist der schon nicht so alt,", "tokens": ["und", "bes\u00b7ser", "stirbt", "als", "er", ".", "Ist", "der", "schon", "nicht", "so", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "KOUS", "PPER", "$.", "VAFIN", "ART", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "so hat er ja auch nicht so viel und mannigfalt", "tokens": ["so", "hat", "er", "ja", "auch", "nicht", "so", "viel", "und", "man\u00b7nig\u00b7falt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "verletzet seinen Gott. Di\u00df ists, das uns das Ende", "tokens": ["ver\u00b7let\u00b7zet", "sei\u00b7nen", "Gott", ".", "Di\u00df", "ists", ",", "das", "uns", "das", "En\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "PDS", "VAFIN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "zu machen sauer pflegt, da\u00df man nicht reine H\u00e4nde", "tokens": ["zu", "ma\u00b7chen", "sau\u00b7er", "pflegt", ",", "da\u00df", "man", "nicht", "rei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ADJD", "VVFIN", "$,", "KOUS", "PIS", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und ein Gewissen hat, da\u00df ihm nichts ist bewust", "tokens": ["und", "ein", "Ge\u00b7wis\u00b7sen", "hat", ",", "da\u00df", "ihm", "nichts", "ist", "be\u00b7wust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "PIS", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "als treue Redligkeit. Ein Junger stirbt mit Lust,", "tokens": ["als", "treu\u00b7e", "Red\u00b7lig\u00b7keit", ".", "Ein", "Jun\u00b7ger", "stirbt", "mit", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "wei\u00df nicht, was Seelenangst und Herzensst\u00f6\u00dfe hei\u00dfen,", "tokens": ["wei\u00df", "nicht", ",", "was", "See\u00b7len\u00b7angst", "und", "Her\u00b7zens\u00b7st\u00f6\u00b7\u00dfe", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "die \u00e4rger als der Krebs nach frischer Seelen bei\u00dfen", "tokens": ["die", "\u00e4r\u00b7ger", "als", "der", "Krebs", "nach", "fri\u00b7scher", "See\u00b7len", "bei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "und t\u00f6ten, eh' der Tod uns noch die Sense beut", "tokens": ["und", "t\u00f6\u00b7ten", ",", "eh'", "der", "Tod", "uns", "noch", "die", "Sen\u00b7se", "beut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und auf das kranke Fleisch aus vollen Kr\u00e4ften h\u00e4ut.", "tokens": ["und", "auf", "das", "kran\u00b7ke", "Fleisch", "aus", "vol\u00b7len", "Kr\u00e4f\u00b7ten", "h\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Im Sterben findet sichs: wie Einer hat gelebet,", "tokens": ["Im", "Ster\u00b7ben", "fin\u00b7det", "sichs", ":", "wie", "Ei\u00b7ner", "hat", "ge\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "$.", "PWAV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "so krankt, so stirbt er auch. Ein furchtsam Herze bebet", "tokens": ["so", "krankt", ",", "so", "stirbt", "er", "auch", ".", "Ein", "furcht\u00b7sam", "Her\u00b7ze", "be\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$.", "ART", "ADJD", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "und steht in steter Angst. Wer Gott zum Freunde wei\u00df,", "tokens": ["und", "steht", "in", "ste\u00b7ter", "Angst", ".", "Wer", "Gott", "zum", "Freun\u00b7de", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$.", "PWS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "dem macht kein Schrecken kalt, kein Tr\u00fcbsalsfeuer hei\u00df.", "tokens": ["dem", "macht", "kein", "Schre\u00b7cken", "kalt", ",", "kein", "Tr\u00fcb\u00b7sals\u00b7feu\u00b7er", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "ADJD", "$,", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So stirbt ein junger Mensch. Was ists noch zu erz\u00e4hlen,", "tokens": ["So", "stirbt", "ein", "jun\u00b7ger", "Mensch", ".", "Was", "ists", "noch", "zu", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "PWS", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "mit was wir Alten sonst uns pflegen stets zu qu\u00e4len,", "tokens": ["mit", "was", "wir", "Al\u00b7ten", "sonst", "uns", "pfle\u00b7gen", "stets", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "ADV", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "das uns bei Tage bla\u00df, bei Nachte bange macht?", "tokens": ["das", "uns", "bei", "Ta\u00b7ge", "bla\u00df", ",", "bei", "Nach\u00b7te", "ban\u00b7ge", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADJD", "$,", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein Ieder wei\u00df f\u00fcr sich, wie, wo, was er verbracht,", "tokens": ["Ein", "Ie\u00b7der", "wei\u00df", "f\u00fcr", "sich", ",", "wie", ",", "wo", ",", "was", "er", "ver\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "$,", "PWAV", "$,", "PWAV", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "das jener gro\u00dfe Tag soll an die Sonne bringen,", "tokens": ["das", "je\u00b7ner", "gro\u00b7\u00dfe", "Tag", "soll", "an", "die", "Son\u00b7ne", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "daf\u00fcr sich mancher scheut. Vor so dergleichen Dingen", "tokens": ["da\u00b7f\u00fcr", "sich", "man\u00b7cher", "scheut", ".", "Vor", "so", "derg\u00b7lei\u00b7chen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PRF", "PIS", "VVFIN", "$.", "APPR", "ADV", "PIS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "sind Kinder noch befreit. Drum, blasse Mutter, denkt,", "tokens": ["sind", "Kin\u00b7der", "noch", "be\u00b7freit", ".", "Drum", ",", "blas\u00b7se", "Mut\u00b7ter", ",", "denkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "VVPP", "$.", "PAV", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "ob euch der harte Fall auch denn so billich kr\u00e4nkt,", "tokens": ["ob", "euch", "der", "har\u00b7te", "Fall", "auch", "denn", "so", "bil\u00b7lich", "kr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "als wie ihr wol vermeint! Wem fromme Kinder sterben,", "tokens": ["als", "wie", "ihr", "wol", "ver\u00b7meint", "!", "Wem", "from\u00b7me", "Kin\u00b7der", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPER", "ADV", "VVPP", "$.", "PWS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "der wei\u00df, was er der Welt und Himmel l\u00e4\u00dft zu erben:", "tokens": ["der", "wei\u00df", ",", "was", "er", "der", "Welt", "und", "Him\u00b7mel", "l\u00e4\u00dft", "zu", "er\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$,", "PWS", "PPER", "ART", "NN", "KON", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "der Erden zwar den Leib, als der sie Mutter hei\u00dft,", "tokens": ["der", "Er\u00b7den", "zwar", "den", "Leib", ",", "als", "der", "sie", "Mut\u00b7ter", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$,", "KOUS", "ART", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "und als sein Vaterrecht dem Himmel seinen Geist.", "tokens": ["und", "als", "sein", "Va\u00b7ter\u00b7recht", "dem", "Him\u00b7mel", "sei\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer jung stirbt, der stirbt wol. Wen Gott zu lieben pflegt,", "tokens": ["Wer", "jung", "stirbt", ",", "der", "stirbt", "wol", ".", "Wen", "Gott", "zu", "lie\u00b7ben", "pflegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "$.", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der wird in seiner Bl\u00fct' in frischen Sand gelegt.", "tokens": ["der", "wird", "in", "sei\u00b7ner", "Bl\u00fct'", "in", "fri\u00b7schen", "Sand", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Tod h\u00e4lt gleiches Recht. Wer hundertj\u00e4hrig stirbet,", "tokens": ["Der", "Tod", "h\u00e4lt", "glei\u00b7ches", "Recht", ".", "Wer", "hun\u00b7dert\u00b7j\u00e4h\u00b7rig", "stir\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$.", "PWS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "verweset ja so bald, als der, so jung verdirbet", "tokens": ["ver\u00b7we\u00b7set", "ja", "so", "bald", ",", "als", "der", ",", "so", "jung", "ver\u00b7dir\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "$,", "KOUS", "ART", "$,", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measu