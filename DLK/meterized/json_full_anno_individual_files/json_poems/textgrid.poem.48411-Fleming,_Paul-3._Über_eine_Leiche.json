, "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und auf das kranke Fleisch aus vollen Kr\u00e4ften h\u00e4ut.", "tokens": ["und", "auf", "das", "kran\u00b7ke", "Fleisch", "aus", "vol\u00b7len", "Kr\u00e4f\u00b7ten", "h\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Im Sterben findet sichs: wie Einer hat gelebet,", "tokens": ["Im", "Ster\u00b7ben", "fin\u00b7det", "sichs", ":", "wie", "Ei\u00b7ner", "hat", "ge\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "$.", "PWAV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "so krankt, so stirbt er auch. Ein furchtsam Herze bebet", "tokens": ["so", "krankt", ",", "so", "stirbt", "er", "auch", ".", "Ein", "furcht\u00b7sam", "Her\u00b7ze", "be\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$.", "ART", "ADJD", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "und steht in steter Angst. Wer Gott zum Freunde wei\u00df,", "tokens": ["und", "steht", "in", "ste\u00b7ter", "Angst", ".", "Wer", "Gott", "zum", "Freun\u00b7de", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$.", "PWS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "dem macht kein Schrecken kalt, kein Tr\u00f