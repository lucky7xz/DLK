{"textgrid.poem.24416": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Winter sey so unbeqvem/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Winter sey so unbeqvem/", "tokens": ["Der", "Win\u00b7ter", "sey", "so", "un\u00b7be\u00b7qvem", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man schimpfe seine kalten Tage:", "tokens": ["Man", "schimp\u00b7fe", "sei\u00b7ne", "kal\u00b7ten", "Ta\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So ist er mir doch angenehm.", "tokens": ["So", "ist", "er", "mir", "doch", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat jede Jahres Zeit", "tokens": ["Hat", "je\u00b7de", "Jah\u00b7res", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht ihre Plage?", "tokens": ["Nicht", "ih\u00b7re", "Pla\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Der Fr\u00fchling ist voll Unbest\u00e4ndigkeit/", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "voll", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$("], "meter": "-