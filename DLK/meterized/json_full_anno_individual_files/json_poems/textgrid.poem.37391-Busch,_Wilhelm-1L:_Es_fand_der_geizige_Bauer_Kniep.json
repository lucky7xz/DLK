{"textgrid.poem.37391": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es fand der geizige Bauer Kniep", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fand der geizige Bauer Kniep", "tokens": ["Es", "fand", "der", "gei\u00b7zi\u00b7ge", "Bau\u00b7er", "Kniep"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Grabe keine Ruhe.", "tokens": ["Im", "Gra\u00b7be", "kei\u00b7ne", "Ru\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sehnsucht nach dem Gelde trieb", "tokens": ["Die", "Sehn\u00b7sucht", "nach", "dem", "Gel\u00b7de", "trieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn wieder zu seiner Truhe.", "tokens": ["Ihn", "wie\u00b7der", "zu", "sei\u00b7ner", "Tru\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Erben wollten diesen Gast", "tokens": ["Die", "Er\u00b7ben", "woll\u00b7ten", "die\u00b7sen", "Gast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Haus durchaus nicht haben,", "tokens": ["Im", "Haus", "durc\u00b7haus", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil ihnen der Verkehr verha\u00dft", "tokens": ["Weil", "ih\u00b7nen", "der", "Ver\u00b7kehr", "ver\u00b7ha\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem, der schon begraben.", "tokens": ["Mit", "ei\u00b7nem", ",", "der", "schon", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie dachten, vor Drudenfu\u00df und Kreuz", "tokens": ["Sie", "dach\u00b7ten", ",", "vor", "Dru\u00b7den\u00b7fu\u00df", "und", "Kreuz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ergebenst verschwinden sollt er.", "tokens": ["Er\u00b7ge\u00b7benst", "ver\u00b7schwin\u00b7den", "sollt", "er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er aber vollf\u00fchrte seinerseits", "tokens": ["Er", "a\u00b7ber", "voll\u00b7f\u00fchr\u00b7te", "sei\u00b7ner\u00b7seits"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nur um so mehr Gepolter.", "tokens": ["Nur", "um", "so", "mehr", "Ge\u00b7pol\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zum Gl\u00fcck kam gerade zugereist", "tokens": ["Zum", "Gl\u00fcck", "kam", "ge\u00b7ra\u00b7de", "zu\u00b7ge\u00b7reist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "VVPP"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ein Meister, der vieles erkundet.", "tokens": ["Ein", "Meis\u00b7ter", ",", "der", "vie\u00b7les", "er\u00b7kun\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der hat gar schlau den b\u00f6sen Geist", "tokens": ["Der", "hat", "gar", "schlau", "den", "b\u00f6\u00b7sen", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einem Fa\u00df verspundet.", "tokens": ["In", "ei\u00b7nem", "Fa\u00df", "ver\u00b7spun\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Man fuhr es bequem, als w\u00e4r es leer,", "tokens": ["Man", "fuhr", "es", "be\u00b7quem", ",", "als", "w\u00e4r", "es", "leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "$,", "KOKOM", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bis an ein flie\u00dfend Gew\u00e4sser.", "tokens": ["Bis", "an", "ein", "flie\u00b7\u00dfend", "Ge\u00b7w\u00e4s\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da pl\u00f6tzlich machte sich Kniep so schwer", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "mach\u00b7te", "sich", "Kniep", "so", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PRF", "NE", "ADV", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie zehn gef\u00fcllte F\u00e4sser.", "tokens": ["Wie", "zehn", "ge\u00b7f\u00fcll\u00b7te", "F\u00e4s\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gottlieb, der Kutscher, wundert sich.", "tokens": ["Gott\u00b7lieb", ",", "der", "Kut\u00b7scher", ",", "wun\u00b7dert", "sich", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach r\u00fcckw\u00e4rts blickt er schnelle.", "tokens": ["Nach", "r\u00fcck\u00b7w\u00e4rts", "blickt", "er", "schnel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wumm, knallt der Spund. Der Geist entwich", "tokens": ["Wumm", ",", "knallt", "der", "Spund", ".", "Der", "Geist", "ent\u00b7wich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "$.", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und spukt an der alten Stelle.", "tokens": ["Und", "spukt", "an", "der", "al\u00b7ten", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Wie sonst besucht er jede Nacht", "tokens": ["Wie", "sonst", "be\u00b7sucht", "er", "je\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die eisenbeschlagene Kiste", "tokens": ["Die", "ei\u00b7sen\u00b7be\u00b7schla\u00b7ge\u00b7ne", "Kis\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und rumpelt, hustet, niest und lacht,", "tokens": ["Und", "rum\u00b7pelt", ",", "hus\u00b7tet", ",", "niest", "und", "lacht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob er von nichts was w\u00fc\u00dfte.", "tokens": ["Als", "ob", "er", "von", "nichts", "was", "w\u00fc\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PIS", "PWS", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Kein Mittel erwies sich als probat.", "tokens": ["Kein", "Mit\u00b7tel", "er\u00b7wies", "sich", "als", "pro\u00b7bat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "KOKOM", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Geist ward nur erboster.", "tokens": ["Der", "Geist", "ward", "nur", "er\u00b7bos\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man trug, es blieb kein andrer Rat,", "tokens": ["Man", "trug", ",", "es", "blieb", "kein", "an\u00b7drer", "Rat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Kasten zum n\u00e4chsten Kloster.", "tokens": ["Den", "Kas\u00b7ten", "zum", "n\u00e4chs\u00b7ten", "Klos\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Der Pf\u00f6rtner sprach: Willkommen im Stift", "tokens": ["Der", "Pf\u00f6rt\u00b7ner", "sprach", ":", "Will\u00b7kom\u00b7men", "im", "Stift"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und herzlich guten Morgen!", "tokens": ["Und", "herz\u00b7lich", "gu\u00b7ten", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was Geld und b\u00f6se Geister betrifft,", "tokens": ["Was", "Geld", "und", "b\u00f6\u00b7se", "Geis\u00b7ter", "be\u00b7tr\u00b7ifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das wollen wir schon besorgen.", "tokens": ["Das", "wol\u00b7len", "wir", "schon", "be\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Es fand der geizige Bauer Kniep", "tokens": ["Es", "fand", "der", "gei\u00b7zi\u00b7ge", "Bau\u00b7er", "Kniep"], "token_info": ["word", "word", "word", "word", "word", "