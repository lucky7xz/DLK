{"textgrid.poem.63528": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Waldesnacht", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Waldesnacht, du wunderk\u00fchle,", "tokens": ["Wal\u00b7des\u00b7nacht", ",", "du", "wun\u00b7der\u00b7k\u00fch\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich tausend Male gr\u00fc\u00df',", "tokens": ["Die", "ich", "tau\u00b7send", "Ma\u00b7le", "gr\u00fc\u00df'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "CARD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem lauten Weltgew\u00fchle", "tokens": ["Nach", "dem", "lau\u00b7ten", "Welt\u00b7ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie ist dein Rauschen s\u00fc\u00df!", "tokens": ["O", "wie", "ist", "dein", "Rau\u00b7schen", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00e4umerisch die m\u00fcden Glieder", "tokens": ["Tr\u00e4u\u00b7me\u00b7risch", "die", "m\u00fc\u00b7den", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Berg' ich weich ins Moos,", "tokens": ["Ber\u00b7g'", "ich", "weich", "ins", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPRART", "N