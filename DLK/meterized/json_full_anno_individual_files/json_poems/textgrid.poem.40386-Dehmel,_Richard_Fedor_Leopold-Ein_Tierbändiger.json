{"textgrid.poem.40386": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Tierb\u00e4ndiger", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann Tierb\u00e4ndiger werden,", "tokens": ["Ich", "kann", "Tier\u00b7b\u00e4n\u00b7di\u00b7ger", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "ich bin den Bestien gut;", "tokens": ["ich", "bin", "den", "Be\u00b7sti\u00b7en", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "sie w\u00fcrden gerne Menschen sein,", "tokens": ["sie", "w\u00fcr\u00b7den", "ger\u00b7ne", "Men\u00b7schen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "nur Qual ist ihre Wut,", "tokens": ["nur", "Qual", "ist", "ih\u00b7re", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "drum sind ihre Augen so traurig.", "tokens": ["drum", "sind", "ih\u00b7re", "Au\u00b7gen", "so", "trau\u00b7rig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "So wie in Wahnsinn versunken,", "tokens": ["So", "wie", "in", "Wahn\u00b7sinn", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "so gl\u00e4sern manchmal, so stier.", "tokens": ["so", "gl\u00e4\u00b7sern", "manch\u00b7mal", ",", "so", "stier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Aber man braucht sie blos zu lieben,", "tokens": ["A\u00b7ber", "man", "braucht", "sie", "blos", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "das f\u00fchlen sie ganz wie wir", "tokens": ["das", "f\u00fch\u00b7len", "sie", "ganz", "wie", "wir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "KOKOM", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "und lernen Vernunft annehmen.", "tokens": ["und", "ler\u00b7nen", "Ver\u00b7nunft", "an\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Neulich am Raubtierk\u00e4fig", "tokens": ["Neu\u00b7lich", "am", "Raub\u00b7tier\u00b7k\u00e4\u00b7fig"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "bot ich dem Tiger die Hand.", "tokens": ["bot", "ich", "dem", "Ti\u00b7ger", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Er sah mich lange schnurrig an,", "tokens": ["Er", "sah", "mich", "lan\u00b7ge", "schnur\u00b7rig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "bis er mein Herz verstand;", "tokens": ["bis", "er", "mein", "Herz", "ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "dann lie\u00df er sich ruhig tatzeln.", "tokens": ["dann", "lie\u00df", "er", "sich", "ru\u00b7hig", "tat\u00b7zeln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Er g\u00e4hnte wie im Cirkus", "tokens": ["Er", "g\u00e4hn\u00b7te", "wie", "im", "Cir\u00b7kus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und bog die Schwanzspitze sacht.", "tokens": ["und", "bog", "die", "Schwanz\u00b7spit\u00b7ze", "sacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wette, den d\u00fcrft ich karbatschen,", "tokens": ["Ich", "wet\u00b7te", ",", "den", "d\u00fcrft", "ich", "kar\u00b7bat\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "er d\u00e4chte: Du hast die Macht,", "tokens": ["er", "d\u00e4ch\u00b7te", ":", "Du", "hast", "die", "Macht", ","], "token_inf