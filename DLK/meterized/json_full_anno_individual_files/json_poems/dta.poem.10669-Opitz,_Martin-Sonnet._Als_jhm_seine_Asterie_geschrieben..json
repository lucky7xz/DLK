{"dta.poem.10669": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n  Als jhm seine Asterie geschrieben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.57", "af:0.42"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Wer solte dieses wol in sein Gem\u00fcthe bringen/", "tokens": ["Wer", "sol\u00b7te", "die\u00b7ses", "wol", "in", "sein", "Ge\u00b7m\u00fc\u00b7the", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df vnder wei\u00df vnd schwartz verborgen solche Freudt?", "tokens": ["Da\u00df", "vn\u00b7der", "wei\u00df", "vnd", "schwartz", "ver\u00b7bor\u00b7gen", "sol\u00b7che", "Freudt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "ADJD", "VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df nur ein einig Brieff nemm\u2019 alle Traurigkeit?", "tokens": ["Da\u00df", "nur", "ein", "ei\u00b7nig", "Brieff", "nemm'", "al\u00b7le", "Trau\u00b7rig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan auch der Augenlust so weit ins Hertze dringen?", "tokens": ["Kan", "auch", "der", "Au\u00b7gen\u00b7lust", "so", "weit", "ins", "Hert\u00b7ze", "drin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "ADJD", "APPRART", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wei\u00df die Sinne fast nicht h\u00f6her mehr zuschwingen/", "tokens": ["Ich", "wei\u00df", "die", "Sin\u00b7ne", "fast", "nicht", "h\u00f6\u00b7her", "mehr", "zu\u00b7schwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd habe wol mit flei\u00df gelesen jederzeit/", "tokens": ["Vnd", "ha\u00b7be", "wol", "mit", "flei\u00df", "ge\u00b7le\u00b7sen", "je\u00b7der\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "NN", "VVPP", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was von der Liebe nur gefunden weit vnd breit/", "tokens": ["Was", "von", "der", "Lie\u00b7be", "nur", "ge\u00b7fun\u00b7den", "weit", "vnd", "breit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "VVPP", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es hat mich aber nichts vermocht so sehr zuzwingen/", "tokens": ["Es", "hat", "mich", "a\u00b7ber", "nichts", "ver\u00b7mocht", "so", "sehr", "zu\u00b7zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PIS", "VVFIN", "ADV", "ADV", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Grich Anacreon/ der Sappho sch\u00f6n Gedicht/", "tokens": ["Der", "Grich", "A\u00b7na\u00b7cre\u00b7on", "/", "der", "Sap\u00b7pho", "sch\u00f6n", "Ge\u00b7dicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$(", "ART", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd auch Ovidius sind jhm zugleichen nicht/", "tokens": ["Vnd", "auch", "O\u00b7vi\u00b7dius", "sind", "jhm", "zu\u00b7glei\u00b7chen", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "VAFIN", "PPER", "VVFIN", "PTKNEG", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Der k\u00fcnstlich Amadis ist nie so hoch gegangen.", "tokens": ["Der", "k\u00fcnst\u00b7lich", "A\u00b7ma\u00b7dis", "ist", "nie", "so", "hoch", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gl\u00fcckseelig ist die Hand/ die diesen Brieff gemacht/", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "ist", "die", "Hand", "/", "die", "die\u00b7sen", "Brieff", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$(", "ART", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gl\u00fcckseelig ich die Dint vnd auch die Feder acht/", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "ich", "die", "Dint", "vnd", "auch", "die", "Fe\u00b7der", "acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "KON", "ADV", "ART", "NN", "CARD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd mehr gl\u00fcckseelig mich/ der ich jhn hab empfangen.", "tokens": ["Vnd", "mehr", "gl\u00fcck\u00b7see\u00b7lig", "mich", "/", "der", "ich", "jhn", "hab", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "$(", "PRELS", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}