{"dta.poem.9913": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was nur siehet/ fieht auch mich/ da ich doch bin ohne leibe/", "tokens": ["Was", "nur", "sie\u00b7het", "/", "fieht", "auch", "mich", "/", "da", "ich", "doch", "bin", "oh\u00b7ne", "lei\u00b7be", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$(", "VVFIN", "ADV", "PPER", "$(", "KOUS", "PPER", "ADV", "VAFIN", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Wie der sonnen es gef\u00e4llt/ weich ich/ wo ich bin und bleibe;", "tokens": ["Wie", "der", "son\u00b7nen", "es", "ge\u00b7f\u00e4llt", "/", "weich", "ich", "/", "wo", "ich", "bin", "und", "blei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "PPER", "VVPP", "$(", "ADJD", "PPER", "$(", "PWAV", "PPER", "VAFIN", "KON", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Sterbe/ wenn sie gehet auf/ lebe bey dem untergehen:", "tokens": ["Ster\u00b7be", "/", "wenn", "sie", "ge\u00b7het", "auf", "/", "le\u00b7be", "bey", "dem", "un\u00b7ter\u00b7ge\u00b7hen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "VVFIN", "PTKVZ", "$(", "VVFIN", "APPR", "ART", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Menschen folg ich immer nach/ wo sie gehen oder stehen.", "tokens": ["Men\u00b7schen", "folg", "ich", "im\u00b7mer", "nach", "/", "wo", "sie", "ge\u00b7hen", "o\u00b7der", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "$(", "PWAV", "PPER", "VVINF", "KON", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "(hin/", "tokens": ["(", "hin", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "PTKVZ", "$("], "meter": "+", "measure": "single.up"}}}}}