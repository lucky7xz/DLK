{"dta.poem.15971": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die K\u00f6rperwelt bedarf des Lichtes, um Gestalten", "tokens": ["Die", "K\u00f6r\u00b7per\u00b7welt", "be\u00b7darf", "des", "Lich\u00b7tes", ",", "um", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In immer reicherer Entwicklung zu entfalten.", "tokens": ["In", "im\u00b7mer", "rei\u00b7che\u00b7rer", "Ent\u00b7wick\u00b7lung", "zu", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Geister, die im Stoff gefangen, werden frei,", "tokens": ["Die", "Geis\u00b7ter", ",", "die", "im", "Stoff", "ge\u00b7fan\u00b7gen", ",", "wer\u00b7den", "frei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "PTKVZ", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur wo der freieste tr\u00e4gt zur Befreiung bei.", "tokens": ["Nur", "wo", "der", "frei\u00b7es\u00b7te", "tr\u00e4gt", "zur", "Be\u00b7frei\u00b7ung", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "ADJA", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Das Licht hinwieder auch bedarf der K\u00f6rperwelt,", "tokens": ["Das", "Licht", "hin\u00b7wie\u00b7der", "auch", "be\u00b7darf", "der", "K\u00f6r\u00b7per\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil Manigfaltigkeit es nur durch sie erh\u00e4lt.", "tokens": ["Weil", "Ma\u00b7nig\u00b7fal\u00b7tig\u00b7keit", "es", "nur", "durch", "sie", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denn es ist einfach eins und strebt zu scheinen vieles,", "tokens": ["Denn", "es", "ist", "ein\u00b7fach", "eins", "und", "strebt", "zu", "schei\u00b7nen", "vie\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "CARD", "KON", "VVFIN", "PTKZU", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist der Zweck des mit der Welt getriebnen Spieles.", "tokens": ["Das", "ist", "der", "Zweck", "des", "mit", "der", "Welt", "ge\u00b7trieb\u00b7nen", "Spie\u00b7les", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Zu sieben Farben wirds an jedem Wolkensaum,", "tokens": ["Zu", "sie\u00b7ben", "Far\u00b7ben", "wirds", "an", "je\u00b7dem", "Wol\u00b7ken\u00b7saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tausendfache Bl\u00fct' und Frucht an jedem Baum.", "tokens": ["Und", "tau\u00b7send\u00b7fa\u00b7che", "Bl\u00fct'", "und", "Frucht", "an", "je\u00b7dem", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es freut sich seines Spiels, und ihm zum Spiel zu dienen", "tokens": ["Es", "freut", "sich", "sei\u00b7nes", "Spiels", ",", "und", "ihm", "zum", "Spiel", "zu", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "KON", "PPER", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Freut sich die Welt, und wir freun billig uns mit ihnen.", "tokens": ["Freut", "sich", "die", "Welt", ",", "und", "wir", "freun", "bil\u00b7lig", "uns", "mit", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "KON", "PPER", "ADV", "ADJD", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}