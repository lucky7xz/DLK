{"textgrid.poem.47743": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[singe, singe, singe M\u00fcndlein]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singe, singe, singe M\u00fcndlein,", "tokens": ["Sin\u00b7ge", ",", "sin\u00b7ge", ",", "sin\u00b7ge", "M\u00fcnd\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zwitschere, mein Vogelz\u00fcnglein,", "tokens": ["Zwit\u00b7sche\u00b7re", ",", "mein", "Vo\u00b7gel\u00b7z\u00fcn\u00b7glein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lodere, mein Lebekerzlein,", "tokens": ["Lo\u00b7de\u00b7re", ",", "mein", "Le\u00b7be\u00b7kerz\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Jubele, mein Lebest\u00fcndlein,", "tokens": ["Ju\u00b7be\u00b7le", ",", "mein", "Le\u00b7be\u00b7st\u00fcnd\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Liebe, liebe, was da lieblich,", "tokens": ["Lie\u00b7be", ",", "lie\u00b7be", ",", "was", "da", "lieb\u00b7lich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Poche laut, mein liebes Herzlein!", "tokens": ["Po\u00b7che", "laut", ",", "mein", "lie\u00b7bes", "Herz\u00b7lein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Alles, Alles wirst du missen,", "tokens": ["Al\u00b7les", ",", "Al\u00b7les", "wirst", "du", "mis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alles, Alles wird entrissen;", "tokens": ["Al\u00b7les", ",", "Al\u00b7les", "wird", "ent\u00b7ris\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Bald genug in schwarzer Erde", "tokens": ["Bald", "ge\u00b7nug", "in", "schwar\u00b7zer", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Werden wir zu schweigen wissen,", "tokens": ["Wer\u00b7den", "wir", "zu", "schwei\u00b7gen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ach, nur allzu stille sein,", "tokens": ["Ach", ",", "nur", "all\u00b7zu", "stil\u00b7le", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "PTKA", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wenn die Tanne glatt gehobelt,", "tokens": ["Wenn", "die", "Tan\u00b7ne", "glatt", "ge\u00b7ho\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wenn wir unter Fr\u00fchlingswettern", "tokens": ["Wenn", "wir", "un\u00b7ter", "Fr\u00fch\u00b7lings\u00b7wet\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Weilen in den wei\u00dfen Brettern,", "tokens": ["Wei\u00b7len", "in", "den", "wei\u00b7\u00dfen", "Bret\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Hausen in dem engen Schrein.", "tokens": ["Hau\u00b7sen", "in", "dem", "en\u00b7gen", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Singe, singe, singe M\u00fcndlein,", "tokens": ["Sin\u00b7ge", ",", "sin\u00b7ge", ",", "sin\u00b7ge", "M\u00fcnd\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zwitschere, mein Vogelz\u00fcnglein,", "tokens": ["Zwit\u00b7sche\u00b7re", ",", "mein", "Vo\u00b7gel\u00b7z\u00fcn\u00b7glein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lodere, mein Lebekerzlein,", "tokens": ["Lo\u00b7de\u00b7re", ",", "mein", "Le\u00b7be\u00b7kerz\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Jubele, mein Lebest\u00fcndlein,", "tokens": ["Ju\u00b7be\u00b7le", ",", "mein", "Le\u00b7be\u00b7st\u00