{"textgrid.poem.64921": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was du an mir gethan, als schlecht mu\u00df ich's erkennen:", "tokens": ["Was", "du", "an", "mir", "ge\u00b7than", ",", "als", "schlecht", "mu\u00df", "ich's", "er\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "$,", "KOUS", "ADJD", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch bin ich weit entfernt, dich selber schlecht zu nennen.", "tokens": ["Doch", "bin", "ich", "weit", "ent\u00b7fernt", ",", "dich", "sel\u00b7ber", "schlecht", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVPP", "$,", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn jene That kam nicht aus deiner Wesenheit,", "tokens": ["Denn", "je\u00b7ne", "That", "kam", "nicht", "aus", "dei\u00b7ner", "We\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und du begingst sie erst nach langem, inn'rem Streit.", "tokens": ["Und", "du", "be\u00b7gingst", "sie", "erst", "nach", "lan\u00b7gem", ",", "inn'\u00b7rem", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und h\u00e4ttst du treu befolgt dein innerstes Verlangen,", "tokens": ["Und", "h\u00e4ttst", "du", "treu", "be\u00b7folgt", "dein", "in\u00b7ners\u00b7tes", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Herzens echten Trieb, sie w\u00e4re nicht begangen.", "tokens": ["Des", "Her\u00b7zens", "ech\u00b7ten", "Trieb", ",", "sie", "w\u00e4\u00b7re", "nicht", "be\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Thor, der den gift'gen Pilz mit M\u00fch zu heften sucht", "tokens": ["Thor", ",", "der", "den", "gift'\u00b7gen", "Pilz", "mit", "M\u00fch", "zu", "hef\u00b7ten", "sucht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "An seines Wesens Stamm, dem fremd so schlimme Frucht!", "tokens": ["An", "sei\u00b7nes", "We\u00b7sens", "Stamm", ",", "dem", "fremd", "so", "schlim\u00b7me", "Frucht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "PRELS", "ADJD", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was