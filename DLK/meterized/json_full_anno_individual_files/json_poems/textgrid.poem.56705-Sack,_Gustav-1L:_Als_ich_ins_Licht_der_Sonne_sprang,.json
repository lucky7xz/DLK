{"textgrid.poem.56705": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich ins Licht der Sonne sprang,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich ins Licht der Sonne sprang,", "tokens": ["Als", "ich", "ins", "Licht", "der", "Son\u00b7ne", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "stand meines Lebens Zickzackgang", "tokens": ["stand", "mei\u00b7nes", "Le\u00b7bens", "Zick\u00b7zack\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "schon unzerst\u00f6rbar vorgeschrieben;", "tokens": ["schon", "un\u00b7zer\u00b7st\u00f6r\u00b7bar", "vor\u00b7ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "nun springe ich die Wege lang", "tokens": ["nun", "sprin\u00b7ge", "ich", "die", "We\u00b7ge", "lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und fange diesen Zickzackgang", "tokens": ["und", "fan\u00b7ge", "die\u00b7sen", "Zick\u00b7zack\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wahrhaftig leise an zu lieben \u2013", "tokens": ["wahr\u00b7haf\u00b7tig", "lei\u00b7se", "an", "zu", "lie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "doch dieser letzte Zickzackgang", "tokens": ["doch", "die\u00b7ser", "letz\u00b7te", "Zick\u00b7zack\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und lendenlahme Liebessang", "tokens": ["und", "len\u00b7den\u00b7lah\u00b7me", "Lie\u00b7bes\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "ist eben auch schon vorgeschrieben.", "tokens": ["ist", "e\u00b7ben", "auch", "schon", "vor\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als ich ins Licht der Sonne sprang,", "tokens": ["Als", "ich", "ins", "Licht", "der", "Son\u00b7ne", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "stand meines Lebens Zickzackgang", "tokens": ["stand", "mei\u00b7nes", "Le\u00b7bens", "Zick\u00b7zack\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "schon unzerst\u00f6rbar vorgeschrieben;", "tokens": ["schon", "un\u00b7zer\u00b7st\u00f6r\u00b7bar", "vor\u00b7ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "nun springe ich die Wege lang", "tokens": ["nun", "sprin\u00b7ge", "ich", "die", "We\u00b7ge", "lang"], "token_inf