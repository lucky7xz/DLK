{"dta.poem.11296": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf herrn Christian Weisens  \n drey ertz-narren.  \n C. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der weise Weise hat es hoch genung gebracht,", "tokens": ["Der", "wei\u00b7se", "Wei\u00b7se", "hat", "es", "hoch", "ge\u00b7nung", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er die redner-kunst uns deutlicher gemacht;", "tokens": ["Da\u00df", "er", "die", "red\u00b7ner\u00b7kunst", "uns", "deut\u00b7li\u00b7cher", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch glaub ich, da\u00df bey ihm nichts artigers beklieben,", "tokens": ["Doch", "glaub", "ich", ",", "da\u00df", "bey", "ihm", "nichts", "ar\u00b7ti\u00b7gers", "be\u00b7klie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PPER", "PIS", "ADJA", "VVINF", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Als da die weise faust der narren thun beschrieben.", "tokens": ["Als", "da", "die", "wei\u00b7se", "faust", "der", "nar\u00b7ren", "thun", "be\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}