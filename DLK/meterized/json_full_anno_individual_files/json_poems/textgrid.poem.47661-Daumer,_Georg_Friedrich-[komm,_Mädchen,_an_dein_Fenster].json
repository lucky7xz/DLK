{"textgrid.poem.47661": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[komm, M\u00e4dchen, an dein Fenster]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, M\u00e4dchen, an dein Fenster,", "tokens": ["Komm", ",", "M\u00e4d\u00b7chen", ",", "an", "dein", "Fens\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Komm, wenn die Schatten d\u00fcstern,", "tokens": ["Komm", ",", "wenn", "die", "Schat\u00b7ten", "d\u00fcs\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm, wenn die Sterne funkeln,", "tokens": ["Komm", ",", "wenn", "die", "Ster\u00b7ne", "fun\u00b7keln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und halte deine Wacht!", "tokens": ["Und", "hal\u00b7te", "dei\u00b7ne", "Wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich werde dir im Dunkeln,", "tokens": ["Ich", "wer\u00b7de", "dir", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein W\u00f6rtchen, ein geheimes,", "tokens": ["Ein", "W\u00f6rt\u00b7chen", ",", "ein", "ge\u00b7hei\u00b7mes", ","], "token_info"