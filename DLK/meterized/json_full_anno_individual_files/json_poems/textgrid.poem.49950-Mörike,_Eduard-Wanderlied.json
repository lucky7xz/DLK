{"textgrid.poem.49950": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Wanderlied", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entflohn sind wir der Stadt Gedr\u00e4nge:", "tokens": ["Ent\u00b7flohn", "sind", "wir", "der", "Stadt", "Ge\u00b7dr\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie anders leuchtet hier der Tag!", "tokens": ["Wie", "an\u00b7ders", "leuch\u00b7tet", "hier", "der", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie klingt in unsre Lustges\u00e4nge", "tokens": ["Wie", "klingt", "in", "uns\u00b7re", "Lust\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lerchensang", "tokens": ["Ler\u00b7chen\u00b7sang"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "hier und Wachtelschlag!", "tokens": ["hier", "und", "Wach\u00b7tel\u00b7schlag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Nun wandern wir und lassen gerne", "tokens": ["Nun", "wan\u00b7dern", "wir", "und", "las\u00b7sen", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Herrn Griesgram zu Haus;", "toke