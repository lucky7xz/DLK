{"dta.poem.22380": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "183. In der mitten siht man alles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Setz dich in Mittelpunct/ so sihstu alls zugleich/", "tokens": ["Setz", "dich", "in", "Mit\u00b7tel\u00b7punct", "/", "so", "sih\u00b7stu", "alls", "zu\u00b7gleich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$(", "ADV", "ADJD", "KOKOM", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was jetz und dann geschicht/ hier und im Him\u0303elreich.", "tokens": ["Was", "jetz", "und", "dann", "ge\u00b7schicht", "/", "hier", "und", "im", "Him\u0303el\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KON", "ADV", "VVPP", "$(", "ADV", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}