{"dta.poem.7823": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Pegnesische  \n Fr\u00fclings-Freude.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00f6bus/ als er kam itzund von dem grossen", "tokens": ["F\u00f6\u00b7bus", "/", "als", "er", "kam", "it\u00b7zund", "von", "dem", "gros\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "KOUS", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA"], "meter": "+-----+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "von dem Python/ den sein Pfeil hatte todt", "tokens": ["von", "dem", "Py\u00b7thon", "/", "den", "sein", "Pfeil", "hat\u00b7te", "todt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "$(", "ART", "PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "fande dort den Bogen spannen ungef\u00e4hr/ des", "tokens": ["fan\u00b7de", "dort", "den", "Bo\u00b7gen", "span\u00b7nen", "un\u00b7ge\u00b7f\u00e4hr", "/", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVFIN", "ADJD", "$(", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "B\u00fcbchen! (sprach er) solche Waffen keine", "tokens": ["B\u00fcb\u00b7chen", "!", "(", "sprach", "er", ")", "sol\u00b7che", "Waf\u00b7fen", "kei\u00b7ne"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "$(", "VVFIN", "PPER", "$(", "PIAT", "NN", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mir geh\u00f6ret di\u00df Gescho\u00df/ dir geb\u00fchrt es", "tokens": ["Mir", "ge\u00b7h\u00f6\u00b7ret", "di\u00df", "Ge\u00b7scho\u00df", "/", "dir", "ge\u00b7b\u00fchrt", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "NN", "$(", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "ich kan pfeilen auf das Wild. ich kan Fein-", "tokens": ["ich", "kan", "pfei\u00b7len", "auf", "das", "Wild", ".", "ich", "kan", "Fein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVFIN", "APPR", "ART", "NN", "$.", "PPER", "VMFIN", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Schaue dort den Python ligen/ der vier Mor-", "tokens": ["Schau\u00b7e", "dort", "den", "Py\u00b7thon", "li\u00b7gen", "/", "der", "vier", "Mor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NE", "VVFIN", "$(", "ART", "CARD", "NN"], "meter": "+-+--+-++-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "denn hat itzt/ mit tausend Pfeilen/ meine", "tokens": ["denn", "hat", "itzt", "/", "mit", "tau\u00b7send", "Pfei\u00b7len", "/", "mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ADV", "$(", "APPR", "CARD", "NN", "$(", "PPOSAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Was thust du? dein Fackelbrand macht ich", "tokens": ["Was", "thust", "du", "?", "dein", "Fa\u00b7ckel\u00b7brand", "macht", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Streb nur nicht nach meinem Ruhm! wer", "tokens": ["Streb", "nur", "nicht", "nach", "mei\u00b7nem", "Ruhm", "!", "wer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$.", "PWS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "deine Torheit dich vergn\u00fcge: brenn/ Cupi-", "tokens": ["dei\u00b7ne", "Tor\u00b7heit", "dich", "ver\u00b7gn\u00fc\u00b7ge", ":", "brenn", "/", "Cu\u00b7pi"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$.", "VVFIN", "$(", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "R\u00f6te stieg/ ob solcher Rede/ diesem Kind ins", "tokens": ["R\u00f6\u00b7te", "stieg", "/", "ob", "sol\u00b7cher", "Re\u00b7de", "/", "die\u00b7sem", "Kind", "ins"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$(", "KOUS", "PIAT", "NN", "$(", "PDAT", "NN", "APPRART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Zornig sprach er/ und beschamt: du magst", "tokens": ["Zor\u00b7nig", "sprach", "er", "/", "und", "be\u00b7schamt", ":", "du", "magst"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$(", "KON", "VVPP", "$.", "PPER", "VMFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "F\u00f6bus! ich will treffen dich/ dir selbst will ich", "tokens": ["F\u00f6\u00b7bus", "!", "ich", "will", "tref\u00b7fen", "dich", "/", "dir", "selbst", "will", "ich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VMFIN", "VVINF", "PPER", "$(", "PPER", "ADV", "VMFIN", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Soviel gr\u00f6sser sind die G\u00f6tter/ als die Thiere/", "tokens": ["So\u00b7viel", "gr\u00f6s\u00b7ser", "sind", "die", "G\u00f6t\u00b7ter", "/", "als", "die", "Thie\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VAFIN", "ART", "NN", "$(", "KOUS", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "so viel h\u00f6her meine Ehre/ als die deine werden", "tokens": ["so", "viel", "h\u00f6\u00b7her", "mei\u00b7ne", "Eh\u00b7re", "/", "als", "die", "dei\u00b7ne", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "PPOSAT", "NN", "$(", "KOUS", "ART", "PPOSAT", "VAINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "Bald schwang er sich in die Luft/ die er schnit-", "tokens": ["Bald", "schwang", "er", "sich", "in", "die", "Luft", "/", "die", "er", "schnit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$(", "PRELS", "PPER", "TRUNC"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "bis da\u00df sie ihn hingebracht zu Cyth\u00e4rens", "tokens": ["bis", "da\u00df", "sie", "ihn", "hin\u00b7ge\u00b7bracht", "zu", "Cyt\u00b7h\u00e4\u00b7rens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPER", "VVFIN", "APPR", "NE"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.19": {"text": "Dorten sucht\u2019 er aus dem K\u00f6cher/ ein paar", "tokens": ["Dor\u00b7ten", "sucht'", "er", "aus", "dem", "K\u00f6\u00b7cher", "/", "ein", "paar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "ART", "PIAT"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "einer/ stumpf/ aus Bley gegossen/ kan ein", "tokens": ["ei\u00b7ner", "/", "stumpf", "/", "aus", "Bley", "ge\u00b7gos\u00b7sen", "/", "kan", "ein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "$(", "ADJD", "$(", "APPR", "NN", "VVPP", "$(", "VMFIN", "ART"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.21": {"text": "und der andre macht verliebt/ ist von Gold/", "tokens": ["und", "der", "and\u00b7re", "macht", "ver\u00b7liebt", "/", "ist", "von", "Gold", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "VVPP", "$(", "VAFIN", "APPR", "NN", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "Diese bringt er: F\u00f6bus Herz/ wird vom le-", "tokens": ["Die\u00b7se", "bringt", "er", ":", "F\u00f6\u00b7bus", "Herz", "/", "wird", "vom", "le"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$.", "NE", "NN", "$(", "VAFIN", "APPRART", "TRUNC"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "und der erste/ Ha\u00df und K\u00e4lte in die sch\u00f6ne", "tokens": ["und", "der", "ers\u00b7te", "/", "Ha\u00df", "und", "K\u00e4l\u00b7te", "in", "die", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "$(", "NN", "KON", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "Bald so liebet jener diese: diese jenes Fein-", "tokens": ["Bald", "so", "lie\u00b7bet", "je\u00b7ner", "die\u00b7se", ":", "die\u00b7se", "je\u00b7nes", "Fein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PDAT", "PDAT", "$.", "PDAT", "PDAT", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.25": {"text": "Sie/ die Nymfe/ hasst die Lieb; liebt/ was", "tokens": ["Sie", "/", "die", "Nym\u00b7fe", "/", "hasst", "die", "Lieb", ";", "liebt", "/", "was"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "$(", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$.", "VVFIN", "$(", "PWS"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "lauft und jagt dem Wilde nach/ irrend durch", "tokens": ["lauft", "und", "jagt", "dem", "Wil\u00b7de", "nach", "/", "ir\u00b7rend", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "APPR", "$(", "VVPP", "APPR"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "mit des F\u00f6bus seiner Schwester; l\u00e4sst/ das", "tokens": ["mit", "des", "F\u00f6\u00b7bus", "sei\u00b7ner", "Schwes\u00b7ter", ";", "l\u00e4sst", "/", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$.", "VVFIN", "$(", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "ihrer Haare/ ledig fliegen. Tausend S\u00f6hne", "tokens": ["ih\u00b7rer", "Haa\u00b7re", "/", "le\u00b7dig", "flie\u00b7gen", ".", "Tau\u00b7send", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "ADJD", "VVINF", "$.", "CARD", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.29": {"text": "und verlangen ihre Gunst: noch kan keiner", "tokens": ["und", "ver\u00b7lan\u00b7gen", "ih\u00b7re", "Gunst", ":", "noch", "kan", "kei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "VMFIN", "PIS"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "sie vergeht sich im Geb\u00fcsch: da\u00df ihr nicht", "tokens": ["sie", "ver\u00b7geht", "sich", "im", "Ge\u00b7b\u00fcsch", ":", "da\u00df", "ihr", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$.", "KOUS", "PPER", "PTKNEG"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.31": {"text": "Nichts fragt sie nach Hochzeitmachen/ ob der", "tokens": ["Nichts", "fragt", "sie", "nach", "Hoch\u00b7zeit\u00b7ma\u00b7chen", "/", "ob", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN", "$(", "KOUS", "ART"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Tochter! du bist einen Eydam/ Enkel bist du", "tokens": ["Toch\u00b7ter", "!", "du", "bist", "ei\u00b7nen", "Ey\u00b7dam", "/", "En\u00b7kel", "bist", "du"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VAFIN", "ART", "NN", "$(", "NN", "VAFIN", "PPER"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.33": {"text": "Sie err\u00f6tet ob dem Raht: Rosen bl\u00fchen auf", "tokens": ["Sie", "er\u00b7r\u00f6\u00b7tet", "ob", "dem", "Raht", ":", "Ro\u00b7sen", "bl\u00fc\u00b7hen", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "NN", "$.", "NN", "VVFIN", "APPR"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "ihrer Wangen voller Scham. Schand f\u00fcrcht", "tokens": ["ih\u00b7rer", "Wan\u00b7gen", "vol\u00b7ler", "Scham", ".", "Schand", "f\u00fcrcht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "f\u00e4llet \u00fcm den Hals dem Vatter: Vatter", "tokens": ["f\u00e4l\u00b7let", "\u00fcm", "den", "Hals", "dem", "Vat\u00b7ter", ":", "Vat\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "la\u00df mich eine Jungfrau bleiben. Auch Dia-", "tokens": ["la\u00df", "mich", "ei\u00b7ne", "Jung\u00b7frau", "blei\u00b7ben", ".", "Auch", "Dia"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$.", "ADV", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "hat ihr Vatter nicht versagt. Er zwar/ l\u00e4sst", "tokens": ["hat", "ihr", "Vat\u00b7ter", "nicht", "ver\u00b7sagt", ".", "Er", "zwar", "/", "l\u00e4sst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$.", "PPER", "ADV", "$(", "VVFIN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.38": {"text": "aber (sagt er) wird man auch dich so sch\u00f6ne", "tokens": ["a\u00b7ber", "(", "sagt", "er", ")", "wird", "man", "auch", "dich", "so", "sch\u00f6\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "VVFIN", "PPER", "$(", "VAFIN", "PIS", "ADV", "PPER", "ADV", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.39": {"text": "ungeliebet/ ungenossen? die Gestalt es leidet", "tokens": ["un\u00b7ge\u00b7lie\u00b7bet", "/", "un\u00b7ge\u00b7nos\u00b7sen", "?", "die", "Ge\u00b7stalt", "es", "lei\u00b7det"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$(", "ADJD", "$.", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.40": {"text": "dich zu meide\u0304: deinem Wunsche deine Sch\u00f6n-", "tokens": ["dich", "zu", "mei\u00b7d\u0113", ":", "dei\u00b7nem", "Wun\u00b7sche", "dei\u00b7ne", "Sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "$.", "PPOSAT", "NN", "PPOSAT", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.41": {"text": "F\u00f6bus unterdessen brennt/ (Dafne macht", "tokens": ["F\u00f6\u00b7bus", "un\u00b7ter\u00b7des\u00b7sen", "brennt", "/", "(", "Daf\u00b7ne", "macht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "VVPP", "VVFIN", "$(", "$(", "NE", "VVFIN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.42": {"text": "ist selbst sein Orakel/ hofft/ was er w\u00fcnschet/", "tokens": ["ist", "selbst", "sein", "O\u00b7ra\u00b7kel", "/", "hofft", "/", "was", "er", "w\u00fcn\u00b7schet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$(", "VVFIN", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.43": {"text": "doch vergebens; nehrt/ ohn Hoffnung/ mit", "tokens": ["doch", "ver\u00b7ge\u00b7bens", ";", "nehrt", "/", "ohn", "Hoff\u00b7nung", "/", "mit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "$.", "VVFIN", "$(", "APPR", "NN", "$(", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.44": {"text": "sihet/ liebet/ brennet/ w\u00fcnschet/ hoffet/ sucht/", "tokens": ["si\u00b7het", "/", "lie\u00b7bet", "/", "bren\u00b7net", "/", "w\u00fcn\u00b7schet", "/", "hof\u00b7fet", "/", "sucht", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN",