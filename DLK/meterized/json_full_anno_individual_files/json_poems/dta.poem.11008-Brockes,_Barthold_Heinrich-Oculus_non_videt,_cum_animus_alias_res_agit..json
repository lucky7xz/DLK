{"dta.poem.11008": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Oculus non videt,  \n cum animus alias res agit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es siehet unser Auge nicht, wenn unser Geist was anders\ndenket.", "tokens": ["Es", "sie\u00b7het", "un\u00b7ser", "Au\u00b7ge", "nicht", ",", "wenn", "un\u00b7ser", "Geist", "was", "an\u00b7ders", "den\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "PWS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "So ist es ja nicht zu bewundern, da\u00df man des Sch\u00f6pfers", "tokens": ["So", "ist", "es", "ja", "nicht", "zu", "be\u00b7wun\u00b7dern", ",", "da\u00df", "man", "des", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,", "KOUS", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ob sie uns gleich f\u00fcr Augen liegen, da das nicht ruhige Ge-", "tokens": ["Ob", "sie", "uns", "gleich", "f\u00fcr", "Au\u00b7gen", "lie\u00b7gen", ",", "da", "das", "nicht", "ru\u00b7hi\u00b7ge", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,", "KOUS", "PDS", "PTKNEG", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.4": {"text": "Mit Ehr und Lust und Geld besch\u00e4fftigt, sich stets von ihnen", "tokens": ["Mit", "Ehr", "und", "Lust", "und", "Geld", "be\u00b7sch\u00e4ff\u00b7tigt", ",", "sich", "stets", "von", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVPP", "$,", "PRF", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Nun wissen wir den Grund, woher wir hier mit sehnden Au-", "tokens": ["Nun", "wis\u00b7sen", "wir", "den", "Grund", ",", "wo\u00b7her", "wir", "hier", "mit", "sehn\u00b7den", "Au"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Und auch mit offnen Ohren taub, f\u00fcr Gott und seine Wer-", "tokens": ["Und", "auch", "mit", "off\u00b7nen", "Oh\u00b7ren", "taub", ",", "f\u00fcr", "Gott", "und", "sei\u00b7ne", "Wer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ADJD", "$,", "APPR", "NN", "KON", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}