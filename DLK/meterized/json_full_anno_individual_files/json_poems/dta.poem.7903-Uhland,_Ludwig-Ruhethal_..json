{"dta.poem.7903": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ruhethal .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wann im letzten Abendstral", "tokens": ["Wann", "im", "letz\u00b7ten", "A\u00b7bend\u00b7stral"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldne Wolkenberge steigen", "tokens": ["Gold\u00b7ne", "Wol\u00b7ken\u00b7ber\u00b7ge", "stei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie Alpen sich erzeigen,", "tokens": ["Und", "wie", "Al\u00b7pen", "sich", "er\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "PRF", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Frag\u2019 ich oft mit Thr\u00e4nen:", "tokens": ["Frag'", "ich", "oft", "mit", "Thr\u00e4\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Liegt wohl zwischen jenen", "tokens": ["Liegt", "wohl", "zwi\u00b7schen", "je\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PDAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mein ersehntes Ruhethal?", "tokens": ["Mein", "er\u00b7sehn\u00b7tes", "Ru\u00b7het\u00b7hal", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}