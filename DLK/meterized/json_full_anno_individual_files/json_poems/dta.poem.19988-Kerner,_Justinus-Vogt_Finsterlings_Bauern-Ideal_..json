{"dta.poem.19988": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Vogt Finsterlings Bauern-Ideal .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O m\u00f6chte mir ein treu Gem\u00e4hlde gl\u00fccken", "tokens": ["O", "m\u00f6ch\u00b7te", "mir", "ein", "treu", "Ge\u00b7m\u00e4hl\u00b7de", "gl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ART", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Bau'r, wie sich derselbe mu\u00df gestalten,", "tokens": ["Vom", "Bau'r", ",", "wie", "sich", "der\u00b7sel\u00b7be", "mu\u00df", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PRF", "PDAT", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um uns, die wir das Richteramt verwalten,", "tokens": ["Um", "uns", ",", "die", "wir", "das", "Rich\u00b7ter\u00b7amt", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die heil'ge Amtsehr' niemals zu verr\u00fccken!", "tokens": ["Die", "heil'\u00b7ge", "Amts\u00b7ehr'", "nie\u00b7mals", "zu", "ver\u00b7r\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dies Ideal steht lang mit krummem R\u00fccken", "tokens": ["Dies", "I\u00b7deal", "steht", "lang", "mit", "krum\u00b7mem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Vor uns den urtheilsprechenden Gewalten;", "tokens": ["Vor", "uns", "den", "ur\u00b7theil\u00b7spre\u00b7chen\u00b7den", "Ge\u00b7wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.7": {"text": "Wir schreiben, sandeln, zieh'n die Stirn in Falten,", "tokens": ["Wir", "schrei\u00b7ben", ",", "san\u00b7deln", ",", "zieh'n", "die", "Stirn", "in", "Fal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Donnern: was giebt's?! Und es wagt auf zu blicken,", "tokens": ["Don\u00b7nern", ":", "was", "giebt's", "?!", "Und", "es", "wagt", "auf", "zu", "bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VVFIN", "$.", "KON", "PPER", "VVFIN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Fragt weder was noch wie, was wir auch sagen.", "tokens": ["Fragt", "we\u00b7der", "was", "noch", "wie", ",", "was", "wir", "auch", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PWS", "ADV", "KOKOM", "$,", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wir sagen: \u201epackt euch! theu'r sind unsre Stunden!!\u201c", "tokens": ["Wir", "sa\u00b7gen", ":", "\u201e", "packt", "euch", "!", "theu'r", "sind", "uns\u00b7re", "Stun\u00b7den", "!!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "PPER", "$.", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dann beugt sich's, geht und stirbt mit dem Gedanken:", "tokens": ["Dann", "beugt", "sich's", ",", "geht", "und", "stirbt", "mit", "dem", "Ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Es komme bald Bescheid auf seine Klagen.", "tokens": ["Es", "kom\u00b7me", "bald", "Be\u00b7scheid", "auf", "sei\u00b7ne", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}