{"textgrid.poem.58718": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist denn mein Leben nicht kurz? Hiob 10, 20.", "tokens": ["Ist", "denn", "mein", "Le\u00b7ben", "nicht", "kurz", "?", "Hiob", "10", ",", "20."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "number", "punct", "ordinal"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "ADJD", "$.", "NE", "CARD", "$,", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du hast ein Ziel gesetzt, das wird er nicht \u00fcberschreiten. Hiob 14, 5", "tokens": ["Du", "hast", "ein", "Ziel", "ge\u00b7setzt", ",", "das", "wird", "er", "nicht", "\u00fc\u00b7bersc\u00b7hrei\u00b7ten", ".", "Hiob", "14", ",", "5"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "PDS", "VAFIN", "PPER", "PTKNEG", "VVINF", "$.", "NE", "CARD", "$,", "CARD"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein Gott, mein Liebster, denk, da\u00df mir", "tokens": ["Mein", "Gott", ",", "mein", "Liebs\u00b7ter", ",", "denk", ",", "da\u00df", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die kurzen Tag' dem Schatten gleich vergehen;", "tokens": ["Die", "kur\u00b7zen", "Tag'", "dem", "Schat\u00b7ten", "gleich", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In diesem dunkeln Hause hier", "tokens": ["In", "die\u00b7sem", "dun\u00b7keln", "Hau\u00b7se", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann mir, wenn dir's beliebt, ein sch\u00f6ner Tag aufgehen!", "tokens": ["Kann", "mir", ",", "wenn", "dir's", "be\u00b7liebt", ",", "ein", "sch\u00f6\u00b7ner", "Tag", "auf\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PIS", "ADJD", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dein Nahsein ist des Lichtes Bronn,", "tokens": ["Dein", "Na\u00b7hsein", "ist", "des", "Lich\u00b7tes", "Bronn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Dein Fernsein macht, da\u00df ich die Nacht nur finde;", "tokens": ["Dein", "Fern\u00b7sein", "macht", ",", "da\u00df", "ich", "die", "Nacht", "nur", "fin\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gib vollen Glanz, du Gnadensonn,", "tokens": ["Gib", "vol\u00b7len", "Glanz", ",", "du", "Gna\u00b7den\u00b7sonn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erle