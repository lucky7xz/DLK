{"dta.poem.12051": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Blick' auf und sage dir: wo ist der Regenbogen?", "tokens": ["Blick'", "auf", "und", "sa\u00b7ge", "dir", ":", "wo", "ist", "der", "Re\u00b7gen\u00b7bo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "PPER", "$.", "PWAV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er scheinet dort dem Saum der Wolken angeflogen.", "tokens": ["Er", "schei\u00b7net", "dort", "dem", "Saum", "der", "Wol\u00b7ken", "an\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch in der Wolke, w\u00e4r' er dort wol ohn' ein Auge,", "tokens": ["Doch", "in", "der", "Wol\u00b7ke", ",", "w\u00e4r'", "er", "dort", "wol", "ohn'", "ein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das deinige, das ihn dir in die Seele sauge?", "tokens": ["Das", "dei\u00b7ni\u00b7ge", ",", "das", "ihn", "dir", "in", "die", "See\u00b7le", "sau\u00b7ge", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du wirst es dir bewust: es sind der Sonne Stralen,", "tokens": ["Du", "wirst", "es", "dir", "be\u00b7wust", ":", "es", "sind", "der", "Son\u00b7ne", "Stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die du getrunken hast aus Regenbogenschalen.", "tokens": ["Die", "du", "ge\u00b7trun\u00b7ken", "hast", "aus", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7scha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nichts ist das Farbenspiel, nur wirklich ist die Sonne;", "tokens": ["Nichts", "ist", "das", "Far\u00b7ben\u00b7spiel", ",", "nur", "wirk\u00b7lich", "ist", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die lichte T\u00e4uschung doch ist deiner Augen Wonne.", "tokens": ["Die", "lich\u00b7te", "T\u00e4u\u00b7schung", "doch", "ist", "dei\u00b7ner", "Au\u00b7gen", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was unterm Himmel gl\u00e4nzt, ist nur der Sonne Licht,", "tokens": ["Was", "un\u00b7term", "Him\u00b7mel", "gl\u00e4nzt", ",", "ist", "nur", "der", "Son\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN", "$,", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mannichfaltig sich in tr\u00fcben Stoffen bricht.", "tokens": ["Das", "man\u00b7nich\u00b7fal\u00b7tig", "sich", "in", "tr\u00fc\u00b7ben", "Stof\u00b7fen", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Was unterm Himmel gl\u00e4nzt, ist nur ein Widerschein,", "tokens": ["Was", "un\u00b7term", "Him\u00b7mel", "gl\u00e4nzt", ",", "ist", "nur", "ein", "Wi\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN", "$,", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein bunter Schattenwurf der Himmelssonn' allein.", "tokens": ["Ein", "bun\u00b7ter", "Schat\u00b7ten\u00b7wurf", "der", "Him\u00b7mels\u00b7sonn'", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ein solcher Widerschein ist selbst die Sonne nur", "tokens": ["Ein", "sol\u00b7cher", "Wi\u00b7der\u00b7schein", "ist", "selbst", "die", "Son\u00b7ne", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der h\u00f6chsten Geistersonn' im Spiegel der Natur.", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Geis\u00b7ter\u00b7sonn'", "im", "Spie\u00b7gel", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}