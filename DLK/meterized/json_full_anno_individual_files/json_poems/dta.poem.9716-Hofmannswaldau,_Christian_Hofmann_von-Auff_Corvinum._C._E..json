{"dta.poem.9716": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff Corvinum.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der scheu\u00dfliche Corvin betheurte neulich hoch/", "tokens": ["Der", "scheu\u00df\u00b7li\u00b7che", "Cor\u00b7vin", "be\u00b7theur\u00b7te", "neu\u00b7lich", "hoch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lupine sey der geist zu seines c\u00f6rpers h\u00f6le.", "tokens": ["Lu\u00b7pi\u00b7ne", "sey", "der", "geist", "zu", "sei\u00b7nes", "c\u00f6r\u00b7pers", "h\u00f6\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O allzu freches wort! rieff ich; wie schickt sich doch", "tokens": ["O", "all\u00b7zu", "fre\u00b7ches", "wort", "!", "rieff", "ich", ";", "wie", "schickt", "sich", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKA", "ADJA", "NN", "$.", "VVFIN", "PPER", "$.", "PWAV", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu deinem raben-leib solch eine sch\u00f6ne seele?", "tokens": ["Zu", "dei\u00b7nem", "ra\u00b7ben\u00b7leib", "solch", "ei\u00b7ne", "sch\u00f6\u00b7ne", "see\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "mea