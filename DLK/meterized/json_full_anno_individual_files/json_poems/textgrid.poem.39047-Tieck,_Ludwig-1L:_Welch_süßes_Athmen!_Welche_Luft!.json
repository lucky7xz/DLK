{"textgrid.poem.39047": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welch s\u00fc\u00dfes Athmen! Welche Luft!", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch s\u00fc\u00dfes Athmen! Welche Luft!", "tokens": ["Welch", "s\u00fc\u00b7\u00dfes", "Ath\u00b7men", "!", "Wel\u00b7che", "Luft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie hebt sich die Brust, nur mehr und immer mehr", "tokens": ["Wie", "hebt", "sich", "die", "Brust", ",", "nur", "mehr", "und", "im\u00b7mer", "mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "$,", "ADV", "ADV", "KON", "ADV", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der zarten Wellen zu trinken,", "tokens": ["Der", "zar\u00b7ten", "Wel\u00b7len", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die mit Ged\u00fcft des Oelbaums getr\u00e4nkt,", "tokens": ["Die", "mit", "Ge\u00b7d\u00fcft", "des", "O\u00b7el\u00b7baums", "ge\u00b7tr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit der bl\u00fchenden Myrthe Wohlgeruch,", "tokens": ["Mit", "der", "bl\u00fc\u00b7hen\u00b7den", "Myr\u00b7the", "Wohl\u00b7ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART"