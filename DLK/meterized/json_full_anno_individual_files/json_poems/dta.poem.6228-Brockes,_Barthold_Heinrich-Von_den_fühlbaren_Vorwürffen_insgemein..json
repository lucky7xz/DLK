{"dta.poem.6228": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den f\u00fchlbaren Vorw\u00fcrffen  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein fast unendliches Gemisch ist hier auf Erden,", "tokens": ["Ein", "fast", "un\u00b7end\u00b7li\u00b7ches", "Ge\u00b7misch", "ist", "hier", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel tausend tausend Ding\u2019empfindet unser Sinn.", "tokens": ["Viel", "tau\u00b7send", "tau\u00b7send", "Ding'\u00b7emp\u00b7fin\u00b7det", "un\u00b7ser", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "CARD", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn dieses uns erschreckt; zieht das uns nach sich hin,", "tokens": ["Wenn", "die\u00b7ses", "uns", "er\u00b7schreckt", ";", "zieht", "das", "uns", "nach", "sich", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "PPER", "VVPP", "$.", "VVFIN", "ART", "PPER", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da wir von allen bald vergn\u00fcget,", "tokens": ["Da", "wir", "von", "al\u00b7len", "bald", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bald aber auch bekrieget,", "tokens": ["Bald", "a\u00b7ber", "auch", "be\u00b7krie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ergetzet bald, und bald verletzet werden:", "tokens": ["Er\u00b7get\u00b7zet", "bald", ",", "und", "bald", "ver\u00b7let\u00b7zet", "wer\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KON", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bald st\u00fcrmet gegen uns der Elementen Wuth,", "tokens": ["Bald", "st\u00fcr\u00b7met", "ge\u00b7gen", "uns", "der", "E\u00b7le\u00b7men\u00b7ten", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Bald sch\u00fctten sie auf uns hingegen all ihr Gut.", "tokens": ["Bald", "sch\u00fct\u00b7ten", "sie", "auf", "uns", "hin\u00b7ge\u00b7gen", "all", "ihr", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bald siehet man den lauen Lentzen", "tokens": ["Bald", "sie\u00b7het", "man", "den", "lau\u00b7en", "Lent\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Sich mit den holden Bluhmen kr\u00e4ntzen.", "tokens": ["Sich", "mit", "den", "hol\u00b7den", "Bluh\u00b7men", "kr\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Bald suchen uns mit Ceres blonden Sch\u00e4tzen", "tokens": ["Bald", "su\u00b7chen", "uns", "mit", "Ce\u00b7res", "blon\u00b7den", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die fetten Furchen zu ergetzen.", "tokens": ["Die", "fet\u00b7ten", "Fur\u00b7chen", "zu", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die G\u00e4rten sind mit Fr\u00fcchten angef\u00fcllt.", "tokens": ["Die", "G\u00e4r\u00b7ten", "sind", "mit", "Fr\u00fcch\u00b7ten", "an\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Den Winter bringen drauf die strengen Norden-Winde,", "tokens": ["Den", "Win\u00b7ter", "brin\u00b7gen", "drauf", "die", "stren\u00b7gen", "Nor\u00b7den\u00b7Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Felder sind in Schnee und Eys geh\u00fcllt.", "tokens": ["Die", "Fel\u00b7der", "sind", "in", "Schnee", "und", "Eys", "ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der Fl\u00fcsse klare Fluth fliesst itzo, str\u00f6mt gelinde", "tokens": ["Der", "Fl\u00fcs\u00b7se", "kla\u00b7re", "Fluth", "fliesst", "it\u00b7zo", ",", "str\u00f6mt", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ADV", "$,", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und schw\u00e4ngert Feld und Land;", "tokens": ["Und", "schw\u00e4n\u00b7gert", "Feld", "und", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Bald aber, wei\u00df vom Schaum, verschlinget sie den Strand,", "tokens": ["Bald", "a\u00b7ber", ",", "wei\u00df", "vom", "Schaum", ",", "ver\u00b7schlin\u00b7get", "sie", "den", "Strand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und \u00fcberstr\u00f6mt das Feld. Jtzt \u00fcberg\u00fcldet, mahlet,", "tokens": ["Und", "\u00fc\u00b7ber\u00b7str\u00f6mt", "das", "Feld", ".", "Jtzt", "\u00fc\u00b7berg\u00b7\u00fcl\u00b7det", ",", "mah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "ADV", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das heitre Sonnen-Licht die Wolcken: aber bald", "tokens": ["Das", "heit\u00b7re", "Son\u00b7nen\u00b7Licht", "die", "Wol\u00b7cken", ":", "a\u00b7ber", "bald"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$.", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Scheint durch der St\u00fcrme Grimm und w\u00fctende Gewalt", "tokens": ["Scheint", "durch", "der", "St\u00fcr\u00b7me", "Grimm", "und", "w\u00fc\u00b7ten\u00b7de", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NE", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}