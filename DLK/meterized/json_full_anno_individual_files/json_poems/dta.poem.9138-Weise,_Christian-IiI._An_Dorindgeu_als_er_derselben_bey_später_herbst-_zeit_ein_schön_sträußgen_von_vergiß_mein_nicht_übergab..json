{"dta.poem.9138": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n An Dorindgeu/ als er derselben bey sp\u00e4ter herbst-  \n zeit ein sch\u00f6n str\u00e4u\u00dfgen von vergi\u00df mein  \n nicht/ \u00fcbergab.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.71", "af:0.28"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Dorindgen nimm die blume", "tokens": ["Mein", "Do\u00b7rind\u00b7gen", "nimm", "die", "blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVIMP", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit beliebten h\u00e4nden an/", "tokens": ["Mit", "be\u00b7lieb\u00b7ten", "h\u00e4n\u00b7den", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche dir zu sch\u00f6nen ruhme", "tokens": ["Wel\u00b7che", "dir", "zu", "sch\u00f6\u00b7nen", "ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst den fr\u00fchling trotzen kan/", "tokens": ["Selbst", "den", "fr\u00fch\u00b7ling", "trot\u00b7zen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "G\u00f6nn ihr doch dein angesicht/", "tokens": ["G\u00f6nn", "ihr", "doch", "dein", "an\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil sie so beweglich spricht/", "tokens": ["Weil", "sie", "so", "be\u00b7weg\u00b7lich", "spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vergi\u00df mein nicht.", "tokens": ["Ver\u00b7gi\u00df", "mein", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "2. H\u00f6re/ mein Dorindgen h\u00f6re/", "tokens": ["H\u00f6\u00b7re", "/", "mein", "Do\u00b7rind\u00b7gen", "h\u00f6\u00b7re", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der stumme redner schreyt/", "tokens": ["Wie", "der", "stum\u00b7me", "red\u00b7ner", "schreyt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ertheile mir die ehre", "tokens": ["Und", "er\u00b7thei\u00b7le", "mir", "die", "eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner unvergessenheit/", "tokens": ["Dei\u00b7ner", "un\u00b7ver\u00b7ges\u00b7sen\u00b7heit", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du verstehst es wol/ mein liecht/", "tokens": ["Du", "ver\u00b7stehst", "es", "wol", "/", "mein", "liecht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann ein jedes bl\u00e4tgen spricht:", "tokens": ["Dann", "ein", "je\u00b7des", "bl\u00e4t\u00b7gen", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vergi\u00df mein nicht.", "tokens": ["Ver\u00b7gi\u00df", "mein", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}