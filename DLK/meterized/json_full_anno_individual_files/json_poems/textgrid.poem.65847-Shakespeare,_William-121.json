{"textgrid.poem.65847": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "121", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Besser schlecht sein als scheinen wo, wie sein,", "tokens": ["Bes\u00b7ser", "schlecht", "sein", "als", "schei\u00b7nen", "wo", ",", "wie", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAINF", "KOKOM", "VVFIN", "PWAV", "$,", "PWAV", "VAINF", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Das nicht sein Schmach gebiert, und zu entbehren", "tokens": ["Das", "nicht", "sein", "Schmach", "ge\u00b7biert", ",", "und", "zu", "ent\u00b7beh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gerechter Freude dran, wenn sie der Schein,", "tokens": ["Ge\u00b7rech\u00b7ter", "Freu\u00b7de", "dran", ",", "wenn", "sie", "der", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht unser Selbstbewu\u00dftsein soll bew\u00e4hren.", "tokens": ["Nicht", "un\u00b7ser", "Selbst\u00b7be\u00b7wu\u00df\u00b7tsein", "soll", "be\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn warum soll verschrobner Augen T\u00fccke", "tokens": ["Denn", "wa\u00b7rum", "soll", "ver\u00b7schrob\u00b7ner", "Au\u00b7gen", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VMFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein wildes Blut bel\u00e4cheln? Was vereint", "tokens": ["Mein", "wil\u00b7des", "Blut", "be\u00b7l\u00e4\u00b7cheln", "?", "Was", "ver\u00b7eint"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auf meine Schw\u00e4chen schw\u00e4ch'rer Sp\u00e4her Blicke,", "tokens": ["Auf", "mei\u00b7ne", "Schw\u00e4\u00b7chen", "schw\u00e4ch'\u00b7rer", "Sp\u00e4\u00b7her", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die \u00fcbel deuten, was ich gut gemeint?", "tokens": ["Die", "\u00fc\u00b7bel", "deu\u00b7ten", ",", "was", "ich", "gut", "ge\u00b7meint", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,", "PWS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nein! Ich bin, der ich bin, und was sie summen", "tokens": ["Nein", "!", "Ich", "bin", ",", "der", "ich", "bin", ",", "und", "was", "sie", "sum\u00b7men"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$,", "KON", "PWS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Von meiner Schuld, ist ", "tokens": ["Von", "mei\u00b7ner", "Schuld", ",", "ist"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,",