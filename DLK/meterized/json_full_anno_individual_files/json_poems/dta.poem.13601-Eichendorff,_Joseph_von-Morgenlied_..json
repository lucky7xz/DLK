{"dta.poem.13601": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Stern still nach dem andern f\u00e4llt", "tokens": ["Ein", "Stern", "still", "nach", "dem", "an\u00b7dern", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tief in des Himmels Kluft,", "tokens": ["Tief", "in", "des", "Him\u00b7mels", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon zucken Strahlen durch die Welt,", "tokens": ["Schon", "zu\u00b7cken", "Strah\u00b7len", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wittre Morgenluft.", "tokens": ["Ich", "witt\u00b7re", "Mor\u00b7gen\u00b7luft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In Qualmen steigt und sinkt das Thal;", "tokens": ["In", "Qual\u00b7men", "steigt", "und", "sinkt", "das", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ver\u00f6det noch vom Fest", "tokens": ["Ver\u00b7\u00f6\u00b7det", "noch", "vom", "Fest"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Liegt still der weite Freudensaal,", "tokens": ["Liegt", "still", "der", "wei\u00b7te", "Freu\u00b7den\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und todt noch alle G\u00e4st'.", "tokens": ["Und", "todt", "noch", "al\u00b7le", "G\u00e4st'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da hebt die Sonne aus dem Meer", "tokens": ["Da", "hebt", "die", "Son\u00b7ne", "aus", "dem", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erathmend ihren Lauf:", "tokens": ["E\u00b7rath\u00b7mend", "ih\u00b7ren", "Lauf", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur Erde geht, was feucht und schwer,", "tokens": ["Zur", "Er\u00b7de", "geht", ",", "was", "feucht", "und", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was klar, zu ihr hinauf.", "tokens": ["Was", "klar", ",", "zu", "ihr", "hin\u00b7auf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hebt gr\u00fcner W\u00e4lder Trieb und Macht", "tokens": ["Hebt", "gr\u00fc\u00b7ner", "W\u00e4l\u00b7der", "Trieb", "und", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Neurauschend in die Luft,", "tokens": ["Neu\u00b7rau\u00b7schend", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zieht hinten St\u00e4dte, eitel Pracht,", "tokens": ["Zieht", "hin\u00b7ten", "St\u00e4d\u00b7te", ",", "ei\u00b7tel", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blau' Berge durch den Duft.", "tokens": ["Blau'", "Ber\u00b7ge", "durch", "den", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Spannt aus die gr\u00fcnen Tepp'che weich,", "tokens": ["Spannt", "aus", "die", "gr\u00fc\u00b7nen", "Tepp'\u00b7che", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Str\u00f6men hell durchrankt,", "tokens": ["Von", "Str\u00f6\u00b7men", "hell", "durc\u00b7hrankt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schallend gl\u00e4nzt das frische Reich,", "tokens": ["Und", "schal\u00b7lend", "gl\u00e4nzt", "das", "fri\u00b7sche", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So weit das Auge langt.", "tokens": ["So", "weit", "das", "Au\u00b7ge", "langt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Mensch nun aus der tiefen Welt", "tokens": ["Der", "Mensch", "nun", "aus", "der", "tie\u00b7fen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tr\u00e4ume tritt heraus,", "tokens": ["Der", "Tr\u00e4u\u00b7me", "tritt", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Freut sich, da\u00df alles noch so h\u00e4lt,", "tokens": ["Freut", "sich", ",", "da\u00df", "al\u00b7les", "noch", "so", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "PIS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df noch das Spiel nicht aus.", "tokens": ["Da\u00df", "noch", "das", "Spiel", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und nun geht's an ein Flei\u00dfigsein!", "tokens": ["Und", "nun", "geht's", "an", "ein", "Flei\u00b7\u00dfig\u00b7sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umsumsend Berg und Thal,", "tokens": ["Um\u00b7sum\u00b7send", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Agiret lustig Gro\u00df und Klein", "tokens": ["A\u00b7gi\u00b7ret", "lus\u00b7tig", "Gro\u00df", "und", "Klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "ADJD", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Den Plunder allzumal.", "tokens": ["Den", "Plun\u00b7der", "all\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Sonne steiget einsam auf,", "tokens": ["Die", "Son\u00b7ne", "stei\u00b7get", "ein\u00b7sam", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ernst \u00fcber Lust und Weh", "tokens": ["Ernst", "\u00fc\u00b7ber", "Lust", "und", "Weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lenkt sie den ungest\u00f6rten Lauf,", "tokens": ["Lenkt", "sie", "den", "un\u00b7ge\u00b7st\u00f6r\u00b7ten", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu stiller Glorie. \u2014", "tokens": ["Zu", "stil\u00b7ler", "Glo\u00b7rie", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der Mensch kann nimmermehr hinaus", "tokens": ["Der", "Mensch", "kann", "nim\u00b7mer\u00b7mehr", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus dieser Narrenwelt.", "tokens": ["Aus", "die\u00b7ser", "Nar\u00b7ren\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}