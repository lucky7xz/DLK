{"dta.poem.1606": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ehren-Ged\u00e4chtni\u00df Herrn Christoph  \n Fibings.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Hier ruhst du graues Haubt und schl\u00e4ffst nach langem Wa-", "tokens": ["Hier", "ruhst", "du", "grau\u00b7es", "Haubt", "und", "schl\u00e4ffst", "nach", "lan\u00b7gem", "Wa"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "KON", "VVFIN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "chen/", "tokens": ["chen", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Die Jugend f\u00fcrcht\u2019te GOtt/ dient\u2019 ihrem Herren treu/", "tokens": ["Die", "Ju\u00b7gend", "f\u00fcrcht'\u00b7te", "Gott", "/", "dient'", "ih\u00b7rem", "Her\u00b7ren", "treu", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$(", "VVFIN", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wagte Leib und Blutt bey ihnen ohne Scheu/", "tokens": ["Und", "wag\u00b7te", "Leib", "und", "Blutt", "bey", "ih\u00b7nen", "oh\u00b7ne", "Scheu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "ADJD", "APPR", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ergab sich nicht der Welt und Wollust eitlen Sachen/", "tokens": ["Er\u00b7gab", "sich", "nicht", "der", "Welt", "und", "Wol\u00b7lust", "eit\u00b7len", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "ART", "NN", "KON", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mied falschen Eigennutz/ die Pest der besten Leute/", "tokens": ["Mied", "fal\u00b7schen", "Ei\u00b7gen\u00b7nutz", "/", "die", "Pest", "der", "bes\u00b7ten", "Leu\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum r\u00fchmt und klaget dich ein edles Hau\u00df noch heute.", "tokens": ["Drum", "r\u00fchmt", "und", "kla\u00b7get", "dich", "ein", "ed\u00b7les", "Hau\u00df", "noch", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als dich nun wolten Sorg und Alter m\u00fcrbe machen/", "tokens": ["Als", "dich", "nun", "wol\u00b7ten", "Sorg", "und", "Al\u00b7ter", "m\u00fcr\u00b7be", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "NN", "KON", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gabst du die Sorgen auff/ erwehltest dir die Ruh/", "tokens": ["Gabst", "du", "die", "Sor\u00b7gen", "auff", "/", "er\u00b7wehl\u00b7test", "dir", "die", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "$(", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und brachtst der Jahre Rest mit frommer Andacht zu/", "tokens": ["Und", "brachtst", "der", "Jah\u00b7re", "Rest", "mit", "from\u00b7mer", "An\u00b7dacht", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "APPR", "ADJA", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Izt hastu sie erlangt bi\u00df Erd und Himmel krachen.", "tokens": ["Izt", "has\u00b7tu", "sie", "er\u00b7langt", "bi\u00df", "Erd", "und", "Him\u00b7mel", "kra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dir g\u00f6nnte GOttes Gunst ein r\u00fchmlich langes Leben;", "tokens": ["Dir", "g\u00f6nn\u00b7te", "Got\u00b7tes", "Gunst", "ein", "r\u00fchm\u00b7lich", "lan\u00b7ges", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und wird dir izt darzu ein ewigs Erbe geben.", "tokens": ["Und", "wird", "dir", "izt", "dar\u00b7zu", "ein", "e\u00b7wigs", "Er\u00b7be", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}