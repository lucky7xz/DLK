{"textgrid.poem.57394": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weisst du auch, Gleim noch, wie, o undurstigster", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weisst du auch, Gleim noch, wie, o undurstigster", "tokens": ["Weisst", "du", "auch", ",", "Gleim", "noch", ",", "wie", ",", "o", "un\u00b7durs\u00b7tigs\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "$,", "PWAV", "$,", "FM", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von allen S\u00e4ngern, denen des Weines Lob", "tokens": ["Von", "al\u00b7len", "S\u00e4n\u00b7gern", ",", "de\u00b7nen", "des", "Wei\u00b7nes", "Lob"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Geist, und ihrer eingab, wie wir,", "tokens": ["Sein", "Geist", ",", "und", "ih\u00b7rer", "ein\u00b7gab", ",", "wie", "wir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPOSAT", "VVFIN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ruhend auf Rosen, und Schmidt uns freuten?", "tokens": ["Ru\u00b7hend", "auf", "Ro\u00b7sen", ",", "und", "Schmidt", "uns", "freu\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "KON", "NE", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Im kleinen Garten bl\u00fchten nur sie; und bald", "tokens": ["Im", "klei\u00b7nen", "Gar\u00b7ten", "bl\u00fch\u00b7ten", "nur", "sie", ";", "und", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "PPER", "$.", "KON", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Stand auf dem Marmor blinkend der alte Rhein!", "tokens": ["Stand", "auf", "dem", "Mar\u00b7mor", "blin\u00b7kend", "der", "al\u00b7te", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Wirth' ein Wink; und alle B\u00fcsche", "tokens": ["Dem", "Wir\u00b7th'", "ein", "Wink", ";", "und", "al\u00b7le", "B\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "KON", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wurden gepfl\u00fcckt, und der ganze Saal ward", "tokens": ["Wur\u00b7den", "ge\u00b7pfl\u00fcckt", ",", "und", "der", "gan\u00b7ze", "Saal", "ward"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "KON", "ART", "ADJA", "NN", "VAFIN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Zu R\u00f6the, ward durchstr\u00f6met von s\u00fcssem Duft:", "tokens": ["Zu", "R\u00f6\u00b7the", ",", "ward", "durch\u00b7str\u00f6\u00b7met", "von", "s\u00fcs\u00b7sem", "Duft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "VAFIN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus Rosen ragte halb nur die Flasch' hervor,", "tokens": ["Aus", "Ro\u00b7sen", "rag\u00b7te", "halb", "nur", "die", "Fla\u00b7sch'", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und kaum der Becher. Wag' es, Gleim, nicht", "tokens": ["Und", "kaum", "der", "Be\u00b7cher", ".", "Wag'", "es", ",", "Gleim", ",", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "ART", "NN", "$.", "NN", "PPER", "$,", "NE", "$,", "PTKNEG"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir zu erz\u00e4hlen, wie froh wir sprachen!", "tokens": ["Mir", "zu", "er\u00b7z\u00e4h\u00b7len", ",", "wie", "froh", "wir", "spra\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Wie hell das Lied scholl! Weste verwehn, und selbst", "tokens": ["Wie", "hell", "das", "Lied", "scholl", "!", "Wes\u00b7te", "ver\u00b7wehn", ",", "und", "selbst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADJD", "$.", "NN", "VVINF", "$,", "KON", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Silberquell' ist eh wol versiegt. Was ging", "tokens": ["Die", "Sil\u00b7ber\u00b7quell'", "ist", "eh", "wol", "ver\u00b7siegt", ".", "Was", "ging"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "KOUS", "ADV", "VVPP", "$.", "PWS", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Uns dieses an? Wenn sie auch wollte,", "tokens": ["Uns", "die\u00b7ses", "an", "?", "Wenn", "sie", "auch", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "PTKVZ", "$.", "KOUS", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Konnte denn schweigen der Freuden frohste?", "tokens": ["Konn\u00b7te", "denn", "schwei\u00b7gen", "der", "Freu\u00b7den", "frohs\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "ART", "NN", "ADJA", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Drey waren unser, und der kristallenen", "tokens": ["Drey", "wa\u00b7ren", "un\u00b7ser", ",", "und", "der", "kris\u00b7tal\u00b7le\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "VAFIN", "ADJD", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geb\u00e4ude zwey nur, eins nicht die H\u00e4lfte leer:", "tokens": ["Ge\u00b7b\u00e4u\u00b7de", "zwey", "nur", ",", "eins", "nicht", "die", "H\u00e4lf\u00b7te", "leer", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADV", "$,", "PIS", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und dennoch wallten wir, da hoch schon", "tokens": ["Und", "den\u00b7noch", "wall\u00b7ten", "wir", ",", "da", "hoch", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "ADJD", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Strahlte die Sonne, den sp\u00e4ten Heimweg", "tokens": ["Strahl\u00b7te", "die", "Son\u00b7ne", ",", "den", "sp\u00e4\u00b7ten", "Heim\u00b7weg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit jenem S\u00f6nnchen, welchem der Biene Kunst", "tokens": ["Mit", "je\u00b7nem", "S\u00f6nn\u00b7chen", ",", "wel\u00b7chem", "der", "Bie\u00b7ne", "Kunst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Docht beseelet, welches dem B\u00fcchersaal", "tokens": ["Den", "Docht", "be\u00b7see\u00b7let", ",", "wel\u00b7ches", "dem", "B\u00fc\u00b7cher\u00b7saal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sonst nur die Nacht entscheucht, wenn Gr\u00fcbler", "tokens": ["Sonst", "nur", "die", "Nacht", "ent\u00b7scheucht", ",", "wenn", "Gr\u00fcb\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Endlich die durstige Feder tr\u00e4nken.", "tokens": ["End\u00b7lich", "die", "durs\u00b7ti\u00b7ge", "Fe\u00b7der", "tr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Bekr\u00e4nzt das Haupt mir, Bl\u00fcthen des Rebenhains:", "tokens": ["Be\u00b7kr\u00e4nzt", "das", "Haupt", "mir", ",", "Bl\u00fc\u00b7then", "des", "Re\u00b7ben\u00b7hains", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich trug die Kerze! Aber ach schnell erlosch", "tokens": ["Ich", "trug", "die", "Ker\u00b7ze", "!", "A\u00b7ber", "ach", "schnell", "er\u00b7losch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "KON", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die kleine Sonne! Welk', o Reben", "tokens": ["Die", "klei\u00b7ne", "Son\u00b7ne", "!", "Wel\u00b7k'", ",", "o", "Re\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$,", "FM", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bl\u00fcthe, nur weg; denn ich blies das Licht aus.", "tokens": ["Bl\u00fc\u00b7the", ",", "nur", "weg", ";", "denn", "ich", "blies", "das", "Licht", "aus", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Weisst du auch, Gleim, noch, wie in den K\u00fchlungen", "tokens": ["Weisst", "du", "auch", ",", "Gleim", ",", "noch", ",", "wie", "in", "den", "K\u00fch\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NE", "$,", "ADV", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Des hohen Ahorns, und in der Grotte Bach ...", "tokens": ["Des", "ho\u00b7hen", "A\u00b7horns", ",", "und", "in", "der", "Grot\u00b7te", "Bach", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "APPR", "ART", "NN", "NE", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O gl\u00fcckte mir's, dass ich des Wassers", "tokens": ["O", "gl\u00fcck\u00b7te", "mir's", ",", "dass", "ich", "des", "Was\u00b7sers"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Loh zu dem Lobe des Weines stimte.", "tokens": ["Loh", "zu", "dem", "Lo\u00b7be", "des", "Wei\u00b7nes", "stim\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Am Bache sassen wir in den Frischungen", "tokens": ["Am", "Ba\u00b7che", "sas\u00b7sen", "wir", "in", "den", "Fri\u00b7schun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Schattens. Wenig wurde der scheue Fuss", "tokens": ["Des", "Schat\u00b7tens", ".", "We\u00b7nig", "wur\u00b7de", "der", "scheu\u00b7e", "Fuss"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zuerst gesenkt, bald ganz vertiefet,", "tokens": ["Zu\u00b7erst", "ge\u00b7senkt", ",", "bald", "ganz", "ver\u00b7tie\u00b7fet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun auch das Knie, und gewandert ward dann", "tokens": ["Nun", "auch", "das", "Knie", ",", "und", "ge\u00b7wan\u00b7dert", "ward", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "KON", "VVPP", "VAFIN", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Selbst in des Felsen W\u00f6lbung! Geh\u00f6hlet war", "tokens": ["Selbst", "in", "des", "Fel\u00b7sen", "W\u00f6l\u00b7bung", "!", "Ge\u00b7h\u00f6h\u00b7let", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$.", "NN", "VAFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die eingetauchte Hand, o wie sch\u00f6pften wir!", "tokens": ["Die", "ein\u00b7ge\u00b7tauch\u00b7te", "Hand", ",", "o", "wie", "sch\u00f6pf\u00b7ten", "wir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "FM", "KOKOM", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aus unsrer tiefen, vollen Urne", "tokens": ["Aus", "uns\u00b7rer", "tie\u00b7fen", ",", "vol\u00b7len", "Ur\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rieselt' es nicht in des Freundes Locken.", "tokens": ["Rie\u00b7selt'", "es", "nicht", "in", "des", "Freun\u00b7des", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Des Dorfes M\u00e4dchen brachten den \u00c4hrenkranz,", "tokens": ["Des", "Dor\u00b7fes", "M\u00e4d\u00b7chen", "brach\u00b7ten", "den", "\u00c4h\u00b7ren\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durchschimmert von der Bl\u00e4ue der lieblichen", "tokens": ["Durch\u00b7schim\u00b7mert", "von", "der", "Bl\u00e4u\u00b7e", "der", "lieb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kornblume. \u00bbGebet, gebt! doch schmucker", "tokens": ["Korn\u00b7blu\u00b7me", ".", "\u00bb", "Ge\u00b7bet", ",", "gebt", "!", "doch", "schmu\u00b7cker"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "$(", "NN", "$,", "VVFIN", "$.", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4ret ihr uns, wenn ihr Eimer br\u00e4chtet!\u00ab", "tokens": ["W\u00e4\u00b7ret", "ihr", "uns", ",", "wenn", "ihr", "Ei\u00b7mer", "br\u00e4ch\u00b7tet", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Schnell standen vor uns nicht dana\u00efdische,", "tokens": ["Schnell", "stan\u00b7den", "vor", "uns", "nicht", "dana\u00ef\u00b7di\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Geraume Eimer. Freude! die Wasserschlacht", "tokens": ["Ge\u00b7rau\u00b7me", "Ei\u00b7mer", ".", "Freu\u00b7de", "!", "die", "Was\u00b7ser\u00b7schlacht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$.", "NN", "$.", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Begann! Geschehn sind Thaten, derer", "tokens": ["Be\u00b7gann", "!", "Ge\u00b7schehn", "sind", "Tha\u00b7ten", ",", "de\u00b7rer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "NN", "VAFIN", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzo noch Meldung des Pfl\u00fcgers Mund thut.", "tokens": ["Jet\u00b7zo", "noch", "Mel\u00b7dung", "des", "Pfl\u00fc\u00b7gers", "Mund", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Da galt es St\u00e4rke, Kunst: Wer am weitesten,", "tokens": ["Da", "galt", "es", "St\u00e4r\u00b7ke", ",", "Kunst", ":", "Wer", "am", "wei\u00b7tes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "$.", "PWS", "APPRART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im h\u00f6chsten Bogen tr\u00e4fe des Auges Stern!", "tokens": ["Im", "h\u00f6chs\u00b7ten", "Bo\u00b7gen", "tr\u00e4\u00b7fe", "des", "Au\u00b7ges", "Stern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Fehlg\u00fcsse Jachten wir, der Hofhund", "tokens": ["Fehl\u00b7g\u00fcs\u00b7se", "Jach\u00b7ten", "wir", ",", "der", "Hof\u00b7hund"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Bellte sie, kr\u00e4hte der Henne Mann aus.", "tokens": ["Bell\u00b7te", "sie", ",", "kr\u00e4h\u00b7te", "der", "Hen\u00b7ne", "Mann", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Hoch auf dem H\u00fcgel stand bey der Kirche Thurm", "tokens": ["Hoch", "auf", "dem", "H\u00fc\u00b7gel", "stand", "bey", "der", "Kir\u00b7che", "Thurm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der feiste K\u00fcster, \u00e4ugelte keck nach uns", "tokens": ["Der", "feis\u00b7te", "K\u00fcs\u00b7ter", ",", "\u00e4u\u00b7gel\u00b7te", "keck", "nach", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ADJD", "APPR", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Durch's lange Rohr. Mit vollen Eimern", "tokens": ["Durch's", "lan\u00b7ge", "Rohr", ".", "Mit", "vol\u00b7len", "Ei\u00b7mern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schritten wir hin; doch er war entronnen.", "tokens": ["Schrit\u00b7ten", "wir", "hin", ";", "doch", "er", "war", "ent\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Weisst du auch, Gleim noch, wie, o undurstigster", "tokens": ["Weisst", "du", "auch", ",", "Gleim", "noch", ",", "wie", ",", "o", "un\u00b7durs\u00b7tigs\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "$,", "PWAV", "$,", "FM", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von allen S\u00e4ngern, denen des Weines Lob", "tokens": ["Von", "al\u00b7len", "S\u00e4n\u00b7gern", ",", "de\u00b7nen", "des", "Wei\u00b7nes", "Lob"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Geist, und ihrer eingab, wie wir,", "tokens": ["Sein", "Geist", ",", "und", "ih\u00b7rer", "ein\u00b7gab", ",", "wie", "wir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPOSAT", "VVFIN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ruhend auf Rosen, und Schmidt uns freuten?", "tokens": ["Ru\u00b7hend", "auf", "Ro\u00b7sen", ",", "und", "Schmidt", "uns", "freu\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "KON", "NE", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.16": {"line.1": {"text": "Im kleinen Garten bl\u00fchten nur sie; und bald", "tokens": ["Im", "klei\u00b7nen", "Gar\u00b7ten", "bl\u00fch\u00b7ten", "nur", "sie", ";", "und", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADV", "PPER", "$.", "KON", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Stand auf dem Marmor blinkend der alte Rhein!", "tokens": ["Stand", "auf", "dem", "Mar\u00b7mor", "blin\u00b7kend", "der", "al\u00b7te", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Wirth' ein Wink; und alle B\u00fcsche", "tokens": ["Dem", "Wir\u00b7th'", "ein", "Wink", ";", "und", "al\u00b7le", "B\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "KON", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wurden gepfl\u00fcckt, und der ganze Saal ward", "tokens": ["Wur\u00b7den", "ge\u00b7pfl\u00fcckt", ",", "und", "der", "gan\u00b7ze", "Saal", "ward"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "KON", "ART", "ADJA", "NN", "VAFIN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.17": {"line.1": {"text": "Zu R\u00f6the, ward durchstr\u00f6met von s\u00fcssem Duft:", "tokens": ["Zu", "R\u00f6\u00b7the", ",", "ward", "durch\u00b7str\u00f6\u00b7met", "von", "s\u00fcs\u00b7sem", "Duft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "VAFIN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus Rosen ragte halb nur die Flasch' hervor,", "tokens": ["Aus", "Ro\u00b7sen", "rag\u00b7te", "halb", "nur", "die", "Fla\u00b7sch'", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und kaum der Becher. Wag' es, Gleim, nicht", "tokens": ["Und", "kaum", "der", "Be\u00b7cher", ".", "Wag'", "es", ",", "Gleim", ",", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "ART", "NN", "$.", "NN", "PPER", "$,", "NE", "$,", "PTKNEG"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir zu erz\u00e4hlen, wie froh wir sprachen!", "tokens": ["Mir", "zu", "er\u00b7z\u00e4h\u00b7len", ",", "wie", "froh", "wir", "spra\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.18": {"line.1": {"text": "Wie hell das Lied scholl! Weste verwehn, und selbst", "tokens": ["Wie", "hell", "das", "Lied", "scholl", "!", "Wes\u00b7te", "ver\u00b7wehn", ",", "und", "selbst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADJD", "$.", "NN", "VVINF", "$,", "KON", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Silberquell' ist eh wol versiegt. Was ging", "tokens": ["Die", "Sil\u00b7ber\u00b7quell'", "ist", "eh", "wol", "ver\u00b7siegt", ".", "Was", "ging"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "KOUS", "ADV", "VVPP", "$.", "PWS", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Uns dieses an? Wenn sie auch wollte,", "tokens": ["Uns", "die\u00b7ses", "an", "?", "Wenn", "sie", "auch", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "PTKVZ", "$.", "KOUS", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Konnte denn schweigen der Freuden frohste?", "tokens": ["Konn\u00b7te", "denn", "schwei\u00b7gen", "der", "Freu\u00b7den", "frohs\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "ART", "NN", "ADJA", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.19": {"line.1": {"text": "Drey waren unser, und der kristallenen", "tokens": ["Drey", "wa\u00b7ren", "un\u00b7ser", ",", "und", "der", "kris\u00b7tal\u00b7le\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "VAFIN", "ADJD", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geb\u00e4ude zwey nur, eins nicht die H\u00e4lfte leer:", "tokens": ["Ge\u00b7b\u00e4u\u00b7de", "zwey", "nur", ",", "eins", "nicht", "die", "H\u00e4lf\u00b7te", "leer", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADV", "$,", "PIS", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und dennoch wallten wir, da hoch schon", "tokens": ["Und", "den\u00b7noch", "wall\u00b7ten", "wir", ",", "da", "hoch", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "ADJD", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Strahlte die Sonne, den sp\u00e4ten Heimweg", "tokens": ["Strahl\u00b7te", "die", "Son\u00b7ne", ",", "den", "sp\u00e4\u00b7ten", "Heim\u00b7weg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.20": {"line.1": {"text": "Mit jenem S\u00f6nnchen, welchem der Biene Kunst", "tokens": ["Mit", "je\u00b7nem", "S\u00f6nn\u00b7chen", ",", "wel\u00b7chem", "der", "Bie\u00b7ne", "Kunst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Docht beseelet, welches dem B\u00fcchersaal", "tokens": ["Den", "Docht", "be\u00b7see\u00b7let", ",", "wel\u00b7ches", "dem", "B\u00fc\u00b7cher\u00b7saal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sonst nur die Nacht entscheucht, wenn Gr\u00fcbler", "tokens": ["Sonst", "nur", "die", "Nacht", "ent\u00b7scheucht", ",", "wenn", "Gr\u00fcb\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Endlich die durstige Feder tr\u00e4nken.", "tokens": ["End\u00b7lich", "die", "durs\u00b7ti\u00b7ge", "Fe\u00b7der", "tr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.21": {"line.1": {"text": "Bekr\u00e4nzt das Haupt mir, Bl\u00fcthen des Rebenhains:", "tokens": ["Be\u00b7kr\u00e4nzt", "das", "Haupt", "mir", ",", "Bl\u00fc\u00b7then", "des", "Re\u00b7ben\u00b7hains", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich trug die Kerze! Aber ach schnell erlosch", "tokens": ["Ich", "trug", "die", "Ker\u00b7ze", "!", "A\u00b7ber", "ach", "schnell", "er\u00b7losch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "KON", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die kleine Sonne! Welk', o Reben", "tokens": ["Die", "klei\u00b7ne", "Son\u00b7ne", "!", "Wel\u00b7k'", ",", "o", "Re\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$,", "FM", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bl\u00fcthe, nur weg; denn ich blies das Licht aus.", "tokens": ["Bl\u00fc\u00b7the", ",", "nur", "weg", ";", "denn", "ich", "blies", "das", "Licht", "aus", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.22": {"line.1": {"text": "Weisst du auch, Gleim, noch, wie in den K\u00fchlungen", "tokens": ["Weisst", "du", "auch", ",", "Gleim", ",", "noch", ",", "wie", "in", "den", "K\u00fch\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NE", "$,", "ADV", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Des hohen Ahorns, und in der Grotte Bach ...", "tokens": ["Des", "ho\u00b7hen", "A\u00b7horns", ",", "und", "in", "der", "Grot\u00b7te", "Bach", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "APPR", "ART", "NN", "NE", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O gl\u00fcckte mir's, dass ich des Wassers", "tokens": ["O", "gl\u00fcck\u00b7te", "mir's", ",", "dass", "ich", "des", "Was\u00b7sers"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Loh zu dem Lobe des Weines stimte.", "tokens": ["Loh", "zu", "dem", "Lo\u00b7be", "des", "Wei\u00b7nes", "stim\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.23": {"line.1": {"text": "Am Bache sassen wir in den Frischungen", "tokens": ["Am", "Ba\u00b7che", "sas\u00b7sen", "wir", "in", "den", "Fri\u00b7schun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Schattens. Wenig wurde der scheue Fuss", "tokens": ["Des", "Schat\u00b7tens", ".", "We\u00b7nig", "wur\u00b7de", "der", "scheu\u00b7e", "Fuss"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zuerst gesenkt, bald ganz vertiefet,", "tokens": ["Zu\u00b7erst", "ge\u00b7senkt", ",", "bald", "ganz", "ver\u00b7tie\u00b7fet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun auch das Knie, und gewandert ward dann", "tokens": ["Nun", "auch", "das", "Knie", ",", "und", "ge\u00b7wan\u00b7dert", "ward", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "KON", "VVPP", "VAFIN", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.24": {"line.1": {"text": "Selbst in des Felsen W\u00f6lbung! Geh\u00f6hlet war", "tokens": ["Selbst", "in", "des", "Fel\u00b7sen", "W\u00f6l\u00b7bung", "!", "Ge\u00b7h\u00f6h\u00b7let", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$.", "NN", "VAFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die eingetauchte Hand, o wie sch\u00f6pften wir!", "tokens": ["Die", "ein\u00b7ge\u00b7tauch\u00b7te", "Hand", ",", "o", "wie", "sch\u00f6pf\u00b7ten", "wir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "FM", "KOKOM", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aus unsrer tiefen, vollen Urne", "tokens": ["Aus", "uns\u00b7rer", "tie\u00b7fen", ",", "vol\u00b7len", "Ur\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rieselt' es nicht in des Freundes Locken.", "tokens": ["Rie\u00b7selt'", "es", "nicht", "in", "des", "Freun\u00b7des", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.25": {"line.1": {"text": "Des Dorfes M\u00e4dchen brachten den \u00c4hrenkranz,", "tokens": ["Des", "Dor\u00b7fes", "M\u00e4d\u00b7chen", "brach\u00b7ten", "den", "\u00c4h\u00b7ren\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durchschimmert von der Bl\u00e4ue der lieblichen", "tokens": ["Durch\u00b7schim\u00b7mert", "von", "der", "Bl\u00e4u\u00b7e", "der", "lieb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kornblume. \u00bbGebet, gebt! doch schmucker", "tokens": ["Korn\u00b7blu\u00b7me", ".", "\u00bb", "Ge\u00b7bet", ",", "gebt", "!", "doch", "schmu\u00b7cker"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "$(", "NN", "$,", "VVFIN", "$.", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4ret ihr uns, wenn ihr Eimer br\u00e4chtet!\u00ab", "tokens": ["W\u00e4\u00b7ret", "ihr", "uns", ",", "wenn", "ihr", "Ei\u00b7mer", "br\u00e4ch\u00b7tet", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.26": {"line.1": {"text": "Schnell standen vor uns nicht dana\u00efdische,", "tokens": ["Schnell", "stan\u00b7den", "vor", "uns", "nicht", "dana\u00ef\u00b7di\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Geraume Eimer. Freude! die Wasserschlacht", "tokens": ["Ge\u00b7rau\u00b7me", "Ei\u00b7mer", ".", "Freu\u00b7de", "!", "die", "Was\u00b7ser\u00b7schlacht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$.", "NN", "$.", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Begann! Geschehn sind Thaten, derer", "tokens": ["Be\u00b7gann", "!", "Ge\u00b7schehn", "sind", "Tha\u00b7ten", ",", "de\u00b7rer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "NN", "VAFIN", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzo noch Meldung des Pfl\u00fcgers Mund thut.", "tokens": ["Jet\u00b7zo", "noch", "Mel\u00b7dung", "des", "Pfl\u00fc\u00b7gers", "Mund", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.27": {"line.1": {"text": "Da galt es St\u00e4rke, Kunst: Wer am weitesten,", "tokens": ["Da", "galt", "es", "St\u00e4r\u00b7ke", ",", "Kunst", ":", "Wer", "am", "wei\u00b7tes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "$.", "PWS", "APPRART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im h\u00f6chsten Bogen tr\u00e4fe des Auges Stern!", "tokens": ["Im", "h\u00f6chs\u00b7ten", "Bo\u00b7gen", "tr\u00e4\u00b7fe", "des", "Au\u00b7ges", "Stern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Fehlg\u00fcsse Jachten wir, der Hofhund", "tokens": ["Fehl\u00b7g\u00fcs\u00b7se", "Jach\u00b7ten", "wir", ",", "der", "Hof\u00b7hund"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Bellte sie, kr\u00e4hte der Henne Mann aus.", "tokens": ["Bell\u00b7te", "sie", ",", "kr\u00e4h\u00b7te", "der", "Hen\u00b7ne", "Mann", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.28": {"line.1": {"text": "Hoch auf dem H\u00fcgel stand bey der Kirche Thurm", "tokens": ["Hoch", "auf", "dem", "H\u00fc\u00b7gel", "stand", "bey", "der", "Kir\u00b7che", "Thurm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der feiste K\u00fcster, \u00e4ugelte keck nach uns", "tokens": ["Der", "feis\u00b7te", "K\u00fcs\u00b7ter", ",", "\u00e4u\u00b7gel\u00b7te", "keck", "nach", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ADJD", "APPR", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Durch's lange Rohr. Mit vollen Eimern", "tokens": ["Durch's", "lan\u00b7ge", "Rohr", ".", "Mit", "vol\u00b7len", "Ei\u00b7mern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schritten wir hin; doch er war entronnen.", "tokens": ["Schrit\u00b7ten", "wir", "hin", ";", "doch", "er", "war", "ent\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}