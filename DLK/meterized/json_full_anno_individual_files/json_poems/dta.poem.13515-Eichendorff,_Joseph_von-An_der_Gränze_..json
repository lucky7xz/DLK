{"dta.poem.13515": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An der Gr\u00e4nze .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die treuen Berg' steh'n auf der Wacht:", "tokens": ["Die", "treu\u00b7en", "Ber\u00b7g'", "steh'n", "auf", "der", "Wacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u201ewer streicht bei stiller Morgenzeit", "tokens": ["\u201e", "wer", "streicht", "bei", "stil\u00b7ler", "Mor\u00b7gen\u00b7zeit"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da aus der Fremde durch die Haid'?\u201c \u2014", "tokens": ["Da", "aus", "der", "Frem\u00b7de", "durch", "die", "Haid'", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich aber mir die Berg' betracht'", "tokens": ["Ich", "a\u00b7ber", "mir", "die", "Ber\u00b7g'", "be\u00b7tracht'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und lach' in mich vor gro\u00dfer Lust,", "tokens": ["Und", "lach'", "in", "mich", "vor", "gro\u00b7\u00dfer", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und rufe recht aus frischer Brust", "tokens": ["Und", "ru\u00b7fe", "recht", "aus", "fri\u00b7scher", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Parol und Feldgeschrei sogleich:", "tokens": ["Pa\u00b7rol", "und", "Feld\u00b7ge\u00b7schrei", "sog\u00b7leich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vivat Oestreich!", "tokens": ["Vi\u00b7vat", "O\u00b7e\u00b7streich", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Da kennt mich erst die ganze Rund,", "tokens": ["Da", "kennt", "mich", "erst", "die", "gan\u00b7ze", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun gr\u00fc\u00dfen Bach und V\u00f6glein zart", "tokens": ["Nun", "gr\u00fc\u00b7\u00dfen", "Bach", "und", "V\u00f6\u00b7glein", "zart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und W\u00e4lder rings nach Landesart,", "tokens": ["Und", "W\u00e4l\u00b7der", "rings", "nach", "Lan\u00b7des\u00b7art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Donau blitzt aus tiefem Grund,", "tokens": ["Die", "Do\u00b7nau", "blitzt", "aus", "tie\u00b7fem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Stephansthurm auch ganz von fern", "tokens": ["Der", "Ste\u00b7phanst\u00b7hurm", "auch", "ganz", "von", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Guckt \u00fcber'n Berg und s\u00e4h' mich gern,", "tokens": ["Guckt", "\u00fc\u00b7ber'n", "Berg", "und", "s\u00e4h'", "mich", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ist er's nicht, so kommt er doch gleich,", "tokens": ["Und", "ist", "er's", "nicht", ",", "so", "kommt", "er", "doch", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Vivat Oestreich!", "tokens": ["Vi\u00b7vat", "O\u00b7e\u00b7streich", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}