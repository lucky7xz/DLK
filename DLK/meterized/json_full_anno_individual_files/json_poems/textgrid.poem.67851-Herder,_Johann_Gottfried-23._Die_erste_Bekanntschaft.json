{"textgrid.poem.67851": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "23. Die erste Bekanntschaft", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief in Nacht, im Dunkel,", "tokens": ["Tief", "in", "Nacht", ",", "im", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Tief im dicken Walde,", "tokens": ["Tief", "im", "di\u00b7cken", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ferne war mein liebes M\u00e4dchen,", "tokens": ["Fer\u00b7ne", "war", "mein", "lie\u00b7bes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh ich sie noch kannte.", "tokens": ["Eh", "ich", "sie", "noch", "kann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ohne sie zu kennen,", "tokens": ["Oh\u00b7ne", "sie", "zu", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ritt ich ungef\u00e4hr hin,", "tokens": ["Ritt", "ich", "un\u00b7ge\u00b7f\u00e4hr", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3