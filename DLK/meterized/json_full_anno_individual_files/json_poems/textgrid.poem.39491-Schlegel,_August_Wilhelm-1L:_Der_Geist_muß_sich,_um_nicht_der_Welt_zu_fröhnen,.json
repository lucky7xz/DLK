{"textgrid.poem.39491": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Geist mu\u00df sich, um nicht der Welt zu fr\u00f6hnen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Geist mu\u00df sich, um nicht der Welt zu fr\u00f6hnen,", "tokens": ["Der", "Geist", "mu\u00df", "sich", ",", "um", "nicht", "der", "Welt", "zu", "fr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "$,", "KOUI", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Weltanschauung in sich selbst vertiefen.", "tokens": ["Zur", "Welt\u00b7an\u00b7schau\u00b7ung", "in", "sich", "selbst", "ver\u00b7tie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Begreifend schafft er Kr\u00e4fte, welche schliefen,", "tokens": ["Be\u00b7grei\u00b7fend", "schafft", "er", "Kr\u00e4f\u00b7te", ",", "wel\u00b7che", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die durch Bewu\u00dftsein sich als m\u00fcndig kr\u00f6nen.", "tokens": ["Die", "durch", "Be\u00b7wu\u00df\u00b7tsein", "sich", "als", "m\u00fcn\u00b7dig", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PRF", "KOUS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1