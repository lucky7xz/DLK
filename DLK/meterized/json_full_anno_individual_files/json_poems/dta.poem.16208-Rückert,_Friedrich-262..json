{"dta.poem.16208": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "262.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie sagen mir, ich glaubs, allein ich f\u00fchl' es nicht,", "tokens": ["Sie", "sa\u00b7gen", "mir", ",", "ich", "glaubs", ",", "al\u00b7lein", "ich", "f\u00fchl'", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "NE", "$,", "ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nun mein Haupt ein Kranz von Dichterlaub umflicht.", "tokens": ["Da\u00df", "nun", "mein", "Haupt", "ein", "Kranz", "von", "Dich\u00b7ter\u00b7laub", "um\u00b7flicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was hilft, den andre sehn, der Kranz, den ich nicht f\u00fchle,", "tokens": ["Was", "hilft", ",", "den", "and\u00b7re", "sehn", ",", "der", "Kranz", ",", "den", "ich", "nicht", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "PIS", "VVINF", "$,", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht f\u00fchle, da\u00df er mir die hei\u00dfen Schl\u00e4fe k\u00fchle!", "tokens": ["Nicht", "f\u00fch\u00b7le", ",", "da\u00df", "er", "mir", "die", "hei\u00b7\u00df