{"dta.poem.15840": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beim Schlafengehn, als ich das Licht l\u00f6scht' in der Nacht,", "tokens": ["Beim", "Schla\u00b7fen\u00b7gehn", ",", "als", "ich", "das", "Licht", "l\u00f6scht'", "in", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kam ein Gedanke mir, den ich noch nie gedacht:", "tokens": ["Kam", "ein", "Ge\u00b7dan\u00b7ke", "mir", ",", "den", "ich", "noch", "nie", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verloschen ist das Licht des Tages, und dazu", "tokens": ["Ver\u00b7lo\u00b7schen", "ist", "das", "Licht", "des", "Ta\u00b7ges", ",", "und", "da\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$,", "KON", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier seinen schwachen Stellvertreter l\u00f6schest du.", "tokens": ["Hier", "sei\u00b7nen", "schwa\u00b7chen", "Stell\u00b7ver\u00b7tre\u00b7ter", "l\u00f6\u00b7schest", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wei\u00dft du, ob das Licht dein Auge wieder sieht,", "tokens": ["Und", "wei\u00dft", "du", ",", "ob", "das", "Licht", "dein", "Au\u00b7ge", "wie\u00b7der", "sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob ew'ge Nacht es nicht in dieser Nacht umzieht?", "tokens": ["Ob", "ew'\u00b7ge", "Nacht", "es", "nicht", "in", "die\u00b7ser", "Nacht", "um\u00b7zieht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}