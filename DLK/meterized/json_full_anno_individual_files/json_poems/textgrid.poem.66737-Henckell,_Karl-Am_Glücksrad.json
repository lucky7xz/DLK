{"textgrid.poem.66737": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Am Gl\u00fccksrad", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst flog es von \u00bbbraunen Lappen\u00ab", "tokens": ["Einst", "flog", "es", "von", "\u00bb", "brau\u00b7nen", "Lap\u00b7pen", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "$(", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr, wenn sie nur wollte, so zu \u2013", "tokens": ["Ihr", ",", "wenn", "sie", "nur", "woll\u00b7te", ",", "so", "zu", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "ADV", "VMFIN", "$,", "ADV", "PTKZU", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie fuhr mit den feurigsten Rappen", "tokens": ["Sie", "fuhr", "mit", "den", "feu\u00b7rigs\u00b7ten", "Rap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und stand mit Reichsgrafen auf Du.", "tokens": ["Und", "stand", "mit", "Reichs\u00b7gra\u00b7fen", "auf", "Du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "PPER", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Von Polen her hatte sie Rasse,", "tokens": ["Von", "Po\u00b7len", "her", "hat\u00b7te", "sie", "Ras\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APZR", "VAFIN", "PPER", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dazu eine Stimme so s\u00fc\u00df,", "tokens": ["Da\u00b7zu", "ei\u00b7ne", "Stim\u00b7me", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der P\u00f6bel blieb stehn auf der Gasse,", "tokens": ["Der", "P\u00f6\u00b7bel", "blieb", "stehn", "auf", "der", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das klang wie vom Paradies.", "tokens": ["Das", "klang", "wie", "vom", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und abends erdr\u00f6hnten die W\u00e4nde", "tokens": ["Und", "a\u00b7bends", "er\u00b7dr\u00f6hn\u00b7ten", "die", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von donnerndem, wildem Applaus,", "tokens": ["Von", "don\u00b7nern\u00b7dem", ",", "wil\u00b7dem", "Ap\u00b7plaus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PAV", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Den J\u00fcnglingen brannten die H\u00e4nde,", "tokens": ["Den", "J\u00fcng\u00b7lin\u00b7gen", "brann\u00b7ten", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Man spannte die Pferde ihr aus.", "tokens": ["Man", "spann\u00b7te", "die", "Pfer\u00b7de", "ihr", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Das waren die Zeiten der Wonne,", "tokens": ["Das", "wa\u00b7ren", "die", "Zei\u00b7ten", "der", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da ging es ihr m\u00e4rchenhaft gut,", "tokens": ["Da", "ging", "es", "ihr", "m\u00e4r\u00b7chen\u00b7haft", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie war nicht geschaffen zur Nonne,", "tokens": ["Sie", "war", "nicht", "ge\u00b7schaf\u00b7fen", "zur", "Non\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie b\u00fc\u00dfte ihr polnisches Blut.", "tokens": ["Sie", "b\u00fc\u00df\u00b7te", "ihr", "pol\u00b7ni\u00b7sches", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch einen, den liebt' sie am meisten,", "tokens": ["Doch", "ei\u00b7nen", ",", "den", "liebt'", "sie", "am", "meis\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ART", "VVFIN", "PPER", "PTKA", "PIS", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das war nur ein armer Student,", "tokens": ["Das", "war", "nur", "ein", "ar\u00b7mer", "Stu\u00b7dent", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der konnte kein M\u00e4del sich leisten,", "tokens": ["Der", "konn\u00b7te", "kein", "M\u00e4\u00b7del", "sich", "leis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und Liebe, ja Liebe, die brennt.", "tokens": ["Und", "Lie\u00b7be", ",", "ja", "Lie\u00b7be", ",", "die", "brennt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Dem hat sie aus Liebe gesungen", "tokens": ["Dem", "hat", "sie", "aus", "Lie\u00b7be", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und tat es doch sonst nur f\u00fcr Geld,", "tokens": ["Und", "tat", "es", "doch", "sonst", "nur", "f\u00fcr", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er sagte, das h\u00e4tte geklungen", "tokens": ["Er", "sag\u00b7te", ",", "das", "h\u00e4t\u00b7te", "ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VAFIN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie die Lerche auf mohnrotem Feld ...", "tokens": ["Wie", "die", "Ler\u00b7che", "auf", "mohn\u00b7ro\u00b7tem", "Feld", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Die Lerche sie sank in die Rinne,", "tokens": ["Die", "Ler\u00b7che", "sie", "sank", "in", "die", "Rin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Welt ist ein tr\u00fcber Kanal ...", "tokens": ["Die", "Welt", "ist", "ein", "tr\u00fc\u00b7ber", "Ka\u00b7nal", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Lorbeeren und goldne Gewinne \u2013", "tokens": ["Lor\u00b7bee\u00b7ren", "und", "gold\u00b7ne", "Ge\u00b7win\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wann war denn das einmal?", "tokens": ["Wann", "war", "denn", "das", "ein\u00b7mal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie kann sich noch kaum drauf besinnen,", "tokens": ["Sie", "kann", "sich", "noch", "kaum", "drauf", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist wie ein uralter Traum \u2013", "tokens": ["Es", "ist", "wie", "ein", "ur\u00b7al\u00b7ter", "Traum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die blinden Tage verrinnen", "tokens": ["Die", "blin\u00b7den", "Ta\u00b7ge", "ver\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zunichte wie Schatten und Schaum.", "tokens": ["Zu\u00b7nich\u00b7te", "wie", "Schat\u00b7ten", "und", "Schaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Ihre Stimme tot und verrostet,", "tokens": ["Ih\u00b7re", "Stim\u00b7me", "tot", "und", "ver\u00b7ros\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zerw\u00fchlt und starr ihr Gesicht \u2013", "tokens": ["Zer\u00b7w\u00fchlt", "und", "starr", "ihr", "Ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Man fragt, \u00bbwas die Gl\u00fcckspartie kostet\u00ab,", "tokens": ["Man", "fragt", ",", "\u00bb", "was", "die", "Gl\u00fccks\u00b7par\u00b7tie", "kos\u00b7tet", "\u00ab", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "$,", "$(", "PWS", "ART", "NN", "VVFIN", "$(", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie sagt: \u00bbEinen Groschen \u2013 mehr nicht.\u00ab", "tokens": ["Sie", "sagt", ":", "\u00bb", "Ei\u00b7nen", "Gro\u00b7schen", "\u2013", "mehr", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "$(", "ADV", "PTKNEG", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Dann wirft sie den Kopf in den Nacken", "tokens": ["Dann", "wirft", "sie", "den", "Kopf", "in", "den", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und horcht wie auf fernes Getrab,", "tokens": ["Und", "horcht", "wie", "auf", "fer\u00b7nes", "Ge\u00b7trab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Bald holt ja mit schwarzen Schabracken", "tokens": ["Bald", "holt", "ja", "mit", "schwar\u00b7zen", "Scha\u00b7bra\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der m\u00e4chtigste Reichsgraf sie ab.", "tokens": ["Der", "m\u00e4ch\u00b7tigs\u00b7te", "Reichs\u00b7graf", "sie", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Einst flog es von \u00bbbraunen Lappen\u00ab", "tokens": ["Einst", "flog", "es", "von", "\u00bb", "brau\u00b7nen", "Lap\u00b7pen", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "$(", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.re