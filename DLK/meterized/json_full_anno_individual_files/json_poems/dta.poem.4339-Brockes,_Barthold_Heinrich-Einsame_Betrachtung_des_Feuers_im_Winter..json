"text": "Giebt mir in Einsamkeit oft ein Vergn\u00fcgen.", "tokens": ["Giebt", "mir", "in", "Ein\u00b7sam\u00b7keit", "oft", "ein", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Oft dienet ihrer Fl\u00fcchtigkeit,", "tokens": ["Oft", "die\u00b7net", "ih\u00b7rer", "Fl\u00fcch\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fast von den meisten ird\u2019schen Dingen", "tokens": ["Fast", "von", "den", "meis\u00b7ten", "ird'\u00b7schen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir ein Erwegen beyzubringen.", "tokens": ["Mir", "ein", "Er\u00b7we\u00b7gen", "bey\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Oft denk\u2019 ich selbst dabey an meine fl\u00fccht\u2019ge Zeit.", "tokens": ["Oft", "denk'", "ich", "selbst", "da\u00b7bey", "an", "mei\u00b7ne", "fl\u00fccht'\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Oft dient es mir, von meinem Leben", "tokens": ["Oft", "dient", "es", "mir", ",", "von", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,",