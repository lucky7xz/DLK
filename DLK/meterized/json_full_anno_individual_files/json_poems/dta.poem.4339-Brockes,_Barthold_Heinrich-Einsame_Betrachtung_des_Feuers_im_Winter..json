{"dta.poem.4339": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einsame Betrachtung des Feuers  \n im Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich j\u00fcngst, zur kalten Winters-Zeit,", "tokens": ["In\u00b7dem", "ich", "j\u00fcngst", ",", "zur", "kal\u00b7ten", "Win\u00b7ter\u00b7sZeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da Berg und Wald bereift, das Feld beschneyt,", "tokens": ["Da", "Berg", "und", "Wald", "be\u00b7reift", ",", "das", "Feld", "be\u00b7schneyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Fluht gefroren war, im Zimmer, beym Camin,", "tokens": ["Die", "Fluht", "ge\u00b7fro\u00b7ren", "war", ",", "im", "Zim\u00b7mer", ",", "beym", "Ca\u00b7min", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein helles Feur theils lodern und theils gl\u00fchn,", "tokens": ["Ein", "hel\u00b7les", "Feur", "theils", "lo\u00b7dern", "und", "theils", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit Freuden sah, mit Anmuht f\u00fchlte,", "tokens": ["Mit", "Freu\u00b7den", "sah", ",", "mit", "An\u00b7muht", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und, da ich eben ganz allein,", "tokens": ["Und", ",", "da", "ich", "e\u00b7ben", "ganz", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bald auf der Flamme regen Schein,", "tokens": ["Bald", "auf", "der", "Flam\u00b7me", "re\u00b7gen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die mit gef\u00e4rbten Spitzen spielte,", "tokens": ["Die", "mit", "ge\u00b7f\u00e4rb\u00b7ten", "Spit\u00b7zen", "spiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mein angestrahltes Auge lenket;", "tokens": ["Mein", "an\u00b7ge\u00b7strahl\u00b7tes", "Au\u00b7ge", "len\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Bald in der Kohlen rohte Gluht,", "tokens": ["Bald", "in", "der", "Koh\u00b7len", "roh\u00b7te", "Gluht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Recht als in einer Feuer-Fluht,", "tokens": ["Recht", "als", "in", "ei\u00b7ner", "Feu\u00b7er\u00b7Fluht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die neu-begier\u2019ge Blicke senket;", "tokens": ["Die", "neu\u00b7be\u00b7gier'\u00b7ge", "Bli\u00b7cke", "sen\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Beschlo\u00df ich, nicht so sehr des Feuers Kraft", "tokens": ["Be\u00b7schlo\u00df", "ich", ",", "nicht", "so", "sehr", "des", "Feu\u00b7ers", "Kraft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PTKNEG", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und dessen w\u00e4rmend\u2019 Eigenschaft,", "tokens": ["Und", "des\u00b7sen", "w\u00e4r\u00b7me\u00b7nd'", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "(dieweil es schon vorhin von mir geschehn)", "tokens": ["(", "die\u00b7weil", "es", "schon", "vor\u00b7hin", "von", "mir", "ge\u00b7schehn", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Als dessen Form und Farb\u2019, aufmerksam anzusehn.", "tokens": ["Als", "des\u00b7sen", "Form", "und", "Fa\u00b7rb'", ",", "auf\u00b7merk\u00b7sam", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "KON", "NN", "$,", "ADJD", "VVIZU", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein Gott! wie werd\u2019 ich hier und dar", "tokens": ["Mein", "Gott", "!", "wie", "werd'", "ich", "hier", "und", "dar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "VAFIN", "PPER", "ADV", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So viel Ver\u00e4nderung in dieser Gluht gewahr!", "tokens": ["So", "viel", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "in", "die\u00b7ser", "Gluht", "ge\u00b7wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie sehr vergn\u00fcgt sich nicht", "tokens": ["Wie", "sehr", "ver\u00b7gn\u00fcgt", "sich", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PRF", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein stark darauf gewandt Gesicht!", "tokens": ["Mein", "stark", "da\u00b7rauf", "ge\u00b7wandt", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "PAV", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jhrer Flammen wallend Licht", "tokens": ["Ih\u00b7rer", "Flam\u00b7men", "wal\u00b7lend", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnaubet, zischet, rauschet', sauset,", "tokens": ["Schnau\u00b7bet", ",", "zi\u00b7schet", ",", "rau\u00b7schet'", ",", "sau\u00b7set", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lodert, knallet, prasselt, brauset.", "tokens": ["Lo\u00b7dert", ",", "knal\u00b7let", ",", "pras\u00b7selt", ",", "brau\u00b7set", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fast entsetzlich, und doch sch\u00f6n", "tokens": ["Fast", "ent\u00b7setz\u00b7lich", ",", "und", "doch", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist ihr Glanz und ihr Get\u00f6n.", "tokens": ["Ist", "ihr", "Glanz", "und", "ihr", "Ge\u00b7t\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht glaublich ist, wie mancherley,", "tokens": ["Nicht", "glaub\u00b7lich", "ist", ",", "wie", "man\u00b7cher\u00b7ley", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "$,", "PWAV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An buntem Glanz, an Schimmer, Farb\u2019 und Licht,", "tokens": ["An", "bun\u00b7tem", "Glanz", ",", "an", "Schim\u00b7mer", ",", "Fa\u00b7rb'", "und", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "F\u00fcr ein betrachtendes Gesicht,", "tokens": ["F\u00fcr", "ein", "be\u00b7trach\u00b7ten\u00b7des", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In stiller Einsamkeit, im Feur, zu sehen sey.", "tokens": ["In", "stil\u00b7ler", "Ein\u00b7sam\u00b7keit", ",", "im", "Feur", ",", "zu", "se\u00b7hen", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "$,", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Oft strahlt, in rohiem Glanz, ein Blau wie ein Sapphir", "tokens": ["Oft", "strahlt", ",", "in", "ro\u00b7hi\u00b7em", "Glanz", ",", "ein", "Blau", "wie", "ein", "Sap\u00b7phir"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durch einen weissen Rauch, der gleichfalls gl\u00e4nzt, herf\u00fcr.", "tokens": ["Durch", "ei\u00b7nen", "weis\u00b7sen", "Rauch", ",", "der", "gleich\u00b7falls", "gl\u00e4nzt", ",", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn oberwerts, mit gelblich-weissen Blitzen,", "tokens": ["Wenn", "o\u00b7ber\u00b7werts", ",", "mit", "gelb\u00b7lich\u00b7weis\u00b7sen", "Blit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die schnellen Flammen sich mit hellem Lodern spitzen;", "tokens": ["Die", "schnel\u00b7len", "Flam\u00b7men", "sich", "mit", "hel\u00b7lem", "Lo\u00b7dern", "spit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So scheinet unterwerts, gleich funkelndem Rubin,", "tokens": ["So", "schei\u00b7net", "un\u00b7ter\u00b7werts", ",", "gleich", "fun\u00b7keln\u00b7dem", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ADV", "ADJA", "NE", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der rohten Kohlen Heer fast schrecklich-sch\u00f6n zu gl\u00fchn.", "tokens": ["Der", "roh\u00b7ten", "Koh\u00b7len", "Heer", "fast", "schreck\u00b7lich\u00b7sch\u00f6n", "zu", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In tausendfach geformt-bald klein- bald grossen St\u00fccken", "tokens": ["In", "tau\u00b7send\u00b7fach", "ge\u00b7form\u00b7tbald", "klein", "bald", "gros\u00b7sen", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "TRUNC", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erblickt man Spalten, H\u00f6hlen, L\u00fccken,", "tokens": ["Er\u00b7blickt", "man", "Spal\u00b7ten", ",", "H\u00f6h\u00b7len", ",", "L\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Geborstnen Felsen gleich. In ihren Tiefen glimmt", "tokens": ["Ge\u00b7borst\u00b7nen", "Fel\u00b7sen", "gleich", ".", "In", "ih\u00b7ren", "Tie\u00b7fen", "glimmt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "$.", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der kleinen Flammen Rest; sie wallen recht wie Wellen,", "tokens": ["Der", "klei\u00b7nen", "Flam\u00b7men", "Rest", ";", "sie", "wal\u00b7len", "recht", "wie", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "PPER", "VVINF", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In denen hier und dort an unterschied\u2019nen Stellen,", "tokens": ["In", "de\u00b7nen", "hier", "und", "dort", "an", "un\u00b7ter\u00b7schie\u00b7d'\u00b7nen", "Stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Nebst einem zarten Dunst, von Asch ein Anfang schwimmt,", "tokens": ["Nebst", "ei\u00b7nem", "zar\u00b7ten", "Dunst", ",", "von", "Asch", "ein", "An\u00b7fang", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der sanfte seinen Fortgang nimmt,", "tokens": ["Der", "sanf\u00b7te", "sei\u00b7nen", "Fort\u00b7gang", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und, eh man sichs versicht, die rohte Gluht verstecket.", "tokens": ["Und", ",", "eh", "man", "sichs", "ver\u00b7sicht", ",", "die", "roh\u00b7te", "Gluht", "ver\u00b7ste\u00b7cket", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "PIS", "VVPP", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "In ihrer, blo\u00df allein im Reiche der Natur,", "tokens": ["In", "ih\u00b7rer", ",", "blo\u00df", "al\u00b7lein", "im", "Rei\u00b7che", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$,", "ADV", "ADV", "APPRART", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Feur besiegenden Gewalt", "tokens": ["Das", "Feur", "be\u00b7sie\u00b7gen\u00b7den", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird in der Loder-Asch oft mancherley Figur,", "tokens": ["Wird", "in", "der", "Lo\u00b7der\u00b7Asch", "oft", "man\u00b7cher\u00b7ley", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Oft manche sonst fast nicht zu bildende Gestalt,", "tokens": ["Oft", "man\u00b7che", "sonst", "fast", "nicht", "zu", "bil\u00b7den\u00b7de", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "ADV", "PTKNEG", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"te