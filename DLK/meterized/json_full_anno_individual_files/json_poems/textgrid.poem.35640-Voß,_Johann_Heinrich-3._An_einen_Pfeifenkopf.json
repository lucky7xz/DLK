{"textgrid.poem.35640": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3. An einen Pfeifenkopf", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, aus Meerschaum gezeugt, br\u00e4unlicher Pfeifenkopf,", "tokens": ["Du", ",", "aus", "Meer\u00b7schaum", "ge\u00b7zeugt", ",", "br\u00e4un\u00b7li\u00b7cher", "Pfei\u00b7fen\u00b7kopf", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "NN", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mit der Pflanze gef\u00fcllt, welche Tabago n\u00e4hrt;", "tokens": ["Mit", "der", "Pflan\u00b7ze", "ge\u00b7f\u00fcllt", ",", "wel\u00b7che", "Ta\u00b7ba\u00b7go", "n\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "PWAT", "NN", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Dieses Band sei dir heilig,", "tokens": ["Die\u00b7ses", "Band", "sei", "dir", "hei\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Das mit purpurnen Wellen spielt;", "tokens": ["Das", "mit", "pur\u00b7pur\u00b7nen", "Wel\u00b7len", "spielt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Und gewebt war, den Schnee einer beseelten Brust", "tokens": ["Und", "ge\u00b7webt", "war", ",", "den", "Schnee", "ei\u00b7ner", "be\u00b7seel\u00b7ten", "Brust"], "token_info": ["word", "w