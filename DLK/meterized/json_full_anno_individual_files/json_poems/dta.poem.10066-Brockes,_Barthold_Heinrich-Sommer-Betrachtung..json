{"dta.poem.10066": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sommer-Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alles stehet ietzt im Licht, alles gl\u00e4ntzet, alles gl\u00fchet,", "tokens": ["Al\u00b7les", "ste\u00b7het", "ietzt", "im", "Licht", ",", "al\u00b7les", "gl\u00e4nt\u00b7zet", ",", "al\u00b7les", "gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "NN", "$,", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Alles ist voll W\u00e4rm\u2019 und Schimmer: Und der Son-", "tokens": ["Al\u00b7les", "ist", "voll", "W\u00e4rm'", "und", "Schim\u00b7mer", ":", "Und", "der", "Son"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADJD", "NN", "KON", "NN", "$.", "KON", "ART", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Glimmt und flammet \u00fcberall. Jedem Vorwurff, den man", "tokens": ["Glimmt", "und", "flam\u00b7met", "\u00fc\u00b7be\u00b7rall", ".", "Je\u00b7dem", "Vor\u00b7wurff", ",", "den", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "$.", "PIAT", "NN", "$,", "PRELS", "PIS"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Fl\u00f6sset ihre Segens-Quelle Fruchtbarkeit und Farben ein.", "tokens": ["Fl\u00f6s\u00b7set", "ih\u00b7re", "Se\u00b7gens\u00b7Quel\u00b7le", "Frucht\u00b7bar\u00b7keit", "und", "Far\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Eine lieblich blaue Gluth f\u00fcllt das tieffe Firmament,", "tokens": ["Ei\u00b7ne", "lieb\u00b7lich", "blau\u00b7e", "Gluth", "f\u00fcllt", "das", "tief\u00b7fe", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Wann ein Silber-weisses Feuer in bestrahlten Wolcken", "tokens": ["Wann", "ein", "Sil\u00b7ber\u00b7weis\u00b7ses", "Feu\u00b7er", "in", "be\u00b7strahl\u00b7ten", "Wol\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Eine Purpur-farbne, g\u00fcldne, eine Rosen-farbne Gluth", "tokens": ["Ei\u00b7ne", "Pur\u00b7pur\u00b7fa\u00b7rb\u00b7ne", ",", "g\u00fcld\u00b7ne", ",", "ei\u00b7ne", "Ro\u00b7sen\u00b7fa\u00b7rb\u00b7ne", "Gluth"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.8": {"text": "Brennt in ihnen, ja verdoppelt ihren Glantz in glatter Fluth.", "tokens": ["Brennt", "in", "ih\u00b7nen", ",", "ja", "ver\u00b7dop\u00b7pelt", "ih\u00b7ren", "Glantz", "in", "glat\u00b7ter", "Fluth", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Eine lieblich gr\u00fcne decket Wiesen, Felder, Berg\u2019 und Auen,", "tokens": ["Ei\u00b7ne", "lieb\u00b7lich", "gr\u00fc\u00b7ne", "de\u00b7cket", "Wie\u00b7sen", ",", "Fel\u00b7der", ",", "Ber\u00b7g'", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "VVFIN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.10": {"text": "Wann wir, in gef\u00e4rbten Flammen, bunte G\u00e4rten funckeln", "tokens": ["Wann", "wir", ",", "in", "ge\u00b7f\u00e4rb\u00b7ten", "Flam\u00b7men", ",", "bun\u00b7te", "G\u00e4r\u00b7ten", "fun\u00b7ckeln"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "M\u00f6gte doch so manches Feur, das wir allenthalben", "tokens": ["M\u00f6g\u00b7te", "doch", "so", "man\u00b7ches", "Feur", ",", "das", "wir", "al\u00b7len\u00b7thal\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Durch das Aug\u2019 in unsre Brust auch ein Andachts-Feur", "tokens": ["Durch", "das", "Aug'", "in", "uns\u00b7re", "Brust", "auch", "ein", "An\u00b7dachts\u00b7Feur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "M\u00f6gte doch der Sch\u00f6pfer dieses auch nicht minder Wunder-", "tokens": ["M\u00f6g\u00b7te", "doch", "der", "Sch\u00f6p\u00b7fer", "die\u00b7ses", "auch", "nicht", "min\u00b7der", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "PDAT", "ADV", "PTKNEG", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Als ein geistig Feuer-Werck, Jhm zur Ehre, brennen sehn!", "tokens": ["Als", "ein", "geis\u00b7tig", "Feu\u00b7er\u00b7\u00b7Werck", ",", "Jhm", "zur", "Eh\u00b7re", ",", "bren\u00b7nen", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "$,", "PPER", "APPRART", "NN", "$,", "VVINF", "VVINF", "$."], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}