{"textgrid.poem.66609": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Heimfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Nebel schlummern Tal und Flur;", "tokens": ["Im", "Ne\u00b7bel", "schlum\u00b7mern", "Tal", "und", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Sturmgebraus und Regen", "tokens": ["Durch", "Sturm\u00b7ge\u00b7braus", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die tiefaufdonnernde Eisenspur", "tokens": ["Die", "tie\u00b7fauf\u00b7don\u00b7nern\u00b7de", "Ei\u00b7sen\u00b7spur"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Saus' ich dem Morgen entgegen.", "tokens": ["Saus'", "ich", "dem", "Mor\u00b7gen", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es graut, und fahler Schein erwacht", "tokens": ["Es", "graut", ",", "und", "fah\u00b7ler", "Schein", "er\u00b7wacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dort \u00fcber jenen H\u00f6hen,", "tokens": ["Dort", "\u00fc\u00b7ber", "je\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ins F\u00f6hrendickicht verkriecht die Nacht \u2013", "tokens": ["Ins", "F\u00f6h\u00b7ren\u00b7di\u00b7ckicht", "ver\u00b7kriecht", "die", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Nur weiter in Lust und in Wehen!", "tokens": ["Nur", "wei\u00b7ter", "in", "Lust", "und", "in", "We\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Sto\u00df aus, du eherner Kolo\u00df,", "tokens": ["Sto\u00df", "aus", ",", "du", "e\u00b7her\u00b7ner", "Ko\u00b7lo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Die wei\u00dfe Dampfess\u00e4ule,", "tokens": ["Die", "wei\u00b7\u00dfe", "Damp\u00b7fes\u00b7s\u00e4u\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Trag mich vor\u00fcber an Dorf und Schlo\u00df,", "tokens": ["Trag", "mich", "vor\u00b7\u00fc\u00b7ber", "an", "Dorf", "und", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Vor\u00fcber in rasender Eile!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "in", "ra\u00b7sen\u00b7der", "Ei\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach