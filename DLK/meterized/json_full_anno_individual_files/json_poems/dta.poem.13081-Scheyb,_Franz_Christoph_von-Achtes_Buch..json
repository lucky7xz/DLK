{"dta.poem.13081": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201egesezt: es sey die Kunst verschm\u00e4ht, die Stadt verheert;", "tokens": ["\u201e", "ge\u00b7sezt", ":", "es", "sey", "die", "Kunst", "ver\u00b7schm\u00e4ht", ",", "die", "Stadt", "ver\u00b7heert", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201esagt! lebte nicht, was jezt beysammen ist, verst\u00f6rt?", "tokens": ["\u201e", "sagt", "!", "leb\u00b7te", "nicht", ",", "was", "jezt", "bey\u00b7sam\u00b7men", "ist", ",", "ver\u00b7st\u00f6rt", "?"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "VVFIN", "PTKNEG", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewo wurde man den Thron der K\u00f6niginn erbauen?", "tokens": ["\u201e", "wo", "wur\u00b7de", "man", "den", "Thron", "der", "K\u00f6\u00b7ni\u00b7ginn", "er\u00b7bau\u00b7en", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PIS", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "120\u201dVielleicht in einem Thal, auf Bergen, in den Auen?", "tokens": ["\"", "Viel\u00b7leicht", "in", "ei\u00b7nem", "Thal", ",", "auf", "Ber\u00b7gen", ",", "in", "den", "Au\u00b7en", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "$,", "APPR", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewo hielt der Staat sich auf? wo wohnten Herr und Knecht?", "tokens": ["\u201e", "wo", "hielt", "der", "Staat", "sich", "auf", "?", "wo", "wohn\u00b7ten", "Herr", "und", "Knecht", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$.", "PWAV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edie Menschen hiessen nur ein irrendes Geschlecht.", "tokens": ["\u201e", "die", "Men\u00b7schen", "hies\u00b7sen", "nur", "ein", "ir\u00b7ren\u00b7des", "Ge\u00b7schlecht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eund ihr? wo n\u00e4hmet ihr der Tugend Aufenthalten?", "tokens": ["\u201e", "und", "ihr", "?", "wo", "n\u00e4h\u00b7met", "ihr", "der", "Tu\u00b7gend", "Auf\u00b7ent\u00b7hal\u00b7ten", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "$.", "PWAV", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewer wurde mehr sein Herz der Lehre nach gestallten?", "tokens": ["\u201e", "wer", "wur\u00b7de", "mehr", "sein", "Herz", "der", "Leh\u00b7re", "nach", "ge\u00b7stall\u00b7ten", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}