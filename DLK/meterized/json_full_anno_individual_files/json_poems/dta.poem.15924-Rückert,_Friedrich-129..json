{"dta.poem.15924": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "129.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer still steht, bleibt zur\u00fcck, wenn Andre vorw\u00e4rts gehn;", "tokens": ["Wer", "still", "steht", ",", "bleibt", "zu\u00b7r\u00fcck", ",", "wenn", "And\u00b7re", "vor\u00b7w\u00e4rts", "gehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "KOUS", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Ungl\u00fcck und o Gl\u00fcck! nie darfst du stille stehn.", "tokens": ["O", "Un\u00b7gl\u00fcck", "und", "o", "Gl\u00fcck", "!", "nie", "darfst", "du", "stil\u00b7le", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "FM", "NN", "$.", "ADV", "VVFIN", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was hilfts, wonach du rennst, als H\u00f6chstes zu erkennen,", "tokens": ["Was", "hilfts", ",", "wo\u00b7nach", "du", "rennst", ",", "als", "H\u00f6chs\u00b7tes", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du zugleich erkennst, es sei nicht zu errennen.", "tokens": ["Wenn", "du", "zu\u00b7gleich", "er\u00b7kennst", ",", "es", "sei", "nicht", "zu", "er\u00b7ren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der grade Weg ist nicht, nur immer gradaus gehn;", "tokens": ["Der", "gra\u00b7de", "Weg", "ist", "nicht", ",", "nur", "im\u00b7mer", "gra\u00b7daus", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "$,", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du mu\u00dft dich nach dem Ziel, das stets sich wendet, drehn.", "tokens": ["Du", "mu\u00dft", "dich", "nach", "dem", "Ziel", ",", "das", "stets", "sich", "wen\u00b7det", ",", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$,", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie gern beschied' ich mich, ich sei noch nicht am Ende,", "tokens": ["Wie", "gern", "be\u00b7schied'", "ich", "mich", ",", "ich", "sei", "noch", "nicht", "am", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ich mich nur nicht stets am Anfang wieder f\u00e4nde!", "tokens": ["Wenn", "ich", "mich", "nur", "nicht", "stets", "am", "An\u00b7fang", "wie\u00b7der", "f\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}