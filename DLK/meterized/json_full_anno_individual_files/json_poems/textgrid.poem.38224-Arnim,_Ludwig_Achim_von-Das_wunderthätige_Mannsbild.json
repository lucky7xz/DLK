{"textgrid.poem.38224": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das wunderth\u00e4tige Mannsbild", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tochter bat die Mutter sch\u00f6n,", "tokens": ["Die", "Toch\u00b7ter", "bat", "die", "Mut\u00b7ter", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie m\u00f6chte in die Kirche gehn,", "tokens": ["Sie", "m\u00f6ch\u00b7te", "in", "die", "Kir\u00b7che", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Bilder anzubeten,", "tokens": ["Die", "Bil\u00b7der", "an\u00b7zu\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn sie jezt gro\u00dfe Heiligkeit", "tokens": ["Denn", "sie", "jezt", "gro\u00b7\u00dfe", "Hei\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Inbr\u00fcnstig h\u00e4tt betreten.", "tokens": ["In\u00b7br\u00fcns\u00b7tig", "h\u00e4tt", "be\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Tochter das war gar verrucht,", "tokens": ["O", "Toch\u00b7ter", "das", "war", "gar", "ver\u00b7rucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "