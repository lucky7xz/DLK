{"dta.poem.13002": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Leanders antwort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die ausflucht ist umsonst; denn was Florette dencket,", "tokens": ["Die", "aus\u00b7flucht", "ist", "um\u00b7sonst", ";", "denn", "was", "Flo\u00b7ret\u00b7te", "den\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "VAFIN", "ADV", "$.", "KON", "PWS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zu papiere bringt, ist allezeit galant.", "tokens": ["Und", "zu", "pa\u00b7pie\u00b7re", "bringt", ",", "ist", "al\u00b7le\u00b7zeit", "ga\u00b7lant", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwar wo ein bittrer schlu\u00df die s\u00fc\u00dfe liebe kr\u00e4ncket,", "tokens": ["Zwar", "wo", "ein", "bit\u00b7trer", "schlu\u00df", "die", "s\u00fc\u00b7\u00dfe", "lie\u00b7be", "kr\u00e4n\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "ADJA", "NN", "ART", "ADJA", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bin ich allerdings zu herber angst verbannt.", "tokens": ["So", "bin", "ich", "al\u00b7ler\u00b7dings", "zu", "her\u00b7ber", "angst", "ver\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Indessen darff ich doch nicht ungehorsam bleiben,", "tokens": ["In\u00b7des\u00b7sen", "darff", "ich", "doch", "nicht", "un\u00b7ge\u00b7hor\u00b7sam", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Drum nimm nunmehr das lied von deiner hoffnung an.", "tokens": ["Drum", "nimm", "nun\u00b7mehr", "das", "lied", "von", "dei\u00b7ner", "hoff\u00b7nung", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und la\u00df den zarten kiel nur so viel antwort schreiben,", "tokens": ["Und", "la\u00df", "den", "zar\u00b7ten", "kiel", "nur", "so", "viel", "ant\u00b7wort", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "ADV", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Damit Leanders hertz auch etwas hoffen kan.", "tokens": ["Da\u00b7mit", "Le\u00b7an\u00b7ders", "hertz", "auch", "et\u00b7was", "hof\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "NN", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}