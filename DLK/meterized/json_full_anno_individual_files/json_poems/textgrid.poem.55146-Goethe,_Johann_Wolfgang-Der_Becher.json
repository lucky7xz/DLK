{"textgrid.poem.55146": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der Becher", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen wohlgeschnitzten vollen Becher", "tokens": ["Ei\u00b7nen", "wohl\u00b7ge\u00b7schnitz\u00b7ten", "vol\u00b7len", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hielt ich dr\u00fcckend in den beiden H\u00e4nden,", "tokens": ["Hielt", "ich", "dr\u00fc\u00b7ckend", "in", "den", "bei\u00b7den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sog begierig s\u00fc\u00dfen Wein vom Rande,", "tokens": ["Sog", "be\u00b7gie\u00b7rig", "s\u00fc\u00b7\u00dfen", "Wein", "vom", "Ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gram und Sorg auf einmal zu vertrinken.", "tokens": ["Gram", "und", "Sorg", "auf", "ein\u00b7mal", "zu", "ver\u00b7trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Amor trat herein und fand mich sitzen,", "tokens": ["A\u00b7mor", "trat", "her\u00b7ein", "und", "fand", "mich", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "KON", "VVFIN", "PRF", "VVINF", 