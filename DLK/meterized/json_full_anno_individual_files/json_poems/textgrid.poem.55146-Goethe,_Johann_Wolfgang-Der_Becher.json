, "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Langbewahrter Liebe Balsam koste,", "tokens": ["Lang\u00b7be\u00b7wahr\u00b7ter", "Lie\u00b7be", "Bal\u00b7sam", "kos\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Selig sprech ich dann zu meinem Geiste:", "tokens": ["Se\u00b7lig", "sprech", "ich", "dann", "zu", "mei\u00b7nem", "Geis\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Nein, ein solch Gef\u00e4\u00df hat au\u00dfer Amorn", "tokens": ["Nein", ",", "ein", "solch", "Ge\u00b7f\u00e4\u00df", "hat", "au\u00b7\u00dfer", "A\u00b7morn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "PIAT", "NN", "VAFIN", "APPR", "NE"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nie ein Gott gebildet noch besessen!", "tokens": ["Nie", "ein", "Gott", "ge\u00b7bil\u00b7det", "noch", "be\u00b7ses\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Solche Formen treibet nie Vulcanus", "tokens": ["Sol\u00b7che", "For\u00b7men", "trei\u00b7bet", "nie", "Vul\u00b7ca\u00b7nus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "NE"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mit den sinnbegabten, feinen H\u00e4mmern!", "tokens": ["Mit", "den", "sinn\u00b7be\u00b7gab\u00b7ten", ",", "fei\u00b7nen", "H\u00e4m\u00b7mern", "!"], "token_info": ["word", "word", "word", "punct", 