{"textgrid.poem.34013": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Heiligste", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Zwei sich in einander still versenken,", "tokens": ["Wenn", "Zwei", "sich", "in", "ein\u00b7an\u00b7der", "still", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "PRF", "APPR", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht durch ein schn\u00f6des Feuer aufgewiegelt,", "tokens": ["Nicht", "durch", "ein", "schn\u00f6\u00b7des", "Feu\u00b7er", "auf\u00b7ge\u00b7wie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nein, keusch in Liebe, die die Unschuld spiegelt,", "tokens": ["Nein", ",", "keusch", "in", "Lie\u00b7be", ",", "die", "die", "Un\u00b7schuld", "spie\u00b7gelt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "APPR", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schaamhaft zitternd, w\u00e4hrend sie sich tr\u00e4nken;", "tokens": ["Und", "schaam\u00b7haft", "zit\u00b7ternd", ",", "w\u00e4h\u00b7rend", "sie", "sich", "tr\u00e4n\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann m\u00fcssen beide Welten sich verschr\u00e4nken,", "tokens": ["Dann", "m\u00fcs\u00b7sen", "bei\u00b7de", "Wel\u00b7ten", "sich", "ver\u00b7schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann wird die Tiefe der Natur entriegelt,", "tokens": ["Dann", "wird", "die", "Tie\u00b7fe", "der", "Na\u00b7tur", "ent\u00b7rie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und aus dem Sch\u00f6pfungsborn, im Ich entsiegelt,", "tokens": ["Und", "aus", "dem", "Sch\u00f6p\u00b7fungs\u00b7born", ",", "im", "Ich", "ent\u00b7sie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPRART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Springt eine Welle, die die Sterne lenken.", "tokens": ["Springt", "ei\u00b7ne", "Wel\u00b7le", ",", "die", "die", "Ster\u00b7ne", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.3": {"line.1": {"text": "Was in dem Geist des Mannes, ungestaltet,", "tokens": ["Was", "in", "dem", "Geist", "des", "Man\u00b7nes", ",", "un\u00b7ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und in der Brust des Weibes, kaum empfunden,", "tokens": ["Und", "in", "der", "Brust", "des", "Wei\u00b7bes", ",", "kaum", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Sch\u00f6nstes d\u00e4mmerte, das mu\u00df sich mischen;", "tokens": ["Als", "Sch\u00f6ns\u00b7tes", "d\u00e4m\u00b7mer\u00b7te", ",", "das", "mu\u00df", "sich", "mi\u00b7schen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "PDS", "VMFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gott aber thut, die eben sich entfaltet,", "tokens": ["Gott", "a\u00b7ber", "thut", ",", "die", "e\u00b7ben", "sich", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die lichten Bilder seiner j\u00fcngsten Stunden", "tokens": ["Die", "lich\u00b7ten", "Bil\u00b7der", "sei\u00b7ner", "j\u00fcng\u00b7sten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinzu, die unverk\u00f6rperten und frischen.", "tokens": ["Hin\u00b7zu", ",", "die", "un\u00b7ver\u00b7k\u00f6r\u00b7per\u00b7ten", "und", "fri\u00b7schen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "ART", "ADJA", "KON", "VVINF", "$."], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Wenn Zwei sich in einander still versenken,", "tokens": ["Wenn", "Zwei", "sich", "in", "ein\u00b7an\u00b7der", "still", "ver\u00b7sen\u00b7ken", ",