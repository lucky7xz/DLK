d"], "pos": ["ART", "NN", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Den ganzen, luchsgleichen Blick,", "tokens": ["Den", "gan\u00b7zen", ",", "luchs\u00b7glei\u00b7chen", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Dehnt die Klauen,", "tokens": ["Dehnt", "die", "Klau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "St\u00fcrzt und schl\u00e4gt", "tokens": ["St\u00fcrzt", "und", "schl\u00e4gt"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Hinterlistig sie", "tokens": ["Hin\u00b7ter\u00b7lis\u00b7tig", "sie"], "token_info": ["word", "word"], "pos": ["ADJD", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Dir in die Schultern.", "tokens": ["Dir", "in", "die", "Schul\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Stark sind die magern Arme", "tokens": ["Stark", "sind", "die", "ma\u00b7gern", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wie Pantherarme,", "tokens": ["Wie", "Pan\u00b7ther\u00b7ar\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Er sch\u00fcttelt dich", "tokens": ["Er", "sch\u00fct\u00b7telt", "dich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und rei\u00dft dich los.", "tokens": ["Und", "rei\u00dft", "dich", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Tod ist Trennung,", "tokens": ["Tod", "ist", "Tren\u00b7nung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Dreifacher Tod", "tokens": ["Drei\u00b7fa\u00b7cher", "Tod"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Trennung ohne Hoffnung", "tokens": ["Tren\u00b7nung", "oh\u00b7ne", "Hoff\u00b7nung"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wiederzusehn.", "tokens": ["Wie\u00b7der\u00b7zu\u00b7sehn", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Gerne verlie\u00dfest du", "tokens": ["Ger\u00b7ne", "ver\u00b7lie\u00b7\u00dfest", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Dieses geha\u00dfte Land,", "tokens": ["Die\u00b7ses", "ge\u00b7ha\u00df\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Hielte dich nicht Freundschaft", "tokens": ["Hiel\u00b7te", "dich", "nicht", "Freund\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mit Blumenfesseln an mir.", "tokens": ["Mit", "Blu\u00b7men\u00b7fes\u00b7seln", "an", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Zerrei\u00df sie! Ich klage nicht.", "tokens": ["Zer\u00b7rei\u00df", "sie", "!", "Ich", "kla\u00b7ge", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Kein edler Freund", "tokens": ["Kein", "ed\u00b7ler", "Freund"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "H\u00e4lt den Mitgefangnen,", "tokens": ["H\u00e4lt", "den", "Mit\u00b7ge\u00b7fang\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der fliehn kann, zur\u00fcck.", "tokens": ["Der", "fliehn", "kann", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "$,", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Der Gedanke", "tokens": ["Der", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Von des Freundes Freiheit", "tokens": ["Von", "des", "Freun\u00b7des", "Frei\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist ihm Freiheit", "tokens": ["Ist", "ihm", "Frei\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Im Kerker.", "tokens": ["Im", "Ker\u00b7ker", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "sta