{"textgrid.poem.63865": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief in unsres Herzens Grunde ruht ein Schutt- und Tr\u00fcmmerhauf.", "tokens": ["Tief", "in", "uns\u00b7res", "Her\u00b7zens", "Grun\u00b7de", "ruht", "ein", "Schutt", "und", "Tr\u00fcm\u00b7mer\u00b7hauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "ART", "TRUNC", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aufgew\u00fchlt in dunkler Stunde dr\u00e4ngt er ungest\u00fcm herauf.", "tokens": ["Auf\u00b7ge\u00b7w\u00fchlt", "in", "dunk\u00b7ler", "Stun\u00b7de", "dr\u00e4ngt", "er", "un\u00b7ge\u00b7st\u00fcm", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Fr\u00fchzerst\u00f6rte Illusionen, Unbill, die das Blut emp\u00f6rt,", "tokens": ["Fr\u00fch\u00b7zer\u00b7st\u00f6r\u00b7te", "Il\u00b7lu\u00b7si\u00b7o\u00b7nen", ",", "Un\u00b7bill", ",", "die", "das", "Blut", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Hohn auf all das ungesunde Gaukelspiel im Weltenlauf.", "tokens": ["Hohn", "auf", "all", "das", "un\u00b7ge\u00b7sun\u00b7de", "Gau\u00b7kel\u00b7spiel", "im", "Wel\u00b7ten\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Darum, wenn sein Herz der Dichter l\u00fcften will im Selbstgespr\u00e4ch,", "tokens": ["Da\u00b7rum", ",", "wenn", "sein", "Herz", "der", "Dich\u00b7ter", "l\u00fcf\u00b7ten", "will", "im", "Selbst\u00b7ge\u00b7spr\u00e4ch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "VMFIN", "APPRART", "NN", "$,"], "meter": "----+-+-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.6": {"text": "T\u00f6nen aus dem Liedermunde Klagen oft zum Licht hinauf.", "tokens": ["T\u00f6\u00b7nen", "aus", "dem", "Lie\u00b7der\u00b7mun\u00b7de", "Kla\u00b7gen", "oft", "zum", "Licht", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Ach, sein R\u00fcgelied \u2013 nur selten, da die Welt gedankenlos,", "tokens": ["Ach", ",", "sein", "R\u00fc\u00b7ge\u00b7lied", "\u2013", "nur", "sel\u00b7ten", ",", "da", "die", "Welt", "ge\u00b7dan\u00b7ken\u00b7los", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$(", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Weckt es in der weiten Runde ein verstehend Echo auf.", "tokens": ["Weckt", "es", "in", "der", "wei\u00b7ten", "Run\u00b7de", "ein", "ver\u00b7ste\u00b7hend", "E\u00b7cho", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "wo