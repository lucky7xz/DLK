{"textgrid.poem.38533": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Damons Garten", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00fchender Garten! deine Apfelb\u00e4ume,", "tokens": ["Bl\u00fc\u00b7hen\u00b7der", "Gar\u00b7ten", "!", "dei\u00b7ne", "Ap\u00b7fel\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Deren r\u00f6thliche Loken Zefir k\u00fc\u00dfte,", "tokens": ["De\u00b7ren", "r\u00f6th\u00b7li\u00b7che", "Lo\u00b7ken", "Ze\u00b7fir", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "NE", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und die dichtverschlungenen Schattenarme", "tokens": ["Und", "die", "dicht\u00b7ver\u00b7schlun\u00b7ge\u00b7nen", "Schat\u00b7ten\u00b7ar\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Deiner Alleen,", "tokens": ["Dei\u00b7ner", "Al\u00b7leen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Wo, in geheimer D\u00e4mrung, Maienk\u00e4fer,", "tokens": ["Wo", ",", "in", "ge\u00b7hei\u00b7mer", "D\u00e4m\u00b7rung", ",", "Mai\u00b7en\u00b7k\u00e4\u00b7fer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ADJA", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Um die Stunde der Abendgloke, sumten,", "tokens": ["Um", "die", "Stun\u00b7de", "der", "A\u00b7bend\u00b7glo\u00b7ke", ",", "sum\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Turteltauben Klage begannen, werd' ich", "tokens": ["Tur\u00b7tel\u00b7tau\u00b7ben", "Kla\u00b7ge", "be\u00b7gan\u00b7nen", ",", "werd'", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "VVINF", "$,", "VAFIN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Nimmer vergessen!", "tokens": ["Nim\u00b7mer", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ach! es entrauschte manche Wonnestunde,", "tokens": ["Ach", "!", "es", "ent\u00b7rauschte", "man\u00b7che", "Won\u00b7nes\u00b7tun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Beim sokratischen Mahl, an Damons Busen,", "tokens": ["Beim", "sok\u00b7ra\u00b7ti\u00b7schen", "Mahl", ",", "an", "Da\u00b7mons", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir im s\u00fc\u00dfvertraulichen Dunkel deiner", "tokens": ["Mir", "im", "s\u00fc\u00df\u00b7ver\u00b7trau\u00b7li\u00b7chen", "Dun\u00b7kel", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN", "PPOSAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Bl\