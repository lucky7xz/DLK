{"dta.poem.11169": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An den grossen Leopold.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wolte dich die kunst in alabaster dr\u00fccken:", "tokens": ["Es", "wol\u00b7te", "dich", "die", "kunst", "in", "a\u00b7la\u00b7bas\u00b7ter", "dr\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nein! sprach die ewigkeit, es mu\u00df was bessers seyn;", "tokens": ["Nein", "!", "sprach", "die", "e\u00b7wig\u00b7keit", ",", "es", "mu\u00df", "was", "bes\u00b7sers", "seyn", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "ART", "NN", "$,", "PPER", "VMFIN", "PWS", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn unter welchem wir die goldne zeit erblicken,", "tokens": ["Denn", "un\u00b7ter", "wel\u00b7chem", "wir", "die", "gold\u00b7ne", "zeit", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des bildni\u00df mu\u00df auch wohl von purem golde seyn.", "tokens": ["Des", "bild\u00b7ni\u00df", "mu\u00df", "auch", "wohl", "von", "pu\u00b7rem", "gol\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "A