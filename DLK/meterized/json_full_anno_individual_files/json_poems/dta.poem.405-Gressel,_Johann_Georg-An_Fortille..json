{"dta.poem.405": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Fortille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wer deine Wunder-Macht/ Fortille, sehen will", "tokens": ["Wer", "dei\u00b7ne", "Wun\u00b7der\u00b7Macht", "/", "For\u00b7til\u00b7le", ",", "se\u00b7hen", "will"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "$(", "NN", "$,", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deiner Sch\u00f6nheit Krafft recht eigentlich will wissen;", "tokens": ["Und", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "Krafft", "recht", "ei\u00b7gent\u00b7lich", "will", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Derselbe stehe nur bey deinem Busen still", "tokens": ["Der\u00b7sel\u00b7be", "ste\u00b7he", "nur", "bey", "dei\u00b7nem", "Bu\u00b7sen", "still"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Alsdenn wird er mit mir mit guter Folge schliessen/", "tokens": ["Als\u00b7denn", "wird", "er", "mit", "mir", "mit", "gu\u00b7ter", "Fol\u00b7ge", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Sie setzet uns im Feur/ sie z\u00fcndt die Geister an/", "tokens": ["Sie", "set\u00b7zet", "uns", "im", "Feur", "/", "sie", "z\u00fcndt", "die", "Geis\u00b7ter", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df man dieselbe wol den ", "tokens": ["Da\u00df", "man", "die\u00b7sel\u00b7be", "wol", "den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PDAT", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}