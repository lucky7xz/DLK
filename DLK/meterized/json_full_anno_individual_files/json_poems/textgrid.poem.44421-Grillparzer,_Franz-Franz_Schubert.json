{"textgrid.poem.44421": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Franz Schubert", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schubert hei\u00df ich, Schubert bin ich", "tokens": ["Schu\u00b7bert", "hei\u00df", "ich", ",", "Schu\u00b7bert", "bin", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "NE", "VAFIN", "PPER"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und als solchen geb ich mich.", "tokens": ["Und", "als", "sol\u00b7chen", "geb", "ich", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was die Besten je geleistet,", "tokens": ["Was", "die", "Bes\u00b7ten", "je", "ge\u00b7leis\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich erkenn es, ich verehr es,", "tokens": ["Ich", "er\u00b7kenn", "es", ",", "ich", "ver\u00b7ehr", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer doch bleibts au\u00dfer mir.", "tokens": ["Im\u00b7mer", "doch", "bleibts", "au\u00b7\u00dfer", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Selbst die Kunst, die Kr\u00e4nze windet,", "tokens": ["Selbst", "die", "Kunst", ",", "die", "Kr\u00e4n\u00b7ze", "win\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Blumen sammelt, w\u00e4hlt und bindet,", "tokens": ["Blu\u00b7men", "sam\u00b7melt", ",", "w\u00e4hlt", "und", "bin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich kann ihr nur Blumen bieten,", "tokens": ["Ich", "kann", "ihr", "nur", "Blu\u00b7men", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Sichte sie und w\u00e4hlet ihr.", "tokens": ["Sich\u00b7te", "sie", "und", "w\u00e4h\u00b7let", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Lobt ihr mich, es soll mich freuen,", "tokens": ["Lobt", "ihr", "mich", ",", "es", "soll", "mich", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Schm\u00e4ht ihr mich, ich mu\u00df es dulden,", "tokens": ["Schm\u00e4ht", "ihr", "mich", ",", "ich", "mu\u00df", "es", "dul\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Schubert hei\u00df ich, Schubert bin ich,", "tokens": ["Schu\u00b7bert", "hei\u00df", "ich", ",", "Schu\u00b7bert", "bin", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NE", "VAFIN", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Mag nicht hindern, kann nicht laden,", "tokens": ["Mag", "nicht", "hin\u00b7dern", ",", "kann", "nicht", "la\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVINF", "$,", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Geht ihr gern auf meinen Pfaden,", "tokens": ["Geht", "ihr", "gern", "auf", "mei\u00b7nen", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Nun wohlan, so folget mir!", "tokens": ["Nun", "wo\u00b7hlan", ",", "so", "fol\u00b7get", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schubert hei\u00df ich, Schubert bin ich", "tokens": ["Schu\u00b7bert", "hei\u00df", "ich", ",", "Schu\u00b7bert", "bin", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "NE", "VAFIN", "PPER"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und als solchen geb ich mich.", "tokens": ["Und", "als", "sol\u00b7chen", "geb", "ich", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was die Besten je geleistet,", "tokens": ["Wa