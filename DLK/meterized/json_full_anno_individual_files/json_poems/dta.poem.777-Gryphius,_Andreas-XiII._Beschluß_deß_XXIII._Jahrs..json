{"dta.poem.777": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XiII.  \n Beschlu\u00df de\u00df  XXIII.  Jahrs.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Jn Angst/ in tr\u00fcber noth/ in hoffnung/ schmertz vnd pein ", "tokens": ["In", "Angst", "/", "in", "tr\u00fc\u00b7ber", "noth", "/", "in", "hoff\u00b7nung", "/", "schmertz", "vnd", "pein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "APPR", "ADJA", "NN", "$(", "APPR", "NN", "$(", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In sorgen vnd in ach/ hab ich di\u00df kurtze Leben/", "tokens": ["In", "sor\u00b7gen", "vnd", "in", "ach", "/", "hab", "ich", "di\u00df", "kurt\u00b7ze", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "XY", "$(", "VAFIN", "PPER", "PDS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo fern es leben hei\u00dft/ der Eitelkeit gegeben.", "tokens": ["Wo", "fern", "es", "le\u00b7ben", "hei\u00dft", "/", "der", "Ei\u00b7tel\u00b7keit", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVINF", "VVFIN", "$(", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hab ich von ach vnd furcht je ledig m\u00f6gen seyn?", "tokens": ["Hab", "ich", "von", "ach", "vnd", "furcht", "je", "le\u00b7dig", "m\u00f6\u00b7gen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "XY", "KON", "NN", "ADV", "ADJD", "VMFIN", "VAINF", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ade Welt! Gunst Ade! jetzt bin ich nicht mehr dein.", "tokens": ["A\u00b7de", "Welt", "!", "Gunst", "A\u00b7de", "!", "jetzt", "bin", "ich", "nicht", "mehr", "dein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "NN", "$.", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ich wil den freyen Geist/ nun wehmut frey/ erheben/", "tokens": ["Ich", "wil", "den", "frey\u00b7en", "Geist", "/", "nun", "weh\u00b7mut", "frey", "/", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "$(", "ADV", "ADJD", "ADJD", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wil mit freyem Sinn/ weit vber alle schweben.", "tokens": ["Ich", "wil", "mit", "frey\u00b7em", "Sinn", "/", "weit", "vber", "al\u00b7le", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "$(", "ADJD", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die str\u00e4nge dinstbarkeit schleu\u00dft in viel ketten eyn!", "tokens": ["Die", "str\u00e4n\u00b7ge", "dinst\u00b7bar\u00b7keit", "schleu\u00dft", "in", "viel", "ket\u00b7ten", "eyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Mich soll kein glimpf noch schimpff/ kein weh vnd wohl mehr", "tokens": ["Mich", "soll", "kein", "glimpf", "noch", "schimpff", "/", "kein", "weh", "vnd", "wohl", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "VVFIN", "$(", "PIAT", "ADJD", "KON", "ADV", "ADV"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "binden/", "tokens": ["bin\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Man soll fortan mich frey von zweifel stricken finden/", "tokens": ["Man", "soll", "for\u00b7tan", "mich", "frey", "von", "zwei\u00b7fel", "stri\u00b7cken", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "PPER", "ADJD", "APPR", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich mag nicht toller lust/ mehr zu gebotte stehn.", "tokens": ["Ich", "mag", "nicht", "tol\u00b7ler", "lust", "/", "mehr", "zu", "ge\u00b7bot\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJA", "NN", "$(", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gib allzeit freyer GOTT der du dich frey heist ehren:", "tokens": ["Gib", "all\u00b7zeit", "frey\u00b7er", "GoTT", "der", "du", "dich", "frey", "heist", "eh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJA", "NN", "ART", "PPER", "PRF", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich dich m\u00f6g allzeit mit freyen O hren h\u00f6ren/", "tokens": ["Da\u00df", "ich", "dich", "m\u00f6g", "all\u00b7zeit", "mit", "frey\u00b7en", "O", "hren", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "ADV", "APPR", "ADJA", "NE", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bi\u00df ich durch dich/ zu dir/ werd\u2019 in die Freystatt gehn.", "tokens": ["Bi\u00df", "ich", "durch", "dich", "/", "zu", "dir", "/", "werd'", "in", "die", "Freys\u00b7tatt", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$(", "APPR", "PPER", "$(", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}