{"dta.poem.3925": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 3.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Auch kostets manchen viel, wenn sie Gevattern", "tokens": ["Auch", "kos\u00b7tets", "man\u00b7chen", "viel", ",", "wenn", "sie", "Ge\u00b7vat\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "PIS", "$,", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "stehen,", "tokens": ["ste\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Geburths- und Nahmens-Tag wohl zwey, dreymal", "tokens": ["Ge\u00b7burths", "und", "Nah\u00b7mens\u00b7Tag", "wohl", "zwey", ",", "drey\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["TRUNC", "KON", "NN", "ADV", "CARD", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "begehen:", "tokens": ["be\u00b7ge\u00b7hen", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Erlebt nun einst der Man\u0303 auch ein dergleichen Spiel,", "tokens": ["Er\u00b7lebt", "nun", "einst", "der", "Ma\u00f1", "auch", "ein", "derg\u00b7lei\u00b7chen", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "ADV", "ART", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So schencke sie als Frau, ihm wiederum so viel.", "tokens": ["So", "schen\u00b7cke", "sie", "als", "Frau", ",", "ihm", "wie\u00b7de\u00b7rum", "so", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN", "$,", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}