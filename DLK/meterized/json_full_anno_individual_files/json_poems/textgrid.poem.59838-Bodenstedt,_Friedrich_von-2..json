{"textgrid.poem.59838": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sucht der echte Weise,", "tokens": ["Es", "sucht", "der", "ech\u00b7te", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df er das Rechte finde:", "tokens": ["Da\u00df", "er", "das", "Rech\u00b7te", "fin\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jung wird er nicht zum Greife,", "tokens": ["Jung", "wird", "er", "nicht", "zum", "Grei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Alt wird er nicht zum Kinde!", "tokens": ["Alt", "wird", "er", "nicht", "zum", "Kin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Winter treibt keine Bl\u00fcte,", "tokens": ["Der", "Win\u00b7ter", "treibt", "kei\u00b7ne", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Sommer treibt kein Eis \u2013", "tokens": ["Der", "Som\u00b7mer", "treibt", "kein", "Eis", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["