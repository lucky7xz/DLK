{"dta.poem.10640": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Auff die Melodey: Kehr vmb mein Seel/ rc.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt blicken au\u00df des Himmels Saal", "tokens": ["Jetzt", "bli\u00b7cken", "au\u00df", "des", "Him\u00b7mels", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die g\u00fcldne Sternen allzumahl/", "tokens": ["Die", "g\u00fcld\u00b7ne", "Ster\u00b7nen", "all\u00b7zu\u00b7mahl", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin ohn Hoffnung gantz allein/", "tokens": ["Ich", "bin", "ohn", "Hoff\u00b7nung", "gantz", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wach/ vnd andre schlaffen ein.", "tokens": ["Ich", "wach", "/", "vnd", "and\u00b7re", "schlaf\u00b7fen", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Jungfraw/ lieget in der Ruh/", "tokens": ["Ihr", "Jung\u00b7fraw", "/", "lie\u00b7get", "in", "der", "Ruh", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd habet ewer \u00e4uglein zu/", "tokens": ["Vnd", "ha\u00b7bet", "e\u00b7wer", "\u00e4ug\u00b7lein", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJD", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr blaset au\u00df dem rothen Mundt", "tokens": ["Ihr", "bla\u00b7set", "au\u00df", "dem", "ro\u00b7then", "Mundt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das s\u00fcsse Gifft/ so mich verwundt.", "tokens": ["Das", "s\u00fcs\u00b7se", "Gifft", "/", "so", "mich", "ver\u00b7wundt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr dencket nicht an meine Noth/", "tokens": ["Ihr", "den\u00b7cket", "nicht", "an", "mei\u00b7ne", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Noch an den starcken Liebes-Gott/", "tokens": ["Noch", "an", "den", "star\u00b7cken", "Lie\u00b7bes\u00b7Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der mein betr\u00fcbt Gem\u00fcth gebracht", "tokens": ["Der", "mein", "be\u00b7tr\u00fcbt", "Ge\u00b7m\u00fcth", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "VVFIN", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In ewer Hand vnd grosse Macht.", "tokens": ["In