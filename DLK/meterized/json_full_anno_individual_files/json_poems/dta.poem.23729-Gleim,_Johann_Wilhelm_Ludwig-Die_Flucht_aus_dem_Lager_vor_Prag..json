{"dta.poem.23729": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Flucht aus dem Lager  \n vor Prag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ein Heer die letzten Kr&#228;fte", "tokens": ["Als", "ein", "Heer", "die", "letz\u00b7ten", "Kr", "&#228;", "fte"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "$(", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Auf dem Ziskaberge wagte,", "tokens": ["Auf", "dem", "Zis\u00b7ka\u00b7ber\u00b7ge", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und noch Bomben oder Kugeln", "tokens": ["Und", "noch", "Bom\u00b7ben", "o\u00b7der", "Ku\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem nahen Lager tobten;", "tokens": ["In", "dem", "na\u00b7hen", "La\u00b7ger", "tob\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als ich noch der Kugel fluchte,", "tokens": ["Als", "ich", "noch", "der", "Ku\u00b7gel", "fluch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die mir meinen Prinzen raubte,", "tokens": ["Die", "mir", "mei\u00b7nen", "Prin\u00b7zen", "raub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kam, mit schnellen Taubenfl\u00fcgeln,", "tokens": ["Kam", ",", "mit", "schnel\u00b7len", "Tau\u00b7ben\u00b7fl\u00fc\u00b7geln", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Amor in mein Zelt geflogen.", "tokens": ["A\u00b7mor", "in", "mein", "Zelt", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dreister, sprach der Gott der Liebe,", "tokens": ["Dreis\u00b7ter", ",", "sprach", "der", "Gott", "der", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dreister, kanst du hier verweilen?", "tokens": ["Dreis\u00b7ter", ",", "kanst", "du", "hier", "ver\u00b7wei\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Hier, wo die verwegnen Menschen", "tokens": ["Hier", ",", "wo", "die", "ver\u00b7weg\u00b7nen", "Men\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "T\u00f6dten, und sich t\u00f6dten lassen;", "tokens": ["T\u00f6d\u00b7ten", ",", "und", "sich", "t\u00f6d\u00b7ten", "las\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PRF", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Hier, wo die erz\u00fcrnten G\u00f6tter,", "tokens": ["Hier", ",", "wo", "die", "er\u00b7z\u00fcrn\u00b7ten", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.14": {"text": "Auch die besten Helden t\u00f6dten?", "tokens": ["Auch", "die", "bes\u00b7ten", "Hel\u00b7den", "t\u00f6d\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Ist dein Prinz nicht schon get\u00f6dtet?", "tokens": ["Ist", "dein", "Prinz", "nicht", "schon", "ge\u00b7t\u00f6d\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Falscher, geh, dein M\u00e4dchen weinet,", "tokens": ["Fal\u00b7scher", ",", "geh", ",", "dein", "M\u00e4d\u00b7chen", "wei\u00b7net", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Geh, eh dich die Kugeln t\u00f6dten,", "tokens": ["Geh", ",", "eh", "dich", "die", "Ku\u00b7geln", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Geh, was machst du bei den Helden,", "tokens": ["Geh", ",", "was", "machst", "du", "bei", "den", "Hel\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Geh, ich kan nicht l\u00e4nger sehen,", "tokens": ["Geh", ",", "ich", "kan", "nicht", "l\u00e4n\u00b7ger", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Wie dein armes M\u00e4dchen weinet!", "tokens": ["Wie", "dein", "ar\u00b7mes", "M\u00e4d\u00b7chen", "wei\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}