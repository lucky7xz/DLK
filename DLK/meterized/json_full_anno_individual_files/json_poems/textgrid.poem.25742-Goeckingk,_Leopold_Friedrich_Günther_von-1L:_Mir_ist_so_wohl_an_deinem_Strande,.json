{"textgrid.poem.25742": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mir ist so wohl an deinem Strande,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir ist so wohl an deinem Strande,", "tokens": ["Mir", "ist", "so", "wohl", "an", "dei\u00b7nem", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie nie mir noch am Wasser war.", "tokens": ["Wie", "nie", "mir", "noch", "am", "Was\u00b7ser", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O h\u00e4tt' ich dich in meinem Lande,", "tokens": ["O", "h\u00e4tt'", "ich", "dich", "in", "mei\u00b7nem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du fesseltest mich immerdar.", "tokens": ["Du", "fes\u00b7sel\u00b7test", "mich", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und immer dich, du sch\u00f6ner Spiegel,", "tokens": ["Und", "im\u00b7mer", "dich", ",", "du", "sch\u00f6\u00b7ner", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus meinem Fenster anzuschaun,", "tokens": ["Aus", "mei\u00b7nem", "Fens\u00b7ter", "an\u00b7zu\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wollt' ich ein Haus auf diesem H\u00fcgel,", "tokens": ["Wollt'", "ich", "ein", "Haus", "auf", "die\u00b7sem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "W\u00e4r's auch nur Eins von Rasen, baun.", "tokens": ["W\u00e4r's", "auch", "nur", "Eins", "von", "Ra\u00b7sen", ",", "baun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "APPR", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Erhabner sieht das Meer, von Zonen", "tokens": ["Er\u00b7hab\u00b7ner", "sieht", "das", "Meer", ",", "von", "Zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Zu Zonen wallend, freilich aus;", "tokens": ["Zu", "Zo\u00b7nen", "wal\u00b7lend", ",", "frei\u00b7lich", "aus", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch m\u00f6cht' ich nicht am Meere wohnen,", "tokens": ["Doch", "m\u00f6cht'", "ich", "nicht", "am", "Mee\u00b7re", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und schenkt' ein F\u00fcrst mir dort ein Haus.", "tokens": ["Und", "schenkt'", "ein", "F\u00fcrst", "mir", "dort", "ein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mich weckte jeder Sturm; ich st\u00fcnde", "tokens": ["Mich", "weck\u00b7te", "je\u00b7der", "Sturm", ";", "ich", "st\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die Nacht mit wilder Phantasei", "tokens": ["Die", "Nacht", "mit", "wil\u00b7der", "Phan\u00b7ta\u00b7sei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Am Strand', und im Geheul' der Winde", "tokens": ["Am", "Strand'", ",", "und", "im", "Ge\u00b7heul'", "der", "Win\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "H\u00f6rt' ich der Wracke H\u00fclfsgeschrei.", "tokens": ["H\u00f6rt'", "ich", "der", "Wra\u00b7cke", "H\u00fclfs\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und k\u00f6nnte nichts, als um Verschonen", "tokens": ["Und", "k\u00f6nn\u00b7te", "nichts", ",", "als", "um", "Ver\u00b7scho\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-