 "measure": "iambic.tri"}, "line.2": {"text": "Ihr freudiges Gewimmel", "tokens": ["Ihr", "freu\u00b7di\u00b7ges", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Aufgang bis zum Niedergang:", "tokens": ["Vom", "Auf\u00b7gang", "bis", "zum", "Nie\u00b7der\u00b7gang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn seine Kinder sammeln,", "tokens": ["Denn", "sei\u00b7ne", "Kin\u00b7der", "sam\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ihr vereintes Stammeln", "tokens": ["Und", "ihr", "ver\u00b7ein\u00b7tes", "Stam\u00b7meln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "T\u00f6nt ihm in tausend Sprachen Dank.", "tokens": ["T\u00f6nt", "ihm", "in", "tau\u00b7send", "Spra\u00b7chen", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Lobsinget seinem Namen,", "tokens": ["Lob\u00b7sin\u00b7get", "sei\u00b7nem", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und strebt ihm nachzuahmen,", "tokens": ["Und", "strebt", "ihm", "nach\u00b7zu\u00b7ah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihm, dessen Gnad' 