{"textgrid.poem.67167": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jetzt komme, Feuer!", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt komme, Feuer!", "tokens": ["Jetzt", "kom\u00b7me", ",", "Feu\u00b7er", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Begierig sind wir,", "tokens": ["Be\u00b7gie\u00b7rig", "sind", "wir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zu schauen den Tag,", "tokens": ["Zu", "schau\u00b7en", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und wenn die Pr\u00fcfung", "tokens": ["Und", "wenn", "die", "Pr\u00fc\u00b7fung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ist durch die Knie gegangen,", "tokens": ["Ist", "durch", "die", "Knie", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mag einer sp\u00fcren das Waldgeschrei.", "tokens": ["Mag", "ei\u00b7ner", "sp\u00fc\u00b7ren", "das", "Wald\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wir singen aber vom Indus her", "tokens": ["Wir", "sin\u00b7gen", "a\u00b7ber", "vom", "In\u00b7dus", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NE", "APZR"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Fernangekommen und", "tokens": ["Fer\u00b7nan\u00b7ge\u00b7kom\u00b7men", "und"], "token_info": ["word", "word"], "pos": ["NN", "KON"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Vom Alpheus, lange haben", "tokens": ["Vom", "Al\u00b7pheus", ",", "lan\u00b7ge", "ha\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Das Schickliche wir gesucht,", "tokens": ["Das", "Schick\u00b7li\u00b7che", "wir", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Nicht ohne Schwingen mag", "tokens": ["Nicht", "oh\u00b7ne", "Schwin\u00b7gen", "mag"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Zum N\u00e4chsten einer greifen", "tokens": ["Zum", "N\u00e4chs\u00b7ten", "ei\u00b7ner", "grei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Geradezu", "tokens": ["Ge\u00b7ra\u00b7de\u00b7zu"], "token_info": ["word"], "pos": ["NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Und kommen auf die andere Seite.", "tokens": ["Und", "kom\u00b7men", "auf", "die", "an\u00b7de\u00b7re", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Hier aber wollen wir bauen.", "tokens": ["Hier", "a\u00b7ber", "wol\u00b7len", "wir", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Denn Str\u00f6me machen urbar", "tokens": ["Denn", "Str\u00f6\u00b7me", "ma\u00b7chen", "ur\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVINF", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Das Land. Wenn n\u00e4mlich Kr\u00e4uter wachsen", "tokens": ["Das", "Land", ".", "Wenn", "n\u00e4m\u00b7lich", "Kr\u00e4u\u00b7ter", "wach\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KOUS", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und an denselben gehn", "tokens": ["Und", "an", "den\u00b7sel\u00b7ben", "gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDS", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Im Sommer zu trinken die Tiere,", "tokens": ["Im", "Som\u00b7mer", "zu", "trin\u00b7ken", "die", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.20": {"text": "So gehn auch Menschen daran.", "tokens": ["So", "gehn", "auch", "Men\u00b7schen", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Man nennet aber diesen den Ister.", "tokens": ["Man", "nen\u00b7net", "a\u00b7ber", "die\u00b7sen", "den", "Is\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PDAT", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sch\u00f6n wohnt er. Es brennet der S\u00e4ulen Laub,", "tokens": ["Sch\u00f6n", "wohnt", "er", ".", "Es", "bren\u00b7net", "der", "S\u00e4u\u00b7len", "Laub", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und reget sich. Wild stehn", "tokens": ["Und", "re\u00b7get", "sich", ".", "Wild", "stehn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "$.", "NE", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sie aufgerichtet, untereinander; darob", "tokens": ["Sie", "auf\u00b7ge\u00b7rich\u00b7tet", ",", "un\u00b7ter\u00b7ein\u00b7an\u00b7der", ";", "da\u00b7rob"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVPP", "$,", "ADV", "$.", "PAV"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ein zweites Ma\u00df, springt vor", "tokens": ["Ein", "zwei\u00b7tes", "Ma\u00df", ",", "springt", "vor"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Von Felsen das Dach. So wundert", "tokens": ["Von", "Fel\u00b7sen", "das", "Dach", ".", "So", "wun\u00b7dert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Mich nicht, da\u00df er", "tokens": ["Mich", "nicht", ",", "da\u00df", "er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKNEG", "$,", "KOUS", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Den Herkules zu Gaste geladen,", "tokens": ["Den", "Her\u00b7ku\u00b7les", "zu", "Gas\u00b7te", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Ferngl\u00e4nzend, am Olympos drunten,", "tokens": ["Fern\u00b7gl\u00e4n\u00b7zend", ",", "am", "O\u00b7lym\u00b7pos", "drun\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da der, sich Schatten zu suchen", "tokens": ["Da", "der", ",", "sich", "Schat\u00b7ten", "zu", "su\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "$,", "PRF", "NN", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Vom hei\u00dfen Isthmos kam,", "tokens": ["Vom", "hei\u00b7\u00dfen", "Isth\u00b7mos", "kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Denn voll des Mutes waren", "tokens": ["Denn", "voll", "des", "Mu\u00b7tes", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Daselbst sie, es bedarf aber, der Geister wegen,", "tokens": ["Da\u00b7selbst", "sie", ",", "es", "be\u00b7darf", "a\u00b7ber", ",", "der", "Geis\u00b7ter", "we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "$,", "PPER", "VVFIN", "ADV", "$,", "ART", "NN", "APPR", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Der K\u00fchlung auch. Darum zog jener lieber", "tokens": ["Der", "K\u00fch\u00b7lung", "auch", ".", "Da\u00b7rum", "zog", "je\u00b7ner", "lie\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "PAV", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "An die Wasserquellen hieher und gelben Ufer,", "tokens": ["An", "die", "Was\u00b7ser\u00b7quel\u00b7len", "hie\u00b7her", "und", "gel\u00b7ben", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Hoch duftend oben, und schwarz", "tokens": ["Hoch", "duf\u00b7tend", "o\u00b7ben", ",", "und", "schwarz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADJD", "ADV", "$,", "KON", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Vom Fichtenwald, wo in den Tiefen", "tokens": ["Vom", "Fich\u00b7ten\u00b7wald", ",", "wo", "in", "den", "Tie\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ein J\u00e4ger gern lustwandelt", "tokens": ["Ein", "J\u00e4\u00b7ger", "gern", "lust\u00b7wan\u00b7delt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Mittags, und Wachstum h\u00f6rbar ist", "tokens": ["Mit\u00b7tags", ",", "und", "Wachs\u00b7tum", "h\u00f6r\u00b7bar", "ist"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KON", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "An harzigen B\u00e4umen des Isters,", "tokens": ["An", "har\u00b7zi\u00b7gen", "B\u00e4u\u00b7men", "des", "Is\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Der scheinet aber fast", "tokens": ["Der", "schei\u00b7net", "a\u00b7ber", "fast"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "R\u00fcckw\u00e4rts zu gehen und", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "zu", "ge\u00b7hen", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "KON"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich mein, er m\u00fcsse kommen", "tokens": ["Ich", "mein", ",", "er", "m\u00fcs\u00b7se", "kom\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "$,", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Osten.", "tokens": ["Von", "Os\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Vieles w\u00e4re", "tokens": ["Vie\u00b7les", "w\u00e4\u00b7re"], "token_info": ["word", "word"], "pos": ["PIS", "VAFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Zu sagen davon. Und warum h\u00e4ngt er", "tokens": ["Zu", "sa\u00b7gen", "da\u00b7von", ".", "Und", "wa\u00b7rum", "h\u00e4ngt", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PAV", "$.", "KON", "PWAV", "VVFIN", "PPER"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "An den Bergen gerad? Der andre,", "tokens": ["An", "den", "Ber\u00b7gen", "ge\u00b7rad", "?", "Der", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$.", "ART", "PIS", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Der Rhein, ist seitw\u00e4rts", "tokens": ["Der", "Rhein", ",", "ist", "seit\u00b7w\u00e4rts"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "$,", "VAFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Hinweggegangen. Umsonst nicht gehn", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7gan\u00b7gen", ".", "Um\u00b7sonst", "nicht", "gehn"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Im Trocknen die Str\u00f6me. Aber wie? Ein Zeichen braucht es,", "tokens": ["Im", "Trock\u00b7nen", "die", "Str\u00f6\u00b7me", ".", "A\u00b7ber", "wie", "?", "Ein", "Zei\u00b7chen", "braucht", "es", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "KON", "PWAV", "$.", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Nichts anderes, schlecht und recht, damit es Sonn", "tokens": ["Nichts", "an\u00b7de\u00b7res", ",", "schlecht", "und", "recht", ",", "da\u00b7mit", "es", "Sonn"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "PIS", "$,", "ADJD", "KON", "ADJD", "$,", "KOUS", "PPER", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und Mond trag im Gem\u00fct, untrennbar,", "tokens": ["Und", "Mond", "trag", "im", "Ge\u00b7m\u00fct", ",", "un\u00b7trenn\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und fortgeh, Tag und Nacht auch, und", "tokens": ["Und", "fort\u00b7geh", ",", "Tag", "und", "Nacht", "auch", ",", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "NN", "KON", "NN", "ADV", "$,", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Himmlischen warm sich f\u00fchlen aneinander.", "tokens": ["Die", "Himm\u00b7li\u00b7schen", "warm", "sich", "f\u00fch\u00b7len", "an\u00b7ein\u00b7an\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PRF", "VVFIN", "ADV", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Darum sind jene auch", "tokens": ["Da\u00b7rum", "sind", "je\u00b7ne", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PDS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Die Freude des H\u00f6chsten. Denn wie k\u00e4m er", "tokens": ["Die", "Freu\u00b7de", "des", "H\u00f6chs\u00b7ten", ".", "Denn", "wie", "k\u00e4m", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "KON", "PWAV", "VVFIN", "PPER"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Herunter? Und wie Hertha gr\u00fcn,", "tokens": ["Her\u00b7un\u00b7ter", "?", "Und", "wie", "Her\u00b7tha", "gr\u00fcn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PWAV", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sind sie die Kinder des Himmels. Aber allzugedultig", "tokens": ["Sind", "sie", "die", "Kin\u00b7der", "des", "Him\u00b7mels", ".", "A\u00b7ber", "all\u00b7zu\u00b7ge\u00b7dul\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$.", "KON", "ADJD"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.19": {"text": "Scheint der mir, nicht", "tokens": ["Scheint", "der", "mir", ",", "nicht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "PPER", "$,", "PTKNEG"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Freier, und fast zu spotten. N\u00e4mlich wenn", "tokens": ["Frei\u00b7er", ",", "und", "fast", "zu", "spot\u00b7ten", ".", "N\u00e4m\u00b7lich", "wenn"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "ADV", "PTKZU", "VVINF", "$.", "ADV", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Angehen soll der Tag", "tokens": ["An\u00b7ge\u00b7hen", "soll", "der", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "In der Jugend, wo er zu wachsen", "tokens": ["In", "der", "Ju\u00b7gend", ",", "wo", "er", "zu", "wach\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "PTKZU", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Anf\u00e4ngt, es treibet ein anderer da", "tokens": ["An\u00b7f\u00e4ngt", ",", "es", "trei\u00b7bet", "ein", "an\u00b7de\u00b7rer", "da"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hoch schon die Pracht, und F\u00fcllen gleich", "tokens": ["Hoch", "schon", "die", "Pracht", ",", "und", "F\u00fcl\u00b7len", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "ART", "NN", "$,", "KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In den Zaum knirscht er, und weithin h\u00f6ren", "tokens": ["In", "den", "Zaum", "knirscht", "er", ",", "und", "weit\u00b7hin", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "KON", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Das Treiben die L\u00fcfte,", "tokens": ["Das", "Trei\u00b7ben", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ist der zufrieden;", "tokens": ["Ist", "der", "zu\u00b7frie\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Es brauchet aber Stiche der Fels", "tokens": ["Es", "brau\u00b7chet", "a\u00b7ber", "Sti\u00b7che", "der", "Fels"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Und Furchen die Erd,", "tokens": ["Und", "Fur\u00b7chen", "die", "Erd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Unwirtbar w\u00e4r es, ohne Weile;", "tokens": ["Un\u00b7wirt\u00b7bar", "w\u00e4r", "es", ",", "oh\u00b7ne", "Wei\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Was aber jener tuet, der Strom,", "tokens": ["Was", "a\u00b7ber", "je\u00b7ner", "tu\u00b7et", ",", "der", "Strom", ","],