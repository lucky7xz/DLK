{"textgrid.poem.65207": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ein Andrer", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du wandelst auf der Prager Br\u00fccken,", "tokens": ["Wenn", "du", "wan\u00b7delst", "auf", "der", "Pra\u00b7ger", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Thut vor dir Sankt Nepomuk sich b\u00fccken,", "tokens": ["Thut", "vor", "dir", "Sankt", "Ne\u00b7po\u00b7muk", "sich", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVFIN", "NE", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Arme hebt er auf zum Segen", "tokens": ["Und", "die", "Ar\u00b7me", "hebt", "er", "auf", "zum", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deiner schwarzen Schelmenaugen wegen.", "tokens": ["Dei\u00b7ner", "schwar\u00b7zen", "Schel\u00b7men\u00b7au\u00b7gen", "we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, wie soll man heut' ein Heil'ger werden,", "tokens": ["Ach", ",", "wie", "soll", "man", "heut'", "ein", "Heil'\u00b7ger", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWA