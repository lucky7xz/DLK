{"textgrid.poem.42162": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Morgenstimmung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leise schleich ich wie auf Eiern", "tokens": ["Lei\u00b7se", "schleich", "ich", "wie", "auf", "Ei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich aus Liebchens Paradies,", "tokens": ["Mich", "aus", "Lieb\u00b7chens", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich hinter dichten Schleiern", "tokens": ["Wo", "ich", "hin\u00b7ter", "dich\u00b7ten", "Schlei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine besten Kr\u00e4fte lie\u00df.", "tokens": ["Mei\u00b7ne", "bes\u00b7ten", "Kr\u00e4f\u00b7te", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Traurig spiegelt sich der bleiche", "tokens": ["Trau\u00b7rig", "spie\u00b7gelt", "sich", "der", "blei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mond in meinem alten Frack;", "tokens": ["Mond", "in", "mei\u00b7nem", "al\u00b7ten", "Frack", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+