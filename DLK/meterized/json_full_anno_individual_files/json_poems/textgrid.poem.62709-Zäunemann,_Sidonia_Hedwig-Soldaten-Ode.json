 ["Was", "mit", "sei\u00b7nem", "Na\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und Geschmeide ausger\u00fcst,", "tokens": ["Und", "Ge\u00b7schmei\u00b7de", "aus\u00b7ge\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kan mein Herze r\u00fchren,", "tokens": ["Kan", "mein", "Her\u00b7ze", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und zur Freude f\u00fchren.", "tokens": ["Und", "zur", "Freu\u00b7de", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Alle meine Glieder", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Glie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sind des Mavors Eigenthum,", "tokens": ["Sind", "des", "Ma\u00b7vors", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor ihm fall ich nieder;", "tokens": ["Vor", "ihm", "fall", "ich", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Seine Waffen sind mein Ruhm;", "tokens": ["Sei\u00b7ne", "Waf\u00b7fen", "sind", "mein", "Ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "P