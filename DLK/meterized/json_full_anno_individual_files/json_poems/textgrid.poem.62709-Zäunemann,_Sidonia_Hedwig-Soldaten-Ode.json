{"textgrid.poem.62709": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Soldaten-Ode", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was kan mich vergn\u00fcgen?", "tokens": ["Was", "kan", "mich", "ver\u00b7gn\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nichts als Mavors blankes Schwerdt.", "tokens": ["Nichts", "als", "Ma\u00b7vors", "blan\u00b7kes", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter ihm zu siegen,", "tokens": ["Un\u00b7ter", "ihm", "zu", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist was meine Seel begehrt.", "tokens": ["Ist", "was", "mei\u00b7ne", "Seel", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, ich falle diesen", "tokens": ["Ja", ",", "ich", "fal\u00b7le", "die\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PDAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Andachts-voll zu F\u00fcssen.", "tokens": ["An\u00b7dachts\u00b7voll", "zu", "F\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Andre Himmels-F\u00fcrsten,", "tokens": ["And\u00b7re", "Him\u00b7mels\u00b7F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und ihr Dienst wird nicht geacht;", "tokens": ["Und", "ihr", "Dienst", "wird", "nicht", "ge\u00b7acht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Mavors d\u00fcrsten,", "tokens": ["Nach", "dem", "Ma\u00b7vors", "d\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist mein Wunsch bey Tag und Nacht.", "tokens": ["Ist", "mein", "Wunsch", "bey", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Schaar zu mehren,", "tokens": ["Sei\u00b7ne", "Schaar", "zu", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Will ich mich verschw\u00f6ren.", "tokens": ["Will", "ich", "mich", "ver\u00b7schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "In das Feld zu ziehen,", "tokens": ["In", "das", "Feld", "zu", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und durch meinen Helden-Muth", "tokens": ["Und", "durch", "mei\u00b7nen", "Hel\u00b7den\u00b7Muth"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich um Ruhm bem\u00fchen,", "tokens": ["Mich", "um", "Ruhm", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Halt ich vor mein h\u00f6chstes Gut;", "tokens": ["Halt", "ich", "vor", "mein", "h\u00f6chs\u00b7tes", "Gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um den Feind zu d\u00e4mpfen.", "tokens": ["Um", "den", "Feind", "zu", "d\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Will ich tapfer k\u00e4mpfen.", "tokens": ["Will", "ich", "tap\u00b7fer", "k\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "St\u00e4dte zu belagern,", "tokens": ["St\u00e4d\u00b7te", "zu", "be\u00b7la\u00b7gern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist die Freude meiner Brust,", "tokens": ["Ist", "die", "Freu\u00b7de", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbe auszumagern,", "tokens": ["Sel\u00b7be", "aus\u00b7zu\u00b7ma\u00b7gern", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVIZU", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist und bleibet meine Lust:", "tokens": ["Ist", "und", "blei\u00b7bet", "mei\u00b7ne", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die\u00df wei\u00dft im Kriege", "tokens": ["Denn", "die\u00df", "wei\u00dft", "im", "Krie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mir den Weg zum Siege.", "tokens": ["Mir", "den", "Weg", "zum", "Sie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Was kan mich erquicken?", "tokens": ["Was", "kan", "mich", "er\u00b7qui\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Meines Feindes Todtes-Schwei\u00df:", "tokens": ["Mei\u00b7nes", "Fein\u00b7des", "Tod\u00b7tes\u00b7Schwei\u00df", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihn im Blut ersticken", "tokens": ["Ihn", "im", "Blut", "er\u00b7sti\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist mein Purpur, Schmuck und Prei\u00df:", "tokens": ["Ist", "mein", "Pur\u00b7pur", ",", "Schmuck", "und", "Prei\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Raub der Fahne,", "tokens": ["Und", "der", "Raub", "der", "Fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Bringt zur Ehren-Bahne.", "tokens": ["Bringt", "zur", "Eh\u00b7ren\u00b7Bah\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Sebel, Spie\u00df und Degen", "tokens": ["Se\u00b7bel", ",", "Spie\u00df", "und", "De\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ganz entbl\u00f6\u00dfet anzusehn,", "tokens": ["Ganz", "ent\u00b7bl\u00f6\u00b7\u00dfet", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVIZU", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist mir wohl gelegen,", "tokens": ["Ist", "mir", "wohl", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und wird stets mit Lust geschehn.", "tokens": ["Und", "wird", "stets", "mit", "Lust", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Pulver und Canonen", "tokens": ["Pul\u00b7ver", "und", "Ca\u00b7no\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Nenn ich meine Cronen.", "tokens": ["Nenn", "ich", "mei\u00b7ne", "Cro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Was von Martis Saamen", "tokens": ["Was", "von", "Mar\u00b7tis", "Saa\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In der Welt zu finden ist;", "tokens": ["In", "der", "Welt", "zu", "fin\u00b7den", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was mit seinem Namen", "tokens": ["Was", "mit", "sei\u00b7nem", "Na\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und Geschmeide ausger\u00fcst,", "tokens": ["Und", "Ge\u00b7schmei\u00b7de", "aus\u00b7ge\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kan mein Herze r\u00fchren,", "tokens": ["Kan", "mein", "Her\u00b7ze", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und zur Freude f\u00fchren.", "tokens": ["Und", "zur", "Freu\u00b7de", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Alle meine Glieder", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Glie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sind des Mavors Eigenthum,", "tokens": ["Sind", "des", "Ma\u00b7vors", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor ihm fall ich nieder;", "tokens": ["Vor", "ihm", "fall", "ich", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Seine Waffen sind mein Ruhm;", "tokens": ["Sei\u00b7ne", "Waf\u00b7fen", "sind", "mein", "Ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich will ihm mein Leben,", "tokens": ["Ich", "will", "ihm", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "G\u00e4nzlich \u00fcbergeben.", "tokens": ["G\u00e4nz\u00b7lich", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Was kan mich vergn\u00fcgen?", "tokens": ["Was", "kan", "mich", "ver\u00b7gn\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nichts als Mavors blankes Schwerdt.", "tokens": ["Nichts", "als", "Ma\u00b7vors", "blan\u00b7kes", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NE", "ADJA", "NN", "$."], "meter": "