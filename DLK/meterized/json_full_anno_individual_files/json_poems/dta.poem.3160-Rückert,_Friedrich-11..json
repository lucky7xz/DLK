{"dta.poem.3160": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich dachte nun erst warm im Alter dich zu pflegen,", "tokens": ["Ich", "dach\u00b7te", "nun", "erst", "warm", "im", "Al\u00b7ter", "dich", "zu", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mu\u00df statt aller Pfleg' ins kalte Grab dich legen.", "tokens": ["Und", "mu\u00df", "statt", "al\u00b7ler", "Pfleg'", "ins", "kal\u00b7te", "Grab", "dich", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PIAT", "NN", "APPRART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Zinsen dacht' ich erst der Schuld dir abzutragen", "tokens": ["Die", "Zin\u00b7sen", "dacht'", "ich", "erst", "der", "Schuld", "dir", "ab\u00b7zu\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "PPER", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sohnesdankbarkeit, statt dich ins Grab zu tragen.", "tokens": ["Der", "Soh\u00b7nes\u00b7dank\u00b7bar\u00b7keit", ",", "statt", "dich", "ins", "Grab", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gott nimmt den Willen f\u00fcr die That; nicht mir beschieden", "tokens": ["Gott", "nimmt", "den", "Wil\u00b7len", "f\u00fcr", "die", "That", ";", "nicht", "mir", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$.", "PTKNEG", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War's, dir zu schaffen Ruh; er schuf dir Ruh und Frieden.", "tokens": ["Wa\u00b7r's", ",", "dir", "zu", "schaf\u00b7fen", "Ruh", ";", "er", "schuf", "dir", "Ruh", "und", "Frie\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPER", "PTKZU", "VVINF", "NN", "$.", "PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "----+-+-+-+-+-", "measure": "unknown.measure.penta"}}}}}