 "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Darum, Freunde! will ich reisen;", "tokens": ["Da\u00b7rum", ",", "Freun\u00b7de", "!", "will", "ich", "rei\u00b7sen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weiset Stra\u00dfe mir und Ziel!", "tokens": ["Wei\u00b7set", "Stra\u00b7\u00dfe", "mir", "und", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In der Heimat stillen Kreisen", "tokens": ["In", "der", "Hei\u00b7mat", "stil\u00b7len", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schw\u00e4rmt das Herz doch allzuviel.", "tokens": ["Schw\u00e4rmt", "das", "Herz", "doch", "all\u00b7zu\u00b7viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}