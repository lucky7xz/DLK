{"dta.poem.12893": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gespr\u00e4che zwischen dem Filidor  \n und der Sylvia. Aus dem   Pays.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "F. WArum so gar allein? Geliebte Sylvie!", "tokens": ["F.", "Wa\u00b7rum", "so", "gar", "al\u00b7lein", "?", "Ge\u00b7lieb\u00b7te", "Syl\u00b7vie", "!"], "token_info": ["abbreviation", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADV", "ADV", "$.", "NN", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Gef\u00e4llt dir dieser pusch mehr als die bunten matten?", "tokens": ["Ge\u00b7f\u00e4llt", "dir", "die\u00b7ser", "pusch", "mehr", "als", "die", "bun\u00b7ten", "mat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "ADJD", "PIAT", "KOKOM", "ART", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "S. Ich hielt mit allem flei\u00df in diesem gr\u00fcnen schatten", "tokens": ["S.", "Ich", "hielt", "mit", "al\u00b7lem", "flei\u00df", "in", "die\u00b7sem", "gr\u00fc\u00b7nen", "schat\u00b7ten"], "token_info": ["abbreviation", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "APPR", "PIS", "VVFIN", "APPR", "PDAT", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf meinen Filidor, den ich so gerne seh.", "tokens": ["Auf", "mei\u00b7nen", "Fi\u00b7li\u00b7dor", ",", "den", "ich", "so", "ger\u00b7ne", "seh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "F. Ach w\u00e4re dieses so! o wie vergn\u00fcgt w\u00e4r ich!", "tokens": ["F.", "Ach", "w\u00e4\u00b7re", "die\u00b7ses", "so", "!", "o", "wie", "ver\u00b7gn\u00fcgt", "w\u00e4r", "ich", "!"], "token_info": ["abbreviation", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PDS", "ADV", "$.", "FM", "KOKOM", "VVPP", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So bl\u00fchte meine lust, und st\u00fcrb meine plage.", "tokens": ["So", "bl\u00fch\u00b7te", "mei\u00b7ne", "lust", ",", "und", "st\u00fcrb", "mei\u00b7ne", "pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "S. Der himmel wei\u00df gar wohl, da\u00df ich die wahrheit sage;", "tokens": ["S.", "Der", "him\u00b7mel", "wei\u00df", "gar", "wohl", ",", "da\u00df", "ich", "die", "wahr\u00b7heit", "sa\u00b7ge", ";"], "token_info": ["abbreviation", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Allein berichte doch, wie steht es sonst um dich?", "tokens": ["Al\u00b7lein", "be\u00b7rich\u00b7te", "doch", ",", "wie", "steht", "es", "sonst", "um", "dich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "F. Meynst du, was mich betrifft? Ich komm itzunder gleich", "tokens": ["F.", "Meynst", "du", ",", "was", "mich", "be\u00b7tr\u00b7ifft", "?", "Ich", "komm", "it\u00b7zun\u00b7der", "gleich"], "token_info": ["abbreviation", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "$,", "PWS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Aus unsrem dorffe her, du kennst wohl Amarillen:", "tokens": ["Aus", "uns\u00b7rem", "dorf\u00b7fe", "her", ",", "du", "kennst", "wohl", "A\u00b7ma\u00b7ril\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die that sehr gro\u00df mit mir, doch wider meinen willen,", "tokens": ["Die", "that", "sehr", "gro\u00df", "mit", "mir", ",", "doch", "wi\u00b7der", "mei\u00b7nen", "wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich frage viel nach ihr, und w\u00e4r sie noch so reich.", "tokens": ["Ich", "fra\u00b7ge", "viel", "nach", "ihr", ",", "und", "w\u00e4r", "sie", "noch", "so", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "S. Ich wei\u00df wahrhafftig nicht, (ich mu\u00df es nur gestehn)", "tokens": ["S.", "Ich", "wei\u00df", "wahr\u00b7haff\u00b7tig", "nicht", ",", "(", "ich", "mu\u00df", "es", "nur", "ge\u00b7stehn", ")"], "token_info": ["abbreviation", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ADJD", "PTKNEG", "$,", "$(", "PPER", "VMFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ob dir zu trauen ist. Es k\u00f6nte leicht geschehen,", "tokens": ["Ob", "dir", "zu", "trau\u00b7en", "ist", ".", "Es", "k\u00f6n\u00b7te", "leicht", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "VAFIN", "$.", "PPER", "VMFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du kriegtest anderweit ein ander kind zu sehen,", "tokens": ["Du", "krieg\u00b7test", "an\u00b7der\u00b7weit", "ein", "an\u00b7der", "kind", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So sch\u00f6ner w\u00e4r\u2019, als ich, denn m\u00f6cht\u2019 ich immer gehn.", "tokens": ["So", "sch\u00f6\u00b7ner", "w\u00e4r'", ",", "als", "ich", ",", "denn", "m\u00f6cht'", "ich", "im\u00b7mer", "gehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "$,", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "F. Bef\u00fcrchte dieses nicht. Du magst gesichert seyn,", "tokens": ["F.", "Be\u00b7f\u00fcrch\u00b7te", "die\u00b7ses", "nicht", ".", "Du", "magst", "ge\u00b7si\u00b7chert", "seyn", ","], "token_info": ["abbreviation", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "PTKNEG", "$.", "PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df Filidor es recht von grund der seele meyne.", "tokens": ["Da\u00df", "Fi\u00b7li\u00b7dor", "es", "recht", "von", "grund", "der", "see\u00b7le", "mey\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "ADV", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich liebe keine mehr; ich sterbe blos der deine.", "tokens": ["Ich", "lie\u00b7be", "kei\u00b7ne", "mehr", ";", "ich", "ster\u00b7be", "blos", "der", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PIS", "$.", "PPER", "VVFIN", "ADV", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Drum bilde dir von mir nicht so viel arges ein.", "tokens": ["Drum", "bil\u00b7de", "dir", "von", "mir", "nicht", "so", "viel", "ar\u00b7ges", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PPER", "PTKNEG", "ADV", "PIAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Mein geist gelobet dir bey diesen eichen an:", "tokens": ["Mein", "geist", "ge\u00b7lo\u00b7bet", "dir", "bey", "die\u00b7sen", "ei\u00b7chen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mein mund soll keinen sonst, als nur den deinen, k\u00fcssen.", "tokens": ["Mein", "mund", "soll", "kei\u00b7nen", "sonst", ",", "als", "nur", "den", "dei\u00b7nen", ",", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIAT", "ADV", "$,", "KOUS", "ADV", "ART", "PPOSAT", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wenn schon ein engel uns das hertze hat entrissen,", "tokens": ["Wenn", "schon", "ein", "en\u00b7gel", "uns", "das", "hert\u00b7ze", "hat", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wer glaubt wol, da\u00df ein mensch es ihm entf\u00fchren kan?", "tokens": ["Wer", "glaubt", "wol", ",", "da\u00df", "ein", "mensch", "es", "ihm", "ent\u00b7f\u00fch\u00b7ren", "kan", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "S. Was aber wilst du denn von meiner armen hand", "tokens": ["S.", "Was", "a\u00b7ber", "wilst", "du", "denn", "von", "mei\u00b7ner", "ar\u00b7men", "hand"], "token_info": ["abbreviation", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PWS", "ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Vor deine s\u00fc\u00dfe treu vor angenehme sachen?", "tokens": ["Vor", "dei\u00b7ne", "s\u00fc\u00b7\u00dfe", "treu", "vor", "an\u00b7ge\u00b7neh\u00b7me", "sa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJD", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wie gerne wolt ich dich zu einem f\u00fcrsten machen;", "tokens": ["Wie", "ger\u00b7ne", "wolt", "ich", "dich", "zu", "ei\u00b7nem", "f\u00fcrs\u00b7ten", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Allein du siehest wol, ich habe selbst kein land.", "tokens": ["Al\u00b7lein", "du", "sie\u00b7hest", "wol", ",", "ich", "ha\u00b7be", "selbst", "kein", "land", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "F. Ach, Liebste Sylvia! du kanst durch deine hold", "tokens": ["F.", "Ach", ",", "Liebs\u00b7te", "Syl\u00b7via", "!", "du", "kanst", "durch", "dei\u00b7ne", "hold"], "token_info": ["abbreviation", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ITJ", "$,", "ADJA", "NE", "$.", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Mich \u00fcber alles gl\u00fcck erhabner f\u00fcrsten setzen:", "tokens": ["Mich", "\u00fc\u00b7ber", "al\u00b7les", "gl\u00fcck", "er\u00b7hab\u00b7ner", "f\u00fcrs\u00b7ten", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIS", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mag doch ein k\u00f6nig sich an gold und perlen letzen;", "tokens": ["Mag", "doch", "ein", "k\u00f6\u00b7nig", "sich", "an", "gold", "und", "per\u00b7len", "let\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJD", "PRF", "APPR", "NN", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dein hertze gilt mir mehr, als perle, land und gold.", "tokens": ["Dein", "hert\u00b7ze", "gilt", "mir", "mehr", ",", "als", "per\u00b7le", ",", "land", "und", "gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "S. Ach weh! ich sinck entz\u00fcckt zu deinen armen hin.", "tokens": ["S.", "Ach", "weh", "!", "ich", "sinck", "ent\u00b7z\u00fcckt", "zu", "dei\u00b7nen", "ar\u00b7men", "hin", "."], "token_info": ["abbreviation", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$.", "PPER", "ADJD", "VVPP", "APPR", "PPOSAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der himmel leitet mich zu diesem angelsterne.", "tokens": ["Der", "him\u00b7mel", "lei\u00b7tet", "mich", "zu", "die\u00b7sem", "an\u00b7gels\u00b7ter\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Nimm alles von mir hin, ich g\u00f6nne dir es gerne;", "tokens": ["Nimm", "al\u00b7les", "von", "mir", "hin", ",", "ich", "g\u00f6n\u00b7ne", "dir", "es", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "APPR", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Denn dein vergn\u00fcgen ist mein leben und gewinn.", "tokens": ["Denn", "dein", "ver\u00b7gn\u00fc\u00b7gen", "ist", "mein", "le\u00b7ben", "und", "ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVPP", "VAFIN", "PPOSAT", "VVINF", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}