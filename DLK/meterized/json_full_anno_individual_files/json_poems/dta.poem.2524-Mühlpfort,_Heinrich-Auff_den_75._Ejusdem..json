{"dta.poem.2524": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auff den  75.  Ejusdem.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem/ Hoch Edler Herr/ und Nestor unsrer Stadt/", "tokens": ["Nach\u00b7dem", "/", "Hoch", "Ed\u00b7ler", "Herr", "/", "und", "Nes\u00b7tor", "uns\u00b7rer", "Stadt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "ADJD", "ADJA", "NN", "$(", "KON", "NN", "PPOSAT", "NN", "$("], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Sich l\u00e4ngst die Ewigkeit mit seinem Ruhm verm\u00e4hlet/", "tokens": ["Sich", "l\u00e4ngst", "die", "E\u00b7wig\u00b7keit", "mit", "sei\u00b7nem", "Ruhm", "ver\u00b7m\u00e4h\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "De\u00df Alters Ehren-Schnee/ so siebentzig gezehlet/", "tokens": ["De\u00df", "Al\u00b7ters", "Eh\u00b7ren\u00b7Schnee", "/", "so", "sie\u00b7bent\u00b7zig", "ge\u00b7zeh\u00b7let", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nun auch das f\u00fcnffte Jahr darzu geleget hat/", "tokens": ["Nun", "auch", "das", "f\u00fcnff\u00b7te", "Jahr", "dar\u00b7zu", "ge\u00b7le\u00b7get", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "PAV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bl\u00fcht sein Hoch-Adlich Hau\u00df nicht nur allein in Freuden/", "tokens": ["Bl\u00fcht", "sein", "Hoch\u00b7Ad\u00b7lich", "Hau\u00df", "nicht", "nur", "al\u00b7lein", "in", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "PTKNEG", "ADV", "ADV", "APPR", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Der Himmel wil die Stadt mit gleicher Lust bekleiden.", "tokens": ["Der", "Him\u00b7mel", "wil", "die", "Stadt", "mit", "glei\u00b7cher", "Lust", "be\u00b7klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der B\u00fcrger treuer Wunsch/ der Unterthanen Pflicht/", "tokens": ["Der", "B\u00fcr\u00b7ger", "treu\u00b7er", "Wunsch", "/", "der", "Un\u00b7ter\u00b7tha\u00b7nen", "Pflicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So vor den Weyrauch-Klo\u00df die Hertzen selbst gewehret/", "tokens": ["So", "vor", "den", "Wey\u00b7rauch\u00b7Klo\u00df", "die", "Hert\u00b7zen", "selbst", "ge\u00b7weh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hat nie nichts eyfriger von GOttes Hand begehret", "tokens": ["Hat", "nie", "nichts", "ey\u00b7fri\u00b7ger", "von", "Got\u00b7tes", "Hand", "be\u00b7ge\u00b7hret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIS", "ADJA", "APPR", "NN", "NN", "VVFIN"], "meter": "-+-++--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Als diesen Namens-Tag/ das hoch-erw\u00fcnschte Licht;", "tokens": ["Als", "die\u00b7sen", "Na\u00b7mens\u00b7Tag", "/", "das", "hoch\u00b7er\u00b7w\u00fcnschte", "Licht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Dem gar Aurora sich zu Diensten sch\u00f6ner schm\u00fccket/", "tokens": ["Dem", "gar", "Au\u00b7ro\u00b7ra", "sich", "zu", "Diens\u00b7ten", "sch\u00f6\u00b7ner", "schm\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NE", "PRF", "APPR", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und in ihr g\u00f6ldnes Haar die frischen Rosen dr\u00fccket.", "tokens": ["Und", "in", "ihr", "g\u00f6ld\u00b7nes", "Haar", "die", "fri\u00b7schen", "Ro\u00b7sen", "dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Lorber-Wald erklingt von einem s\u00fcssen Thon/", "tokens": ["Der", "Lor\u00b7ber\u00b7Wald", "er\u00b7klingt", "von", "ei\u00b7nem", "s\u00fcs\u00b7sen", "Thon", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die G\u00f6tter sind bem\u00fcht ihr Opffer abzulegen.", "tokens": ["Die", "G\u00f6t\u00b7ter", "sind", "be\u00b7m\u00fcht", "ihr", "Opf\u00b7fer", "ab\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es heiligt Jupiter des hohen Alters wegen", "tokens": ["Es", "hei\u00b7ligt", "Ju\u00b7pi\u00b7ter", "des", "ho\u00b7hen", "Al\u00b7ters", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Den Eichbaum/ wie er pflegt/ und Ph\u00f6bus reicht die Kron", "tokens": ["Den", "Eich\u00b7baum", "/", "wie", "er", "pflegt", "/", "und", "Ph\u00f6\u00b7bus", "reicht", "die", "Kron"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PWAV", "PPER", "VVFIN", "$(", "KON", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von seinen Lorbern dar/ Cybele Fichten-Aeste/", "tokens": ["Von", "sei\u00b7nen", "Lor\u00b7bern", "dar", "/", "Cy\u00b7be\u00b7le", "Fich\u00b7ten\u00b7Aes\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$(", "NE", "NE", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Und Hercules ertheilt die Pappeln solchem Feste.", "tokens": ["Und", "Her\u00b7cu\u00b7les", "er\u00b7theilt", "die", "Pap\u00b7peln", "sol\u00b7chem", "Fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was sol ich aber thun/ Stern der Gerechtigkeit/", "tokens": ["Was", "sol", "ich", "a\u00b7ber", "thun", "/", "Stern", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$(", "NN", "ART", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Hochwerthester Patron? Mein Einfalt heist mich schweigen/", "tokens": ["Hoch\u00b7wert\u00b7hes\u00b7ter", "Pat\u00b7ron", "?", "Mein", "Ein\u00b7falt", "heist", "mich", "schwei\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "+----+-+-+-+-", "measure": "dactylic.init"}, "line.21": {"text": "Die tieff-verbundne Schuld sich danckbar zu erzeigen.", "tokens": ["Die", "tief\u00b7fver\u00b7bund\u00b7ne", "Schuld", "sich", "dan\u00b7ck\u00b7bar", "zu", "er\u00b7zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Minerva rufft mir zu: Wie mir vor jener Zeit", "tokens": ["Mi\u00b7ner\u00b7va", "rufft", "mir", "zu", ":", "Wie", "mir", "vor", "je\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "PPER", "APPR", "PDAT", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Athen/ des Friedens Bild/ den Oelbaum pflag zu weyhen/", "tokens": ["A\u00b7then", "/", "des", "Frie\u00b7dens", "Bild", "/", "den", "O\u00b7el\u00b7baum", "pflag", "zu", "wey\u00b7hen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "NN", "NN", "$(", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.24": {"text": "So wil denselben ich zum Opffer dir verleyhen.", "tokens": ["So", "wil", "den\u00b7sel\u00b7ben", "ich", "zum", "Opf\u00b7fer", "dir", "ver\u00b7ley\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wolan! Nun ietzt die Welt mit Krieg zu Rahte geht/", "tokens": ["Wo\u00b7lan", "!", "Nun", "ietzt", "die", "Welt", "mit", "Krieg", "zu", "Rah\u00b7te", "geht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "ADV", "ART", "NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und Mars sein Blut-Fahn schwingt/ Bellona durch ihr rasen", "tokens": ["Und", "Mars", "sein", "Blut\u00b7Fahn", "schwingt", "/", "Bel\u00b7lo\u00b7na", "durch", "ihr", "ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "PPOSAT", "NN", "VVFIN", "$(", "NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.27": {"text": "Die liechte Krieges-Glut in Deutschland auffgeblasen;", "tokens": ["Die", "liech\u00b7te", "Krie\u00b7ge\u00b7sGlut", "in", "Deutschland", "auff\u00b7ge\u00b7bla\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Hingegen noch bey uns der Friedens-Oelbaum steht", "tokens": ["Hin\u00b7ge\u00b7gen", "noch", "bey", "uns", "der", "Frie\u00b7dens\u00b7O\u00b7el\u00b7baum", "steht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "+---+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "In unverwelckter Pracht/ so heist mich ", "tokens": ["In", "un\u00b7ver\u00b7welck\u00b7ter", "Pracht", "/", "so", "heist", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Die Pallas ihren Baum zu einem Denckmahl setzen.", "tokens": ["Die", "Pal\u00b7las", "ih\u00b7ren", "Baum", "zu", "ei\u00b7nem", "Denck\u00b7mahl", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df g\u00fcldner Fried und Ruh in unsern Mauren wohnt/", "tokens": ["Da\u00df", "g\u00fcld\u00b7ner", "Fried", "und", "Ruh", "in", "un\u00b7sern", "Mau\u00b7ren", "wohnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Recht und Gerechtigkeit einander sich noch k\u00fcssen/", "tokens": ["Recht", "und", "Ge\u00b7rech\u00b7tig\u00b7keit", "ein\u00b7an\u00b7der", "sich", "noch", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "PRF", "ADV", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.33": {"text": "Und heilsame Gesetz/ als wie aus Quellen fliessen/", "tokens": ["Und", "heil\u00b7sa\u00b7me", "Ge\u00b7setz", "/", "als", "wie", "aus", "Quel\u00b7len", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "KOUS", "KOKOM", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df man die Laster strafft und Tugenden belohnt/", "tokens": ["Da\u00df", "man", "die", "Las\u00b7ter", "strafft", "und", "Tu\u00b7gen\u00b7den", "be\u00b7lohnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und sich gemeines Heil in gutem Wolstand findet/", "tokens": ["Und", "sich", "ge\u00b7mei\u00b7nes", "Heil", "in", "gu\u00b7tem", "Wol\u00b7stand", "fin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wird durch sein Regiment, ", "tokens": ["Wird", "durch", "sein", "Re\u00b7gi\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Es geht an Nutzbarkeit der Oelbaum allen f\u00fcr.", "tokens": ["Es", "geht", "an", "Nutz\u00b7bar\u00b7keit", "der", "O\u00b7el\u00b7baum", "al\u00b7len", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "PIAT", "APPR", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "Es hie\u00df des H\u00f6chsten Mund sein Volck auch Oele steuren/", "tokens": ["Es", "hie\u00df", "des", "H\u00f