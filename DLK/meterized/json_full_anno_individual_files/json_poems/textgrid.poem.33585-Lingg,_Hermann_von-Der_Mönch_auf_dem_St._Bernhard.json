rd", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.24": {"line.1": {"text": "Wer immer es sei, wen die Nacht \u00fcberrascht,", "tokens": ["Wer", "im\u00b7mer", "es", "sei", ",", "wen", "die", "Nacht", "\u00fc\u00b7berr\u00b7ascht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VAFIN", "$,", "PWS", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wen der Sturm und wen die Lawin' erhascht,", "tokens": ["Wen", "der", "Sturm", "und", "wen", "die", "La\u00b7win'", "er\u00b7hascht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.25": {"line.1": {"text": "Wer mit wankendem Fu\u00df am Abgrund hangt,", "tokens": ["Wer", "mit", "wan\u00b7ken\u00b7dem", "Fu\u00df", "am", "Ab\u00b7grund", "hangt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Einen Strauch, eine Wurzel am Felsen erlangt,", "tokens": ["Ei\u00b7nen", "Strauch", ",", "ei\u00b7ne", "Wur\u00b7zel", "am", "Fel\u00b7sen", "er\u00b7langt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.26": {"line.1": {"text": "Der M\u00f6nch und sein Hund sind nah und fern", "tokens": ["Der", "M\u00f6nch", "und", "sein", "Hund", "sind", "nah", "und", "fern"], "token_info": ["word", "word", "word", "word", "w