{"dta.poem.3009": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr wollt doch \u00fcberall etwas Apartes haben,", "tokens": ["Ihr", "wollt", "doch", "\u00fc\u00b7be\u00b7rall", "et\u00b7was", "A\u00b7par\u00b7tes", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PIAT", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unsterblichkeit sogar soll vorzugsweis euch laben.", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", "so\u00b7gar", "soll", "vor\u00b7zugs\u00b7weis", "euch", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als denkenstarke bald und bald als glaubenfeste", "tokens": ["Als", "den\u00b7ken\u00b7star\u00b7ke", "bald", "und", "bald", "als", "glau\u00b7ben\u00b7fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "ADV", "KON", "ADV", "KOKOM", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprecht ihr sie an f\u00fcr euch, und sprecht sie ab dem Reste.", "tokens": ["Sprecht", "ihr", "sie", "an", "f\u00fcr", "euch", ",", "und", "sprecht", "sie", "ab", "dem", "Res\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "APPR", "PPER", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gemeine Menschen sind mit Seelen nur begabt,", "tokens": ["Ge\u00b7mei\u00b7ne", "Men\u00b7schen", "sind", "mit", "See\u00b7len", "nur", "be\u00b7gabt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Thierseelen gleich, indes ihr Geister Geist nur habt.", "tokens": ["Thier\u00b7see\u00b7len", "gleich", ",", "in\u00b7des", "ihr", "Geis\u00b7ter", "Geist", "nur", "habt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "PPOSAT", "NN", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fcrchte, dieser Geist des D\u00fcnkels sprengt die Flasche,", "tokens": ["Ich", "f\u00fcrch\u00b7te", ",", "die\u00b7ser", "Geist", "des", "D\u00fcn\u00b7kels", "sprengt", "die", "Fla\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDAT", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verpufft, verdunstet so da\u00df Nichts ihn wieder hasche;", "tokens": ["Ver\u00b7pufft", ",", "ver\u00b7duns\u00b7tet", "so", "da\u00df", "Nichts", "ihn", "wie\u00b7der", "ha\u00b7sche", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und weder droben wird zum Lohn euch noch hienieden", "tokens": ["Und", "we\u00b7der", "dro\u00b7ben", "wird", "zum", "Lohn", "euch", "noch", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KON", "ADV", "VAFIN", "APPRART", "NN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unsterblichkeit daf\u00fcr von Gott und Welt beschieden.", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", "da\u00b7f\u00fcr", "von", "Gott", "und", "Welt", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}