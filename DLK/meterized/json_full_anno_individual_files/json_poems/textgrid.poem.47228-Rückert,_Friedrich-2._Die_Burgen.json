{"textgrid.poem.47228": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2. Die Burgen", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Tr\u00fcmmern sankt ihr unter Schicksalsstreichen,", "tokens": ["Zu", "Tr\u00fcm\u00b7mern", "sankt", "ihr", "un\u00b7ter", "Schick\u00b7sals\u00b7strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch noch in Tr\u00fcmmern scheint ihr stolz zu stehen,", "tokens": ["Doch", "noch", "in", "Tr\u00fcm\u00b7mern", "scheint", "ihr", "stolz", "zu", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verdammend von den H\u00f6h'n herabzusehen", "tokens": ["Ver\u00b7dam\u00b7mend", "von", "den", "H\u00f6h'n", "her\u00b7ab\u00b7zu\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf eine Welt, die nicht an euch kann reichen.", "tokens": ["Auf", "ei\u00b7ne", "Welt", ",", "die", "nicht", "an", "euch", "kann", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PTKNEG", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein enger Sinn baut H\u00fctten, die ihm gleichen,", "tokens": ["Ein", "en\u00b7ger", "Sinn", "baut", "H\u00fct\u00b7ten", ",", "die", "ihm", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,", "PRELS", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Thal, wo nicht der Freiheit L\u00fcfte wehen!", "tokens": ["Im", "Thal", ",", "wo", "nicht", "der", "Frei\u00b7heit", "L\u00fcf\u00b7te", "we\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PTKNEG", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr seht sie baun, seht wieder sie vergehen,", "tokens": ["Ihr", "seht", "sie", "baun", ",", "seht", "wie\u00b7der", "sie", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihr steht droben, ewige Todeszeichen.", "tokens": ["Und", "ihr", "steht", "dro\u00b7ben", ",", "e\u00b7wi\u00b7ge", "To\u00b7des\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Niemand erk\u00fchnt sich, eure Riesenw\u00e4lle", "tokens": ["Nie\u00b7mand", "er\u00b7k\u00fchnt", "sich", ",", "eu\u00b7re", "Rie\u00b7sen\u00b7w\u00e4l\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit des Besitztums Anspruch anzutasten,", "tokens": ["Mit", "des", "Be\u00b7sitz\u00b7tums", "An\u00b7spruch", "an\u00b7zu\u00b7tas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf euren Schutt zu flicken seine Schwelle.", "tokens": ["Auf", "eu\u00b7ren", "Schutt", "zu", "fli\u00b7cken", "sei\u00b7ne", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja kaum der Wandrer wagt auf euch zu rasten,", "tokens": ["Ja", "kaum", "der", "Wand\u00b7rer", "wagt", "auf", "euch", "zu", "ras\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich als entehr' er alter Kraft Grabstelle", "tokens": ["Gleich", "als", "ent\u00b7ehr'", "er", "al\u00b7ter", "Kraft", "Grab\u00b7stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VVFIN", "PPER", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch seines Feiglingstrittes nichtige Lasten.", "tokens": ["Durch", "sei\u00b7nes", "Feig\u00b7ling\u00b7strit\u00b7tes", "nich\u00b7ti\u00b7ge", "Las\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Zu Tr\u00fcmmern sankt ihr unter Schicksalsstreichen,", "tokens": ["Zu", "Tr\u00fcm\u00b7mern", "sankt", "ihr", "un\u00b7ter", "Schick\u00b7sals\u00b7strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word",