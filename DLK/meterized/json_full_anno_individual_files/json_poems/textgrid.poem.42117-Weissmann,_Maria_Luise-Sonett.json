{"textgrid.poem.42117": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wende den Blick hinweg! Er traf mich lang", "tokens": ["Wen\u00b7de", "den", "Blick", "hin\u00b7weg", "!", "Er", "traf", "mich", "lang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und traf mich t\u00f6dlich. Zwar ich gleite nicht", "tokens": ["Und", "traf", "mich", "t\u00f6d\u00b7lich", ".", "Zwar", "ich", "glei\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$.", "ADV", "PPER", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Unwillig hin, nicht zu vergehen bang:", "tokens": ["Un\u00b7wil\u00b7lig", "hin", ",", "nicht", "zu", "ver\u00b7ge\u00b7hen", "bang", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PTKNEG", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur nimm von meinem Tod dies dunkle Licht,", "tokens": ["Nur", "nimm", "von", "mei\u00b7nem", "Tod", "dies", "dunk\u00b7le", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPR", "PPOSAT", "NN", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nimm Deinen Blick hinweg! Kein Dickicht ist", "tokens": ["Nimm", "Dei\u00b7nen", "Blick", "hin\u00b7weg", "!", "Kein", "Di\u00b7ckicht", "ist"], "token_info": ["word", "wor