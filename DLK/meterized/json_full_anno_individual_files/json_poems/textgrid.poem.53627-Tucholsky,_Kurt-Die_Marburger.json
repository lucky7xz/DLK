{"textgrid.poem.53627": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Marburger", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbherr Kamerad? \u2013", "tokens": ["\u00bb", "herr", "Ka\u00b7me\u00b7rad", "?", "\u2013"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die fuffzehn Leute?", "tokens": ["Die", "fuff\u00b7zehn", "Leu\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Was soll damit?\u00ab \u2013 \u00bbNa so \u2013 \u2013, das mu\u00df noch heute \u2013 \u2013,", "tokens": ["Was", "soll", "da\u00b7mit", "?", "\u00ab", "\u2013", "\u00bb", "Na", "so", "\u2013", "\u2013", ",", "das", "mu\u00df", "noch", "heu\u00b7te", "\u2013", "\u2013", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "VMFIN", "PAV", "$.", "$(", "$(", "$(", "ITJ", "ADV", "$(", "$(", "$,", "PDS", "VMFIN", "ADV", "ADV", "$(", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "am besten gleich.\u00ab \u2013 Und eine Handbewegung. F\u00fcnfzehn trotten,", "tokens": ["am", "bes\u00b7ten", "gleich", ".", "\u00ab", "\u2013", "Und", "ei\u00b7ne", "Hand\u00b7be\u00b7we\u00b7gung", ".", "F\u00fcnf\u00b7zehn", "trot\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADV", "$.", "$(", "$(", "KON", "ART", "NN", "$.", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "vom Kolbenschlag getrieben, vorw\u00e4rts.", "tokens": ["vom", "Kol\u00b7ben\u00b7schlag", "ge\u00b7trie\u00b7ben", ",", "vor\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Junge Herren, in knapper Uniform der Offiziere,", "tokens": ["Jun\u00b7ge", "Her\u00b7ren", ",", "in", "knap\u00b7per", "U\u00b7ni\u00b7form", "der", "Of\u00b7fi\u00b7zie\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "mit Orden, Ehrenzeichen und Monokel,", "tokens": ["mit", "Or\u00b7den", ",", "Eh\u00b7ren\u00b7zei\u00b7chen", "und", "Mo\u00b7no\u00b7kel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "daneben her. Die Leute schleppen", "tokens": ["da\u00b7ne\u00b7ben", "her", ".", "Die", "Leu\u00b7te", "schlep\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PTKVZ", "$.", "ART", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "sich m\u00fchsam weiter. Einer blutet. Viele st\u00f6hnen.", "tokens": ["sich", "m\u00fch\u00b7sam", "wei\u00b7ter", ".", "Ei\u00b7ner", "blu\u00b7tet", ".", "Vie\u00b7le", "st\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKVZ", "$.", "PIS", "VVFIN", "$.", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Herren rauchen, l\u00e4rmen, lachen, sprechen.", "tokens": ["Die", "Her\u00b7ren", "rau\u00b7chen", ",", "l\u00e4r\u00b7men", ",", "la\u00b7chen", ",", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und leise, einer, von den Offizieren: \u00bbAn den Stra\u00dfengraben!\u00ab", "tokens": ["Und", "lei\u00b7se", ",", "ei\u00b7ner", ",", "von", "den", "Of\u00b7fi\u00b7zie\u00b7ren", ":", "\u00bb", "An", "den", "Stra\u00b7\u00dfen\u00b7gra\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "$,", "PIS", "$,", "APPR", "ART", "NN", "$.", "$(", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.12": {"text": "Und: \u00bbSechste Kompanie nach vorne!", "tokens": ["Und", ":", "\u00bb", "Sechs\u00b7te", "Kom\u00b7pa\u00b7nie", "nach", "vor\u00b7ne", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "$(", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der Trupp mit den Gefangenen an den Schlu\u00df!\u00ab", "tokens": ["Der", "Trupp", "mit", "den", "Ge\u00b7fan\u00b7ge\u00b7nen", "an", "den", "Schlu\u00df", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "ruft eine Stimme. Aufgerissene Augen . . .", "tokens": ["ruft", "ei\u00b7ne", "Stim\u00b7me", ".", "Auf\u00b7ge\u00b7ris\u00b7se\u00b7ne", "Au\u00b7gen", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ADJA", "NN", "$.", "$.", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.15": {"text": "Sie wehren sich, die Zivilisten. Fliehn?", "tokens": ["Sie", "weh\u00b7ren", "sich", ",", "die", "Zi\u00b7vi\u00b7lis\u00b7ten", ".", "Fliehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Sie kennen das. Sie wollen nicht. Und die Studenten", "tokens": ["Sie", "ken\u00b7nen", "das", ".", "Sie", "wol\u00b7len", "nicht", ".", "Und", "die", "Stu\u00b7den\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "$.", "PPER", "VMFIN", "PTKNEG", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "haun mit den Kolben aufgekr\u00fcmmte R\u00fccken,", "tokens": ["haun", "mit", "den", "Kol\u00b7ben", "auf\u00b7ge\u00b7kr\u00fcmm\u00b7te", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.18": {"text": "\u00bbmarsch an den Stra\u00dfenrand! Ihr wollt nicht? Hunde \u2013!\u00ab", "tokens": ["\u00bb", "marsch", "an", "den", "Stra\u00b7\u00dfen\u00b7rand", "!", "Ihr", "wollt", "nicht", "?", "Hun\u00b7de", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["$(", "ADJD", "APPR", "ART", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "$.", "NN", "$(", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Den Korpsstudenten, die, als Offiziere kost\u00fcmiert,", "tokens": ["Den", "Korps\u00b7stu\u00b7den\u00b7ten", ",", "die", ",", "als", "Of\u00b7fi\u00b7zie\u00b7re", "kos\u00b7t\u00fc\u00b7miert", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "den Zug begleiten, wird der Kram zu bunt.", "tokens": ["den", "Zug", "be\u00b7glei\u00b7ten", ",", "wird", "der", "Kram", "zu", "bunt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "VAFIN", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "\u00bbna, los doch, Kinder! Macht doch Schlu\u00df!\u00ab \u2013", "tokens": ["\u00bb", "na", ",", "los", "doch", ",", "Kin\u00b7der", "!", "Macht", "doch", "Schlu\u00df", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "ADV", "ADV", "$,", "NN", "$.", "NN", "KON", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und Sch\u00fcsse hallen durch den Morgennebel.", "tokens": ["Und", "Sch\u00fcs\u00b7se", "hal\u00b7len", "durch", "den", "Mor\u00b7gen\u00b7ne\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.23": {"text": "Geschrei, Gest\u00f6hn. Die blutigen K\u00f6rper rollen", "tokens": ["Ge\u00b7schrei", ",", "Ge\u00b7st\u00f6hn", ".", "Die", "blu\u00b7ti\u00b7gen", "K\u00f6r\u00b7per", "rol\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "im Staub. Die Leutnants lachen. \u00bbSo, die sind wir los!", "tokens": ["im", "Staub", ".", "Die", "Leut\u00b7nants", "la\u00b7chen", ".", "\u00bb", "So", ",", "die", "sind", "wir", "los", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ART", "NN", "VVINF", "$.", "$(", "ADV", "$,", "PRELS", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die sind erledigt \u2013!\u00ab", "tokens": ["Die", "sind", "er\u00b7le\u00b7digt", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$(", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ihr werdet sie nicht los.", "tokens": ["Ihr", "wer\u00b7det", "sie", "nicht", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Korporalschaft,", "tokens": ["Die", "Kor\u00b7po\u00b7ral\u00b7schaft", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "die da korrekt und kameradschaftlich", "tokens": ["die", "da", "kor\u00b7rekt", "und", "ka\u00b7me\u00b7rad\u00b7schaft\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "KON", "ADJD"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gericht markierte, \u00e4ndert nichts daran:", "tokens": ["Ge\u00b7richt", "mar\u00b7kier\u00b7te", ",", "\u00e4n\u00b7dert", "nichts", "da\u00b7ran", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "PIS", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ihr habt gekniffen!", "tokens": ["Und", "ihr", "habt", "ge\u00b7knif\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ihr m\u00fc\u00dft dem Pack Autorit\u00e4t beweisen?", "tokens": ["Ihr", "m\u00fc\u00dft", "dem", "Pack", "Au\u00b7to\u00b7ri\u00b7t\u00e4t", "be\u00b7wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und h\u00e4tte einer nun von denen blind-fanatisch", "tokens": ["Und", "h\u00e4t\u00b7te", "ei\u00b7ner", "nun", "von", "de\u00b7nen", "blin\u00b7dfa\u00b7na\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADV", "APPR", "PRELS", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "den General und einen nur von euch", "tokens": ["den", "Ge\u00b7ne\u00b7ral", "und", "ei\u00b7nen", "nur", "von", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "erschossen oder auch verwundet \u2013: ein Gericht", "tokens": ["er\u00b7schos\u00b7sen", "o\u00b7der", "auch", "ver\u00b7wun\u00b7det", "\u2013", ":", "ein", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VVPP", "KON", "ADV", "VVPP", "$(", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "w\u00e4r \u00fcber diesen Mann hereingebrochen,", "tokens": ["w\u00e4r", "\u00fc\u00b7ber", "die\u00b7sen", "Mann", "her\u00b7ein\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "da\u00df er gewu\u00dft h\u00e4tt, was er da getan.", "tokens": ["da\u00df", "er", "ge\u00b7wu\u00dft", "h\u00e4tt", ",", "was", "er", "da", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Zu Tode! \u2013 Zuchthaus! \u2013 Zuchthaus! \u2013 Glatt zum Tode! \u2013", "tokens": ["Zu", "To\u00b7de", "!", "\u2013", "Zucht\u00b7haus", "!", "\u2013", "Zucht\u00b7haus", "!", "\u2013", "Glatt", "zum", "To\u00b7de", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$.", "$(", "NN", "$.", "$(", "NN", "$.", "$(", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und ihr \u2013?", "tokens": ["Und", "ihr", "\u2013", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "$(", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Ihr habt gekniffen.", "tokens": ["Ihr", "habt", "ge\u00b7knif\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Besinnt euch pl\u00f6tzlich, da\u00df ihr eigentlich Soldaten,", "tokens": ["Be\u00b7sinnt", "euch", "pl\u00f6tz\u00b7lich", ",", "da\u00df", "ihr", "ei\u00b7gent\u00b7lich", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "und da\u00df ein Kriegsgericht da kompetent.", "tokens": ["und", "da\u00df", "ein", "Kriegs\u00b7ge\u00b7richt", "da", "kom\u00b7pe\u00b7tent", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ein Standgericht? Standesgericht sollts hei\u00dfen.", "tokens": ["Ein", "Stand\u00b7ge\u00b7richt", "?", "Stan\u00b7des\u00b7ge\u00b7richt", "sollts", "hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Feg sie hinweg, wenn du noch Atem hast!", "tokens": ["Feg", "sie", "hin\u00b7weg", ",", "wenn", "du", "noch", "A\u00b7tem", "hast", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Volk!", "tokens": ["Volk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Das werden deine Richter und Beamte!", "tokens": ["Das", "wer\u00b7den", "dei\u00b7ne", "Rich\u00b7ter", "und", "Be\u00b7am\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Volk!", "tokens": ["Volk", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Das darf dich einmal richten und verwalten,", "tokens": ["Das", "darf", "dich", "ein\u00b7mal", "rich\u00b7ten", "und", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word