{"dta.poem.20610": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Verg\u00e4nglichkeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4rsch, d\u2019 L\u00fct sin n\u00fcmme do, wenns", "tokens": ["N\u00e4rsch", ",", "d'", "L\u00fct", "sin", "n\u00fcm\u00b7me", "do", ",", "wenns"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "KOUS"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "brennt, sie sin \u2014", "tokens": ["brennt", ",", "sie", "sin"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "wo sin sie? Seig dn frumm, und halt di", "tokens": ["wo", "sin", "sie", "?", "Seig", "dn", "frumm", ",", "und", "halt", "di"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "NE", "NE", "ADJD", "$,", "KON", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wohl,", "tokens": ["wohl", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "geb, wo de bisch, und bhalt di Gwisse rein!", "tokens": ["geb", ",", "wo", "de", "bisch", ",", "und", "bhalt", "di", "Gwis\u00b7se", "rein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NE", "ADJD", "$,", "KON", "VVFIN", "NE", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Siehsch nit, wie d\u2019 Luft mit sch\u00f6ne Sterne", "tokens": ["Sieh\u00b7sch", "nit", ",", "wie", "d'", "Luft", "mit", "sch\u00f6\u00b7ne", "Ster\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "$,", "PWAV", "NE", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "prangt!", "tokens": ["prangt", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "\u2019s isch iede Stern verglichlige ne Dorf,", "tokens": ["'s", "isch", "ie\u00b7de", "Stern", "ver\u00b7glich\u00b7li\u00b7ge", "ne", "Dorf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PIAT", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "und witer oben isch e sch\u00f6ni Stadt,", "tokens": ["und", "wi\u00b7ter", "o\u00b7ben", "isch", "e", "sch\u00f6\u00b7ni", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "me sieht sie nit vo do, und haltsch di gut,", "tokens": ["me", "sieht", "sie", "nit", "vo", "do", ",", "und", "haltsch", "di", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "$,", "KON", "ADJD", "NE", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "se chunnsch in so ne Stern, und \u2019s isch der", "tokens": ["se", "chunnsch", "in", "so", "ne", "Stern", ",", "und", "'s", "isch", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADV", "ADJA", "NN", "$,", "KON", "PPER", "ADJD", "ART"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "wohl,", "tokens": ["wohl", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "und findsch der Aetti d\u00f6rt, wenns Gottswill", "tokens": ["und", "findsch", "der", "A\u00b7et\u00b7ti", "d\u00f6rt", ",", "wenns", "Gotts\u00b7will"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$,", "KOUS", "NE"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "isch,", "tokens": ["isch", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "und \u2019s Ch\u00fcngi selig, d\u2019 Mutter. Oebbe", "tokens": ["und", "'s", "Ch\u00fcn\u00b7gi", "se\u00b7lig", ",", "d'", "Mut\u00b7ter", ".", "O\u00b7eb\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "NE", "ADJD", "$,", "NE", "NN", "$.", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "fahrsch", "tokens": ["fahr\u00b7sch"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "au d\u2019 Milchstro\u00df uf in die verborgeni Stadt,", "tokens": ["au", "d'", "Milch\u00b7stro\u00df", "uf", "in", "die", "ver\u00b7bor\u00b7ge\u00b7ni", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.18": {"text": "und wenn de sitw\u00e4rts abe luegsch, was siehsch?", "tokens": ["und", "wenn", "de", "sit\u00b7w\u00e4rts", "a\u00b7be", "lu\u00b7egsch", ",", "was", "sieh\u00b7sch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "ADV", "VVFIN", "ADJD", "$,", "PWS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "e ", "tokens": ["e"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.20": {"text": "verchohlt,", "tokens": ["ver\u00b7chohlt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "der Blauen au, as wie zwee alti Th\u00fcrn,", "tokens": ["der", "Blau\u00b7en", "au", ",", "as", "wie", "zwee", "al\u00b7ti", "Th\u00fcrn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "und zwische drinn isch alles use brennt.", "tokens": ["und", "zwi\u00b7sche", "drinn", "isch", "al\u00b7les", "u\u00b7se", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "bis tief in Boden abe. D\u2019 Wiese het", "tokens": ["bis", "tief", "in", "Bo\u00b7den", "a\u00b7be", ".", "D'", "Wie\u00b7se", "het"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "PTKVZ", "$.", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "ke Wasser meh, \u2019s isch alles \u00f6d und schwarz", "tokens": ["ke", "Was\u00b7ser", "meh", ",", "'s", "isch", "al\u00b7les", "\u00f6d", "und", "schwarz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "NE", "$,", "PPER", "ADJD", "PIS", "ADJD", "KON", "ADJD"], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.25": {"text": "und todtestill, so wit me luegt \u2014 das siehsch,", "tokens": ["und", "tod\u00b7tes\u00b7till", ",", "so", "wit", "me", "luegt", "das", "sieh\u00b7sch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "FM", "FM", "FM", "$(", "ART", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.26": {"text": "und seisch di\u2019m Cammerad, wo mitder goht:", "tokens": ["und", "seisch", "di'm", "Cam\u00b7me\u00b7rad", ",", "wo", "mit\u00b7der", "goht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "\u201elueg, d\u00f6rt isch d\u2019", "tokens": ["\u201e", "lu\u00b7eg", ",", "d\u00f6rt", "isch", "d'"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "NE", "NE"], "meter": "+----", "measure": "dactylic.init"}, "line.28": {"text": "\u201ehet Belche ghei\u00dfe! Nit gar wiit dervo", "tokens": ["\u201e", "het", "Bel\u00b7che", "ghei\u00b7\u00dfe", "!", "Nit", "gar", "wiit", "der\u00b7vo"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "NN", "VVFIN", "$.", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "\u201eisch Wisleth gsi, d\u00f6rt hani au scho glebt,", "tokens": ["\u201e", "isch", "Wis\u00b7leth", "gsi", ",", "d\u00f6rt", "ha\u00b7ni", "au", "scho", "glebt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "\u201eund Stiere g\u2019 wettet, Holz go Basel gf\u00fchrt,", "tokens": ["\u201e", "und", "Stie\u00b7re", "g'", "wet\u00b7tet", ",", "Holz", "go", "Ba\u00b7sel", "gf\u00fchrt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "NN", "NE", "VVFIN", "$,", "NN", "NE", "NE", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "\u201eund brochet, Matte g\u2019raust, und Liecht-", "tokens": ["\u201e", "und", "bro\u00b7chet", ",", "Mat\u00b7te", "g'\u00b7raust", ",", "und", "Liecht"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "KON", "VVFIN", "$,", "NN", "VVFIN", "$,", "KON", "TRUNC"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Sp\u00f6h\u2019 gmacht,", "tokens": ["Sp\u00f6h'", "gmacht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.33": {"text": "\u201eund gv\u00e4tterlet, bi\u00df an mi selig End,", "tokens": ["\u201e", "und", "gv\u00e4t\u00b7ter\u00b7let", ",", "bi\u00df", "an", "mi", "se\u00b7lig", "End", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "$,", "KOUS", "APPR", "NE", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "\u201eund m\u00f6cht iez n\u00fcmme hi.\u201c \u2014 ", "tokens": ["\u201e", "und", "m\u00f6cht", "iez", "n\u00fcm\u00b7me", "hi", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "KON", "VMFIN", "FM", "FM", "FM", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Laubi, Merz!             ", "tokens": ["Lau\u00b7bi", ",", "Merz", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}