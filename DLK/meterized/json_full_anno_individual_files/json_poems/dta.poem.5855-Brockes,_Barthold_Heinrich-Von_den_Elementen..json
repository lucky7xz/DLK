{"dta.poem.5855": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem wir sie als ein Geb\u00e4u besehn,", "tokens": ["In\u00b7dem", "wir", "sie", "als", "ein", "Ge\u00b7b\u00e4u", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das wunderbarlich gro\u00df und wunderbarlich sch\u00f6n;", "tokens": ["Das", "wun\u00b7der\u00b7bar\u00b7lich", "gro\u00df", "und", "wun\u00b7der\u00b7bar\u00b7lich", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und unsere Gedancken dahin gehn,", "tokens": ["Und", "un\u00b7se\u00b7re", "Ge\u00b7dan\u00b7cken", "da\u00b7hin", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Ordnungen geheimes Band zu finden,", "tokens": ["Der", "Ord\u00b7nun\u00b7gen", "ge\u00b7hei\u00b7mes", "Band", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So Zier-als Aend\u2019rungen zu fassen, zu ergr\u00fcnden;", "tokens": ["So", "Zier\u00b7als", "A\u00b7en\u00b7d'\u00b7run\u00b7gen", "zu", "fas\u00b7sen", ",", "zu", "er\u00b7gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "So untersucht mit Recht im Anfang unser Geist", "tokens": ["So", "un\u00b7ter\u00b7sucht", "mit", "Recht", "im", "An\u00b7fang", "un\u00b7ser", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das, was man ", "tokens": ["Das", ",", "was", "man"], "token_info": ["word", "punct", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Auch wie das Wesen aller Dinge", "tokens": ["Auch", "wie", "das", "We\u00b7sen", "al\u00b7ler", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Aus der Materie entspringe;", "tokens": ["Aus", "der", "Ma\u00b7te\u00b7rie", "ent\u00b7sprin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nicht minder das, was man an C\u00f6rpern nennt,", "tokens": ["Nicht", "min\u00b7der", "das", ",", "was", "man", "an", "C\u00f6r\u00b7pern", "nennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PDS", "$,", "PRELS", "PIS", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Vermischung, Eigenschafft, wie es sich f\u00fcgt und trennt:", "tokens": ["Ver\u00b7mi\u00b7schung", ",", "Ei\u00b7gen\u00b7schafft", ",", "wie", "es", "sich", "f\u00fcgt", "und", "trennt", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vor allen auch den Ursprung und den Grund,", "tokens": ["Vor", "al\u00b7len", "auch", "den", "Ur\u00b7sprung", "und", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Woher so mancherley gew\u00fcrcket werde,", "tokens": ["Wo\u00b7her", "so", "man\u00b7cher\u00b7ley", "ge\u00b7w\u00fcr\u00b7cket", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Jm Himmel, in der Lufft, im Wasser, auf der Erde?", "tokens": ["Jm", "Him\u00b7mel", ",", "in", "der", "Lufft", ",", "im", "Was\u00b7ser", ",", "auf", "der", "Er\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}