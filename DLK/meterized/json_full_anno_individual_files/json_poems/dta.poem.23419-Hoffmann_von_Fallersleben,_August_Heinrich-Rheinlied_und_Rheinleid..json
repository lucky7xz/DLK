{"dta.poem.23419": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Rheinlied und Rheinleid.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.85", "id:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In jedem Haus' ein Klimperkasten", "tokens": ["In", "je\u00b7dem", "Haus'", "ein", "Klim\u00b7per\u00b7kas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In jedem Hause Stimm' und Hand,", "tokens": ["In", "je\u00b7dem", "Hau\u00b7se", "Stimm'", "und", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In jedem Haus' Enthusiasten", "tokens": ["In", "je\u00b7dem", "Haus'", "En\u00b7thu\u00b7si\u00b7as\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr's liebe deutsche Vaterland.", "tokens": ["F\u00fcr's", "lie\u00b7be", "deut\u00b7sche", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die Begeistrung nimmt kein Ende", "tokens": ["Und", "die", "Be\u00b7geis\u00b7trung", "nimmt", "kein", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht sich Luft bei Tag und Nacht,", "tokens": ["Und", "macht", "sich", "Luft", "bei", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie dringt durch Th\u00fcren, Schr\u00e4nk' und W\u00e4nde,", "tokens": ["Sie", "dringt", "durch", "Th\u00fc\u00b7ren", ",", "Schr\u00e4nk'", "und", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df man noch aus dem Schlaf erwacht.", "tokens": ["Da\u00df", "man", "noch", "aus", "dem", "Schlaf", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du stehest auf, du legst dich nieder,", "tokens": ["Du", "ste\u00b7hest", "auf", ",", "du", "legst", "dich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du h\u00f6rst vom freien deutschen Rhein,", "tokens": ["Du", "h\u00f6rst", "vom", "frei\u00b7en", "deut\u00b7schen", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wachest auf und h\u00f6rest wieder", "tokens": ["Du", "wa\u00b7chest", "auf", "und", "h\u00f6\u00b7rest", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom freien deutschen Rheine schrei'n.", "tokens": ["Vom", "frei\u00b7en", "deut\u00b7schen", "Rhei\u00b7ne", "schrei'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPRART", "ADJA", "ADJA", "NE", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du magst nun ruhen, gehen, traben,", "tokens": ["Du", "magst", "nun", "ru\u00b7hen", ",", "ge\u00b7hen", ",", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du h\u00f6rst in tausend Melodein:", "tokens": ["Du", "h\u00f6rst", "in", "tau\u00b7send", "Me\u00b7lo\u00b7dein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201esie sollen, sollen ihn nicht haben!\u201c", "tokens": ["\u201e", "sie", "sol\u00b7len", ",", "sol\u00b7len", "ihn", "nicht", "ha\u00b7ben", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "$,", "VMFIN", "PPER", "PTKNEG", "VAINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Tilsit bis nach Wesel schrei'n.", "tokens": ["Von", "Til\u00b7sit", "bis", "nach", "We\u00b7sel", "schrei'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "NE", "ADV", "APPR", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ganz Deutschland singt \u2014 und unterdessen,", "tokens": ["Ganz", "Deutschland", "singt", "und", "un\u00b7ter\u00b7des\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "$(", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der liebe freie deutsche Rhein!", "tokens": ["Der", "lie\u00b7be", "frei\u00b7e", "deut\u00b7sche", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da schmei\u00dfen unsre blinden Hessen", "tokens": ["Da", "schmei\u00b7\u00dfen", "uns\u00b7re", "blin\u00b7den", "Hes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm Quaderstein' ins Bett' hinein.", "tokens": ["Ihm", "Qua\u00b7der\u00b7stein'", "ins", "Bett'", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}