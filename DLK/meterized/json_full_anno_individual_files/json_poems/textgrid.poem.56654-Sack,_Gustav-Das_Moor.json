{"textgrid.poem.56654": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Das Moor", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh du Geliebte, wenn ich je ged\u00e4chte,", "tokens": ["Oh", "du", "Ge\u00b7lieb\u00b7te", ",", "wenn", "ich", "je", "ge\u00b7d\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dich einem Erdendinge zu vergleichen,", "tokens": ["dich", "ei\u00b7nem", "Er\u00b7den\u00b7din\u00b7ge", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "so w\u00e4hlte ich den Berg unz\u00e4hliger Leichen,", "tokens": ["so", "w\u00e4hl\u00b7te", "ich", "den", "Berg", "un\u00b7z\u00e4h\u00b7li\u00b7ger", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "so w\u00e4hlte ich das Moor und seine N\u00e4chte.", "tokens": ["so", "w\u00e4hl\u00b7te", "ich", "das", "Moor", "und", "sei\u00b7ne", "N\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du schmutziger Kn\u00e4uel bodenloser Sch\u00e4chte", "tokens": ["Du", "schmut\u00b7zi\u00b7ger", "Kn\u00e4u\u00b7el", "bo\u00b7den\u00b7lo\u00b7ser", "Sch\u00