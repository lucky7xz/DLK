{"dta.poem.4373": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die vern\u00fcnftige und unvern\u00fcnftige  \n Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von allen Leidenschaften scheinet die Liebe die nat\u00fcr-", "tokens": ["Von", "al\u00b7len", "Lei\u00b7den\u00b7schaf\u00b7ten", "schei\u00b7net", "die", "Lie\u00b7be", "die", "na\u00b7t\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "lichste", "tokens": ["lichs\u00b7te"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "(weil sie nicht weniger den Geist, als wie den Leib, ja", "tokens": ["(", "weil", "sie", "nicht", "we\u00b7ni\u00b7ger", "den", "Geist", ",", "als", "wie", "den", "Leib", ",", "ja"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "nicht allein", "tokens": ["nicht", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["PTKNEG", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Des Liebenden, auch der Geliebten, zu ihrem Vorwurf", "tokens": ["Des", "Lie\u00b7ben\u00b7den", ",", "auch", "der", "Ge\u00b7lieb\u00b7ten", ",", "zu", "ih\u00b7rem", "Vor\u00b7wurf"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.6": {"text": "hat) zu seyn.", "tokens": ["hat", ")", "zu", "seyn", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$(", "PTKZU", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Nun ist bekannt, da\u00df jeder Mensch zugleich aus Leib und", "tokens": ["Nun", "ist", "be\u00b7kannt", ",", "da\u00df", "je\u00b7der", "Mensch", "zu\u00b7gleich", "aus", "Leib", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "$,", "KOUS", "PIAT", "NN", "ADV", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Geist besteh,", "tokens": ["Geist", "be\u00b7steh", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Daher besch\u00e4ftigt er sich ganz, indem er liebt, und seine", "tokens": ["Da\u00b7her", "be\u00b7sch\u00e4f\u00b7tigt", "er", "sich", "ganz", ",", "in\u00b7dem", "er", "liebt", ",", "und", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "PPOSAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Liebe", "tokens": ["Lie\u00b7be"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Genie\u00dft noch \u00fcberdem des Geists und Leibes s\u00fcsse Wech-", "tokens": ["Ge\u00b7nie\u00dft", "noch", "\u00fc\u00b7ber\u00b7dem", "des", "Geists", "und", "Lei\u00b7bes", "s\u00fcs\u00b7se", "Wech"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "KON", "NN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "sel-Triebe", "tokens": ["sel\u00b7Trie\u00b7be"], "token_info": ["word"], "pos": ["ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "De\u00df, der von ihm geliebet wird. Wann Liebende nun", "tokens": ["De\u00df", ",", "der", "von", "ihm", "ge\u00b7lie\u00b7bet", "wird", ".", "Wann", "Lie\u00b7ben\u00b7de", "nun"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PRELS", "APPR", "PPER", "VVPP", "VAFIN", "$.", "PWAV", "NN", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.14": {"text": "solche Gaben,", "tokens": ["sol\u00b7che", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Da\u00df sie mit Recht vermeynen k\u00f6nnen, durch ihre c\u00f6rper-", "tokens": ["Da\u00df", "sie", "mit", "Recht", "ver\u00b7mey\u00b7nen", "k\u00f6n\u00b7nen", ",", "durch", "ih\u00b7re", "c\u00f6\u00b7rper"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "VMINF", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "liche Zier,", "tokens": ["li\u00b7che", "Zier", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Durch Sch\u00f6nheit, St\u00e4rke, Stand und Ansehn, den sch\u00f6-", "tokens": ["Durch", "Sch\u00f6n\u00b7heit", ",", "St\u00e4r\u00b7ke", ",", "Stand", "und", "An\u00b7sehn", ",", "den", "sch\u00f6"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "nen Vorwurf der Begier", "tokens": ["nen", "Vor\u00b7wurf", "der", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Dadurch sich angenehm zu machen, und ihn zu reizen, an", "tokens": ["Da\u00b7durch", "sich", "an\u00b7ge\u00b7nehm", "zu", "ma\u00b7chen", ",", "und", "ihn", "zu", "rei\u00b7zen", ",", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "PRF", "ADJD", "PTKZU", "VVINF", "$,", "KON", "PPER", "PTKZU", "VVINF", "$,", "APPR"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "sich haben,", "tokens": ["sich", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VAFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Zugleich, bey ihrem Gegenwurf, desgleichen auch f\u00fcr sich", "tokens": ["Zu\u00b7gleich", ",", "bey", "ih\u00b7rem", "Ge\u00b7gen\u00b7wurf", ",", "des\u00b7glei\u00b7chen", "auch", "f\u00fcr", "sich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "APPR", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "zu finden,", "tokens": ["zu", "fin\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Mit Fug und Recht, vermuhten k\u00f6nnen; so wird in ihrer", "tokens": ["Mit", "Fug", "und", "Recht", ",", "ver\u00b7muh\u00b7ten", "k\u00f6n\u00b7nen", ";", "so", "wird", "in", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVINF", "VMINF", "$.", "ADV", "VAFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "beyder Brust,", "tokens": ["bey\u00b7der", "Brust", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Sowohl so gleich, als im Besitz, ein\u2019 angenehme Wech-", "tokens": ["So\u00b7wohl", "so", "gleich", ",", "als", "im", "Be\u00b7sitz", ",", "ein'", "an\u00b7ge\u00b7neh\u00b7me", "Wech"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$,", "KOUS", "APPRART", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "sel-Lust,", "tokens": ["sel\u00b7Lust", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Mit Banden, voller s\u00fcssen Anmuht, die nimmer ecklen", "tokens": ["Mit", "Ban\u00b7den", ",", "vol\u00b7ler", "s\u00fcs\u00b7sen", "An\u00b7muht", ",", "die", "nim\u00b7mer", "eck\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "ADJA", "ADJA", "NN", "$,", "PRELS", "ADV", "VVINF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Herzen binden.", "tokens": ["Her\u00b7zen", "bin\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Kommt nun die Gleichheit der Gem\u00fchter zu ihrer Anmuht", "tokens": ["Kommt", "nun", "die", "Gleich\u00b7heit", "der", "Ge\u00b7m\u00fch\u00b7ter", "zu", "ih\u00b7rer", "An\u00b7muht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "noch hinzu,", "tokens": ["noch", "hin\u00b7zu", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "So findet sich die meistens sonst umsonst gesuchte Seelen-", "tokens": ["So", "fin\u00b7det", "sich", "die", "meis\u00b7tens", "sonst", "um\u00b7sonst", "ge\u00b7such\u00b7te", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADV", "ADV", "ADV", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Ruh", "tokens": ["Ruh"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Bey beyden \u00fcberschwenglich ein: Zumahl die letztere noch", "tokens": ["Bey", "bey\u00b7den", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "ein", ":", "Zu\u00b7mahl", "die", "letz\u00b7te\u00b7re", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJD", "PTKVZ", "$.", "KOUS", "ART", "PIS", "ADV"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "w\u00e4hret,", "tokens": ["w\u00e4h\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Wenn etwan unsers Leibes Vorzug, als welcher fl\u00fcchtig,", "tokens": ["Wenn", "et\u00b7wan", "un\u00b7sers", "Lei\u00b7bes", "Vor\u00b7zug", ",", "als", "wel\u00b7cher", "fl\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "NN", "$,", "KOUS", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "uns entf\u00e4hret,", "tokens": ["uns", "ent\u00b7f\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Und durch sich selber f\u00e4hig ist, wofern sie billig, an den", "tokens": ["Und", "durch", "sich", "sel\u00b7ber", "f\u00e4\u00b7hig", "ist", ",", "wo\u00b7fern", "sie", "bil\u00b7lig", ",", "an", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PRF", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ADJD", "$,", "APPR", "ART"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Z\u00fcgen,", "tokens": ["Z\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Die die Geliebte einst besessen, sich, im Erinnern, zu ver-", "tokens": ["Die", "die", "Ge\u00b7lieb\u00b7te", "einst", "be\u00b7ses\u00b7sen", ",", "sich", ",", "im", "E\u00b7rin\u00b7nern", ",", "zu", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$,", "PRF", "$,", "APPRART", "NN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-++-+-+-", "measure": "unknown.measure.octa.plus"}, "line.12": {"text": "gn\u00fcgen,", "tokens": ["gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Wenn dessen Geistes Sch\u00f6nheit nur der c\u00f6rperlichen Platz", "tokens": ["Wenn", "des\u00b7sen", "Geis\u00b7tes", "Sch\u00f6n\u00b7heit", "nur", "der", "c\u00f6r\u00b7per\u00b7li\u00b7chen", "Platz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "ADJA", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+---+", "measure": "unknown.measure.hexa"}, "line.14": {"text": "vertritt,", "tokens": ["ver\u00b7tritt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Und seine Freund- und Z\u00e4rtlichkeit das, was die Zeit ge-", "tokens": ["Und", "sei\u00b7ne", "Freun\u00b7d", "und", "Z\u00e4rt\u00b7lich\u00b7keit", "das", ",", "was", "die", "Zeit", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "TRUNC", "KON", "NN", "PDS", "$,", "PRELS", "ART", "NN", "TRUNC"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "raubt, ersetzet,", "tokens": ["raubt", ",", "er\u00b7set\u00b7zet", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Indem sie nicht den andern nur, zugleich in seiner Lust,", "tokens": ["In\u00b7dem", "sie", "nicht", "den", "an\u00b7dern", "nur", ",", "zu\u00b7gleich", "in", "sei\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "ADJA", "ADV", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "sich mit-", "tokens": ["sich", "mit"], "token_info": ["word", "word"], "pos": ["PRF", "TRUNC"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Ja, wo er recht vern\u00fcnftig ist, sich \u00f6fters mehr, als er,", "tokens": ["Ja", ",", "wo", "er", "recht", "ver\u00b7n\u00fcnf\u00b7tig", "ist", ",", "sich", "\u00f6f\u00b7ters", "mehr", ",", "als", "er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$,", "PRF", "ADV", "ADV", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+-++-", "measure": "unknown.measure.septa"}, "line.20": {"text": "ergetzet.", "tokens": ["er\u00b7get\u00b7zet", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Wann aber jemand, welcher liebet, von solchen Gaben", "tokens": ["Wann", "a\u00b7ber", "je\u00b7mand", ",", "wel\u00b7cher", "lie\u00b7bet", ",", "von", "sol\u00b7chen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIS", "$,", "PRELS", "VVFIN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "nichts besitzt,", "tokens": ["nichts", "be\u00b7sitzt", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wie kann er denn mit Recht begehren vom Gegentheil", "tokens": ["Wie", "kann", "er", "denn", "mit", "Recht", "be\u00b7geh\u00b7ren", "vom", "Ge\u00b7gen\u00b7theil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "geliebt zu seyn?", "tokens": ["ge\u00b7liebt", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Zwar findet sich, da\u00df in der Ehe zuweilen die Gewohn-", "tokens": ["Zwar", "fin\u00b7det", "sich", ",", "da\u00df", "in", "der", "E\u00b7he", "zu\u00b7wei\u00b7len", "die", "Ge\u00b7wohn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "$,", "KOUS", "APPR", "ART", "NN", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "heit n\u00fctzt;", "tokens": ["heit", "n\u00fctzt", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Doch dienet sie zur Liebe selten, nur zur Ertr\u00e4glichkeit", "tokens": ["Doch", "die\u00b7net", "sie", "zur", "Lie\u00b7be", "sel\u00b7ten", ",", "nur", "zur", "Er\u00b7tr\u00e4g\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "allein.", "tokens": ["al\u00b7lein", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Es wird denn die\u00df von selbsten folgen, und ist das Sprich-", "tokens": ["Es", "wird", "denn", "die\u00df", "von", "selbs\u00b7ten", "fol\u00b7gen", ",", "und", "ist", "das", "Sprich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PDS", "APPR", "ADJA", "VVINF", "$,", "KON", "VAFIN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "wort wahr vor allen:", "tokens": ["wort", "wahr", "vor", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Ein jeder mu\u00df gef\u00e4llig seyn, wo er gedenket zu gefallen.", "tokens": ["Ein", "je\u00b7der", "mu\u00df", "ge\u00b7f\u00e4l\u00b7lig", "seyn", ",", "wo", "er", "ge\u00b7den\u00b7ket", "zu", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ADJD", "VAINF", "$,", "PWAV", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.pl