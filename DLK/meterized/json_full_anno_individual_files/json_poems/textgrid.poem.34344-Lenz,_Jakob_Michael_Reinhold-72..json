{"textgrid.poem.34344": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Darf eine fremde Hand ged\u00e4mpfte Sayten schlagen", "tokens": ["Darf", "ei\u00b7ne", "frem\u00b7de", "Hand", "ge\u00b7d\u00e4mpf\u00b7te", "Say\u00b7ten", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier, wo dein hoher Schmerz die Gegend schweigen macht \u2013", "tokens": ["Hier", ",", "wo", "dein", "ho\u00b7her", "Schmerz", "die", "Ge\u00b7gend", "schwei\u00b7gen", "macht", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Prinzessin! oft hat todter B\u00e4ume Klagen", "tokens": ["Prin\u00b7zes\u00b7sin", "!", "oft", "hat", "tod\u00b7ter", "B\u00e4u\u00b7me", "Kla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VAFIN", "ADJA", "NN", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In wunde Herzen Trost gebracht.", "tokens": ["In", "wun\u00b7de", "Her\u00b7zen", "Trost", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Darf ich es nennen, was in seufzenden Alleen", "tokens": ["Darf", "ich", "es", "nen\u00b7nen", ",", "was", "in", "seuf\u00b7zen\u00b7den", "Al\u00b7leen"