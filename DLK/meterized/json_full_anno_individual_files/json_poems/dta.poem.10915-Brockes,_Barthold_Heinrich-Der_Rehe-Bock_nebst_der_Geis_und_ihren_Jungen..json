{"dta.poem.10915": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Rehe-Bock  \n nebst der Geis und ihren Jungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier weis wirklich unser Geist sich so bald nicht zu ent-\nschliessen,", "tokens": ["Hier", "weis", "wirk\u00b7lich", "un\u00b7ser", "Geist", "sich", "so", "bald", "nicht", "zu", "ent", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "ADJD", "PPOSAT", "NN", "PRF", "ADV", "ADV", "PTKNEG", "APPR", "TRUNC", "VVINF", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ob er sehn soll, oder h\u00f6ren. L\u00e4\u00dft er seine Blicke schiessen,", "tokens": ["Ob", "er", "sehn", "soll", ",", "o\u00b7der", "h\u00f6\u00b7ren", ".", "L\u00e4\u00dft", "er", "sei\u00b7ne", "Bli\u00b7cke", "schies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "KON", "VVINF", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Auf die Thiere, die sich regen, auf die Kunst und die Natur,", "tokens": ["Auf", "die", "Thie\u00b7re", ",", "die", "sich", "re\u00b7gen", ",", "auf", "die", "Kunst", "und", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "ADJA", "$,", "APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Die in diesem pr\u00e4chtgen Garten, so durch Farben, als Figur,", "tokens": ["Die", "in", "die\u00b7sem", "pr\u00e4cht\u00b7gen", "Gar\u00b7ten", ",", "so", "durch", "Far\u00b7ben", ",", "als", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Majest\u00e4tisch sich verbinden; reizt ein angenehmer Schall,", "tokens": ["Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "sich", "ver\u00b7bin\u00b7den", ";", "reizt", "ein", "an\u00b7ge\u00b7neh\u00b7mer", "Schall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$.", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Reizt ein sprudelndes Get\u00f6s\u2019, in dem weissen Wasserfall,", "tokens": ["Reizt", "ein", "spru\u00b7deln\u00b7des", "Ge\u00b7t\u00f6s'", ",", "in", "dem", "weis\u00b7sen", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Durchs Geh\u00f6r sich zu vergn\u00fcgen. Ja noch mehr, das laute", "tokens": ["Durchs", "Ge\u00b7h\u00f6r", "sich", "zu", "ver\u00b7gn\u00fc\u00b7gen", ".", "Ja", "noch", "mehr", ",", "das", "lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PRF", "PTKZU", "VVINF", "$.", "NN", "ADV", "ADV", "$,", "PDS", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Und das schmatzende Get\u00f6n des gesch\u00e4fftgen B\u00f6ckleins hemmt", "tokens": ["Und", "das", "schmat\u00b7zen\u00b7de", "Ge\u00b7t\u00f6n", "des", "ge\u00b7sch\u00e4fft\u00b7gen", "B\u00f6c\u00b7kleins", "hemmt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Jhm, durchs Ohr, noch eine zeitlang dem Gebrauch der frohen", "tokens": ["Jhm", ",", "durchs", "Ohr", ",", "noch", "ei\u00b7ne", "zeit\u00b7lang", "dem", "Ge\u00b7brauch", "der", "fro\u00b7hen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "APPRART", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Endlich aber sieht er wieder. Seht, wie sich das Thierchen stremmt", "tokens": ["End\u00b7lich", "a\u00b7ber", "sieht", "er", "wie\u00b7der", ".", "Seht", ",", "wie", "sich", "das", "Thier\u00b7chen", "stremmt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$.", "VVFIN", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Und mit kurzen St\u00f6\u00dfen zieht! In der kleinen Schenkel Biegen,", "tokens": ["Und", "mit", "kur\u00b7zen", "St\u00f6\u00b7\u00dfen", "zieht", "!", "In", "der", "klei\u00b7nen", "Schen\u00b7kel", "Bie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$.", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Sieht man eine sanfte Lust. Und ein m\u00fctterlichs Vergn\u00fcgen,", "tokens": ["Sieht", "man", "ei\u00b7ne", "sanf\u00b7te", "Lust", ".", "Und", "ein", "m\u00fct\u00b7ter\u00b7lichs", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$.", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Ein empfindliches Gef\u00fchl, in der Geis Zung, Aug und Ohr,", "tokens": ["Ein", "emp\u00b7find\u00b7li\u00b7ches", "Ge\u00b7f\u00fchl", ",", "in", "der", "Geis", "Zung", ",", "Aug", "und", "Ohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Was ihr Innerstes empfindet, giebt sie \u00e4u\u00dferlich hervor,", "tokens": ["Was", "ihr", "In\u00b7ners\u00b7tes", "emp\u00b7fin\u00b7det", ",", "giebt", "sie", "\u00e4u\u00b7\u00dfer\u00b7lich", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Und man siehet, was sie f\u00fchlet. Seht, welch einen ernsten Geist", "tokens": ["Und", "man", "sie\u00b7het", ",", "was", "sie", "f\u00fch\u00b7let", ".", "Seht", ",", "welch", "ei\u00b7nen", "erns\u00b7ten", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$.", "VVFIN", "$,", "PWAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Der gestreckte Rehe-Bock, der der sanften Ruh geneust,", "tokens": ["Der", "ge\u00b7streck\u00b7te", "Re\u00b7he\u00b7Bock", ",", "der", "der", "sanf\u00b7ten", "Ruh", "ge\u00b7neust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Welchen muntern seine Zucht, in der raschen Stellung, weist.", "tokens": ["Wel\u00b7chen", "mun\u00b7tern", "sei\u00b7ne", "Zucht", ",", "in", "der", "ra\u00b7schen", "Stel\u00b7lung", ",", "weist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAT", "NN", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Aber seht doch auch dabey, wenn ihr diesen Abdruck sehet,", "tokens": ["A\u00b7ber", "seht", "doch", "auch", "da\u00b7bey", ",", "wenn", "ihr", "die\u00b7sen", "Ab\u00b7druck", "se\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PAV", "$,", "KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Wie er, nebst dem sch\u00f6nen Urbild, unsers Sch\u00f6pfers Macht", "tokens": ["Wie", "er", ",", "nebst", "dem", "sch\u00f6\u00b7nen", "Ur\u00b7bild", ",", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}