{"textgrid.poem.67225": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn Menschen sich aus innrem Werte kennen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Menschen sich aus innrem Werte kennen,", "tokens": ["Wenn", "Men\u00b7schen", "sich", "aus", "inn\u00b7rem", "Wer\u00b7te", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So k\u00f6nnen sie sich freudig Freunde nennen,", "tokens": ["So", "k\u00f6n\u00b7nen", "sie", "sich", "freu\u00b7dig", "Freun\u00b7de", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Leben ist den Menschen so bekannter,", "tokens": ["Das", "Le\u00b7ben", "ist", "den", "Men\u00b7schen", "so", "be\u00b7kann\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie finden es im Geist interessanter.", "tokens": ["Sie", "fin\u00b7den", "es", "im", "Geist", "in\u00b7ter\u00b7es\u00b7san\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der hohe Geist ist nicht der Freundschaft ferne,", "tokens": ["Der", "ho\u00b7he", "Geist", "ist", "nicht", "der", "Freund\u00b7schaft", "fer\u00b7ne", ","], "token_i