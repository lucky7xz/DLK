{"dta.poem.11517": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Rath einer Alten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.57", "nl:0.42"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Bin jung gewesen,", "tokens": ["Bin", "jung", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VAPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Kann auch mit reden,", "tokens": ["Kann", "auch", "mit", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und alt geworden,", "tokens": ["Und", "alt", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Drum gilt mein Wort.", "tokens": ["Drum", "gilt", "mein", "Wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n reife Beeren", "tokens": ["Sch\u00f6n", "rei\u00b7fe", "Bee\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "---+-", "measure": "unknown.measure.single"}, "line.2": {"text": "Am B\u00e4umchen hangen:", "tokens": ["Am", "B\u00e4um\u00b7chen", "han\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nachbar, da hilft kein", "tokens": ["Nach\u00b7bar", ",", "da", "hilft", "kein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PIAT"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Zaun um den Garten;", "tokens": ["Zaun", "um", "den", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Lustige V\u00f6gel", "tokens": ["Lus\u00b7ti\u00b7ge", "V\u00f6\u00b7gel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wissen den Weg.", "tokens": ["Wis\u00b7sen", "den", "Weg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Aber, mein Dirnchen,", "tokens": ["A\u00b7ber", ",", "mein", "Dirn\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Du la\u00df dir rathen:", "tokens": ["Du", "la\u00df", "dir", "ra\u00b7then", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Halte dein Sch\u00e4tzchen", "tokens": ["Hal\u00b7te", "dein", "Sch\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wohl in der Liebe,", "tokens": ["Wohl", "in", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wohl im Respekt!", "tokens": ["Wohl", "im", "Res\u00b7pekt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Mit den zwei F\u00e4dlein,", "tokens": ["Mit", "den", "zwei", "F\u00e4d\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "In Eins gedrehet,", "tokens": ["In", "Eins", "ge\u00b7dre\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ziehst du am kleinen", "tokens": ["Ziehst", "du", "am", "klei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Finger ihn nach.", "tokens": ["Fin\u00b7ger", "ihn", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Aufrichtig Herze,", "tokens": ["Auf\u00b7rich\u00b7tig", "Her\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Doch schweigen k\u00f6nnen,", "tokens": ["Doch", "schwei\u00b7gen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Fr\u00fch mit der Sonne", "tokens": ["Fr\u00fch", "mit", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Muthig zur Arbeit,", "tokens": ["Mut\u00b7hig", "zur", "Ar\u00b7beit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Gesunde Glieder,", "tokens": ["Ge\u00b7sun\u00b7de", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Saubere Linnen,", "tokens": ["Sau\u00b7be\u00b7re", "Lin\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Das machet M\u00e4dchen", "tokens": ["Das", "ma\u00b7chet", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und Weibchen werth.", "tokens": ["Und", "Weib\u00b7chen", "werth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}