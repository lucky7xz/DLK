{"dta.poem.20756": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Doktor Kr\u00fcnitz  \n wegen ihres Pettschafts.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du druckest Dir das Siegel ab,               ", "tokens": ["Du", "dru\u00b7ckest", "Dir", "das", "Sie\u00b7gel", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das mir ein gro\u00dfer Mann geschenket,", "tokens": ["Das", "mir", "ein", "gro\u00b7\u00dfer", "Mann", "ge\u00b7schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und von dem Geist, den die Natur mir gab,", "tokens": ["Und", "von", "dem", "Geist", ",", "den", "die", "Na\u00b7tur", "mir", "gab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der in mir lebt und in mir denket,", "tokens": ["Der", "in", "mir", "lebt", "und", "in", "mir", "den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "KON", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist jedes Lied die Abkopie;", "tokens": ["Ist", "je\u00b7des", "Lied", "die", "Ab\u00b7ko\u00b7pie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du kleiner Geizhals sammlest sie", "tokens": ["Du", "klei\u00b7ner", "Geiz\u00b7hals", "samm\u00b7lest", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit gr\u00f6\u00dferm Eifer als die Prinzen", "tokens": ["Mit", "gr\u00f6\u00b7\u00dferm", "Ei\u00b7fer", "als", "die", "Prin\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu C\u00e4sars Zeit geschlagne M\u00fcnzen.", "tokens": ["Zu", "C\u00e4\u00b7sars", "Zeit", "ge\u00b7schlag\u00b7ne", "M\u00fcn\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}