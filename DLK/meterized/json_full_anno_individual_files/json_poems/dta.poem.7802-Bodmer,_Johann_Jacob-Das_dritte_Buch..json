{"dta.poem.7802": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Buch.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ein verschlo\u00dfner Wind, wann er gewinnet Luft,", "tokens": ["Wie", "ein", "ver\u00b7schlo\u00df\u00b7ner", "Wind", ",", "wann", "er", "ge\u00b7win\u00b7net", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bricht lo\u00df mit Ungest\u00fcm aus der verschlo\u00dfnen Kluft;", "tokens": ["Bricht", "lo\u00df", "mit", "Un\u00b7ge\u00b7st\u00fcm", "aus", "der", "ver\u00b7schlo\u00df\u00b7nen", "Kluft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So gieng es auch allhie. Des K\u00f6nigs Trauerwesen,", "tokens": ["So", "gieng", "es", "auch", "all\u00b7hie", ".", "Des", "K\u00f6\u00b7nigs", "Trau\u00b7er\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$.", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das da in dieser Zeit war wie versperrt gewesen,", "tokens": ["Das", "da", "in", "die\u00b7ser", "Zeit", "war", "wie", "ver\u00b7sperrt", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PDAT", "NN", "VAFIN", "KOKOM", "VVPP", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In eingebildter Ruh, in f\u00fcrgemalter Ehr,", "tokens": ["In", "ein\u00b7ge\u00b7bild\u00b7ter", "Ruh", ",", "in", "f\u00fcr\u00b7ge\u00b7mal\u00b7ter", "Ehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Brach pl\u00f6tzlich wieder aus und tobete daher.", "tokens": ["Brach", "pl\u00f6tz\u00b7lich", "wie\u00b7der", "aus", "und", "to\u00b7be\u00b7te", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "PTKVZ", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja wie ein tr\u00fcb Gew\u00fclck den Himmel schnell beziehet,", "tokens": ["Ja", "wie", "ein", "tr\u00fcb", "Ge\u00b7w\u00fclck", "den", "Him\u00b7mel", "schnell", "be\u00b7zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOKOM", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So da\u00df die helle Sonn im Augenblick entfliehet,", "tokens": ["So", "da\u00df", "die", "hel\u00b7le", "Sonn", "im", "Au\u00b7gen\u00b7blick", "ent\u00b7flie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So trieben diese Wort auf einmahl hin die Lust.", "tokens": ["So", "trie\u00b7ben", "die\u00b7se", "Wort", "auf", "ein\u00b7mahl", "hin", "die", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "APPR", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein tr\u00fcber dunckler Schein erwiese, wie entrust", "tokens": ["Ein", "tr\u00fc\u00b7ber", "dunck\u00b7ler", "Schein", "er\u00b7wie\u00b7se", ",", "wie", "ent\u00b7rust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Des K\u00f6nigs Hertze war; das hube an zu springen,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Hert\u00b7ze", "war", ";", "das", "hu\u00b7be", "an", "zu", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VAFIN", "$.", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein Eiferfeur erhitzt die Augen da\u00df sie dringen", "tokens": ["Ein", "Ei\u00b7fer\u00b7feur", "er\u00b7hitzt", "die", "Au\u00b7gen", "da\u00df", "sie", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit Grimm auf Michal zu: die nimmt es nicht in Acht,", "tokens": ["Mit", "Grimm", "auf", "Mic\u00b7hal", "zu", ":", "die", "nimmt", "es", "nicht", "in", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "PTKVZ", "$.", "ART", "VVFIN", "PPER", "PTKNEG", "APPR", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie ist in Unschuld nur auf Davids Lob bedacht.", "tokens": ["Sie", "ist", "in", "Un\u00b7schuld", "nur", "auf", "Da\u00b7vids", "Lob", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das Lied wird wiederholt, es geht in beyden Choren", "tokens": ["Das", "Lied", "wird", "wie\u00b7der\u00b7holt", ",", "es", "geht", "in", "bey\u00b7den", "Cho\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Zwar lieblich, aber doch in Sauls gespitzten Ohren", "tokens": ["Zwar", "lieb\u00b7lich", ",", "a\u00b7ber", "doch", "in", "Sauls", "ge\u00b7spitz\u00b7ten", "Oh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Schallt es erschrecklich hin; der schaut mit Unmuth an", "tokens": ["Schallt", "es", "er\u00b7schreck\u00b7lich", "hin", ";", "der", "schaut", "mit", "Un\u00b7muth", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "ART", "VVFIN", "APPR", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Ehr die David wird erzeigt von jedermann.", "tokens": ["Die", "Ehr", "die", "Da\u00b7vid", "wird", "er\u00b7zeigt", "von", "je\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "VAFIN", "VVPP", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Jedoch da\u00df keiner nicht m\u00f6g diese Schwachheit mercken,", "tokens": ["Je\u00b7doch", "da\u00df", "kei\u00b7ner", "nicht", "m\u00f6g", "die\u00b7se", "Schwach\u00b7heit", "mer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "PTKNEG", "VMFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Erholet er sich bald; sein F\u00fcrsatz kan ihn st\u00e4rcken,", "tokens": ["Er\u00b7ho\u00b7let", "er", "sich", "bald", ";", "sein", "F\u00fcr\u00b7satz", "kan", "ihn", "st\u00e4r\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$.", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie er sich r\u00e4chen woll; drauf gr\u00fcst er alle Schaar", "tokens": ["Wie", "er", "sich", "r\u00e4\u00b7chen", "woll", ";", "drauf", "gr\u00fcst", "er", "al\u00b7le", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$.", "PAV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So Haufenwei\u00df am Thor der Stadt versammelt war.", "tokens": ["So", "Hau\u00b7fen\u00b7wei\u00df", "am", "Thor", "der", "Stadt", "ver\u00b7sam\u00b7melt", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}