{"dta.poem.20695": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den ber\u00fchmten Maler  \n Herrn Rode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch G\u00f6tter \u00e4rgern sich. Von eines Aergers Glut                 ", "tokens": ["Auch", "G\u00f6t\u00b7ter", "\u00e4r\u00b7gern", "sich", ".", "Von", "ei\u00b7nes", "A\u00b7er\u00b7gers", "Glut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PRF", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Wird Amor selbst einmal entbrannt,", "tokens": ["Wird", "A\u00b7mor", "selbst", "ein\u00b7mal", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schwarze Galle kocht sein rosinfarbnes Blut,", "tokens": ["Und", "schwar\u00b7ze", "Gal\u00b7le", "kocht", "sein", "ro\u00b7sin\u00b7farb\u00b7nes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sein Gesicht ist braun, und schwach ist sein Verstand.", "tokens": ["Und", "sein", "Ge\u00b7sicht", "ist", "braun", ",", "und", "schwach", "ist", "sein", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O Himmel, Himmel! seufzt die arme Venus schon,", "tokens": ["O", "Him\u00b7mel", ",", "Him\u00b7mel", "!", "seufzt", "die", "ar\u00b7me", "Ve\u00b7nus", "schon", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$.", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach ich verliere meinen Sohn!", "tokens": ["Ach", "ich", "ver\u00b7lie\u00b7re", "mei\u00b7nen", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit schnellen Schritten kommt der Doctor Aeskulap", "tokens": ["Mit", "schnel\u00b7len", "Schrit\u00b7ten", "kommt", "der", "Doc\u00b7tor", "A\u00b7es\u00b7ku\u00b7lap"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Und stehet vor des Knaben Bette,", "tokens": ["Und", "ste\u00b7het", "vor", "des", "Kna\u00b7ben", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Begreifet seinen Puls, spricht ihm das Leben ab,", "tokens": ["Be\u00b7grei\u00b7fet", "sei\u00b7nen", "Puls", ",", "spricht", "ihm", "das", "Le\u00b7ben", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Begreift ihn noch einmal, besinnt sich, spricht: \u201eich rette", "tokens": ["Be\u00b7greift", "ihn", "noch", "ein\u00b7mal", ",", "be\u00b7sinnt", "sich", ",", "spricht", ":", "\u201e", "ich", "ret\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "VVFIN", "PRF", "$,", "VVFIN", "$.", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den kleinen allerliebsten Sohn", "tokens": ["Den", "klei\u00b7nen", "al\u00b7ler\u00b7liebs\u00b7ten", "Sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mehr als einen Ku\u00df verlang ich nicht zum Lohn;", "tokens": ["Und", "mehr", "als", "ei\u00b7nen", "Ku\u00df", "ver\u00b7lang", "ich", "nicht", "zum", "Lohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ist ein kleines Gallenfieber,", "tokens": ["Es", "ist", "ein", "klei\u00b7nes", "Gal\u00b7len\u00b7fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In einem Tag ist es vor\u00fcber.", "tokens": ["In", "ei\u00b7nem", "Tag", "ist", "es", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Kribbelk\u00f6pfchen ist der kleine liebe Sohn,", "tokens": ["Ein", "Krib\u00b7bel\u00b7k\u00f6pf\u00b7chen", "ist", "der", "klei\u00b7ne", "lie\u00b7be", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht selten l\u00e4uft die Gall ihm \u00fcber,", "tokens": ["Nicht", "sel\u00b7ten", "l\u00e4uft", "die", "Gall", "ihm", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "ART", "NN", "PPER", "APPR", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein er stirbt doch nicht davon,", "tokens": ["Al\u00b7lein", "er", "stirbt", "doch", "nicht", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vielmehr verd\u00fcnnt sie ihm den dickgewordnen Saft,", "tokens": ["Viel\u00b7mehr", "ver\u00b7d\u00fcnnt", "sie", "ihm", "den", "dick\u00b7ge\u00b7word\u00b7nen", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Macht seine Nieren rein, und giebt ihm neue Kraft.", "tokens": ["Macht", "sei\u00b7ne", "Nie\u00b7ren", "rein", ",", "und", "giebt", "ihm", "neu\u00b7e", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$,", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bed\u00e4chtig sprach also der G\u00f6tter Medicus;", "tokens": ["Be\u00b7d\u00e4ch\u00b7tig", "sprach", "al\u00b7so", "der", "G\u00f6t\u00b7ter", "Me\u00b7di\u00b7cus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Knabe ward gesund, und Venus gab den Ku\u00df!", "tokens": ["Der", "Kna\u00b7be", "ward", "ge\u00b7sund", ",", "und", "Ve\u00b7nus", "gab", "den", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die G\u00f6tter spotteten, da sie den Ku\u00df ihm gab.", "tokens": ["Die", "G\u00f6t\u00b7ter", "spot\u00b7te\u00b7ten", ",", "da", "sie", "den", "Ku\u00df", "ihm", "gab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit Dank nahm er ihn an und bat um einen noch,", "tokens": ["Mit", "Dank", "nahm", "er", "ihn", "an", "und", "bat", "um", "ei\u00b7nen", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und lachend, wie ein Schalk, rief Amor: Sehet doch,", "tokens": ["Und", "la\u00b7chend", ",", "wie", "ein", "Schalk", ",", "rief", "A\u00b7mor", ":", "Se\u00b7het", "doch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "NE", "$.", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Mutter k\u00fc\u00dft den Aeskulap.", "tokens": ["Die", "Mut\u00b7ter", "k\u00fc\u00dft", "den", "A\u00b7es\u00b7ku\u00b7lap", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Mein Rhode sage mir, was willst Du lieber malen:", "tokens": ["Mein", "Rho\u00b7de", "sa\u00b7ge", "mir", ",", "was", "willst", "Du", "lie\u00b7ber", "ma\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie ernsthaft Aeskulap beim kranken Amor ist?", "tokens": ["Wie", "ernst\u00b7haft", "A\u00b7es\u00b7ku\u00b7lap", "beim", "kran\u00b7ken", "A\u00b7mor", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NE", "APPRART", "NN", "NE", "VAFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Wie? oder wie ihn Venus k\u00fc\u00dft?", "tokens": ["Wie", "?", "o\u00b7der", "wie", "ihn", "Ve\u00b7nus", "k\u00fc\u00dft", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mit einem Kusse soll Belinde Dich bezahlen.", "tokens": ["Mit", "ei\u00b7nem", "Kus\u00b7se", "soll", "Be\u00b7lin\u00b7de", "Dich", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}