{"textgrid.poem.36092": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "5. Als Herr Johann Michels zu Leipzig Doctor ward", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist es vberhin. Der Berg der ist erstiegen,", "tokens": ["Nun", "ist", "es", "vberh\u00b7in", ".", "Der", "Berg", "der", "ist", "er\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "ART", "NN", "ART", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den ihr mit Lust k\u00f6nnt sehn zu ewren F\u00fc\u00dfen liegen,", "tokens": ["Den", "ihr", "mit", "Lust", "k\u00f6nnt", "sehn", "zu", "ew\u00b7ren", "F\u00fc\u00b7\u00dfen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "VVINF", "PTKZU", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was euch so lange Jahr' hat M\u00fch' vnd Schwei\u00df gemacht", "tokens": ["Was", "euch", "so", "lan\u00b7ge", "Jahr'", "hat", "M\u00fch'", "vnd", "Schwei\u00df", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "ADJA", "NN", "VAFIN", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd vmb so manche Nacht vnd s\u00fc\u00dfen Schlaf gebracht.", "tokens": ["Vnd", "vmb", "so", "man\u00b7che", "Nacht", "vnd", "s\u00fc\u00b7\u00dfen", "Schlaf", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "PIAT", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es kunte Lucifer euch niemals schamroth machen,", "tokens": ["Es", "kun\u00b7te", "Lu\u00b7ci\u00b7fer", "euch", "nie\u00b7mals", "scham\u00b7roth", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie fr\u00fch' er auch stund auf; mit euch die Wette machen", "tokens": ["Wie", "fr\u00fch'", "er", "auch", "stund", "auf", ";", "mit", "euch", "die", "Wet\u00b7te", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$.", "APPR", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vermochte Luna nicht; ihr habt sie ausgemacht;", "tokens": ["Ver\u00b7moch\u00b7te", "Lu\u00b7na", "nicht", ";", "ihr", "habt", "sie", "aus\u00b7ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Eh' Phebus sein Gespan zu Zaum vnd Sattel bracht,", "tokens": ["Eh'", "Phe\u00b7bus", "sein", "Ge\u00b7span", "zu", "Zaum", "vnd", "Sat\u00b7tel", "bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hat ewer fr\u00fcher Sinn sein Tagwerk schon gelesen,", "tokens": ["Hat", "e\u00b7wer", "fr\u00fc\u00b7her", "Sinn", "sein", "Tag\u00b7werk", "schon", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist in der B\u00fccher Schaar ietzt hier ietzt da gewesen.", "tokens": ["Ist", "in", "der", "B\u00fc\u00b7cher", "Schaar", "ietzt", "hier", "ietzt", "da", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "ADV", "ADV", "ADV", "ADV", "VAPP", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Bald vnten in dem Schacht, wo der Mercur sich findt,", "tokens": ["Bald", "vn\u00b7ten", "in", "dem", "Schacht", ",", "wo", "der", "Mer\u00b7cur", "sich", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bald wo die Perle w\u00e4chst, vnd thewrer Hyacinth.", "tokens": ["Bald", "wo", "die", "Per\u00b7le", "w\u00e4chst", ",", "vnd", "thew\u00b7rer", "Hya\u00b7cin\u00b7th", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "VVFIN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bald wo der Balsam schwitzt im Lande der Idumen,", "tokens": ["Bald", "wo", "der", "Bal\u00b7sam", "schwitzt", "im", "Lan\u00b7de", "der", "I\u00b7du\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bald in der newen Welt, wo man die frembden Blumen", "tokens": ["Bald", "in", "der", "ne\u00b7wen", "Welt", ",", "wo", "man", "die", "fremb\u00b7den", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "PIS", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Vnd Fr\u00fcchte holen mu\u00df; bald, wo man Meermo\u00df findt,", "tokens": ["Vnd", "Fr\u00fcch\u00b7te", "ho\u00b7len", "mu\u00df", ";", "bald", ",", "wo", "man", "Meer\u00b7mo\u00df", "findt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VMFIN", "$.", "ADV", "$,", "PWAV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Mutter der Corall, vnd wo Rubinen sind.", "tokens": ["Die", "Mut\u00b7ter", "der", "Co\u00b7rall", ",", "vnd", "wo", "Ru\u00b7bi\u00b7nen", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KON", "PWAV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ja, eh' dir Delia f\u00fcnf Lichter aus wird brennen,", "tokens": ["Ja", ",", "eh'", "dir", "De\u00b7lia", "f\u00fcnf", "Lich\u00b7ter", "aus", "wird", "bren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "NE", "CARD", "NN", "APPR", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "Vnd Cynthius bis hin zur Wageschale rennen,", "tokens": ["Vnd", "Cyn\u00b7thi\u00b7us", "bis", "hin", "zur", "Wa\u00b7ge\u00b7scha\u00b7le", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wird vns durch ewren Flei\u00df der ganze Quercetan,", "tokens": ["Wird", "vns", "durch", "ew\u00b7ren", "Flei\u00df", "der", "gan\u00b7ze", "Quer\u00b7ce\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das vor noch nie geschehn, Lateinisch reden an.", "tokens": ["Das", "vor", "noch", "nie", "ge\u00b7schehn", ",", "La\u00b7tei\u00b7nisch", "re\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADV", "ADV", "VVPP", "$,", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was vns die Glieder l\u00e4hmt, die Kraft der zarten Sinnen,", "tokens": ["Was", "vns", "die", "Glie\u00b7der", "l\u00e4hmt", ",", "die", "Kraft", "der", "zar\u00b7ten", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Geister bleyern macht, da\u00df sie nicht steigen k\u00f6nnen;", "tokens": ["Die", "Geis\u00b7ter", "bley\u00b7ern", "macht", ",", "da\u00df", "sie", "nicht", "stei\u00b7gen", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Was vns die Zunge h\u00e4mmt, die wei\u00dfe Haut verg\u00e4llt,", "tokens": ["Was", "vns", "die", "Zun\u00b7ge", "h\u00e4mmt", ",", "die", "wei\u00b7\u00dfe", "Haut", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vnd voller Wa\u00dfer macht, vnd Wind vnd Geist verh\u00e4llt,", "tokens": ["Vnd", "vol\u00b7ler", "Wa\u00b7\u00dfer", "macht", ",", "vnd", "Wind", "vnd", "Geist", "ver\u00b7h\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,", "KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das wi\u00dft ihr auch, vnd la\u00dft euch doch noch nicht vergn\u00fcgen.", "tokens": ["Das", "wi\u00dft", "ihr", "auch", ",", "vnd", "la\u00dft", "euch", "doch", "noch", "nicht", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "KON", "VVIMP", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der loberhitzte Sinn wil immer h\u00f6her fliegen,", "tokens": ["Der", "lo\u00b7ber\u00b7hitz\u00b7te", "Sinn", "wil", "im\u00b7mer", "h\u00f6\u00b7her", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Vnd denkt nur wolckenan. Was t\u00e4glich vnd gemein,", "tokens": ["Vnd", "denkt", "nur", "wol\u00b7cke\u00b7nan", ".", "Was", "t\u00e4g\u00b7lich", "vnd", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VMFIN", "$.", "PWS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das ist ihm nur Verdru\u00df. Er kan nicht niedrig sein,", "tokens": ["Das", "ist", "ihm", "nur", "Ver\u00b7dru\u00df", ".", "Er", "kan", "nicht", "nied\u00b7rig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das ihr doch nie gewest. Die geistesvolle Gaben", "tokens": ["Das", "ihr", "doch", "nie", "ge\u00b7west", ".", "Die", "geis\u00b7tes\u00b7vol\u00b7le", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die haben euch schon l\u00e4ngst vom P\u00f6vel weg erhaben.", "tokens": ["Die", "ha\u00b7ben", "euch", "schon", "l\u00e4ngst", "vom", "P\u00f6\u00b7vel", "weg", "er\u00b7ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So da\u00df man auch von fern' erblicket ewren Schein,", "tokens": ["So", "da\u00df", "man", "auch", "von", "fern'", "er\u00b7bli\u00b7cket", "ew\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADV", "APPR", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Vnd sp\u00fcrt, da\u00df Febus euch recht m\u00fc\u00dfe g\u00fcnstig sein.", "tokens": ["Vnd", "sp\u00fcrt", ",", "da\u00df", "Fe\u00b7bus", "euch", "recht", "m\u00fc\u00b7\u00dfe", "g\u00fcns\u00b7tig", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NE", "PPER", "ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Drumb auch das Musenvolk viel frische Rosen binden,", "tokens": ["Drumb", "auch", "das", "Mu\u00b7sen\u00b7volk", "viel", "fri\u00b7sche", "Ro\u00b7sen", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mit Myrten vntersetzt, darmit sie euch bewinden", "tokens": ["Mit", "Myr\u00b7ten", "vn\u00b7ter\u00b7setzt", ",", "dar\u00b7mit", "sie", "euch", "be\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "KOUS", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Das wolverdiente Haupt. Apollo steht im Kreys,", "tokens": ["Das", "wol\u00b7ver\u00b7dien\u00b7te", "Haupt", ".", "A\u00b7pol\u00b7lo", "steht", "im", "Kreys", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Vnd reicht den Purpurdank vnd hohen Lobeerpreys.", "tokens": ["Vnd", "reicht", "den", "Pur\u00b7pur\u00b7dank", "vnd", "ho\u00b7hen", "Lo\u00b7beer\u00b7preys", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Meditrin l\u00e4\u00df auch zu solchem Ehrenprangen", "tokens": ["Die", "Me\u00b7di\u00b7trin", "l\u00e4\u00df", "auch", "zu", "sol\u00b7chem", "Eh\u00b7ren\u00b7pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "An ihrer Tempelwand ein solch Ged\u00e4chtn\u00fc\u00df hangen,", "tokens": ["An", "ih\u00b7rer", "Tem\u00b7pel\u00b7wand", "ein", "solch", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da\u00df Marmel, Erz vnd Stahl, vnd die verge\u00dfne Zeit,", "tokens": ["Da\u00df", "Mar\u00b7mel", ",", "Erz", "vnd", "Stahl", ",", "vnd", "die", "ver\u00b7ge\u00df\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ja Grab vnd Tod betrutzt. Zur greisen Ewigkeit", "tokens": ["Ja", "Grab", "vnd", "Tod", "be\u00b7trutzt", ".", "Zur", "grei\u00b7sen", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["