, "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das ist ihm nur Verdru\u00df. Er kan nicht niedrig sein,", "tokens": ["Das", "ist", "ihm", "nur", "Ver\u00b7dru\u00df", ".", "Er", "kan", "nicht", "nied\u00b7rig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das ihr doch nie gewest. Die geistesvolle Gaben", "tokens": ["Das", "ihr", "doch", "nie", "ge\u00b7west", ".", "Die", "geis\u00b7tes\u00b7vol\u00b7le", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die haben euch schon l\u00e4ngst vom P\u00f6vel weg erhaben.", "tokens": ["Die", "ha\u00b7ben", "euch", "schon", "l\u00e4ngst", "vom", "P\u00f6\u00b7vel", "weg", "er\u00b7ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So da\u00df man auch von fern' erblicket ewren Schein,", "tokens": ["So", "da\u00df", "man", "auch", "von", "fern'", "er\u00b7bli\u00b7cket", "ew\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADV", "APPR", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Vnd sp\u00fcrt, da\u00df Febus euch recht m\u00fc\u00dfe g\u00fcnstig sein.", "tokens": ["Vnd", "sp\u00fcrt", ",", "da\u00df", "Fe\u00b7bus", "eu