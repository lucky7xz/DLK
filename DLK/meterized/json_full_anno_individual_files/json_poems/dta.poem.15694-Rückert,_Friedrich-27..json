{"dta.poem.15694": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Begreifen willst du Gott? la\u00df deinen bl\u00f6den Eifer!", "tokens": ["Be\u00b7grei\u00b7fen", "willst", "du", "Gott", "?", "la\u00df", "dei\u00b7nen", "bl\u00f6\u00b7den", "Ei\u00b7fer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "$.", "VVIMP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn mehr mu\u00df seyn als das Begriffne sein Begreifer.", "tokens": ["Denn", "mehr", "mu\u00df", "seyn", "als", "das", "Be\u00b7griff\u00b7ne", "sein", "Be\u00b7grei\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "VAINF", "KOKOM", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Darum ja, wenn du ihn begriffest, w\u00e4rst du mehr;", "tokens": ["Da\u00b7rum", "ja", ",", "wenn", "du", "ihn", "be\u00b7grif\u00b7fest", ",", "w\u00e4rst", "du", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir, den er minder schuf, ist unbegreiflich Er.", "tokens": ["Dir", ",", "den", "er", "min\u00b7der", "schuf", ",", "ist", "un\u00b7be\u00b7greif\u00b7lich", "Er", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Begreifest du dich selbst? und f\u00fchlest den Beruf,", "tokens": ["Be\u00b7grei\u00b7fest", "du", "dich", "selbst", "?", "und", "f\u00fch\u00b7lest", "den", "Be\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$.", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den zu begreifen, der dich dir ein R\u00e4thsel schuf?", "tokens": ["Den", "zu", "be\u00b7grei\u00b7fen", ",", "der", "dich", "dir", "ein", "R\u00e4th\u00b7sel", "schuf", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}