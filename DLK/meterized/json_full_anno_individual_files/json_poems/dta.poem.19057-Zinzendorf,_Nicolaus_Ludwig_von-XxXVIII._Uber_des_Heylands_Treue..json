{"dta.poem.19057": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxXVIII.  Uber des Heylands Treue.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.57", "af:0.42"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Oliebe, die in fremde Noth", "tokens": ["O\u00b7lie\u00b7be", ",", "die", "in", "frem\u00b7de", "Noth"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich selbst hinein gest\u00fcrtzt,", "tokens": ["Sich", "selbst", "hin\u00b7ein", "ge\u00b7st\u00fcrtzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APZR", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die damit dem ewgen Tod", "tokens": ["Und", "die", "da\u00b7mit", "dem", "ew\u00b7gen", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Stachel abgek\u00fcrtzt.", "tokens": ["Den", "Sta\u00b7chel", "ab\u00b7ge\u00b7k\u00fcrtzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir sehen deine Herrlichkeit", "tokens": ["Wir", "se\u00b7hen", "dei\u00b7ne", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jm Thal der Demuth bl\u00fchn,", "tokens": ["Jm", "Thal", "der", "De\u00b7muth", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und uns durch dein empfindlich Leid", "tokens": ["Und", "uns", "durch", "dein", "emp\u00b7find\u00b7lich", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus allem Leiden ziehn.", "tokens": ["Aus", "al\u00b7lem", "Lei\u00b7den", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da\u00df du nun unser B\u00fcrge bist,", "tokens": ["Da\u00df", "du", "nun", "un\u00b7ser", "B\u00fcr\u00b7ge", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das heist man wohl gethan,", "tokens": ["Das", "heist", "man", "wohl", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nimmt den Menschen JEsum Christ", "tokens": ["Und", "nimmt", "den", "Men\u00b7schen", "Je\u00b7sum", "Christ"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum S\u00fcnden-Tilger an.", "tokens": ["Zum", "S\u00fcn\u00b7den\u00b7Til\u00b7ger", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Allein, wie wenig wird man sehn,", "tokens": ["Al\u00b7lein", ",", "wie", "we\u00b7nig", "wird", "man", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIS", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die zu bereden seyn,", "tokens": ["Die", "zu", "be\u00b7re\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVINF", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df niemand kan ins Leben gehn,", "tokens": ["Da\u00df", "nie\u00b7mand", "kan", "ins", "Le\u00b7ben", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als durch die Creutzes-Pein.", "tokens": ["Als", "durch", "die", "Creut\u00b7zes\u00b7Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So gieb denn deinem Wort vom Creutz", "tokens": ["So", "gieb", "denn", "dei\u00b7nem", "Wort", "vom", "Creutz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In denen Seelen Kraft,", "tokens": ["In", "de\u00b7nen", "See\u00b7len", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df es dieselben allerseits", "tokens": ["Da\u00df", "es", "die\u00b7sel\u00b7ben", "al\u00b7ler\u00b7seits"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mithin zum Creutze raft.", "tokens": ["Mi\u00b7thin", "zum", "Creut\u00b7ze", "raft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Denn das ist einmal gantz gewi\u00df,", "tokens": ["Denn", "das", "ist", "ein\u00b7mal", "gantz", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist zu gleicher Zeit", "tokens": ["Du", "bist", "zu", "glei\u00b7cher", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Gegen-Gift vors Todes-Bi\u00df,", "tokens": ["Ein", "Ge\u00b7gen\u00b7Gift", "vors", "To\u00b7des\u00b7Bi\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unsre Heiligkeit.", "tokens": ["Und", "uns\u00b7re", "Hei\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Drum, der du angekommen bist,", "tokens": ["Drum", ",", "der", "du", "an\u00b7ge\u00b7kom\u00b7men", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Knechts-Gestalt zu gehn,", "tokens": ["In", "Knechts\u00b7Ge\u00b7stalt", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Weise nie gewesen ist,", "tokens": ["Des", "Wei\u00b7se", "nie", "ge\u00b7we\u00b7sen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich selber zu erh\u00f6hn.", "tokens": ["Sich", "sel\u00b7ber", "zu", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Komm! wincke unsrer stoltzen Art", "tokens": ["Komm", "!", "win\u00b7cke", "uns\u00b7rer", "stolt\u00b7zen", "Art"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins edle Nichts hinein,", "tokens": ["Ins", "ed\u00b7le", "Nichts", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darinn sich erstlich offenbahrt,", "tokens": ["Da\u00b7rinn", "sich", "erst\u00b7lich", "of\u00b7fen\u00b7bahrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wir GOtt Etwas seyn.", "tokens": ["Da\u00df", "wir", "Gott", "Et\u00b7was", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der du noch in der letzten Nacht,", "tokens": ["Der", "du", "noch", "in", "der", "letz\u00b7ten", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eh dich der Feind gefa\u00dft,", "tokens": ["Eh", "dich", "der", "Feind", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Den Deinen von der Liebe Macht", "tokens": ["Den", "Dei\u00b7nen", "von", "der", "Lie\u00b7be", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sch\u00f6n gepredigt hast.", "tokens": ["So", "sch\u00f6n", "ge\u00b7pre\u00b7digt", "hast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Erinnre deine kleine Schaar,", "tokens": ["E\u00b7rinn\u00b7re", "dei\u00b7ne", "klei\u00b7ne", "Schaar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sich so leichte zweyt,", "tokens": ["Die", "sich", "so", "leich\u00b7te", "zweyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was deine letzte Sorge war:", "tokens": ["Was", "dei\u00b7ne", "letz\u00b7te", "Sor\u00b7ge", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Glieder Einigkeit.", "tokens": ["Der", "Glie\u00b7der", "Ei\u00b7nig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du opferst deine J\u00fcnger noch", "tokens": ["Du", "op\u00b7ferst", "dei\u00b7ne", "J\u00fcn\u00b7ger", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Vater im Gebet.", "tokens": ["Dem", "Va\u00b7ter", "im", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "O! w\u00fcrden unsre Sinnen doch", "tokens": ["O", "!", "w\u00fcr\u00b7den", "uns\u00b7re", "Sin\u00b7nen", "doch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VAFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oft im Gebet erh\u00f6ht.", "tokens": ["Oft", "im", "Ge\u00b7bet", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-++--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Der du um unsre Seligkeit", "tokens": ["Der", "du", "um", "uns\u00b7re", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit blut\u2019gem Schweisse rangst,", "tokens": ["Mit", "blut'\u00b7gem", "Schweis\u00b7se", "rangst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und durch der Thr\u00e4nen bangen Streit", "tokens": ["Und", "durch", "der", "Thr\u00e4\u00b7nen", "ban\u00b7gen", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Grimmes Macht verdrangst.", "tokens": ["Des", "Grim\u00b7mes", "Macht", "ver\u00b7drangst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ersch\u00fcttre doch den tr\u00e4gen Sinn,", "tokens": ["Er\u00b7sch\u00fctt\u00b7re", "doch", "den", "tr\u00e4\u00b7gen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der nichts von Arbeit wei\u00df,", "tokens": ["Der", "nichts", "von", "Ar\u00b7beit", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und rei\u00df ihn aus der Faulheit hin", "tokens": ["Und", "rei\u00df", "ihn", "aus", "der", "Faul\u00b7heit", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu deinem Kampf und Schwei\u00df.", "tokens": ["Zu", "dei\u00b7nem", "Kampf", "und", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Der du dich deines Vaters Zorn", "tokens": ["Der", "du", "dich", "dei\u00b7nes", "Va\u00b7ters", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Pfande eingethan,", "tokens": ["Zum", "Pfan\u00b7de", "ein\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimm uns, aus deinem Geist gebohrn,", "tokens": ["Nimm", "uns", ",", "aus", "dei\u00b7nem", "Geist", "ge\u00b7bohrn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Gegen-Pfande an.", "tokens": ["Zum", "Ge\u00b7gen\u00b7Pfan\u00b7de", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "War zu der Herrlichkeit die Schmach", "tokens": ["War", "zu", "der", "Herr\u00b7lich\u00b7keit", "die", "Schmach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein ordentlicher Weg;", "tokens": ["Dein", "or\u00b7dent\u00b7li\u00b7cher", "Weg", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So geht dir deine Heerde nach", "tokens": ["So", "geht", "dir", "dei\u00b7ne", "Heer\u00b7de", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch \u00fcber diesen Steg.", "tokens": ["Auch", "\u00fc\u00b7ber", "die\u00b7sen", "Steg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Und da dich deine Niedrigkeit", "tokens": ["Und", "da", "dich", "dei\u00b7ne", "Nied\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Pf\u00e4hle 