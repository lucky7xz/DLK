{"dta.poem.10926": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Verkehrtes Beginnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kann denn bey dem, was wir, auf Erden, vom Sch\u00f6pfer\nwunderw\u00fcrdigs sehn,", "tokens": ["Kann", "denn", "bey", "dem", ",", "was", "wir", ",", "auf", "Er\u00b7den", ",", "vom", "Sch\u00f6p\u00b7fer", "wun\u00b7der\u00b7w\u00fcr\u00b7digs", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "$,", "PRELS", "PPER", "$,", "APPR", "NN", "$,", "APPRART", "NN", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wann wir es Gott zum Ruhm gebrauchen, der Seelen Sorge", "tokens": ["Wann", "wir", "es", "Gott", "zum", "Ruhm", "ge\u00b7brau\u00b7chen", ",", "der", "See\u00b7len", "Sor\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "NN", "APPRART", "NN", "VVINF", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ist dieses nicht unstreitig wahr, wenn der Gesch\u00f6pfe Pracht", "tokens": ["Ist", "die\u00b7ses", "nicht", "un\u00b7strei\u00b7tig", "wahr", ",", "wenn", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJD", "PTKVZ", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+++-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Da\u00df sie zu ihrem gro\u00dfen Ursprung, zur Gottheit, Staffel-weis", "tokens": ["Da\u00df", "sie", "zu", "ih\u00b7rem", "gro\u00b7\u00dfen", "Ur\u00b7sprung", ",", "zur", "Got\u00b7theit", ",", "Staf\u00b7fel\u00b7weis"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,", "APPRART", "NN", "$,", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Wer also Gottes Werk verachtet, und seiner Creaturen", "tokens": ["Wer", "al\u00b7so", "Got\u00b7tes", "Werk", "ver\u00b7ach\u00b7tet", ",", "und", "sei\u00b7ner", "Crea\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "NN", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Der st\u00f6\u00dft die untre Leiter um, um auf die obre zu ge-", "tokens": ["Der", "st\u00f6\u00dft", "die", "un\u00b7tre", "Lei\u00b7ter", "um", ",", "um", "auf", "die", "ob\u00b7re", "zu", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,", "KOUI", "APPR", "ART", "ADJA", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}