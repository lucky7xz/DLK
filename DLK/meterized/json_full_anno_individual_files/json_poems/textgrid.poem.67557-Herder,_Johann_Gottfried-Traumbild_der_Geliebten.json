{"textgrid.poem.67557": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Traumbild der Geliebten", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die als ein Himmelsengel mich,", "tokens": ["Die", "als", "ein", "Him\u00b7mel\u00b7sen\u00b7gel", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach, kurz begegnete,", "tokens": ["Ach", ",", "kurz", "be\u00b7ge\u00b7gne\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zu Schottlands H\u00fctte feierlich", "tokens": ["Zu", "Schott\u00b7lands", "H\u00fct\u00b7te", "fei\u00b7er\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich scheidend segnete", "tokens": ["Mich", "schei\u00b7dend", "seg\u00b7ne\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und dann noch br\u00fcnstig mich umfing", "tokens": ["Und", "dann", "noch", "br\u00fcns\u00b7tig", "mich", "um\u00b7fing"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dann an meinem Bette", "tokens": ["Und", "dann", "an", "mei\u00b7nem", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Noch wie an meinem Schatten hing,", "tokens": ["Noch", "wie", "an", "mei\u00b7nem", "Schat\u00b7ten", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ob da sie H\u00fctte h\u00e4tte.", "tokens": ["Ob", "da", "sie", "H\u00fct\u00b7te", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, Schottlands H\u00fctte ist hier nicht,", "tokens": ["Ach", ",", "Schott\u00b7lands", "H\u00fct\u00b7te", "ist", "hier", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "VAFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein leeres Haus ist sie!", "tokens": ["Ein", "lee\u00b7res", "Haus", "ist", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Greis Fingal's Menschen sind hier nicht,", "tokens": ["Greis", "Fin\u00b7gal's", "Men\u00b7schen", "sind", "hier", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "VAFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind Alle Sklaven sie!", "tokens": ["Sind", "Al\u00b7le", "Skla\u00b7ven", "sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und ich, mit armem, w\u00fcstem Blick", "tokens": ["Und", "ich", ",", "mit", "ar\u00b7mem", ",", "w\u00fcs\u00b7tem", "Blick"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Such' ich mich ringsum wieder:", "tokens": ["Such'", "ich", "mich", "ring\u00b7sum", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbkomm, Traum! kommt, Br\u00fcder, noch zur\u00fcck!\u00ab", "tokens": ["\u00bb", "komm", ",", "Traum", "!", "kommt", ",", "Br\u00fc\u00b7der", ",", "noch", "zu\u00b7r\u00fcck", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$.", "VVFIN", "$,", "NN", "$,", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und finde keine Br\u00fcder!", "tokens": ["Und", "fin\u00b7de", "kei\u00b7ne", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nicht Einen, Keinen find' ich hier,", "tokens": ["Nicht", "Ei\u00b7nen", ",", "Kei\u00b7nen", "find'", "ich", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "$,", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der, Bruder f\u00fcr mein Herz,", "tokens": ["Der", ",", "Bru\u00b7der", "f\u00fcr", "mein", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich liebte, mit mir lebte, mir", "tokens": ["Mich", "lieb\u00b7te", ",", "mit", "mir", "leb\u00b7te", ",", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Freudeton und Schmerz,", "tokens": ["Im", "Freu\u00b7de\u00b7ton", "und", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach, ", "tokens": ["Ach", ","], "token_info": ["word", "punct"], "pos": ["ITJ", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Drum sink' ich jetzt in welche Ruh!", "tokens": ["Drum", "sink'", "ich", "jetzt", "in", "wel\u00b7che", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit schmerzbet\u00e4ubten Wunden,", "tokens": ["Mit", "schmerz\u00b7be\u00b7t\u00e4ub\u00b7ten", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und speise mich mit Tr\u00e4umen! Ist,", "tokens": ["Und", "spei\u00b7se", "mich", "mit", "Tr\u00e4u\u00b7men", "!", "Ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$.", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ist der s\u00fc\u00dfste Traum? \u2013", "tokens": ["Wo", "ist", "der", "s\u00fc\u00df\u00b7ste", "Traum", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du, Schottlands H\u00fctte, wo? wo bist", "tokens": ["Du", ",", "Schott\u00b7lands", "H\u00fct\u00b7te", ",", "wo", "?", "wo", "bist"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "PWAV", "$.", "PWAV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du, rauschend wilder Baum,", "tokens": ["Du", ",", "rau\u00b7schend", "wil\u00b7der", "Baum", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der noch auch edle Seelen kann,", "tokens": ["Der", "noch", "auch", "ed\u00b7le", "See\u00b7len", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein liebes Dach, beschirmen,", "tokens": ["Ein", "lie\u00b7bes", "Dach", ",", "be\u00b7schir\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vor Niedertracht beschirmen kann", "tokens": ["Vor", "Nie\u00b7der\u00b7tracht", "be\u00b7schir\u00b7men", "kann"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und bergen froh in St\u00fcrmen!", "tokens": ["Und", "ber\u00b7gen", "froh", "in", "St\u00fcr\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach, unser freies Vaterland,", "tokens": ["Ach", ",", "un\u00b7ser", "frei\u00b7es", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Sklaven nur gebiert,", "tokens": ["Was", "Skla\u00b7ven", "nur", "ge\u00b7biert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo unter Knechtetitel Tand", "tokens": ["Wo", "un\u00b7ter", "Knech\u00b7te\u00b7ti\u00b7tel", "Tand"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich Muth und Geist verliert,", "tokens": ["Sich", "Muth", "und", "Geist", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo Viehesdummheit, Stolz und Neid", "tokens": ["Wo", "Vie\u00b7hes\u00b7dumm\u00b7heit", ",", "Stolz", "und", "Neid"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Affenaberglauben", "tokens": ["Und", "Af\u00b7fen\u00b7a\u00b7berg\u00b7lau\u00b7ben"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und P\u00f6belniedertr\u00e4chtigkeit,", "tokens": ["Und", "P\u00f6\u00b7bel\u00b7nie\u00b7der\u00b7tr\u00e4ch\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ach, welch ein Herz mir rauben!", "tokens": ["Ach", ",", "welch", "ein", "Herz", "mir", "rau\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAT", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wegrauben edles Selbstgef\u00fchl", "tokens": ["We\u00b7grau\u00b7ben", "ed\u00b7les", "Selbst\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und That- und Lebensmuth,", "tokens": ["Und", "That", "und", "Le\u00b7bens\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der besten Stunden s\u00fc\u00dfes Spiel", "tokens": ["Der", "bes\u00b7ten", "Stun\u00b7den", "s\u00fc\u00b7\u00dfes", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im warmen Jugendblut", "tokens": ["Im", "war\u00b7men", "Ju\u00b7gend\u00b7blut"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Menschheit rauben! \u2013 ach! und mir", "tokens": ["Und", "Menschheit", "rau\u00b7ben", "!", "\u2013", "ach", "!", "und", "mir"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VVINF", "$.", "$(", "ITJ", "$.", "KON", "PPER"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Sind wenig meiner Tage.", "tokens": ["Sind", "we\u00b7nig", "mei\u00b7ner", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und, Mutter Vorsicht, dank' ich Dir", "tokens": ["Und", ",", "Mut\u00b7ter", "Vor\u00b7sicht", ",", "dank'", "ich", "Dir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "NN", "NN", "$,", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die wenigen als Plage?", "tokens": ["Die", "we\u00b7ni\u00b7gen", "als", "Pla\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Komm, Traumesm\u00e4dchen, komm zur\u00fcck", "tokens": ["Komm", ",", "Trau\u00b7mes\u00b7m\u00e4d\u00b7chen", ",", "komm", "zu\u00b7r\u00fcck"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Deiner lichten Spur,", "tokens": ["In", "Dei\u00b7ner", "lich\u00b7ten", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Deinem Liebethr\u00e4nenblick,", "tokens": ["Mit", "Dei\u00b7nem", "Lie\u00b7be\u00b7th\u00b7r\u00e4\u00b7nen\u00b7blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auch als ein Schatte nur,", "tokens": ["Auch", "als", "ein", "Schat\u00b7te", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als Traumbild nur, mit Zauberwort,", "tokens": ["Als", "Traum\u00b7bild", "nur", ",", "mit", "Zau\u00b7ber\u00b7wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sprich zu meiner Seele", "tokens": ["Und", "sprich", "zu", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und schweb und zeige mir den Ort", "tokens": ["Und", "schweb", "und", "zei\u00b7ge", "mir", "den", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und zeig ihn meiner Seele:", "tokens": ["Und", "zeig", "ihn", "mei\u00b7ner", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbdort wird, dort wird die H\u00fctte sein,", "tokens": ["\u00bb", "dort", "wird", ",", "dort", "wird", "die", "H\u00fct\u00b7te", "sein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "$,", "ADV", "VAFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Liebe sel'ges Dach,", "tokens": ["Der", "Lie\u00b7be", "sel'\u00b7ges", "Dach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In jener Au', in jenem Hain!\u00ab", "tokens": ["In", "je\u00b7ner", "Au'", ",", "in", "je\u00b7nem", "Hain", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, Zephyr, sprich ihr nach,", "tokens": ["Ach", ",", "Ze\u00b7phyr", ",", "sprich", "ihr", "nach", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VVFI