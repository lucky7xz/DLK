{"dta.poem.21651": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Blendend zittert gold\u2019nes Licht.", "tokens": ["Blen\u00b7dend", "zit\u00b7tert", "gold'\u00b7nes", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die sehnsuchtsvollen Wangen,", "tokens": ["Um", "die", "sehn\u00b7suchts\u00b7vol\u00b7len", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Strahl auf Strahl durch Wolken bricht,", "tokens": ["Strahl", "auf", "Strahl", "durch", "Wol\u00b7ken", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das nebelgraue Bangen", "tokens": ["Und", "das", "ne\u00b7bel\u00b7grau\u00b7e", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ist vergangen.", "tokens": ["Ist", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "In dem warmen Sonnenmeer", "tokens": ["In", "dem", "war\u00b7men", "Son\u00b7nen\u00b7meer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich baden traumversunken,", "tokens": ["Will", "ich", "ba\u00b7den", "traum\u00b7ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blitzend wogen um mich her", "tokens": ["Blit\u00b7zend", "wo\u00b7gen", "um", "mich", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PRF", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schie\u00dfend, wirbelnd, wonnetrunken,", "tokens": ["Schie\u00b7\u00dfend", ",", "wir\u00b7belnd", ",", "won\u00b7ne\u00b7trun\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,", "PWAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmelsfunken.", "tokens": ["Him\u00b7mels\u00b7fun\u00b7ken", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "O du wesenloser Geist,", "tokens": ["O", "du", "we\u00b7sen\u00b7lo\u00b7ser", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott der Strahlen, Glanz geboren,", "tokens": ["Gott", "der", "Strah\u00b7len", ",", "Glanz", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den das Weltall jauchzend preist,", "tokens": ["Den", "das", "Wel\u00b7tall", "jauch\u00b7zend", "preist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den zum Spotte nun die Thoren", "tokens": ["Den", "zum", "Spot\u00b7te", "nun", "die", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich erkoren:", "tokens": ["Sich", "er\u00b7ko\u00b7ren", ":"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Geist erhab\u2019ner Liebesmacht,", "tokens": ["Geist", "er\u00b7hab'\u00b7ner", "Lie\u00b7bes\u00b7macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geist des Wahren und des Guten,", "tokens": ["Geist", "des", "Wah\u00b7ren", "und", "des", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der du durch des Irrthums Nacht", "tokens": ["Der", "du", "durch", "des", "Irr\u00b7thums", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Des Gedankens helle Gluthen", "tokens": ["Des", "Ge\u00b7dan\u00b7kens", "hel\u00b7le", "Glu\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4ssest fluthen:", "tokens": ["L\u00e4s\u00b7sest", "flut\u00b7hen", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Sendest nieder du den Hauch", "tokens": ["Sen\u00b7dest", "nie\u00b7der", "du", "den", "Hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines wunderbaren Lebens,", "tokens": ["Dei\u00b7nes", "wun\u00b7der\u00b7ba\u00b7ren", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Str\u00f6mt durch meine Seele auch", "tokens": ["Str\u00f6mt", "durch", "mei\u00b7ne", "See\u00b7le", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Voll geheimni\u00df-s\u00fc\u00dfen Webens", "tokens": ["Voll", "ge\u00b7heim\u00b7ni\u00df\u00b7s\u00fc\u00b7\u00dfen", "We\u00b7bens"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kraft des Strebens.", "tokens": ["Kraft", "des", "Stre\u00b7bens", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Heil dir Sonne, jauchzend soll", "tokens": ["Heil", "dir", "Son\u00b7ne", ",", "jauch\u00b7zend", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "NN", "$,", "ADJD", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir mein Lied zum Aether wallen,", "tokens": ["Dir", "mein", "Lied", "zum", "A\u00b7e\u00b7ther", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "In die Saiten schlag\u2019 ich voll,", "tokens": ["In", "die", "Sai\u00b7ten", "schlag'", "ich", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie durch der Erde Hallen", "tokens": ["Da\u00df", "sie", "durch", "der", "Er\u00b7de", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hell erschallen!", "tokens": ["Hell", "er\u00b7schal\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "In Verkl\u00e4rung blickt empor", "tokens": ["In", "Ver\u00b7kl\u00e4\u00b7rung", "blickt", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann die Menschheit, lichtdurchdrungen,", "tokens": ["Dann", "die", "Menschheit", ",", "licht\u00b7durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Spenden dir im Jubelchor,", "tokens": ["Spen\u00b7den", "dir", "im", "Ju\u00b7be\u00b7lchor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott der G\u00f6tter, tausend Zungen", "tokens": ["Gott", "der", "G\u00f6t\u00b7ter", ",", "tau\u00b7send", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Huldigungen!", "tokens": ["Hul\u00b7di\u00b7gun\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}