{"dta.poem.128": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-Brief an  Lucretien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Erbrich galantes Kind das Schreiben und mein Hertze", "tokens": ["Er\u00b7brich", "ga\u00b7lan\u00b7tes", "Kind", "das", "Schrei\u00b7ben", "und", "mein", "Hert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "ART", "NN", "KON", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schaue/ da\u00df ein Feur in beyden Theilen brennt/", "tokens": ["Und", "schau\u00b7e", "/", "da\u00df", "ein", "Feur", "in", "bey\u00b7den", "Thei\u00b7len", "brennt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So deiner Augen-Strahl und holde Sch\u00f6nheits-Kertze", "tokens": ["So", "dei\u00b7ner", "Au\u00b7gen\u00b7Strahl", "und", "hol\u00b7de", "Sch\u00f6n\u00b7heits\u00b7Kert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als seinen Z\u00fcnde-Strick und ersten Ursprung kennt.", "tokens": ["Als", "sei\u00b7nen", "Z\u00fcn\u00b7de\u00b7Strick", "und", "ers\u00b7ten", "Ur\u00b7sprung", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein l\u00e4chelnd Angesicht/ dein angenehmes Wesen", "tokens": ["Dein", "l\u00e4\u00b7chelnd", "An\u00b7ge\u00b7sicht", "/", "dein", "an\u00b7ge\u00b7neh\u00b7mes", "We\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das spielet Gluht auf Gluht in meine Geister ein/", "tokens": ["Das", "spie\u00b7let", "Gluht", "auf", "Gluht", "in", "mei\u00b7ne", "Geis\u00b7ter", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein s\u00fcsser Blick l\u00e4st mich bald sterben/ bald genesen/", "tokens": ["Dein", "s\u00fcs\u00b7ser", "Blick", "l\u00e4st", "mich", "bald", "ster\u00b7ben", "/", "bald", "ge\u00b7ne\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$(", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und deiner Sch\u00f6nheit mu\u00df ich unterth\u00e4nig seyn.", "tokens": ["Und", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "mu\u00df", "ich", "un\u00b7ter\u00b7th\u00e4\u00b7nig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Augen/ so am Pracht den Himmel angewinnen/", "tokens": ["Die", "Au\u00b7gen", "/", "so", "am", "Pracht", "den", "Him\u00b7mel", "an\u00b7ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn er in blauer Farb die sch\u00f6nsten Wolcken zeigt/", "tokens": ["Wenn", "er", "in", "blau\u00b7er", "Farb", "die", "sch\u00f6ns\u00b7ten", "Wol\u00b7cken", "zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die machen solche Gluht/ da\u00df Hertz und Geist zerrinnen", "tokens": ["Die", "ma\u00b7chen", "sol\u00b7che", "Gluht", "/", "da\u00df", "Hertz", "und", "Geist", "zer\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$(", "KOUS", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als wenn das Sonnen-Licht ein d\u00fcnnes Wachs erweicht.", "tokens": ["Als", "wenn", "das", "Son\u00b7nen\u00b7Licht", "ein", "d\u00fcn\u00b7nes", "Wachs", "er\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die holden ", "tokens": ["Die", "hol\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Vor deiner Wunder-Pracht wird ", "tokens": ["Vor", "dei\u00b7ner", "Wun\u00b7der\u00b7Pracht", "wird"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die Sch\u00f6nheit selbst vor dir ins Meer zu Gnaden geht.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "selbst", "vor", "dir", "ins", "Meer", "zu", "Gna\u00b7den", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "APPRART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von diesem Angesicht w\u00fcnsch ich beliebte Blicke", "tokens": ["Von", "die\u00b7sem", "An\u00b7ge\u00b7sicht", "w\u00fcnsch", "ich", "be\u00b7lieb\u00b7te", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Damit mein Hertze mag vollkommen fr\u00f6lich seyn/", "tokens": ["Da\u00b7mit", "mein", "Hert\u00b7ze", "mag", "voll\u00b7kom\u00b7men", "fr\u00f6\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "VVFIN", "VMFIN", "ADJD", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es gehet Freud und Lust in meine Seel zur\u00fccke", "tokens": ["Es", "ge\u00b7het", "Freud", "und", "Lust", "in", "mei\u00b7ne", "Seel", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn dein beliebter Blick ein sanffter Sonnen-Schein.", "tokens": ["Wenn", "dein", "be\u00b7lieb\u00b7ter", "Blick", "ein", "sanff\u00b7ter", "Son\u00b7nen\u00b7Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich lebe blo\u00df in dir mein auserwehltes Leben/", "tokens": ["Ich", "le\u00b7be", "blo\u00df", "in", "dir", "mein", "au\u00b7ser\u00b7wehl\u00b7tes", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dein angenehmer Geist beseelet meine Brust/", "tokens": ["Dein", "an\u00b7ge\u00b7neh\u00b7mer", "Geist", "be\u00b7see\u00b7let", "mei\u00b7ne", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die ich dem sch\u00f6nen Geist schon lange eingegeben/", "tokens": ["Die", "ich", "dem", "sch\u00f6\u00b7nen", "Geist", "schon", "lan\u00b7ge", "ein\u00b7ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ob es/ mein Auffenthalt/ dir schon noch unbewust.", "tokens": ["Ob", "es", "/", "mein", "Auf\u00b7fent\u00b7halt", "/", "dir", "schon", "noch", "un\u00b7be\u00b7wust", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PPOSAT", "NN", "$(", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So treibt dein sch\u00f6ner Geist mein Hertz\u2019 an dich zu lieben/", "tokens": ["So", "treibt", "dein", "sch\u00f6\u00b7ner", "Geist", "mein", "Hertz'", "an", "dich", "zu", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und mein verliebtes Hertz noch mehr durch ihn entbrennt;", "tokens": ["Und", "mein", "ver\u00b7lieb\u00b7tes", "Hertz", "noch", "mehr", "durch", "ihn", "ent\u00b7brennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du Sch\u00f6nste/ schreibst es selbst/ was ich allhier geschrieben/", "tokens": ["Du", "Sch\u00f6ns\u00b7te", "/", "schreibst", "es", "selbst", "/", "was", "ich", "all\u00b7hier", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "VVFIN", "PPER", "ADV", "$(", "PWS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dein angenehmer Geist den rechten Schreiber nennt.", "tokens": ["Dein", "an\u00b7ge\u00b7neh\u00b7mer", "Geist", "den", "rech\u00b7ten", "Schrei\u00b7ber", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach blick ", "tokens": ["Ach", "blick"], "token_info": ["word", "word"], "pos": ["ITJ", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.30": {"text": "Dich selbst und deinen Knecht in meiner Seelen an/", "tokens": ["Dich", "selbst", "und", "dei\u00b7nen", "Knecht", "in", "mei\u00b7ner", "See\u00b7len", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Doch was verlange ich? ich wei\u00df von keinen Quaaleu", "tokens": ["Doch", "was", "ver\u00b7lan\u00b7ge", "ich", "?", "ich", "wei\u00df", "von", "kei\u00b7nen", "Qua\u00b7a\u00b7leu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.32": {"text": "Denn weil du in mir bist mich nichts betr\u00fcben kan.", "tokens": ["Denn", "weil", "du", "in", "mir", "bist", "mich", "nichts", "be\u00b7tr\u00fc\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VAFIN", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dein holdes Angesicht/ dein angenehmes Wesen/", "tokens": ["Dein", "hol\u00b7des", "An\u00b7ge\u00b7sicht", "/", "dein", "an\u00b7ge\u00b7neh\u00b7mes", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Das machet tausend Lust/ und leget allem Schmertz/", "tokens": ["Das", "ma\u00b7chet", "tau\u00b7send", "Lust", "/", "und", "le\u00b7get", "al\u00b7lem", "Schmertz", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "CARD", "NN", "$(", "KON", "VVFIN", "PIS", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ja ich mu\u00df nur davon die sch\u00f6nsten Fr\u00fcchte lesen", "tokens": ["Ja", "ich", "mu\u00df", "nur", "da\u00b7von", "die", "sch\u00f6ns\u00b7ten", "Fr\u00fcch\u00b7te", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VMFIN", "ADV", "PAV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ach/ fl\u00f6\u00df den sch\u00f6nen Geist nur offte durch mein Hertz?", "tokens": ["Ach", "/", "fl\u00f6\u00df", "den", "sch\u00f6\u00b7nen", "Geist", "nur", "off\u00b7te", "durch", "mein", "Hertz", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "VVFIN", "ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Doch wie nach dunckler Nacht der Sonnen g\u00fcldne Strahlen", "tokens": ["Doch", "wie", "nach", "dunck\u00b7ler", "Nacht", "der", "Son\u00b7nen", "g\u00fcld\u00b7ne", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Durch angenehmen Schein das dunckel braune Feld/", "tokens": ["Durch", "an\u00b7ge\u00b7neh\u00b7men", "Schein", "das", "dun\u00b7ckel", "brau\u00b7ne", "Feld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Mit hellem Tages-Licht zu neuer Lust bemahlen/", "tokens": ["Mit", "hel\u00b7lem", "Ta\u00b7ges\u00b7Licht", "zu", "neu\u00b7er", "Lust", "be\u00b7mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ob gleich ihr Wunder-Glantz best\u00e4ndig in der Welt.", "tokens": ["Ob", "gleich", "ihr", "Wun\u00b7der\u00b7Glantz", "be\u00b7st\u00e4n\u00b7dig", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So wollst du mich noch mehr durch neue Gunst begl\u00fccken", "tokens": ["So", "wollst", "du", "mich", "noch", "mehr", "durch", "neu\u00b7e", "Gunst", "be\u00b7gl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Den Geist/ der in mir ist/ la\u00df in dein Hertze ein", "tokens": ["Den", "Geist", "/", "der", "in", "mir", "ist", "/", "la\u00df", "in", "dein", "Hert\u00b7ze", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "APPR", "PPER", "VAFIN", "$(", "VVIMP", "APPR", "PPOSAT", "VVFIN", "ART"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.43": {"text": "Dein Geist ruht schon in mir/ der Wechsel wird es schicken", "tokens": ["Dein", "Geist", "ruht", "schon", "in", "mir", "/", "der", "Wech\u00b7sel", "wird", "es", "schi\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", 