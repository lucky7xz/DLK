{"textgrid.poem.62975": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Derwisch fand in einem Wald", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Derwisch fand in einem Wald", "tokens": ["Ein", "Der\u00b7wisch", "fand", "in", "ei\u00b7nem", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kind von reitzender Gestalt.", "tokens": ["Ein", "Kind", "von", "reit\u00b7zen\u00b7der", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hob es auf. Ach Gott, ein Junge!", "tokens": ["Er", "hob", "es", "auf", ".", "Ach", "Gott", ",", "ein", "Jun\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "ITJ", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rief er mit halberstarrter Zunge;", "tokens": ["Rief", "er", "mit", "hal\u00b7ber\u00b7starr\u00b7ter", "Zun\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wohl dir, wohl dir, armes Kind!", "tokens": ["Doch", "wohl", "dir", ",", "wohl", "dir", ",", "ar\u00b7mes", "Kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "ADV", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6