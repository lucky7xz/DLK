{"dta.poem.7403": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An den Verstorbenen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leg' Deine Lanze ein!", "tokens": ["Leg'", "Dei\u00b7ne", "Lan\u00b7ze", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie soll in tausend Splitter", "tokens": ["Sie", "soll", "in", "tau\u00b7send", "Split\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von mir zertr\u00fcmmert sein.", "tokens": ["Von", "mir", "zer\u00b7tr\u00fcm\u00b7mert", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Heran auf Deinem Rappen,", "tokens": ["He\u00b7ran", "auf", "Dei\u00b7nem", "Rap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du bist ein arger Schalk,", "tokens": ["Du", "bist", "ein", "ar\u00b7ger", "Schalk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Trotz Knappen und Trotz Wappen,", "tokens": ["Trotz", "Knap\u00b7pen", "und", "Trotz", "Wap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Trotz Falk und Katafalk!", "tokens": ["Trotz", "Falk", "und", "Ka\u00b7ta\u00b7falk", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich steh' nicht bei dem Trosse,", "tokens": ["Ich", "steh'", "nicht", "bei", "dem", "Tros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der r\u00e4uchernd vor Dir schweigt,", "tokens": ["Der", "r\u00e4u\u00b7chernd", "vor", "Dir", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil Du ein Herz f\u00fcr Rosse", "tokens": ["Weil", "Du", "ein", "Herz", "f\u00fcr", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und f\u00fcr's Kameel gezeigt;", "tokens": ["Und", "f\u00fcr's", "Ka\u00b7me\u00b7el", "ge\u00b7zeigt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Baschkire oder Mandschu \u2014", "tokens": ["Baschki\u00b7re", "o\u00b7der", "Mand\u00b7schu"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Was schiert mich Deine Welt?", "tokens": ["Was", "schiert", "mich", "Dei\u00b7ne", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich schleudre meinen Handschuh", "tokens": ["Ich", "schleud\u00b7re", "mei\u00b7nen", "Hand\u00b7schuh"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dir in Dein \u00f6des Zelt.", "tokens": ["Dir", "in", "Dein", "\u00f6\u00b7des", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dem Reich der Mameluken", "tokens": ["Dem", "Reich", "der", "Ma\u00b7me\u00b7lu\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weissagst Du Auferstehn,", "tokens": ["Weis\u00b7sagst", "Du", "Auf\u00b7er\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und s\u00e4hest ohne Zucken", "tokens": ["Und", "s\u00e4\u00b7hest", "oh\u00b7ne", "Zu\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Vaterland vergehn;", "tokens": ["Dein", "Va\u00b7ter\u00b7land", "ver\u00b7gehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wiegtest unter Palmen", "tokens": ["Doch", "wieg\u00b7test", "un\u00b7ter", "Pal\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du Dein Profetenhaubt,", "tokens": ["Du", "Dein", "Pro\u00b7fe\u00b7ten\u00b7haubt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn nicht aus unsern Halmen", "tokens": ["Wenn", "nicht", "aus", "un\u00b7sern", "Hal\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du erst Dein Gold geraubt?", "tokens": ["Du", "erst", "Dein", "Gold", "ge\u00b7raubt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du steuerst nun so lange", "tokens": ["Du", "steu\u00b7erst", "nun", "so", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Weltmeer aus und ein,", "tokens": ["Im", "Welt\u00b7meer", "aus", "und", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ward es nie Dir bange,", "tokens": ["Und", "ward", "es", "nie", "Dir", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df Du so klein, so klein?", "tokens": ["Da\u00df", "Du", "so", "klein", ",", "so", "klein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist er Dir nie erschienen,", "tokens": ["Ist", "er", "Dir", "nie", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der F\u00fcrst von Ithaka,", "tokens": ["Der", "F\u00fcrst", "von", "I\u00b7tha\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn Deine S\u00fcndermienen", "tokens": ["Wenn", "Dei\u00b7ne", "S\u00fcn\u00b7der\u00b7mie\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In seinem Reich er sah?", "tokens": ["In", "sei\u00b7nem", "Reich", "er", "sah", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und sprach er nie mit Grollen:?", "tokens": ["Und", "sprach", "er", "nie", "mit", "Grol\u00b7len", ":", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201efort aus dem freien Meer!", "tokens": ["\u201e", "fort", "aus", "dem", "frei\u00b7en", "Meer", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "\u201ewirf nicht in seinen Schollen", "tokens": ["\u201e", "wirf", "nicht", "in", "sei\u00b7nen", "Schol\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201edein L\u00fcgenkorn umher!", "tokens": ["\u201e", "dein", "L\u00fc\u00b7gen\u00b7korn", "um\u00b7her", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201ezieh' heim an Deine Pleisse,", "tokens": ["\u201e", "zieh'", "heim", "an", "Dei\u00b7ne", "Pleis\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201ezieh' heim an Deine Spree;", "tokens": ["\u201e", "zieh'", "heim", "an", "Dei\u00b7ne", "Spree", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u201enicht jede F\u00fcrstenreise", "tokens": ["\u201e", "nicht", "je\u00b7de", "F\u00fcrs\u00b7ten\u00b7rei\u00b7se"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PTKNEG", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u201eist eine Odyssee.\u201c", "tokens": ["\u201e", "ist", "ei\u00b7ne", "O\u00b7dys\u00b7see", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ART", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl ist er unerreichbar", "tokens": ["Wohl", "ist", "er", "un\u00b7er\u00b7reich\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der g\u00f6ttliche Uli\u00df,", "tokens": ["Der", "g\u00f6tt\u00b7li\u00b7che", "U\u00b7li\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Doch Du bist ihm vergleichbar", "tokens": ["Doch", "Du", "bist", "ihm", "ver\u00b7gleich\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am wenigsten gewi\u00df.", "tokens": ["Am", "we\u00b7nigs\u00b7ten", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Im Saus nicht und im Brause", "tokens": ["Im", "Saus", "nicht", "und", "im", "Brau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKNEG", "KON", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat er die Zeit verdehnt,", "tokens": ["Hat", "er", "die", "Zeit", "ver\u00b7dehnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er hat sich stets nach Hause", "tokens": ["Er", "hat", "sich", "stets", "nach", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu Weib und Volk gesehnt.", "tokens": ["Zu", "Weib", "und", "Volk", "ge\u00b7sehnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "F\u00fcr Deines Volkes Rechte,", "tokens": ["F\u00fcr", "Dei\u00b7nes", "Vol\u00b7kes", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie fochtest Du so schlecht!", "tokens": ["Wie", "foch\u00b7test", "Du", "so", "schlecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du standest im Gefechte \u2014", "tokens": ["Du", "stan\u00b7dest", "im", "Ge\u00b7fech\u00b7te"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja, f\u00fcr das T\u00fcrkenrecht;", "tokens": ["Ja", ",", "f\u00fcr", "das", "T\u00fcr\u00b7ken\u00b7recht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du stirbst auch auf dem Schilde,", "tokens": ["Du", "stirbst", "auch", "auf", "dem", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ja, auf dem Wappenschild;", "tokens": ["Ja", ",", "auf", "dem", "Wap\u00b7pen\u00b7schild", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Klag' nicht, da\u00df Deine Gilde", "tokens": ["Klag'", "nicht", ",", "da\u00df", "Dei\u00b7ne", "Gil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Fortan bei uns Nichts gilt!", "tokens": ["For\u00b7tan", "bei", "uns", "Nichts", "gilt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PIS", "VVFIN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.8": {"line.1": {"text": "Den Marmor bringt Carrara", "tokens": ["Den", "Mar\u00b7mor", "bringt", "Car\u00b7ra\u00b7ra"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Noch nicht f\u00fcr den hervor,", "tokens": ["Noch", "nicht", "f\u00fcr", "den", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "An den der Niagara", "tokens": ["An", "den", "der", "Nia\u00b7ga\u00b7ra"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den Donner selbst verlor,", "tokens": ["Den", "Don\u00b7ner", "selbst", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der nur in alle Fernen", "tokens": ["Der", "nur", "in", "al\u00b7le", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu seiner Schmach gereist,", "tokens": ["Zu", "sei\u00b7ner", "Schmach", "ge\u00b7reist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und noch vor Gottes Sternen", "tokens": ["Und", "noch", "vor", "Got\u00b7tes", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf seine Sternchen weist.", "tokens": ["Auf", "sei\u00b7ne", "Stern\u00b7chen", "weist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O Ritter, schlechter Ritter,", "tokens": ["O", "Rit\u00b7ter", ",", "schlech\u00b7ter", "Rit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Leg' Deine Lanze ein!", "tokens": ["Leg'", "Dei\u00b7ne", "Lan\u00b7ze", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie soll in tausend Splitter", "tokens": ["Sie", "soll", "in", "tau\u00b7send", "Split\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von mir zertr\u00fcmmert sein.", "tokens": ["Von", "mir", "zer\u00b7tr\u00fcm\u00b7mert", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lass' ab, lass' ab und sp\u00e4he", "tokens": ["Lass'", "ab", ",", "lass'", "ab", "und", "sp\u00e4\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht nach der W\u00fcste Sand!", "tokens": ["Nicht", "nach", "der", "W\u00fcs\u00b7te", "Sand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich setze in der N\u00e4he", "tokens": ["Ich", "set\u00b7ze", "in", "der", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dich in Dein Vaterland.", "tokens": ["Dich", "in", "Dein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}