{"dta.poem.3901": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Brief.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Verf\u00fchrer meiner Zucht, so schreibet Valiere,", "tokens": ["Ver\u00b7f\u00fch\u00b7rer", "mei\u00b7ner", "Zucht", ",", "so", "schrei\u00b7bet", "Va\u00b7lie\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nimm, Falscher, diesen Brief mit Furcht und", "tokens": ["Nimm", ",", "Fal\u00b7scher", ",", "die\u00b7sen", "Brief", "mit", "Furcht", "und"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PDAT", "NN", "APPR", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zittern hin.", "tokens": ["Zit\u00b7tern", "hin", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Wohl mir, wofern ich stets ein Nichts geblieben", "tokens": ["Wohl", "mir", ",", "wo\u00b7fern", "ich", "stets", "ein", "Nichts", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "ADV", "ART", "PIS", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "w\u00e4re,", "tokens": ["w\u00e4\u00b7re", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Weh mir, da\u00df ich zur Zeit ein Etwas worden bin.", "tokens": ["Weh", "mir", ",", "da\u00df", "ich", "zur", "Zeit", "ein", "Et\u00b7was", "wor\u00b7den", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "ART", "ADV", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich bin von GOtt verflucht, drum mu\u00df auch ich", "tokens": ["Ich", "bin", "von", "Gott", "ver\u00b7flucht", ",", "drum", "mu\u00df", "auch", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,", "PAV", "VMFIN", "ADV", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "verfluchen", "tokens": ["ver\u00b7flu\u00b7chen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Des Vaters Meisterst\u00fcck, der Mutter Fruchtbar-", "tokens": ["Des", "Va\u00b7ters", "Meis\u00b7ter\u00b7st\u00fcck", ",", "der", "Mut\u00b7ter", "Frucht\u00b7ba\u00b7r"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "keit.", "tokens": ["keit", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Gott hat ein Him\u0303elreich, da hab ich nichts zu suchen,", "tokens": ["Gott", "hat", "ein", "Him\u0303el\u00b7reich", ",", "da", "hab", "ich", "nichts", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Gott hat ein H\u00f6llenreich, das ist vor mich bereit.", "tokens": ["Gott", "hat", "ein", "H\u00f6l\u00b7len\u00b7reich", ",", "das", "ist", "vor", "mich", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Natur, du hast ja sonst manch Monstrum ausge-", "tokens": ["Na\u00b7tur", ",", "du", "hast", "ja", "sonst", "manch", "Monst\u00b7rum", "aus\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "br\u00fctet,", "tokens": ["br\u00fc\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Weswegen hast du mich nicht auch darzu gemacht,", "tokens": ["Wes\u00b7we\u00b7gen", "hast", "du", "mich", "nicht", "auch", "dar\u00b7zu", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "PTKNEG", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Denn, h\u00e4tt ich als ein L\u00f6w und Tyger-Thier ge-", "tokens": ["Denn", ",", "h\u00e4tt", "ich", "als", "ein", "L\u00f6w", "und", "Ty\u00b7ger\u00b7Thier", "ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VAFIN", "PPER", "KOUS", "ART", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "w\u00fctet,", "tokens": ["w\u00fc\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "So h\u00e4tte mich kein Mensch in diese Schmach ge-", "tokens": ["So", "h\u00e4t\u00b7te", "mich", "kein", "Mensch", "in", "die\u00b7se", "Schmach", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "APPR", "PDAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "bracht,", "tokens": ["bracht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Vermaledeyter Blick! der mich zuerst verf\u00fchrte,", "tokens": ["Ver\u00b7ma\u00b7le\u00b7dey\u00b7ter", "Blick", "!", "der", "mich", "zu\u00b7erst", "ver\u00b7f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vermaledeyter Mund! der erst: ich liebe, sprach,", "tokens": ["Ver\u00b7ma\u00b7le\u00b7dey\u00b7ter", "Mund", "!", "der", "erst", ":", "ich", "lie\u00b7be", ",", "sprach", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "ADV", "$.", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Vermaledeyter Ku\u00df! der mich zuerst ber\u00fchrte,", "tokens": ["Ver\u00b7ma\u00b7le\u00b7dey\u00b7ter", "Ku\u00df", "!", "der", "mich", "zu\u00b7erst", "be\u00b7r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Vermaledeyter Griff! der meine Rose brach.", "tokens": ["Ver\u00b7ma\u00b7le\u00b7dey\u00b7ter", "Griff", "!", "der", "mei\u00b7ne", "Ro\u00b7se", "brach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Doch was verfluch ich den, den ich als Vater ehre,", "tokens": ["Doch", "was", "ver\u00b7fluch", "ich", "den", ",", "den", "ich", "als", "Va\u00b7ter", "eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ART", "$,", "PRELS", "PPER", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Warum verdamm ich den, den GOtt gesetzet hat;", "tokens": ["Wa\u00b7rum", "ver\u00b7damm", "ich", "den", ",", "den", "Gott", "ge\u00b7set\u00b7zet", "hat", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "$,", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Geh in dein eigen Hertz, du schn\u00f6de ", "tokens": ["Geh", "in", "dein", "ei\u00b7gen", "Hertz", ",", "du", "schn\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "ADJD", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Darinnen ist der Grund von deiner Missethat.", "tokens": ["Da\u00b7rin\u00b7nen", "ist", "der", "Grund", "von", "dei\u00b7ner", "Mis\u00b7se\u00b7that", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ja wohl, ich reitzte dich mit meinen frechen Vlicken,", "tokens": ["Ja", "wohl", ",", "ich", "reitz\u00b7te", "dich", "mit", "mei\u00b7nen", "fre\u00b7chen", "Vli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Minen zogen dich, als ein Magnet, heran,", "tokens": ["Die", "Mi\u00b7nen", "zo\u00b7gen", "dich", ",", "als", "ein", "Mag\u00b7net", ",", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Die Seuffzer wusten dich durch Sehnsucht zu ent-", "tokens": ["Die", "Seuff\u00b7zer", "wus\u00b7ten", "dich", "durch", "Sehn\u00b7sucht", "zu", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "z\u00fccken,", "tokens": ["z\u00fc\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Die Worte b\u00e4hnten dir die unbetretne Bahn.", "tokens": ["Die", "Wor\u00b7te", "b\u00e4hn\u00b7ten", "dir", "die", "un\u00b7be\u00b7tret\u00b7ne", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Bi\u00dfweilen that ich so, als wenn ich widerst\u00fcnde,", "tokens": ["Bi\u00df\u00b7wei\u00b7len", "that", "ich", "so", ",", "als", "wenn", "ich", "wi\u00b7der\u00b7st\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Damit dein Appetit nur solte gr\u00f6sser seyn,", "tokens": ["Da\u00b7mit", "dein", "Ap\u00b7pe\u00b7tit", "nur", "sol\u00b7te", "gr\u00f6s\u00b7ser", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ADV", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wornach dein Hertze stund, das nennt ich lauter", "tokens": ["Wor\u00b7nach", "dein", "Hert\u00b7ze", "stund", ",", "das", "nennt", "ich", "lau\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "VVFIN", "ADJD", "$,", "PDS", "VVFIN", "PPER", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "S\u00fcnde,", "tokens": ["S\u00fcn\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.37": {"text": "Und also go\u00df ich Oel in deine Flammen ein.", "tokens": ["Und", "al\u00b7so", "go\u00df", "ich", "O\u00b7el", "in", "dei\u00b7ne", "Flam\u00b7men", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "So hab ich dich gereitzt, so hab ich dich verf\u00fchret,", "tokens": ["So", "hab", "ich", "dich", "ge\u00b7reitzt", ",", "so", "hab", "ich", "dich", "ver\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,", "ADV", "VAFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So hab ich dich gelockt, so hab ich dich versucht.", "tokens": ["So", "hab", "ich", "dich", "ge\u00b7lockt", ",", "so", "hab", "ich", "dich", "ver\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,", "ADV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die S\u00fcnden wachen auf, das Hertze wird ger\u00fchret,", "tokens": ["Die", "S\u00fcn\u00b7den", "wa\u00b7chen", "auf", ",", "das", "Hert\u00b7ze", "wird", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PDS", "VVFIN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und mein Gewissen spricht, du bist von GOtt ver-", "tokens": ["Und", "mein", "Ge\u00b7wis\u00b7sen", "spricht", ",", "du", "bist", "von", "Gott", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "flucht.", "tokens": ["flucht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.43": {"text": "Darum verzweifle nur, verdammte ", "tokens": ["Da\u00b7rum", "ver\u00b7zweif\u00b7le", "nur", ",", "ver\u00b7damm\u00b7te"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PAV", "VVFIN", "ADV", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Du glaubest nicht an GOtt, drum bist du schon ge-", "tokens": ["Du", "glau\u00b7best", "nicht", "an", "Gott", ",", "drum", "bist", "du", "schon", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,", "PAV", "VAFIN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.45": {"text": "richt,", "tokens": ["richt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.46": {"text": "Der Himmel z\u00fcrnt mit dir, er giebt dir kein Geh\u00f6re,", "tokens": ["Der", "Him\u00b7mel", "z\u00fcrnt", "mit", "dir", ",", "er", "giebt", "dir", "kein", "Ge\u00b7h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die Thr\u00e4nen sind umsonst, dir hilfft die Busse nicht.", "tokens": ["Die", "Thr\u00e4\u00b7nen", "sind", "um\u00b7sonst", ",", "dir", "hilfft", "die", "Bus\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "O! kan kein Don\u0303erkeil mein Lebens Ziel verk\u00fcrtzen?", "tokens": ["O", "!", "kan", "kein", "Do\u00f1er\u00b7keil", "mein", "Le\u00b7bens", "Ziel", "ver\u00b7k\u00fcrt\u00b7zen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PIAT", "NN", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.49": {"text": "Ist den\u0303 kein wildes Thier, das meinen Leib verzehrt?", "tokens": ["Ist", "de\u00f1", "kein", "wil\u