{"dta.poem.8910": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XlIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Vermag zum Geist der Geist, indem er denket;", "tokens": ["Ver\u00b7mag", "zum", "Geist", "der", "Geist", ",", "in\u00b7dem", "er", "den\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn meine Seele sich in dich versenket,", "tokens": ["Wenn", "mei\u00b7ne", "See\u00b7le", "sich", "in", "dich", "ver\u00b7sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So mein' ich, m\u00fc\u00dft' es dir im Ohre klingen.", "tokens": ["So", "mein'", "ich", ",", "m\u00fc\u00dft'", "es", "dir", "im", "Oh\u00b7re", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bes\u00e4\u00dfe nicht der Gott der Liebe Schwingen,", "tokens": ["Be\u00b7s\u00e4\u00b7\u00dfe", "nicht", "der", "Gott", "der", "Lie\u00b7be", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er h\u00e4tte nie zum Himmel sie gelenket,", "tokens": ["Er", "h\u00e4t\u00b7te", "nie", "zum", "Him\u00b7mel", "sie", "ge\u00b7len\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn dein Herz er mir im Traume schenket,", "tokens": ["Und", "wenn", "dein", "Herz", "er", "mir", "im", "Trau\u00b7me", "schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von wem als dir vermag er mir's zu bringen?", "tokens": ["Von", "wem", "als", "dir", "ver\u00b7mag", "er", "mir's", "zu", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KOKOM", "PPER", "VVFIN", "PPER", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn du mich liebst, so will ich gern ertragen,", "tokens": ["Wenn", "du", "mich", "liebst", ",", "so", "will", "ich", "gern", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir fern zu seyn, weil ich zu gut verstehe,", "tokens": ["Dir", "fern", "zu", "seyn", ",", "weil", "ich", "zu", "gut", "ver\u00b7ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VAINF", "$,", "KOUS", "PPER", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was unsre Seelen ohne Laut sich klagen.", "tokens": ["Was", "uns\u00b7re", "See\u00b7len", "oh\u00b7ne", "Laut", "sich", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Allein so lang' ich noch in Zweifel stehe,", "tokens": ["Al\u00b7lein", "so", "lang'", "ich", "noch", "in", "Zwei\u00b7fel", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und gerne m\u00f6chte deine Blicke fragen,", "tokens": ["Und", "ger\u00b7ne", "m\u00f6ch\u00b7te", "dei\u00b7ne", "Bli\u00b7cke", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Acht' ich Entfernung als das gr\u00f6\u00dfte Wehe.", "tokens": ["Acht'", "ich", "Ent\u00b7fer\u00b7nung", "als", "das", "gr\u00f6\u00df\u00b7te", "We\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}