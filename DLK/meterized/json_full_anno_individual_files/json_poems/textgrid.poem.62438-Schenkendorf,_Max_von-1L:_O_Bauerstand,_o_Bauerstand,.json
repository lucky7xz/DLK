{"textgrid.poem.62438": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Bauerstand, o Bauerstand,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Bauerstand, o Bauerstand,", "tokens": ["O", "Bau\u00b7er\u00b7stand", ",", "o", "Bau\u00b7er\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du liebster mir von allen,", "tokens": ["Du", "liebs\u00b7ter", "mir", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "PPER", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Erbtheil ist ein freies Land", "tokens": ["Zum", "E\u00b7rbtheil", "ist", "ein", "frei\u00b7es", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir herrlich zugefallen.", "tokens": ["Dir", "herr\u00b7lich", "zu\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Hoffart zehrt, ein b\u00f6ser Wurm,", "tokens": ["Die", "Hof\u00b7fart", "zehrt", ",", "ein", "b\u00f6\u00b7ser", "Wurm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Rost an Ritterschilden;", "tokens": ["Ein", "Rost", "an", "Rit\u00b7ter\u00b7schil\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zerfallen sind im Zeitensturm", "tokens": ["Zer\u00b7fal\u00b7len", "sind", "im", "Zei\u00b7ten\u00b7sturm"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die reichen B\u00fcrgergilden.", "tokens": ["Die", "rei\u00b7chen", "B\u00fcr\u00b7ger\u00b7gil\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du aber bau'st ein festes Haus,", "tokens": ["Du", "a\u00b7ber", "bau'st", "ein", "fes\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sch\u00f6ne gr\u00fcne Erde,", "tokens": ["Die", "sch\u00f6\u00b7ne", "gr\u00fc\u00b7ne", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und streuest goldnen Samen aus", "tokens": ["Und", "streu\u00b7est", "gold\u00b7nen", "Sa\u00b7men", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ohn' Argwohn und Gef\u00e4hrde.", "tokens": ["Ohn'", "Arg\u00b7wohn", "und", "Ge\u00b7f\u00e4hr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Hast Gotteslust und Gottesstrahl,", "tokens": ["Hast", "Got\u00b7tes\u00b7lust", "und", "Got\u00b7tes\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um eilig zu genesen,", "tokens": ["Um", "ei\u00b7lig", "zu", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Wenn sich in deine H\u00fcrd' einmal", "tokens": ["Wenn", "sich", "in", "dei\u00b7ne", "H\u00fcrd'", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geschlichen fremdes Wesen.", "tokens": ["Ge\u00b7schli\u00b7chen", "frem\u00b7des", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was unsre bl\u00f6de Welt nicht kennt", "tokens": ["Was", "uns\u00b7re", "bl\u00f6\u00b7de", "Welt", "nicht", "kennt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihrem eitlen Treiben,", "tokens": ["Mit", "ih\u00b7rem", "eit\u00b7len", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wovon im alten Testament", "tokens": ["Wo\u00b7von", "im", "al\u00b7ten", "Tes\u00b7ta\u00b7ment"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die heil'gen M\u00e4nner schreiben,", "tokens": ["Die", "heil'\u00b7gen", "M\u00e4n\u00b7ner", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das soll noch oft wie Morgenwind", "tokens": ["Das", "soll", "noch", "oft", "wie", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um meinen Busen wehen,", "tokens": ["Um", "mei\u00b7nen", "Bu\u00b7sen", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das hab' ich wol an manchem Kind", "tokens": ["Das", "hab'", "ich", "wol", "an", "man\u00b7chem", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im stillen Thal gesehen.", "tokens": ["Im", "stil\u00b7len", "Thal", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Demuth und die Dienstbarkeit", "tokens": ["Die", "De\u00b7muth", "und", "die", "Dienst\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Sch\u00f6nheit und der St\u00e4rke,", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "und", "der", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Einfalt, die sich endlich freut", "tokens": ["Die", "Ein\u00b7falt", ",", "die", "sich", "end\u00b7lich", "freut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An jedem Gotteswerke.", "tokens": ["An", "je\u00b7dem", "Got\u00b7tes\u00b7wer\u00b7ke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Des J\u00fcnglings fr\u00fche T\u00fcchtigkeit", "tokens": ["Des", "J\u00fcng\u00b7lings", "fr\u00fc\u00b7he", "T\u00fcch\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In w\u00fcrdigen Gesch\u00e4ften,", "tokens": ["In", "w\u00fcr\u00b7di\u00b7gen", "Ge\u00b7sch\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der alten M\u00e4nner Trefflichkeit,", "tokens": ["Der", "al\u00b7ten", "M\u00e4n\u00b7ner", "Treff\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bescheiden in den Kr\u00e4ften.", "tokens": ["Be\u00b7schei\u00b7den", "in", "den", "Kr\u00e4f\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wol manches Zeichen, manchen Wink", "tokens": ["Wol", "man\u00b7ches", "Zei\u00b7chen", ",", "man\u00b7chen", "Wink"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Kann man da drau\u00dfen sehen,", "tokens": ["Kann", "man", "da", "drau\u00b7\u00dfen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wovon wir in dem Mauerring", "tokens": ["Wo\u00b7von", "wir", "in", "dem", "Mau\u00b7er\u00b7ring"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die H\u00e4lfte nicht verstehen.", "tokens": ["Die", "H\u00e4lf\u00b7te", "nicht", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Vom Bauerstand, von unten aus", "tokens": ["Vom", "Bau\u00b7er\u00b7stand", ",", "von", "un\u00b7ten", "aus"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll sich das neue Leben", "tokens": ["Soll", "sich", "das", "neu\u00b7e", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Adels Schlo\u00df und B\u00fcrgers Haus", "tokens": ["In", "A\u00b7dels", "Schlo\u00df", "und", "B\u00fcr\u00b7gers", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein frischer Quell erheben.", "tokens": ["Ein", "fri\u00b7scher", "Quell", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Doch eines, lieber \u00e4ltster Stand,", "tokens": ["Doch", "ei\u00b7nes", ",", "lie\u00b7ber", "\u00e4lts\u00b7ter", "Stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kann gr\u00f6\u00dfres Lob dir schaffen:", "tokens": ["Kann", "gr\u00f6\u00df\u00b7res", "Lob", "dir", "schaf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nie m\u00fc\u00dfig h\u00e4ngen an der Wand", "tokens": ["Nie", "m\u00fc\u00b7\u00dfig", "h\u00e4n\u00b7gen", "an", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df deine Bauernwaffen.", "tokens": ["La\u00df", "dei\u00b7ne", "Bau\u00b7ern\u00b7waf\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der scharfe Speer, das gute Schwert", "tokens": ["Der", "schar\u00b7fe", "Speer", ",", "das", "gu\u00b7te", "Schwert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df \u00f6fter dich begleiten,", "tokens": ["Mu\u00df", "\u00f6f\u00b7ter", "dich", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um fr\u00f6hlich f\u00fcr Gesetz und Herd", "tokens": ["Um", "fr\u00f6h\u00b7lich", "f\u00fcr", "Ge\u00b7setz", "und", "Herd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcr das Heil zu streiten.", "tokens": ["Und", "f\u00fcr", "das", "Heil", "zu", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Zieh' fr\u00f6hlich, wenn erschallt das Horn,", "tokens": ["Zieh'", "fr\u00f6h\u00b7lich", ",", "wenn", "er\u00b7schallt", "das", "Horn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Sturm auf allen Wegen", "tokens": ["Ein", "Sturm", "auf", "al\u00b7len", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wirf ein hei\u00dfes blaues Korn", "tokens": ["Und", "wirf", "ein", "hei\u00b7\u00dfes", "blau\u00b7es", "Korn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem R\u00e4uber k\u00fchn entgegen.", "tokens": ["Dem", "R\u00e4u\u00b7ber", "k\u00fchn", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Die Siegessaat, die Freiheitssaat,", "tokens": ["Die", "Sie\u00b7ges\u00b7saat", ",", "die", "Frei\u00b7heits\u00b7saat", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie herrlich wird sie sprie\u00dfen!", "tokens": ["Wie", "herr\u00b7lich", "wird", "sie", "sprie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du Bauer sollst f\u00fcr solche That", "tokens": ["Du", "Bau\u00b7er", "sollst", "f\u00fcr", "sol\u00b7che", "That"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VMFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ernten selbst genie\u00dfen.", "tokens": ["Die", "Ern\u00b7ten", "selbst", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Der Arm, der harte Erde gr\u00e4bt", "tokens": ["Der", "Arm", ",", "der", "har\u00b7te", "Er\u00b7de", "gr\u00e4bt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Stiere wei\u00df zu zwingen,", "tokens": ["Und", "Stie\u00b7re", "wei\u00df", "zu", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kann wol, vom Heldengeist belebt,", "tokens": ["Kann", "wol", ",", "vom", "Hel\u00b7den\u00b7geist", "be\u00b7lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit jedem Feinde ringen.", "tokens": ["Mit", "je\u00b7dem", "Fein\u00b7de", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-