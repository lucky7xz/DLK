{"textgrid.poem.40321": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Socia", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da gab's Branntwein und Bier,", "tokens": ["Da", "gab's", "Brannt\u00b7wein", "und", "Bier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "im Spelunkenrevier,", "tokens": ["im", "Spe\u00b7lun\u00b7ken\u00b7re\u00b7vier", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "und ein Lied scholl r\u00fchrend durch die T\u00fcr;", "tokens": ["und", "ein", "Lied", "scholl", "r\u00fch\u00b7rend", "durch", "die", "T\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "und das sangen und spielten die traurigen Vier,", "tokens": ["und", "das", "san\u00b7gen", "und", "spiel\u00b7ten", "die", "trau\u00b7ri\u00b7gen", "Vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "ein Vater mit seinen drei T\u00f6chtern.", "tokens": ["ein", "Va\u00b7ter", "mit", "sei\u00b7nen", "drei", "T\u00f6ch\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "CARD", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Er stand am Ofen, die Geige am Kinn,", "tokens": ["Er", "stand", "am", "O\u00b7fen", ",", "die", "Gei\u00b7ge", "am", "Kinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "schief neben ihm hockte die Harfnerin,", "tokens": ["schief", "ne\u00b7ben", "ihm", "hock\u00b7te", "die", "Harf\u00b7ne\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "und die J\u00fcngste knixte und schlo\u00df ihr Lied,", "tokens": ["und", "die", "J\u00fcngs\u00b7te", "knix\u00b7te", "und", "schlo\u00df", "ihr", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "die Geige machte ti-flieti-fliet:", "tokens": ["die", "Gei\u00b7ge", "mach\u00b7te", "ti\u00b7flie\u00b7ti\u00b7fliet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "\u00bbwar Eine, die nur Einen lieben kunnt.\u00ab", "tokens": ["\u00bb", "war", "Ei\u00b7ne", ",", "die", "nur", "Ei\u00b7nen", "lie\u00b7ben", "kunnt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ART", "$,", "PRELS", "ADV", "ART", "ADJA", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Dritte ging stumm", "tokens": ["Die", "Drit\u00b7te", "ging", "stumm"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "mit dem Teller herum,", "tokens": ["mit", "dem", "Tel\u00b7ler", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "ums polternde Biljard, bla\u00df und krumm;", "tokens": ["ums", "pol\u00b7tern\u00b7de", "Bil\u00b7jard", ",", "bla\u00df", "und", "krumm", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und nun drehte der Alte die Fidel um", "tokens": ["und", "nun", "dreh\u00b7te", "der", "Al\u00b7te", "die", "Fi\u00b7del", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "und klappte darauf mit dem Bogen.", "tokens": ["und", "klapp\u00b7te", "da\u00b7rauf", "mit", "dem", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und auf einmal schwieg der Keller ganz,", "tokens": ["Und", "auf", "ein\u00b7mal", "schwieg", "der", "Kel\u00b7ler", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "die J\u00fcngste hob die R\u00f6cke zum Tanz;", "tokens": ["die", "J\u00fcngs\u00b7te", "hob", "die", "R\u00f6\u00b7cke", "zum", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "die Harfe machte ti-plinki-plunk,", "tokens": ["die", "Har\u00b7fe", "mach\u00b7te", "ti\u00b7plin\u00b7ki\u00b7plunk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "und die J\u00fcngste war so kinderjung", "tokens": ["und", "die", "J\u00fcngs\u00b7te", "war", "so", "kin\u00b7der\u00b7jung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "und sang zum Tanz ein w\u00fcstes Hurenlied.", "tokens": ["und", "sang", "zum", "Tanz", "ein", "w\u00fcs\u00b7tes", "Hu\u00b7ren\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie sang's mit Glut,", "tokens": ["Sie", "sang's", "mit", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "das zarte Blut;", "tokens": ["das", "zar\u00b7te", "Blut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "und der schwarze zerknitterte Ro\u00dfhaarhut", "tokens": ["und", "der", "schwar\u00b7ze", "zer\u00b7knit\u00b7ter\u00b7te", "Ro\u00df\u00b7haar\u00b7hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "stand zu der plumpen Harfe gut,", "tokens": ["stand", "zu", "der", "plum\u00b7pen", "Har\u00b7fe", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "mit den wei\u00dfen papiernen Rosen.", "tokens": ["mit", "den", "wei\u00b7\u00dfen", "pa\u00b7pier\u00b7nen", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Laut schrillten die Saiten tiflieti-plunk,", "tokens": ["Laut", "schrill\u00b7ten", "die", "Sai\u00b7ten", "ti\u00b7flie\u00b7ti\u00b7plunk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "und Alle beklatschten den letzten Sprung,", "tokens": ["und", "Al\u00b7le", "be\u00b7klatschten", "den", "letz\u00b7ten", "Sprung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "und vor mir stand die Tellermarie.", "tokens": ["und", "vor", "mir", "stand", "die", "Tel\u00b7ler\u00b7ma\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbspielt mir noch einmal\u00ab, bat ich sie,", "tokens": ["\u00bb", "spielt", "mir", "noch", "ein\u00b7mal", "\u00ab", ",", "bat", "ich", "sie", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ADV", "$(", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "\u00bbwar Eine, die nur ", "tokens": ["\u00bb", "war", "Ei\u00b7ne", ",", "die", "nur"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "VAFIN", "ART", "$,", "PRELS", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.5": {"line.1": {"text": "All mein dumpfes Gl\u00fcckverlangen", "tokens": ["All", "mein", "dum\u00b7pfes", "Gl\u00fcck\u00b7ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schien dies eine Wort zu kl\u00e4ren;", "tokens": ["schien", "dies", "ei\u00b7ne", "Wort", "zu", "kl\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "meine guten Geister sprangen", "tokens": ["mei\u00b7ne", "gu\u00b7ten", "Geis\u00b7ter", "spran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "auf, als sei's Musik der Sph\u00e4ren.", "tokens": ["auf", ",", "als", "sei's", "Mu\u00b7sik", "der", "Sph\u00e4\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Am Altar der Seele traten", "tokens": ["Am", "Al\u00b7tar", "der", "See\u00b7le", "tra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sie zusammen, flugbereit:", "tokens": ["sie", "zu\u00b7sam\u00b7men", ",", "flug\u00b7be\u00b7reit", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zartsinn, Ehrfurcht, Gro\u00dfmut, Lust zu Taten,", "tokens": ["Zar\u00b7tsinn", ",", "Ehr\u00b7furcht", ",", "Gro\u00df\u00b7mut", ",", "Lust", "zu", "Ta\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sehnsucht nach Unsterblichkeit.", "tokens": ["Sehn\u00b7sucht", "nach", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Aber w\u00e4hrend sie die Herrin feiern,", "tokens": ["A\u00b7ber", "w\u00e4h\u00b7rend", "sie", "die", "Her\u00b7rin", "fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00fcbermannt mein sterbliches Herz ein Schaudern:",