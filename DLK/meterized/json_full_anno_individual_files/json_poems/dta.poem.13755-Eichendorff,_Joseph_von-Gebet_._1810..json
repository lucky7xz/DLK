{"dta.poem.13755": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gebet   .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Warum gabst Du mir die G\u00fcte,", "tokens": ["Wa\u00b7rum", "gabst", "Du", "mir", "die", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Gedanken himmelw\u00e4rts,", "tokens": ["Die", "Ge\u00b7dan\u00b7ken", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und ein ritterlich Gem\u00fcthe,", "tokens": ["Und", "ein", "rit\u00b7ter\u00b7lich", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das die Treue heilig h\u00fcte", "tokens": ["Das", "die", "Treu\u00b7e", "hei\u00b7lig", "h\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Zeit treulosem Scherz?", "tokens": ["In", "der", "Zeit", "treu\u00b7lo\u00b7sem", "Scherz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}