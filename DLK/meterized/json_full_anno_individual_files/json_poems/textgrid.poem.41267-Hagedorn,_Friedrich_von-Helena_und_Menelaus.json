{"textgrid.poem.41267": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Helena und Menelaus", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Menelaus kam die Helena zur\u00fcck,", "tokens": ["Zum", "Me\u00b7ne\u00b7laus", "kam", "die", "He\u00b7le\u00b7na", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NE", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sprach, mit Recht besch\u00e4mt, und mit bethr\u00e4ntem Blick:", "tokens": ["Und", "sprach", ",", "mit", "Recht", "be\u00b7sch\u00e4mt", ",", "und", "mit", "be\u00b7thr\u00e4n\u00b7tem", "Blick", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ward dir zwar mein Leib, die irdische Last, entrissen;", "tokens": ["Es", "ward", "dir", "zwar", "mein", "Leib", ",", "die", "ir\u00b7di\u00b7sche", "Last", ",", "ent\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch, wie der Himmel wei\u00df, blieb meine Seele dein.", "tokens": ["Doch", ",", "wie", "der", "Him\u00b7mel", "wei\u00df", ",", "blieb", "mei\u00b7ne", "See\u00b7le", "dein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er sprach: Ich glaub' es gern; hingegen magst du wissen:", "tokens": ["Er", "sprach", ":", "Ich", "glaub'", "es", "gern", ";", "hin\u00b7ge\u00b7gen", "magst", "du", "wis\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was du mir lie\u00dfest, scheint dein schlechtes Theil zu sein.", "tokens": ["Was", "du", "mir", "lie\u00b7\u00dfest", ",", "scheint", "dein", "schlech\u00b7tes", "Theil", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zum Menelaus kam die Helena zur\u00fcck,", "tokens": ["Zum", "Me\u00b7ne\u00b7laus", "kam", "die", "He\u00b7le\u00b7na", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NE", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sprach, mit Recht besch\u00e4mt, und mit bethr\u00e4ntem Blick:", "tokens": ["Und", "sprach", ",", "mit", "Recht", "be\u00b7sch\u00e4mt", ",", "und", "mit", "be\u00b7thr\u00e4n\u00b7tem", "Blick", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure"