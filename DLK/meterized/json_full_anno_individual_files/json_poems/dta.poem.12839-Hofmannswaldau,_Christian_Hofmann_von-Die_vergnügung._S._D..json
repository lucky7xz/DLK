{"dta.poem.12839": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die vergn\u00fcgung.  \n S. D.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl dem, der sich nur l\u00e4st begn\u00fcgen", "tokens": ["Wohl", "dem", ",", "der", "sich", "nur", "l\u00e4st", "be\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Daran, was ihm auf GOttes gunst", "tokens": ["Da\u00b7ran", ",", "was", "ihm", "auf", "Got\u00b7tes", "gunst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PWS", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das gl\u00fcck ohnfehlbar zu mu\u00df f\u00fcgen,", "tokens": ["Das", "gl\u00fcck", "ohn\u00b7fehl\u00b7bar", "zu", "mu\u00df", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nehrt sich redlich seiner kunst:", "tokens": ["Und", "nehrt", "sich", "red\u00b7lich", "sei\u00b7ner", "kunst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ander halt auf geld und guth;", "tokens": ["Ein", "an\u00b7der", "halt", "auf", "geld", "und", "guth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe kunst und freyen muth.", "tokens": ["Ich", "lie\u00b7be", "kunst", "und", "frey\u00b7en", "muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie bald kan reichthum dich verlassen?", "tokens": ["Wie", "bald", "kan", "reicht\u00b7hum", "dich", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bist du elend gnug daran;", "tokens": ["So", "bist", "du", "e\u00b7lend", "gnug", "da\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kunst aber wird dich stets umfassen,", "tokens": ["Kunst", "a\u00b7ber", "wird", "dich", "stets", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie nehret treulich ihren mann.", "tokens": ["Sie", "neh\u00b7ret", "treu\u00b7lich", "ih\u00b7ren", "mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ander halt auf geld und guth;", "tokens": ["Ein", "an\u00b7der", "halt", "auf", "geld", "und", "guth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe kunst und freyen muth.", "tokens": ["Ich", "lie\u00b7be", "kunst", "und", "frey\u00b7en", "muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Giebt sie mir nicht viel goldes-tonnen,", "tokens": ["Giebt", "sie", "mir", "nicht", "viel", "gol\u00b7des\u00b7ton\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So macht sie mich doch besser satt,", "tokens": ["So", "macht", "sie", "mich", "doch", "bes\u00b7ser", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als den sein geld, der viel gewonnen,", "tokens": ["Als", "den", "sein", "geld", ",", "der", "viel", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und herr nicht ist, de\u00df, was er hat.", "tokens": ["Und", "herr", "nicht", "ist", ",", "de\u00df", ",", "was", "er", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VAFIN", "$,", "ART", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ander halt auf geld und guth;", "tokens": ["Ein", "an\u00b7der", "halt", "auf", "geld", "und", "guth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe kunst und freyen muth.", "tokens": ["Ich", "lie\u00b7be", "kunst", "und", "frey\u00b7en", "muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie manchem hat der krieg genommen,", "tokens": ["Wie", "man\u00b7chem", "hat", "der", "krieg", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ihm vorhin das gl\u00fccke gab,", "tokens": ["Was", "ihm", "vor\u00b7hin", "das", "gl\u00fc\u00b7cke", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der itzt f\u00fcr alles geld bekommen", "tokens": ["Der", "itzt", "f\u00fcr", "al\u00b7les", "geld", "be\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur einen kahlen bettel-stab!", "tokens": ["Nur", "ei\u00b7nen", "kah\u00b7len", "bet\u00b7tel\u00b7stab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ander halt auf geld und guth;", "tokens": ["Ein", "an\u00b7der", "halt", "auf", "geld", "und", "guth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe kunst und freyen muth.", "tokens": ["Ich", "lie\u00b7be", "kunst", "und", "frey\u00b7en", "muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer was gelernt, scheut keine waffen,", "tokens": ["Wer", "was", "ge\u00b7lernt", ",", "scheut", "kei\u00b7ne", "waf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVPP", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die kunst ist ihm f\u00fcr alles geld;", "tokens": ["Die", "kunst", "ist", "ihm", "f\u00fcr", "al\u00b7les", "geld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mu\u00df in steten \u00e4ngsten schlaffen,", "tokens": ["Der", "mu\u00df", "in", "ste\u00b7ten", "\u00e4ngs\u00b7ten", "schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ADJA", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nur den schatz im kasten h\u00e4lt.", "tokens": ["Der", "nur", "den", "schatz", "im", "kas\u00b7ten", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ander halt auf geld und guth;", "tokens": ["Ein", "an\u00b7der", "halt", "auf", "geld", "und", "guth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe kunst und freyen muth.", "tokens": ["Ich", "lie\u00b7be", "kunst", "und", "frey\u00b7en", "muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was ich besitz, ist nicht im kasten;", "tokens": ["Was", "ich", "be\u00b7sitz", ",", "ist", "nicht", "im", "kas\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "$,", "VAFIN", "PTKNEG", "APPRART", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will iemand meinen g\u00fcthern an,", "tokens": ["Will", "ie\u00b7mand", "mei\u00b7nen", "g\u00fc\u00b7thern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mu\u00df mein leben selbst antasten,", "tokens": ["Der", "mu\u00df", "mein", "le\u00b7ben", "selbst", "an\u00b7tas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist dis nun hin, was darff ich dann?", "tokens": ["Ist", "dis", "nun", "hin", ",", "was", "darff", "ich", "dann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "PTKVZ", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ander halt auf geld und guth,", "tokens": ["Ein", "an\u00b7der", "halt", "auf", "geld", "und", "guth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe kunst und freyen muth.", "tokens": ["Ich", "lie\u00b7be", "kunst", "und", "frey\u00b7en", "muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bring\u2019 mich dahin aus diesem lande,", "tokens": ["Bring'", "mich", "da\u00b7hin", "aus", "die\u00b7sem", "lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PAV", "APPR", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo nie der tag recht bricht herf\u00fcr,", "tokens": ["Wo", "nie", "der", "tag", "recht", "bricht", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch kunst kan ich im fremden sande", "tokens": ["Durch", "kunst", "kan", "ich", "im", "frem\u00b7den", "san\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So seelig leben, gleich wie hier!", "tokens": ["So", "see\u00b7lig", "le\u00b7ben", ",", "gleich", "wie", "hier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ander halt auf geld und guth;", "tokens": ["Ein", "an\u00b7der", "halt", "auf", "geld", "und", "guth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe kunst und freyen muth.", "tokens": ["Ich", "lie\u00b7be", "kunst", "und", "frey\u00b7en", "muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mu\u00df gleich die kunst nach brodt itzt gehen,", "tokens": ["Mu\u00df", "gleich", "die", "kunst", "nach", "brodt", "itzt", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie man von ihr ver\u00e4chtlich schw\u00e4tzt;", "tokens": ["Wie", "man", "von", "ihr", "ver\u00b7\u00e4cht\u00b7lich", "schw\u00e4tzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So will ich dennoch bey ihr stehen,", "tokens": ["So", "will", "ich", "den\u00b7noch", "bey", "ihr", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil sie mich inniglich ergetzt.", "tokens": ["Weil", "sie", "mich", "in\u00b7nig\u00b7lich", "er\u00b7getzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein ander halt auf geld und guth;", "tokens": ["Ein", "an\u00b7der", "halt", "auf", "geld", "und", "guth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich liebe kunst und freyen muth.", "tokens": ["Ich", "lie\u00b7be", "kunst", "und", "frey\u00b7en", "muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenn mir der H\u00f6chste das nur giebet,", "tokens": ["Wenn", "mir", "der", "H\u00f6chs\u00b7te", "das", "nur", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was mir zu leben n\u00f6thig ist,", "tokens": ["Was", "mir", "zu", "le\u00b7ben", "n\u00f6\u00b7thig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eine seele, die mich liebet,", "tokens": ["Und", "ei\u00b7ne", "see\u00b7le", ",", "die", "mich", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich vor allen auserkiest;", "tokens": ["Und", "mich", "vor", "al\u00b7len", "aus\u00b7er\u00b7kiest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lieb\u2019 ich \u00fcber geld und guth,", "tokens": ["So", "lieb'", "ich", "\u00fc\u00b7ber", "geld", "und", "guth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie, und die k\u00fcnst\u2019 und freyen muth.", "tokens": ["Sie", ",", "und", "die", "k\u00fcnst'", "und", "frey\u00b7en", "muth", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KON", "ART", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}