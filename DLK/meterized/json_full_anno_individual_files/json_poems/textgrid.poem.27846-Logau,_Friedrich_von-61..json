{"textgrid.poem.27846": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel erdulden, nichts nicht fechten;", "tokens": ["Viel", "er\u00b7dul\u00b7den", ",", "nichts", "nicht", "fech\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PIS", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaden leiden, doch nicht rechten;", "tokens": ["Scha\u00b7den", "lei\u00b7den", ",", "doch", "nicht", "rech\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Andre v\u00f6llen, sich entleeren;", "tokens": ["And\u00b7re", "v\u00f6l\u00b7len", ",", "sich", "ent\u00b7lee\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lohnen, doch den Dienst entberen;", "tokens": ["Loh\u00b7nen", ",", "doch", "den", "Dienst", "ent\u00b7be\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer geben, nimmer nemen;", "tokens": ["Im\u00b7mer", "ge\u00b7ben", ",", "nim\u00b7mer", "ne\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmer lachen, immer gr\u00e4men;", "tokens": ["Nim\u00b7mer", "la\u00b7chen", ",", "im\u00b7mer", "gr\u00e4\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Herrschen, gleichwol dienen m\u00fcssen;", "tokens": ["Herr\u00b7schen", ",", "gleich\u00b7wol", "die\u00b7nen", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Viel verwenden, nichts genissen;", "tokens": ["Viel", "ver\u00b7wen\u00b7den", ",", "nichts", "ge\u00b7nis\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenig haben, offte geben;", "tokens": ["We\u00b7nig", "ha\u00b7ben", ",", "off\u00b7te", "ge\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Selbsten fallen, andre heben;", "tokens": ["Selbs\u00b7ten", "fal\u00b7len", ",", "and\u00b7re", "he\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wann dann Gut, Blut, Marck und Kr\u00e4fften", "tokens": ["Wann", "dann", "Gut", ",", "Blut", ",", "Marck", "und", "Kr\u00e4ff\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Liegen f\u00fcr so viel Gesch\u00e4fften,", "tokens": ["Lie\u00b7gen", "f\u00fcr", "so", "viel", "Ge\u00b7sch\u00e4ff\u00b7ten", ","], "token_i