{"dta.poem.273": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An Sie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nste deiner Augen-Strahl", "tokens": ["Sch\u00f6ns\u00b7te", "dei\u00b7ner", "Au\u00b7gen\u00b7Strahl"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht s\u00fcsse Quaal", "tokens": ["Macht", "s\u00fcs\u00b7se", "Qua\u00b7al"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "In meinem Hertzen;", "tokens": ["In", "mei\u00b7nem", "Hert\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Doch die Schmertzen", "tokens": ["Doch", "die", "Schmert\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Sind ohne Pein.", "tokens": ["Sind", "oh\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Deine Blicke/", "tokens": ["Dei\u00b7ne", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Mir ein Gl\u00fccke", "tokens": ["Mir", "ein", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "K\u00f6nnen in dem Sterben seyn.", "tokens": ["K\u00f6n\u00b7nen", "in", "dem", "Ster\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lachen/ Weinen/ Leben/ Tod/", "tokens": ["La\u00b7chen", "/", "Wei\u00b7nen", "/", "Le\u00b7ben", "/", "Tod", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Noht ohne Noht", "tokens": ["Noht", "oh\u00b7ne", "Noht"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "---+", "measure": "unknown.measure.single"}, "line.3": {"text": "Entsteht aus Lieben;", "tokens": ["Ent\u00b7steht", "aus", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und Betr\u00fcben", "tokens": ["Und", "Be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Folgt auf die Lust/", "tokens": ["Folgt", "auf", "die", "Lust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wenn die Sinnen", "tokens": ["Wenn", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Nicht gewinnen", "tokens": ["Nicht", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word"], "pos": ["PTKNEG", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Sch\u00f6nste deine ", "tokens": ["Sch\u00f6ns\u00b7te", "dei\u00b7ne"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Ist es/ da\u00df ich sterben soll?", "tokens": ["Ist", "es", "/", "da\u00df", "ich", "ster\u00b7ben", "soll", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So fahr\u2019 ich wohl/", "tokens": ["So", "fahr'", "ich", "wohl", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wenn auf den Br\u00fcsten/", "tokens": ["Wenn", "auf", "den", "Br\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In den L\u00fcsten/", "tokens": ["In", "den", "L\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "Ich fahre hin/", "tokens": ["Ich", "fah\u00b7re", "hin", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Da mein Sterben", "tokens": ["Da", "mein", "Ster\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Kan erwerben", "tokens": ["Kan", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word"], "pos": ["VMFIN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Sich ein Grab bey dem Rubin.", "tokens": ["Sich", "ein", "Grab", "bey", "dem", "Ru\u00b7bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPR", "ART", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Sieh mein Auge fr\u00f6lich bricht/", "tokens": ["Sieh", "mein", "Au\u00b7ge", "fr\u00f6\u00b7lich", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil mir mein Licht", "tokens": ["Weil", "mir", "mein", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Allda erscheinet", "tokens": ["All\u00b7da", "er\u00b7schei\u00b7net"], "token_info": ["word", "word"], "pos": ["PAV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sch\u00f6n vereinet", "tokens": ["Sch\u00f6n", "ver\u00b7ei\u00b7net"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Mit Rosen-Bl\u00fcth;", "tokens": ["Mit", "Ro\u00b7sen\u00b7B\u00b7l\u00fcth", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und die Wangen", "tokens": ["Und", "die", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.7": {"text": "Selbst verlangen/", "tokens": ["Selbst", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Da\u00df mein Leben von mir flieht.", "tokens": ["Da\u00df", "mein", "Le\u00b7ben", "von", "mir", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}