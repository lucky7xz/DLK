{"textgrid.poem.48583": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Di\u00df ist das Licht,", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df ist das Licht,", "tokens": ["Di\u00df", "ist", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "das euch den Lohn verspricht", "tokens": ["das", "euch", "den", "Lohn", "ver\u00b7spricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "f\u00fcr eure Treu',", "tokens": ["f\u00fcr", "eu\u00b7re", "Treu'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "ihr wolgepaarten Zwei,", "tokens": ["ihr", "wol\u00b7ge\u00b7paar\u00b7ten", "Zwei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "und dieses ist die Zeit,", "tokens": ["und", "die\u00b7ses", "ist", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "die euch mit Lust erfreut!", "tokens": ["die", "euch", "mit", "Lust", "er\u00b7freut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zythere singt,", "tokens": ["Zy\u00b7the\u00b7re", "singt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+