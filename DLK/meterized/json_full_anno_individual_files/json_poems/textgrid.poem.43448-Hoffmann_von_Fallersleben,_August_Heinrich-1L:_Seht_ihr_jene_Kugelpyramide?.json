a"}, "line.2": {"text": "Und was thut f\u00fcr's Ganze ", "tokens": ["Und", "was", "thut", "f\u00fcr's", "Gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist denn ", "tokens": ["Ist", "denn"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Ist denn ", "tokens": ["Ist", "denn"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "O du schlichte Kugelpyramide!", "tokens": ["O", "du", "schlich\u00b7te", "Ku\u00b7gel\u00b7py\u00b7ra\u00b7mi\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00e4llt dir denn im Leben gar nichts ein?", "tokens": ["F\u00e4llt", "dir", "denn", "im", "Le\u00b7ben", "gar", "nichts", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Lerne doch aus diesem kleinen Liede,", "tokens": ["Ler\u00b7ne", "doch", "aus", "die\u00b7sem", "klei\u00b7nen", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Was du bist und was du k\u00f6nntest sein!", "tokens": ["Was", "du", "bist", "und", "was", "du", "k\u00f6nn\u00b7test", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "KON", "PWS", "PPER", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "s