{"textgrid.poem.44126": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kein Mensch hat von des H\u00f6chsten G\u00fcte", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Mensch hat von des H\u00f6chsten G\u00fcte", "tokens": ["Kein", "Mensch", "hat", "von", "des", "H\u00f6chs\u00b7ten", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein gr\u00f6\u00dfer Zeugn\u00fc\u00df auf der Welt,", "tokens": ["Ein", "gr\u00f6\u00b7\u00dfer", "Zeug\u00b7n\u00fc\u00df", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wem sie ein getreu Gem\u00fcthe", "tokens": ["Als", "wem", "sie", "ein", "ge\u00b7treu", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PWS", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch seltne F\u00fchrung zugesellt,", "tokens": ["Durch", "selt\u00b7ne", "F\u00fch\u00b7rung", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Dergleichen Schaz lehrt uns auf Erden,", "tokens": ["Derg\u00b7lei\u00b7chen", "Schaz", "lehrt", "uns", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Viel eitler W\u00fcntsche loszuwerden.", "tokens": ["Viel", "eit\u00b7ler", "W\u00fcnt\u00b7sche", "los\u00b7zu\u00b7wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die G\u00fcter des bedr\u00e4ngten Lebens", "tokens": ["Die", "G\u00fc\u00b7ter", "des", "be\u00b7dr\u00e4ng\u00b7ten", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind insgemein mehr Schein als Werth.", "tokens": ["Sind", "ins\u00b7ge\u00b7mein", "mehr", "Schein", "als", "Werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man sucht das Gl\u00fccke da vergebens,", "tokens": ["Man", "sucht", "das", "Gl\u00fc\u00b7cke", "da", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Ehr und Pracht das Haupt beschwert", "tokens": ["Wo", "Ehr", "und", "Pracht", "das", "Haupt", "be\u00b7schwert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wo Gefahr und Last und S\u00fcnden", "tokens": ["Und", "wo", "Ge\u00b7fahr", "und", "Last", "und", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Purpur Plaz und Nahrung finden.", "tokens": ["Im", "Pur\u00b7pur", "Plaz", "und", "Nah\u00b7rung", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der geile Saft von Sodoms Fr\u00fcchten", "tokens": ["Der", "gei\u00b7le", "Saft", "von", "So\u00b7doms", "Fr\u00fcch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erg\u00f6zt uns durch ein s\u00fc\u00dfes Gift,", "tokens": ["Er\u00b7g\u00f6zt", "uns", "durch", "ein", "s\u00fc\u00b7\u00dfes", "Gift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis Zeit und Lust den Leib zernichten", "tokens": ["Bis", "Zeit", "und", "Lust", "den", "Leib", "zer\u00b7nich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Rach und Glut die Seelen trift,", "tokens": ["Und", "Rach", "und", "Glut", "die", "See\u00b7len", "trift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die mancher sch\u00f6nen Lais K\u00fc\u00dfen", "tokens": ["Die", "man\u00b7cher", "sch\u00f6\u00b7nen", "Lais", "K\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zulezt umsonst verfluchen m\u00fc\u00dfen.", "tokens": ["Zu\u00b7lezt", "um\u00b7sonst", "ver\u00b7flu\u00b7chen", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Mammon macht in aller Ohren", "tokens": ["Der", "Mam\u00b7mon", "macht", "in", "al\u00b7ler", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den sch\u00f6n- und angenehmsten Klang.", "tokens": ["Den", "sch\u00f6n", "und", "an\u00b7ge\u00b7nehms\u00b7ten", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zehlt immerhin, ihr kargen Thoren,", "tokens": ["Zehlt", "im\u00b7mer\u00b7hin", ",", "ihr", "kar\u00b7gen", "Tho\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Finger blau, das Silber blanck;", "tokens": ["Die", "Fin\u00b7ger", "blau", ",", "das", "Sil\u00b7ber", "blanck", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies niedertr\u00e4chtige Vergn\u00fcgen", "tokens": ["Dies", "nie\u00b7der\u00b7tr\u00e4ch\u00b7ti\u00b7ge", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Soll mich nicht um die Ruh betriegen.", "tokens": ["Soll", "mich", "nicht", "um", "die", "Ruh", "be\u00b7trie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Herz, das mit mir lacht und weinet,", "tokens": ["Ein", "Herz", ",", "das", "mit", "mir", "lacht", "und", "wei\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nachdem sich mein Verh\u00e4ngn\u00fc\u00df kehrt,", "tokens": ["Nach\u00b7dem", "sich", "mein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das, was es sagt, auch denckt und meinet,", "tokens": ["Das", ",", "was", "es", "sagt", ",", "auch", "denckt", "und", "mei\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Nechsten Heil wie seins begehrt,", "tokens": ["Des", "Nechs\u00b7ten", "Heil", "wie", "seins", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KOKOM", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich freundlich straft und unterrichtet", "tokens": ["Mich", "freund\u00b7lich", "straft", "und", "un\u00b7ter\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJD", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und allen Zanck mit Sanftmuth schlichtet,", "tokens": ["Und", "al\u00b7len", "Zanck", "mit", "Sanft\u00b7muth", "schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein solches Herz ist meinem Herzen", "tokens": ["Ein", "sol\u00b7ches", "Herz", "ist", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Reichthum, den kein Dieb ber\u00fchrt,", "tokens": ["Ein", "Reicht\u00b7hum", ",", "den", "kein", "Dieb", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Stab und Trost in Fall und Schmerzen,", "tokens": ["Ein", "Stab", "und", "Trost", "in", "Fall", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Ancker, den kein Sturm entf\u00fchrt,", "tokens": ["Ein", "An\u00b7cker", ",", "den", "kein", "Sturm", "ent\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Arzt, der Schlag und Wunden heilet", "tokens": ["Ein", "Arzt", ",", "der", "Schlag", "und", "Wun\u00b7den", "hei\u00b7let"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und allzeit sichern Rath ertheilet.", "tokens": ["Und", "all\u00b7zeit", "si\u00b7chern", "Rath", "er\u00b7thei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Im Gl\u00fcck ist dies mein gr\u00f6stes Gl\u00fccke,", "tokens": ["Im", "Gl\u00fcck", "ist", "dies", "mein", "gr\u00f6s\u00b7tes", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PDS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df so ein Freund es mitgenie\u00dft,", "tokens": ["Da\u00df", "so", "ein", "Freund", "es", "mit\u00b7ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und giebt der Himmel saure Blicke,", "tokens": ["Und", "giebt", "der", "Him\u00b7mel", "sau\u00b7re", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wird die Bitterkeit vers\u00fc\u00dft,", "tokens": ["So", "wird", "die", "Bit\u00b7ter\u00b7keit", "ver\u00b7s\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Jonathan und David ringen,", "tokens": ["Wenn", "Jo\u00b7na\u00b7than", "und", "Da\u00b7vid", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Einander ehrlich beyzuspringen.", "tokens": ["Ein\u00b7an\u00b7der", "ehr\u00b7lich", "bey\u00b7zu\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da trennt kein Eigennuz die Seelen,", "tokens": ["Da", "trennt", "kein", "Ei\u00b7gen\u00b7nuz", "die", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die in zwey C\u00f6rpern eines sind,", "tokens": ["Die", "in", "zwey", "C\u00f6r\u00b7pern", "ei\u00b7nes", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "CARD", "NN", "ART", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da darf man nichts aus Furcht verheelen,", "tokens": ["Da", "darf", "man", "nichts", "aus", "Furcht", "ver\u00b7hee\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da kommt die List der Misgunst blind,", "tokens": ["Da", "kommt", "die", "List", "der", "Mis\u00b7gunst", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da m\u00fc\u00dfen Argwohn, Neid und Ha\u00dfen", "tokens": ["Da", "m\u00fc\u00b7\u00dfen", "Arg\u00b7wohn", ",", "Neid", "und", "Ha\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Bund wohl unzerri\u00dfen la\u00dfen.", "tokens": ["Den", "Bund", "wohl", "un\u00b7zer\u00b7ri\u00b7\u00dfen", "la\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wir sezen uns vertraut zusammen,", "tokens": ["Wir", "se\u00b7zen", "uns", "ver\u00b7traut", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Betrachten Gott, uns und die Welt.", "tokens": ["Be\u00b7trach\u00b7ten", "Gott", ",", "uns", "und", "die", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald fluchen wir den Kriegesflammen,", "tokens": ["Bald", "flu\u00b7chen", "wir", "den", "Krie\u00b7ges\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wodurch manch sch\u00f6nes Reich zerf\u00e4llt,", "tokens": ["Wo\u00b7durch", "manch", "sch\u00f6\u00b7nes", "Reich", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald w\u00fcntschen wir des Friedens wegen", "tokens": ["Bald", "w\u00fcnt\u00b7schen", "wir", "des", "Frie\u00b7dens", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem gro\u00dfen Carlen Sieg un