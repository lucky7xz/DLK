{"textgrid.poem.64085": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchlingstag", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn \u00fcber mir das reine Blau der Luft", "tokens": ["Wenn", "\u00fc\u00b7ber", "mir", "das", "rei\u00b7ne", "Blau", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und rings um mich der Bl\u00fcthenb\u00e4ume Duft", "tokens": ["Und", "rings", "um", "mich", "der", "Bl\u00fc\u00b7then\u00b7b\u00e4u\u00b7me", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Fr\u00fchlingstag in mein Ged\u00e4chtniss ruft,", "tokens": ["Den", "Fr\u00fch\u00b7lings\u00b7tag", "in", "mein", "Ge\u00b7d\u00e4cht\u00b7niss", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der unsre Herzen liebend einst verband,", "tokens": ["Der", "uns\u00b7re", "Her\u00b7zen", "lie\u00b7bend", "einst", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als ich zuerst Dein Innerstes verstand \u2013", "tokens": ["Als", "ich", "zu\u00b7erst", "Dein", "In\u00b7ners\u00b7tes", "ver\u00b7stand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann blick' ich, wie in meines Gl\u00fccks Ruinen,", "tokens": ["Dann", "blick'", "ich", ",", "wie", "in", "mei\u00b7nes", "Gl\u00fccks", "Ru\u00b7i\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hin auf Dein Grab, um das Cipressen gr\u00fcnen.", "tokens": ["Hin", "auf", "Dein", "Grab", ",", "um", "das", "Cip\u00b7res\u00b7sen", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "KOUI", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und dann ber\u00fchrt das Bild vergangner Stunden", "tokens": ["Und", "dann", "be\u00b7r\u00fchrt", "das", "Bild", "ver\u00b7gang\u00b7ner", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf's neu in mir der ew'gen Trennung Wunden.", "tokens": ["Auf's", "neu", "in", "mir", "der", "ew'\u00b7gen", "Tren\u00b7nung", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "APPR", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich zu verlieren hatt' ich Dich gefunden! \u2013", "tokens": ["Dich", "zu", "ver\u00b7lie\u00b7ren", "hatt'", "ich", "Dich", "ge\u00b7fun\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VAFIN", "PPER", "PRF", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Thr\u00e4nen fliessen jenem Fr\u00fchlingstag", "tokens": ["Und", "Thr\u00e4\u00b7nen", "flies\u00b7sen", "je\u00b7nem", "Fr\u00fch\u00b7lings\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Dir, die Du ihm l\u00e4chelnd glichest, nach.", "tokens": ["Und", "Dir", ",", "die", "Du", "ihm", "l\u00e4\u00b7chelnd", "gli\u00b7chest", ",", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch ach, so heiss, so bitter sie auch rinnen \u2013", "tokens": ["Doch", "ach", ",", "so", "heiss", ",", "so", "bit\u00b7ter", "sie", "auch", "rin\u00b7nen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie k\u00f6nnen nicht der Gruft Dich abgewinnen.", "tokens": ["Sie", "k\u00f6n\u00b7nen", "nicht", "der", "Gruft", "Dich", "ab\u00b7ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn \u00fcber mir das reine Blau der Luft", "tokens": ["Wenn", "\u00fc\u00b7ber", "mir", "das", "rei\u00b7ne", "Blau", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "ADJA", "NN",