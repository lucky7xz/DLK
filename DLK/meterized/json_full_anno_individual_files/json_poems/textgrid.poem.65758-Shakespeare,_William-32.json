{"textgrid.poem.65758": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "32", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn, \u00fcberlebend meiner Tage Ziel,", "tokens": ["Wenn", ",", "\u00fc\u00b7berl\u00b7e\u00b7bend", "mei\u00b7ner", "Ta\u00b7ge", "Ziel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVPP", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nachdem schon dies Gebein ein Raub des alten Feindes,", "tokens": ["Nach\u00b7dem", "schon", "dies", "Ge\u00b7bein", "ein", "Raub", "des", "al\u00b7ten", "Fein\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Durch Zufall noch einmal dir in die Augen fiel", "tokens": ["Durch", "Zu\u00b7fall", "noch", "ein\u00b7mal", "dir", "in", "die", "Au\u00b7gen", "fiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dies arme rohe Blatt des hingeschiednen Freundes;", "tokens": ["Dies", "ar\u00b7me", "ro\u00b7he", "Blatt", "des", "hin\u00b7ge\u00b7schied\u00b7nen", "Freun\u00b7des", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vergleich' es mit der Zeiten Besserung,", "tokens": ["Ver\u00b7gleich'", "es", "mit", "der", "Zei\u00b7ten", "Bes\u00b7se\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Und wenn auch aller Federn es besiegen,", "tokens": ["Und", "wenn", "auch", "al\u00b7ler", "Fe\u00b7dern", "es", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bewahr' es mir zu lieb, nicht um des Reimes Schwung,", "tokens": ["Be\u00b7wahr'", "es", "mir", "zu", "lieb", ",", "nicht", "um", "des", "Rei\u00b7mes", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKA", "ADJD", "$,", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den gl\u00fccklichere Geister \u00fcberfliegen.", "tokens": ["Den", "gl\u00fcck\u00b7li\u00b7che\u00b7re", "Geis\u00b7ter", "\u00fc\u00b7berf\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Dann denke liebend nur von mir: \u00bbO w\u00e4re", "tokens": ["Dann", "den\u00b7ke", "lie\u00b7bend", "nur", "von", "mir", ":", "\u00bb", "O", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "APPR", "PPER", "$.", "$(", "NE", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Des Freundes Mus' in reif'rer Zeit erbl\u00fcht,", "tokens": ["Des", "Freun\u00b7des", "Mus'", "in", "reif'\u00b7rer", "Zeit", "er\u00b7bl\u00fcht", ","], "token