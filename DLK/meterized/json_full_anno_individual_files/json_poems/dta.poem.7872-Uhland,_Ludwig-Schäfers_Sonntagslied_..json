{"dta.poem.7872": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00e4fers Sonntagslied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist der Tag des Herrn!", "tokens": ["Das", "ist", "der", "Tag", "des", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich bin allein auf weiter Flur,", "tokens": ["Ich", "bin", "al\u00b7lein", "auf", "wei\u00b7ter", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch Eine Morgenglocke nur!", "tokens": ["Noch", "Ei\u00b7ne", "Mor\u00b7gen\u00b7glo\u00b7cke", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun Stille nah und fern!", "tokens": ["Nun", "Stil\u00b7le", "nah", "und", "fern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Anbetend knie\u2019 ich hier.", "tokens": ["An\u00b7be\u00b7tend", "knie'", "ich", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "O s\u00fc\u00dfes Graun! geheimes Wehn!", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "Graun", "!", "ge\u00b7hei\u00b7mes", "Wehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als knieten Viele ungesehn", "tokens": ["Als", "knie\u00b7ten", "Vie\u00b7le", "un\u00b7ge\u00b7sehn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PIS", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und beteten mit mir.", "tokens": ["Und", "be\u00b7te\u00b7ten", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+---+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Der Himmel, nah und fern,", "tokens": ["Der", "Him\u00b7mel", ",", "nah", "und", "fern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Er ist so klar und feierlich,", "tokens": ["Er", "ist", "so", "klar", "und", "fei\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ganz, als wollt\u2019 er \u00f6ffnen sich.", "tokens": ["So", "ganz", ",", "als", "wollt'", "er", "\u00f6ff\u00b7nen", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "VMFIN", "PPER", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist der Tag des Herrn!", "tokens": ["Das", "ist", "der", "Tag", "des", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}