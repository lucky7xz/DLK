{"textgrid.poem.57107": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du liebes junges Menschenkind,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liebes junges Menschenkind,", "tokens": ["Du", "lie\u00b7bes", "jun\u00b7ges", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "was hast du H\u00e4nde feine!", "tokens": ["was", "hast", "du", "H\u00e4n\u00b7de", "fei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie meine derb daneben sind,", "tokens": ["Wie", "mei\u00b7ne", "derb", "da\u00b7ne\u00b7ben", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJD", "PAV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "du Wunderliche, Reine!", "tokens": ["du", "Wun\u00b7der\u00b7li\u00b7che", ",", "Rei\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und doch ri\u00df dich das Leben schon", "tokens": ["Und", "doch", "ri\u00df", "dich", "das", "Le\u00b7ben", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "in seinen verwirrenden Reigen:", "tokens": ["in", "sei\u00b7nen", "ver\u00b7wir\u00b7ren\u00b7den", "Rei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "du bist einmal herumgeflohn \u2013", "tokens": ["du", "bist", "ein\u00b7mal", "her\u00b7um\u00b7ge\u00b7flohn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVIZU", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "und tratst dann zur\u00fcck ins Schweigen.", "tokens": ["und", "tratst", "dann", "zu\u00b7r\u00fcck", "ins", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und nun die H\u00e4nde aufs Herz gepre\u00dft", "tokens": ["Und", "nun", "die", "H\u00e4n\u00b7de", "aufs", "Herz", "ge\u00b7pre\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "NN", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "seh' ich dich stillhin gehen \u2013", "tokens": ["seh'", "ich", "dich", "still\u00b7hin", "ge\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "die feinen H\u00e4nde so fest, so fest", "tokens": ["die", "fei\u00b7nen", "H\u00e4n\u00b7de", "so", "fest", ",", "so", "fest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKVZ", "$,", "ADV", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "auf dem Herzen, dem heimlich wehen.", "tokens": ["auf", "dem", "Her\u00b7zen", ",", "dem", "heim\u00b7lich", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"]