{"dta.poem.2566": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf die H. u. H. eheliche Verbindung den 14.  \n Julii 1676. in eines andern Namen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es th\u00fcrmte sich ein Berg in Cypris Insel auf/", "tokens": ["Es", "th\u00fcrm\u00b7te", "sich", "ein", "Berg", "in", "Cyp\u00b7ris", "In\u00b7sel", "auf", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "NE", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den l\u00e4ngst das Alterthum der Venus hat geweihet/", "tokens": ["Den", "l\u00e4ngst", "das", "Al\u00b7ter\u00b7thum", "der", "Ve\u00b7nus", "hat", "ge\u00b7wei\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und den die Anmuth selbst mit Rosen \u00fcberstreuet/", "tokens": ["Und", "den", "die", "An\u00b7muth", "selbst", "mit", "Ro\u00b7sen", "\u00fc\u00b7bers\u00b7treu\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es nahm ein grosses Volck zu diesem seinen Lauff/", "tokens": ["Es", "nahm", "ein", "gros\u00b7ses", "Volck", "zu", "die\u00b7sem", "sei\u00b7nen", "Lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Theils umb die Heiligkeit der G\u00f6ttin zu vers\u00f6hnen/", "tokens": ["Theils", "umb", "die", "Hei\u00b7lig\u00b7keit", "der", "G\u00f6t\u00b7tin", "zu", "ver\u00b7s\u00f6h\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Theils auch aus Schuld und Pflicht ihr Lust-Altar zu kr\u00f6nen.", "tokens": ["Theils", "auch", "aus", "Schuld", "und", "Pflicht", "ihr", "Lust\u00b7Al\u00b7tar", "zu", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Himmels Meister-St\u00fcck/ das liebliche Geschlecht/", "tokens": ["Des", "Him\u00b7mels", "Meis\u00b7ter\u00b7St\u00fcck", "/", "das", "lieb\u00b7li\u00b7che", "Ge\u00b7schlecht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das sch\u00f6ne Jungfern-Volck/ dem zu der M\u00e4nner Leben", "tokens": ["Das", "sch\u00f6\u00b7ne", "Jung\u00b7fern\u00b7Volck", "/", "dem", "zu", "der", "M\u00e4n\u00b7ner", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der kleine Liebes-Gott die Schl\u00fcssel hat gegeben/", "tokens": ["Der", "klei\u00b7ne", "Lie\u00b7bes\u00b7Gott", "die", "Schl\u00fcs\u00b7sel", "hat", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "War wachsam da\u00df niemand unreines Opffer br\u00e4cht/", "tokens": ["War", "wach\u00b7sam", "da\u00df", "nie\u00b7mand", "un\u00b7rei\u00b7nes", "Opf\u00b7fer", "br\u00e4cht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOUS", "PIS", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Und ihrer Ampeln Glut mit falschem Oel entz\u00fcndte/", "tokens": ["Und", "ih\u00b7rer", "Am\u00b7peln", "Glut", "mit", "fal\u00b7schem", "O\u00b7el", "ent\u00b7z\u00fcnd\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Als da\u00df zu steter Treu ein ewig Schwur verbindte.", "tokens": ["Als", "da\u00df", "zu", "ste\u00b7ter", "Treu", "ein", "e\u00b7wig", "Schwur", "ver\u00b7bind\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "NN", "ART", "ADJD", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Viel klagten ihre Noth und zogen jener Zeit/", "tokens": ["Viel", "klag\u00b7ten", "ih\u00b7re", "Noth", "und", "zo\u00b7gen", "je\u00b7ner", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Verbla\u00dfte Buhler an/ wie Dido sich erstochen/", "tokens": ["Ver\u00b7bla\u00df\u00b7te", "Buh\u00b7ler", "an", "/", "wie", "Di\u00b7do", "sich", "er\u00b7sto\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$(", "KOKOM", "NE", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und Sappho von dem Fels den k\u00fchnen Hals gebrochen/", "tokens": ["Und", "Sap\u00b7pho", "von", "dem", "Fels", "den", "k\u00fch\u00b7nen", "Hals", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Leandern in der Flut sein Ende war bereit;", "tokens": ["Le\u00b7an\u00b7dern", "in", "der", "Flut", "sein", "En\u00b7de", "war", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Adon/ den Venus gar dem Himmel f\u00fcrgesetzet/", "tokens": ["A\u00b7don", "/", "den", "Ve\u00b7nus", "gar", "dem", "Him\u00b7mel", "f\u00fcr\u00b7ge\u00b7set\u00b7zet", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.18": {"text": "Durch ein ergrimmtes Schwein im Walde ward verletzet.", "tokens": ["Durch", "ein", "er\u00b7grimm\u00b7tes", "Schwein", "im", "Wal\u00b7de", "ward", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Theils stunden murmelnde und wolten ihr Gesicht/", "tokens": ["Theils", "stun\u00b7den", "mur\u00b7meln\u00b7de", "und", "wol\u00b7ten", "ihr", "Ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "VMFIN", "PPOSAT", "NN", "$("], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Von Schaam und Furcht bestrickt nicht zu der G\u00f6ttin heben:", "tokens": ["Von", "Schaam", "und", "Furcht", "be\u00b7strickt", "nicht", "zu", "der", "G\u00f6t\u00b7tin", "he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die schalt ihr Einsamkeit und Kummer-volles Leben;", "tokens": ["Die", "schalt", "ihr", "Ein\u00b7sam\u00b7keit", "und", "Kum\u00b7mer\u00b7vol\u00b7les", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "KON", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein\u2019 andre; da\u00df der Lieb oft der Verstand gebricht/", "tokens": ["Ein'", "and\u00b7re", ";", "da\u00df", "der", "Lieb", "oft", "der", "Ver\u00b7stand", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$.", "KOUS", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und das/ der heute k\u00fc\u00dft/ offt morgen nicht mehr gr\u00fcsset/", "tokens": ["Und", "das", "/", "der", "heu\u00b7te", "k\u00fc\u00dft", "/", "offt", "mor\u00b7gen", "nicht", "mehr", "gr\u00fcs\u00b7set", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "ART", "ADV", "VVFIN", "$(", "ADV", "ADV", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und bey geschworner Treu man den Bestand vermisset.", "tokens": ["Und", "bey", "ge\u00b7schwor\u00b7ner", "Treu", "man", "den", "Be\u00b7stand", "ver\u00b7mis\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Viel seufftzten/ da\u00df sie viel auf Worte nur getraut/", "tokens": ["Viel", "seufftz\u00b7ten", "/", "da\u00df", "sie", "viel", "auf", "Wor\u00b7te", "nur", "ge\u00b7traut", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPER", "ADV", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da doch der M\u00e4nner Schwur ein Spiel der leichten Winde:", "tokens": ["Da", "doch", "der", "M\u00e4n\u00b7ner", "Schwur", "ein", "Spiel", "der", "leich\u00b7ten", "Win\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die weinten/ da\u00df die Zeit und Sch\u00f6nheit bald verschwinde/", "tokens": ["Die", "wein\u00b7ten", "/", "da\u00df", "die", "Zeit", "und", "Sch\u00f6n\u00b7heit", "bald", "ver\u00b7schwin\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "KOUS", "ART", "NN", "KON", "NN", "ADV", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und da\u00df auf schlipffrig Ei\u00df man in dem Lieben baut:", "tokens": ["Und", "da\u00df", "auf", "schlipf\u00b7frig", "Ei\u00df", "man", "in", "dem", "Lie\u00b7ben", "baut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJD", "NN", "PIS", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Noch andre wolten gar des Lebens Rest verdammen/", "tokens": ["Noch", "and\u00b7re", "wol\u00b7ten", "gar", "des", "Le\u00b7bens", "Rest", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "ADV", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "In dem Cupido sie nicht einmal f\u00fcgt zusammen.", "tokens": ["In", "dem", "Cu\u00b7pi\u00b7do", "sie", "nicht", "ein\u00b7mal", "f\u00fcgt", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "PPER", "PTKNEG", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dione derer Thron mit Herrligkeit umzirckt/", "tokens": ["Dio\u00b7ne", "de\u00b7rer", "Thron", "mit", "Herr\u00b7lig\u00b7keit", "um\u00b7zirckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.32": {"text": "Und von Je\u00dfminen-Oel und Balsam angefeuchtet/", "tokens": ["Und", "von", "Je\u00df\u00b7mi\u00b7nen\u00b7O\u00b7el", "und", "Bal\u00b7sam", "an\u00b7ge\u00b7feuch\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "KON", "NN", "VVPP", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.33": {"text": "Sah\u2019 wie die Morgenr\u00f6th\u2019 im ersten Purpur leuchtet:", "tokens": ["Sah'", "wie", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7th'", "im", "ers\u00b7ten", "Pur\u00b7pur", "leuch\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.34": {"text": "Es schien ihr g\u00fcldnes Kleid von Sternen gantz durchw\u00fcrckt/", "tokens": ["Es", "schien", "ihr", "g\u00fcld\u00b7nes", "Kleid", "von", "Ster\u00b7nen", "gantz", "durch\u00b7w\u00fcrckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und als de\u00df Scepters Stab sie dreymahl kr\u00e4fftig r\u00fchrte/", "tokens": ["Und", "als", "de\u00df", "Scep\u00b7ters", "Stab", "sie", "drey\u00b7mahl", "kr\u00e4ff\u00b7tig", "r\u00fchr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Vernahm man ohngefehr die Worte/ so sie f\u00fchrte:", "tokens": ["Ver\u00b7nahm", "man", "ohn\u00b7ge\u00b7fehr", "die", "Wor\u00b7te", "/", "so", "sie", "f\u00fchr\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "ART", "NN", "$(", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich G\u00f6ttin/ der das Reich der vier-getheilten Welt", "tokens": ["Ich", "G\u00f6t\u00b7tin", "/", "der", "das", "Reich", "der", "vier\u00b7get\u00b7heil\u00b7ten", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$(", "ART", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mu\u00df unterth\u00e4nig seyn; die G\u00f6tter kan bezwingen/", "tokens": ["Mu\u00df", "un\u00b7ter\u00b7th\u00e4\u00b7nig", "seyn", ";", "die", "G\u00f6t\u00b7ter", "kan", "be\u00b7zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VAINF", "$.", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und Hertzen von Metall in meine Dienste bringen/", "tokens": ["Und", "Hert\u00b7zen", "von", "Me\u00b7tall", "in", "mei\u00b7ne", "Diens\u00b7te", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der selbst der Donner-Gott geb\u00fcckt zu Fusse f\u00e4llt/", "tokens": ["Der", "selbst", "der", "Don\u00b7ner\u00b7Gott", "ge\u00b7b\u00fcckt", "zu", "Fus\u00b7se", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Will meines Reiches Macht auf heute noch erweitern/", "tokens": ["Will", "mei\u00b7nes", "Rei\u00b7ches", "Macht", "auf", "heu\u00b7te",