okens": ["Son\u00b7nen\u00b7strahl", "wie", "warm", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kannst die Wange bald versengen!", "tokens": ["Kannst", "die", "Wan\u00b7ge", "bald", "ver\u00b7sen\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ei wer sieht's im Tanz so schnelle,", "tokens": ["Ei", "wer", "sieht's", "im", "Tanz", "so", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Farben da sich dr\u00e4ngen:", "tokens": ["Al\u00b7le", "Far\u00b7ben", "da", "sich", "dr\u00e4n\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KOUS", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Amor schwingt die Fackel helle,", "tokens": ["A\u00b7mor", "schwingt", "die", "Fa\u00b7ckel", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieht so listig auf den Grund,", "tokens": ["Sieht", "so", "lis\u00b7tig", "auf", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sieht so leicht die falsche Stelle,", "tokens": ["Sieht", "so", "leicht", "die", "fal\u00b7sche", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schminke k\u00fcsset nicht sein Mund.", "tokens": ["Schmin\u00b7ke", "k\u00fcs\u00b7set", "nicht", "sein", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wer sich Amor kann verstecken,", "tokens": ["Wer", "sich", "A\u00b7mor", "kann", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "NE", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann auch nimmer selig lieben,", "tokens": ["Kann", "auch", "nim\u00b7mer", "se\u00b7lig", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer ihn aus dem Schlaf kann wecken,", "tokens": ["Wer", "ihn", "aus", "dem", "Schlaf", "kann", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann das Kindlein hart betr\u00fcben:", "tokens": ["Kann", "das", "Kin\u00b7dlein", "hart", "be\u00b7tr\u00fc\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei auch Lieb durch Sch\u00f6nheit fl\u00fcchtig,", "tokens": ["Sei", "auch", "Lieb", "durch", "Sch\u00f6n\u00b7heit", "fl\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wir entfliehen ja mit ihr,", "tokens": ["Wir", "ent\u00b7flie\u00b7hen", "ja", "mit", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bl\u00fche Wein und trage t\u00fcchtig,", "tokens": ["Bl\u00fc\u00b7he", "Wein", "und", "tra\u00b7ge", "t\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00f6nre Kinder bleiben hier.", "tokens": ["Sch\u00f6n\u00b7re", "Kin\u00b7der", "blei\u00b7ben", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Statt des einen Amor viele,", "tokens": ["Statt", "des", "ei\u00b7nen", "A\u00b7mor", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NE", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Viele Amors ohne Fl\u00fcgel", "tokens": ["Vie\u00b7le", "A\u00b7mors", "oh\u00b7ne", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kr\u00e4nzen Grazien im Spiele", "tokens": ["Kr\u00e4n\u00b7zen", "Gra\u00b7zi\u00b7en", "im", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du siehst dich ohne Spiegel:", "tokens": ["Und", "du", "siehst", "dich", "oh\u00b7ne", "Spie\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Siehst du deine Sch\u00f6nheit wieder", "tokens": ["Siehst", "du", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In den Kindern, die einst dein,", "tokens": ["In", "den", "Kin\u00b7dern", ",", "die", "einst", "dein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schlage nicht die Augen nieder:", "tokens": ["Schla\u00b7ge", "nicht", "die", "Au\u00b7gen", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach wie sch\u00f6n, so sch\u00f6n zu sein.", "tokens": ["Ach", "wie", "sch\u00f6n", ",", "so", "sch\u00f6n", "zu", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "$,", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}