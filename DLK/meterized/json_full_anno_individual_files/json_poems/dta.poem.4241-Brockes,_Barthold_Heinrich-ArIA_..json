{"dta.poem.4241": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ArIA .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Gefl\u00fcgels k\u00fcnstlich Klingen", "tokens": ["Des", "Ge\u00b7fl\u00fc\u00b7gels", "k\u00fcnst\u00b7lich", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stimmt gewi\u00df kein Ungefehr.", "tokens": ["Stimmt", "ge\u00b7wi\u00df", "kein", "Un\u00b7ge\u00b7fehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So wie alle Ding\u2019 entspringen", "tokens": ["So", "wie", "al\u00b7le", "Ding'", "ent\u00b7sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus des Sch\u00f6pfers Weisheit-Meer;", "tokens": ["Aus", "des", "Sch\u00f6p\u00b7fers", "Weis\u00b7heit\u00b7Meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wu\u00dft\u2019 auch GOtt hervorzubringen,", "tokens": ["Wu\u00dft'", "auch", "Gott", "her\u00b7vor\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Luft, so helle Ch\u00f6re", "tokens": ["In", "der", "Luft", ",", "so", "hel\u00b7le", "Ch\u00f6\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fcr das menschliche Geh\u00f6re,", "tokens": ["F\u00fcr", "das", "menschli\u00b7che", "Ge\u00b7h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Welches Er uns gleichfalls schenkt.", "tokens": ["Wel\u00b7ches", "Er", "uns", "gleich\u00b7falls", "schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Darum, liebste Menschen, denkt,", "tokens": ["Da\u00b7rum", ",", "liebs\u00b7te", "Men\u00b7schen", ",", "denkt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Und erweget diese Lehre:", "tokens": ["Und", "er\u00b7we\u00b7get", "die\u00b7se", "Leh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Aller V\u00f6gel Zungen singen", "tokens": ["Al\u00b7ler", "V\u00f6\u00b7gel", "Zun\u00b7gen", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Uns zur Lust, und GOtt zur Ehre.", "tokens": ["Uns", "zur", "Lust", ",", "und", "Gott", "zur", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "KON", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Des Gefl\u00fcgels rc.", "tokens": ["Des", "Ge\u00b7fl\u00fc\u00b7gels", "rc", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}