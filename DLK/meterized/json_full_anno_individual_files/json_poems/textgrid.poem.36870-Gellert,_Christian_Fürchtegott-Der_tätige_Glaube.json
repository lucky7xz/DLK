r Liebe bleibet.", "tokens": ["Wer", "in", "der", "Lie\u00b7be", "blei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Lieb ist's, die die Cherubim,", "tokens": ["Die", "Lieb", "ist's", ",", "die", "die", "Che\u00b7ru\u00b7bim", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott zu gehorchen, treibet.", "tokens": ["Gott", "zu", "ge\u00b7hor\u00b7chen", ",", "trei\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gott ist die Lieb; an seinem Heil", "tokens": ["Gott", "ist", "die", "Lieb", ";", "an", "sei\u00b7nem", "Heil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat ohne Liebe niemand teil.", "tokens": ["Hat", "oh\u00b7ne", "Lie\u00b7be", "nie\u00b7mand", "teil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PIS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}