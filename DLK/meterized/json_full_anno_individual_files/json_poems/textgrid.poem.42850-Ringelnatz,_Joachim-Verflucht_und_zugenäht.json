{"textgrid.poem.42850": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Verflucht und zugen\u00e4ht", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sollte den Gesetzen", "tokens": ["Man", "soll\u00b7te", "den", "Ge\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Kleinigkeiten", "tokens": ["In", "Klei\u00b7nig\u00b7kei\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ein Bein stellen und sie verletzen", "tokens": ["Ein", "Bein", "stel\u00b7len", "und", "sie", "ver\u00b7let\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "PPER", "VVINF"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und sie, von G\u00f6nnern geldunterst\u00fctzt,", "tokens": ["Und", "sie", ",", "von", "G\u00f6n\u00b7nern", "geld\u00b7un\u00b7ter\u00b7st\u00fctzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u00dcberschreiten.", "tokens": ["\u00dc\u00b7bersc\u00b7hrei\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Man sollte den Richter,", "tokens": ["Man", "soll\u00b7te", "den", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Der K\u00fcnstler, Dichter", "tokens": ["Der", "K\u00fcnst\u00b7ler", ",", "Dich\u00b7ter"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Oder nur Mensch ist, unb\u00e4ndig verehren.", "tokens": ["O\u00b7der", "nur", "Mensch", "ist", ",", "un\u00b7b\u00e4n\u00b7dig", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VAFIN", "$,", "ADJD", "VVFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Man sollte das andre, kontr\u00e4re Gelichter", "tokens": ["Man", "soll\u00b7te", "das", "and\u00b7re", ",", "kont\u00b7r\u00e4\u00b7re", "Ge\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Zermalmen und sich selber vermehren.", "tokens": ["Zer\u00b7mal\u00b7men", "und", "sich", "sel\u00b7ber", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PRF", "ADV", "VVINF", "$."], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Man sollte so sein, wie ich es bin.", "tokens": ["Man", "soll\u00b7te", "so", "sein", ",", "wie", "ich", "es", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "VAINF", "$,", "PWAV", "PPER", "PPER", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Man sollte \u2013 \u2013", "tokens": ["Man", "soll\u00b7te", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PIS", "VMFIN", "$(", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Wenn nicht der liebe Gott es hin", "tokens": ["Wenn", "nicht", "der", "lie\u00b7be", "Gott", "es", "hin"], "token_info": ["word", "wor