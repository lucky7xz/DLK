{"textgrid.poem.46473": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ziegelstein an Ziegelstein mit Kalk und Schwei\u00df geklebt \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1915, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ziegelstein an Ziegelstein mit Kalk und Schwei\u00df geklebt \u2013", "tokens": ["Zie\u00b7gel\u00b7stein", "an", "Zie\u00b7gel\u00b7stein", "mit", "Kalk", "und", "Schwei\u00df", "ge\u00b7klebt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Rote, wei\u00dfgefugte Mauer \u00fcber Mauern strebt \u2013", "tokens": ["Ro\u00b7te", ",", "wei\u00df\u00b7ge\u00b7fug\u00b7te", "Mau\u00b7er", "\u00fc\u00b7ber", "Mau\u00b7ern", "strebt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Winden knirschen \u2013 H\u00e4nde heben, fassen \u2013", "tokens": ["Win\u00b7den", "knir\u00b7schen", "\u2013", "H\u00e4n\u00b7de", "he\u00b7ben", ",", "fas\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "NN", "VVFIN", "$,", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Axtschall \u2013 roher Dachstuhl Richtfestbier \u2013", "tokens": ["Axt\u00b7schall", "\u2013", "ro\u00b7her", "Dach\u00b7stuhl", "Richt\u00b7fest\u00b7bier", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "T\u00fcr und Fensterglas ward eingelassen \u2013", "tokens": ["T\u00fcr", "und", "Fens\u00b7ter\u00b7glas", "ward", "ein\u00b7ge\u00b7las\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "So wuchsest du in dieser Stra\u00dfe hier:", "tokens": ["So", "wuch\u00b7sest", "du", "in", "die\u00b7ser", "Stra\u00b7\u00dfe", "hier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kummergraue, f\u00fcnfstockhohe Mietskaserne.", "tokens": ["Kum\u00b7mer\u00b7grau\u00b7e", ",", "f\u00fcnf\u00b7stock\u00b7ho\u00b7he", "Miets\u00b7ka\u00b7ser\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Achtzigfensterbreite, mit dem G\u00e4rtchen schmal davor,", "tokens": ["Acht\u00b7zig\u00b7fens\u00b7ter\u00b7brei\u00b7te", ",", "mit", "dem", "G\u00e4rt\u00b7chen", "schmal", "da\u00b7vor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "ADJD", "PAV", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Mit dem Eisentor, der tr\u00fcben Flurlaterne \u2013", "tokens": ["Mit", "dem", "Ei\u00b7sen\u00b7tor", ",", "der", "tr\u00fc\u00b7ben", "Flur\u00b7la\u00b7ter\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Du Haus, das Jahr um Jahr vom Sonnenprall bemalt,", "tokens": ["Du", "Haus", ",", "das", "Jahr", "um", "Jahr", "vom", "Son\u00b7nen\u00b7prall", "be\u00b7malt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ART", "NN", "APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Hagelschnee und Regensturz betr\u00e4uft, bestrichen,", "tokens": ["Von", "Ha\u00b7gel\u00b7schnee", "und", "Re\u00b7gen\u00b7sturz", "be\u00b7tr\u00e4uft", ",", "be\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von D\u00e4mmerung umrauscht, von Wintern\u00e4chten \u00fcberschlichen,", "tokens": ["Von", "D\u00e4m\u00b7me\u00b7rung", "um\u00b7rauscht", ",", "von", "Win\u00b7ter\u00b7n\u00e4ch\u00b7ten", "\u00fc\u00b7berschli\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vom Mond, von Gaslaternenschein bestrahlt \u2013", "tokens": ["Vom", "Mond", ",", "von", "Gas\u00b7la\u00b7ter\u00b7nen\u00b7schein", "be\u00b7strahlt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von Wagenfahrt ersch\u00fcttert und von Stra\u00dfenbraus,", "tokens": ["Von", "Wa\u00b7gen\u00b7fahrt", "er\u00b7sch\u00fct\u00b7tert", "und", "von", "Stra\u00b7\u00dfen\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von Kinderschrei, von Werkstattl\u00e4rm durchzittert,", "tokens": ["Von", "Kin\u00b7der\u00b7schrei", ",", "von", "Werk\u00b7statt\u00b7l\u00e4rm", "durch\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Von Sterbezimmerschweigen schw\u00fcl durchwittert \u2013", "tokens": ["Von", "Ster\u00b7be\u00b7zim\u00b7mer\u00b7schwei\u00b7gen", "schw\u00fcl", "durch\u00b7wit\u00b7tert", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du, des kleinen Lebens und des gro\u00dfen Todes Haus.", "tokens": ["Du", ",", "des", "klei\u00b7nen", "Le\u00b7bens", "und", "des", "gro\u00b7\u00dfen", "To\u00b7des", "Haus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.4": {"line.1": {"text": "Wer hat nicht diese Dielen, diese Schwellen schon beschritten,", "tokens": ["Wer", "hat", "nicht", "die\u00b7se", "Die\u00b7len", ",", "die\u00b7se", "Schwel\u00b7len", "schon", "be\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "PDAT", "NN", "$,", "PDAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Hinter diesen T\u00fcren Sorge oder Liebe schon gelitten,", "tokens": ["Hin\u00b7ter", "die\u00b7sen", "T\u00fc\u00b7ren", "Sor\u00b7ge", "o\u00b7der", "Lie\u00b7be", "schon", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Am K\u00fcchentisch das karge Brot gebrochen,", "tokens": ["Am", "K\u00fc\u00b7chen\u00b7tisch", "das", "kar\u00b7ge", "Brot", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Zeitungen von Krieg und Politik gesprochen \u2013", "tokens": ["Aus", "Zei\u00b7tun\u00b7gen", "von", "Krieg", "und", "Po\u00b7li\u00b7tik", "ge\u00b7spro\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "All des Alltags Wandrer, die hier eingezogen:", "tokens": ["All", "des", "All\u00b7tags", "Wand\u00b7rer", ",", "die", "hier", "ein\u00b7ge\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Arbeitsmann und junges Weib;", "tokens": ["Ar\u00b7beits\u00b7mann", "und", "jun\u00b7ges", "Weib", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Rentnerpaar, verarmt, vom Leben nur betrogen,", "tokens": ["Rent\u00b7ner\u00b7paar", ",", "ver\u00b7armt", ",", "vom", "Le\u00b7ben", "nur", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Mit stillem Sinn, erloschnem Leib;", "tokens": ["Mit", "stil\u00b7lem", "Sinn", ",", "er\u00b7lo\u00b7schnem", "Leib", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Handwerksmeister mit den sieben lauten Jungen;", "tokens": ["Hand\u00b7werks\u00b7meis\u00b7ter", "mit", "den", "sie\u00b7ben", "lau\u00b7ten", "Jun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Schreiber, H\u00e4ndler, Lehrerstochter, die so gern gesungen \u2013 \u2013", "tokens": ["Schrei\u00b7ber", ",", "H\u00e4nd\u00b7ler", ",", "Leh\u00b7rer\u00b7stoch\u00b7ter", ",", "die", "so", "gern", "ge\u00b7sun\u00b7gen", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "$(", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.5": {"line.1": {"text": "Wieviel schwarze S\u00e4rge sah man auch hinunter schleppen.", "tokens": ["Wie\u00b7viel", "schwar\u00b7ze", "S\u00e4r\u00b7ge", "sah", "man", "auch", "hin\u00b7un\u00b7ter", "schlep\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PIS", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Wieviel neue Mieter, neue Menschen kamen;", "tokens": ["Wie\u00b7viel", "neu\u00b7e", "Mie\u00b7ter", ",", "neu\u00b7e", "Men\u00b7schen", "ka\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Trugen M\u00f6bel, stiegen \u00fcber diese Treppen,", "tokens": ["Tru\u00b7gen", "M\u00f6\u00b7bel", ",", "stie\u00b7gen", "\u00fc\u00b7ber", "die\u00b7se", "Trep\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Andere Familien, andere Gesichter, andre Namen.", "tokens": ["An\u00b7de\u00b7re", "Fa\u00b7mi\u00b7li\u00b7en", ",", "an\u00b7de\u00b7re", "Ge\u00b7sich\u00b7ter", ",", "and\u00b7re", "Na\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.5": {"text": "Von deren Gl\u00fcck und Fluch und dringlichem Gebet", "tokens": ["Von", "de\u00b7ren", "Gl\u00fcck", "und", "Fluch", "und", "dring\u00b7li\u00b7chem", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Der Schatten noch in diesen R\u00e4umen weht.", "tokens": ["Der", "Schat\u00b7ten", "noch", "in", "die\u00b7sen", "R\u00e4u\u00b7men", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Geist der V\u00e4ter, die hier feierabendlich versammelt waren,", "tokens": ["Geist", "der", "V\u00e4\u00b7ter", ",", "die", "hier", "fei\u00b7er\u00b7a\u00b7bend\u00b7lich", "ver\u00b7sam\u00b7melt", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "O' Geist der herben M\u00fctter, die in diesen W\u00e4nden Kinder einst gebaren,", "tokens": ["O'", "Geist", "der", "her\u00b7ben", "M\u00fct\u00b7ter", ",", "die", "in", "die\u00b7sen", "W\u00e4n\u00b7den", "Kin\u00b7der", "einst", "ge\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PDAT", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Kleinkindergeist ins graue Licht der Not gezw\u00e4ngt \u2013", "tokens": ["Klein\u00b7kin\u00b7der\u00b7geist", "ins", "grau\u00b7e", "Licht", "der", "Not", "ge\u00b7zw\u00e4ngt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beschirmt uns unter diesem Dache, da wir wohnen,", "tokens": ["Be\u00b7schirmt", "uns", "un\u00b7ter", "die\u00b7sem", "Da\u00b7che", ",", "da", "wir", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die wir, wie ihr auch einst mit Schwei\u00df und Kraft der Arbeit fronen;", "tokens": ["Die", "wir", ",", "wie", "ihr", "auch", "einst", "mit", "Schwei\u00df", "und", "Kraft", "der", "Ar\u00b7beit", "fro\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], 