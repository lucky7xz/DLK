{"textgrid.poem.37210": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trinkt und f\u00fcllt ohn Unterla\u00df", "tokens": ["Trinkt", "und", "f\u00fcllt", "ohn", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Pokal, den Rosen kr\u00e4nzen,", "tokens": ["Den", "Po\u00b7kal", ",", "den", "Ro\u00b7sen", "kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis wir gleich dem Wein im Glas,", "tokens": ["Bis", "wir", "gleich", "dem", "Wein", "im", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich des Kranzes Rosen gl\u00e4nzen!", "tokens": ["Gleich", "des", "Kran\u00b7zes", "Ro\u00b7sen", "gl\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So verachten wollen wir", "tokens": ["So", "ver\u00b7ach\u00b7ten", "wol\u00b7len", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VMFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles Gold in Schicht und Schachten,", "tokens": ["Al\u00b7les", "Gold", "in", "Schicht", "und", "Schach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wollen auch der Ruhmbegier,", "tokens": ["Wol\u00b7len", "auch", "der", "Ruhm\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch der Liebe selbst nicht achten.", "tokens": ["Auch", "der", "Lie\u00b7be", "selbst", "nicht", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heut ist unser! La\u00dft das heut,", "tokens": ["Heut", "ist", "un\u00b7ser", "!", "La\u00dft", "das", "heut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "$.", "VVIMP", "ART", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Freund', uns so beh\u00e4glich halten,", "tokens": ["Freund'", ",", "uns", "so", "be\u00b7h\u00e4g\u00b7lich", "hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das es schier ihm selber reut,", "tokens": ["Das", "es", "schier", "ihm", "sel\u00b7ber", "reut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich in morgen umzustalten!", "tokens": ["Sich", "in", "mor\u00b7gen", "um\u00b7zu\u00b7stal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADV", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute lacht uns noch die Welt,", "tokens": ["Heu\u00b7te", "lacht", "uns", "noch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Heut ist alles wohl geborgen;", "tokens": ["Heut", "ist", "al\u00b7les", "wohl", "ge\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Guten G\u00f6ttern heimgestellt", "tokens": ["Gu\u00b7ten", "G\u00f6t\u00b7tern", "heim\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bleibe bis er kommt der Morgen!", "tokens": ["Blei\u00b7be", "bis", "er", "kommt", "der", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Trinkt und f\u00fcllt ohn Unterla\u00df", "tokens": ["Trinkt", "und", "f\u00fcllt", "ohn", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Pokal, den Rosen kr\u00e4nzen,", "tokens": ["Den", "Po\u00b7kal", ",", "den", "Ro\u00b7sen", "kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis wir gleich dem Wein im Glas,", "tokens": ["Bis", "wir", "gleich", "dem", "Wein", "im", "Glas", ","], "token_info": ["word", "word", "word", "word", "word",