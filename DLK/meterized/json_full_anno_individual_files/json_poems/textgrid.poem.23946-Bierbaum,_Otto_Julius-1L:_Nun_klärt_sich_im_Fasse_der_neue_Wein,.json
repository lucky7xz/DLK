{"textgrid.poem.23946": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun kl\u00e4rt sich im Fasse der neue Wein,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun kl\u00e4rt sich im Fasse der neue Wein,", "tokens": ["Nun", "kl\u00e4rt", "sich", "im", "Fas\u00b7se", "der", "neu\u00b7e", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Doch drau\u00dfen wird es tr\u00fcbe,", "tokens": ["Doch", "drau\u00b7\u00dfen", "wird", "es", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur manchmal thut der Sonnenschein,", "tokens": ["Nur", "manch\u00b7mal", "thut", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob er den Nebel h\u00fcbe;", "tokens": ["Als", "ob", "er", "den", "Ne\u00b7bel", "h\u00fc\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das Feld behauptet stolz allein", "tokens": ["Das", "Feld", "be\u00b7haup\u00b7tet", "stolz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die brave Zuckerr\u00fcbe,", "tokens": ["Die", "br