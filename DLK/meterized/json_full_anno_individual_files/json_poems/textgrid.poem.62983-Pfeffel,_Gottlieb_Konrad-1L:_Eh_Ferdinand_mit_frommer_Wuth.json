{"textgrid.poem.62983": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eh Ferdinand mit frommer Wuth", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eh Ferdinand mit frommer Wuth", "tokens": ["Eh", "Fer\u00b7di\u00b7nand", "mit", "from\u00b7mer", "Wuth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mauren von sich stie\u00df,", "tokens": ["Die", "Mau\u00b7ren", "von", "sich", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Flo\u00df Omars junges Heldenblut", "tokens": ["Flo\u00df", "O\u00b7mars", "jun\u00b7ges", "Hel\u00b7den\u00b7blut"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Gusmanns Ritterspie\u00df.", "tokens": ["Durch", "Gus\u00b7manns", "Rit\u00b7ter\u00b7spie\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aus Furcht der Rache (reich und gro\u00df", "tokens": ["Aus", "Furcht", "der", "Ra\u00b7che", "(", "reich", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$(", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War dieser Saracen)", "tokens": ["War", "die\u00b7ser", "Sa\u00b7ra\u00b7cen", ")"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Floh Gusmann und blieb athemlos", "tokens": ["Floh", "Gus\u00b7mann", "und", "blieb", "at\u00b7hem\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor einem Garten stehn.", "tokens": ["Vor", "ei\u00b7nem", "Gar\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hoch war die Mauer, doch er schwang", "tokens": ["Hoch", "war", "die", "Mau\u00b7er", ",", "doch", "er", "schwang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sich wie ein Pfeil hinein,", "tokens": ["Sich", "wie", "ein", "Pfeil", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fand in einem Bogengang", "tokens": ["Und", "fand", "in", "ei\u00b7nem", "Bo\u00b7gen\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Herrn des Guts allein.", "tokens": ["Den", "Herrn", "des", "Guts", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er fleht um Schutz. Mit seinem Stab", "tokens": ["Er", "fleht", "um", "Schutz", ".", "Mit", "sei\u00b7nem", "Stab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4gt Emir Ibrahim", "tokens": ["Schl\u00e4gt", "E\u00b7mir", "Ib\u00b7ra\u00b7him"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voll Ernst itzt einen Pfersich ab", "tokens": ["Voll", "Ernst", "itzt", "ei\u00b7nen", "Pfer\u00b7sich", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und theilet ihn mit ihm.", "tokens": ["Und", "thei\u00b7let", "ihn", "mit", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nimm hin, sprach er, du bist mein Gast,", "tokens": ["Nimm", "hin", ",", "sprach", "er", ",", "du", "bist", "mein", "Gast", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies ist des Schutzes Pfand,", "tokens": ["Dies", "ist", "des", "Schut\u00b7zes", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den du von mir zu hoffen hast", "tokens": ["Den", "du", "von", "mir", "zu", "hof\u00b7fen", "hast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gab ihm seine Hand.", "tokens": ["Und", "gab", "ihm", "sei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch pl\u00f6tzlich rief ein M\u00fctterlein", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "rief", "ein", "M\u00fct\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den edeln Greis hinaus;", "tokens": ["Den", "e\u00b7deln", "Greis", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schlo\u00df, um unentdeckt zu seyn,", "tokens": ["Er", "schlo\u00df", ",", "um", "un\u00b7ent\u00b7deckt", "zu", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Gast ins Gartenhaus.", "tokens": ["Den", "Gast", "ins", "Gar\u00b7ten\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es wurde Mitternacht; es kam", "tokens": ["Es", "wur\u00b7de", "Mit\u00b7ter\u00b7nacht", ";", "es", "kam"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der neue Gastfreund nicht.", "tokens": ["Der", "neu\u00b7e", "Gast\u00b7freund", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun k\u00f6mmt er; aber bleicher Gram", "tokens": ["Nun", "k\u00f6mmt", "er", ";", "a\u00b7ber", "blei\u00b7cher", "Gram"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entstellet sein Gesicht.", "tokens": ["Ent\u00b7stel\u00b7let", "sein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Den du erschlugst, grausamer Christ,", "tokens": ["Den", "du", "er\u00b7schlugst", ",", "grau\u00b7sa\u00b7mer", "Christ", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach er, der war mein Sohn:", "tokens": ["Sprach", "er", ",", "der", "war", "mein", "Sohn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00f6n ist die Rache, sch\u00f6ner ist", "tokens": ["Sch\u00f6n", "ist", "die", "Ra\u00b7che", ",", "sch\u00f6\u00b7ner", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADJA", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gehaltner Treue Lohn.", "tokens": ["Ge\u00b7halt\u00b7ner", "Treu\u00b7e", "Lohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Fleuch; vor der Gartenth\u00fcre steht", "tokens": ["Fleuch", ";", "vor", "der", "Gar\u00b7tent\u00b7h\u00fc\u00b7re", "steht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein bestes Pferd. Man sucht", "tokens": ["Mein", "bes\u00b7tes", "Pferd", ".", "Man", "sucht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PIS", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich an der See. Fleuch nach Toled;", "tokens": ["Dich", "an", "der", "See", ".", "Fleuch", "nach", "To\u00b7led", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$.", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott sch\u00fctze deine Flucht!", "tokens": ["Gott", "sch\u00fct\u00b7ze", "dei\u00b7ne", "Flucht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Siehst du im Greis den halben Gott?", "tokens": ["Siehst", "du", "im", "Greis", "den", "hal\u00b7ben", "Gott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wohlthut seinem Feind,", "tokens": ["Wer", "wohlt\u00b7hut", "sei\u00b7nem", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Sohn, w\u00e4r er ein Hottentott,", "tokens": ["Mein", "Sohn", ",", "w\u00e4r", "er", "ein", "Hot\u00b7ten\u00b7tott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ist er Gottes Freund.", "tokens": ["So", "ist", "er", "Got\u00b7