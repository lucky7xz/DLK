{"textgrid.poem.36449": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An des Prinzen Friedrich Wilhelm, \u00e4ltesten Sohn des Prinzen von Preu\u00dfen, K\u00f6nigliche Hoheit", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tugend sei, sagt man, erschienen", "tokens": ["Die", "Tu\u00b7gend", "sei", ",", "sagt", "man", ",", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "VVFIN", "PIS", "$,", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Einnehmend sch\u00f6n, mit holden Mienen,", "tokens": ["Ein\u00b7neh\u00b7mend", "sch\u00f6n", ",", "mit", "hol\u00b7den", "Mie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem jungen Prinzen ", "tokens": ["Dem", "jun\u00b7gen", "Prin\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dem Sohn des Kaisers Choun, der sie", "tokens": ["Dem", "Sohn", "des", "Kai\u00b7sers", "Choun", ",", "der", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "NE", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von seinem Maler malen lie\u00df,", "tokens": ["Von", "sei\u00b7nem", "Ma\u00b7ler", "ma\u00b7len", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie t\u00e4glich seinem Prinzen wies,", "tokens": ["Sie", "t\u00e4g\u00b7lich", "sei\u00b7nem", "Prin\u00b7zen", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sie ", "tokens": ["Und", "sie"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Sie sagte, sagt man, zu dem Prinzen:", "tokens": ["Sie", "sag\u00b7te", ",", "sagt", "man", ",", "zu", "dem", "Prin\u00b7zen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PIS", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erkennen dich f\u00fcr ihren Herrn!", "tokens": ["Er\u00b7ken\u00b7nen", "dich", "f\u00fcr", "ih\u00b7ren", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich rate, Lieber! geh auf Reisen", "tokens": ["Ich", "ra\u00b7te", ",", "Lie\u00b7ber", "!", "geh", "auf", "Rei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "$.", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In jegliche, du reisest gern!", "tokens": ["In", "jeg\u00b7li\u00b7che", ",", "du", "rei\u00b7sest", "gern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geh! lerne kennen ihre Weisen,", "tokens": ["Geh", "!", "ler\u00b7ne", "ken\u00b7nen", "ih\u00b7re", "Wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und liebe sie wie deine Br\u00fcder,", "tokens": ["Und", "lie\u00b7be", "sie", "wie", "dei\u00b7ne", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und setze dich bei ihnen nieder", "tokens": ["Und", "set\u00b7ze", "dich", "bei", "ih\u00b7nen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf Rasen, oder wo es ist,", "tokens": ["Auf", "Ra\u00b7sen", ",", "o\u00b7der", "wo", "es", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und sage Keinem, wer du bist!", "tokens": ["Und", "sa\u00b7ge", "Kei\u00b7nem", ",", "wer", "du", "bist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und sei wie stumm und froh, zu h\u00f6ren", "tokens": ["Und", "sei", "wie", "stumm", "und", "froh", ",", "zu", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "KOKOM", "ADJD", "KON", "ADJD", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der weisen M\u00e4nner weise Lehren,", "tokens": ["Der", "wei\u00b7sen", "M\u00e4n\u00b7ner", "wei\u00b7se", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und merke dir die weisesten,", "tokens": ["Und", "mer\u00b7ke", "dir", "die", "wei\u00b7ses\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.13": {"text": "Und schreib sie dir ins Herz, und hier", "tokens": ["Und", "schreib", "sie", "dir", "ins", "Herz", ",", "und", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Auf diese goldnen T\u00e4felchen;", "tokens": ["Auf", "die\u00b7se", "gold\u00b7nen", "T\u00e4\u00b7fel\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.15": {"text": "Zu dem Gebrauch schenk ich sie dir.", "tokens": ["Zu", "dem", "Ge\u00b7brauch", "schenk", "ich", "sie", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Prinz ergriff mit schneller Hand", "tokens": ["Der", "Prinz", "er\u00b7griff", "mit", "schnel\u00b7ler", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die T\u00e4felchen; die Schenkerin,", "tokens": ["Die", "T\u00e4\u00b7fel\u00b7chen", ";", "die", "Schen\u00b7ke\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebliche, die Zauberin,", "tokens": ["Die", "Lieb\u00b7li\u00b7che", ",", "die", "Zau\u00b7be\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Auf ihren Tafeln aber stand:", "tokens": ["Auf", "ih\u00b7ren", "Ta\u00b7feln", "a\u00b7ber", "stand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sei weise! sei gerecht! sei gut!", "tokens": ["Sei", "wei\u00b7se", "!", "sei", "ge\u00b7recht", "!", "sei", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "VAFIN", "ADJD", "$.", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn ein Weiser Thaten thut", "tokens": ["Und", "wenn", "ein", "Wei\u00b7ser", "Tha\u00b7ten", "thut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch edler als die Deinigen, dann eile", "tokens": ["Noch", "ed\u00b7ler", "als", "die", "Dei\u00b7ni\u00b7gen", ",", "dann", "ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "KOKOM", "ART", "PPOSS", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sie nachzuthun; hast keine Weile,", "tokens": ["Sie", "nach\u00b7zu\u00b7thun", ";", "hast", "kei\u00b7ne", "Wei\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "$.", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mein Sohn! auf Polstern auszuruhn!", "tokens": ["Mein", "Sohn", "!", "auf", "Pols\u00b7tern", "aus\u00b7zu\u00b7ruhn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wir leben einmal! was uns fehlet", "tokens": ["Wir", "le\u00b7ben", "ein\u00b7mal", "!", "was", "uns", "feh\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "An guten Thaten, wird gez\u00e4hlet,", "tokens": ["An", "gu\u00b7ten", "Tha\u00b7ten", ",", "wird", "ge\u00b7z\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und alles, was wir Gutes thun!", "tokens": ["Und", "al\u00b7les", ",", "was", "wir", "Gu\u00b7tes", "thun", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Prinz besah die Schrift an hellem Lichte, stand", "tokens": ["Der", "Prinz", "be\u00b7sah", "die", "Schrift", "an", "hel\u00b7lem", "Lich\u00b7te", ",", "stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Betreten lange, fand,", "tokens": ["Be\u00b7tre\u00b7ten", "lan\u00b7ge", ",", "fand", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sei von seines Vaters Hand.", "tokens": ["Sie", "sei", "von", "sei\u00b7nes", "Va\u00b7ters", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Tugend sei, sagt man, erschienen", "tokens": ["Die", "Tu\u00b7gend", "sei", ",", "sagt", "man", ",", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "VVFIN", "PIS", "$,", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Einnehmend sch\u00f6n, mit holden Mienen,", "tokens": ["Ein\u00b7neh\u00b7mend", "sch\u00f6n", ",", "mit", "hol\u00b7den", "Mie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem jungen Prinzen ", "tokens": ["Dem", "jun\u00b7gen", "Prin\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dem Sohn des Kaisers Choun, der sie", "tokens": ["Dem", "Sohn", "des", "Kai\u00b7sers", "Choun", ",", "der", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "NE", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von seinem Maler malen lie\u00df,", "tokens": ["Von", "sei\u00b7nem", "Ma\u00b7ler", "ma\u00b7len", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measu