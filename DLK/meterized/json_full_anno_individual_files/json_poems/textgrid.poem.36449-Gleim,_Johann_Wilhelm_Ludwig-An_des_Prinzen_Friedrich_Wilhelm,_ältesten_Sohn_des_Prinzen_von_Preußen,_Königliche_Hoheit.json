and,", "tokens": ["Be\u00b7tre\u00b7ten", "lan\u00b7ge", ",", "fand", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sei von seines Vaters Hand.", "tokens": ["Sie", "sei", "von", "sei\u00b7nes", "Va\u00b7ters", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}