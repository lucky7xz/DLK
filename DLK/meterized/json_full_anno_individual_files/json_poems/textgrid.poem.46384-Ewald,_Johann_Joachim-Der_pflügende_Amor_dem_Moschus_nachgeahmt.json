{"textgrid.poem.46384": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Der pfl\u00fcgende Amor dem Moschus nachgeahmt", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df es auf diese Furchen regnen, die du o Zevs mich ziehen siehst!", "tokens": ["La\u00df", "es", "auf", "die\u00b7se", "Fur\u00b7chen", "reg\u00b7nen", ",", "die", "du", "o", "Zevs", "mich", "zie\u00b7hen", "siehst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PDAT", "NN", "VVINF", "$,", "PRELS", "PPER", "FM", "NE", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+--+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Du zauderst Zevs? Wenn du den Regen mir eine Stunde noch entziehst", "tokens": ["Du", "zau\u00b7derst", "Zevs", "?", "Wenn", "du", "den", "Re\u00b7gen", "mir", "ei\u00b7ne", "Stun\u00b7de", "noch", "ent\u00b7ziehst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "$.", "KOUS", "PPER", "ART", "NN", "PPER", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "So scheue mich, scheu diesen Pflug, aufs neue mach' ich dich zum Stier", "tokens": ["So", "scheu\u00b7e", "mich", ",", "scheu", "die\u00b7sen", "Pflug", ",", "aufs", "neu\u00b7e", "mach'", "ich", "dich", "zum", "Stier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "PDAT", "NN", "$,", "APPRART", "ADJA", "VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "-+-++--+-+-+-+-+", "measure": "iambic.oc