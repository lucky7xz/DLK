{"textgrid.poem.40822": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sah der Aebte Bilder in der Halle,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah der Aebte Bilder in der Halle,", "tokens": ["Ich", "sah", "der", "A\u00b7eb\u00b7te", "Bil\u00b7der", "in", "der", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In schwarzem Kleid, mit Inful, Krummstab alle;", "tokens": ["In", "schwar\u00b7zem", "Kleid", ",", "mit", "In\u00b7ful", ",", "Krumm\u00b7stab", "al\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,", "NN", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir schien's, als st\u00fcnden reihweis' im Spaliere", "tokens": ["Mir", "schien's", ",", "als", "st\u00fcn\u00b7den", "reih\u00b7weis'", "im", "Spa\u00b7lie\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJA", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um mich des Grabes Gardegrenadiere.", "tokens": ["Um", "mich", "des", "Gra\u00b7bes", "Gar\u00b7de\u00b7gre\u00b7na\u00b7die\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du mit dem Busenkreuz von Edelsteinen,", "tokens": ["Du", "mit", "dem", "Bu\u00b7sen\u00b7kreuz", "von", "E\u00b7del\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "J\u00fcngling, magst selbst ein lebend Grabmal scheinen;", "tokens": ["J\u00fcng\u00b7ling", ",", "magst", "selbst", "ein", "le\u00b7bend", "Grab\u00b7mal", "schei\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "ADV", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Mich d\u00fcnkt's, als ob dir unter'm Kreuze l\u00e4gen", "tokens": ["Mich", "d\u00fcnkt's", ",", "als", "ob", "dir", "un\u00b7ter'm", "Kreu\u00b7ze", "l\u00e4\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begraben deines Herzens Fried' und Segen.", "tokens": ["Be\u00b7gra\u00b7ben", "dei\u00b7nes", "Her\u00b7zens", "Fried'", "und", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du, Greis, gebeugt zum Buch vor dir, dem alten,", "tokens": ["Du", ",", "Greis", ",", "ge\u00b7beugt", "zum", "Buch", "vor", "dir", ",", "dem", "al\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVPP", "APPRART", "NN", "APPR", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du gleichst, vertieft in seine morschen Spalten,", "tokens": ["Du", "gleichst", ",", "ver\u00b7tieft", "in", "sei\u00b7ne", "mor\u00b7schen", "Spal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Weide, die sich neigt zum Stein am Grabe,", "tokens": ["Der", "Wei\u00b7de", ",", "die", "sich", "neigt", "zum", "Stein", "am", "Gra\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ob sie's, seine Schrift zu lesen, labe.", "tokens": ["Als", "ob", "sie's", ",", "sei\u00b7ne", "Schrift", "zu", "le\u00b7sen", ",", "la\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du, kr\u00e4ft'ger Mann, wie st\u00fcnd' ein Schwert dir pr\u00e4chtig!", "tokens": ["Du", ",", "kr\u00e4ft'\u00b7ger", "Mann", ",", "wie", "st\u00fcnd'", "ein", "Schwert", "dir", "pr\u00e4ch\u00b7tig", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl auch das Zepter schw\u00e4ng'st du gut und m\u00e4chtig!", "tokens": ["Wohl", "auch", "das", "Zep\u00b7ter", "schw\u00e4ng'st", "du", "gut", "und", "m\u00e4ch\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Schade! Doch vielleicht an einem Tage", "tokens": ["Wie", "Scha\u00b7de", "!", "Doch", "viel\u00b7leicht", "an", "ei\u00b7nem", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$.", "KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wiegt Zepter, Krummstab, Schwert gleich schwer zur Wage.", "tokens": ["Wiegt", "Zep\u00b7ter", ",", "Krumm\u00b7stab", ",", "Schwert", "gleich", "schwer", "zur", "Wa\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "$,", "NN", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du dort, de\u00df Aug' voll Lebensglanzes spielend", "tokens": ["Du", "dort", ",", "de\u00df", "Aug'", "voll", "Le\u00b7bens\u00b7glan\u00b7zes", "spie\u00b7lend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "ADJD", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lieb\u00e4ugelt, nach dem Todtenkopfe schielend!", "tokens": ["Lie\u00b7b\u00e4u\u00b7gelt", ",", "nach", "dem", "Tod\u00b7ten\u00b7kop\u00b7fe", "schie\u00b7lend", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "ADJD", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Jetzt ist's verkehrt! Ins sch\u00f6ne Aug' des Lebens", "tokens": ["Jetzt", "ist's", "ver\u00b7kehrt", "!", "Ins", "sch\u00f6\u00b7ne", "Aug'", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$.", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schielst du, selbst Todtensch\u00e4del, ach vergebens!", "tokens": ["Schielst", "du", ",", "selbst", "Tod\u00b7ten\u00b7sch\u00e4\u00b7del", ",", "ach", "ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "NN", "$,", "ITJ", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch fort von hier! Es will mir nicht gefallen,", "tokens": ["Doch", "fort", "von", "hier", "!", "Es", "will", "mir", "nicht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "APPR", "ADV", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus sichrem Port zu schaun das Sturmmeer wallen,", "tokens": ["Aus", "sich\u00b7rem", "Port", "zu", "schaun", "das", "Sturm\u00b7meer", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Lebensschiffes Wrack im Zwielicht blinken", "tokens": ["Des", "Le\u00b7bens\u00b7schif\u00b7fes", "Wrack", "im", "Zwie\u00b7licht", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Schwimmer krampfhaft rudern, ach, und \u2013 sinken!", "tokens": ["Und", "Schwim\u00b7mer", "krampf\u00b7haft", "ru\u00b7dern", ",", "ach", ",", "und", "\u2013", "sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVINF", "$,", "ITJ", "$,", "KON", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch halt! Sieh dort, wie Vollmond aufgegangen,", "tokens": ["Doch", "halt", "!", "Sieh", "dort", ",", "wie", "Voll\u00b7mond", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "ADV", "$,", "PWAV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Abbasbild mit vollen, ros'gen Wangen,", "tokens": ["Ein", "Ab\u00b7bas\u00b7bild", "mit", "vol\u00b7len", ",", "ros'\u00b7gen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ehrw\u00fcrd'gen Bauchs, da\u00df fast mir angst, es sprenge", "tokens": ["Ehr\u00b7w\u00fcrd'\u00b7gen", "Bauchs", ",", "da\u00df", "fast", "mir", "angst", ",", "es", "spren\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$,", "KOUS", "ADV", "PPER", "VVPP", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Athemzug des goldnen Rahmens Enge!", "tokens": ["Sein", "A\u00b7them\u00b7zug", "des", "gold\u00b7nen", "Rah\u00b7mens", "En\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Den Maler k\u00fc\u00dft' ich gern, der auf den Wangen", "tokens": ["Den", "Ma\u00b7ler", "k\u00fc\u00dft'", "ich", "gern", ",", "der", "auf", "den", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die\u00df sonn'ge L\u00e4cheln haschend eingefangen,", "tokens": ["Die\u00df", "sonn'\u00b7ge", "L\u00e4\u00b7cheln", "ha\u00b7schend", "ein\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Paradiesesvogel, glanzvoll stille", "tokens": ["Den", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7vo\u00b7gel", ",", "glanz\u00b7voll", "stil\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umschwebend dieses Rosengartens F\u00fclle!", "tokens": ["Um\u00b7schwe\u00b7bend", "die\u00b7ses", "Ro\u00b7sen\u00b7gar\u00b7tens", "F\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Er hie\u00df, wie Josua, die Sonne stehen,", "tokens": ["Er", "hie\u00df", ",", "wie", "Jo\u00b7sua", ",", "die", "Son\u00b7ne", "ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NE", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df sie der Enkel noch mag leuchtend sehen,", "tokens": ["Da\u00df", "sie", "der", "En\u00b7kel", "noch", "mag", "leuch\u00b7tend", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df dieses L\u00e4chelns Geister einst nach Jahren", "tokens": ["Da\u00df", "die\u00b7ses", "L\u00e4\u00b7chelns", "Geis\u00b7ter", "einst", "nach", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit mildem Glanz in tr\u00fcbe Seelen fahren!", "tokens": ["Mit", "mil\u00b7dem", "Glanz", "in", "tr\u00fc\u00b7be", "See\u00b7len", "fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wie noch zu uns aus Tagen, l\u00e4ngstvergangen", "tokens": ["Wie", "noch", "zu", "uns", "aus", "Ta\u00b7gen", ",", "l\u00e4ngst\u00b7ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADV", "APPR", "PPER", "APPR", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Manch rundgewalt'ge Tempelkuppeln prangen,", "tokens": ["Manch", "rund\u00b7ge\u00b7walt'\u00b7ge", "Tem\u00b7pel\u00b7kup\u00b7peln", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So in das magre Jetzt aus bessren Tagen", "tokens": ["So", "in", "das", "mag\u00b7re", "Jetzt", "aus", "bess\u00b7ren", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seh' k\u00fchn ich deinen Bauch her\u00fcberragen.", "tokens": ["Seh'", "k\u00fchn", "ich", "dei\u00b7nen", "Bauch", "her\u00b7\u00fc\u00b7berr\u00b7a\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wie eines bombenfesten Kellers Bogen,", "tokens": ["Wie", "ei\u00b7nes", "bom\u00b7ben\u00b7fes\u00b7ten", "Kel\u00b7lers", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drein sich die Fr\u00f6hlichkeit zur\u00fcckgezogen,", "tokens": ["Drein", "sich", "die", "Fr\u00f6h\u00b7lich\u00b7keit", "zu\u00b7r\u00fcck\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der vom Gesch\u00fctz des Witzes nur ersch\u00fcttert,", "tokens": ["Der", "vom", "Ge\u00b7sch\u00fctz", "des", "Wit\u00b7zes", "nur", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur von des Lachens Erdebeben zittert!", "tokens": ["Nur", "von", "des", "La\u00b7chens", "Er\u00b7de\u00b7be\u00b7ben", "zit\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und \u00fcber ihm und seinem Kleid, dem dunkeln,", "tokens": ["Und", "\u00fc\u00b7ber", "ihm", "und", "sei\u00b7nem", "Kleid", ",", "dem", "dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,"