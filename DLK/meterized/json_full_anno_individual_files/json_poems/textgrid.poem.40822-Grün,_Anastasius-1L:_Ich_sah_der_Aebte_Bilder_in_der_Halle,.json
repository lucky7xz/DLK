 "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df keck auf deines Bauches Polsterpf\u00fchle", "tokens": ["Da\u00df", "keck", "auf", "dei\u00b7nes", "Bau\u00b7ches", "Pols\u00b7ter\u00b7pf\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt meine Phantasie, das Kindlein, spiele.", "tokens": ["Jetzt", "mei\u00b7ne", "Phan\u00b7ta\u00b7sie", ",", "das", "Kin\u00b7dlein", ",", "spie\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "So bl\u00fchst du jetzt noch als gef\u00fcllte Rose", "tokens": ["So", "bl\u00fchst", "du", "jetzt", "noch", "als", "ge\u00b7f\u00fcll\u00b7te", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch die\u00df Gestr\u00fcpp' ringsum, das bl\u00e4tterlose;", "tokens": ["Durch", "die\u00df", "Ge\u00b7str\u00fcpp'", "ring\u00b7sum", ",", "das", "bl\u00e4t\u00b7ter\u00b7lo\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "ADV", "$,", "ART", "ADJA", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "So gie\u00dfest du als Vollmond milde Strahle", "tokens": ["So", "gie\u00b7\u00dfest", "du", "als", "Voll\u00b7mond", "mil\u00b7de", "Strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vers\u00f6hnend rings auf d\u00fcstre Trauermale.", "tokens": ["Ver\u00b7s\u00f6h\u00b7nend", "rings", "auf", "d\u00fcst\u00b7re", "Trau\u00b7er\u00b7ma\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Ich sah der Aebte Bilder in der Halle,", "tokens": ["Ich", "sah", "der", "A\u00b7eb\u00b7te", "Bil\u00b7der", "in", "der", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In schwarzem Kleid, mit Inful, Krummstab alle;", "tokens": ["In", "schwar\u00b7zem", "Kleid", ",", "mit", "In\u00b7ful", ",", "Krumm\u00b7stab", "al\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,", "NN", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir schien's, als st\u00fcnden reihweis' im Spaliere", "tokens": ["Mir", "schien's", ",", "als", "st\u00fcn\u00b7den", "reih\u00b7weis'", "im", "Spa\u00b7lie\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJA", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um mich des Grabes Gardegrenadiere.", "tokens": ["Um", "mich", "des", "Gra\u00b7bes", "Gar\u00b7de\u00b7gre\u00b7na\u00b7die\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Du mit dem Busenkreuz von Edelsteinen,", "tokens": ["Du", "mit", "dem", "Bu\u00b7sen\u0