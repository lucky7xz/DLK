{"dta.poem.232": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich die Seuffzer schick nach Jris heilgen Throne/", "tokens": ["Wenn", "ich", "die", "Seuff\u00b7zer", "schick", "nach", "Jris", "heil\u00b7gen", "Thro\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "APPR", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So setz\u2019 ich mich in Quaal/ und kriege Angst zum Lohne:", "tokens": ["So", "setz'", "ich", "mich", "in", "Qua\u00b7al", "/", "und", "krie\u00b7ge", "Angst", "zum", "Loh\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$(", "KON", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mein Lieben wird veracht/ die ", "tokens": ["Mein", "Lie\u00b7ben", "wird", "ver\u00b7acht", "/", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "VAFIN", "VVPP", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie lachet meiner Quaal wenn ich die Seuffzer schick.", "tokens": ["Sie", "la\u00b7chet", "mei\u00b7ner", "Qua\u00b7al", "wenn", "ich", "die", "Seuff\u00b7zer", "schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KOUS", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein Hertze ist verliebt/ mich t\u00f6dten ", "tokens": ["Mein", "Hert\u00b7ze", "ist", "ver\u00b7liebt", "/", "mich", "t\u00f6d\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "VVPP", "$(", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich gebe solches zu/ und acht\u2019 es vor mein Gl\u00fccke:", "tokens": ["Ich", "ge\u00b7be", "sol\u00b7ches", "zu", "/", "und", "acht'", "es", "vor", "mein", "Gl\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKZU", "$(", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch saget mir warum eur Mund die Losung giebt?", "tokens": ["Doch", "sa\u00b7get", "mir", "wa\u00b7rum", "eur", "Mund", "die", "Lo\u00b7sung", "giebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PWAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ihr euch von mir trennt/ mein Hertze ist verliebt.", "tokens": ["Wenn", "ihr", "euch", "von", "mir", "trennt", "/", "mein", "Hert\u00b7ze", "ist", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$(", "PPOSAT", "VVFIN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Das geb ich zu/ doch wenn man Seuffzend mu\u00df verschmachten/", "tokens": ["Das", "geb", "ich", "zu", "/", "doch", "wenn", "man", "Seuff\u00b7zend", "mu\u00df", "ver\u00b7schmach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKZU", "$(", "ADV", "KOUS", "PIS", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und scheun ihr Grausahm-seyn/ so sage ich gantz frey/", "tokens": ["Und", "scheun", "ihr", "Grau\u00b7sahm\u00b7seyn", "/", "so", "sa\u00b7ge", "ich", "gantz", "frey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich achte es gar nicht/ da\u00df ", "tokens": ["Ich", "ach\u00b7te", "es", "gar", "nicht", "/", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$(", "KOUS"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}