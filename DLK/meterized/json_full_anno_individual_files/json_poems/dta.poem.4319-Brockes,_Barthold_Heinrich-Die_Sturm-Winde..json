{"dta.poem.4319": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sturm-Winde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Obgleich bekannt, da\u00df viele Winde, durch einen Luft-", "tokens": ["Ob\u00b7gleich", "be\u00b7kannt", ",", "da\u00df", "vie\u00b7le", "Win\u00b7de", ",", "durch", "ei\u00b7nen", "Luft"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADJD", "$,", "KOUS", "PIAT", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Strohm in der Erden,", "tokens": ["Strohm", "in", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Den das in ihr verschlo\u00dfne Feur mit Macht verjagt, er-", "tokens": ["Den", "das", "in", "ihr", "ver\u00b7schlo\u00df\u00b7ne", "Feur", "mit", "Macht", "ver\u00b7jagt", ",", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ART", "APPR", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "zeuget werden;", "tokens": ["zeu\u00b7get", "wer\u00b7den", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VAINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "So wei\u00df man doch, da\u00df viele Winde auch in der Wolken", "tokens": ["So", "wei\u00df", "man", "doch", ",", "da\u00df", "vie\u00b7le", "Win\u00b7de", "auch", "in", "der", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "KOUS", "PIAT", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "feuchten H\u00f6h'n,", "tokens": ["feuch\u00b7ten", "H\u00f6h'n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Durch Dr\u00fccken, Drengen und Zersprengen der Bl\u00e4sgen,", "tokens": ["Durch", "Dr\u00fc\u00b7cken", ",", "Dren\u00b7gen", "und", "Zer\u00b7spren\u00b7gen", "der", "Bl\u00e4s\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "die von Luft, entsteh'n.", "tokens": ["die", "von", "Luft", ",", "ent\u00b7steh'", "n."], "token_info": ["word", "word", "word", "punct", "word", "abbreviation"], "pos": ["ART", "APPR", "NN", "$,", "XY", "XY"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Von welchen oft ein kleiner Land-Strich ver\u00f6det, alles", "tokens": ["Von", "wel\u00b7chen", "oft", "ein", "klei\u00b7ner", "Lan\u00b7dStrich", "ver\u00b7\u00f6\u00b7det", ",", "al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PWAT", "ADV", "ART", "ADJA", "NN", "VVPP", "$,", "PIS"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "umgekehrt,", "tokens": ["um\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Zerstreuet, ganz vernichtigt wird, und, eh man sichs ver-", "tokens": ["Zer\u00b7streu\u00b7et", ",", "ganz", "ver\u00b7nich\u00b7tigt", "wird", ",", "und", ",", "eh", "man", "sichs", "ver"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "VVPP", "VAFIN", "$,", "KON", "$,", "KOUS", "PIS", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sieht, verheert,", "tokens": ["sieht", ",", "ver\u00b7heert", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Indem, mit gr\u00e4ulicher Gewalt, sie oftmahls, recht wie", "tokens": ["In\u00b7dem", ",", "mit", "gr\u00e4u\u00b7li\u00b7cher", "Ge\u00b7walt", ",", "sie", "oft\u00b7mahls", ",", "recht", "wie"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "ADV", "$,", "ADJD", "KOKOM"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Bley, von oben", "tokens": ["Bley", ",", "von", "o\u00b7ben"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "APPR", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "In wilden Wirbeln abwerts st\u00fcrzen, und fast auf einer", "tokens": ["In", "wil\u00b7den", "Wir\u00b7beln", "ab\u00b7werts", "st\u00fcr\u00b7zen", ",", "und", "fast", "auf", "ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF", "$,", "KON", "ADV", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Stelle toben.", "tokens": ["Stel\u00b7le", "to\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Den Ursprung trifft man ebenfalls in den zersprengten", "tokens": ["Den", "Ur\u00b7sprung", "trifft", "man", "e\u00b7ben\u00b7falls", "in", "den", "zer\u00b7spreng\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Blasen an,", "tokens": ["Bla\u00b7sen", "an", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wenn nemlich einer Wolken Seite der Macht nicht wider-", "tokens": ["Wenn", "nem\u00b7lich", "ei\u00b7ner", "Wol\u00b7ken", "Sei\u00b7te", "der", "Macht", "nicht", "wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "ART", "NN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "stehen kann,", "tokens": ["ste\u00b7hen", "kann", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Die die gedrengte Dehnungs-Kraft der eingeschlo\u00dfnen", "tokens": ["Die", "die", "ge\u00b7dreng\u00b7te", "Deh\u00b7nungs\u00b7Kraft", "der", "ein\u00b7ge\u00b7schlo\u00df\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Luft erreget;", "tokens": ["Luft", "er\u00b7re\u00b7get", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Daher entsteh\u2019n oft solche Winde, wenns blitzet und der", "tokens": ["Da\u00b7her", "ent\u00b7steh'n", "oft", "sol\u00b7che", "Win\u00b7de", ",", "wenns", "blit\u00b7zet", "und", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "PIAT", "NN", "$,", "KOUS", "VVFIN", "KON", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Donner schl\u00e4get.", "tokens": ["Don\u00b7ner", "schl\u00e4\u00b7get", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Der erste Wind, der vor sich hin den Schwarm der Wolken", "tokens": ["Der", "ers\u00b7te", "Wind", ",", "der", "vor", "sich", "hin", "den", "Schwarm", "der", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PRF", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "treibt und tr\u00e4get,", "tokens": ["treibt", "und", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Weht meistens im geraden Strich, wenn oft ein andrer", "tokens": ["Weht", "meis\u00b7tens", "im", "ge\u00b7ra\u00b7den", "Strich", ",", "wenn", "oft", "ein", "an\u00b7drer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,", "KOUS", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "niedriger,", "tokens": ["nied\u00b7ri\u00b7ger", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.13": {"text": "Ein dritter h\u00f6her ihm zuwider, und heftig ihm entgegen", "tokens": ["Ein", "drit\u00b7ter", "h\u00f6\u00b7her", "ihm", "zu\u00b7wi\u00b7der", ",", "und", "hef\u00b7tig", "ihm", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "PPER", "ADJD", "$,", "KON", "ADJD", "PPER", "PTKVZ"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "wehet,", "tokens": ["we\u00b7het", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Wodurch der Wolken Druck und Sto\u00df, und ein so grau-", "tokens": ["Wo\u00b7durch", "der", "Wol\u00b7ken", "Druck", "und", "Sto\u00df", ",", "und", "ein", "so", "grau"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "NN", "$,", "KON", "ART", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "ser Sturm entstehet.", "tokens": ["ser", "Sturm", "ent\u00b7ste\u00b7het", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}