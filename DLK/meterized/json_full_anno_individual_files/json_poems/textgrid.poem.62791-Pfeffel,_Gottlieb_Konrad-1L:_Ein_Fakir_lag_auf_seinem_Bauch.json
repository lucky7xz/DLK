{"textgrid.poem.62791": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Fakir lag auf seinem Bauch", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fakir lag auf seinem Bauch", "tokens": ["Ein", "Fa\u00b7kir", "lag", "auf", "sei\u00b7nem", "Bauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lie\u00df, die S\u00fcnder zu erbauen,", "tokens": ["Und", "lie\u00df", ",", "die", "S\u00fcn\u00b7der", "zu", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich nach dem alten Ordensbrauch", "tokens": ["Sich", "nach", "dem", "al\u00b7ten", "Or\u00b7dens\u00b7brauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis auf das Blut mit Ruthen hauen.", "tokens": ["Bis", "auf", "das", "Blut", "mit", "Ru\u00b7then", "hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der P\u00f6bel sah den Wundermann", "tokens": ["Der", "P\u00f6\u00b7bel", "sah", "den", "Wun\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit heiligem Erstaunen an.", "tokens": ["Mit", "hei\u00b7li\u00b7gem", "Er\u00b7stau\u00b7nen", "