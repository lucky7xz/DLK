{"textgrid.poem.39263": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Norwich war's, der tapf're Graf,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Norwich war's, der tapf're Graf,", "tokens": ["Von", "Nor\u00b7wich", "wa\u00b7r's", ",", "der", "tapf'\u00b7re", "Graf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der sa\u00df auf blut'gem Pferd,", "tokens": ["Der", "sa\u00df", "auf", "blut'\u00b7gem", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl quoll ihm hell aus Brust und Helm", "tokens": ["Wohl", "quoll", "ihm", "hell", "aus", "Brust", "und", "Helm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Heldenfeuchte wert.", "tokens": ["Die", "Hel\u00b7den\u00b7feuch\u00b7te", "wert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und als zusammenbrach das Ro\u00df", "tokens": ["Und", "als", "zu\u00b7sam\u00b7men\u00b7brach", "das", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im letzten Todeskrampf,", "tokens": ["Im", "letz\u00b7ten", "To\u00b7des\u00b7krampf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sank er vor Herrn Edward hin", "tokens": ["Da", "sank", "er", "vor", "Herrn", "Ed\u00b7ward", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "NE", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im blut'gen Schaumesdampf.", "tokens": ["Im", "blut'\u00b7gen", "Schau\u00b7mes\u00b7dampf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbherr K\u00f6nig auf! Zu Ro\u00df! Zu Ro\u00df!", "tokens": ["\u00bb", "herr", "K\u00f6\u00b7nig", "auf", "!", "Zu", "Ro\u00df", "!", "Zu", "Ro\u00df", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "PTKVZ", "$.", "APPR", "NN", "$.", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt retten Euer Blut!", "tokens": ["Wollt", "ret\u00b7ten", "Eu\u00b7er", "Blut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seht fechten Euren Heldenspro\u00df", "tokens": ["Seht", "fech\u00b7ten", "Eu\u00b7ren", "Hel\u00b7den\u00b7spro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In rascher Todeswut.\u00ab", "tokens": ["In", "ra\u00b7scher", "To\u00b7des\u00b7wut", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbseht fallen seine Ritter all'", "tokens": ["\u00bb", "seht", "fal\u00b7len", "sei\u00b7ne", "Rit\u00b7ter", "all'"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "VVFIN", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vertreten und zerfetzt;", "tokens": ["Ver\u00b7tre\u00b7ten", "und", "zer\u00b7fetzt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schaut, wie der Frankenlilie Schwert", "tokens": ["Schaut", ",", "wie", "der", "Fran\u00b7ken\u00b7li\u00b7lie", "Schwert"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die scharfe Klinge wetzt.\u00ab", "tokens": ["Die", "schar\u00b7fe", "Klin\u00b7ge", "wetzt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbschaut, wie er ficht im hellen Zorn,", "tokens": ["\u00bb", "schaut", ",", "wie", "er", "ficht", "im", "hel\u00b7len", "Zorn", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das Banner in der Faust,", "tokens": ["Das", "Ban\u00b7ner", "in", "der", "Faust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herr K\u00f6nig, auf! Zu Ro\u00df! Zu Ro\u00df!", "tokens": ["Herr", "K\u00f6\u00b7nig", ",", "auf", "!", "Zu", "Ro\u00df", "!", "Zu", "Ro\u00df", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PTKVZ", "$.", "APPR", "NN", "$.", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df neu das Treffen braust.\u00ab", "tokens": ["Da\u00df", "neu", "das", "Tref\u00b7fen", "braust", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbausfechten wollt' er den ersten Strau\u00df,", "tokens": ["\u00bb", "aus\u00b7fech\u00b7ten", "wollt'", "er", "den", "ers\u00b7ten", "Strau\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VMFIN", "PPER", "ART", "ADJA", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Knabe keck und wild,", "tokens": ["Der", "Kna\u00b7be", "keck", "und", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So mag er fechten als guter Held", "tokens": ["So", "mag", "er", "fech\u00b7ten", "als", "gu\u00b7ter", "Held"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "KOKOM", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und sterben auf Englands Schild.\u00ab", "tokens": ["Und", "ster\u00b7ben", "auf", "En\u00b7glands", "Schild", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbich r\u00fchre nimmer die Klinge mein,", "tokens": ["\u00bb", "ich", "r\u00fch\u00b7re", "nim\u00b7mer", "die", "Klin\u00b7ge", "mei