{"dta.poem.7246": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Ein Jambisch Echonisch Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1640", "urn": "urn:nbn:de:kobv:b4-20685-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach k\u00f6ntich doch den busch ", "tokens": ["Ach", "k\u00f6n\u00b7tich", "doch", "den", "busch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da wo mein Liebster innen ", "tokens": ["Da", "wo", "mein", "Liebs\u00b7ter", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein hertz vor lieb\u2019 ist aufge", "tokens": ["Mein", "hertz", "vor", "lieb'", "ist", "auf\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJD", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "vnd wil vor angst fast gar ver", "tokens": ["vnd", "wil", "vor", "angst", "fast", "gar", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "ADJD", "ADV", "ADV", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich ruff euch an Jhr sch\u00f6nsten Eichen/", "tokens": ["Ich", "ruff", "euch", "an", "Ihr", "sch\u00f6ns\u00b7ten", "Ei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Jhr die W\u00e4lder zieret jtzt", "tokens": ["Die", "Ihr", "die", "W\u00e4l\u00b7der", "zie\u00b7ret", "jtzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch h\u00f6r\u2019 ich nichts als wie da ", "tokens": ["Doch", "h\u00f6r'", "ich", "nichts", "als", "wie", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIS", "KOKOM", "KOKOM", "KOUS"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Der wider-ruff auf mich mit keichen.", "tokens": ["Der", "wi\u00b7der\u00b7ruff", "auf", "mich", "mit", "kei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich komme zu den kl\u00fcfften auch/", "tokens": ["Ich", "kom\u00b7me", "zu", "den", "kl\u00fcff\u00b7ten", "auch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vnd schrey nach meinem alten brauch/", "tokens": ["vnd", "schrey", "nach", "mei\u00b7nem", "al\u00b7ten", "brauch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ist auch g\u00e4ntzlich nichts ", "tokens": ["Da", "ist", "auch", "g\u00e4ntz\u00b7lich", "nichts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "hoffen/", "tokens": ["hof\u00b7fen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Als nur der blo\u00dfe wider", "tokens": ["Als", "nur", "der", "blo\u00b7\u00dfe", "wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "der sich ereiget \u00fcberall/", "tokens": ["der", "sich", "er\u00b7ei\u00b7get", "\u00fc\u00b7be\u00b7rall", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich Jhn mu\u00df ohn ab", "tokens": ["Wenn", "ich", "Jhn", "mu\u00df", "ohn", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "APPR", "PTKVZ"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "lasz ruffen.", "tokens": ["lasz", "ruf\u00b7fen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}