{"dta.poem.4212": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Belustigende und erbauliche  \n Morgen-Gedanken.  \n Nach Anleitung  Mr. Thomsons.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es l\u00e4\u00dft das sanfte Morgen-Licht im bunten Osten sich", "tokens": ["Es", "l\u00e4\u00dft", "das", "sanf\u00b7te", "Mor\u00b7gen\u00b7Licht", "im", "bun\u00b7ten", "Os\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "schon seh'n,", "tokens": ["schon", "seh'n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die k\u00fchle Milch der gr\u00fcnen Pflanzen, der Thau, sinkt aus", "tokens": ["Die", "k\u00fch\u00b7le", "Milch", "der", "gr\u00fc\u00b7nen", "Pflan\u00b7zen", ",", "der", "Thau", ",", "sinkt", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "den grauen H\u00f6h'n.", "tokens": ["den", "grau\u00b7en", "H\u00f6h'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Man siehet an der d\u00fcnnen Luft unsichtbarn Kreyses \u00e4ussern", "tokens": ["Man", "sie\u00b7het", "an", "der", "d\u00fcn\u00b7nen", "Luft", "un\u00b7sicht\u00b7barn", "Krey\u00b7ses", "\u00e4us\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Grenzen,", "tokens": ["Gren\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und, in und durch derselben C\u00f6rper, der D\u00e4mmrung annoch", "tokens": ["Und", ",", "in", "und", "durch", "der\u00b7sel\u00b7ben", "C\u00f6r\u00b7per", ",", "der", "D\u00e4mm\u00b7rung", "an\u00b7noch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "KON", "APPR", "PDAT", "NN", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "schwaches Gl\u00e4nzen", "tokens": ["schwa\u00b7ches", "Gl\u00e4n\u00b7zen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Sich mit den dunklen Schatten mischen, und sie gemach,", "tokens": ["Sich", "mit", "den", "dunk\u00b7len", "Schat\u00b7ten", "mi\u00b7schen", ",", "und", "sie", "ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "PPER", "ADV", "$,"], "meter": "---+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "gemach besiegen.", "tokens": ["ge\u00b7mach", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Die Nacht entweicht mit tr\u00e4gem Schritt, der junge Tag", "tokens": ["Die", "Nacht", "ent\u00b7weicht", "mit", "tr\u00e4\u00b7gem", "Schritt", ",", "der", "jun\u00b7ge", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "erscheinet schnell,", "tokens": ["er\u00b7schei\u00b7net", "schnell", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Formieret eine sch\u00f6ne Weite, um unsre Augen zu vergn\u00fcgen.", "tokens": ["For\u00b7mie\u00b7ret", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Wei\u00b7te", ",", "um", "uns\u00b7re", "Au\u00b7gen", "zu", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.14": {"text": "Er zieht vor einem sch\u00f6nen Schauplatz die Decke weg, macht", "tokens": ["Er", "zieht", "vor", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Schau\u00b7platz", "die", "De\u00b7cke", "weg", ",", "macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$,", "VVFIN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "alles hell,", "tokens": ["al\u00b7les", "hell", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Und stellet, was vor unser Aug\u2019 sich durch die Dunkelheit", "tokens": ["Und", "stel\u00b7let", ",", "was", "vor", "un\u00b7ser", "Aug'", "sich", "durch", "die", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "verlor,", "tokens": ["ver\u00b7lor", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Als w\u00fcrd\u2019 es fast aufs neu\u2019 erschaffen, uns, durch den Wech-", "tokens": ["Als", "w\u00fcrd'", "es", "fast", "aufs", "neu'", "er\u00b7schaf\u00b7fen", ",", "uns", ",", "durch", "den", "Wech"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "APPRART", "ADJD", "VVPP", "$,", "PPER", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.19": {"text": "sel, sch\u00f6ner vor.", "tokens": ["sel", ",", "sch\u00f6\u00b7ner", "vor", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Der fernen Berge neblicht Haupt, der hohen Felsen feuchte", "tokens": ["Der", "fer\u00b7nen", "Ber\u00b7ge", "neb\u00b7licht", "Haupt", ",", "der", "ho\u00b7hen", "Fel\u00b7sen", "feuch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.21": {"text": "Gipfel,", "tokens": ["Gip\u00b7fel", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Der tr\u00e4uflenden gebognen B\u00e4ume gew\u00f6lbte Wolken-gleiche", "tokens": ["Der", "tr\u00e4uf\u00b7len\u00b7den", "ge\u00b7bog\u00b7nen", "B\u00e4u\u00b7me", "ge\u00b7w\u00f6lb\u00b7te", "Wol\u00b7ken\u00b7glei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.23": {"text": "Wipfel", "tokens": ["Wip\u00b7fel"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Erscheinen, nehmen sichtbarlich, mit dem gest\u00e4rkten Tage, zu.", "tokens": ["Er\u00b7schei\u00b7nen", ",", "neh\u00b7men", "sicht\u00b7bar\u00b7lich", ",", "mit", "dem", "ge\u00b7st\u00e4rk\u00b7ten", "Ta\u00b7ge", ",", "zu", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "$,", "APPR", "ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.25": {"text": "Was bis daher im Schlaf begraben, wird munter, und ver-", "tokens": ["Was", "bis", "da\u00b7her", "im", "Schlaf", "be\u00b7gra\u00b7ben", ",", "wird", "mun\u00b7ter", ",", "und", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "PAV", "APPRART", "NN", "VVPP", "$,", "VAFIN", "ADJD", "$,", "KON", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "l\u00e4\u00dft die Ruh.", "tokens": ["l\u00e4\u00dft", "die", "Ruh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Auf Feldern hinkt schon hie und dort, im feuchten gr\u00fcnlich-", "tokens": ["Auf", "Fel\u00b7dern", "hinkt", "schon", "hie", "und", "dort", ",", "im", "feuch\u00b7ten", "gr\u00fcn\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ADV", "KON", "ADV", "$,", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "grauen Grase,", "tokens": ["grau\u00b7en", "Gra\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mit einem ungeschickten H\u00fcpfen, der feige lang-geohrte", "tokens": ["Mit", "ei\u00b7nem", "un\u00b7ge\u00b7schick\u00b7ten", "H\u00fcp\u00b7fen", ",", "der", "fei\u00b7ge", "lang\u00b7geohr\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Haase.", "tokens": ["Haa\u00b7se", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "In W\u00e4ldern sieht das leichte Wild den unverhofften Wan-", "tokens": ["In", "W\u00e4l\u00b7dern", "sieht", "das", "leich\u00b7te", "Wild", "den", "un\u00b7ver\u00b7hoff\u00b7ten", "Wan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "dersmann,", "tokens": ["ders\u00b7mann", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Mit stutzigen verst\u00f6hrten Blicken, und ausgereckten H\u00e4lsen,", "tokens": ["Mit", "stut\u00b7zi\u00b7gen", "ver\u00b7st\u00f6hr\u00b7ten", "Bli\u00b7cken", ",", "und", "aus\u00b7ge\u00b7reck\u00b7ten", "H\u00e4l\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "an.", "tokens": ["an", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Ein\u2019 unvermuhtete Music erwachet gleichsam hin und", "tokens": ["Ein'", "un\u00b7ver\u00b7muh\u00b7te\u00b7te", "Mu\u00b7sic", "er\u00b7wa\u00b7chet", "gleich\u00b7sam", "hin", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "KON"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "wieder,", "tokens": ["wie\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "In ganz verschiednen T\u00f6nen schallen der Creaturen Morgen-", "tokens": ["In", "ganz", "ver\u00b7schied\u00b7nen", "T\u00f6\u00b7nen", "schal\u00b7len", "der", "Crea\u00b7tu\u00b7ren", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Lieder.", "tokens": ["Lie\u00b7der", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der V\u00f6gel angenehmes Gurgeln, der Ochsen und der Schafe", "tokens": ["Der", "V\u00f6\u00b7gel", "an\u00b7ge\u00b7neh\u00b7mes", "Gur\u00b7geln", ",", "der", "Och\u00b7sen", "und", "der", "Scha\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Blecken,", "tokens": ["Ble\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Das helle Wiehern muntrer Pferde, der fr\u00fchen H\u00e4hne", "tokens": ["Das", "hel\u00b7le", "Wie\u00b7hern", "mun\u00b7trer", "Pfer\u00b7de", ",", "der", "fr\u00fc\u00b7hen", "H\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "scharfes Kr\u00e4hn,", "tokens": ["schar\u00b7fes", "Kr\u00e4hn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Das hohle Girren treuer Tauben, der Enten schnatterndes", "tokens": ["Das", "hoh\u00b7le", "Gir\u00b7ren", "treu\u00b7er", "Tau\u00b7ben", ",", "der", "En\u00b7ten", "schnat\u00b7tern\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,", "ART", "NN", "ADJA"], "meter": "-+-+-+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Get\u00f6n", "tokens": ["Ge\u00b7t\u00f6n"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Formieren eine Harmonie. Sie scheinen gleichsam zu", "tokens": ["For\u00b7mie\u00b7ren", "ei\u00b7ne", "Har\u00b7mo\u00b7nie", ".", "Sie", "schei\u00b7nen", "gleich\u00b7sam", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PPER", "VVFIN", "ADJD", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "entdecken,", "tokens": ["ent\u00b7de\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Da\u00df sie Dem, Der sie schuf, zur Ehr\u2019, mit Freuden leben,", "tokens": ["Da\u00df", "sie", "Dem", ",", "Der", "sie", "schuf", ",", "zur", "Ehr'", ",", "mit", "Freu\u00b7den", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "$,", "ART", "PPER", "VVFIN", "$,", "APPRART", "NN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "f\u00fchlen, schmecken.", "tokens": ["f\u00fch\u00b7len", ",", "schme\u00b7cken", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Will denn der, sich zum Schaden, faul- und tr\u00e4ge Mensch", "tokens": ["Will", "denn", "der", ",", "sich", "zum", "Scha\u00b7den", ",", "faul", "und", "tr\u00e4\u00b7ge", "Mensch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "$,", "PRF", "APPRART", "NN", "$,", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "das Bett nicht lassen,", "tokens": ["das", "Bett", "nicht", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Des k\u00fchlen Morgens zu geniessen, den lieblichen Geruch zu", "tokens": ["Des", "k\u00fch\u00b7len", "Mor\u00b7gens", "zu", "ge\u00b7nies\u00b7sen", ",", "den", "lieb\u00b7li\u00b7chen", "Ge\u00b7ruch", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "PTKZU", "VVINF", "$,", "ART", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "fassen,", "tokens": ["fas\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Der aus bethauten Bluhmen quillt? Will er der stillen", "tokens": ["Der", "aus", "be\u00b7thau\u00b7ten", "Bluh\u00b7men", "quillt", "?", "Will", "er", "der", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$.", "VMFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Stunden sich", "tokens": ["Stun\u00b7den", "sich"], "token_info": ["word", "word"], "pos": ["NN", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Denn nicht erfreuen, die zum Denken und frohen Danken", "tokens": ["Denn", "nicht", "er\u00b7freu\u00b7en", ",", "die", "zum", "Den\u00b7ken", "und", "fro\u00b7hen", "Dan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVINF", "$,", "PRELS", "APPRART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "eigentlich", "tokens": ["ei\u00b7gent\u00b7lich"], "token_info": ["word"], "pos": ["ADV"], "meter": "+--", "measure": "dactylic.init"}, "line.23": {"text": "Bestimmt und ihm geschenket scheinen? Will man, da so viel", "tokens": ["Be\u00b7stimmt", "und", "ihm", "ge\u00b7schen\u00b7ket", "schei\u00b7nen", "?", "Will", "man", ",", "da", "so", "viel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "KON", "PPER", "VVPP", "VVFIN", "$.", "VMFIN", "PIS", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "Guts zu sp\u00fchren,", "tokens": ["Guts", "zu", "sp\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Denn, in der That sich selbst bestehlend, so viele Lust mit Flei\u00df", "tokens": ["Denn", ",", "in", "der", "That", "sich", "selbst", "be\u00b7steh\u00b7lend", ",", "so", "vie\u00b7le", "Lust", "mit", "Flei\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "wo