{"dta.poem.10523": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxXX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.42", "fi:0.42", "sv:0.14"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Auf den W\u00e4llen Salamankas", "tokens": ["Auf", "den", "W\u00e4l\u00b7len", "Sa\u00b7la\u00b7man\u00b7kas"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sind die L\u00fcfte lind und labend;", "tokens": ["Sind", "die", "L\u00fcf\u00b7te", "lind", "und", "la\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort, mit meiner holden Donna,", "tokens": ["Dort", ",", "mit", "mei\u00b7ner", "hol\u00b7den", "Don\u00b7na", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandle ich am Sommerabend.", "tokens": ["Wand\u00b7le", "ich", "am", "Som\u00b7mer\u00b7a\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Um den schlanken Leib der Sch\u00f6nen", "tokens": ["Um", "den", "schlan\u00b7ken", "Leib", "der", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich meinen Arm gebogen,", "tokens": ["Hab'", "ich", "mei\u00b7nen", "Arm", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit sel'gem Finger f\u00fchl' ich", "tokens": ["Und", "mit", "sel'\u00b7gem", "Fin\u00b7ger", "f\u00fchl'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihres Busens stolzes Wogen.", "tokens": ["Ih\u00b7res", "Bu\u00b7sens", "stol\u00b7zes", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch ein \u00e4ngstliches Gefl\u00fcster", "tokens": ["Doch", "ein", "\u00e4ngst\u00b7li\u00b7ches", "Ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht sich durch die Lindenb\u00e4ume,", "tokens": ["Zieht", "sich", "durch", "die", "Lin\u00b7den\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der dunkle M\u00fchlbach unten", "tokens": ["Und", "der", "dunk\u00b7le", "M\u00fchl\u00b7bach", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Murmelt b\u00f6se, bange Tr\u00e4ume.", "tokens": ["Mur\u00b7melt", "b\u00f6\u00b7se", ",", "ban\u00b7ge", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201each, Sennora, Ahnung sagt mir:", "tokens": ["\u201e", "ach", ",", "Sen\u00b7no\u00b7ra", ",", "Ah\u00b7nung", "sagt", "mir", ":"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NE", "$,", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst wird man mich relegiren,", "tokens": ["Einst", "wird", "man", "mich", "re\u00b7le\u00b7gi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und auf Salamankas W\u00e4llen", "tokens": ["Und", "auf", "Sa\u00b7la\u00b7man\u00b7kas", "W\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Geh'n wir nimmermehr spazieren.\u201c", "tokens": ["Geh'n", "wir", "nim\u00b7mer\u00b7mehr", "spa\u00b7zie\u00b7ren", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}