{"dta.poem.11768": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das gr\u00f6ste Hinderni\u00df ist oft dem Muthe keines,", "tokens": ["Das", "gr\u00f6s\u00b7te", "Hin\u00b7der\u00b7ni\u00df", "ist", "oft", "dem", "Mu\u00b7the", "kei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den doch erliegen macht zuletzt ein winzig kleines.", "tokens": ["Den", "doch", "er\u00b7lie\u00b7gen", "macht", "zu\u00b7letzt", "ein", "win\u00b7zig", "klei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VVFIN", "ADV", "ART", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Felsenberge h\u00e4tt' ein Wandrer \u00fcberstiegen,", "tokens": ["Die", "Fel\u00b7sen\u00b7ber\u00b7ge", "h\u00e4tt'", "ein", "Wand\u00b7rer", "\u00fc\u00b7bers\u00b7tie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4tt' er ein Steinchen nicht in seinem Schuhe liegen.", "tokens": ["H\u00e4tt'", "er", "ein", "Stein\u00b7chen", "nicht", "in", "sei\u00b7nem", "Schu\u00b7he", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer wandern will mit Gl\u00fcck durchs Leben, sehe zu,", "tokens": ["Wer", "wan\u00b7dern", "will", "mit", "Gl\u00fcck", "durchs", "Le\u00b7ben", ",", "se\u00b7he", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VMFIN", "APPR", "NN", "APPRART", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df innen ihn nicht dr\u00fcck' ein Steinchen in dem Schuh.", "tokens": ["Da\u00df", "in\u00b7nen", "ihn", "nicht", "dr\u00fcck", "ein", "Stein\u00b7chen", "in", "dem", "Schuh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PTKNEG", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}