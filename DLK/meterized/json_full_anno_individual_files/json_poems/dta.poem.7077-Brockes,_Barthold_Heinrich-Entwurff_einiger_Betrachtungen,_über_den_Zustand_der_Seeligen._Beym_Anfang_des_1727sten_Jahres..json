{"dta.poem.7077": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am meisten schien bey uns der Seelen vorgeschrieben,", "tokens": ["Am", "meis\u00b7ten", "schien", "bey", "uns", "der", "See\u00b7len", "vor\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VVFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jm Tugend-Pfad einher zu gehn,", "tokens": ["Jm", "Tu\u00b7gen\u00b7dPfad", "ein\u00b7her", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich in unstr\u00e4fflichen Verrichtungen zu \u00fcben,", "tokens": ["Sich", "in", "uns\u00b7tr\u00e4ff\u00b7li\u00b7chen", "Ver\u00b7rich\u00b7tun\u00b7gen", "zu", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nicht so sehr auf Wissenschafft zu sehn:", "tokens": ["Und", "nicht", "so", "sehr", "auf", "Wis\u00b7sen\u00b7schafft", "zu", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dort aber, in der Seeligkeit,", "tokens": ["Dort", "a\u00b7ber", ",", "in", "der", "See\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird sie sich immer h\u00f6her schwingen,", "tokens": ["Wird", "sie", "sich", "im\u00b7mer", "h\u00f6\u00b7her", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und, sonder Unvollkommenheit,", "tokens": ["Und", ",", "son\u00b7der", "Un\u00b7voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die allertieffste Wissenschafft,", "tokens": ["Die", "al\u00b7ler\u00b7tieffs\u00b7te", "Wis\u00b7sen\u00b7schafft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die allerdunckelsten Geheimnisse durchdringen.", "tokens": ["Die", "al\u00b7ler\u00b7dun\u00b7ckels\u00b7ten", "Ge\u00b7heim\u00b7nis\u00b7se", "durch\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "Welch Anmuth! welche Lust! was f\u00fcr Verwunderung!", "tokens": ["Welch", "An\u00b7muth", "!", "wel\u00b7che", "Lust", "!", "was", "f\u00fcr", "Ver\u00b7wun\u00b7de\u00b7rung", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PWAT", "NN", "$.", "PWS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Welch unaussprechliche Belustigung!", "tokens": ["Welch", "un\u00b7aus\u00b7sprech\u00b7li\u00b7che", "Be\u00b7lus\u00b7ti\u00b7gung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wird solches nicht in ihr erwecken,", "tokens": ["Wird", "sol\u00b7ches", "nicht", "in", "ihr", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wenn sie durch richtige best\u00e4ndige Jdeen,", "tokens": ["Wenn", "sie", "durch", "rich\u00b7ti\u00b7ge", "be\u00b7st\u00e4n\u00b7di\u00b7ge", "Jdeen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Die ihr Gem\u00fcth erleuchten, alles sehen,", "tokens": ["Die", "ihr", "Ge\u00b7m\u00fcth", "er\u00b7leuch\u00b7ten", ",", "al\u00b7les", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und den geheimen Grund von allen kan entdecken!", "tokens": ["Und", "den", "ge\u00b7hei\u00b7men", "Grund", "von", "al\u00b7len", "kan", "ent\u00b7de\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Denn da\u00df wir uns der Dinge dieser Erden", "tokens": ["Denn", "da\u00df", "wir", "uns", "der", "Din\u00b7ge", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Auch dorten wol erinnern werden,", "tokens": ["Auch", "dor\u00b7ten", "wol", "e\u00b7rin\u00b7nern", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Kommt mir sehr glaublich f\u00fcr:", "tokens": ["Kommt", "mir", "sehr", "glaub\u00b7lich", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Unm\u00f6glich kan uns alles, was wir hier,", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "kan", "uns", "al\u00b7les", ",", "was", "wir", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "$,", "PRELS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Durch GOTTES Gnade, Guts begangen,", "tokens": ["Durch", "GoT\u00b7TES", "Gna\u00b7de", ",", "Guts", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Auch was wir von des Sch\u00f6pffers Huld empfangen,", "tokens": ["Auch", "was", "wir", "von", "des", "Sch\u00f6pf\u00b7fers", "Huld", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "So gar gleichg\u00fcltig seyn.", "tokens": ["So", "gar", "gleich\u00b7g\u00fcl\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Was GOTT in uns zu w\u00fcrcken angefangen,", "tokens": ["Was", "GoTT", "in", "uns", "zu", "w\u00fcr\u00b7cken", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "APPR", "PPER", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Schliesst die Vergessenheit unm\u00f6glich ein.", "tokens": ["Schliesst", "die", "Ver\u00b7ges\u00b7sen\u00b7heit", "un\u00b7m\u00f6g\u00b7lich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.25": {"text": "Ist denn so manches Gut, das GOTT uns hier beschehrt,", "tokens": ["Ist", "denn", "so", "man\u00b7ches", "Gut", ",", "das", "GoTT", "uns", "hier", "be\u00b7schehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "$,", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Nicht eines Angedenckens werth?", "tokens": ["Nicht", "ei\u00b7nes", "An\u00b7ge\u00b7den\u00b7ckens", "werth", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Vermuthlich werden sie zu GOTTES Ehren", "tokens": ["Ver\u00b7muth\u00b7lich", "wer\u00b7den", "sie", "zu", "GoT\u00b7TES", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Den Danck in unsrer Freude mehren.", "tokens": ["Den", "Danck", "in", "uns\u00b7rer", "Freu\u00b7de", "meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}