{"textgrid.poem.47658": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zigeuner sind so schlimm nicht;", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zigeuner sind so schlimm nicht;", "tokens": ["Zi\u00b7geu\u00b7ner", "sind", "so", "schlimm", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "PTKNEG", "$."], "meter": "-+-----", "measure": "dactylic.init"}, "line.2": {"text": "Zigeuner sind gute Leute.", "tokens": ["Zi\u00b7geu\u00b7ner", "sind", "gu\u00b7te", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als unser Herr noch jung war,", "tokens": ["Als", "un\u00b7ser", "Herr", "noch", "jung", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Da hielt er's mit den Zigeunern;", "tokens": ["Da", "hielt", "er's", "mit", "den", "Zi\u00b7geu\u00b7nern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nichts thaten ihm die zu Leide,", "tokens": ["Nichts", "tha\u00b7ten", "ihm", "die", "zu", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sie waren ihm hold und freundlich;", "tokens": ["Sie", "wa\u00b7ren", "ihm", "hold", "und", "freund\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dann ging er unter die Fremden,", "tokens": ["Dann", "ging", "er", "un\u00b7ter", "die", "Frem\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die haben ihn gekreuzigt.", "tokens": ["Die", "ha\u00b7ben", "ihn", "ge\u00b7kreu\u00b7zigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zigeuner sind so schlimm nicht;", "tokens": ["Zi\u00b7geu\u00b7ner", "sind", "so", "schlimm", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "PTKNEG", "$."], "meter": "-+-----", "measure": "dactylic.init"}, "line.2": {"text": "Zigeuner sind gute Leute.", "tokens": ["Zi\u00b7geu\u00b7ner", "sind", "gu\u00b7te", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als unser Herr noch jung war,", "tokens": ["Als", "un\u00b7ser", "Herr", "noch", "jung", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Da hielt er's mit den Zigeunern;", "tokens": ["Da", "hielt", "er's", "mit", "den", "Zi\u00b7geu\u00b7nern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5"