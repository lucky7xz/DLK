{"textgrid.poem.67208": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn bleicher Schnee versch\u00f6nert die Gefilde,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn bleicher Schnee versch\u00f6nert die Gefilde,", "tokens": ["Wenn", "blei\u00b7cher", "Schnee", "ver\u00b7sch\u00f6\u00b7nert", "die", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hoher Glanz auf weiter Ebne blinkt,", "tokens": ["Und", "ho\u00b7her", "Glanz", "auf", "wei\u00b7ter", "Eb\u00b7ne", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So reizt der Sommer fern, und milde", "tokens": ["So", "reizt", "der", "Som\u00b7mer", "fern", ",", "und", "mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Naht sich der Fr\u00fchling oft, indes die Stunde sinkt.", "tokens": ["Naht", "sich", "der", "Fr\u00fch\u00b7ling", "oft", ",", "in\u00b7des", "die", "Stun\u00b7de", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "ADV", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die pr\u00e4chtige Erscheinung ist, die Luft ist feiner,", "tokens": ["Die", "pr\u00e4ch\u00b7ti\u00b7ge"