{"textgrid.poem.36269": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Unzufriedene", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es merket kaum die Leiden", "tokens": ["Es", "mer\u00b7ket", "kaum", "die", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das leicht erfreute Auge;", "tokens": ["Das", "leicht", "er\u00b7freu\u00b7te", "Au\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ob auch Fremde horchen,", "tokens": ["Und", "ob", "auch", "Frem\u00b7de", "hor\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00dft du dich stets im Dunkel einsam glauben.", "tokens": ["Mu\u00dft", "du", "dich", "stets", "im", "Dun\u00b7kel", "ein\u00b7sam", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie eilen schnell und weiter", "tokens": ["Sie", "ei\u00b7len", "schnell", "und", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bewu\u00dftlos hin im Raube,", "tokens": ["Be\u00b7wu\u00dft\u00b7los", "hin", "im", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit leichtem Mut sich t\u00e4uschend,", "tokens": ["Mit", "leich\u00b7tem", "Mut", "sich", "t\u00e4u\u00b7schend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Todesschlummer kaum geweckt vom Traume.", "tokens": ["Zum", "To\u00b7des\u00b7schlum\u00b7mer", "kaum", "ge\u00b7weckt", "vom", "Trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und kehr' ich zu den Besten,", "tokens": ["Und", "kehr'", "ich", "zu", "den", "Bes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So mu\u00df ich tiefer trauern,", "tokens": ["So", "mu\u00df", "ich", "tie\u00b7fer", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Edles so verdorben,", "tokens": ["Wenn", "Ed\u00b7les", "so", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als ob der Mensch nur zum Gemeinsten tauge.", "tokens": ["Als", "ob", "der", "Mensch", "nur", "zum", "Ge\u00b7meins\u00b7ten", "tau\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "s