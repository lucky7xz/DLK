{"textgrid.poem.54436": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Autor ad librum. Quem Genere, Ingenio, Eruditione Virtute Excellentiss. Dn. Michaelem Borck", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh weil dich ja so sehr gel\u00fcst/ von mir zu gehen:", "tokens": ["Geh", "weil", "dich", "ja", "so", "sehr", "ge\u00b7l\u00fcst", "/", "von", "mir", "zu", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "ADV", "ADV", "VVPP", "$(", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Preussens Crone gl\u00e4ntzt/ die wundersch\u00f6ne Stadt/", "tokens": ["Wo", "Preus\u00b7sens", "Cro\u00b7ne", "gl\u00e4ntzt", "/", "die", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Stadt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "VVFIN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die aller V\u00f6lcker Zier vnd Gaben an sich hat;", "tokens": ["Die", "al\u00b7ler", "V\u00f6l\u00b7cker", "Zier", "vnd", "Ga\u00b7ben", "an", "sich", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "KON", "NN", "APPR", "PRF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd sprich den Herren an/ durch dessen Trew sie stehen", "tokens": ["Vnd", "sprich", "den", "Her\u00b7ren", "an", "/", "durch", "des\u00b7sen", "Trew", "sie", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "wor