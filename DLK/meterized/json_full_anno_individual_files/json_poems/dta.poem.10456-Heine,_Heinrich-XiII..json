{"dta.poem.10456": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XiII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich an deinem Hause", "tokens": ["Wenn", "ich", "an", "dei\u00b7nem", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Morgens vor\u00fcber geh',", "tokens": ["Des", "Mor\u00b7gens", "vor\u00b7\u00fc\u00b7ber", "geh'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So freut's mich, du liebe Kleine,", "tokens": ["So", "freut's", "mich", ",", "du", "lie\u00b7be", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenn ich dich am Fenster seh'.", "tokens": ["Wenn", "ich", "dich", "am", "Fens\u00b7ter", "seh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit deinen schwarzbraunen Augen", "tokens": ["Mit", "dei\u00b7nen", "schwarz\u00b7brau\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Siehst du mich forschend an:", "tokens": ["Siehst", "du", "mich", "for\u00b7schend", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer bist du, und was fehlt dir,", "tokens": ["Wer", "bist", "du", ",", "und", "was", "fehlt", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KON", "PWS", "VVFIN", "PPER", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Du fremder, kranker Mann?", "tokens": ["Du", "frem\u00b7der", ",", "kran\u00b7ker", "Mann", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201eich bin ein deutscher Dichter,", "tokens": ["\u201e", "ich", "bin", "ein", "deut\u00b7scher", "Dich\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bekannt im deutschen Land;", "tokens": ["Be\u00b7kannt", "im", "deut\u00b7schen", "Land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nennt man die besten Namen,", "tokens": ["Nennt", "man", "die", "bes\u00b7ten", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "So wird auch der meine genannt.", "tokens": ["So", "wird", "auch", "der", "mei\u00b7ne", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "PPOSAT", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Und was mir fehlt, du Kleine,", "tokens": ["Und", "was", "mir", "fehlt", ",", "du", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fehlt Manchem im deutschen Land;", "tokens": ["Fehlt", "Man\u00b7chem", "im", "deut\u00b7schen", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nennt man die schlimmsten Schmerzen,", "tokens": ["Nennt", "man", "die", "schlimms\u00b7ten", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wird auch der meine genannt.\u201c", "tokens": ["So", "wird", "auch", "der", "mei\u00b7ne", "ge\u00b7nannt", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "PPOSAT", "VVPP", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}