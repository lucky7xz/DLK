{"textgrid.poem.61086": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Ermunterung", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blick' um dich her! Es redet dir vom Lieben,", "tokens": ["Blick'", "um", "dich", "her", "!", "Es", "re\u00b7det", "dir", "vom", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was du nur schaust in aller H\u00f6h' und Tiefe;", "tokens": ["Was", "du", "nur", "schaust", "in", "al\u00b7ler", "H\u00f6h'", "und", "Tie\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Rose l\u00e4ge still im Moos und schliefe,", "tokens": ["Die", "Ro\u00b7se", "l\u00e4\u00b7ge", "still", "im", "Moos", "und", "schlie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sie die Liebe nicht ans Licht getrieben.", "tokens": ["Wenn", "sie", "die", "Lie\u00b7be", "nicht", "ans", "Licht", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es w\u00e4re stumm die Nachtigall geblieben,", "tokens": ["Es", 