{"dta.poem.9963": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr monarchen bin ich nur/ nicht f\u00fcr die geringen leute/", "tokens": ["F\u00fcr", "mon\u00b7ar\u00b7chen", "bin", "ich", "nur", "/", "nicht", "f\u00fcr", "die", "ge\u00b7rin\u00b7gen", "leu\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "$(", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Offt mit f\u00fcssen trirt auff mich der/ so mich bekommen heute:", "tokens": ["Offt", "mit", "f\u00fcs\u00b7sen", "trirt", "auff", "mich", "der", "/", "so", "mich", "be\u00b7kom\u00b7men", "heu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVINF", "VVFIN", "APPR", "PPER", "ART", "$(", "ADV", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Zweyen bin ich nie bedienet/ und doch feindet man mich an/", "tokens": ["Zwe\u00b7yen", "bin", "ich", "nie", "be\u00b7die\u00b7net", "/", "und", "doch", "fein\u00b7det", "man", "mich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVFIN", "$(", "KON", "ADV", "VVFIN", "PIS", "PRF", "APPR", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da ich aus den menschen/ g\u00f6tter auf der erden machen kan.", "tokens": ["Da", "ich", "aus", "den", "men\u00b7schen", "/", "g\u00f6t\u00b7ter", "auf", "der", "er\u00b7den", "ma\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$(", "ADJA", "APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}}}}}