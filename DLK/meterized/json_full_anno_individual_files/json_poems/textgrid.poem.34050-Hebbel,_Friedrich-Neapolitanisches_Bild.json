{"textgrid.poem.34050": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Neapolitanisches Bild", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flei\u00dfig h\u00e4mmert der Schmied, mein Nachbar, da naht sich bed\u00e4chtig", "tokens": ["Flei\u00b7\u00dfig", "h\u00e4m\u00b7mert", "der", "Schmied", ",", "mein", "Nach\u00b7bar", ",", "da", "naht", "sich", "be\u00b7d\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PRF", "ADJD"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Ihm der heischende M\u00f6nch, willig auch reicht ihm der Mann,", "tokens": ["Ihm", "der", "hei\u00b7schen\u00b7de", "M\u00f6nch", ",", "wil\u00b7lig", "auch", "reicht", "ihm", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "ADJD", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Den er noch kaum verdient durch fr\u00fche Arbeit, den Groschen,", "tokens": ["Den", "er", "noch", "kaum", "ver\u00b7dient", "durch", "fr\u00fc\u00b7he", "Ar\u00b7beit", ",", "den", "Gro\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "APPR", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und es beut ihm der M\u00f6nch einen gedoppelten Dank,", "tokens": ["Und", "es", "beut", "ihm", "der", "M\u00f6nch", "ei\u00b7nen", "ge\u00b7dop\u00b7pel\u00b7ten", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Erst die Madonna zum Ku\u00df und dann die Dose zum Schnupfen,", "tokens": ["Erst", "die", "Ma\u00b7don\u00b7na", "zum", "Ku\u00df", "und", "dann", "die", "Do\u00b7se", "zum", "Schnup\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "KON", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Jener k\u00fc\u00dft und nimmt ruhig die Prise darauf.", "tokens": ["Je\u00b7ner", "k\u00fc\u00dft", "und", "nimmt", "ru\u00b7hig", "die", "Pri\u00b7se", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "KON", "VVFIN", "ADJD", "ART", "NN", "PAV", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.2": {"line.1": {"text": "Flei\u00dfig h\u00e4mmert der Schmied, mein Nachbar, da naht sich bed\u00e4chtig", "tokens": ["Flei\u00b7\u00dfig", "h\u00e4m\u00b7mert", "der", "Schmied", ",", "mein", "Nach\u00b7bar", ",", "da", "naht", "sich", "be\u00b7d\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PRF", "ADJD"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Ihm der heischende M\u00f6nch, willig auch reicht ihm der Mann,", "tokens": ["Ihm", "der", "hei\u00b7schen\u00b7de", "M\u00f6nch", ",", "wil\u00b7lig", "auch", "reicht", "ihm", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "w