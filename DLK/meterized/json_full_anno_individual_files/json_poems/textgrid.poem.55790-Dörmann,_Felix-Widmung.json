{"textgrid.poem.55790": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Widmung", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin nicht einer, dem die Worte flie\u00dfen,", "tokens": ["Ich", "bin", "nicht", "ei\u00b7ner", ",", "dem", "die", "Wor\u00b7te", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem leicht das rechte von der Lippe springt,", "tokens": ["Dem", "leicht", "das", "rech\u00b7te", "von", "der", "Lip\u00b7pe", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "ADJA", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und meiner Lieder Nachtviolen sprie\u00dfen", "tokens": ["Und", "mei\u00b7ner", "Lie\u00b7der", "Nacht\u00b7vi\u00b7o\u00b7len", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur dann empor, wenn mich die Qual bezwingt.", "tokens": ["Nur", "dann", "em\u00b7por", ",", "wenn", "mich", "die", "Qual", "be\u00b7zwingt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O grausenvolle Marter \u2013 sich enth\u00fcllen,", "tokens": ["O", "grau\u00b7sen\u00b7vol\u00b7le", "Mar\u00b7ter", "\u2013", "sich", "ent\u00b7h\u00fcl\