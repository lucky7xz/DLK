{"dta.poem.12453": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Von Volksliedern .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch ist eine H\u00fctte, wie ihr wohl wi\u00dft,", "tokens": ["Auch", "ist", "ei\u00b7ne", "H\u00fct\u00b7te", ",", "wie", "ihr", "wohl", "wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da l\u00e4st man sich w\u00e4gen, wie schwer als man ist,", "tokens": ["Da", "l\u00e4st", "man", "sich", "w\u00e4\u00b7gen", ",", "wie", "schwer", "als", "man", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "VVINF", "$,", "PWAV", "ADJD", "KOKOM", "PIS", "VAFIN", "$,"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich ging auch einmal hin,", "tokens": ["Ich", "ging", "auch", "ein\u00b7mal", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Z' wissen, wie schwer ich bin?", "tokens": ["Z'", "wis\u00b7sen", ",", "wie", "schwer", "ich", "bin", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Kerl war ein Flegel, er sprach: H\u00f6rts der Herr,", "tokens": ["Der", "Kerl", "war", "ein", "Fle\u00b7gel", ",", "er", "sprach", ":", "H\u00f6rts", "der", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "$.", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sie sind gewi\u00df ein Schneider und sind gar nicht schwer.", "tokens": ["Sie", "sind", "ge\u00b7wi\u00df", "ein", "Schnei\u00b7der", "und", "sind", "gar", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NE", "KON", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}