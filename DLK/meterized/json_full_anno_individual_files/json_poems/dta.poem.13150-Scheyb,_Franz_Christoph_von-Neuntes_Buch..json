{"dta.poem.13150": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe sie verzagt von meiner Seite gehn,", "tokens": ["Ich", "sa\u00b7he", "sie", "ver\u00b7zagt", "von", "mei\u00b7ner", "Sei\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem fast jeder Blick derselben nachgesehn.", "tokens": ["In\u00b7dem", "fast", "je\u00b7der", "Blick", "der\u00b7sel\u00b7ben", "nach\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "PDAT", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir schlug der Puls da sie gemach das Aug erh\u00f6hte,", "tokens": ["Mir", "schlug", "der", "Puls", "da", "sie", "ge\u00b7mach", "das", "Aug", "er\u00b7h\u00f6h\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie wies das Angesicht voll Unruh, Furcht und R\u00f6the;", "tokens": ["Sie", "wies", "das", "An\u00b7ge\u00b7sicht", "voll", "Un\u00b7ruh", ",", "Furcht", "und", "R\u00f6\u00b7the", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "245Doch stellte sie mit Herz sich in dem Kreise dar,", "tokens": ["stell\u00b7te", "sie", "mit", "Herz", "sich", "in", "dem", "Krei\u00b7se", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Sprach zwar gemein, doch frey; nicht k\u00fcnstlich, dannoch klar.", "tokens": ["Sprach", "zwar", "ge\u00b7mein", ",", "doch", "frey", ";", "nicht", "k\u00fcnst\u00b7lich", ",", "dan\u00b7noch", "klar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$.", "PTKNEG", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}