{"textgrid.poem.67471": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der schlafende Christus", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schl\u00e4fst Du, liebliches Kind? Erwach und h\u00f6re der Engel", "tokens": ["Schl\u00e4fst", "Du", ",", "lieb\u00b7li\u00b7ches", "Kind", "?", "Er\u00b7wach", "und", "h\u00f6\u00b7re", "der", "En\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Hohes, himmlisches Lied, das Dich auf Erden empf\u00e4ngt,", "tokens": ["Ho\u00b7hes", ",", "himm\u00b7li\u00b7sches", "Lied", ",", "das", "Dich", "auf", "Er\u00b7den", "emp\u00b7f\u00e4ngt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Gl\u00fcck weissagend! es wird Dich \u00fcber Mangel und Krippe", "tokens": ["Gl\u00fcck", "weis\u00b7sa\u00b7gend", "!", "es", "wird", "Dich", "\u00fc\u00b7ber", "Man\u00b7gel", "und", "Krip\u00b7pe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$.", "PPER", "VAFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Tr\u00f6sten; es singet Dir Freude der Himmel ins Herz.", "tokens": ["Tr\u00f6s\u00b7ten", ";", "es", "sin\u00b7get", "Dir", "Freu\u00b7de", "der", "Him\u00b7mel", "ins", "Herz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word",