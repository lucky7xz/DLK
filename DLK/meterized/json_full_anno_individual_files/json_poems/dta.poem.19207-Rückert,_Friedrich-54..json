{"dta.poem.19207": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist dir es nicht verliehn, lebendig anzuschaun", "tokens": ["Ist", "dir", "es", "nicht", "ver\u00b7liehn", ",", "le\u00b7ben\u00b7dig", "an\u00b7zu\u00b7schaun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,", "ADJD", "VVIZU"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt, als einen Leib mit Geist sie aufzubaun,", "tokens": ["Die", "Welt", ",", "als", "ei\u00b7nen", "Leib", "mit", "Geist", "sie", "auf\u00b7zu\u00b7baun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "APPR", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So zimmre lieber sie aus sto\u00dfenden Atomen", "tokens": ["So", "zimm\u00b7re", "lie\u00b7ber", "sie", "aus", "sto\u00b7\u00dfen\u00b7den", "A\u00b7to\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tr\u00e4gen Kr\u00e4ften, als aus dunstigen Fantomen.", "tokens": ["Und", "tr\u00e4\u00b7gen", "Kr\u00e4f\u00b7ten", ",", "als", "aus", "duns\u00b7ti\u00b7gen", "Fan\u00b7to\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "KOUS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was f\u00fcr ein Hirngespinst du auch in ihr erkennst,", "tokens": ["Was", "f\u00fcr", "ein", "Hirn\u00b7ge\u00b7spinst", "du", "auch", "in", "ihr", "er\u00b7kennst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du in ihr nur siehst kein grinsendes Gespenst,", "tokens": ["Wenn", "du", "in", "ihr", "nur", "siehst", "kein", "grin\u00b7sen\u00b7des", "Ge\u00b7spenst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie solch ein kranker Geist, der seine Todeswunden", "tokens": ["Wie", "solch", "ein", "kran\u00b7ker", "Geist", ",", "der", "sei\u00b7ne", "To\u00b7des\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gern f\u00fchlet \u00fcberall, macht aus der kerngesunden.", "tokens": ["Gern", "f\u00fch\u00b7let", "\u00fc\u00b7be\u00b7rall", ",", "macht", "aus", "der", "kern\u00b7ge\u00b7sun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}