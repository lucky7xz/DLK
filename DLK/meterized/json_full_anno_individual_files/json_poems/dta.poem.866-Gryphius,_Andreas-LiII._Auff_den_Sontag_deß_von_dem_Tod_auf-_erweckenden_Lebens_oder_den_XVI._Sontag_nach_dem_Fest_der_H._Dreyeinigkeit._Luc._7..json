{"dta.poem.866": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "LiII.  Auff den Sontag de\u00df von dem Tod auf-  \n erweckenden Lebens/ oder den  XVI.  Sontag  \n nach dem Fest der H. Dreyeinigkeit.  \n Luc. 7.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Schaw mich hat lebend schon die letzte Noth verschlung&#7869;! ", "tokens": ["Schaw", "mich", "hat", "le\u00b7bend", "schon", "die", "letz\u00b7te", "Noth", "ver\u00b7schlung", "&#7869;", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "punct"], "pos": ["NN", "PPER", "VAFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "NN", "$(", "$."], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "O grosser Lebens F\u00fcrst! Mein Hertz ist \u00f6d vnd kalt!", "tokens": ["O", "gros\u00b7ser", "Le\u00b7bens", "F\u00fcrst", "!", "Mein", "Hertz", "ist", "\u00f6d", "vnd", "kalt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Erden stinckt mich an! mein Fleisch wird vnge- ", "tokens": ["Die", "Er\u00b7den", "stinckt", "mich", "an", "!", "mein", "Fleisch", "wird", "vn\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "stalt ", "tokens": ["stalt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "In stettem weh\u2019 vnd ach! ich habe l\u00e4ngst gerungen", "tokens": ["In", "stet\u00b7tem", "weh'", "vnd", "ach", "!", "ich", "ha\u00b7be", "l\u00e4ngst", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "XY", "$.", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit grimmer Todes angst! Wie stammelt meine Zungen.", "tokens": ["Mit", "grim\u00b7mer", "To\u00b7des", "angst", "!", "Wie", "stam\u00b7melt", "mei\u00b7ne", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ich dich preisen wil! ob zwar mein Blut noch ", "tokens": ["Wenn", "ich", "dich", "prei\u00b7sen", "wil", "!", "ob", "zwar", "mein", "Blut", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$.", "KOUS", "ADV", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Start doch der schwache Leib/ ob in dem Ohr erschallt", "tokens": ["Start", "doch", "der", "schwa\u00b7che", "Leib", "/", "ob", "in", "dem", "Ohr", "er\u00b7schallt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$(", "KOUS", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn du dich h\u00f6ren l\u00e4\u00df\u2019t doch bin ich gantz durch drungen", "tokens": ["Wenn", "du", "dich", "h\u00f6\u00b7ren", "l\u00e4\u00df't", "doch", "bin", "ich", "gantz", "durch", "drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "ADV", "VAFIN", "PPER", "ADV", "APPR", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Von dem was sterben heist. Selbst bin ich meine bahr/", "tokens": ["Von", "dem", "was", "ster\u00b7ben", "heist", ".", "Selbst", "bin", "ich", "mei\u00b7ne", "bahr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "VVINF", "VAFIN", "$.", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch selbst mein eigen Grab/ die matte Sinnen Schaar", "tokens": ["Auch", "selbst", "mein", "ei\u00b7gen", "Grab", "/", "die", "mat\u00b7te", "Sin\u00b7nen", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Laufft trawrig vmb mich her! Wilst du mich nicht erl\u00f6sen?", "tokens": ["Laufft", "traw\u00b7rig", "vmb", "mich", "her", "!", "Wilst", "du", "mich", "nicht", "er\u00b7l\u00f6\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "PTKVZ", "$.", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Vnd in die Statt der Lust von dieser grufft weg-gehn.", "tokens": ["Vnd", "in", "die", "Statt", "der", "Lust", "von", "die\u00b7ser", "grufft", "weg\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "APPR", "PDAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Stets leben werd ich dir. Absterben stets dem b\u00f6sen. ", "tokens": ["Stets", "le\u00b7ben", "werd", "ich", "dir", ".", "A\u00b7bster\u00b7ben", "stets", "dem", "b\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "PPER", "PPER", "$.", "NN", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}