{"textgrid.poem.58542": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "580. Noch zum Geburtstage", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ich war", "tokens": ["Wie", "ich", "war"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Sechzig Jahr", "tokens": ["Sech\u00b7zig", "Jahr"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und im Blick", "tokens": ["Und", "im", "Blick"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.4": {"text": "Sah zur\u00fcck", "tokens": ["Sah", "zu\u00b7r\u00fcck"], "token_info": ["word", "word"], "pos": ["VVFIN", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Aufs Versehen,", "tokens": ["Aufs", "Ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Aufs Vers\u00e4umen,", "tokens": ["Aufs", "Ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Dacht' ich: Pfui, wir Menschen tr\u00e4umen!", "tokens": ["Dacht'", "ich", ":", "Pfui", ",", "wir", "Men\u00b7schen", "tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "NN", "$,", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hab' ich in so vielen Jahren", "tokens": ["Hab'", "ich", "in", "so", "vie\u00b7len", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"