{"dta.poem.20112": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Die Braut ist allsobald verschwunden,", "tokens": ["Die", "Braut", "ist", "all\u00b7so\u00b7bald", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umsonst durchsp\u00e4ht man Flur und Wald,", "tokens": ["Um\u00b7sonst", "durch\u00b7sp\u00e4ht", "man", "Flur", "und", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hat die Grenze schon gefunden,", "tokens": ["Sie", "hat", "die", "Gren\u00b7ze", "schon", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihrer Br\u00fcder Aufenthalt.", "tokens": ["Und", "ih\u00b7rer", "Br\u00fc\u00b7der", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon in des Cheviot wilden Kesseln", "tokens": ["Schon", "in", "des", "Che\u00b7vi\u00b7ot", "wil\u00b7den", "Kes\u00b7seln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hat sie ihr Brautgewand zerfetzt,", "tokens": ["Hat", "sie", "ihr", "Braut\u00b7ge\u00b7wand", "zer\u00b7fetzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und l\u00f6st die langgetragnen Fesseln,", "tokens": ["Und", "l\u00f6st", "die", "lang\u00b7ge\u00b7trag\u00b7nen", "Fes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie ihre schwarzen Flechten jetzt.", "tokens": ["Wie", "ih\u00b7re", "schwar\u00b7zen", "Flech\u00b7ten", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Schon lagert Alt und Jung im Kreise", "tokens": ["Schon", "la\u00b7gert", "Alt", "und", "Jung", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Um eines Feuers Flackerbrand,", "tokens": ["Um", "ei\u00b7nes", "Feu\u00b7ers", "Fla\u00b7cker\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und ihres Liedes wilde Weise", "tokens": ["Und", "ih\u00b7res", "Lie\u00b7des", "wil\u00b7de", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Hallt fort von Fels zu Felsenwand:", "tokens": ["Hallt", "fort", "von", "Fels", "zu", "Fel\u00b7sen\u00b7wand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u201ezur W\u00fcste wieder will der L\u00f6we,", "tokens": ["\u201e", "zur", "W\u00fcs\u00b7te", "wie\u00b7der", "will", "der", "L\u00f6\u00b7we", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "ADV", "VMFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Aar zur\u00fcck in seinen Horst,", "tokens": ["Der", "Aar", "zu\u00b7r\u00fcck", "in", "sei\u00b7nen", "Horst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nur auf dem Meere jauchzt die M\u00f6ve, \u2014", "tokens": ["Nur", "auf", "dem", "Mee\u00b7re", "jauchzt", "die", "M\u00f6\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und wir allein in Schlucht und Forst.", "tokens": ["Und", "wir", "al\u00b7lein", "in", "Schlucht", "und", "Forst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ihr k\u00f6nnt den Sturzbach nimmer z\u00e4hmen,", "tokens": ["Ihr", "k\u00f6nnt", "den", "Sturz\u00b7bach", "nim\u00b7mer", "z\u00e4h\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Die Wildheit ist sein Wesen nur; \u2014", "tokens": ["Die", "Wild\u00b7heit", "ist", "sein", "We\u00b7sen", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Es hei\u00dft uns Luft und Leben nehmen,", "tokens": ["Es", "hei\u00dft", "uns", "Luft", "und", "Le\u00b7ben", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Nimmt man uns Freiheit und Natur.\u201c", "tokens": ["Nimmt", "man", "uns", "Frei\u00b7heit", "und", "Na\u00b7tur", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "PPER", "NN", "KON", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}