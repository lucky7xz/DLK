{"textgrid.poem.45826": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[um die Harfe sind Kr\u00e4nze geschlungen]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um die Harfe sind Kr\u00e4nze geschlungen,", "tokens": ["Um", "die", "Har\u00b7fe", "sind", "Kr\u00e4n\u00b7ze", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Schwebte Lieb' in der Saiten Klang:", "tokens": ["Schweb\u00b7te", "Lieb'", "in", "der", "Sai\u00b7ten", "Klang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Oft wohl hab' ich mir einsam gesungen,", "tokens": ["Oft", "wohl", "hab'", "ich", "mir", "ein\u00b7sam", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wenn einsam und still ich sang,", "tokens": ["Und", "wenn", "ein\u00b7sam", "und", "still", "ich", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "KON", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Rauschten die Saiten im t\u00f6nenden Spiel,", "tokens": ["Rauschten", "die", "Sai\u00b7ten", "im", "t\u00f6\u00b7nen\u00b7den", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Bis aus dem Kranze, vom Klange durchsch\u00fcttert,", "tokens": ["Bis", "aus", "dem", "Kran\u00b7ze", ",", "vom", "Klan\u00b7ge", "durch\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Und von der Klage der Liebe durchzittert,", "tokens": ["Und", "von", "der", "Kla\u00b7ge", "der", "Lie\u00b7be", "durch\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sinkend die Blume herniederfiel.", "tokens": ["Sin\u00b7kend", "die", "Blu\u00b7me", "her\u00b7nie\u00b7der\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Weinend sah ich zur Erde dann nieder,", "tokens": ["Wei\u00b7nend", "sah", "ich", "zur", "Er\u00b7de", "dann", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Liegt die Bl\u00fcte so still und tot;", "tokens": ["Liegt", "die", "Bl\u00fc\u00b7te", "so", "still", "und", "tot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Seh' die Kr\u00e4nz' an der Harfe nun wieder, \u2013", "tokens": ["Seh'", "die", "Kr\u00e4nz'", "an", "der", "Har\u00b7fe", "nun", "wie\u00b7der", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADV", "ADV", "$,", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Auch verschwunden des Lebens Rot,", "tokens": ["Auch", "ver\u00b7schwun\u00b7den", "des", "Le\u00b7bens", "Rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Winken mir traurig wie schattiges Grab,", "tokens": ["Win\u00b7ken", "mir", "trau\u00b7rig", "wie", "schat\u00b7ti\u00b7ges", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Wehen so kalt in den t\u00f6nenden Saiten,", "tokens": ["We\u00b7hen", "so", "kalt", "in", "den", "t\u00f6\u00b7nen\u00b7den", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Wehen so bang und so traurig: Es gleiten", "tokens": ["We\u00b7hen", "so", "bang", "und", "so", "trau\u00b7rig", ":", "Es", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$.", "PPER", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Brennende Tr\u00e4nen die Wang' herab.", "tokens": ["Bren\u00b7nen\u00b7de", "Tr\u00e4\u00b7nen", "die", "Wang'", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Nie ert\u00f6nt meine Stimme nun wieder,", "tokens": ["Nie", "er\u00b7t\u00f6nt", "mei\u00b7ne", "Stim\u00b7me", "nun", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wenn nicht freundlich die Bl\u00fcte winkt;", "tokens": ["Wenn", "nicht", "freund\u00b7lich", "die", "Bl\u00fc\u00b7te", "winkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ewig sterben und schweigen die Lieder,", "tokens": ["E\u00b7wig", "ster\u00b7ben", "und", "schwei\u00b7gen", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wenn die Blume mir nicht mehr sinkt.", "tokens": ["Wenn", "die", "Blu\u00b7me", "mir", "nicht", "mehr", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Schon sind die meisten der holden entflohn;", "tokens": ["Schon", "sind", "die", "meis\u00b7ten", "der", "hol\u00b7den", "ent\u00b7flohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Ach! wenn die Kr\u00e4nze die Harfe verlassen,", "tokens": ["Ach", "!", "wenn", "die", "Kr\u00e4n\u00b7ze", "die", "Har\u00b7fe", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Dann will ich sterben; die Wangen erblassen,", "tokens": ["Dann", "will", "ich", "ster\u00b7ben", ";", "die", "Wan\u00b7gen", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$.", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Stumm ist die Lippe, verhallt der Ton.", "tokens": ["Stumm", "ist", "die", "Lip\u00b7pe", ",", "ver\u00b7hallt", "der", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Aber Wonn', es entsprosset zum Leben", "tokens": ["A\u00b7ber", "Wonn'", ",", "es", "ent\u00b7spros\u00b7set", "zum", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "--+--++-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Meiner Asche, so hell und sch\u00f6n,", "tokens": ["Mei\u00b7ner", "A\u00b7sche", ",", "so", "hell", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Eine Blume. \u2013 Mit freudigem Beben", "tokens": ["Ei\u00b7ne", "Blu\u00b7me", ".", "\u2013", "Mit", "freu\u00b7di\u00b7gem", "Be\u00b7ben"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "APPR", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.