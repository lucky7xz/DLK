{"textgrid.poem.64932": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gef\u00e4hrte mir zu sein auf sel'gem Liebeszug,", "tokens": ["Ge\u00b7f\u00e4hr\u00b7te", "mir", "zu", "sein", "auf", "sel'\u00b7gem", "Lie\u00b7bes\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VAINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War dein Gem\u00fcth nicht schwach und war nicht stark genug.", "tokens": ["War", "dein", "Ge\u00b7m\u00fcth", "nicht", "schwach", "und", "war", "nicht", "stark", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "KON", "VAFIN", "PTKNEG", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht schwach, um willenlos sich mir zu \u00fcberlassen;", "tokens": ["Nicht", "schwach", ",", "um", "wil\u00b7len\u00b7los", "sich", "mir", "zu", "\u00fc\u00b7ber\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "KOUI", "ADJD", "PRF", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht stark genug, mein Selbst gebieterisch zu fassen.", "tokens": ["Nicht", "stark", "ge\u00b7nug", ",", "mein", "Selbst", "ge\u00b7bie\u00b7te\u00b7risch", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "$,", "PPOSAT", "NN", "ADJD", "PTKZU", 