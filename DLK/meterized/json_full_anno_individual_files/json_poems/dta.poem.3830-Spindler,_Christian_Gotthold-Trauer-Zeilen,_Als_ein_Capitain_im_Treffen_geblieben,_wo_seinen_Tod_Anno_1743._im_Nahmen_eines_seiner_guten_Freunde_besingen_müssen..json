{"dta.poem.3830": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Zeilen,  \n Als ein  Capitain  im Treffen geblieben, wo seinen Tod  Anno  \n 1743. im Nahmen eines seiner guten Freunde besingen  \n m\u00fcssen.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Falsches Bley, verha\u00dfter Schu\u00df!", "tokens": ["Fal\u00b7sches", "Bley", ",", "ver\u00b7ha\u00df\u00b7ter", "Schu\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der empfindlich treffen mu\u00df.", "tokens": ["Der", "emp\u00b7find\u00b7lich", "tref\u00b7fen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So gehts im Soldaten-Stande,", "tokens": ["So", "gehts", "im", "Sol\u00b7da\u00b7ten\u00b7Stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenn man seinem Vaterlande,", "tokens": ["Wenn", "man", "sei\u00b7nem", "Va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus besondrer Redligkeit,", "tokens": ["Aus", "be\u00b7sond\u00b7rer", "Red\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leben, Muth und Blut geweyht.", "tokens": ["Le\u00b7ben", ",", "Muth", "und", "Blut", "ge\u00b7weyht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn so Bomb als M\u00f6rser knallen,", "tokens": ["Wenn", "so", "Bomb", "als", "M\u00f6r\u00b7ser", "knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn geh\u00e4uffte Feuer-Ballen", "tokens": ["Wenn", "ge\u00b7h\u00e4uff\u00b7te", "Feu\u00b7er\u00b7Bal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In erth\u00f6nten L\u00fcfften gl\u00fchn,", "tokens": ["In", "er\u00b7th\u00f6n\u00b7ten", "L\u00fcff\u00b7ten", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Steigen, rauchen, blitzen, spr\u00fchn.", "tokens": ["Stei\u00b7gen", ",", "rau\u00b7chen", ",", "blit\u00b7zen", ",", "spr\u00fchn", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wenn das widrige Geschicke,", "tokens": ["Wenn", "das", "wid\u00b7ri\u00b7ge", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wenn die offt geladnen St\u00fccke", "tokens": ["Wenn", "die", "offt", "ge\u00b7lad\u00b7nen", "St\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Feuer, Dampff und Hagel speyn,", "tokens": ["Feu\u00b7er", ",", "Dampff", "und", "Ha\u00b7gel", "speyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Und der Weg zur Baare seyn.", "tokens": ["Und", "der", "Weg", "zur", "Baa\u00b7re", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Also k\u00f6nnen auch die Waffen", "tokens": ["Al\u00b7so", "k\u00f6n\u00b7nen", "auch", "die", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Uns ein Bett der Ehren schaffen;", "tokens": ["Uns", "ein", "Bett", "der", "Eh\u00b7ren", "schaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ist die Brust mit Blut benetzt,", "tokens": ["Ist", "die", "Brust", "mit", "Blut", "be\u00b7netzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Hat sie gleich ein Bley verletzt,", "tokens": ["Hat", "sie", "gleich", "ein", "Bley", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "So mu\u00df dieses Beyspiel lehren,", "tokens": ["So", "mu\u00df", "die\u00b7ses", "Bey\u00b7spiel", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Dieses hei\u00dft ein Bett der Ehren.", "tokens": ["Die\u00b7ses", "hei\u00dft", "ein", "Bett", "der", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Dieses hast du nun erlangt,", "tokens": ["Die\u00b7ses", "hast", "du", "nun", "er\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Da dein siegend Hertze prangt", "tokens": ["Da", "dein", "sie\u00b7gend", "Hert\u00b7ze", "prangt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJD", "VVFIN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Vor dem Thron der Cherubinen,", "tokens": ["Vor", "dem", "Thron", "der", "Che\u00b7ru\u00b7bi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Die dem Sieges-F\u00fcrsten dienen.", "tokens": ["Die", "dem", "Sie\u00b7ges\u00b7F\u00fcrs\u00b7ten", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "So erlangst du durch den Krieg,", "tokens": ["So", "er\u00b7langst", "du", "durch", "den", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Freund! den ewig frohen Sieg,", "tokens": ["Freund", "!", "den", "e\u00b7wig", "fro\u00b7hen", "Sieg", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Ja das l\u00e4ngst erw\u00fcnschte Gosen,", "tokens": ["Ja", "das", "l\u00e4ngst", "er\u00b7w\u00fcnschte", "Go\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PDS", "ADV", "VVFIN", "NE", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Lorber, Palm und Sieges-Rosen,", "tokens": ["Lor\u00b7ber", ",", "Palm", "und", "Sie\u00b7ges\u00b7Ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "In der h\u00f6chsterw\u00fcnschten Ruh,", "tokens": ["In", "der", "h\u00f6chs\u00b7ter\u00b7w\u00fcnschten", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.30": {"text": "O du Wohlgestorbner du!", "tokens": ["O", "du", "Wohl\u00b7ge\u00b7storb\u00b7ner", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Bist du vor dem Feind geblieben,", "tokens": ["Bist", "du", "vor", "dem", "Feind", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Wird dir auf den Sarg geschrieben:", "tokens": ["Wird", "dir", "auf", "den", "Sarg", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Unser Freund starb in dem Feld,", "tokens": ["Un\u00b7ser", "Freund", "starb", "in", "dem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Als ein tapffrer Herr und Held.", "tokens": ["Als", "ein", "tapf\u00b7frer", "Herr", "und", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}