{"dta.poem.7891": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Schmied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6r\u2019 meinen Schatz,", "tokens": ["Ich", "h\u00f6r'", "mei\u00b7nen", "Schatz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Den Hammer er schwinget,", "tokens": ["Den", "Ham\u00b7mer", "er", "schwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das rauschet, das klinget,", "tokens": ["Das", "rau\u00b7schet", ",", "das", "klin\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das dringt in die Weite,", "tokens": ["Das", "dringt", "in", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wie Glockengel\u00e4ute,", "tokens": ["Wie", "Glo\u00b7cken\u00b7ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Durch Gassen und Platz.", "tokens": ["Durch", "Gas\u00b7sen", "und", "Platz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Am schwarzen Kamin,", "tokens": ["Am", "schwar\u00b7zen", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Da sitzet mein Lieber,", "tokens": ["Da", "sit\u00b7zet", "mein", "Lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Doch geh\u2019 ich vor\u00fcber,", "tokens": ["Doch", "geh'", "ich", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die B\u00e4lge dann sausen,", "tokens": ["Die", "B\u00e4l\u00b7ge", "dann", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Flammen aufbrausen", "tokens": ["Die", "Flam\u00b7men", "auf\u00b7brau\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und lodern um ihn.", "tokens": ["Und", "lo\u00b7dern", "um", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}