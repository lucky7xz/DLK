{"dta.poem.23570": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vorw\u00e4rts und Haltauf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Vorw\u00e4rts war sein ganzes Streben,", "tokens": ["Ein", "Vor\u00b7w\u00e4rts", "war", "sein", "gan\u00b7zes", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Vorw\u00e4rts f\u00fcr sein Vaterland,", "tokens": ["Ein", "Vor\u00b7w\u00e4rts", "f\u00fcr", "sein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum ward er auch bei seinem Leben", "tokens": ["Drum", "ward", "er", "auch", "bei", "sei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Marschall Vorw\u00e4rts nur genannt.", "tokens": ["Der", "Mar\u00b7schall", "Vor\u00b7w\u00e4rts", "nur", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er konnte nie ein Haltauf leiden", "tokens": ["Er", "konn\u00b7te", "nie", "ein", "Halt\u00b7auf", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und was ihn hemmt' in seinem Lauf;", "tokens": ["Und", "was", "ihn", "hemmt'", "in", "sei\u00b7nem", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Vorw\u00e4rts mu\u00df das Haltauf meiden,", "tokens": ["Ein", "Vor\u00b7w\u00e4rts", "mu\u00df", "das", "Halt\u00b7auf", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst h\u00f6ret es von selber auf.", "tokens": ["Sonst", "h\u00f6\u00b7ret", "es", "von", "sel\u00b7ber", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und so auch dachten seine Erben,", "tokens": ["Und", "so", "auch", "dach\u00b7ten", "sei\u00b7ne", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil jeder gern den Ahnen gleicht;", "tokens": ["Weil", "je\u00b7der", "gern", "den", "Ah\u00b7nen", "gleicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wollten ohne Haltauf sterben,", "tokens": ["Sie", "woll\u00b7ten", "oh\u00b7ne", "Halt\u00b7auf", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und haben auch ihr Ziel erreicht.", "tokens": ["Und", "ha\u00b7ben", "auch", "ihr", "Ziel", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Bl\u00fcchersch Gut, bei Oels gelegen,", "tokens": ["Ein", "Bl\u00fc\u00b7cher\u00b7sch", "Gut", ",", "bei", "O\u00b7els", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Haltauf genannt, ist sequestriert,", "tokens": ["Halt\u00b7auf", "ge\u00b7nannt", ",", "ist", "se\u00b7quest\u00b7riert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wird nun eben dieserwegen", "tokens": ["Und", "wird", "nun", "e\u00b7ben", "die\u00b7ser\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im n\u00e4chsten Juni ", "tokens": ["Im", "n\u00e4chs\u00b7ten", "Ju\u00b7ni"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}}}}