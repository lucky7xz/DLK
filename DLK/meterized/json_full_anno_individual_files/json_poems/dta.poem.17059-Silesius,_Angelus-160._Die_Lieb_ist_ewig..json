{"dta.poem.17059": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "160.  Die Lieb ist ewig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Hoffnung h\u00f6ret auf: der Glaube kombt zum schauen/", "tokens": ["Die", "Hoff\u00b7nung", "h\u00f6\u00b7ret", "auf", ":", "der", "Glau\u00b7be", "kombt", "zum", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "APPRART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sprachen redt man nicht/ und alles was wir bauen/", "tokens": ["Die", "Spra\u00b7chen", "redt", "man", "nicht", "/", "und", "al\u00b7les", "was", "wir", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKNEG", "$(", "KON", "PIS", "PWS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vergehet mit der Zeit: die Liebe bleibt allein:", "tokens": ["Ver\u00b7ge\u00b7het", "mit", "der", "Zeit", ":", "die", "Lie\u00b7be", "bleibt", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So last un\u00df doch schon jetz auf sie befliessen seyn.", "tokens": ["So", "last", "un\u00df", "doch", "schon", "jetz", "auf", "sie", "be\u00b7flies\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}