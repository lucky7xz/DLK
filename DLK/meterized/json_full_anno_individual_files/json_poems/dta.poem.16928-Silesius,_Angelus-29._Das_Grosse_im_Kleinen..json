{"dta.poem.16928": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "29.  Das Grosse im Kleinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Du sprichst/ das Grosse kan nicht in dem Kleinen seyn/", "tokens": ["Du", "sprichst", "/", "das", "Gros\u00b7se", "kan", "nicht", "in", "dem", "Klei\u00b7nen", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Him\u0304el schleust man nicht ins Erdenst\u00fcpffchen ein.", "tokens": ["Den", "Him\u0304el", "schleust", "man", "nicht", "ins", "Er\u00b7dens\u00b7t\u00fcpf\u00b7fchen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKNEG", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Komb schau der Jungfraun Kind; so sihstu in der", "tokens": ["Komb", "schau", "der", "Jung\u00b7fraun", "Kind", ";", "so", "sih\u00b7stu", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$.", "ADV", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wiegen/", "tokens": ["Wie\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Den Himmel und die Erd\u2019/ und hundert Welte liegen.", "tokens": ["Den", "Him\u00b7mel", "und", "die", "Erd'", "/", "und", "hun\u00b7dert", "Wel\u00b7te", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$(", "KON", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}