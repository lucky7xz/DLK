{"dta.poem.3065": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "111.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht von Unwissenheit gen\u00fcgt es frei zu sein;", "tokens": ["Nicht", "von", "Un\u00b7wis\u00b7sen\u00b7heit", "ge\u00b7n\u00fcgt", "es", "frei", "zu", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "PPER", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer selbst sich hat befreit, will andre auch befrein.", "tokens": ["Wer", "selbst", "sich", "hat", "be\u00b7freit", ",", "will", "and\u00b7re", "auch", "be\u00b7fr\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "VAFIN", "VVPP", "$,", "VMFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Durch Mitbewustsein soll sich dein Bewustsein mehren;", "tokens": ["Durch", "Mit\u00b7be\u00b7wust\u00b7sein", "soll", "sich", "dein", "Be\u00b7wust\u00b7sein", "meh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darum, was du gelernt, willst du alsbald auch lehren.", "tokens": ["Da\u00b7rum", ",", "was", "du", "ge\u00b7lernt", ",", "willst", "du", "als\u00b7bald", "auch", "leh\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWS", "PPER", "VVPP", "$,", "VMFIN", "PP