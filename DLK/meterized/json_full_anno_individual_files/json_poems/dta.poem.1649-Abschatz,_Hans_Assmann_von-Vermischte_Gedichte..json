{"dta.poem.1649": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Thorheit ists/ da\u00df unsre Zeit soll in Furcht und Angst ver-\nfl\u00fcssen/ ", "tokens": ["Thor\u00b7heit", "ists", "/", "da\u00df", "uns\u00b7re", "Zeit", "soll", "in", "Furcht", "und", "Angst", "ver", "fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$(", "KOUS", "PPOSAT", "NN", "VMFIN", "APPR", "NN", "KON", "NN", "TRUNC", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Besser ists in Freud und Lust der bestimmten Frist gen\u00fcssen/", "tokens": ["Bes\u00b7ser", "ists", "in", "Freud", "und", "Lust", "der", "be\u00b7stimm\u00b7ten", "Frist", "ge\u00b7n\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Nimm die Stunden willig an/", "tokens": ["Nimm", "die", "Stun\u00b7den", "wil\u00b7lig", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dir GOtt und Gl\u00fccke schencken/", "tokens": ["Die", "dir", "Gott", "und", "Gl\u00fc\u00b7cke", "schen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Warum soll dich heute kr\u00e4ncken", "tokens": ["Wa\u00b7rum", "soll", "dich", "heu\u00b7te", "kr\u00e4n\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was sich morgen \u00e4ndern kan?", "tokens": ["Was", "sich", "mor\u00b7gen", "\u00e4n\u00b7dern", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df das ernste Sauersehn/ la\u00df die eingezogne Stirne/", "tokens": ["La\u00df", "das", "erns\u00b7te", "Sau\u00b7er\u00b7sehn", "/", "la\u00df", "die", "ein\u00b7ge\u00b7zog\u00b7ne", "Stir\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$(", "VVIMP", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "La\u00df den ausgehangnen Mund/ la\u00df die Grillen im Gehirne/", "tokens": ["La\u00df", "den", "aus\u00b7ge\u00b7hang\u00b7nen", "Mund", "/", "la\u00df", "die", "Gril\u00b7len", "im", "Ge\u00b7hir\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$(", "VVIMP", "ART", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "La\u00df die Mucken um das Haubt", "tokens": ["La\u00df", "die", "Mu\u00b7cken", "um", "das", "Haubt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bi\u00df ins Alter seyn verschoben/", "tokens": ["Bi\u00df", "ins", "Al\u00b7ter", "seyn", "ver\u00b7scho\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "PPOSAT", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bi\u00df sich allgemach von oben", "tokens": ["Bi\u00df", "sich", "all\u00b7ge\u00b7mach", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ADV", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kopff und Hals zum Grabe schraubt.", "tokens": ["Kopff", "und", "Hals", "zum", "Gra\u00b7be", "schraubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df der alten Weisen Schaar frey und ohnbewegt erscheinen/", "tokens": ["La\u00df", "der", "al\u00b7ten", "Wei\u00b7sen", "Schaar", "frey", "und", "ohn\u00b7be\u00b7wegt", "er\u00b7schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "NN", "ADJD", "KON", "ADJD", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "La\u00df den tollen Heraclit gantze Thr\u00e4nen-Str\u00f6me weinen.", "tokens": ["La\u00df", "den", "tol\u00b7len", "Her\u00b7a\u00b7clit", "gant\u00b7ze", "Thr\u00e4\u00b7nen\u00b7Str\u00f6\u00b7me", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Menschen die empfindlich seyn", "tokens": ["Men\u00b7schen", "die", "emp\u00b7find\u00b7lich", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Halten mehr von Schertz und Lachen;", "tokens": ["Hal\u00b7ten", "mehr", "von", "Schertz", "und", "La\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich vergebens traurig machen", "tokens": ["Sich", "ver\u00b7ge\u00b7bens", "trau\u00b7rig", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gehet ja wohl bitter ein.", "tokens": ["Ge\u00b7het", "ja", "wohl", "bit\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir an Beutel/ Weib und Ort/ Zeit und Zustand ungebun-", "tokens": ["Wir", "an", "Beu\u00b7tel", "/", "Weib", "und", "Ort", "/", "Zeit", "und", "Zu\u00b7stand", "un\u00b7ge\u00b7bun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "$(", "NN", "KON", "NN", "$(", "NN", "KON", "NN", "TRUNC"], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Brauchen billig/ weil es geht/ unsrer Jugend freye Stunden/", "tokens": ["Brau\u00b7chen", "bil\u00b7lig", "/", "weil", "es", "geht", "/", "uns\u00b7rer", "Ju\u00b7gend", "frey\u00b7e", "Stun\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$(", "KOUS", "PPER", "VVFIN", "$(", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Was bringts einem Kargen ein/", "tokens": ["Was", "bringts", "ei\u00b7nem", "Kar\u00b7gen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenn er noch so reich an Sch\u00e4tzen:", "tokens": ["Wenn", "er", "noch", "so", "reich", "an", "Sch\u00e4t\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Also/ was kan ohn Erg\u00f6tzen", "tokens": ["Al\u00b7so", "/", "was", "kan", "ohn", "Er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "PWS", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unser bestes Leben seyn?", "tokens": ["Un\u00b7ser", "bes\u00b7tes", "Le\u00b7ben", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Rosen/ die der Sommer giebt/ kan man nicht im Winter pfl\u00fc-", "tokens": ["Ro\u00b7sen", "/", "die", "der", "Som\u00b7mer", "giebt", "/", "kan", "man", "nicht", "im", "Win\u00b7ter", "pfl\u00fc"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "PRELS", "ART", "NN", "VVFIN", "$(", "VMFIN", "PIS", "PTKNEG", "APPRART", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Freude/ so die Jugend hegt/ kan das Alter nicht erquicken/", "tokens": ["Freu\u00b7de", "/", "so", "die", "Ju\u00b7gend", "hegt", "/", "kan", "das", "Al\u00b7ter", "nicht", "er\u00b7qui\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "ART", "NN", "VVFIN", "$(", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Wenn des siechen Leibes Hau\u00df", "tokens": ["Wenn", "des", "sie\u00b7chen", "Lei\u00b7bes", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zum schnellen Falle neiget/", "tokens": ["Sich", "zum", "schnel\u00b7len", "Fal\u00b7le", "nei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn der Lebens-Strom verseiget/", "tokens": ["Wenn", "der", "Le\u00b7bens\u00b7Strom", "ver\u00b7sei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Glutt und Hitze d\u00e4mpffen aus.", "tokens": ["Glutt", "und", "Hit\u00b7ze", "d\u00e4mpf\u00b7fen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum last uns den edlen Schatz der erlaubten Zeit gebrauchen", "tokens": ["Drum", "last", "uns", "den", "ed\u00b7len", "Schatz", "der", "er\u00b7laub\u00b7ten", "Zeit", "ge\u00b7brau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Eh die Kr\u00e4ffte noch in uns/ eh noch Marck und Blutt ver-", "tokens": ["Eh", "die", "Kr\u00e4ff\u00b7te", "noch", "in", "uns", "/", "eh", "noch", "Marck", "und", "Blutt", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPER", "$(", "KOUS", "ADV", "NN", "KON", "ADJD", "TRUNC"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Last uns dieses Tages Schein/", "tokens": ["Last", "uns", "die\u00b7ses", "Ta\u00b7ges", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sich offte noch soll finden/", "tokens": ["Der", "sich", "off\u00b7te", "noch", "soll", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "ADV", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dich mit Freuden anzubinden/", "tokens": ["Dich", "mit", "Freu\u00b7den", "an\u00b7zu\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVIZU", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Frey und froh und freudig seyn.", "tokens": ["Frey", "und", "froh", "und", "freu\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Unter Freunden eine Lust kan uns kein Gesetze wehren/", "tokens": ["Un\u00b7ter", "Freun\u00b7den", "ei\u00b7ne", "Lust", "kan", "uns", "kein", "Ge\u00b7set\u00b7ze", "weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wer wei\u00df/ was das Gl\u00fccke noch ein- und andern kan besche-", "tokens": ["Wer", "wei\u00df", "/", "was", "das", "Gl\u00fc\u00b7cke", "noch", "ein", "und", "an\u00b7dern", "kan", "be\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$(", "PWS", "ART", "NN", "ADV", "TRUNC", "KON", "PIS", "VMFIN", "TRUNC"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ey so geht es Sorgen-blo\u00df", "tokens": ["Ey", "so", "geht", "es", "Sor\u00b7gen\u00b7blo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auff die unverzehrten G\u00fctter", "tokens": ["Auff", "die", "un\u00b7ver\u00b7zehr\u00b7ten", "G\u00fct\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsrer lieben Schwieger-M\u00fctter/", "tokens": ["Uns\u00b7rer", "lie\u00b7ben", "Schwie\u00b7ger\u00b7M\u00fct\u00b7ter", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bruder/ im Vertrauen lo\u00df.", "tokens": ["Bru\u00b7der", "/", "im", "Ver\u00b7trau\u00b7en", "lo\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}