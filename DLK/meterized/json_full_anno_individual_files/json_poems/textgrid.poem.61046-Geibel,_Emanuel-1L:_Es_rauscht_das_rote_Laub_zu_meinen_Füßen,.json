{"textgrid.poem.61046": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es rauscht das rote Laub zu meinen F\u00fc\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es rauscht das rote Laub zu meinen F\u00fc\u00dfen,", "tokens": ["Es", "rauscht", "das", "ro\u00b7te", "Laub", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wenn es wieder gr\u00fcnt, wo weil' ich dann?", "tokens": ["Doch", "wenn", "es", "wie\u00b7der", "gr\u00fcnt", ",", "wo", "weil'", "ich", "dann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo werden mich die ersten Schwalben gr\u00fc\u00dfen?", "tokens": ["Wo", "wer\u00b7den", "mich", "die", "ers\u00b7ten", "Schwal\u00b7ben", "gr\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach ferne, fern der S\u00fc\u00dfen,", "tokens": ["Ach", "fer\u00b7ne", ",", "fern", "der", "S\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und nimmer bin ich mehr ein froher Mann.", "tokens": ["Und", "nim\u00b7mer", "bin", "ich", "mehr", "ein", "fro\u00b7her", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sonst sang ich stets durch Flur und Bergeshalde", "tokens": ["Sonst", "sang", "ich", "stets", "durch", "Flur", "und", "Ber\u00b7ges\u00b7hal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im braunen Herbst, in flock'ger Winterszeit:", "tokens": ["Im", "brau\u00b7nen", "Herbst", ",", "in", "flock'\u00b7ger", "Win\u00b7ter\u00b7szeit", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O sch\u00f6ner Fr\u00fchling, komm zu deinem Walde,", "tokens": ["O", "sch\u00f6\u00b7ner", "Fr\u00fch\u00b7ling", ",", "komm", "zu", "dei\u00b7nem", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Komme balde, balde, balde!", "tokens": ["Kom\u00b7me", "bal\u00b7de", ",", "bal\u00b7de", ",", "bal\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADV", "$,", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun sing' ich: Sch\u00f6ner Fr\u00fchling, bleibe weit!", "tokens": ["Nun", "sing'", "ich", ":", "Sch\u00f6\u00b7ner", "Fr\u00fch\u00b7ling", ",", "blei\u00b7be", "weit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "w