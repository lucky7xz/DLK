{"dta.poem.1282": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vergebene Hoffnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Offters traumt dem/ der gebunden/", "tokens": ["Off\u00b7ters", "traumt", "dem", "/", "der", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "$(", "ART", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er seine Freyheit hat:", "tokens": ["Da\u00df", "er", "sei\u00b7ne", "Frey\u00b7heit", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der Morgen angegangen/", "tokens": ["Wenn", "der", "Mor\u00b7gen", "an\u00b7ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der falsche Traum verschwunden/", "tokens": ["Ist", "der", "fal\u00b7sche", "Traum", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Hoffnung findt nicht statt.", "tokens": ["Und", "die", "Hoff\u00b7nung", "findt", "nicht", "statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Er bleibt an den Fesseln hangen/", "tokens": ["Er", "bleibt", "an", "den", "Fes\u00b7seln", "han\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Das geht ihm viel schwerer ein:", "tokens": ["Das", "geht", "ihm", "viel", "schwe\u00b7rer", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Besser ists ohn Hoffnung seyn!", "tokens": ["Bes\u00b7ser", "ists", "ohn", "Hoff\u00b7nung", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bettler sind offt reich an Sch\u00e4tzen/", "tokens": ["Bett\u00b7ler", "sind", "offt", "reich", "an", "Sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Die der falsche Traum gewehrt;", "tokens": ["Die", "der", "fal\u00b7sche", "Traum", "ge\u00b7wehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der Tag ist angebrochen", "tokens": ["Wenn", "der", "Tag", "ist", "an\u00b7ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So verschwindet ihr Erg\u00f6tzen/", "tokens": ["So", "ver\u00b7schwin\u00b7det", "ihr", "Er\u00b7g\u00f6t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Beutel bleibt geleert.", "tokens": ["Und", "der", "Beu\u00b7tel", "bleibt", "ge\u00b7leert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Brod f\u00fcr fremder Th\u00fcre suchen", "tokens": ["Brod", "f\u00fcr", "frem\u00b7der", "Th\u00fc\u00b7re", "su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Geht hernach viel schwerer ein:", "tokens": ["Geht", "her\u00b7nach", "viel", "schwe\u00b7rer", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Besser ists ohn Hoffnung seyn!", "tokens": ["Bes\u00b7ser", "ists", "ohn", "Hoff\u00b7nung", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}