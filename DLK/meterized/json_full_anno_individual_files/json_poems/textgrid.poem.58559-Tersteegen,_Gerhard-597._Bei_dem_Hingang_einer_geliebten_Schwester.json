{"textgrid.poem.58559": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "597. Bei dem Hingang einer geliebten Schwester", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schwester Gl\u00fcck ist wahrlich gro\u00df,", "tokens": ["Der", "Schwes\u00b7ter", "Gl\u00fcck", "ist", "wahr\u00b7lich", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kriegt das letzt' und beste Los;", "tokens": ["Sie", "kriegt", "das", "letzt'", "und", "bes\u00b7te", "Los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Hirt' nimmt 's Sch\u00e4flein von der Heid',", "tokens": ["Der", "Hirt'", "nimmt", "'", "s", "Sch\u00e4f\u00b7lein", "von", "der", "Heid", "'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ADJA", "NN", "APPR", "ART", "NN", "$(", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wohin? Auf Zions gr\u00fcne Weid'.", "tokens": ["Wo\u00b7hin", "?", "Auf", "Zi\u00b7ons", "gr\u00fc\u00b7ne", "Weid'", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr andern kommt, geht frisch drauf zu,", "tokens": ["Ihr", "an\u00b7dern", "kommt", ",", "geht", "frisch", "drauf", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "$,", "VVFIN", "ADJD", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf muntres Gehn folgt sel'ge Ruh,", "tokens": ["Auf", "mun\u00b7tres", "Gehn", "folgt", "sel'\u00b7ge", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sterbt euch, lebt Gotte von nun an,", "tokens": ["Sterbt", "euch", ",", "lebt", "Got\u00b7te", "von", "nun", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "NN", "APPR", "ADV", "PTKVZ", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dann ist eu'r Sterben bald getan!", "tokens": ["Dann", "ist", "eu'r", "Ster\u00b7ben", "bald", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer in Gott stirbt, stirbt in Gott selig,", "tokens": ["Wer", "in", "Gott", "stirbt", ",", "stirbt", "in", "Gott", "se\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$,", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Stirbt schon nicht jeder auch gleich fr\u00f6hlich.", "tokens": ["Stirbt", "schon", "nicht", "je\u00b7der", "auch", "gleich", "fr\u00f6h\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "PIS", "ADV", "ADV", "ADJD", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Der Schwester Gl\u00fcck ist wahrlich gro\u00df,", "tokens": ["Der", "S