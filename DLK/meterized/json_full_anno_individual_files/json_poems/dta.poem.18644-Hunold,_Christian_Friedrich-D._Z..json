{"dta.poem.18644": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "D. Z.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt mein Hertz/ und spricht/ ich bin besieget/", "tokens": ["Hier", "liegt", "mein", "Hertz", "/", "und", "spricht", "/", "ich", "bin", "be\u00b7sie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "$(", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier beut mein Geist sich dir zum Sclaven an.", "tokens": ["Hier", "beut", "mein", "Geist", "sich", "dir", "zum", "Scla\u00b7ven", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Freyheit hat mich nicht so sehr vergn\u00fcget/", "tokens": ["Die", "Frey\u00b7heit", "hat", "mich", "nicht", "so", "sehr", "ver\u00b7gn\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als da du mir die Fessel angethan.", "tokens": ["Als", "da", "du", "mir", "die", "Fes\u00b7sel", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer mich befreyt/ sucht mein Verderben/", "tokens": ["Wer", "mich", "be\u00b7freyt", "/", "sucht", "mein", "Ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist nur Sterben", "tokens": ["Es", "ist", "nur", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Mein l\u00f6se Geld.", "tokens": ["Mein", "l\u00f6\u00b7se", "Geld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Dinstbarkeit/ die ich so willig Leide-", "tokens": ["Die", "Dinst\u00b7bar\u00b7keit", "/", "die", "ich", "so", "wil\u00b7lig", "Lei\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist viel zu sch\u00f6n/ da\u00df jemand lo\u00df begehrt.", "tokens": ["Ist", "viel", "zu", "sch\u00f6n", "/", "da\u00df", "je\u00b7mand", "lo\u00df", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKA", "ADJD", "$(", "KOUS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein edles Joch ist weicher Sammt und Seide/", "tokens": ["Mein", "ed\u00b7les", "Joch", "ist", "wei\u00b7cher", "Sammt", "und", "Sei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJA", "NN", "KON", "NE", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Ketten sind des besten Goldes werth.", "tokens": ["Die", "Ket\u00b7ten", "sind", "des", "bes\u00b7ten", "Gol\u00b7des", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Sclaven Hau\u00df der holden Plagen/", "tokens": ["Das", "Scla\u00b7ven", "Hau\u00df", "der", "hol\u00b7den", "Pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist kurtz zu sagen/", "tokens": ["Ist", "kurtz", "zu", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Mein Paradie\u00df. ", "tokens": ["Mein", "Pa\u00b7ra\u00b7die\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Es mag die Welt ihr Jubel-Fest begehen/", "tokens": ["Es", "mag", "die", "Welt", "ihr", "Ju\u00b7bel\u00b7Fest", "be\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ruffe Rom die freyen Zeiten aus.", "tokens": ["Es", "ruf\u00b7fe", "Rom", "die", "frey\u00b7en", "Zei\u00b7ten", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich will bestrickt/ und stets gebunden stehen", "tokens": ["Ich", "will", "be\u00b7strickt", "/", "und", "stets", "ge\u00b7bun\u00b7den", "ste\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVPP", "$(", "KON", "ADV", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es sucht dein Knecht bey dir sein Sclaven-Hau\u00df", "tokens": ["Es", "sucht", "dein", "Knecht", "bey", "dir", "sein", "Scla\u00b7ven\u00b7Hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mein keusches Hertz/ so dir verbunden/", "tokens": ["Mein", "keu\u00b7sches", "Hertz", "/", "so", "dir", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ADV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Rufft alle Stunden/", "tokens": ["Rufft", "al\u00b7le", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ach zweiffle nicht.", "tokens": ["Ach", "zweiff\u00b7le", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Du solt mein Kind allein dieselbe bleiben/", "tokens": ["Du", "solt", "mein", "Kind", "al\u00b7lein", "die\u00b7sel\u00b7be", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "PDAT", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von der ein Winck mir gleich befehlen kan.", "tokens": ["Von", "der", "ein", "Win\u00b7ck", "mir", "gleich", "be\u00b7feh\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es soll mich nichts von deiner Seite treiben", "tokens": ["Es", "soll", "mich", "nichts", "von", "dei\u00b7ner", "Sei\u00b7te", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es fasse mich gleich Todt und Ungl\u00fcck an:", "tokens": ["Es", "fas\u00b7se", "mich", "gleich", "Todt", "und", "Un\u00b7gl\u00fcck", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es wird die Treu doch nicht gebrochen/", "tokens": ["Es", "wird", "die", "Treu", "doch", "nicht", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die ich versprochen/", "tokens": ["Die", "ich", "ver\u00b7spro\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Das schwer ich dir.", "tokens": ["Das", "schwer", "ich", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPER", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}