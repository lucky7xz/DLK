{"textgrid.poem.36087": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XlVIII. Durch Nacht zum Licht.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und ist kein Arm zu finden, der den Gebeugten h\u00e4lt,", "tokens": ["Und", "ist", "kein", "Arm", "zu", "fin\u00b7den", ",", "der", "den", "Ge\u00b7beug\u00b7ten", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der tief in Labyrinthen verzweifelnd irrt und f\u00e4llt?", "tokens": ["Der", "tief", "in", "La\u00b7by\u00b7rin\u00b7then", "ver\u00b7zwei\u00b7felnd", "irrt", "und", "f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVPP", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "F\u00fchrt keine Hand der Lieb' ihn zur\u00fcck vom Abgrundrand?", "tokens": ["F\u00fchrt", "kei\u00b7ne", "Hand", "der", "Lieb'", "ihn", "zu\u00b7r\u00fcck", "vom", "Ab\u00b7grun\u00b7drand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nicht eine Hand im Himmel, auf Erden keine Hand? \u2013", "tokens": ["Nicht", "ei\u00b7ne", "Hand", "im", "Him\u00b7mel", ",", "auf", "Er\u00b7den", "kei\u00b7ne", "Hand", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPRART", "NN", "$,", "APPR", "NN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Weh! Rettungslos verloren! Du armer, armer Mann,", "tokens": ["Weh", "!", "Ret\u00b7tungs\u00b7los", "ver\u00b7lo\u00b7ren", "!", "Du", "ar\u00b7mer", ",", "ar\u00b7mer", "Mann", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "VVPP", "$.", "PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dem auch die letzte Hoffnung wie Wellenschaum zerrann!", "tokens": ["Dem", "auch", "die", "letz\u00b7te", "Hoff\u00b7nung", "wie", "Wel\u00b7len\u00b7schaum", "zer\u00b7rann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Des Geistes Arglist stahl Dir teuflisch den holden Schein;", "tokens": ["Des", "Geis\u00b7tes", "Arg\u00b7list", "stahl", "Dir", "teuf\u00b7lisch", "den", "hol\u00b7den", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Blieb Dir nur ", "tokens": ["Blieb", "Dir", "nur"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Es wallen Donnerwolken wie Flor der Nacht um Dich,", "tokens": ["Es", "wal\u00b7len", "Don\u00b7ner\u00b7wol\u00b7ken", "wie", "Flor", "der", "Nacht", "um", "Dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "NN", "KOKOM", "NN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und grausam reisst des Abgrunds Gewalt Dich hin zu sich.", "tokens": ["Und", "grau\u00b7sam", "reisst", "des", "Ab\u00b7grunds", "Ge\u00b7walt", "Dich", "hin", "zu", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "PPER", "ADV", "APPR", "PRF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch Wunder! Welcher Lichtglanz? \u2013 Ein Zauberruthenschlag!", "tokens": ["Doch", "Wun\u00b7der", "!", "Wel\u00b7cher", "Licht\u00b7glanz", "?", "\u2013", "Ein", "Zau\u00b7ber\u00b7ru\u00b7then\u00b7schlag", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "PWAT", "NN", "$.", "$(", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Aus Nacht und Donnerwolken aufgl\u00fcht ein goldner Tag!", "tokens": ["Aus", "Nacht", "und", "Don\u00b7ner\u00b7wol\u00b7ken", "auf\u00b7gl\u00fcht", "ein", "gold\u00b7ner", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Schatten fliehn; es kleidet in Helle sich die Nacht;", "tokens": ["Die", "Schat\u00b7ten", "fliehn", ";", "es", "klei\u00b7det", "in", "Hel\u00b7le", "sich", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "PPER", "VVFIN", "APPR", "NE", "PRF", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Wolken weinen Perlen und blauer Aether lacht.", "tokens": ["Die", "Wol\u00b7ken", "wei\u00b7nen", "Per\u00b7len", "und", "blau\u00b7er", "A\u00b7e\u00b7ther", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was schwebt, von Irisfarben, von Silberschmelz umwallt,", "tokens": ["Was", "schwebt", ",", "von", "I\u00b7ris\u00b7far\u00b7ben", ",", "von", "Sil\u00b7ber\u00b7schmelz", "um\u00b7wallt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Huldreich zur Erde nieder, ein Seraph an Gestalt?", "tokens": ["Huld\u00b7reich", "zur", "Er\u00b7de", "nie\u00b7der", ",", "ein", "Se\u00b7raph", "an", "Ge\u00b7stalt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Wer ist die heilge Jungfrau, die Leid und Schmerzen stillt,", "tokens": ["Wer", "ist", "die", "heil\u00b7ge", "Jung\u00b7frau", ",", "die", "Leid", "und", "Schmer\u00b7zen", "stillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der aus dem Sonnenauge die warme Z\u00e4hre quillt?", "tokens": ["Der", "aus", "dem", "Son\u00b7nen\u00b7au\u00b7ge", "die", "war\u00b7me", "Z\u00e4h\u00b7re", "quillt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der eine sanfte R\u00fchrung das Engelherz bewegt?", "tokens": ["Der", "ei\u00b7ne", "sanf\u00b7te", "R\u00fch\u00b7rung", "das", "En\u00b7gel\u00b7herz", "be\u00b7wegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die t\u00f6nend in der Rechten die Sternenlyra tr\u00fcgt?", "tokens": ["Die", "t\u00f6\u00b7nend", "in", "der", "Rech\u00b7ten", "die", "Ster\u00b7nen\u00b7ly\u00b7ra", "tr\u00fcgt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Ihr Blick sucht den Gefallnen, ", "tokens": ["Ihr", "Blick", "sucht", "den", "Ge\u00b7fall\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr m\u00e4chtger Wink entr\u00fcckt ihn schnell in ihr Heimathland.", "tokens": ["Ihr", "m\u00e4cht\u00b7ger", "Wink", "ent\u00b7r\u00fcckt", "ihn", "schnell", "in", "ihr", "Hei\u00b7mat\u00b7hland", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dort, unter Zauberb\u00e4umen schl\u00e4ft Faustus sanft und s\u00fcss,", "tokens": ["Dort", ",", "un\u00b7ter", "Zau\u00b7ber\u00b7b\u00e4u\u00b7men", "schl\u00e4ft", "Faus\u00b7tus", "sanft", "und", "s\u00fcss", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "VVFIN", "NE", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und schaut in sch\u00f6nen Tr\u00e4umen ein neues Paradies.", "tokens": ["Und", "schaut", "in", "sch\u00f6\u00b7nen", "Tr\u00e4u\u00b7men", "ein", "neu\u00b7es", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Und leise greift die Jungfrau in ihre Saiten ein,", "tokens": ["Und", "lei\u00b7se", "greift", "die", "Jung\u00b7frau", "in", "ih\u00b7re", "Sai\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Wunderklang entstr\u00f6mt ihr melodisch, himmlisch rein.", "tokens": ["Und", "Wun\u00b7der\u00b7klang", "ent\u00b7str\u00f6mt", "ihr", "me\u00b7lo\u00b7disch", ",", "himm\u00b7lisch", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So rauscht es ird'schen S\u00e4ngern von Harfensaiten nie,", "tokens": ["So", "rauscht", "es", "ird'\u00b7schen", "S\u00e4n\u00b7gern", "von", "Har\u00b7fen\u00b7sai\u00b7ten", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das macht, die hehre G\u00f6ttin ist selbst die ", "tokens": ["Das", "macht", ",", "die", "heh\u00b7re", "G\u00f6t\u00b7tin", "ist", "selbst", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "NN", "VAFIN", "ADV", "ART"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Sie rettet den Gesunknen, sie nimmt sich seiner an,", "tokens": ["Sie", "ret\u00b7tet", "den", "Ge\u00b7sun\u00b7knen", ",", "sie", "nimmt", "sich", "sei\u00b7ner", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "PPOSAT", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie hat in ihren Armen ihm Himmel aufgethan.", "tokens": ["Sie", "hat", "in", "ih\u00b7ren", "Ar\u00b7men", "ihm", "Him\u00b7mel", "auf\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie hat ihn sanft gebettet; ihn, den die Nacht bezwang,", "tokens": ["Sie", "hat", "ihn", "sanft", "ge\u00b7bet\u00b7tet", ";", "ihn", ",", "den", "die", "Nacht", "be\u00b7zwang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$.", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hat Poesie gerettet vom ew'gen Untergang.", "tokens": ["Hat", "Poe\u00b7sie", "ge\u00b7ret\u00b7tet", "vom", "ew'\u00b7gen", "Un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Und er hat nicht vergebens gerungen und gestrebt,", "tokens": ["Und", "er", "hat", "nicht", "ver\u00b7ge\u00b7bens", "ge\u00b7run\u00b7gen", "und", "ge\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ob mannichfach auch irrend, sein stolzer Name \u2013 ", "tokens": ["Ob", "man\u00b7nich\u00b7fach", "auch", "ir\u00b7rend", ",", "sein", "stol\u00b7zer", "Na\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$("], "meter": "--+--+--+---", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Glorreich durch manch Jahrhundert, und wird nicht untergehn.", "tokens": ["Glor\u00b7reich", "durch", "manch", "Jahr\u00b7hun\u00b7dert", ",", "und", "wird", "nicht", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "$,", "KON", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.10": {"line.1": {"text": "Ihm ward von hohen Meistern manch k\u00fchner Sang geweiht,", "tokens": ["Ihm", "ward", "von", "ho\u00b7hen", "Meis\u00b7tern", "manch", "k\u00fch\u00b7ner", "Sang", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sein Name wird begeistern auch S\u00e4nger sp\u00e4trer Zeit.", "tokens": ["Sein", "Na\u00b7me", "wird", "be\u00b7geis\u00b7tern", "auch", "S\u00e4n\u00b7ger", "sp\u00e4t\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN", "ADV", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Dichter Liedesbl\u00fcthen, ihr unverwelklich Wort,", "tokens": ["Der", "Dich\u00b7ter", "Lie\u00b7des\u00b7bl\u00fc\u00b7then", ",", "ihr", "un\u00b7ver\u00b7wel\u00b7klich", "Wort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sind W\u00e4chter die da h\u00fcten der Sage heil'gen Hort. \u2013", "tokens": ["Sind", "W\u00e4ch\u00b7ter", "die", "da", "h\u00fc\u00b7ten", "der", "Sa\u00b7ge", "heil'\u00b7gen", "Hort", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NE", "ART", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Ist nicht manch Dichterleben dem Leben Fausts verwandt?", "tokens": ["Ist", "nicht", "manch", "Dich\u00b7ter\u00b7le\u00b7ben", "dem", "Le\u00b7ben", "Fausts", "ver\u00b7wandt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PIAT", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein unerreichtes Streben, ein ewiggl\u00fchnder Brand?", "tokens": ["Ein", "un\u00b7er\u00b7reich\u00b7tes", "Stre\u00b7ben", ",", "ein", "e\u00b7wig\u00b7gl\u00fchn\u00b7der", "Brand", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein St\u00fcrmen der Gef\u00fchle, ein Drang, den Keiner stillt,", "tokens": ["Ein", "St\u00fcr\u00b7men", "der", "Ge\u00b7f\u00fch\u00b7le", ",", "ein", "Drang", ",", "den", "Kei\u00b7ner", "stillt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Den oft der Schwermuth Schw\u00fcle d\u00e4monisch \u00fcberh\u00fcllt?", "tokens": ["Den", "oft", "der", "Schwer\u00b7muth", "Schw\u00fc\u00b7le", "d\u00e4\u00b7mo\u00b7nisch", "\u00fc\u00b7berh\u00b7\u00fcllt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Es poche trotzig Keiner auf seines Geistes Kraft!", "tokens": ["Es", "po\u00b7che", "trot\u00b7zig", "Kei\u00b7ner", "auf", "sei\u00b7nes", "Geis\u00b7tes", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PIS", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Manch Irrender, manch Reiner ward fr\u00fch dahingerafft.", "tokens": ["Manch", "Ir\u00b7ren\u00b7der", ",", "manch", "Rei\u00b7ner", "ward", "fr\u00fch", "da\u00b7hin\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wer fragt, wenn Du dahin bist, was Du gef\u00fchlt, gewollt?", "tokens": ["Wer", "fragt", ",", "wenn", "Du", "da\u00b7hin", "bist", ",", "was", "Du", "ge\u00b7f\u00fchlt", ",", "ge\u00b7wollt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PAV", "VAFIN", "$,", "PWS", "PPER", "VVPP", "$,", "VMPP", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wer rettet, wenn Dein Name hinab zum Lethe rollt?", "tokens": ["Wer", "ret\u00b7tet", ",", "wenn", "Dein", "Na\u00b7me", "hin\u00b7ab", "zum", "Le\u00b7the", "rollt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "O Leben, ewges R\u00e4thsel! Magus, \u2013 D\u00e4mon zugleich!", "tokens": ["O", "Le\u00b7ben", ",", "ew\u00b7ges", "R\u00e4th\u00b7sel", "!", "Ma\u00b7gus", ",", "\u2013", "D\u00e4\u00b7mon", "zu\u00b7gleich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$.", "NE", "$,", "$(", "NN", "ADV", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Du bleibst f\u00fcr Deine Kinder ein dunkles Geisterreich.", "tokens": ["Du", "bleibst", "f\u00fcr", "Dei\u00b7ne", "Kin\u00b7der", "ein", "dunk\u00b7les", "Geis\u00b7ter\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie treten in die Kreise, beschw\u00f6ren das Geschick;", "tokens": ["Sie", "tre\u00b7ten", "in", "die", "Krei\u00b7se", ",", "be\u00b7schw\u00f6\u00b7ren", "das", "Ge\u00b7schick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "F\u00fcr Himmel oder H\u00f6lle weiht oft \u2013 der Augenblick. \u2013", "tokens": ["F\u00fcr", "Him\u00b7mel", "o\u00b7der", "H\u00f6l\u00b7le", "weiht", "oft", "\u2013", "der", "Au\u00b7gen\u00b7blick", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ADV", "$(", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "O selig wem ein Lichtreich in Poesie erbl\u00fcht!", "tokens": ["O", "se\u00b7lig", "wem", "ein", "Lich\u00b7treich", "in", "Poe\u00b7sie", "er\u00b7bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PWS", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "---+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der S\u00e4nger tr\u00e4gt Erl\u00f6sung zum Ufer des Kozyt.", "tokens": ["Der", "S\u00e4n\u00b7ger", "tr\u00e4gt", "Er\u00b7l\u00f6\u00b7sung", "zum", "U\u00b7fer", "des", "Ko\u00b7zyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und in dem Kranz, den Nachruhm um seine Stirne flicht,", "tokens": ["Und", "in", "dem", "Kranz", ",", "den", "Nach\u00b7ruhm", "um", "sei\u00b7ne", "Stir\u00b7ne", "flicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Flammt's, eine Schrift aus Sternen, goldhell: ", "tokens": ["Flammt's", ",", "ei\u00b7ne", "Schrift", "aus", "Ster\u00b7nen", ",", "gold\u00b7hell", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "APPR", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.15": {"line.1": {"text": "Und ist kein Arm zu finden, der den Gebeugten h\u00e4lt,", "tokens": ["Und", "ist", "kein", "Arm", "zu", "fin\u00b7den", ",", "der", "den", "Ge\u00b7beug\u00b7ten", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der tief in Labyrinthen verzweifelnd irrt und f\u00e4llt?", "tokens": ["Der", "tief", "in", "La\u00b7by\u00b7rin\u00b7then", "ver\u00b7zwei\u00b7felnd", "irrt", "und", "f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVPP", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "F\u00fchrt keine Hand der Lieb' ihn zur\u00fcck vom Abgrundrand?", "tokens": ["F\u00fchrt", "kei\u00b7ne", "Hand", "der", "Lieb'", "ihn", "zu\u00b7r\u00fcck", "vom", "Ab\u00b7grun\u00b7drand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nicht eine Hand im Himmel, auf Erden keine Hand? \u2013", "tokens": ["Nicht", "ei\u00b7ne", "Hand", "im", "Him\u00b7mel", ",", "auf", "Er\u00b7den", "kei\u00b7ne", "Hand", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPRART", "NN", "$,", "APPR", "NN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Weh! Rettungslos verloren! Du armer, armer Mann,", "tokens": ["Weh", "!", "Ret\u00b7tungs\u00b7los", "ver\u00b7lo\u00b7ren", "!", "Du", "ar\u00b7mer", ",", "ar\u00b7mer", "Mann", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "VVPP", "$.", "PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dem auch die letzte Hoffnung wie Wellenschaum zerrann!", "tokens": ["Dem", "auch", "die", "letz\u00b7te", "Hoff\u00b7nung", "wie", "Wel\u00b7len\u00b7schaum", "zer\u00b7rann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Des Geistes Arglist stahl Dir teuflisch den holden Schein;", "tokens": ["Des", "Geis\u00b7tes", "Arg\u00b7list", "stahl", "Dir", "teuf\u00b7lisch", "den", "hol\u00b7den", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Blieb Dir nur ", "tokens": ["Blieb", "Dir", "nur"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+--", "measure": "dactylic.init"}}, "stanza.17": {"line.1": {"text": "Es wallen Donnerwolken wie Flor der Nacht um Dich,", "tokens": ["Es", "wal\u00b7len", "Don\u00b7ner\u00b7wol\u00b7ken", "wie", "Flor", "der", "Nacht", "um", "Dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "NN", "KOKOM", "NN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und grausam reisst des Abgrunds Gewalt Dich hin zu sich.", "tokens": ["Und", "grau\u00b7sam", "reisst", "des", "Ab\u00b7grunds", "Ge\u00b7walt", "Dich", "hin", "zu", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "PPER", "ADV", "APPR", "PRF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch Wunder! Welcher Lichtglanz? \u2013 Ein Zauberruthenschlag!", "tokens": ["Doch", "Wun\u00b7der", "!", "Wel\u00b7cher", "Licht\u00b7glanz", "?", "\u2013", "Ein", "Zau\u00b7ber\u00b7ru\u00b7then\u00b7schlag", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "PWAT", "NN", "$.", "$(", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Aus Nacht und Donnerwolken aufgl\u00fcht ein goldner Tag!", "tokens": ["Aus", "Nacht", "und", "Don\u00b7ner\u00b7wol\u00b7ken", "auf\u00b7gl\u00fcht", "ein", "gold\u00b7ner", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.18": {"line.1": {"text": "Die Schatten fliehn; es kleidet in Helle sich die Nacht;", "tokens": ["Die", "Schat\u00b7ten", "fliehn", ";", "es", "klei\u00b7det", "in", "Hel\u00b7le", "sich", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "PPER", "VVFIN", "APPR", "NE", "PRF", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Wolken weinen Perlen und blauer Aether lacht.", "tokens": ["Die", "Wol\u00b7ken", "wei\u00b7nen", "Per\u00b7len", "und", "blau\u00b7er", "A\u00b7e\u00b7ther", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was schwebt, von Irisfarben, von Silberschmelz umwallt,", "tokens": ["Was", "schwebt", ",", "von", "I\u00b7ris\u00b7far\u00b7ben", ",", "von", "Sil\u00b7ber\u00b7schmelz", "um\u00b7wallt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Huldreich zur Erde nieder, ein Seraph an Gestalt?", "tokens": ["Huld\u00b7reich", "zur", "Er\u00b7de", "nie\u00b7der", ",", "ein", "Se\u00b7raph", "an", "Ge\u00b7stalt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.19": {"line.1": {"text": "Wer ist die heilge Jungfrau, die Leid und Schmerzen stillt,", "tokens": ["Wer", "ist", "die", "heil\u00b7ge", "Jung\u00b7frau", ",", "die", "Leid", "und", "Schmer\u00b7zen", "stillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$,", "