{"dta.poem.2838": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Feld ist das Gem\u00fcth, und du bist sein Besteller;", "tokens": ["Ein", "Feld", "ist", "das", "Ge\u00b7m\u00fcth", ",", "und", "du", "bist", "sein", "Be\u00b7stel\u00b7ler", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Baust du es gut, so w\u00e4chst darauf das Gute schneller.", "tokens": ["Baust", "du", "es", "gut", ",", "so", "w\u00e4chst", "da\u00b7rauf", "das", "Gu\u00b7te", "schnel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PAV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch nicht w\u00e4chst Nichts darauf, weil du es nicht gebaut;", "tokens": ["Doch", "nicht", "w\u00e4chst", "Nichts", "da\u00b7rauf", ",", "weil", "du", "es", "nicht", "ge\u00b7baut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "PIS", "PAV", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Unkraut stellt von selbst sich ein, wo fehlt das Kraut.", "tokens": ["Das", "Un\u00b7kraut", "stellt", "von", "selbst", "sich", "ein", ",", "wo", "fehlt", "das", "Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "PRF", "PTKVZ", "$,", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und auszuraufen auch das Unkraut, hilft dir nimmer,", "tokens": ["Und", "aus\u00b7zu\u00b7rau\u00b7fen", "auch", "das", "Un\u00b7kraut", ",", "hilft", "dir", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn seine Wurzeln doch l\u00e4\u00dft es im Boden immer.", "tokens": ["Denn", "sei\u00b7ne", "Wur\u00b7zeln", "doch", "l\u00e4\u00dft", "es", "im", "Bo\u00b7den", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Und willst du es im Grund entwurzeln ganz und gar,", "tokens": ["Und", "willst", "du", "es", "im", "Grund", "ent\u00b7wur\u00b7zeln", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "APPRART", "NN", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu untergraben mit das Kraut l\u00e4ufst du Gefahr.", "tokens": ["Zu", "un\u00b7ter\u00b7gra\u00b7ben", "mit", "das", "Kraut", "l\u00e4ufst", "du", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was also bleibt zu thun? das Unkraut niederhalten,", "tokens": ["Was", "al\u00b7so", "bleibt", "zu", "thun", "?", "das", "Un\u00b7kraut", "nie\u00b7der\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PTKZU", "VVINF", "$.", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df oben finde Raum das Kraut, sich zu entfalten.", "tokens": ["Da\u00df", "o\u00b7ben", "fin\u00b7de", "Raum", "das", "Kraut", ",", "sich", "zu", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "NN", "ART", "NN", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und hat das Kraut sein Netz dicht \u00fcbers Feld gestrickt,", "tokens": ["Und", "hat", "das", "Kraut", "sein", "Netz", "dicht", "\u00fc\u00b7bers", "Feld", "ge\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darunter ist zuletzt das Unkraut selbst erstickt.", "tokens": ["Da\u00b7run\u00b7ter", "ist", "zu\u00b7letzt", "das", "Un\u00b7kraut", "selbst", "er\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}