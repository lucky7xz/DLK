{"textgrid.poem.26557": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die ihr kampfbereit", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihr kampfbereit", "tokens": ["Die", "ihr", "kampf\u00b7be\u00b7reit"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Und kampfger\u00fcstet steht,", "tokens": ["Und", "kampf\u00b7ge\u00b7r\u00fcs\u00b7tet", "steht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Unerschrocken nach den G\u00fctern der Erde greift", "tokens": ["Un\u00b7er\u00b7schro\u00b7cken", "nach", "den", "G\u00fc\u00b7tern", "der", "Er\u00b7de", "greift"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und euch empor schwingt zu des Daseins Gipfeln:", "tokens": ["Und", "euch", "em\u00b7por", "schwingt", "zu", "des", "Da\u00b7seins", "Gip\u00b7feln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKVZ", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht \u00fcberhebt eu'rer Kraft euch", "tokens": ["Nicht", "\u00fc\u00b7ber\u00b7hebt", "eu'\u00b7rer", "Kraft", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "PPER"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und ver\u00e4chtlich nicht,", "tokens": ["Und", "ver\u00b7\u00e4cht\u00b7lich", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Inde\u00df ihr sch\u00e4umende Freudenbecher leert,", "tokens": ["In\u00b7de\u00df", "ihr", "sch\u00e4u\u00b7men\u00b7de", "Freu\u00b7den\u00b7be\u00b7cher", "leert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Blickt auf die schw\u00e4cheren Br\u00fcder hinab,", "tokens": ["Blickt", "auf", "die", "schw\u00e4\u00b7che\u00b7ren", "Br\u00fc\u00b7der", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die unten in drangvollen Tiefen", "tokens": ["Die", "un\u00b7ten", "in", "drang\u00b7vol\u00b7len", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Gl\u00fccklose Tage leben.", "tokens": ["Gl\u00fcck\u00b7lo\u00b7se", "Ta\u00b7ge", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Denn seht: auch ihnen f\u00fcllt Sehnsucht den Busen", "tokens": ["Denn", "seht", ":", "auch", "ih\u00b7nen", "f\u00fcllt", "Sehn\u00b7sucht", "den", "Bu\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ADV", "PPER", "VVFIN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach jenen lichten und freien H\u00f6h'n;", "tokens": ["Nach", "je\u00b7nen", "lich\u00b7ten", "und", "frei\u00b7en", "H\u00f6h'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch im Aufschwung ermatten sie,", "tokens": ["Doch", "im", "Auf\u00b7schwung", "er\u00b7mat\u00b7ten", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hinsinken mit blutender Stirn,", "tokens": ["Und", "hin\u00b7sin\u00b7ken", "mit", "blu\u00b7ten\u00b7der", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Bald n\u00e4her,", "tokens": ["Bald", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Bald entfernter dem Ziel,", "tokens": ["Bald", "ent\u00b7fern\u00b7ter", "dem", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die fruchtlos Ringenden,", "tokens": ["Die", "frucht\u00b7los", "Rin\u00b7gen\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.8": {"text": "Um bei den langsamen", "tokens": ["Um", "bei", "den", "lang\u00b7sa\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.9": {"text": "Schlangenbissen der Selbsterkenntni\u00df", "tokens": ["Schlan\u00b7gen\u00b7bis\u00b7sen", "der", "Selbs\u00b7ter\u00b7kennt\u00b7ni\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Besch\u00e4mender Ohnmacht Qual zu empfinden.", "tokens": ["Be\u00b7sch\u00e4\u00b7men\u00b7der", "Ohn\u00b7macht", "Qual", "zu", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "D'rum niemals komm' \u00fcber die Lippen euch", "tokens": ["D'\u00b7rum", "nie\u00b7mals", "komm'", "\u00fc\u00b7ber", "die", "Lip\u00b7pen", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Das furchtbar gedankenlose Pharis\u00e4erwort:", "tokens": ["Das", "furcht\u00b7bar", "ge\u00b7dan\u00b7ken\u00b7lo\u00b7se", "Pha\u00b7ri\u00b7s\u00e4\u00b7er\u00b7wort", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbdurch eigene Schuld.\u00ab", "tokens": ["\u00bb", "durch", "ei\u00b7ge\u00b7ne", "Schuld", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Liebreich vielmehr", "tokens": ["Lieb\u00b7reich", "viel\u00b7mehr"], "token_info": ["word", "word"], "pos": ["ADJD", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Reicht helfende H\u00e4nde hinab", "tokens": ["Reicht", "hel\u00b7fen\u00b7de", "H\u00e4n\u00b7de", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "PTKVZ"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Und zieht Jene empor,", "tokens": ["Und", "zieht", "Je\u00b7ne", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Die sie noch zu fassen verm\u00f6gen,", "tokens": ["Die", "sie", "noch", "zu", "fas\u00b7sen", "ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Auf da\u00df sie unter euch wandeln,", "tokens": ["Auf", "da\u00df", "sie", "un\u00b7ter", "euch", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Selig dankbar.", "tokens": ["Se\u00b7lig", "dank\u00b7bar", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Und dauernd nicht ist Menschenschicksal.", "tokens": ["Und", "dau\u00b7ernd", "nicht", "ist", "Men\u00b7schen\u00b7schick\u00b7sal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Manchen von Denen,", "tokens": ["Man\u00b7chen", "von", "De\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PDS", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die im Dunkel gefesselt schmachten,", "tokens": ["Die", "im", "Dun\u00b7kel", "ge\u00b7fes\u00b7selt", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Gen\u00fcgt ", "tokens": ["Ge\u00b7n\u00fcgt"], "token_info": ["word"], "pos": ["NE"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Und befreit,", "tokens": ["Und", "be\u00b7freit", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wie von pl\u00f6tzlichen Fl\u00fcgeln getragen,", "tokens": ["Wie", "von", "pl\u00f6tz\u00b7li\u00b7chen", "Fl\u00fc\u00b7geln", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Schweben zu euch sie hinan \u2013", "tokens": ["Schwe\u00b7ben", "zu", "euch", "sie", "hi\u00b7nan", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPER", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und \u00fcber euch weg,", "tokens": ["Und", "\u00fc\u00b7ber", "euch", "weg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Um f\u00fcrder im \u00c4ther zu kreisen.", "tokens": ["Um", "f\u00fcr\u00b7der", "im", "\u00c4\u00b7ther", "zu", "krei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Euere Sohle jedoch", "tokens": ["Eu\u00b7e\u00b7re", "Soh\u00b7le", "je\u00b7doch"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Bedarf des Bodens stets, um d'ran zu haften.", "tokens": ["Be\u00b7darf", "des", "Bo\u00b7dens", "stets", ",", "um", "d'\u00b7ran", "zu", "haf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,", "KOUI", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Weh' euch, wenn dieser wankt!", "tokens": ["Weh'", "euch", ",", "wenn", "die\u00b7ser", "wankt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PDS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Denn ihr wankt", "tokens": ["Denn", "ihr", "wankt"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Und st\u00fcrzt mit ihm,", "tokens": ["Und", "st\u00fcrzt", "mit", "ihm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Und der eigenen Schwere Wucht", "tokens": ["Und", "der", "ei\u00b7ge\u00b7nen", "Schwe\u00b7re", "Wucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Begr\u00e4bt euch im Fall ...", "tokens": ["Be\u00b7gr\u00e4bt", "euch", "im", "Fall", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ver\u00e4chtlich nicht,", "tokens": ["Ver\u00b7\u00e4cht\u00b7lich", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Inde\u00df ihr sch\u00e4umende Freudenbecher leert,", "tokens": ["In\u00b7de\u00df", "ihr", "sch\u00e4u\u00b7men\u00b7de", "Freu\u00b7den\u00b7be\u00b7cher", "leert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT"