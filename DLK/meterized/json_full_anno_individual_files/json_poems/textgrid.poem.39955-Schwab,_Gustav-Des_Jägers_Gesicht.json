{"textgrid.poem.39955": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Des J\u00e4gers Gesicht", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An einem Winterabend hell", "tokens": ["An", "ei\u00b7nem", "Win\u00b7ter\u00b7a\u00b7bend", "hell"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ritter jagt im Forste,", "tokens": ["Der", "Rit\u00b7ter", "jagt", "im", "Fors\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sucht das Reh im braunen Fell,", "tokens": ["Er", "sucht", "das", "Reh", "im", "brau\u00b7nen", "Fell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Eber mit der Borste;", "tokens": ["Den", "E\u00b7ber", "mit", "der", "Bors\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Umsonst ist Hornruf, Hundsgebell,", "tokens": ["Um\u00b7sonst", "ist", "Horn\u00b7ruf", ",", "Hunds\u00b7ge\u00b7bell", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jagdm\u00fcde dringt er zu dem Quell,", "tokens": ["Jagd\u00b7m\u00fc\u00b7de", "dringt", "er", "zu", "dem", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der aus Gestr\u00fcppe rauschet.", "tokens": ["Der", "aus", "Ge\u00b7str\u00fcp\u00b7pe", "rau\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Wunder dort! die Tannen bl\u00fchn", "tokens": ["O", "Wun\u00b7der", "dort", "!", "die", "Tan\u00b7nen", "bl\u00fchn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "ADV", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beknospet all mit Rosen;", "tokens": ["Be\u00b7knos\u00b7pet", "all", "mit", "Ro\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und eine Jungfrau sieht er gl\u00fchn,", "tokens": ["Und", "ei\u00b7ne", "Jung\u00b7frau", "sieht", "er", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem Kinde kosen;", "tokens": ["Mit", "ei\u00b7nem", "Kin\u00b7de", "ko\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Rasen drunter fr\u00fchlingsgr\u00fcn;", "tokens": ["Der", "Ra\u00b7sen", "drun\u00b7ter", "fr\u00fch\u00b7lings\u00b7gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vergessen sind des J\u00e4gers M\u00fchn,", "tokens": ["Ver\u00b7ges\u00b7sen", "sind", "des", "J\u00e4\u00b7gers", "M\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er senkt den Speer und staunet.", "tokens": ["Er", "senkt", "den", "Speer", "und", "stau\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da f\u00fcllt sich schnell die ganze Luft", "tokens": ["Da", "f\u00fcllt", "sich", "schnell", "die", "gan\u00b7ze", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit weichen Lenzesl\u00fcften,", "tokens": ["Mit", "wei\u00b7chen", "Len\u00b7zes\u00b7l\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da mischet sich der Rosenduft", "tokens": ["Da", "mi\u00b7schet", "sich", "der", "Ro\u00b7sen\u00b7duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu andern Blumend\u00fcften.", "tokens": ["Zu", "an\u00b7dern", "Blu\u00b7men\u00b7d\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und nieder braus't es zu der Schluft", "tokens": ["Und", "nie\u00b7der", "braus't", "es", "zu", "der", "Schluft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und senkt sich bis zur Quellenkluft,", "tokens": ["Und", "senkt", "sich", "bis", "zur", "Quel\u00b7len\u00b7kluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein goldner Feuerwagen.", "tokens": ["Ein", "gold\u00b7ner", "Feu\u00b7er\u00b7wa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und L\u00f6w' und Lamm ist das Gespann,", "tokens": ["Und", "L\u00f6\u00b7w'", "und", "Lamm", "ist", "das", "Ge\u00b7spann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Engel fliegt vom Sitze,", "tokens": ["Ein", "En\u00b7gel", "fliegt", "vom", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hebt Jungfrau, hebet Kind hinan,", "tokens": ["Hebt", "Jung\u00b7frau", ",", "he\u00b7bet", "Kind", "hi\u00b7nan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Schwingt auf sich gleich dem Blitze;", "tokens": ["Schwingt", "auf", "sich", "gleich", "dem", "Blit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ADV", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Er zieht beperlte Z\u00fcgel an,", "tokens": ["Er", "zieht", "be\u00b7perl\u00b7te", "Z\u00fc\u00b7gel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und lenkt empor zur Himmelsbahn", "tokens": ["Und", "lenkt", "em\u00b7por", "zur", "Him\u00b7mels\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die raschen Fl\u00fcgelthiere.", "tokens": ["Die", "ra\u00b7schen", "Fl\u00fc\u00b7gelt\u00b7hie\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da sch\u00fcttelt der gestreifte Stamm", "tokens": ["Da", "sch\u00fct\u00b7telt", "der", "ge\u00b7streif\u00b7te", "Stamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Rosen einen Regen,", "tokens": ["Von", "Ro\u00b7sen", "ei\u00b7nen", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Wagen steiget mit Geflamm", "tokens": ["Der", "Wa\u00b7gen", "stei\u00b7get", "mit", "Ge\u00b7flamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Aetherblau entgegen.", "tokens": ["Dem", "A\u00b7e\u00b7ther\u00b7blau", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der gelbe Leu, das wei\u00dfe Lamm,", "tokens": ["Der", "gel\u00b7be", "Leu", ",", "das", "wei\u00b7\u00dfe", "Lamm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Jungfrau Purpur leis verschwamm", "tokens": ["Der", "Jung\u00b7frau", "Pur\u00b7pur", "leis", "ver\u00b7schwamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In sanfter Abendwolke.", "tokens": ["In", "sanf\u00b7ter", "Ab\u00b7end\u00b7wol\u00b7ke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Ritter eine lange Frist", "tokens": ["Der", "Rit\u00b7ter", "ei\u00b7ne", "lan\u00b7ge", "Frist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht in dem Wald versunken,", "tokens": ["Steht", "in", "dem", "Wald", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Auge, farbekrank, vermi\u00dft", "tokens": ["Sein", "Au\u00b7ge", ",", "far\u00b7be\u00b7krank", ",", "ver\u00b7mi\u00dft"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Stral, von dem es trunken,", "tokens": ["Den", "Stral", ",", "von", "dem", "es", "trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann wird ihm klar, da\u00df es der Christ", "tokens": ["Dann", "wird", "ihm", "klar", ",", "da\u00df", "es", "der", "Christ"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seiner sel'gen Mutter ist,", "tokens": ["Mit", "sei\u00b7ner", "sel'\u00b7gen", "Mut\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die ihm im Wald erschienen.", "tokens": ["Die", "ihm", "im", "Wald", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Leb' wohl nun, Jagd, leb' wohl nun, Welt!", "tokens": ["Leb'", "wohl", "nun", ",", "Jagd", ",", "leb'", "wohl", "nun", ",", "Welt", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "$,", "NN", "$,", "VVFIN", "ADV", "ADV", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er baut sich die Kapelle,", "tokens": ["Er", "baut", "sich", "die", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Rosenstr\u00e4uchen aufgehellt,", "tokens": ["Von", "Ro\u00b7sen\u00b7str\u00e4u\u00b7chen", "auf\u00b7ge\u00b7hellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Glaubenstr\u00e4umen helle.", "tokens": ["Von", "Glau\u00b7ben\u00b7str\u00e4u\u00b7men", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So oft des Abends Schleier f\u00e4llt,", "tokens": ["So", "oft", "des", "A\u00b7bends", "Schlei\u00b7er", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Fliegt sein Gebet zum Himmelszelt", "tokens": ["Fliegt", "sein", "Ge\u00b7bet", "zum", "Him\u00b7mels\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In einem Flammenwagen.", "tokens": ["In", "ei\u00b7nem", "Flam\u00b7men\u00b7wa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "An einem Winterabend hell", "tokens": ["An", "ei\u00b7nem", "Win\u00b7ter\u00b7a\u00b7bend", "hell"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ritter jagt im Forste,", "tokens": ["Der", "Rit\u00b7ter", "jagt", "im", "Fors\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sucht das Reh im braunen Fell,", "tokens": ["Er", "sucht", "das", "Reh", "im", "brau\u00b7nen", "Fell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Eber mit der Borste;", "tokens": ["Den", "E\u00b7ber", "mit", "der", "Bors\u00b7te", ";"], "token_info": ["word", "word", "