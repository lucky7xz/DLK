{"dta.poem.7967": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Bekehrung zum Sonett .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Der du noch j\u00fcngst von deinem krit\u2019schen Stuhle", "tokens": ["Der", "du", "noch", "j\u00fcngst", "von", "dei\u00b7nem", "krit'\u00b7schen", "Stuh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns arme Sonettisten abgehudelt,", "tokens": ["Uns", "ar\u00b7me", "So\u00b7net\u00b7tis\u00b7ten", "ab\u00b7ge\u00b7hu\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der du von Gift und Galle recht gesprudelt", "tokens": ["Der", "du", "von", "Gift", "und", "Gal\u00b7le", "recht", "ge\u00b7spru\u00b7delt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und uns verflucht zum tiefsten H\u00f6llenpfuhle:", "tokens": ["Und", "uns", "ver\u00b7flucht", "zum", "tiefs\u00b7ten", "H\u00f6l\u00b7len\u00b7pfuh\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du reines Hermelin der alten Schule,", "tokens": ["Du", "rei\u00b7nes", "Her\u00b7me\u00b7lin", "der", "al\u00b7ten", "Schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie hast du nun dein weisses Fell besudelt!", "tokens": ["Wie", "hast", "du", "nun", "dein", "weis\u00b7ses", "Fell", "be\u00b7su\u00b7delt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja! ein Sonettlein hast du selbst gedudelt,", "tokens": ["Ja", "!", "ein", "So\u00b7net\u00b7tlein", "hast", "du", "selbst", "ge\u00b7du\u00b7delt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein schnalzend Seufzerlein an deine Buhle.", "tokens": ["Ein", "schnal\u00b7zend", "Seuf\u00b7zer\u00b7lein", "an", "dei\u00b7ne", "Buh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hast du die selbstgesteckten Warnungszeichen,", "tokens": ["Hast", "du", "die", "selbst\u00b7ge\u00b7steck\u00b7ten", "War\u00b7nungs\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast du, was halb mit Spott und halb mit Knirschen", "tokens": ["Hast", "du", ",", "was", "halb", "mit", "Spott", "und", "halb", "mit", "Knir\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "ADJD", "APPR", "NN", "KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Altmeister Vo\u00df gepredigt, all vergessen?", "tokens": ["Alt\u00b7meis\u00b7ter", "Vo\u00df", "ge\u00b7pre\u00b7digt", ",", "all", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "VVPP", "$,", "PIAT", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "F\u00fcrwahr! du bist dem Lehrer zu vergleichen,", "tokens": ["F\u00fcr\u00b7wahr", "!", "du", "bist", "dem", "Leh\u00b7rer", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der seinen Z\u00f6gling ob gestohlnen Kirschen", "tokens": ["Der", "sei\u00b7nen", "Z\u00f6g\u00b7ling", "ob", "ge\u00b7stohl\u00b7nen", "Kir\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ausschalt und scheltend selber sie gefressen.", "tokens": ["Aus\u00b7schalt", "und", "schel\u00b7tend", "sel\u00b7ber", "sie", "ge\u00b7fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}