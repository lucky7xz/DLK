{"textgrid.poem.62424": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Die Deutschen an ihren Kaiser", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutscher Kaiser! deutscher Kaiser!", "tokens": ["Deut\u00b7scher", "Kai\u00b7ser", "!", "deut\u00b7scher", "Kai\u00b7ser", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm' zu r\u00e4chen, komm' zu retten,", "tokens": ["Komm'", "zu", "r\u00e4\u00b7chen", ",", "komm'", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKZU", "VVINF", "$,", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00f6se deiner V\u00f6lker Ketten,", "tokens": ["L\u00f6\u00b7se", "dei\u00b7ner", "V\u00f6l\u00b7ker", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimm den Kranz, dir zugedacht.", "tokens": ["Nimm", "den", "Kranz", ",", "dir", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kannst ja doch nicht von uns lassen;", "tokens": ["Kannst", "ja", "doch", "nicht", "von", "uns", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schworst ja bei der Furt am Maine,", "tokens": ["Schworst", "ja", "bei", "der", "Furt", "am", "Mai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Dich zu ein'gen dem Vereine", "tokens": ["Dich", "zu", "ein'\u00b7gen", "dem", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alter V\u00e4terherrlichkeit.", "tokens": ["Al\u00b7ter", "V\u00e4\u00b7ter\u00b7herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schau', wir halten treu am Bunde,", "tokens": ["Schau'", ",", "wir", "hal\u00b7ten", "treu", "am", "Bun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Hoffen, unser Sehnen", "tokens": ["Un\u00b7ser", "Hof\u00b7fen", ",", "un\u00b7ser", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruft nicht Schweden, meint nicht D\u00e4nen,", "tokens": ["Ruft", "nicht", "Schwe\u00b7den", ",", "meint", "nicht", "D\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NE", "$,", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will nur dich, und uns, und Gott.", "tokens": ["Will", "nur", "dich", ",", "und", "uns", ",", "und", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "$,", "KON", "PPER", "$,", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Komm' in deiner heil'gen R\u00fcstung!", "tokens": ["Komm'", "in", "dei\u00b7ner", "heil'\u00b7gen", "R\u00fcs\u00b7tung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Segnend winken, z\u00fcrnend mahnen", "tokens": ["Seg\u00b7nend", "win\u00b7ken", ",", "z\u00fcr\u00b7nend", "mah\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVINF", "$,", "VVPP", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich die kaiserlichen Ahnen,", "tokens": ["Dich", "die", "kai\u00b7ser\u00b7li\u00b7chen", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rufen dich zur V\u00f6lkerschlacht.", "tokens": ["Ru\u00b7fen", "dich", "zur", "V\u00f6l\u00b7ker\u00b7schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mild wie Hirten, stark wie Felsen,", "tokens": ["Mild", "wie", "Hir\u00b7ten", ",", "stark", "wie", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stieg er von den Alpen nieder,", "tokens": ["Stieg", "er", "von", "den", "Al\u00b7pen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gab dem Reich den Kaiser wieder,", "tokens": ["Gab", "dem", "Reich", "den", "Kai\u00b7ser", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rudolph, deines Hauses Hort.", "tokens": ["Ru\u00b7dolph", ",", "dei\u00b7nes", "Hau\u00b7ses", "Hort", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Preis dem wackern Gemsenj\u00e4ger,", "tokens": ["Preis", "dem", "wa\u00b7ckern", "Gem\u00b7sen\u00b7j\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhm in Fehden, Ruhm in Frieden,", "tokens": ["Ruhm", "in", "Feh\u00b7den", ",", "Ruhm", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In Gedichten Ruhm beschieden", "tokens": ["In", "Ge\u00b7dich\u00b7ten", "Ruhm", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir, o ritterlicher Max.", "tokens": ["Dir", ",", "o", "rit\u00b7ter\u00b7li\u00b7cher", "Ma\u00b7x."], "token_info": ["word", "punct", "word", "word", "abbreviation"], "pos": ["PPER", "$,", "FM", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Als das heil'ge Reich sich trennte,", "tokens": ["Als", "das", "heil'\u00b7ge", "Reich", "sich", "trenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niedersanken alte Vesten,", "tokens": ["Nie\u00b7der\u00b7san\u00b7ken", "al\u00b7te", "Ves\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blinder Irrthum zwang die Besten", "tokens": ["Blin\u00b7der", "Irr\u00b7thum", "zwang", "die", "Bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drei\u00dfig bange Jahre lang,", "tokens": ["Drei\u00b7\u00dfig", "ban\u00b7ge", "Jah\u00b7re", "lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Achtend nicht der zarten Kindlein,", "tokens": ["Ach\u00b7tend", "nicht", "der", "zar\u00b7ten", "Kin\u00b7dlein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Priester halb und halb ein Ritter,", "tokens": ["Pries\u00b7ter", "halb", "und", "halb", "ein", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaubensfels im Ungewitter,", "tokens": ["Glau\u00b7bens\u00b7fels", "im", "Un\u00b7ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stand der fromme Ferdinand.", "tokens": ["Stand", "der", "from\u00b7me", "Fer\u00b7di\u00b7nand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Deutscher Kaiser! deutscher Kaiser!", "tokens": ["Deut\u00b7scher", "Kai\u00b7ser", "!", "deut\u00b7scher", "Kai\u00b7ser", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00e4umst du? schl\u00e4fst du? Auf, erwache!", "tokens": ["S\u00e4umst", "du", "?", "schl\u00e4fst", "du", "?", "Auf", ",", "er\u00b7wa\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "PPER", "$.", "APPR", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm' zur S\u00fchne, komm' zur Rache \u2013", "tokens": ["Komm'", "zur", "S\u00fch\u00b7ne", ",", "komm'", "zur", "Ra\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei ein Rudolph, sei ein Karl!", "tokens": ["Sei", "ein", "Ru\u00b7dolph", ",", "sei", "ein", "Karl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ruf' uns in des Reiches Namen,", "tokens": ["Ruf'", "uns", "in", "des", "Rei\u00b7ches", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lenk' uns mit den alten Fahnen,", "tokens": ["Lenk'", "uns", "mit", "den", "al\u00b7ten", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des deutschen Adlers Bahnen", "tokens": ["Auf", "des", "deut\u00b7schen", "Ad\u00b7lers", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcht uns immer noch der Sieg.", "tokens": ["Bl\u00fcht", "uns", "im\u00b7mer", "noch", "der", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Was du lenkest, was du herrschest,", "tokens": ["Was", "du", "len\u00b7kest", ",", "was", "du", "herr\u00b7schest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Alle folgen froh und willig,", "tokens": ["Al\u00b7le", "fol\u00b7gen", "froh", "und", "wil\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle finden's recht und billig,", "tokens": ["Al\u00b7le", "fin\u00b7den's", "recht", "und", "bil\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausflu\u00df h\u00f6chster Majest\u00e4t.", "tokens": ["Aus\u00b7flu\u00df", "h\u00f6chs\u00b7ter", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Schone nimmer der Emp\u00f6rer;", "tokens": ["Scho\u00b7ne", "nim\u00b7mer", "der", "Em\u00b7p\u00f6\u00b7rer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bann und Acht ob ihrem Leben!", "tokens": ["Bann", "und", "Acht", "ob", "ih\u00b7rem", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "CARD", "KOUS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blitzesstrahlen sind gegeben", "tokens": ["Blit\u00b7zes\u00b7strah\u00b7len", "sind", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir in kaiserliche Hand.", "tokens": ["Dir", "in", "kai\u00b7ser\u00b7li\u00b7che", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wirf nicht fort, was Gott geboten;", "tokens": ["Wirf", "nicht", "fort", ",", "was", "Gott", "ge\u00b7bo\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PTKVZ", "$,", "PRELS", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder auf ents\u00fchntem Throne,", "tokens": ["Wie\u00b7der", "auf", "ent\u00b7s\u00fchn\u00b7tem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der alten heil'gen Krone