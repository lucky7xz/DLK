{"textgrid.poem.29058": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solte Krieg nicht alles fressen, musten bi\u00dfher feste Pl\u00e4tze", "tokens": ["Sol\u00b7te", "Krieg", "nicht", "al\u00b7les", "fres\u00b7sen", ",", "mus\u00b7ten", "bi\u00df\u00b7her", "fes\u00b7te", "Pl\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "PTKNEG", "PIS", "VVINF", "$,", "VMFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "(selten hat es viel geholffen) sichren unsre beste Sch\u00e4tze;", "tokens": ["(", "sel\u00b7ten", "hat", "es", "viel", "ge\u00b7holf\u00b7fen", ")", "sich\u00b7ren", "uns\u00b7re", "bes\u00b7te", "Sch\u00e4t\u00b7ze", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "ADV", "VVPP", "$(", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nun der Friede triumphiret, holen wir die besten Sachen,", "tokens": ["Nun", "der", "Frie\u00b7de", "tri\u00b7um\u00b7phi\u00b7ret", ",", "ho\u00b7len", "wir", "die", "bes\u00b7ten", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df wir sie zu unsrem Brauche wieder k\u00fcnnen n\u00fctze machen.", "tokens": ["Da\u00df", "wir", "sie", "zu", "uns\u00b7rem", "Brau\u00b7che", "wie\u00b7der", "k\u00fcn\u00b7nen", "n\u00fct\u00b7ze", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Unsre Freund und unsre Kinder, Sch\u00e4tze, die wir Gott gegeben,", "tokens": ["Uns\u00b7re", "Freund", "und", "uns\u00b7re", "Kin\u00b7der", ",", "Sch\u00e4t\u00b7ze", ",", "die", "wir", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "NN", "$,", "PRELS", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Lassen in der blauen Feste billich wir bey Gotte leben;", "tokens": ["Las\u00b7sen", "in", "der", "blau\u00b7en", "Fes\u00b7te", "bil\u00b7lich", "wir", "bey", "Got\u00b7te", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADJD", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Friede, wann er gleich der sch\u00f6nste, kan die Welt doch nimmer stifften,", "tokens": ["Frie\u00b7de", ",", "wann", "er", "gleich", "der", "sch\u00f6ns\u00b7te", ",", "kan", "die", "Welt", "doch", "nim\u00b7mer", "stiff\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADV", "ART", "ADJA", "$,", "VMFIN", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Da\u00df er frey sey von dem sterben und von tausend Unfalls-Gifften.", "tokens": ["Da\u00df", "er