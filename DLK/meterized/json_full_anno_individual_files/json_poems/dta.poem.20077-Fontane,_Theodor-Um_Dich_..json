{"dta.poem.20077": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Ich las: \u201egl\u00fcckselig sind die Reinen,", "tokens": ["Ich", "las", ":", "\u201e", "gl\u00fcck\u00b7se\u00b7lig", "sind", "die", "Rei\u00b7nen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Sinn ist offen Gott zu schaun;\u201c \u2014", "tokens": ["Ihr", "Sinn", "ist", "of\u00b7fen", "Gott", "zu", "schaun", ";", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "NN", "PTKZU", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ErEs trieb in reuevollem Weinen", "tokens": ["E\u00b7rEs", "trieb", "in", "reu\u00b7e\u00b7vol\u00b7lem", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Hinaus mich in die Fr\u00fchlingsaun.", "tokens": ["Hin\u00b7aus", "mich", "in", "die", "Fr\u00fch\u00b7lin\u00b7gsaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie schwach sind unsre besten Gaben:", "tokens": ["Wie", "schwach", "sind", "uns\u00b7re", "bes\u00b7ten", "Ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebe strauchelt und die Treu,", "tokens": ["Die", "Lie\u00b7be", "strau\u00b7chelt", "und", "die", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Beste was wir Menschen haben,", "tokens": ["Das", "Bes\u00b7te", "was", "wir", "Men\u00b7schen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist unser Wolln und unsre Reu.", "tokens": ["Ist", "un\u00b7ser", "Wolln", "und", "uns\u00b7re", "Reu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich rief zu Gott: \u201ewoll Du mich leiten,", "tokens": ["Ich", "rief", "zu", "Gott", ":", "\u201e", "woll", "Du", "mich", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "$(", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gnade kennt ja kein Zusp\u00e4t!\u201c", "tokens": ["Die", "Gna\u00b7de", "kennt", "ja", "kein", "Zus\u00b7p\u00e4t", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sah ich Ihn vor\u00fcberschreiten,", "tokens": ["Da", "sah", "ich", "Ihn", "vor\u00b7\u00fc\u00b7bersc\u00b7hrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Lenz, in stiller Majest\u00e4t.", "tokens": ["Wie", "Lenz", ",", "in", "stil\u00b7ler", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}