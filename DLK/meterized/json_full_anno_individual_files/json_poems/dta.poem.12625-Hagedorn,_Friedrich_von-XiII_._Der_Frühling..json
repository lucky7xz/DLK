{"dta.poem.12625": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "XiII .  \n  Der Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du Schmelz der bunten Wiesen!", "tokens": ["Du", "Schmelz", "der", "bun\u00b7ten", "Wie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du neu-begr\u00fcnte Flur!", "tokens": ["Du", "neu\u00b7be\u00b7gr\u00fcn\u00b7te", "Flur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sey stets von mir gepriesen,", "tokens": ["Sey", "stets", "von", "mir", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du Schmelz der bunten Wiesen!", "tokens": ["Du", "Schmelz", "der", "bun\u00b7ten", "Wie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es schm\u00fcckt dich und Cephisen", "tokens": ["Es", "schm\u00fcckt", "dich", "und", "Ce\u00b7phi\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Der Lenz und die Natur.", "tokens": ["Der", "Lenz", "und", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du Schmelz der bunten Wiesen!", "tokens": ["Du", "Schmelz", "der", "bun\u00b7ten", "Wie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du neu- begr\u00fcnte Flur!", "tokens": ["Du", "neu", "be\u00b7gr\u00fcn\u00b7te", "Flur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "TRUNC", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du Stille voller Freuden!", "tokens": ["Du", "Stil\u00b7le", "vol\u00b7ler", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Reitzung s\u00fcsser Lust!", "tokens": ["Du", "Reit\u00b7zung", "s\u00fcs\u00b7ser", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie bist du zu beneiden,", "tokens": ["Wie", "bist", "du", "zu", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du Stille voller Freuden!", "tokens": ["Du", "Stil\u00b7le", "vol\u00b7ler", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du mehrest in uns beyden", "tokens": ["Du", "meh\u00b7rest", "in", "uns", "bey\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PPER", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Sehnsucht treuer Brust,", "tokens": ["Die", "Sehn\u00b7sucht", "treu\u00b7er", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du Stille voller Freuden!", "tokens": ["Du", "Stil\u00b7le", "vol\u00b7ler", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du Reitzung s\u00fcsser Lust!", "tokens": ["Du", "Reit\u00b7zung", "s\u00fcs\u00b7ser", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jhr schnellen Augenblicke!", "tokens": ["Ihr", "schnel\u00b7len", "Au\u00b7gen\u00b7bli\u00b7cke", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Macht euch des Fr\u00fchlings wehrt.", "tokens": ["Macht", "euch", "des", "Fr\u00fch\u00b7lings", "wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da\u00df euch ein Ku\u00df begl\u00fccke,", "tokens": ["Da\u00df", "euch", "ein", "Ku\u00df", "be\u00b7gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jhr schnellen Augenblicke!", "tokens": ["Ihr", "schnel\u00b7len", "Au\u00b7gen\u00b7bli\u00b7cke", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df uns der Ku\u00df entz\u00fccke,", "tokens": ["Da\u00df", "uns", "der", "Ku\u00df", "ent\u00b7z\u00fc\u00b7cke", ","], "token_info": ["w