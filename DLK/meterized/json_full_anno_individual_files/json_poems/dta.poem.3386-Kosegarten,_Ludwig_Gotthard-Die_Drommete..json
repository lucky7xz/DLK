{"dta.poem.3386": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Drommete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Donnerredendes Erz, Tochter der Wuth, des Sturms", "tokens": ["Don\u00b7ner\u00b7re\u00b7den\u00b7des", "Erz", ",", "Toch\u00b7ter", "der", "Wuth", ",", "des", "Sturms"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wilde freudige Braut, Schlachtengebieterin,", "tokens": ["Wil\u00b7de", "freu\u00b7di\u00b7ge", "Braut", ",", "Schlach\u00b7ten\u00b7ge\u00b7bie\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Zunge Gottes \u2014 Drommete,", "tokens": ["Zun\u00b7ge", "Got\u00b7tes", "Drom\u00b7me\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$(", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Lass mich singen den Ungest\u00fcm", "tokens": ["Lass", "mich", "sin\u00b7gen", "den", "Un\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Deines Odems. Du hast \u00f6fter die Seele mir", "tokens": ["Dei\u00b7nes", "O\u00b7dems", ".", "Du", "hast", "\u00f6f\u00b7ter", "die", "See\u00b7le", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Himmelaufw\u00e4rts gest\u00fcrmt, hast, wie der V\u00e4ter Ruf,", "tokens": ["Him\u00b7me\u00b7lauf\u00b7w\u00e4rts", "ge\u00b7st\u00fcrmt", ",", "hast", ",", "wie", "der", "V\u00e4\u00b7ter", "Ruf", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VAFIN", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Durst nach Thaten und Ruhmgier", "tokens": ["Durst", "nach", "Tha\u00b7ten", "und", "Ruhm\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In mein schlagendes Herz gestr\u00f6mt.", "tokens": ["In", "mein", "schla\u00b7gen\u00b7des", "Herz", "ge\u00b7str\u00f6mt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ja, der T\u00f6chter des Schalls bist du die Herrlichste.", "tokens": ["Ja", ",", "der", "T\u00f6ch\u00b7ter", "des", "Schalls", "bist", "du", "die", "Herr\u00b7lichs\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Als der Ewige schuf, schuf er durch dich. Du riefst,", "tokens": ["Als", "der", "E\u00b7wi\u00b7ge", "schuf", ",", "schuf", "er", "durch", "dich", ".", "Du", "riefst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$.", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und das Chaos ward Sch\u00f6pfung,", "tokens": ["Und", "das", "Chaos", "ward", "Sch\u00f6p\u00b7fung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und die eiserne Nacht ward Licht.", "tokens": ["Und", "die", "ei\u00b7ser\u00b7ne", "Nacht", "ward", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Wenn der Ewige einst \u00fcber die Welt Gericht", "tokens": ["Wenn", "der", "E\u00b7wi\u00b7ge", "einst", "\u00fc\u00b7ber", "die", "Welt", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ADV", "APPR", "ART", "NN", "NN"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Donnert, st\u00fcrmst du voran, seine Verk\u00fcnderin,", "tokens": ["Don\u00b7nert", ",", "st\u00fcrmst", "du", "vo\u00b7ran", ",", "sei\u00b7ne", "Ver\u00b7k\u00fcn\u00b7de\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und das Licht und die Sch\u00f6pfung", "tokens": ["Und", "das", "Licht", "und", "die", "Sch\u00f6p\u00b7fung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Werden Chaos und alte Nacht.", "tokens": ["Wer\u00b7den", "Chaos", "und", "al\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oft entflammtest du mich, Heldin. Mit stolzerm", "tokens": ["Oft", "ent\u00b7flamm\u00b7test", "du", "mich", ",", "Hel\u00b7din", ".", "Mit", "stol\u00b7zerm"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "NN", "$.", "APPR", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Schritt", "tokens": ["Schritt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Schritt ich unter den Reih'n herrlicher J\u00fcnglinge,", "tokens": ["Schritt", "ich", "un\u00b7ter", "den", "Reih'n", "herr\u00b7li\u00b7cher", "J\u00fcng\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--++---+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wenn dein schmetternder Wirbel", "tokens": ["Wenn", "dein", "schmet\u00b7tern\u00b7der", "Wir\u00b7bel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsern Gliedern vorangebot.", "tokens": ["Un\u00b7sern", "Glie\u00b7dern", "vor\u00b7an\u00b7ge\u00b7bot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Oft entflammtest du mich, Heldin! Im Saal voll", "tokens": ["Oft", "ent\u00b7flamm\u00b7test", "du", "mich", ",", "Hel\u00b7din", "!", "Im", "Saal", "voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "NN", "$.", "APPRART", "NN", "ADJD"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Tanz", "tokens": ["Tanz"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Flog ich wilder dahin, wenn du das Saitenspiel", "tokens": ["Flog", "ich", "wil\u00b7der", "da\u00b7hin", ",", "wenn", "du", "das", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PAV", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "\u00dcberbr\u00fclltest, und heisser", "tokens": ["\u00dc\u00b7berb\u00b7r\u00fcll\u00b7test", ",", "und", "heis\u00b7ser"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "KON", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Schlang ich mich um die T\u00e4nzerin.", "tokens": ["Schlang", "ich", "mich", "um", "die", "T\u00e4n\u00b7ze\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mein s\u00fcsslispelnd Clavier bebt im Gemach, es t\u00f6nt", "tokens": ["Mein", "s\u00fcss\u00b7lis\u00b7pelnd", "Cla\u00b7vier", "bebt", "im", "Ge\u00b7mach", ",", "es", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJD", "NE", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Meine Fl\u00f6te so s\u00fcss Abends im Hayn. Doch ha!", "tokens": ["Mei\u00b7ne", "Fl\u00f6\u00b7te", "so", "s\u00fcss", "A\u00b7bends", "im", "Hayn", ".", "Doch", "ha", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJA", "NN", "APPRART", "NN", "$.", "KON", "NE", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Dein harmonisch Gewitter", "tokens": ["Dein", "har\u00b7mo\u00b7nisch", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schwingt und sch\u00fcttert mir jeden Nerv.", "tokens": ["Schwingt", "und", "sch\u00fct\u00b7tert", "mir", "je\u00b7den", "Nerv", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Heldens\u00e4ngerin, einst, wenn ich mich h\u00f6hern Flugs", "tokens": ["Hel\u00b7den\u00b7s\u00e4n\u00b7ge\u00b7rin", ",", "einst", ",", "wenn", "ich", "mich", "h\u00f6\u00b7hern", "Flugs"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "$,", "KOUS", "PPER", "PRF", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Aufschwing', wenn ich ein Lied um die Unsterb-", "tokens": ["Auf\u00b7schwing'", ",", "wenn", "ich", "ein", "Lied", "um", "die", "Un\u00b7ster\u00b7b"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "lichkeit", "tokens": ["lich\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Singe, sing' ich durch dich es,", "tokens": ["Sin\u00b7ge", ",", "sing'", "ich", "durch", "dich", "es", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "PPER", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Und erring die Unsterblichkeit!", "tokens": ["Und", "er\u00b7ring", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}