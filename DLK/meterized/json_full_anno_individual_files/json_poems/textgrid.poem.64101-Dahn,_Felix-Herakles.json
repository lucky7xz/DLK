{"textgrid.poem.64101": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Herakles", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht stets erfreut der Sitz bei den Himmlischen,", "tokens": ["Nicht", "stets", "er\u00b7freut", "der", "Sitz", "bei", "den", "Himm\u00b7li\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf Purpurpolstern, unter dem Marmordach:", "tokens": ["Auf", "Pur\u00b7pur\u00b7pols\u00b7tern", ",", "un\u00b7ter", "dem", "Mar\u00b7mor\u00b7dach", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auch Goldget\u00e4fels wird man m\u00fcde:", "tokens": ["Auch", "Gold\u00b7ge\u00b7t\u00e4\u00b7fels", "wird", "man", "m\u00fc\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Manchmal verlangt mich nach Wald und Freiheit.", "tokens": ["Manch\u00b7mal", "ver\u00b7langt", "mich", "nach", "Wald", "und", "Frei\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Hier, wo der Fels sich schattend her\u00fcberw\u00f6lbt,", "tokens": ["Hier", ",", "wo", "der", "Fels", "sich", "schat\u00b7tend", "her\u00b7\u00fc\u00b7berw\u00b7\u00f6lbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wo durch den Eichgrund sprudelnd die Quelle rinnt,", "tokens": ["Wo", "durch", "den", "Eich\u00b7grund", "spru\u00b7delnd", "die", "Quel\u00b7le", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Hieher den Mischkrug, den bekr\u00e4nzten,", "tokens": ["Hie\u00b7her", "den", "Mischkrug", ",", "den", "be\u00b7kr\u00e4nz\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Hebe, mein Weib und doch ewig M\u00e4dchen!", "tokens": ["He\u00b7be", ",", "mein", "Weib", "und", "doch", "e\u00b7wig", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "KON", "ADV", "ADJD", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ja, lehne nur, du selige Anmut du,", "tokens": ["Ja", ",", "leh\u00b7ne", "nur", ",", "du", "se\u00b7li\u00b7ge", "An\u00b7mut", "du", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das kleine K\u00f6pfchen mir an das breite Knie", "tokens": ["Das", "klei\u00b7ne", "K\u00f6pf\u00b7chen", "mir", "an", "das", "brei\u00b7te", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und la\u00df in deinem Haar mich spielen,", "tokens": ["Und", "la\u00df", "in", "dei\u00b7nem", "Haar", "mich", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4hrend das Herz ich dir ganz erschlie\u00dfe.", "tokens": ["W\u00e4h\u00b7rend", "das", "Herz", "ich", "dir", "ganz", "er\u00b7schlie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Du wei\u00dft es nicht, du g\u00f6ttlich geborene,", "tokens": ["Du", "wei\u00dft", "es", "nicht", ",", "du", "g\u00f6tt\u00b7lich", "ge\u00b7bo\u00b7re\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "ADJD", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was doch in tiefster Seele der sch\u00f6nste Stolz,", "tokens": ["Was", "doch", "in", "tiefs\u00b7ter", "See\u00b7le", "der", "sch\u00f6ns\u00b7te", "Stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit dem ich oft in leisem L\u00e4cheln", "tokens": ["Mit", "dem", "ich", "oft", "in", "lei\u00b7sem", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "All' die Olympier \u00fcberschaue.", "tokens": ["All'", "die", "O\u00b7lym\u00b7pier", "\u00fc\u00b7bersc\u00b7hau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh, ihnen ward verdienstlos die G\u00f6ttlichkeit,", "tokens": ["Sieh", ",", "ih\u00b7nen", "ward", "ver\u00b7dienst\u00b7los", "die", "G\u00f6tt\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die ich aus eigner Kraft mir im Schwei\u00df errang,", "tokens": ["Die", "ich", "aus", "eig\u00b7ner", "Kraft", "mir", "im", "Schwei\u00df", "er\u00b7rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bis Hera selbst und all' ihr Hassen", "tokens": ["Bis", "He\u00b7ra", "selbst", "und", "all'", "ihr", "Has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADV", "KON", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Endlich besch\u00e4mt sich vers\u00f6hnen mu\u00dfte.", "tokens": ["End\u00b7lich", "be\u00b7sch\u00e4mt", "sich", "ver\u00b7s\u00f6h\u00b7nen", "mu\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "VVINF", "VMFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "O, als sie selbst, die herrliche Feindin, mir", "tokens": ["O", ",", "als", "sie", "selbst", ",", "die", "herr\u00b7li\u00b7che", "Fein\u00b7din", ",", "mir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,", "PPER"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Am G\u00f6tterhochsitz thronend, die Rechte bot, \u2013", "tokens": ["Am", "G\u00f6t\u00b7ter\u00b7hoch\u00b7sitz", "thro\u00b7nend", ",", "die", "Rech\u00b7te", "bot", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Stolz durchdrang mir da die Seele,", "tokens": ["Ein", "Stolz", "durch\u00b7drang", "mir", "da", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welchen ihr Ares doch niemals kannte!", "tokens": ["Wel\u00b7chen", "ihr", "A\u00b7res", "doch", "nie\u00b7mals", "kann\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Drum segn' ich sie um jenen gewalt'gen Groll,", "tokens": ["Drum", "segn'", "ich", "sie", "um", "je\u00b7nen", "ge\u00b7walt'\u00b7gen", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der mich verfolgend trieb bis zur Unterwelt:", "tokens": ["Der", "mich", "ver\u00b7fol\u00b7gend", "trieb", "bis", "zur", "Un\u00b7ter\u00b7welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr Ha\u00df erhob den Sohn Alkmenens", "tokens": ["Ihr", "Ha\u00df", "er\u00b7hob", "den", "Sohn", "A\u00b7lkme\u00b7nens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch zum Olymp und zu Hebes Gatten. \u2013 \u2013", "tokens": ["Hoch", "zum", "O\u00b7lymp", "und", "zu", "He\u00b7bes", "Gat\u00b7ten", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "APPRART", "NN", "KON", "APPR", "NE", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Nur Einem beugen Haupt und Gedanken sich", "tokens": ["Nur", "Ei\u00b7nem", "beu\u00b7gen", "Haupt", "und", "Ge\u00b7dan\u00b7ken", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "NN", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In hei\u00dfer Liebe stiller Bewunderung:", "tokens": ["In", "hei\u00b7\u00dfer", "Lie\u00b7be", "stil\u00b7ler", "Be\u00b7wun\u00b7de\u00b7rung", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dir, gro\u00dfer Vater Zeus Kronion,", "tokens": ["Dir", ",", "gro\u00b7\u00dfer", "Va\u00b7ter", "Zeus", "Kro\u00b7ni\u00b7on", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "NE", "NE", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Herrlichster du in steter Hoheit!", "tokens": ["Herr\u00b7lichs\u00b7ter", "du", "in", "ste\u00b7ter", "Ho\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Einst k\u00f6mmt der Tag, da alle Gewalten sich,", "tokens": ["Einst", "k\u00f6mmt", "der", "Tag", ",", "da", "al\u00b7le", "Ge\u00b7wal\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PIAT", "NN", "PRF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Giganten, G\u00f6tter, Menschen zumal, emp\u00f6rt", "tokens": ["Gi\u00b7gan\u00b7ten", ",", "G\u00f6t\u00b7ter", ",", "Men\u00b7schen", "zu\u00b7mal", ",", "em\u00b7p\u00f6rt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "ADV", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erheben gegen deine Herrschaft,", "tokens": ["Er\u00b7he\u00b7ben", "ge\u00b7gen", "dei\u00b7ne", "Herr\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil sie so viel nicht der Gr\u00f6\u00dfe tragen.", "tokens": ["Weil", "sie", "so", "viel", "nicht", "der", "Gr\u00f6\u00b7\u00dfe", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Dann birst des Hades Tor und Poseidon br\u00fcllt,", "tokens": ["Dann", "birst", "des", "Ha\u00b7des", "Tor", "und", "Po\u00b7sei\u00b7don", "br\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es tost der Kosmos: aber du l\u00e4chelst nur;", "tokens": ["Es", "tost", "der", "Kos\u00b7mos", ":", "a\u00b7ber", "du", "l\u00e4\u00b7chelst", "nur", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dir bleibt dein Blitz, dir bleibt dein Adler, \u2013", "tokens": ["Dir", "bleibt", "dein", "Blitz", ",", "dir", "bleibt", "dein", "Ad\u00b7ler", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herakles bleibt dir und seine Treue. \u2013", "tokens": ["Her\u00b7ak\u00b7les", "bleibt", "dir", "und", "sei\u00b7ne", "Treu\u00b7e", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Nicht stets erfreut der Sitz bei den Himmlischen,", "tokens": ["Nicht", "stets", "er\u00b7freut", "der", "Sitz", "bei", "den", "Himm\u00b7li\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf Purpurpolstern, unter dem Marmordach:", "tokens": ["Auf", "Pur\u00b7pur\u00b7pols\u00b7tern", ",", "un\u00b7ter", "dem", "Mar\u00b7mor\u00b7dach", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auch Goldget\u00e4fels wird man m\u00fcde:", "tokens": ["Auch", "Gold\u00b7ge\u00b7t\u00e4\u00b7fels", "wird", "man", "m\u00fc\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Manchmal verlangt mich nach Wald und Freiheit.", "tokens": ["Manch\u00b7mal", "ver\u00b7langt", "mich", "nach", "Wald", "und", "Frei\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Hier, wo der Fels sich schattend her\u00fcberw\u00f6lbt,", "tokens": ["Hier", ",", "wo", "der", "Fels", "sich", "schat\u00b7tend", "her\u00b7\u00fc\u00b7berw\u00b7\u00f6lbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wo durch den Eichgrund sprudelnd die Quelle rinnt,", "tokens": ["Wo", "durch", "den", "Eich\u00b7grund", "spru\u00b7delnd", "die", "Quel\u00b7le", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Hieher den Mischkrug, den bekr\u00e4nzten,", "tokens": ["Hie\u00b7her", "den", "Mischkrug", ",", "den", "be\u00b7kr\u00e4nz\u00b7ten", ","], "token_info": ["word", "w