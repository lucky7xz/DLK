r da die Seele,", "tokens": ["Ein", "Stolz", "durch\u00b7drang", "mir", "da", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welchen ihr Ares doch niemals kannte!", "tokens": ["Wel\u00b7chen", "ihr", "A\u00b7res", "doch", "nie\u00b7mals", "kann\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.17": {"line.1": {"text": "Drum segn' ich sie um jenen gewalt'gen Groll,", "tokens": ["Drum", "segn'", "ich", "sie", "um", "je\u00b7nen", "ge\u00b7walt'\u00b7gen", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der mich verfolgend trieb bis zur Unterwelt:", "tokens": ["Der", "mich", "ver\u00b7fol\u00b7gend", "trieb", "bis", "zur", "Un\u00b7ter\u00b7welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr Ha\u00df erhob den Sohn Alkmenens", "tokens": ["Ihr", "Ha\u00df", "er\u00b7hob", "den", "Sohn", "A\u00b7lkme\u00b7nens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch zum Olymp und zu Hebes Gatten. \u2013 \u2013", "tokens": ["Hoch", "zum", "O\u00b7lymp", "und", "zu", "He\u00b7bes", "Gat\u00b7ten", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "wo