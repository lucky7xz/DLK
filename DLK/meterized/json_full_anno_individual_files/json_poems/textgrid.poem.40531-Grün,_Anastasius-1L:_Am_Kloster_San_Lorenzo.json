{"textgrid.poem.40531": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am Kloster San Lorenzo", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Kloster San Lorenzo", "tokens": ["Am", "Klos\u00b7ter", "San", "Lo\u00b7ren\u00b7zo"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Bauer leise schellt,", "tokens": ["Ein", "Bau\u00b7er", "lei\u00b7se", "schellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der am verbr\u00e4mten Zaume", "tokens": ["Der", "am", "ver\u00b7br\u00e4m\u00b7ten", "Zau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fest seinen Esel h\u00e4lt.", "tokens": ["Fest", "sei\u00b7nen", "E\u00b7sel", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Thier wiegt auf dem Kopfe", "tokens": ["Das", "Thier", "wiegt", "auf", "dem", "Kop\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stolz seinen Federschwall,", "tokens": ["Stolz", "sei\u00b7nen", "Fe\u00b7der\u00b7schwall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00e4r's in seinem Volke", "tokens": ["Als", "w\u00e4r's", "in", "sei\u00b7nem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schier Hof- und Feldmarschall.", "tokens": ["Schier", "Hof", "und", "Feld\u00b7mar\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es tr\u00e4gt auf seinem R\u00fccken", "tokens": ["Es", "tr\u00e4gt", "auf", "sei\u00b7nem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Korb von ries'gem Ma\u00df,", "tokens": ["Den", "Korb", "von", "ries'\u00b7gem", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dazu des Bauers S\u00f6hnlein", "tokens": ["Da\u00b7zu", "des", "Bau\u00b7ers", "S\u00f6hn\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und H\u00fchnerstall und Fa\u00df.", "tokens": ["Und", "H\u00fch\u00b7ner\u00b7stall", "und", "Fa\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Kind steckt in der Kutte", "tokens": ["Das", "Kind", "steckt", "in", "der", "Kut\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Just nach des Paters Schnitt,", "tokens": ["Just", "nach", "des", "Pa\u00b7ters", "Schnitt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der aus der Klosterpforte", "tokens": ["Der", "aus", "der", "Klos\u00b7ter\u00b7pfor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar feierlich jetzt tritt.", "tokens": ["Gar", "fei\u00b7er\u00b7lich", "jetzt", "tritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So stehn die Zwei beisammen,", "tokens": ["So", "stehn", "die", "Zwei", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "CARD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie L\u00f6wenkatz' und Leu,", "tokens": ["Wie", "L\u00f6\u00b7wen\u00b7katz'", "und", "Leu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Eidechslein und Kaiman,", "tokens": ["Wie", "Ei\u00b7dechs\u00b7lein", "und", "Kai\u00b7man", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Goldfischlein und Hai.", "tokens": ["Wie", "Gold\u00b7fisc\u00b7hlein", "und", "Hai", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbnehmt, Vater, nehmt mein S\u00f6hnlein", "tokens": ["\u00bb", "nehmt", ",", "Va\u00b7ter", ",", "nehmt", "mein", "S\u00f6hn\u00b7lein"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mild auf in Lehr' und Zucht.\u00ab", "tokens": ["Mild", "auf", "in", "Lehr'", "und", "Zucht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmein Sohn, sei uns willkommen!", "tokens": ["\u00bb", "mein", "Sohn", ",", "sei", "uns", "will\u00b7kom\u00b7men", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es findet, wer da sucht!\u00ab", "tokens": ["Es", "fin\u00b7det", ",", "wer", "da", "sucht", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbmein Vater, und wer klopfet,", "tokens": ["\u00bb", "mein", "Va\u00b7ter", ",", "und", "wer", "klop\u00b7fet", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "KON", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem wird ja aufgethan;", "tokens": ["Dem", "wird", "ja", "auf\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gern legte sich zu F\u00fc\u00dfen", "tokens": ["Gern", "leg\u00b7te", "sich", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Euch dieser Puterhahn.\u00ab", "tokens": ["Euch", "die\u00b7ser", "Pu\u00b7ter\u00b7hahn", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbmein Sohn, es ist die wahre,", "tokens": ["\u00bb", "mein", "Sohn", ",", "es", "ist", "die", "wah\u00b7re", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die fromme Furcht des Herrn,", "tokens": ["Die", "from\u00b7me", "Furcht", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die in der Nacht des Lebens", "tokens": ["Die", "in", "der", "Nacht", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ergl\u00e4nzt als heller Stern.\u00ab", "tokens": ["Er\u00b7gl\u00e4nzt", "als", "hel\u00b7ler", "Stern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "KOKOM", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbmein Vater, la\u00dft euch munden", "tokens": ["\u00bb", "mein", "Va\u00b7ter", ",", "la\u00dft", "euch", "mun\u00b7den"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Trank aus diesem Fa\u00df;", "tokens": ["Den", "Trank", "aus", "die\u00b7sem", "Fa\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Orvieto's Fluren quollen", "tokens": ["Or\u00b7vie\u00b7to's", "Flu\u00b7ren", "quol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Noch nie von s\u00fc\u00df'rem Na\u00df!\u00ab", "tokens": ["Noch", "nie", "von", "s\u00fc\u00df'\u00b7rem", "Na\u00df", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbmein Sohn, 's ist N\u00e4chstenliebe,", "tokens": ["\u00bb", "mein", "Sohn", ",", "'s", "ist", "N\u00e4chs\u00b7ten\u00b7lie\u00b7be", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die sch\u00f6n das Dasein kr\u00f6nt,", "tokens": ["Die", "sch\u00f6n", "das", "Da\u00b7sein", "kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gleichwie die Rebguirlande", "tokens": ["Gleich\u00b7wie", "die", "Reb\u00b7gu\u00b7ir\u00b7lan\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Dein Schollenfeld versch\u00f6nt.\u00ab", "tokens": ["Dein", "Schol\u00b7len\u00b7feld", "ver\u00b7sch\u00f6nt", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbmein Vater, Artischocken", "tokens": ["\u00bb", "mein", "Va\u00b7ter", ",", "Ar\u00b7ti\u00b7scho\u00b7cken"], "token_info": ["punct", "word", "word", "punct", "word"], "pos": ["$(", "PPOSAT", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Broccoli, wie die", "tokens": ["Und", "Broc\u00b7co\u00b7li", ",", "wie", "die"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "PWAV", "ART"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "In diesem Korb zu Schocken,", "tokens": ["In", "die\u00b7sem", "Korb", "zu", "Scho\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sch\u00f6ne saht ihr nie!\u00ab", "tokens": ["So", "sch\u00f6\u00b7ne", "saht", "ihr", "nie", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bbmein Sohn, es ist die Tugend", "tokens": ["\u00bb", "mein", "Sohn", ",", "es", "ist", "die", "Tu\u00b7gend"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Samen, den wir s\u00e4'n;", "tokens": ["Der", "Sa\u00b7men", ",", "den", "wir", "s\u00e4'n", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O mag das Herz der Jugend", "tokens": ["O", "mag", "das", "Herz", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll ihrer Saaten stehn!\u00ab", "tokens": ["Voll", "ih\u00b7rer", "Saa\u00b7ten", "stehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Auf led'gem Esel trabte", "tokens": ["Auf", "le\u00b7d'\u00b7gem", "E\u00b7sel", "trab\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Bauersmann davon,", "tokens": ["Der", "Bau\u00b7ers\u00b7mann", "da\u00b7von", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Weisheit Lehre labte", "tokens": ["Der", "Weis\u00b7heit", "Leh\u00b7re", "lab\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Alsbald den zarten Sohn.", "tokens": ["Als\u00b7bald", "den", "zar\u00b7ten", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Fast h\u00f6rt' er den schon klagen:", "tokens": ["Fast", "h\u00f6rt'", "er", "den", "schon", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbo arge, b\u00f6se Zeit!", "tokens": ["\u00bb", "o", "ar\u00b7ge", ",", "b\u00f6\u00b7se", "Zeit", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Tugend wird gesotten", "tokens": ["Die", "Tu\u00b7gend", "wird", "ge\u00b7sot\