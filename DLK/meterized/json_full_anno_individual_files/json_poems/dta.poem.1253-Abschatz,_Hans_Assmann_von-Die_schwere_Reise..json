{"dta.poem.1253": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die schwere Reise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Des Monden tunckel-bleiches Licht", "tokens": ["Des", "Mon\u00b7den", "tun\u00b7ckel\u00b7blei\u00b7ches", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weist sein ersterbend Angesicht", "tokens": ["Weist", "sein", "ers\u00b7ter\u00b7bend", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "VVPP", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Auff des gestirnten Himmels Auen.", "tokens": ["Auff", "des", "ge\u00b7stirn\u00b7ten", "Him\u00b7mels", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sehe bey der braunen Nacht", "tokens": ["Ich", "se\u00b7he", "bey", "der", "brau\u00b7nen", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der muntern Sternen treue Wacht/", "tokens": ["Der", "mun\u00b7tern", "Ster\u00b7nen", "treu\u00b7e", "Wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als Zeugen/ meine Schmertzen schauen.", "tokens": ["Als", "Zeu\u00b7gen", "/", "mei\u00b7ne", "Schmert\u00b7zen", "schau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du liegst/ mein Kind/ in stiller Ruh/", "tokens": ["Du", "liegst", "/", "mein", "Kind", "/", "in", "stil\u00b7ler", "Ruh", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPOSAT", "NN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schliest unbesorgt die Augen zu/", "tokens": ["Schliest", "un\u00b7be\u00b7sorgt", "die", "Au\u00b7gen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und speisest dich mit s\u00fcssen Tr\u00e4umen;", "tokens": ["Und", "spei\u00b7sest", "dich", "mit", "s\u00fcs\u00b7sen", "Tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mu\u00df/ wenn Mitternacht dahin/", "tokens": ["Ich", "mu\u00df", "/", "wenn", "Mit\u00b7ter\u00b7nacht", "da\u00b7hin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "KOUS", "NN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie m\u00fcd\u2019 an Leib und Geist ich bin/", "tokens": ["Wie", "m\u00fcd'", "an", "Leib", "und", "Geist", "ich", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "NN", "KON", "NN", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das harte Lager wieder r\u00e4umen.", "tokens": ["Das", "har\u00b7te", "La\u00b7ger", "wie\u00b7der", "r\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df/ wenn Regen/ Schnee und Wind/", "tokens": ["Ich", "mu\u00df", "/", "wenn", "Re\u00b7gen", "/", "Schnee", "und", "Wind", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "KOUS", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Sturm und Frost ergrimmet sind/", "tokens": ["Wenn", "Sturm", "und", "Frost", "er\u00b7grim\u00b7met", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Felsen/ Berg und W\u00e4ldern reisen/", "tokens": ["In", "Fel\u00b7sen", "/", "Berg", "und", "W\u00e4l\u00b7dern", "rei\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Mangel auch im Uberflu\u00df/", "tokens": ["Mit", "Man\u00b7gel", "auch", "im", "U\u00b7ber\u00b7flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Schweigen/ Seuffzen und Verdru\u00df", "tokens": ["Mit", "Schwei\u00b7gen", "/", "Seuff\u00b7zen", "und", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Kummervolles Hertze speisen.", "tokens": ["Mein", "Kum\u00b7mer\u00b7vol\u00b7les", "Hert\u00b7ze", "spei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch dieses gieng\u2019 als Zucker ein/", "tokens": ["Doch", "die\u00b7ses", "gieng'", "als", "Zu\u00b7cker", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "KOUS", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00f6nt ich/ mein Engel/ bey dir seyn", "tokens": ["K\u00f6nt", "ich", "/", "mein", "En\u00b7gel", "/", "bey", "dir", "seyn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "APPR", "PPER", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deiner Gegenwart gen\u00fcssen/", "tokens": ["Und", "dei\u00b7ner", "Ge\u00b7gen\u00b7wart", "ge\u00b7n\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn deiner hellen Sonnen Licht", "tokens": ["Wenn", "dei\u00b7ner", "hel\u00b7len", "Son\u00b7nen", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Himmelscheinend Angesicht", "tokens": ["Dein", "Him\u00b7mel\u00b7schei\u00b7nend", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Leit-Stern w\u00e4re meinen F\u00fcssen.", "tokens": ["Ein", "Leit\u00b7S\u00b7tern", "w\u00e4\u00b7re", "mei\u00b7nen", "F\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich wolte lustig dahin gehn/", "tokens": ["Ich", "wol\u00b7te", "lus\u00b7tig", "da\u00b7hin", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PAV", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Ph\u00f6bus pfleget auffzustehn", "tokens": ["Wo", "Ph\u00f6\u00b7bus", "pfle\u00b7get", "auff\u00b7zu\u00b7stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "VVFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wo er wieder geht zu Bette/", "tokens": ["Und", "wo", "er", "wie\u00b7der", "geht", "zu", "Bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo kalter L\u00e4nder lange Nacht", "tokens": ["Wo", "kal\u00b7ter", "L\u00e4n\u00b7der", "lan\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Tag von zweyen Stunden macht/", "tokens": ["Den", "Tag", "von", "zwe\u00b7yen", "Stun\u00b7den", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVFIN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ich dich zur Gefertin h\u00e4tte.", "tokens": ["Wenn", "ich", "dich", "zur", "Ge\u00b7fer\u00b7tin", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ach aber! Ach! ich such umsunst", "tokens": ["Ach", "a\u00b7ber", "!", "Ach", "!", "ich", "such", "um\u00b7sunst"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADV", "$.", "ITJ", "$.", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey Gl\u00fcck und Himmel solche Gunst/", "tokens": ["Bey", "Gl\u00fcck", "und", "Him\u00b7mel", "sol\u00b7che", "Gunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mir allein die Hoffnung lassen/", "tokens": ["Die", "mir", "al\u00b7lein", "die", "Hoff\u00b7nung", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mir vielleicht die Zeit verg\u00fcnnt/", "tokens": ["Da\u00df", "mir", "viel\u00b7leicht", "die", "Zeit", "ver\u00b7g\u00fcnnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dich wieder einmahl/ liebstes Kind/", "tokens": ["Dich", "wie\u00b7der", "ein\u00b7mahl", "/", "liebs\u00b7tes", "Kind", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit frohen Armen zu umfassen.", "tokens": ["Mit", "fro\u00b7hen", "Ar\u00b7men", "zu", "um\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Inmittelst soll Best\u00e4ndigkeit", "tokens": ["In\u00b7mit\u00b7telst", "soll", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In das Register grauer Zeit", "tokens": ["In", "das", "Re\u00b7gis\u00b7ter", "grau\u00b7er", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mit Stahl und Diamanten schreiben/", "tokens": ["Mit", "Stahl", "und", "Di\u00b7a\u00b7man\u00b7ten", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df dir/ Celinde/ s\u00fcsses Kind/", "tokens": ["Da\u00df", "dir", "/", "Ce\u00b7lin\u00b7de", "/", "s\u00fcs\u00b7ses", "Kind", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ihm die Augen offen sind/", "tokens": ["Weil", "ihm", "die", "Au\u00b7gen", "of\u00b7fen", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Silvander wird gewogen bleiben.", "tokens": ["Sil\u00b7van\u00b7der", "wird", "ge\u00b7wo\u00b7gen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}