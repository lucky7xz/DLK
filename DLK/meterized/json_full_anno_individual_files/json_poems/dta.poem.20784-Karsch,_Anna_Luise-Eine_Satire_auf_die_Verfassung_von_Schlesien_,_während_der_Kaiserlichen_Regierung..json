{"dta.poem.20784": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Eine Satire  \n auf die  \n  Verfassung von Schlesien ,  \n w\u00e4hrend der Kaiserlichen Regierung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Friedrichs gro\u00dfe Macht in Schlesien marschiret,               ", "tokens": ["Als", "Fried\u00b7richs", "gro\u00b7\u00dfe", "Macht", "in", "Schle\u00b7si\u00b7en", "mar\u00b7schi\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJA", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da bin ich gleichfalls mit als Volontair passiret:", "tokens": ["Da", "bin", "ich", "gleich\u00b7falls", "mit", "als", "Vo\u00b7lon\u00b7tair", "pas\u00b7si\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mich trieb der Vorwitz und die Neubegierde an,", "tokens": ["Mich", "trieb", "der", "Vor\u00b7witz", "und", "die", "Neu\u00b7be\u00b7gier\u00b7de", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So da\u00df ich meinen Weg ein wenig seitw\u00e4rts nahm.", "tokens": ["So", "da\u00df", "ich", "mei\u00b7nen", "Weg", "ein", "we\u00b7nig", "seit\u00b7w\u00e4rts", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN", "ART", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da ich mich von dem Marsch der Preussen abgetrennt,", "tokens": ["Da", "ich", "mich", "von", "dem", "Marsch", "der", "Preus\u00b7sen", "ab\u00b7ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kam ich vor eine Stadt, die man Schwibus benennt,", "tokens": ["Kam", "ich", "vor", "ei\u00b7ne", "Stadt", ",", "die", "man", "Schwi\u00b7bus", "be\u00b7nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und als ich im Begriff daselbst hineinzugehn,", "tokens": ["Und", "als", "ich", "im", "Be\u00b7griff", "da\u00b7selbst", "hin\u00b7ein\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sah ich ein Frauenbild bei einem Baume stehn.", "tokens": ["Sah", "ich", "ein", "Frau\u00b7en\u00b7bild", "bei", "ei\u00b7nem", "Bau\u00b7me", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie lie\u00df die Traurigkeit aus allen Mienen blicken,", "tokens": ["Sie", "lie\u00df", "die", "Trau\u00b7rig\u00b7keit", "aus", "al\u00b7len", "Mie\u00b7nen", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die H\u00e4nde waren ihr gebunden auf den R\u00fccken", "tokens": ["Die", "H\u00e4n\u00b7de", "wa\u00b7ren", "ihr", "ge\u00b7bun\u00b7den", "auf", "den", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Augen thr\u00e4nenvoll, die Haare ganz zerstrent,", "tokens": ["Die", "Au\u00b7gen", "thr\u00e4\u00b7nen\u00b7voll", ",", "die", "Haa\u00b7re", "ganz", "zer\u00b7strent", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und als ich n\u00e4her kam wars die Gerechtigkeit.", "tokens": ["Und", "als", "ich", "n\u00e4\u00b7her", "kam", "wars", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich fragte ganz best\u00fcrzt, was ist euch denn geschehen,", "tokens": ["Ich", "frag\u00b7te", "ganz", "be\u00b7st\u00fcrzt", ",", "was", "ist", "euch", "denn", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Madame, da\u00df man sie hier so betr\u00fcbt soll sehen;", "tokens": ["Ma\u00b7da\u00b7me", ",", "da\u00df", "man", "sie", "hier", "so", "be\u00b7tr\u00fcbt", "soll", "se\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "$,", "KOUS", "PIS", "PPER", "ADV", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wenns nach den Rechten ging, so sollet ihr ja schon", "tokens": ["Wenns", "nach", "den", "Rech\u00b7ten", "ging", ",", "so", "sol\u00b7let", "ihr", "ja", "schon"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Heut auf dem Rathhaus seyn und bei der Session.", "tokens": ["Heut", "auf", "dem", "Rath\u00b7haus", "seyn", "und", "bei", "der", "Ses\u00b7si\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAINF", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach, hub sie seufzend an, dem Himmel seys geklaget,", "tokens": ["Ach", ",", "hub", "sie", "seuf\u00b7zend", "an", ",", "dem", "Him\u00b7mel", "seys", "ge\u00b7kla\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Man hat mich schon vorl\u00e4ngst aus dieser Stadt verjaget,", "tokens": ["Man", "hat", "mich", "schon", "vor\u00b7l\u00e4ngst", "aus", "die\u00b7ser", "Stadt", "ver\u00b7ja\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da lebt ein jeder so wie es ihm selbst beliebt:", "tokens": ["Da", "lebt", "ein", "je\u00b7der", "so", "wie", "es", "ihm", "selbst", "be\u00b7liebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "ADV", "KOKOM", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Das ist es, was mir jetzt so Geist als Herz betr\u00fcbt.", "tokens": ["Das", "ist", "es", ",", "was", "mir", "jetzt", "so", "Geist", "als", "Herz", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PWS", "PPER", "ADV", "ADV", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Bem\u00fchet euch, mein Freund, ein wenig umzusehn,", "tokens": ["Be\u00b7m\u00fc\u00b7het", "euch", ",", "mein", "Freund", ",", "ein", "we\u00b7nig", "um\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ART", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da wird ein neues Haus vor jenem Thore stehen,", "tokens": ["Da", "wird", "ein", "neu\u00b7es", "Haus", "vor", "je\u00b7nem", "Tho\u00b7re", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da wohnt ein Herr vom Rath, ein Schalk in seiner", "tokens": ["Da", "wohnt", "ein", "Herr", "vom", "Rath", ",", "ein", "Schalk", "in", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Haut,", "tokens": ["Haut", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.25": {"text": "Der mit Particken hat dies H\u00e4uschen aufgebaut.", "tokens": ["Der", "mit", "Par\u00b7ti\u00b7cken", "hat", "dies", "H\u00e4usc\u00b7hen", "auf\u00b7ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VAFIN", "PDS", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Da geht der krumme Schalk, schaut wie er spekuliret,", "tokens": ["Da", "geht", "der", "krum\u00b7me", "Schalk", ",", "schaut", "wie", "er", "spe\u00b7ku\u00b7li\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Weil er Betrug und List in seinem Schilde f\u00fchret:", "tokens": ["Weil", "er", "Be\u00b7trug", "und", "List", "in", "sei\u00b7nem", "Schil\u00b7de", "f\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So sieht er unter sich nach Art der falschen Welt,", "tokens": ["So", "sieht", "er", "un\u00b7ter", "sich", "nach", "Art", "der", "fal\u00b7schen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Er sucht die Schl\u00fcssel zu der B\u00fcrger Gut und Geld.", "tokens": ["Er", "sucht", "die", "Schl\u00fcs\u00b7sel", "zu", "der", "B\u00fcr\u00b7ger", "Gut", "und", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Nun wollt ich euch noch mehr von gleicher Gattung", "tokens": ["Nun", "wollt", "ich", "euch", "noch", "mehr", "von", "glei\u00b7cher", "Gat\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "zeigen;", "tokens": ["zei\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Doch weil so Zeit als Ort mir itzt befiehlt zu schweigen,", "tokens": ["Doch", "weil", "so", "Zeit", "als", "Ort", "mir", "itzt", "be\u00b7fiehlt", "zu", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "KOUS", "NN", "PPER", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So sag ich nur noch dies: der Consul und der Rath,", "tokens": ["So", "sag", "ich", "nur", "noch", "dies", ":", "der", "Con\u00b7sul", "und", "der", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PDS", "$.", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Die stimmen \u00fcberein sowohl in Wort als That.", "tokens": ["Die", "stim\u00b7men", "\u00fc\u00b7be\u00b7re\u00b7in", "so\u00b7wohl", "in", "Wort", "als", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "APPR", "NN", "KOUS", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Der gro\u00dfe Carolus, der noch in Schriften lebet,", "tokens": ["Der", "gro\u00b7\u00dfe", "Ca\u00b7ro\u00b7lus", ",", "der", "noch", "in", "Schrif\u00b7ten", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und dessen theure Seel itzt bei der Gottheit schwebet,", "tokens": ["Und", "des\u00b7sen", "theu\u00b7re", "Seel", "itzt", "bei", "der", "Got\u00b7theit", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der gab aus G\u00fctigkeit der Invalidenschaar", "tokens": ["Der", "gab", "aus", "G\u00fc\u00b7tig\u00b7keit", "der", "In\u00b7va\u00b7li\u00b7den\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Gewisses Gnadengeld zur Unterhaltung dar:", "tokens": ["Ge\u00b7wis\u00b7ses", "Gna\u00b7den\u00b7geld", "zur", "Un\u00b7ter\u00b7hal\u00b7tung", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es theilt sich dieses Volk in unterschiedne St\u00e4dte,", "tokens": ["Es", "theilt", "sich", "die\u00b7ses", "Volk", "in", "un\u00b7ter\u00b7schied\u00b7ne", "St\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PDAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Das war nun eben recht vor unsre Herren R\u00e4the.", "tokens": ["Das", "war", "nun", "e\u00b7ben", "recht", "vor", "uns\u00b7re", "Her\u00b7ren", "R\u00e4\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sie delibrirten bald, und machten diesen Schlu\u00df,", "tokens": ["Sie", "de\u00b7li\u00b7brir\u00b7ten", "bald", ",", "und", "mach\u00b7ten", "die\u00b7sen", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Da\u00df man bei unsrer Stadt auch welche haben mu\u00df.", "tokens": ["Da\u00df", "man", "bei", "uns\u00b7rer", "Stadt", "auch", "wel\u00b7che", "ha\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "ADV", "PIS", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Indem sie dieses sagt, vergo\u00df sie bittre Thr\u00e4nen:", "tokens": ["In\u00b7dem", "sie", "die\u00b7ses", "sagt", ",", "ver\u00b7go\u00df", "sie", "bitt\u00b7re", "Thr\u00e4\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ach weh, o Grausamkeit, th\u00e4t sie an mich erw\u00e4hnen,", "tokens": ["Ach", "weh", ",", "o", "Grau\u00b7sam\u00b7keit", ",", "th\u00e4t", "sie", "an", "mich", "er\u00b7w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.45": {"text": "Man hat genommen mir die Wage, welcher Werth!", "tokens": ["Man", "hat", "ge\u00b7nom\u00b7men", "mir", "die", "Wa\u00b7ge", ",", "wel\u00b7cher", "Werth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "PPER", "ART", "NN", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Die H\u00e4nd gebunden mir, dazu geraubt das Schwerdt,", "tokens": ["Die", "H\u00e4nd", "ge\u00b7bun\u00b7den", "mir", ",", "da\u00b7zu", "ge\u00b7raubt", "das", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PPER", "$,", "PAV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die Gro\u00dfen legten an der B\u00fcrgerschaft viel Gaben,", "tokens": ["Die", "Gro\u00b