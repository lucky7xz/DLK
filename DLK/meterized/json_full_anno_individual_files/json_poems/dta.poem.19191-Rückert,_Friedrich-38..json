{"dta.poem.19191": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann wacher Sinne Krieg geschlichtet Gottesfriede,", "tokens": ["Wann", "wa\u00b7cher", "Sin\u00b7ne", "Krieg", "ge\u00b7schlich\u00b7tet", "Got\u00b7tes\u00b7frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aufgehoben hat des Daseyns Unterschiede,", "tokens": ["Und", "auf\u00b7ge\u00b7ho\u00b7ben", "hat", "des", "Da\u00b7seyns", "Un\u00b7ter\u00b7schie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo Inn- und Aeu\u00dfres ist in Einen Duft verschwommen,", "tokens": ["Wo", "In\u00b7n", "und", "A\u00b7e\u00b7u\u00df\u00b7res", "ist", "in", "Ei\u00b7nen", "Duft", "ver\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "TRUNC", "KON", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Besonderheitsgef\u00fchl ins All zur\u00fcckgenommen,", "tokens": ["Be\u00b7son\u00b7der\u00b7heits\u00b7ge\u00b7f\u00fchl", "ins", "All", "zu\u00b7r\u00fcck\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In solchem Schlaf, in den hinein kein Wachen bebt,", "tokens": ["In", "sol\u00b7chem", "Schlaf", ",", "in", "den", "hin\u00b7ein", "kein", "Wa\u00b7chen", "bebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "ART", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dessen Ruh' kein Traum verwirrte Bilder webt;", "tokens": ["In", "des\u00b7sen", "Ruh'", "kein", "Traum", "ver\u00b7wirr\u00b7te", "Bil\u00b7der", "webt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PIAT", "NN", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wann jeder Th\u00e4tigkeit Thorweg geschlossen steht,", "tokens": ["Wann", "je\u00b7der", "Th\u00e4\u00b7tig\u00b7keit", "Thor\u00b7weg", "ge\u00b7schlos\u00b7sen", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ungehemmt nur aus und ein der Athem geht;", "tokens": ["Und", "un\u00b7ge\u00b7hemmt", "nur", "aus", "und", "ein", "der", "A\u00b7them", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "PTKVZ", "KON", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Erloschen ist das Aug' und jedes Bild des Sch\u00f6nen,", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "das", "Aug'", "und", "je\u00b7des", "Bild", "des", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erloschen ist das Ohr mit allen hellen T\u00f6nen,", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "das", "Ohr", "mit", "al\u00b7len", "hel\u00b7len", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Erloschen Red' und Wort mit der Begriffe Samen,", "tokens": ["Er\u00b7lo\u00b7schen", "Red'", "und", "Wort", "mit", "der", "Be\u00b7grif\u00b7fe", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Zeichen aller Ding' und aller Wesen Namen;", "tokens": ["Den", "Zei\u00b7chen", "al\u00b7ler", "Ding'", "und", "al\u00b7ler", "We\u00b7sen", "Na\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Erloschen, ausgel\u00f6scht, das Denken der Gedanken,", "tokens": ["Er\u00b7lo\u00b7schen", ",", "aus\u00b7ge\u00b7l\u00f6scht", ",", "das", "Den\u00b7ken", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Wollens Wallungen und der Gef\u00fchle Schwanken;", "tokens": ["Des", "Wol\u00b7lens", "Wal\u00b7lun\u00b7gen", "und", "der", "Ge\u00b7f\u00fch\u00b7le", "Schwan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Und nur ein stilles Licht, gekl\u00e4rt von Glut und Rauch,", "tokens": ["Und", "nur", "ein", "stil\u00b7les", "Licht", ",", "ge\u00b7kl\u00e4rt", "von", "Glut", "und", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und von dem Leben nichts zur\u00fcckbleibt als der Hauch:", "tokens": ["Und", "von", "dem", "Le\u00b7ben", "nichts", "zu\u00b7r\u00fcck\u00b7bleibt", "als", "der", "Hauch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PIS", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der Hauch (sagt der Brahman), der Gottes Athem ist,", "tokens": ["Der", "Hauch", "(", "sagt", "der", "Brah\u00b7man", ")", ",", "der", "Got\u00b7tes", "A\u00b7them", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "ART", "NN", "$(", "$,", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Bezeugt, da\u00df du in Gott dann aufgenommen bist;", "tokens": ["Be\u00b7zeugt", ",", "da\u00df", "du", "in", "Gott", "dann", "auf\u00b7ge\u00b7nom\u00b7men", "bist", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "APPR", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Und wann du dann vom Schlaf erwachest sanft und k\u00fchl,", "tokens": ["Und", "wann", "du", "dann", "vom", "Schlaf", "er\u00b7wa\u00b7chest", "sanft", "und", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bezeugt dasselbe dir ein seligs Nachgef\u00fchl.", "tokens": ["Be\u00b7zeugt", "das\u00b7sel\u00b7be", "dir", "ein", "se\u00b7ligs", "Nach\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}