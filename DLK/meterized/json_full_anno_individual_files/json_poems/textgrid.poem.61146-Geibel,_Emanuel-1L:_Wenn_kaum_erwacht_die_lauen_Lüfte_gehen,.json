{"textgrid.poem.61146": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn kaum erwacht die lauen L\u00fcfte gehen,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn kaum erwacht die lauen L\u00fcfte gehen,", "tokens": ["Wenn", "kaum", "er\u00b7wacht", "die", "lau\u00b7en", "L\u00fcf\u00b7te", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da singt der Dichter schon von Maienwonnen;", "tokens": ["Da", "singt", "der", "Dich\u00b7ter", "schon", "von", "Mai\u00b7en\u00b7won\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er glaubt beim ersten blassen Strahl der Sonnen", "tokens": ["Er", "glaubt", "beim", "ers\u00b7ten", "blas\u00b7sen", "Strahl", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Welt im Glanz der Pfingsten schon zu sehen.", "tokens": ["Die", "Welt", "im", "Glanz", "der", "Pfings\u00b7ten", "schon", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So spricht er auch von Liebes-Lust und -Wehen,", "tokens": ["So", "spricht", "er", "auch", "von", "Lie\u00b7bes\u00b7Lust", "und", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn kaum ein fl\u00fcchtig L\u00e4cheln er gewonnen;", "tokens": ["Wenn", "kaum", "ein", "fl\u00fcch\u00b7tig", "L\u00e4\u00b7cheln", "er", "ge\u00b7won\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Bl\u00fcte, die zu knospen nur begonnen,", "tokens": ["Die", "Bl\u00fc\u00b7te", ",", "die", "zu", "knos\u00b7pen", "nur", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKZU", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sieht er in Pracht als volle Rose stehen.", "tokens": ["Sieht", "er", "in", "Pracht", "als", "vol\u00b7le", "Ro\u00b7se", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Darum, o Freund, verwundre dich mitnichten,", "tokens": ["Da\u00b7rum", ",", "o", "Freund", ",", "ver\u00b7wund\u00b7re", "dich", "mit\u00b7nich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df oft ein freudig Lied ihm jetzt beschieden,", "tokens": ["Da\u00df", "oft", "ein", "freu\u00b7dig", "Lied", "ihm", "jetzt", "be\u00b7schie\u00b7den", ","], "