{"textgrid.poem.54559": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr/ der durch hohen Ruhm den Glantz so vieler Ahnen", "tokens": ["Herr", "/", "der", "durch", "ho\u00b7hen", "Ruhm", "den", "Glantz", "so", "vie\u00b7ler", "Ah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "ART", "APPR", "ADJA", "NN", "ART", "NN", "ADV", "PIAT", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Weit \u00fcbertroffen hat/ dem selbst der grimme Neid", "tokens": ["Weit", "\u00fc\u00b7ber\u00b7trof\u00b7fen", "hat", "/", "dem", "selbst", "der", "grim\u00b7me", "Neid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VAFIN", "$(", "ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor/ da man Schwerdter wetzt/ und bey nun stiller Zeit", "tokens": ["Vor", "/", "da", "man", "Schwerd\u00b7ter", "wetzt", "/", "und", "bey", "nun", "stil\u00b7ler", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "$(", "KOUS", "PIS", "NN", "VVFIN", "$(", "KON", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Weg zu solcher Ehr/ die ewig bl\u00fcht/ mu\u00df bahnen/", "tokens": ["Den", "Weg", "zu", "sol\u00b7cher", "Ehr", "/", "die", "e\u00b7wig", "bl\u00fcht", "/", "mu\u00df", "bah\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$(