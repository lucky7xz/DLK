{"dta.poem.7760": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bedachtet ihr einmal, was die Unsterblichkeit,", "tokens": ["Be\u00b7dach\u00b7tet", "ihr", "ein\u00b7mal", ",", "was", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach der ihr trachtet, ist, ihr K\u00f6nige der Zeit!", "tokens": ["Nach", "der", "ihr", "trach\u00b7tet", ",", "ist", ",", "ihr", "K\u00f6\u00b7ni\u00b7ge", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "$,", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denkmale stiftet ihr, Bildwerke, Riesenmauern;", "tokens": ["Denk\u00b7ma\u00b7le", "stif\u00b7tet", "ihr", ",", "Bild\u00b7wer\u00b7ke", ",", "Rie\u00b7sen\u00b7mau\u00b7ern", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "NN", "$,", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Die Nachwelt staunt sie an, und dankt nicht den Erbauern.", "tokens": ["Die", "Nach\u00b7welt", "staunt", "sie", "an", ",", "und", "dankt", "nicht", "den", "Er\u00b7bau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wenn man fraget nach dem Namen, wird man sagen:", "tokens": ["Und", "wenn", "man", "fra\u00b7get", "nach", "dem", "Na\u00b7men", ",", "wird", "man", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "APPR", "ART", "NN", "$,", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hoch kam zu Ehren Stein und Erz in dessen Tagen.", "tokens": ["Hoch", "kam", "zu", "Eh\u00b7ren", "Stein", "und", "Erz", "in", "des\u00b7sen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "NN", "KON", "NN", "APPR", "PRELAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "War auch so wohlgefugt des Landes Lust und Gl\u00fcck,", "tokens": ["War", "auch", "so", "wohl\u00b7ge\u00b7fugt", "des", "Lan\u00b7des", "Lust", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Stein und Erz, so ganz aus einem Gu\u00df und St\u00fcck?", "tokens": ["Wie", "Stein", "und", "Erz", ",", "so", "ganz", "aus", "ei\u00b7nem", "Gu\u00df", "und", "St\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,", "ADV", "ADV", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er hat die Ewigkeit gesucht in Stein und Erzen,", "tokens": ["Er", "hat", "die", "E\u00b7wig\u00b7keit", "ge\u00b7sucht", "in", "Stein", "und", "Er\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nach dem Denkmal nicht gefragt in Menschenherzen.", "tokens": ["Und", "nach", "dem", "Denk\u00b7mal", "nicht", "ge\u00b7fragt", "in", "Men\u00b7schen\u00b7her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKNEG", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So sei auf ewig denn der Namen eingeschrieben", "tokens": ["So", "sei", "auf", "e\u00b7wig", "denn", "der", "Na\u00b7men", "ein\u00b7ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJD", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Stein und Erz, anstatt in Herzen welche lieben.", "tokens": ["In", "Stein", "und", "Erz", ",", "an\u00b7statt", "in", "Her\u00b7zen", "wel\u00b7che", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KOUI", "APPR", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}