{"textgrid.poem.45971": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Sonett dem 23. August 1815 geweiht", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh' t\u00e4glich ich mit seltnen Herzensgaben", "tokens": ["Seh'", "t\u00e4g\u00b7lich", "ich", "mit", "selt\u00b7nen", "Her\u00b7zens\u00b7ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mit der reichsten F\u00fclle der Empfindung", "tokens": ["Und", "mit", "der", "reichs\u00b7ten", "F\u00fcl\u00b7le", "der", "Emp\u00b7fin\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So hohe Geistesklarheit in Verbindung,", "tokens": ["So", "ho\u00b7he", "Geis\u00b7tes\u00b7klar\u00b7heit", "in", "Ver\u00b7bin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An Ihrem Umgang Geist und Herz zu laben,", "tokens": ["An", "Ih\u00b7rem", "Um\u00b7gang", "Geist", "und", "Herz", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nach Leiden, die oft schwer gedr\u00fcckt Sie haben,", "tokens": ["Nach", "Lei\u00b7den", ",", "die", "oft", "schwer", "ge\u00b7dr\u00fcckt", "Sie", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", 