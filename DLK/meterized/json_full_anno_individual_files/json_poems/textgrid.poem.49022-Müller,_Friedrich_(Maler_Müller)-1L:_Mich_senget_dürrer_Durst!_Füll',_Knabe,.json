{"textgrid.poem.49022": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mich senget d\u00fcrrer Durst! F\u00fcll', Knabe,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich senget d\u00fcrrer Durst! F\u00fcll', Knabe,", "tokens": ["Mich", "sen\u00b7get", "d\u00fcr\u00b7rer", "Durst", "!", "F\u00fcll'", ",", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$.", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den goldnen Becher hier.", "tokens": ["Den", "gold\u00b7nen", "Be\u00b7cher", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ha! Lieblich teilst du, Evan, deine Gabe;", "tokens": ["Ha", "!", "Lieb\u00b7lich", "teilst", "du", ",", "E\u00b7van", ",", "dei\u00b7ne", "Ga\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "NE", "VVFIN", "PPER", "$,", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie bist du Freudenvater mir!", "tokens": ["Wie", "bist", "du", "Freu\u00b7den\u00b7va\u00b7ter", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "F\u00fcll' wieder! Wonnequell! Geschenke", "tokens": ["F\u00fcll'", "wie\u00b7der", "!", "Won\u00b7ne\u00b7quell", "!", "Ge\u00b7schen\u00b7ke"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "ADV", "$.", "NN", "$.", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der G\u00f6tter! S\u00fc\u00dfer Wein!", "tokens": ["Der", "G\u00f6t\u00b7ter", "!", "S\u00fc\u00b7\u00dfer", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein jeder Tropfen, seliges Getr\u00e4nke", "tokens": ["Ein", "je\u00b7der", "Trop\u00b7fen", ",", "se\u00b7li\u00b7ges", "Ge\u00b7tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von dir, schlie\u00dft einen Himmel ein.", "tokens": ["Von", "dir", ",", "schlie\u00dft", "ei\u00b7nen", "Him\u00b7mel", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wo irr' ich? \u2013 Evan! \u2013 In Corycens Grotte", "tokens": ["Wo", "irr'", "ich", "?", "\u2013", "E\u00b7van", "!", "\u2013", "In", "Co\u00b7ry\u00b7cens", "Grot\u00b7te"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "$(", "NE", "$.", "$(", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umtanzen die Bacchiden mich.", "tokens": ["Um\u00b7tan\u00b7zen", "die", "Bac\u00b7chi\u00b7den", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begeistert, heilig, tauml' ich voll vom Gotte;", "tokens": ["Be\u00b7geis\u00b7tert", ",", "hei\u00b7lig", ",", "tauml'", "ich", "voll", "vom", "Got\u00b7te", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sch\u00f6ne Sonne h\u00fcpft um mich \u2013", "tokens": ["Die", "sch\u00f6\u00b7ne", "Son\u00b7ne", "h\u00fcpft", "um", "mich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00fcpft' fr\u00f6hlich auf, es fliehen meine Sinnen", "tokens": ["H\u00fcpft'", "fr\u00f6h\u00b7lich", "auf", ",", "es", "flie\u00b7hen", "mei\u00b7ne", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und meine Seele schwimmt in Glanz,", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "schwimmt", "in", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein str\u00e4ubend Haar durchbebt Glut der Bacchantinnen \u2013", "tokens": ["Mein", "str\u00e4u\u00b7bend", "Haar", "durch\u00b7bebt", "Glut", "der", "Bac\u00b7chan\u00b7tin\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "NN", "ART", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich seh', ich seh' dich Vater ganz,", "tokens": ["Ich", "seh'", ",", "ich", "seh'", "dich", "Va\u00b7ter", "ganz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie kindlich du im lichten Maientraume", "tokens": ["Wie", "kind\u00b7lich", "du", "im", "lich\u00b7ten", "Mai\u00b7ent\u00b7rau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einst unter goldnem Nymphenchor", "tokens": ["Einst", "un\u00b7ter", "gold\u00b7nem", "Nym\u00b7phen\u00b7chor"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gebunden lagst von Reben an dem Baume", "tokens": ["Ge\u00b7bun\u00b7den", "lagst", "von", "Re\u00b7ben", "an", "dem", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schnell die Traube wuchs hervor:", "tokens": ["Und", "schnell", "die", "Trau\u00b7be", "wuchs", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und Nysa lie\u00df in goldne Schalen tr\u00e4ufeln", "tokens": ["Und", "Ny\u00b7sa", "lie\u00df", "in", "gold\u00b7ne", "Scha\u00b7len", "tr\u00e4u\u00b7feln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der freudenschwangern Beere Saft,", "tokens": ["Der", "freu\u00b7den\u00b7schwan\u00b7gern", "Bee\u00b7re", "Saft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Voll Lust auf dich nun staunt und l\u00e4nger nicht will Zweifeln:", "tokens": ["Voll", "Lust", "auf", "dich", "nun", "staunt", "und", "l\u00e4n\u00b7ger", "nicht", "will", "Zwei\u00b7feln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPER", "ADV", "ADJD", "KON", "ADJD", "PTKNEG", "VMFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du seist ein Gott der Kraft.", "tokens": ["Du", "seist", "ein", "Gott", "der", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Geheiligt durch den Wein, der Aug' und Lippen", "tokens": ["Ge\u00b7hei\u00b7ligt", "durch", "den", "Wein", ",", "der", "Aug'", "und", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald angeflammt, sieht sie nun den Silen,", "tokens": ["Bald", "an\u00b7ge\u00b7flammt", ",", "sieht", "sie", "nun", "den", "Si\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zehntausend Thyrsustr\u00e4ger, hoch auf Wolkenklippen", "tokens": ["Zehn\u00b7tau\u00b7send", "Thyr\u00b7sus\u00b7tr\u00e4\u00b7ger", ",", "hoch", "auf", "Wol\u00b7ken\u00b7klip\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die G\u00f6tter um dich stehn.", "tokens": ["Die", "G\u00f6t\u00b7ter", "um", "dich", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Prophetisch dann, mit hingestorbnen Blicken", "tokens": ["Pro\u00b7phe\u00b7tisch", "dann", ",", "mit", "hin\u00b7ge\u00b7storb\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seelenvollem Haar,", "tokens": ["Und", "see\u00b7len\u00b7vol\u00b7lem", "Haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heult sie herab voll dithyrambischen Entz\u00fccken:", "tokens": ["Heult", "sie", "her\u00b7ab", "voll", "dit\u00b7hy\u00b7ram\u00b7bi\u00b7schen", "Ent\u00b7z\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O heilig! heilig! Bromius gebar,", "tokens": ["O", "hei\u00b7lig", "!", "hei\u00b7lig", "!", "Bro\u00b7mi\u00b7us", "ge\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "ADJD", "$.", "NE", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "O Evan! Stolzer Evan, Jacche!", "tokens": ["O", "E\u00b7van", "!", "Stol\u00b7zer", "E\u00b7van", ",", "Jac\u00b7che", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$.", "NN", "NE", "$,", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Zeus Umarmung, eingeh\u00fcllt", "tokens": ["Aus", "Zeus", "Um\u00b7ar\u00b7mung", ",", "ein\u00b7ge\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "NE", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom roten Blitz, an Dyrcens Quell dich, Bacche,", "tokens": ["Vom", "ro\u00b7ten", "Blitz", ",", "an", "Dyr\u00b7cens", "Quell", "dich", ",", "Bac\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NE", "NN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des m\u00e4cht'gen Vaters Ebenbild,", "tokens": ["Des", "m\u00e4cht'\u00b7gen", "Va\u00b7ters", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der goldnen Schlangen Tochter Semele! Die G\u00f6tter,", "tokens": ["Der", "gold\u00b7nen", "Schlan\u00b7gen", "Toch\u00b7ter", "Se\u00b7me\u00b7le", "!", "Die", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "NE", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie prunken vom Olymp den Tag,", "tokens": ["Sie", "prun\u00b7ken", "vom", "O\u00b7lymp", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Neunmal umleuchtet Zeus in einem Donnerwetter", "tokens": ["Neun\u00b7mal", "um\u00b7leuch\u00b7tet", "Zeus", "in", "ei\u00b7nem", "Don\u00b7ner\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Den Erdball, der in trunknem Schlummer lag.", "tokens": ["Den", "Erd\u00b7ball", ",", "der", "in", "trunk\u00b7nem", "Schlum\u00b7mer", "lag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Dem Jubel neigt die Erde ihre Ohren,", "tokens": ["Dem", "Ju\u00b7bel", "neigt", "die", "Er\u00b7de", "ih\u00b7re", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Sonne, Mond und Himmel singt", "tokens": ["Da\u00df", "Son\u00b7ne", ",", "Mond", "und", "Him\u00b7mel", "singt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom stolzen Knaben, der kaum neu geboren", "tokens": ["Vom", "stol\u00b7zen", "Kna\u00b7ben", ",", "der", "kaum", "neu", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon unter Rebenlauben springt.", "tokens": ["Schon", "un\u00b7ter", "Re\u00b7ben\u00b7lau\u00b7ben", "springt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Froh h\u00f6ren's die Gestirne, die da gl\u00e4nzen", "tokens": ["Froh", "h\u00f6\u00b7ren's", "die", "Ge\u00b7stir\u00b7ne", ",", "die", "da", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Himmelsmeer; da dreht", "tokens": ["Im", "Him\u00b7mels\u00b7meer", ";", "da", "dreht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In mystisch heilig labyrinth'schen T\u00e4nzen", "tokens": ["In", "mys\u00b7tisch", "hei\u00b7lig", "la\u00b7by\u00b7rinth'\u00b7schen", "T\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich jeder taumelnde Planet.", "tokens": ["Sich", "je\u00b7der", "tau\u00b7meln\u00b7de", "Pla\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Da taumeln W\u00e4lder, finstre Grotten h\u00fcpfen!", "tokens": ["Da", "tau\u00b7meln", "W\u00e4l\u00b7der", ",", "finst\u00b7re", "Grot\u00b7ten", "h\u00fcp\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Heil' dir! Heut k\u00fcsset dich die Lust,", "tokens": ["Heil'", "dir", "!", "Heut", "k\u00fcs\u00b7set", "dich", "die", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Welt, zum ersten Mal! Verj\u00fcng