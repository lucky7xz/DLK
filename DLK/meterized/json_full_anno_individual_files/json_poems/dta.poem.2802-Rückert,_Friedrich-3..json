{"dta.poem.2802": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am Ende, wann du nun dich an der Welt genung", "tokens": ["Am", "En\u00b7de", ",", "wann", "du", "nun", "dich", "an", "der", "Welt", "ge\u00b7nung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "ADV", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gefreut hast, freuet dich noch die Erinnerung;", "tokens": ["Ge\u00b7freut", "hast", ",", "freu\u00b7et", "dich", "noch", "die", "E\u00b7rin\u00b7ne\u00b7rung", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Noch die Erinnerung, wie du dich sonst gefreut,", "tokens": ["Noch", "die", "E\u00b7rin\u00b7ne\u00b7rung", ",", "wie", "du", "dich", "sonst", "ge\u00b7freut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann das die Welt dir bot, was sie noch immer beut;", "tokens": ["Wann", "das", "die", "Welt", "dir", "bot", ",", "was", "sie", "noch", "im\u00b7mer", "beut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ART", "NN", "PPER", "VVFIN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie du dich sonst gefreut, wann sich der Kranz erneut", "tokens": ["Wie", "du", "dich", "sonst", "ge\u00b7freut", ",", "wann", "sich", "der", "Kranz", "er\u00b7neut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVPP", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Fr\u00fchlings, wie sich heut sein voller Glanz erneut.", "tokens": ["Des", "Fr\u00fch\u00b7lings", ",", "wie", "sich", "heut", "sein", "vol\u00b7ler", "Glanz", "er\u00b7neut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PRF", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Freut dich nicht mehr der Kranz? Noch immer! doch nur ganz,", "tokens": ["Freut", "dich", "nicht", "mehr", "der", "Kranz", "?", "Noch", "im\u00b7mer", "!", "doch", "nur", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "$.", "ADV", "ADV", "$.", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du dazu denkst, wie dich sonst gefreut der Glanz.", "tokens": ["Wenn", "du", "da\u00b7zu", "denkst", ",", "wie", "dich", "sonst", "ge\u00b7freut", "der", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}