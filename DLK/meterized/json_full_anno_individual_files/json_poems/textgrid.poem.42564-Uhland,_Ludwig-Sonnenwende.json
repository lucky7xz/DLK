{"textgrid.poem.42564": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenwende", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun die Sonne soll vollenden", "tokens": ["Nun", "die", "Son\u00b7ne", "soll", "voll\u00b7en\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre l\u00e4ngste, sch\u00f6nste Bahn,", "tokens": ["Ih\u00b7re", "l\u00e4ngs\u00b7te", ",", "sch\u00f6ns\u00b7te", "Bahn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie z\u00f6gert, sich zu wenden", "tokens": ["Wie", "sie", "z\u00f6\u00b7gert", ",", "sich", "zu", "wen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PRF", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem stillen Ozean!", "tokens": ["Nach", "dem", "stil\u00b7len", "O\u00b7ze\u00b7an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihrer G\u00f6ttin Jugendneige", "tokens": ["Ih\u00b7rer", "G\u00f6t\u00b7tin", "Ju\u00b7gen\u00b7dnei\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchlt die ahnende Natur,", "tokens": ["F\u00fchlt", "die", "ah\u00b7nen\u00b7de", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "tro