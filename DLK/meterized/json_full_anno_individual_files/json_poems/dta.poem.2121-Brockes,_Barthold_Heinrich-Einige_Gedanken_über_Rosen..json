{"dta.poem.2121": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einige Gedanken \u00fcber Rosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der sch\u00f6nste Monat Julius, der, mit verschwende-", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Mo\u00b7nat", "Ju\u00b7lius", ",", "der", ",", "mit", "ver\u00b7schwen\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "$,", "PRELS", "$,", "APPR", "TRUNC"], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "rischer Hand,", "tokens": ["ri\u00b7scher", "Hand", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mit Blumen ungez\u00e4hlter Arten", "tokens": ["Mit", "Blu\u00b7men", "un\u00b7ge\u00b7z\u00e4hl\u00b7ter", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht unsre G\u00e4rten nur, auch Wiesen, Feld und Land,", "tokens": ["Nicht", "uns\u00b7re", "G\u00e4r\u00b7ten", "nur", ",", "auch", "Wie\u00b7sen", ",", "Feld", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "ADV", "$,", "ADV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bedeckt und schm\u00fcckt, bebr\u00e4mt, bemahlt und zierte,", "tokens": ["Be\u00b7deckt", "und", "schm\u00fcckt", ",", "be\u00b7br\u00e4mt", ",", "be\u00b7mahlt", "und", "zier\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Trat seine Herrschaft fr\u00f6lich an, belebet\u2019 alles, und re-", "tokens": ["Trat", "sei\u00b7ne", "Herr\u00b7schaft", "fr\u00f6\u00b7lich", "an", ",", "be\u00b7le\u00b7bet'", "al\u00b7les", ",", "und", "re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,", "VVFIN", "PIS", "$,", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "gierte:", "tokens": ["gier\u00b7te", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Als ich, nach sanftgenossner Ruh, des Morgens fr\u00fch in", "tokens": ["Als", "ich", ",", "nach", "sanft\u00b7ge\u00b7noss\u00b7ner", "Ruh", ",", "des", "Mor\u00b7gens", "fr\u00fch", "in"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADV", "ADJD", "APPR"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.9": {"text": "meinen Garten,", "tokens": ["mei\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Von seinem Glanz durchdrungen, trat. Der Sonnen", "tokens": ["Von", "sei\u00b7nem", "Glanz", "durch\u00b7drun\u00b7gen", ",", "trat", ".", "Der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Glanz war zwar versteckt,", "tokens": ["Glanz", "war", "zwar", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Bey einer sanften Stille war die Luft von klarem Duft", "tokens": ["Bey", "ei\u00b7ner", "sanf\u00b7ten", "Stil\u00b7le", "war", "die", "Luft", "von", "kla\u00b7rem", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "bedeckt;", "tokens": ["be\u00b7deckt", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Es hatte kurz vorher geregnet, der Kr\u00e4uter Heer dadurch", "tokens": ["Es", "hat\u00b7te", "kurz", "vor\u00b7her", "ge\u00b7reg\u00b7net", ",", "der", "Kr\u00e4u\u00b7ter", "Heer", "da\u00b7durch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVPP", "$,", "ART", "NN", "NN", "PAV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "getr\u00e4nket,", "tokens": ["ge\u00b7tr\u00e4n\u00b7ket", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Gest\u00e4rket und versch\u00f6nert, prangt in einem kr\u00e4ftig Dun-", "tokens": ["Ge\u00b7st\u00e4r\u00b7ket", "und", "ver\u00b7sch\u00f6\u00b7nert", ",", "prangt", "in", "ei\u00b7nem", "kr\u00e4f\u00b7tig", "Dun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "$,", "VVFIN", "APPR", "ART", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "kelgr\u00fcnen,", "tokens": ["kel\u00b7gr\u00fc\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Worauf noch hin und wieder Tropfen, wie klare Dia-", "tokens": ["Wo\u00b7rauf", "noch", "hin", "und", "wie\u00b7der", "Trop\u00b7fen", ",", "wie", "kla\u00b7re", "Dia"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "ADV", "PTKVZ", "KON", "ADV", "NN", "$,", "PWAV", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "manten, schienen.", "tokens": ["man\u00b7ten", ",", "schie\u00b7nen", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "In Blumen hatte sich zugleich des Himmels fruchtbar", "tokens": ["In", "Blu\u00b7men", "hat\u00b7te", "sich", "zu\u00b7gleich", "des", "Him\u00b7mels", "frucht\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PRF", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Na\u00df gesenkt,", "tokens": ["Na\u00df", "ge\u00b7senkt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Wodurch die Farben feuriger in ihren bunten Bl\u00e4ttern", "tokens": ["Wo\u00b7durch", "die", "Far\u00b7ben", "feu\u00b7ri\u00b7ger", "in", "ih\u00b7ren", "bun\u00b7ten", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.23": {"text": "gl\u00fchten", "tokens": ["gl\u00fch\u00b7ten"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Und diese sch\u00f6ne Kreaturen noch sch\u00f6ner als vorhero", "tokens": ["Und", "die\u00b7se", "sch\u00f6\u00b7ne", "Kre\u00b7a\u00b7tu\u00b7ren", "noch", "sch\u00f6\u00b7ner", "als", "vor\u00b7he\u00b7ro"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "ADV", "ADJD", "KOKOM", "ADV"], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.25": {"text": "bl\u00fchten.", "tokens": ["bl\u00fch\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Die lieblichgl\u00fchnden rothen Lichter der Rosen rissen son-", "tokens": ["Die", "lieb\u00b7lich\u00b7gl\u00fchn\u00b7den", "ro\u00b7then", "Lich\u00b7ter", "der", "Ro\u00b7sen", "ris\u00b7sen", "son"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "ART", "NN", "VVFIN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.27": {"text": "derlich", "tokens": ["der\u00b7lich"], "token_info": ["word"], "pos": ["ADV"], "meter": "--", "measure": "unknown.measure.zero"}, "line.28": {"text": "Auf den Schmaragden \u00e4hnlichen Geb\u00fcschen Blick und", "tokens": ["Auf", "den", "Schma\u00b7rag\u00b7den", "\u00e4hn\u00b7li\u00b7chen", "Ge\u00b7b\u00fc\u00b7schen", "Blick", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "NN", "KON"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Geist auf sich.", "tokens": ["Geist", "auf", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "$."], "meter": "+--", "measure": "dactylic.init"}}}}}