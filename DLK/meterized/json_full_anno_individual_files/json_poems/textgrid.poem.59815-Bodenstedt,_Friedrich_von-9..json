{"textgrid.poem.59815": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat einmal ein Tor gesagt,", "tokens": ["Es", "hat", "ein\u00b7mal", "ein", "Tor", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df der Mensch zum Leiden geboren worden;", "tokens": ["Da\u00df", "der", "Mensch", "zum", "Lei\u00b7den", "ge\u00b7bo\u00b7ren", "wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVPP", "VAPP", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Seitdem ist dies \u2013 Gott sei's geklagt! \u2013", "tokens": ["Seit\u00b7dem", "ist", "dies", "\u2013", "Gott", "sei's", "ge\u00b7klagt", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VAFIN", "PDS", "$(", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der Spruch aller gl\u00e4ubigen Toren worden.", "tokens": ["Der", "Spruch", "al\u00b7ler", "gl\u00e4u\u00b7bi\u00b7gen", "To\u00b7ren", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN", "VAPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Und weil die Menge aus Toren besteht,", "tokens": ["Und", "weil", "die", "Men\u00b7ge", "aus", "To\u00b7ren", "be\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos":