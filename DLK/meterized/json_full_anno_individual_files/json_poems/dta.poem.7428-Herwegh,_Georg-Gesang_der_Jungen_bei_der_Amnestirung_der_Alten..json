{"dta.poem.7428": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der Jungen bei der Amnestirung der Alten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Wogendonner vom fernen Meer,", "tokens": ["Wie", "Wo\u00b7gen\u00b7don\u00b7ner", "vom", "fer\u00b7nen", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie Wetter und Sturm im Lenze,", "tokens": ["Wie", "Wet\u00b7ter", "und", "Sturm", "im", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So brauset der Tag, der junge, daher,", "tokens": ["So", "brau\u00b7set", "der", "Tag", ",", "der", "jun\u00b7ge", ",", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "PAV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die alten Kerker, sie werden leer \u2014", "tokens": ["Und", "die", "al\u00b7ten", "Ker\u00b7ker", ",", "sie", "wer\u00b7den", "leer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADJD", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Kredenze, mein Liebchen, kredenze! \u2014", "tokens": ["Kre\u00b7den\u00b7ze", ",", "mein", "Lieb\u00b7chen", ",", "kre\u00b7den\u00b7ze", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$.", "$("], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Doch wei\u00df ich noch manch einen wackeren Mann,", "tokens": ["Doch", "wei\u00df", "ich", "noch", "manch", "ei\u00b7nen", "wa\u00b7cke\u00b7ren", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Der drein mit Ehren kommen kann.", "tokens": ["Der", "drein", "mit", "Eh\u00b7ren", "kom\u00b7men", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gott sch\u00fctze Dich, Liebchen!", "tokens": ["Gott", "sch\u00fct\u00b7ze", "Dich", ",", "Lieb\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr habt die Erl\u00f6sung so nahe gedacht,", "tokens": ["Ihr", "habt", "die", "Er\u00b7l\u00f6\u00b7sung", "so", "na\u00b7he", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ihr Br\u00fcder, ihr lustigen Zecher;", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "ihr", "lus\u00b7ti\u00b7gen", "Ze\u00b7cher", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ihr glaubtet zu fallen in blutiger Schlacht;", "tokens": ["Ihr", "glaub\u00b7tet", "zu", "fal\u00b7len", "in", "blu\u00b7ti\u00b7ger", "Schlacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "In den Kerkern wird uns Quartier gemacht \u2014", "tokens": ["In", "den", "Ker\u00b7kern", "wird", "uns", "Quar\u00b7tier", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "NN", "VVPP", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Den Becher, mein Liebchen, den Becher! \u2014", "tokens": ["Den", "Be\u00b7cher", ",", "mein", "Lieb\u00b7chen", ",", "den", "Be\u00b7cher", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die Alten heraus und die Jungen hinein!", "tokens": ["Die", "Al\u00b7ten", "he\u00b7raus", "und", "die", "Jun\u00b7gen", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wie sollte der Weltlauf anders sein?", "tokens": ["Wie", "soll\u00b7te", "der", "Welt\u00b7lauf", "an\u00b7ders", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Gott sch\u00fctze Dich, Liebchen!", "tokens": ["Gott", "sch\u00fct\u00b7ze", "Dich", ",", "Lieb\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Es gehet auf Erden wieder um", "tokens": ["Es", "ge\u00b7het", "auf", "Er\u00b7den", "wie\u00b7der", "um"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "APPR"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Teufel mit wildem Gebr\u00fclle;", "tokens": ["Der", "Teu\u00b7fel", "mit", "wil\u00b7dem", "Ge\u00b7br\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die deutsche Lippe bleibet nicht stumm,", "tokens": ["Die", "deut\u00b7sche", "Lip\u00b7pe", "blei\u00b7bet", "nicht", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Deutsche sch\u00fctzet sein Heiligthum \u2014", "tokens": ["Der", "Deut\u00b7sche", "sch\u00fct\u00b7zet", "sein", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "O f\u00fclle, mein Liebchen, o f\u00fclle! \u2014", "tokens": ["O", "f\u00fcl\u00b7le", ",", "mein", "Lieb\u00b7chen", ",", "o", "f\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "$,", "FM", "FM", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der Himmel will's und das Herz gebeut's:", "tokens": ["Der", "Him\u00b7mel", "will's", "und", "das", "Herz", "ge\u00b7beut's", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.7": {"text": "Wir sprechen wie M\u00e4nner und tragen das Kreuz.", "tokens": ["Wir", "spre\u00b7chen", "wie", "M\u00e4n\u00b7ner", "und", "tra\u00b7gen", "das", "Kreuz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Gott sch\u00fctze Dich, Liebchen!", "tokens": ["Gott", "sch\u00fct\u00b7ze", "Dich", ",", "Lieb\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Vom hohen Thurme schauet ein Aar \u2014", "tokens": ["Vom", "ho\u00b7hen", "Thur\u00b7me", "schau\u00b7et", "ein", "Aar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Denk' mein, Feinliebchen, o denke! \u2014", "tokens": ["Denk'", "mein", ",", "Fein\u00b7lieb\u00b7chen", ",", "o", "den\u00b7ke", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPOSAT", "$,", "NN", "$,", "FM", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort ruhet mein Arm, dort bleichet mein Haar;", "tokens": ["Dort", "ru\u00b7het", "mein", "Arm", ",", "dort", "blei\u00b7chet", "mein", "Haar", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch \u00fcber drei Tage und \u00fcber ein Jahr \u2014", "tokens": ["Doch", "\u00fc\u00b7ber", "drei", "Ta\u00b7ge", "und", "\u00fc\u00b7ber", "ein", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "KON", "APPR", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Schenk' ein, mein Liebchen, o schenke \u2014", "tokens": ["Schenk'", "ein", ",", "mein", "Lieb\u00b7chen", ",", "o", "schen\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "FM", "VVFIN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Da l\u00e4uten die V\u00f6lker zum heiligen Sturm,", "tokens": ["Da", "l\u00e4u\u00b7ten", "die", "V\u00f6l\u00b7ker", "zum", "hei\u00b7li\u00b7gen", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Wir leeren die Gl\u00e4ser, und steigen vom Thurm!", "tokens": ["Wir", "lee\u00b7ren", "die", "Gl\u00e4\u00b7ser", ",", "und", "stei\u00b7gen", "vom", "Thurm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Gott ", "tokens": ["Gott"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}}}}