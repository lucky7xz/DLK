{"dta.poem.20418": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Streit der schwartzen augen/ rothen  \n lippen/ und weissen br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn alles reden will/ wie k\u00f6nnen wir denn schweigen?", "tokens": ["Wenn", "al\u00b7les", "re\u00b7den", "will", "/", "wie", "k\u00f6n\u00b7nen", "wir", "denn", "schwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$(", "PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es will zwar nicht der schnee von unsern h\u00fcgeln gehn;", "tokens": ["Es", "will", "zwar", "nicht", "der", "schnee", "von", "un\u00b7sern", "h\u00fc\u00b7geln", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wollen flammen sich auch auff den spitzen zeigen/", "tokens": ["Doch", "wol\u00b7len", "flam\u00b7men", "sich", "auch", "auff", "den", "spit\u00b7zen", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "PRF", "ADV", "APPR", "ART", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die r\u00fcstig tag und nacht in vollem brande stehn.", "tokens": ["Die", "r\u00fcs\u00b7tig", "tag", "und", "nacht", "in", "vol\u00b7lem", "bran\u00b7de", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer einen leichten blick in diesen zirckel schicket/", "tokens": ["Wer", "ei\u00b7nen", "leich\u00b7ten", "blick", "in", "die\u00b7sen", "zir\u00b7ckel", "schi\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der wird alsbald bestrickt durch s\u00fcsse zauberey/", "tokens": ["Der", "wird", "als\u00b7bald", "be\u00b7strickt", "durch", "s\u00fcs\u00b7se", "zau\u00b7be\u00b7rey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das netze/ so mit lust den leichten geist bestricket/", "tokens": ["Das", "net\u00b7ze", "/", "so", "mit", "lust", "den", "leich\u00b7ten", "geist", "be\u00b7stri\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "ADV", "APPR", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Reist keine helden-hand und harter stahl entzwey.", "tokens": ["Reist", "kei\u00b7ne", "hel\u00b7den\u00b7hand", "und", "har\u00b7ter", "stahl", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}