{"dta.poem.5392": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Schau-Spiel der Natur.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die gr\u00fcn-bewachsnen H\u00fcgel schenen von unten \u00fcber-", "tokens": ["Die", "gr\u00fcn\u00b7be\u00b7wachs\u00b7nen", "H\u00fc\u00b7gel", "sche\u00b7nen", "von", "un\u00b7ten", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "w\u00e4rts zu steigen,", "tokens": ["w\u00e4rts", "zu", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Und, wenn wir auf denselben stehn, von oben abw\u00e4rts sich", "tokens": ["Und", ",", "wenn", "wir", "auf", "den\u00b7sel\u00b7ben", "stehn", ",", "von", "o\u00b7ben", "ab\u00b7w\u00e4rts", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "PDS", "VVINF", "$,", "APPR", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "zu neigen.", "tokens": ["zu", "nei\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Die \u00fcberall-bewachsnen Seiten, wenn sie kein Ackers-Mann", "tokens": ["Die", "\u00fc\u00b7ber\u00b7all\u00b7be\u00b7wachs\u00b7nen", "Sei\u00b7ten", ",", "wenn", "sie", "kein", "A\u00b7cker\u00b7sMann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "bebaut,", "tokens": ["be\u00b7baut", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Deckt Busch-und Strauch-und Kr\u00e4uter-Werck, insonder-", "tokens": ["Deckt", "Busch\u00b7und", "Strauch\u00b7\u00b7und", "Kr\u00e4u\u00b7ter\u00b7\u00b7Werck", ",", "in\u00b7son\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "NE", "NE", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "heit das Farren-Kraut.", "tokens": ["heit", "das", "Far\u00b7ren\u00b7Kraut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Doch ist das aller-angenehmste, wenn auf beb\u00fcschter H\u00fc-", "tokens": ["Doch", "ist", "das", "al\u00b7ler\u00b7an\u00b7ge\u00b7nehms\u00b7te", ",", "wenn", "auf", "be\u00b7b\u00fcschter", "H\u00fc"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "$,", "KOUS", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "gel H\u00f6h'n", "tokens": ["gel", "H\u00f6h'n"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Wir hoher Wipfel gr\u00fcne D\u00e4cher auf S\u00e4ulen-gleichen", "tokens": ["Wir", "ho\u00b7her", "Wip\u00b7fel", "gr\u00fc\u00b7ne", "D\u00e4\u00b7cher", "auf", "S\u00e4u\u00b7len\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "St\u00e4mmen sehn.", "tokens": ["St\u00e4m\u00b7men", "sehn", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Da theils, in steter Nachbarschaft, der B\u00fcchen-Eich- und", "tokens": ["Da", "theils", ",", "in", "ste\u00b7ter", "Nach\u00b7bar\u00b7schaft", ",", "der", "B\u00fc\u00b7chen\u00b7Eich", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "ART", "TRUNC", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Linden Schatten,", "tokens": ["Lin\u00b7den", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Die K\u00fchlung allgemein zu machen, gemeinschaftlich sich", "tokens": ["Die", "K\u00fch\u00b7lung", "all\u00b7ge\u00b7mein", "zu", "ma\u00b7chen", ",", "ge\u00b7mein\u00b7schaft\u00b7lich", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$,", "ADJD", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "gleichsam gatten,", "tokens": ["gleich\u00b7sam", "gat\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Theils, wenn sie mehr entfernet frehn, durch Schatten-", "tokens": ["Theils", ",", "wenn", "sie", "mehr", "ent\u00b7fer\u00b7net", "frehn", ",", "durch", "Schat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "VVINF", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Strich', im Gegensatz", "tokens": ["Strich'", ",", "im", "Ge\u00b7gen\u00b7satz"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Vom gelben Korn, vom gr\u00fcnen Klee, die feurig ange-", "tokens": ["Vom", "gel\u00b7ben", "Korn", ",", "vom", "gr\u00fc\u00b7nen", "Klee", ",", "die", "feu\u00b7rig", "an\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,", "PRELS", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "strahlten Stellen", "tokens": ["strahl\u00b7ten", "Stel\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Noch desto kr\u00e4ftiger erheben, den hellen Schmuck noch", "tokens": ["Noch", "des\u00b7to", "kr\u00e4f\u00b7ti\u00b7ger", "er\u00b7he\u00b7ben", ",", "den", "hel\u00b7len", "Schmuck", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "VVINF", "$,", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "mehr erhellen.", "tokens": ["mehr", "er\u00b7hel\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Hier sieht man, ausser sich vor Lust, manch nach der Schnur", "tokens": ["Hier", "sieht", "man", ",", "aus\u00b7ser", "sich", "vor", "Lust", ",", "manch", "nach", "der", "Schnur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "APPR", "PRF", "APPR", "NN", "$,", "PIAT", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "beflantzten Platz", "tokens": ["be\u00b7flantz\u00b7ten", "Platz"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Von hohen dunckel-gr\u00fcnen Eichen, als Scenen auf dem", "tokens": ["Von", "ho\u00b7hen", "dun\u00b7ckel\u00b7gr\u00fc\u00b7nen", "Ei\u00b7chen", ",", "als", "Sce\u00b7nen", "auf", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,", "KOUS", "NN", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Schau-Platz, stehen,", "tokens": ["Schau\u00b7Platz", ",", "ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Wor zwischen wei\u00df-und gelbe Felder dem Gold und Silber", "tokens": ["Wor", "zwi\u00b7schen", "wei\u00df\u00b7\u00b7und", "gel\u00b7be", "Fel\u00b7der", "dem", "Gold", "und", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "ADJA", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "\u00e4hnlich sehen.", "tokens": ["\u00e4hn\u00b7lich", "se\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}