{"dta.poem.508": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber die ungleichen Schwestern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mich wunderts Delia, wie's denn auch billig Wunder/", "tokens": ["Mich", "wun\u00b7derts", "De\u00b7lia", ",", "wie's", "denn", "auch", "bil\u00b7lig", "Wun\u00b7der", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "NE", "$,", "VVFIN", "ADV", "ADV", "ADJD", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df deine Schwester dir so gar entgegen ist/", "tokens": ["Da\u00df", "dei\u00b7ne", "Schwes\u00b7ter", "dir", "so", "gar", "ent\u00b7ge\u00b7gen", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADV", "PTKVZ", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie tr\u00e4gt ein ehrbahr Kleid/ und nehrt der Wollust-Zunder/", "tokens": ["Sie", "tr\u00e4gt", "ein", "ehr\u00b7bahr", "Kleid", "/", "und", "nehrt", "der", "Wol\u00b7lust\u00b7Zun\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$(", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du in den Kleidern geil/ doch nicht im Hertzen bist.", "tokens": ["Du", "in", "den", "Klei\u00b7dern", "geil", "/", "doch", "nicht", "im", "Hert\u00b7zen", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "$(", "ADV", "PTKNEG", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier ist das Hertze keusch/ dort aber nur das Kleid/", "tokens": ["Hier", "ist", "das", "Hert\u00b7ze", "keusch", "/", "dort", "a\u00b7ber", "nur", "das", "Kleid", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "VVFIN", "ADJD", "$(", "ADV", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch wegen Kleid und Brunst ihr beyde straffbahr seyd.", "tokens": ["Doch", "we\u00b7gen", "Kleid", "und", "Brunst", "ihr", "bey\u00b7de", "straff\u00b7bahr", "seyd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "PPER", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}