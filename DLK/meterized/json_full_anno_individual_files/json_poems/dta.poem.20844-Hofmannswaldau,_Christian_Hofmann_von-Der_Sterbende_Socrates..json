{"dta.poem.20844": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrwar der ersten Menschen Schaar/", "tokens": ["F\u00fcr\u00b7war", "der", "ers\u00b7ten", "Men\u00b7schen", "Schaar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verdienet Opfer und Altar/", "tokens": ["Ver\u00b7die\u00b7net", "Op\u00b7fer", "und", "Al\u00b7tar", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von wegen ihrer klugen Sinnen/", "tokens": ["Von", "we\u00b7gen", "ih\u00b7rer", "klu\u00b7gen", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dem sie stets darauf geschaut/", "tokens": ["In", "dem", "sie", "stets", "da\u00b7rauf", "ge\u00b7schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PAV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit die Nachwelt sey erbaut/", "tokens": ["Da\u00b7mit", "die", "Nach\u00b7welt", "sey", "er\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und durch sie m\u00f6ge wachsen k\u00f6nnen.", "tokens": ["Und", "durch", "sie", "m\u00f6\u00b7ge", "wach\u00b7sen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VMFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Durch sie ist uns nicht unbewust/", "tokens": ["Durch", "sie", "ist", "uns", "nicht", "un\u00b7be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PTKNEG", "ADJD", "$("], "meter": "+----+-+", "measure": "dactylic.init"}, "line.8": {"text": "Da\u00df unsern Geistern voller Lust/", "tokens": ["Da\u00df", "un\u00b7sern", "Geis\u00b7tern", "vol\u00b7ler", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und derer ungeschicktes Wesen", "tokens": ["Und", "de\u00b7rer", "un\u00b7ge\u00b7schick\u00b7tes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sich in dem Leben nicht verkl\u00e4rt/", "tokens": ["Sich", "in", "dem", "Le\u00b7ben", "nicht", "ver\u00b7kl\u00e4rt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Kein ander Kleinoth wird gewehrt/", "tokens": ["Kein", "an\u00b7der", "Klei\u00b7noth", "wird", "ge\u00b7wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Als da\u00df sie nimmermehr genesen.", "tokens": ["Als", "da\u00df", "sie", "nim\u00b7mer\u00b7mehr", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Das Grausen so die H\u00f6lle hat/", "tokens": ["Das", "Grau\u00b7sen", "so", "die", "H\u00f6l\u00b7le", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Nacht die alte L\u00e4gerstadt/", "tokens": ["Die", "Nacht", "die", "al\u00b7te", "L\u00e4\u00b7ger\u00b7stadt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In der die Marter-G\u00f6tter leben/", "tokens": ["In", "der", "die", "Mar\u00b7ter\u00b7G\u00f6t\u00b7ter", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der H\u00f6llenhund geschickt zur Qual/", "tokens": ["Der", "H\u00f6l\u00b7len\u00b7hund", "ge\u00b7schickt", "zur", "Qual", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das Feld/ da bleiche Leichen schweben/", "tokens": ["Das", "Feld", "/", "da", "blei\u00b7che", "Lei\u00b7chen", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ist ihn ein ewig Hospital.", "tokens": ["Ist", "ihn", "ein", "e\u00b7wig", "Hos\u00b7pi\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Da dann ein Tugendreicher Geist/", "tokens": ["Da", "dann", "ein", "Tu\u00b7gen\u00b7drei\u00b7cher", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der ware Kunst und Wei\u00dfheit liebet/", "tokens": ["Der", "wa\u00b7re", "Kunst", "und", "Wei\u00df\u00b7heit", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und so der Freyheit sich befleist/", "tokens": ["Und", "so", "der", "Frey\u00b7heit", "sich", "be\u00b7fleist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Weil ihn die Dienstbarkeit betr\u00fcbet/", "tokens": ["Weil", "ihn", "die", "Dienst\u00b7bar\u00b7keit", "be\u00b7tr\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Kan seine Noth mit seinem Leben schliessen/", "tokens": ["Kan", "sei\u00b7ne", "Noth", "mit", "sei\u00b7nem", "Le\u00b7ben", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und weil er sich im Himmel setzt/", "tokens": ["Und", "weil", "er", "sich", "im", "Him\u00b7mel", "setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "An statt da\u00df Cerberus auf ihn k\u00f6mmt zugerissen/", "tokens": ["An", "statt", "da\u00df", "Cer\u00b7be\u00b7rus", "auf", "ihn", "k\u00f6mmt", "zu\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "KOUS", "NE", "APPR", "PPER", "VVFIN", "VVPP", "$("], "meter": "---+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Von Freudenreicher Schaar der G\u00f6tter wird ergetzt.", "tokens": ["Von", "Freu\u00b7den\u00b7rei\u00b7cher", "Schaar", "der", "G\u00f6t\u00b7ter", "wird", "er\u00b7getzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}