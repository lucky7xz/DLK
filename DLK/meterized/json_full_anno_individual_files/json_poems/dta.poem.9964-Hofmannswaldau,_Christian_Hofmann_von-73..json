{"dta.poem.9964": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sterb offt und werde doch wieder allzeit neu gebohren/", "tokens": ["Ich", "sterb", "offt", "und", "wer\u00b7de", "doch", "wie\u00b7der", "all\u00b7zeit", "neu", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Eh ich leben kan/ mu\u00df ich durch verfaulung gehn verlohren.", "tokens": ["Eh", "ich", "le\u00b7ben", "kan", "/", "mu\u00df", "ich", "durch", "ver\u00b7fau\u00b7lung", "gehn", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$(", "VMFIN", "PPER", "APPR", "NN", "VVINF", "VVPP", "$."], "meter": "+----+-+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Dieser ort der mich erzeuget/ der begr\u00e4bet wieder mich/", "tokens": ["Die\u00b7ser", "ort", "der", "mich", "er\u00b7zeu\u00b7get", "/", "der", "be\u00b7gr\u00e4\u00b7bet", "wie\u00b7der", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "PPER", "VVFIN", "$(", "ART", "VVFIN", "ADV", "PPER", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "In dem krieg und in dem frieden bin ich allzeit nutzbarlich.", "tokens": ["In", "dem", "krieg", "und", "in", "dem", "frie\u00b7den", "bin", "ich", "all\u00b7zeit", "nutz\u00b7bar\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}}}}}