": "Wie Hans der Edle da.", "tokens": ["Wie", "Hans", "der", "Ed\u00b7le", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Doch seine vierzehn Enden nicht", "tokens": ["Doch", "sei\u00b7ne", "vier\u00b7zehn", "En\u00b7den", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "CARD", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seiner Farbe Glanz", "tokens": ["Und", "sei\u00b7ner", "Far\u00b7be", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Besinge unser Klaggedicht;", "tokens": ["Be\u00b7sin\u00b7ge", "un\u00b7ser", "Klag\u00b7ge\u00b7dicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weit edler war der Hans.", "tokens": ["Weit", "ed\u00b7ler", "war", "der", "Hans", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "VAFIN", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So liebt' kein Menschenphilosoph", "tokens": ["So", "liebt'", "kein", "Men\u00b7schen\u00b7phi\u00b7lo\u00b7soph"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie er, die Einsamkeit;", "tokens": ["Wie", "er", ",", "die", "Ein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum hat er das Ger\u00e4usch am Hof", "tokens": ["Drum", "hat", "er", "das", "Ge\u00b7r\u00e4usch", "am", "