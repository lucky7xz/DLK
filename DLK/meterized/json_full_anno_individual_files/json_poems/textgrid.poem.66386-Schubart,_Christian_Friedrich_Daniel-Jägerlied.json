 ["Be\u00b7lei\u00b7dig\u00b7te", "die", "Men\u00b7schen", "nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nahm Speis' aus ihrer Hand,", "tokens": ["Nahm", "Speis'", "aus", "ih\u00b7rer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und legt sich freundlich unter sie,", "tokens": ["Und", "legt", "sich", "freund\u00b7lich", "un\u00b7ter", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als h\u00e4tt' er auch Verstand.", "tokens": ["Als", "h\u00e4tt'", "er", "auch", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Doch immer dacht' er gro\u00df und frei,", "tokens": ["Doch", "im\u00b7mer", "dacht'", "er", "gro\u00df", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(wer frei denkt, denkt auch gro\u00df)", "tokens": ["(", "wer", "frei", "denkt", ",", "denkt", "auch", "gro\u00df", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADJD", "VVFIN", "$,", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und drohte man mit Sklaverei,", "tokens": ["Und", "droh\u00b7te", "man", "mit", "Skla\u00b7ve\u00b7rei", ","], "token_info": ["word", "word", "word", "word"