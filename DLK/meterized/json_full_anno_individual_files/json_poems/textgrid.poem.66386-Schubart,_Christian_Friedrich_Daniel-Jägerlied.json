{"textgrid.poem.66386": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4gerlied", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hans, Hans der edle Hirsch ist todt!", "tokens": ["Hans", ",", "Hans", "der", "ed\u00b7le", "Hirsch", "ist", "todt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Thierwelt klagt um ihn,", "tokens": ["Die", "Thier\u00b7welt", "klagt", "um", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wer ihm Trank und Speise bot,", "tokens": ["Und", "wer", "ihm", "Trank", "und", "Spei\u00b7se", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seufzt kl\u00e4glich: Hans ist hin.", "tokens": ["Seufzt", "kl\u00e4g\u00b7lich", ":", "Hans", "ist", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "NE", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der J\u00e4ger singt ein banges Lied,", "tokens": ["Der", "J\u00e4\u00b7ger", "singt", "ein", "ban\u00b7ges", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es horcht der Wald umher;", "tokens": ["Es", "horcht", "der", "Wald", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn Hans, der Stolz der Solitude,", "tokens": ["Denn", "Hans", ",", "der", "Stolz", "der", "So\u00b7li\u00b7tu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Karls Liebling ist nicht mehr.", "tokens": ["Karls", "Lieb\u00b7ling", "ist", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wei\u00df wie das Licht war unser Hans,", "tokens": ["Wei\u00df", "wie", "das", "Licht", "war", "un\u00b7ser", "Hans", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "VAFIN", "PPOSAT", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Bild aus Duft gewebt,", "tokens": ["Ein", "Bild", "aus", "Duft", "ge\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Versilbert von des Mondes Glanz,", "tokens": ["Ver\u00b7sil\u00b7bert", "von", "des", "Mon\u00b7des", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das auf dem H\u00fcgel schwebt,", "tokens": ["Das", "auf", "dem", "H\u00fc\u00b7gel", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie Ossian in stummer Nacht", "tokens": ["Wie", "Os\u00b7si\u00b7an", "in", "stum\u00b7mer", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Oft vor sich schimmern sah;", "tokens": ["Oft", "vor", "sich", "schim\u00b7mern", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Stand kaum in solcher Wunderpracht,", "tokens": ["Stand", "kaum", "in", "sol\u00b7cher", "Wun\u00b7der\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Hans der Edle da.", "tokens": ["Wie", "Hans", "der", "Ed\u00b7le", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch seine vierzehn Enden nicht", "tokens": ["Doch", "sei\u00b7ne", "vier\u00b7zehn", "En\u00b7den", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "CARD", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seiner Farbe Glanz", "tokens": ["Und", "sei\u00b7ner", "Far\u00b7be", "Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Besinge unser Klaggedicht;", "tokens": ["Be\u00b7sin\u00b7ge", "un\u00b7ser", "Klag\u00b7ge\u00b7dicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weit edler war der Hans.", "tokens": ["Weit", "ed\u00b7ler", "war", "der", "Hans", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "VAFIN", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So liebt' kein Menschenphilosoph", "tokens": ["So", "liebt'", "kein", "Men\u00b7schen\u00b7phi\u00b7lo\u00b7soph"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie er, die Einsamkeit;", "tokens": ["Wie", "er", ",", "die", "Ein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum hat er das Ger\u00e4usch am Hof", "tokens": ["Drum", "hat", "er", "das", "Ge\u00b7r\u00e4usch", "am", "Hof"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Hirschewelt gescheut.", "tokens": ["Der", "Hir\u00b7sche\u00b7welt", "ge\u00b7scheut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nur einen Freund hat er gew\u00e4hlt;", "tokens": ["Nur", "ei\u00b7nen", "Freund", "hat", "er", "ge\u00b7w\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hans dachte so dabei:", "tokens": ["Hans", "dach\u00b7te", "so", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer viele Hirschefreunde z\u00e4hlt,", "tokens": ["Wer", "vie\u00b7le", "Hir\u00b7sche\u00b7freun\u00b7de", "z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem ist oft keiner treu.", "tokens": ["Dem", "ist", "oft", "kei\u00b7ner", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch war er schamhaft, war so keusch", "tokens": ["Auch", "war", "er", "scham\u00b7haft", ",", "war", "so", "keusch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In seinem Lebenslauf,", "tokens": ["In", "sei\u00b7nem", "Le\u00b7bens\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ohne brausendes Ger\u00e4usch", "tokens": ["Und", "oh\u00b7ne", "brau\u00b7sen\u00b7des", "Ge\u00b7r\u00e4usch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sucht er die Hirschkuh auf.", "tokens": ["Sucht", "er", "die", "Hirschkuh", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Noch mehr liebt' Hans die Menschen all,", "tokens": ["Noch", "mehr", "liebt'", "Hans", "die", "Men\u00b7schen", "all", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NE", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War ihnen hold und treu,", "tokens": ["War", "ih\u00b7nen", "hold", "und", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und flog, gelockt durch Pulverknall,", "tokens": ["Und", "flog", ",", "ge\u00b7lockt", "durch", "Pul\u00b7ver\u00b7knall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Lichtesstrahl herbei.", "tokens": ["Wie", "Lich\u00b7tes\u00b7strahl", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Beleidigte die Menschen nie,", "tokens": ["Be\u00b7lei\u00b7dig\u00b7te", "die", "Men\u00b7schen", "nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nahm Speis' aus ihrer Hand,", "tokens": ["Nahm", "Speis'", "aus", "ih\u00b7rer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und legt sich freundlich unter sie,", "tokens": ["Und", "legt", "sich", "freund\u00b7lich", "un\u00b7ter", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als h\u00e4tt' er auch Verstand.", "tokens": ["Als", "h\u00e4tt'", "er", "auch", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch immer dacht' er gro\u00df und frei,", "tokens": ["Doch", "im\u00b7mer", "dacht'", "er", "gro\u00df", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(wer frei denkt, denkt auch gro\u00df)", "tokens": ["(", "wer", "frei", "denkt", ",", "denkt", "auch", "gro\u00df", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADJD", "VVFIN", "$,", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und drohte man mit Sklaverei,", "tokens": ["Und", "droh\u00b7te", "man", "mit", "Skla\u00b7ve\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ri\u00df er sich muthig los.", "tokens": ["Ri\u00df", "er", "sich", "mut\u00b7hig", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "So gern er fra\u00df, so zog er doch", "tokens": ["So", "gern", "er", "fra\u00df", ",", "so", "zog", "er", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des bittern Hungers Tod", "tokens": ["Des", "bit\u00b7tern", "Hun\u00b7gers", "Tod"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Weit vor dem niedern Sklavenjoch,", "tokens": ["Weit", "vor", "dem", "nie\u00b7dern", "Skla\u00b7ven\u00b7joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Womit man ihn bedroht.", "tokens": ["Wo\u00b7mit", "man", "ihn", "be\u00b7droht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und doch \u2013 auch edle Hirsche dr\u00fcckt", "tokens": ["Und", "doch", "\u2013", "auch", "ed\u00b7le", "Hir\u00b7sche", "dr\u00fcckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Last der Eitelkeit \u2013", "tokens": ["Die", "Last", "der", "Ei\u00b7tel\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat er zw\u00f6lf Lenze nur erblickt;", "tokens": ["Hat", "er", "zw\u00f6lf", "Len\u00b7ze", "nur", "er\u00b7blickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "CARD", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie kurz war seine Zeit!", "tokens": ["Wie", "kurz", "war", "sei\u00b7ne", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verendet hat das edle Thier,", "tokens": ["Ver\u00b7en\u00b7det", "hat", "das", "ed\u00b7le", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Fr\u00fchlingsl\u00fcftchen kam,", "tokens": ["Ein", "Fr\u00fch\u00b7lings\u00b7l\u00fcft\u00b7chen", "kam", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das unsern Hansen sanft von hier", "tokens": ["Das", "un\u00b7sern", "Han\u00b7sen", "sanft", "von", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ins Reich der Schatten nahm.", "tokens": ["Ins", "Reich", "der", "Schat\u00b7ten", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Auf einem Karren f\u00fchren ihn", "tokens": ["Auf", "ei\u00b7nem", "Kar\u00b7ren", "f\u00fch\u00b7ren", "ihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die J\u00e4ger nun zur Ruh',", "tokens": ["Die", "J\u00e4\u00b7ger", "nun", "zur", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und scharren mit betr\u00fcbtem Sinn", "tokens": ["Und", "schar\u00b7ren", "mit", "be\u00b7tr\u00fcb\u00b7tem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den todten Hansen zu.", "tokens": ["Den", "tod\u00b7ten", "Han\u00b7sen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die guten J\u00e4ger stehen