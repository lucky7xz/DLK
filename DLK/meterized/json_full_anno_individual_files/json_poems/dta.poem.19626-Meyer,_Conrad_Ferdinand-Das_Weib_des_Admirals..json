{"dta.poem.19626": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Weib des Admirals.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Auf mondenhellem Lager w\u00e4lzt ein Weib,", "tokens": ["Auf", "mon\u00b7den\u00b7hel\u00b7lem", "La\u00b7ger", "w\u00e4lzt", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Ein schlummerloses, sich: \u201eO banger Pf\u00fchl!", "tokens": ["Ein", "schlum\u00b7mer\u00b7lo\u00b7ses", ",", "sich", ":", "\u201e", "O", "ban\u00b7ger", "Pf\u00fchl", "!"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRF", "$.", "$(", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch du, mein sorgender Gemahl, du wachst!", "tokens": ["Auch", "du", ",", "mein", "sor\u00b7gen\u00b7der", "Ge\u00b7mahl", ",", "du", "wachst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer d\u00fcrfte schlafen? Horch, die Folter st\u00f6hnt ...", "tokens": ["Wer", "d\u00fcrf\u00b7te", "schla\u00b7fen", "?", "Horch", ",", "die", "Fol\u00b7ter", "st\u00f6hnt", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVINF", "$.", "NE", "$,", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erw\u00fcrgte modern ohne Leichentuch,", "tokens": ["Er\u00b7w\u00fcrg\u00b7te", "mo\u00b7dern", "oh\u00b7ne", "Lei\u00b7chen\u00b7tuch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sieh unser Linnen, Chatillon, wie fein!", "tokens": ["Sieh", "un\u00b7ser", "Lin\u00b7nen", ",", "Cha\u00b7til\u00b7lon", ",", "wie", "fein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "NE", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gen Himmel schreit der M\u00e4rtrer frommes Blut,", "tokens": ["Gen", "Him\u00b7mel", "schreit", "der", "M\u00e4r\u00b7trer", "from\u00b7mes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich schreie, Herr, in deinen Armen mit!", "tokens": ["Ich", "schrei\u00b7e", ",", "Herr", ",", "in", "dei\u00b7nen", "Ar\u00b7men", "mit", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mein Held, ich rede Zeugni\u00df gegen dich", "tokens": ["Mein", "Held", ",", "ich", "re\u00b7de", "Zeug\u00b7ni\u00df", "ge\u00b7gen", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "NN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Vor Gott, entrollest du dein Banner nicht!\u201c", "tokens": ["Vor", "Gott", ",", "ent\u00b7rol\u00b7lest", "du", "dein", "Ban\u00b7ner", "nicht", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Sie schweigt in d\u00fcstrer Glut. Er sinnt und sagt:", "tokens": ["Sie", "schweigt", "in", "d\u00fcst\u00b7rer", "Glut", ".", "Er", "sinnt", "und", "sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "\u201eerw\u00e4ge, Weib, die Schrecken die du w\u00e4hlst!", "tokens": ["\u201e", "er\u00b7w\u00e4\u00b7ge", ",", "Weib", ",", "die", "Schre\u00b7cken", "die", "du", "w\u00e4hlst", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Dies Haus in Rauch und Tr\u00fcmmern! Dies mein Haupt", "tokens": ["Dies", "Haus", "in", "Rauch", "und", "Tr\u00fcm\u00b7mern", "!", "Dies", "mein", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "APPR", "NN", "KON", "NN", "$.", "PDS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Verfehmt, dem Meuchelmord gezeigt \u2014 geraubt!", "tokens": ["Ver\u00b7fehmt", ",", "dem", "Meu\u00b7chel\u00b7mord", "ge\u00b7zeigt", "ge\u00b7raubt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVPP", "$(", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Entehrt dies Wappen von des Henkers Hand!", "tokens": ["Ent\u00b7ehrt", "dies", "Wap\u00b7pen", "von", "des", "Hen\u00b7kers", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Du mit den Knaben bettelnd auf der Flucht!", "tokens": ["Du", "mit", "den", "Kna\u00b7ben", "bet\u00b7telnd", "auf", "der", "Flucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "W\u00e4hlst du dir Solches? Nimm drei Tage Frist!\u201c", "tokens": ["W\u00e4hlst", "du", "dir", "Sol\u00b7ches", "?", "Nimm", "drei", "Ta\u00b7ge", "Frist", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PIS", "$.", "NE", "CARD", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "\u2014 \u201eDrei Tage Frist? Sie sind vorbei. Brich auf!\u201c", "tokens": ["\u201e", "Drei", "Ta\u00b7ge", "Frist", "?", "Sie", "sind", "vor\u00b7bei", ".", "Brich", "auf", "!", "\u201c"], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "CARD", "NN", "NN", "$.", "PPER", "VAFIN", "PTKVZ", "$.", "NE", "PTKVZ", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}