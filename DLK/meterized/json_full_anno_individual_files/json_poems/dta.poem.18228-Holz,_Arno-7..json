{"dta.poem.18228": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.57", "no:0.42"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Wie er so traurig sa\u00df und sann.", "tokens": ["Wie", "er", "so", "trau\u00b7rig", "sa\u00df", "und", "sann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es schaute ihn der Abend nur", "tokens": ["Es", "schau\u00b7te", "ihn", "der", "A\u00b7bend", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die ersterbende Natur.", "tokens": ["Und", "die", "ers\u00b7ter\u00b7ben\u00b7de", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Von Zeit zu Zeit fuhr aus dem Strauch", "tokens": ["Von", "Zeit", "zu", "Zeit", "fuhr", "aus", "dem", "Strauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm \u00fcbers Haar Novemberhauch,", "tokens": ["Ihm", "\u00fc\u00b7bers", "Haar", "No\u00b7vem\u00b7ber\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Blatt im Baum und Rohr im Ried", "tokens": ["Und", "Blatt", "im", "Baum", "und", "Rohr", "im", "Ried"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sang ihm ins Ohr ihr Schlummerlied.", "tokens": ["Sang", "ihm", "ins", "Ohr", "ihr", "Schlum\u00b7mer\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er aber lauschte statt auf sie", "tokens": ["Er", "a\u00b7ber", "lauschte", "statt", "auf", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und ihre s\u00fc\u00dfe Melodie", "tokens": ["Und", "ih\u00b7re", "s\u00fc\u00b7\u00dfe", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nur in sein eigen Herz hinein", "tokens": ["Nur", "in", "sein", "ei\u00b7gen", "Herz", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und war ganz mutterseelallein.", "tokens": ["Und", "war", "ganz", "mut\u00b7ter\u00b7see\u00b7lal\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da trat zu ihm die dunkle Nacht", "tokens": ["Da", "trat", "zu", "ihm", "die", "dunk\u00b7le", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und sprach es aus, was er gedacht.", "tokens": ["Und", "sprach", "es", "aus", ",", "was", "er", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das klang so ahnungsvoll bewegt,", "tokens": ["Das", "klang", "so", "ah\u00b7nungs\u00b7voll", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wie wenn im Lenz die Drossel schl\u00e4gt:", "tokens": ["Wie", "wenn", "im", "Lenz", "die", "Dros\u00b7sel", "schl\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201ehorchst du noch immer, Menschenkind,", "tokens": ["\u201e", "horchst", "du", "noch", "im\u00b7mer", ",", "Men\u00b7schen\u00b7kind", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie deine Wunde blutend rinnt?", "tokens": ["Wie", "dei\u00b7ne", "Wun\u00b7de", "blu\u00b7tend", "rinnt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und willst du nie nach Todeswehn", "tokens": ["Und", "willst", "du", "nie", "nach", "To\u00b7des\u00b7wehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu neuem Leben auferstehn?", "tokens": ["Zu", "neu\u00b7em", "Le\u00b7ben", "auf\u00b7er\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sieh, dunkel schweigt um dich die Flur,", "tokens": ["Sieh", ",", "dun\u00b7kel", "schweigt", "um", "dich", "die", "Flur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VVFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mit dem Tod ringt die Natur.", "tokens": ["Und", "mit", "dem", "Tod", "ringt", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Doch eh der Thau zum Zweiten f\u00e4llt,", "tokens": ["Doch", "eh", "der", "Thau", "zum", "Zwei\u00b7ten", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ergl\u00e4nzte abermals die Welt;", "tokens": ["Er\u00b7gl\u00e4nz\u00b7te", "a\u00b7ber\u00b7mals", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und schon nach wenig Monden hebt", "tokens": ["Und", "schon", "nach", "we\u00b7nig", "Mon\u00b7den", "hebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ihr Haupt die Erde neubelebt.", "tokens": ["Ihr", "Haupt", "die", "Er\u00b7de", "neu\u00b7be\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Darum bescheide du dich still", "tokens": ["Da\u00b7rum", "be\u00b7schei\u00b7de", "du", "dich", "still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und harre de\u00df, was kommen will.", "tokens": ["Und", "har\u00b7re", "de\u00df", ",", "was", "kom\u00b7men", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$,", "PWS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Denn deines Lebens ", "tokens": ["Denn", "dei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Ruht noch im Schoo\u00df der Ewigkeit.", "tokens": ["Ruht", "noch", "im", "Schoo\u00df", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und naht sich einstmals ihre Stund,", "tokens": ["Und", "naht", "sich", "einst\u00b7mals", "ih\u00b7re", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und k\u00fc\u00dft dich leise auf den Mund,", "tokens": ["Und", "k\u00fc\u00dft", "dich", "lei\u00b7se", "auf", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "O, dann kehrt auch ins Herz zur\u00fcck", "tokens": ["O", ",", "dann", "kehrt", "auch", "ins", "Herz", "zu\u00b7r\u00fcck"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Dir deiner Liebe todtes Gl\u00fcck!\u201c \u2014", "tokens": ["Dir", "dei\u00b7ner", "Lie\u00b7be", "tod\u00b7tes", "Gl\u00fcck", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "So sprach die Nacht und schwieg darauf", "tokens": ["So", "sprach", "die", "Nacht", "und", "schwieg", "da\u00b7rauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und schaute zu den Sternen auf.", "tokens": ["Und", "schau\u00b7te", "zu", "den", "Ster\u00b7nen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Er aber sah sie traurig an,", "tokens": ["Er", "a\u00b7ber", "sah", "sie", "trau\u00b7rig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Denn ach, er glaubte nicht daran.", "tokens": ["Denn", "ach", ",", "er", "glaub\u00b7te", "nicht", "da\u00b7ran", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Er glaubte nur in seiner Noth", "tokens": ["Er", "glaub\u00b7te", "nur", "in", "sei\u00b7ner", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "An seines Seelenlebens Tod.", "tokens": ["An", "sei\u00b7nes", "See\u00b7len\u00b7le\u00b7bens", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Da winkte sie mit wei\u00dfer Hand", "tokens": ["Da", "wink\u00b7te", "sie", "mit", "wei\u00b7\u00dfer", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ihm einen Gru\u00df noch und verschwand.", "tokens": ["Ihm", "ei\u00b7nen", "Gru\u00df", "noch", "und", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Nun war es wieder still um ihn;", "tokens": ["Nun", "war", "es", "wie\u00b7der", "still", "um", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Die wei\u00dfen Nebel sah ich ziehn,", "tokens": ["Die", "wei\u00b7\u00dfen", "Ne\u00b7bel", "sah", "ich", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und droben aus dem Wolkenflor", "tokens": ["Und", "dro\u00b7ben", "aus", "dem", "Wol\u00b7ken\u00b7flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Trat wunderbar der Mond hervor.", "tokens": ["Trat", "wun\u00b7der\u00b7bar", "der", "Mond", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er sandte golden Strahl auf Strahl", "tokens": ["Er", "sand\u00b7te", "gol\u00b7den", "Strahl", "auf", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herab auf Berg und Wald und Thal", "tokens": ["Her\u00b7ab", "auf", "Berg", "und", "Wald", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00f6ste sanft in seinem Lauf", "tokens": ["Und", "l\u00f6s\u00b7te", "sanft", "in", "sei\u00b7nem", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Fremdlings Weh in Wehmuth auf.", "tokens": ["Des", "Fremd\u00b7lings", "Weh", "in", "Weh\u00b7muth", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wie ich ihn nun weinen sah,", "tokens": ["Doch", "wie", "ich", "ihn", "nun", "wei\u00b7nen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da ging mir's in der Seele nah!", "tokens": ["Da", "ging", "mir's", "in", "der", "See\u00b7le", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich w\u00e4re gern mit Rath und That", "tokens": ["Ich", "w\u00e4\u00b7re", "gern", "mit", "Rath", "und", "That"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem weltverlornen Mann genaht", "tokens": ["Dem", "welt\u00b7ver\u00b7lor\u00b7nen", "Mann", "ge\u00b7naht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und h\u00e4tte gern mit ihm getheilt", "tokens": ["Und", "h\u00e4t\u00b7te", "gern", "mit", "ihm", "ge\u00b7theilt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und ihm das kranke Herz geheilt.", "tokens": ["Und", "ihm", "das", "kran\u00b7ke", "Herz", "ge\u00b7heilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch leider Gottes ging's nicht an,", "tokens": ["Doch", "lei\u00b7der", "Got\u00b7tes", "ging's", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Denn ich war selbst der arme Mann!", "tokens": ["Denn", "ich", "war", "selbst", "der", "ar\u00b7me", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}