{"dta.poem.14801": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das kalte Liebchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Er. Sieh! die Sterne schon erblassen.", "tokens": ["Er", ".", "Sieh", "!", "die", "Ster\u00b7ne", "schon", "er\u00b7blas\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "NE", "$.", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie. Schwerer Schlummer f\u00e4llt mich an. \u2014", "tokens": ["Sie", ".", "Schwe\u00b7rer", "Schlum\u00b7mer", "f\u00e4llt", "mich", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$.", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er. Nun, so will ich schnell Dich fassen!", "tokens": ["Er", ".", "Nun", ",", "so", "will", "ich", "schnell", "Dich", "fas\u00b7sen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "ADV", "$,", "ADV", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie. R\u00fchr' mich nicht so gl\u00fchend an.", "tokens": ["Sie", ".", "R\u00fchr'", "mich", "nicht", "so", "gl\u00fc\u00b7hend", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "NN", "PPER", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}