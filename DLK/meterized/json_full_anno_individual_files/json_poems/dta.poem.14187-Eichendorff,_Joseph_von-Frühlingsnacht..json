{"dta.poem.14187": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Jauchzen m\u00f6cht' ich, m\u00f6chte weinen,", "tokens": ["Jauch\u00b7zen", "m\u00f6cht'", "ich", ",", "m\u00f6ch\u00b7te", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mir's doch, als k\u00f6nnt's nicht sein!", "tokens": ["Ist", "mir's", "doch", ",", "als", "k\u00f6nnt's", "nicht", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "$,", "KOUS", "NE", "PTKNEG", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alte Wunder wieder scheinen", "tokens": ["Al\u00b7te", "Wun\u00b7der", "wie\u00b7der", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Mondesglanz herein.", "tokens": ["Mit", "dem", "Mon\u00b7des\u00b7glanz", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}