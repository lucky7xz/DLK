{"textgrid.poem.40131": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsgebet", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling, Wonnegebieter,", "tokens": ["Fr\u00fch\u00b7ling", ",", "Won\u00b7ne\u00b7ge\u00b7bie\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "sonnestarker, lauterster Gott der Erde,", "tokens": ["son\u00b7nes\u00b7tar\u00b7ker", ",", "lau\u00b7ters\u00b7ter", "Gott", "der", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "willst du endlich erscheinen,", "tokens": ["willst", "du", "end\u00b7lich", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "mir ", "tokens": ["mir"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Nach soviel St\u00fcrmen,", "tokens": ["Nach", "so\u00b7viel", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "soviel qu\u00e4lender Wetterwut,", "tokens": ["so\u00b7viel", "qu\u00e4\u00b7len\u00b7der", "Wet\u00b7ter\u00b7wut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "nach manchem ", "tokens": ["nach", "man\u00b7chem"], "token_info": ["word", "word"], "pos": ["APPR", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "voll kalten, stechenden Glanzes:", "tokens": ["voll", "kal\u00b7ten", ",", "ste\u00b7chen\u00b7den", "Glan\u00b7zes", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "willst du endlich ", "tokens": ["willst", "du", "end\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "mein Heiland? \u2013", "tokens": ["mein", "Hei\u00b7land", "?", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Ja! mir k\u00fcnden heilige Schauer:", "tokens": ["Ja", "!", "mir", "k\u00fcn\u00b7den", "hei\u00b7li\u00b7ge", "Schau\u00b7er", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "du auferstehest,", "tokens": ["du", "auf\u00b7er\u00b7ste\u00b7hest", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "den ich dunkel geahnt", "tokens": ["den", "ich", "dun\u00b7kel", "ge\u00b7ahnt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVPP"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "in den D\u00e4mmertagen der Kindheit", "tokens": ["in", "den", "D\u00e4m\u00b7mer\u00b7ta\u00b7gen", "der", "Kind\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "und den ich verloren, vergessen", "tokens": ["und", "den", "ich", "ver\u00b7lo\u00b7ren", ",", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "PPER", "VVPP", "$,", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "im selbst sich verg\u00f6tternden J\u00fcnglingsrausch ...", "tokens": ["im", "selbst", "sich", "ver\u00b7g\u00f6t\u00b7tern\u00b7den", "J\u00fcng\u00b7lings\u00b7rausch", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "PRF", "ADJA", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Oh, senke die Strahlen", "tokens": ["Oh", ",", "sen\u00b7ke", "die", "Strah\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Deines milden Himmelsauges", "tokens": ["Dei\u00b7nes", "mil\u00b7den", "Him\u00b7mel\u00b7sau\u00b7ges"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "s\u00e4nftigend, verkl\u00e4rend mir", "tokens": ["s\u00e4nf\u00b7ti\u00b7gend", ",", "ver\u00b7kl\u00e4\u00b7rend", "mir"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "VVPP", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "in die sehnsucht offne Seele", "tokens": ["in", "die", "sehn\u00b7sucht", "off\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O durchf\u00fclle mich ganz mit Deinem Odem,", "tokens": ["O", "durch\u00b7f\u00fcl\u00b7le", "mich", "ganz", "mit", "Dei\u00b7nem", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Fr\u00fchling, \u00e4ther entsprossener,", "tokens": ["Fr\u00fch\u00b7ling", ",", "\u00e4t\u00b7her", "ent\u00b7spros\u00b7se\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "ADJA", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.7": {"text": "Segen atmender, reiner Sonnensohn!", "tokens": ["Se\u00b7gen", "at\u00b7men\u00b7der", ",", "rei\u00b7ner", "Son\u00b7nen\u00b7sohn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Erf\u00fclle mich mit deiner ", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "mich", "mit", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "nicht der g\u00e4rend sch\u00e4umenden,", "tokens": ["nicht", "der", "g\u00e4\u00b7rend", "sch\u00e4u\u00b7men\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJD", "VVINF", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.10": {"text": "der ziellos wilden, taumelnden Lust", "tokens": ["der", "ziel\u00b7los", "wil\u00b7den", ",", "tau\u00b7meln\u00b7den", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "st\u00fcrmenden Knaben\u00fcbermutes:", "tokens": ["st\u00fcr\u00b7men\u00b7den", "Kna\u00b7ben\u00b7\u00fc\u00b7ber\u00b7mu\u00b7tes", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "mit Deiner ruhig quellenden,", "tokens": ["mit", "Dei\u00b7ner", "ru\u00b7hig", "quel\u00b7len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "still knospenden,", "tokens": ["still", "knos\u00b7pen\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "sicher schaffenden Freudigkeit", "tokens": ["si\u00b7cher", "schaf\u00b7fen\u00b7den", "Freu\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "erf\u00fclle mich, du Gl\u00fcckbeseelter! \u2013", "tokens": ["er\u00b7f\u00fcl\u00b7le", "mich", ",", "du", "Gl\u00fcck\u00b7be\u00b7seel\u00b7ter", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schon jauchze ich.", "tokens": ["Schon", "jauch\u00b7ze", "ich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ja! du erh\u00f6rst mein Gebet!", "tokens": ["Ja", "!", "du", "er\u00b7h\u00f6rst", "mein", "Ge\u00b7bet", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du bist ", "tokens": ["Du", "bist"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "bist, was in mir jubelt \u2013", "tokens": ["bist", ",", "was", "in", "mir", "ju\u00b7belt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "du erh\u00f6rtest mich schon", "tokens": ["du", "er\u00b7h\u00f6r\u00b7test", "mich", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Du, Du, Fr\u00fchling, wurdest", "tokens": ["Du", ",", "Du", ",", "Fr\u00fch\u00b7ling", ",", "wur\u00b7dest"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "$,", "PPER", "$,", "NN", "$,", "VAFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "mir in bangender Seele", "tokens": ["mir", "in", "ban\u00b7gen\u00b7der", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "heimlich ein anderer, neuer Fr\u00fchling:", "tokens": ["heim\u00b7lich", "ein", "an\u00b7de\u00b7rer", ",", "neu\u00b7er", "Fr\u00fch\u00b7ling", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Erster Fr\u00fchling,", "tokens": ["Ers\u00b7ter", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "einziger Fr\u00fchling,", "tokens": ["ein\u00b7zi\u00b7ger", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "bleibe! weile!", "tokens": ["blei\u00b7be", "!", "wei\u00b7le", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "verla\u00df mich nicht,", "tokens": ["ver\u00b7la\u00df", "mich", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "fl\u00fcchten gleich die Tage!", "tokens": ["fl\u00fcch\u00b7ten", "gleich", "die", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dann werden machtlos nahen", "tokens": ["Dann", "wer\u00b7den", "macht\u00b7los", "na\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "meinem geweihten Haupt", "tokens": ["mei\u00b7nem", "ge\u00b7weih\u00b7ten", "Haupt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "des Sommers sengende Sorgen", "tokens": ["des", "Som\u00b7mers", "sen\u00b7gen\u00b7de", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "und des Herbstes tr\u00fcber Mi\u00dfmut", "tokens": ["und", "des", "Herbs\u00b7tes", "tr\u00fc\u00b7ber", "Mi\u00df\u00b7mut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "und des Winters kalte Oede.", "tokens": ["und", "des", "Win\u00b7ters", "kal\u00b7te", "O\u00b7e\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Erster Fr\u00fchling,", "tokens": ["Ers\u00b7ter", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "einziger Fr\u00fchling,", "tokens": ["ein\u00b7zi\u00b7ger", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Du brachtest mir die Erl\u00f6sung:", "tokens": ["Du", "brach\u00b7test", "mir", "die", "Er\u00b7l\u00f6\u00b7sung", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "bringe mir auch das Himmelreich!", "tokens": ["brin\u00b7ge", "mir", "auch", "das", "Him\u00b7mel\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Fr\u00fchling, Wonnegebieter,", "tokens": ["Fr\u00fch\u00b7ling", ",", "Won\u00b7ne\u00b7ge\u00b7bie\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "sonnestarker, lauterster Gott der Erde,", "tokens": ["son\u00b7nes\u00b7tar\u00b7ker", ",", "lau\u00b7ters\u00b7ter", "Gott", "der", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "willst du endlich erscheinen,", "tokens": ["willst", "du", "end\u00b7lich", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "mir ", "tokens": ["mir"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Nach soviel St\u00fcrmen,", "tokens": ["Nach", "so\u00b7viel", "S