{"dta.poem.11801": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft, wie der alte Pfau lehrt fliegen seine Jungen?", "tokens": ["Wei\u00dft", ",", "wie", "der", "al\u00b7te", "Pfau", "lehrt", "flie\u00b7gen", "sei\u00b7ne", "Jun\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie er dem Vater auch sich selbst einst nachgeschwungen.", "tokens": ["Wie", "er", "dem", "Va\u00b7ter", "auch", "sich", "selbst", "einst", "nach\u00b7ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "PRF", "ADV", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Am Tage schreitet er mit Lust im gr\u00fcnen Raum,", "tokens": ["Am", "Ta\u00b7ge", "schrei\u00b7tet", "er", "mit", "Lust", "im", "gr\u00fc\u00b7nen", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Abend w\u00e4hlt er sich zur Rast den h\u00f6chsten Baum.", "tokens": ["Am", "A\u00b7bend", "w\u00e4hlt", "er", "sich", "zur", "Rast", "den", "h\u00f6chs\u00b7ten", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und weil den Jungen kann so hoher Flug nicht gl\u00fccken,", "tokens": ["Und", "weil", "den", "Jun\u00b7gen", "kann", "so", "ho\u00b7her", "Flug", "nicht", "gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VMFIN", "ADV", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So tr\u00e4gt er einzeln sie hinauf auf seinem R\u00fccken.", "tokens": ["So", "tr\u00e4gt", "er", "ein\u00b7zeln", "sie", "hin\u00b7auf", "auf", "sei\u00b7nem", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da ruhn sie nun die Nacht, bis sie der Morgen weckt,", "tokens": ["Da", "ruhn", "sie", "nun", "die", "Nacht", ",", "bis", "sie", "der", "Mor\u00b7gen", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da fliegt der Alte weg, die Jungen sehns erschreckt.", "tokens": ["Da", "fliegt", "der", "Al\u00b7te", "weg", ",", "die", "Jun\u00b7gen", "sehns", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er wandelt unten froh im Gr\u00fcnen hin und wieder;", "tokens": ["Er", "wan\u00b7delt", "un\u00b7ten", "froh", "im", "Gr\u00fc\u00b7nen", "hin", "und", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "PTKVZ", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er trug sie nur hinauf, und holt sie nicht hernieder.", "tokens": ["Er", "trug", "sie", "nur", "hin\u00b7auf", ",", "und", "holt", "sie", "nicht", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Er blicket nur hinauf, um sie herab zu locken,", "tokens": ["Er", "bli\u00b7cket", "nur", "hin\u00b7auf", ",", "um", "sie", "her\u00b7ab", "zu", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KOUI", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da wagen sie den Flug, und sind vor Lust erschrocken,", "tokens": ["Da", "wa\u00b7gen", "sie", "den", "Flug", ",", "und", "sind", "vor", "Lust", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VAFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Zu f\u00fchlen, da\u00df im Wind von selbst die Federn wallen,", "tokens": ["Zu", "f\u00fch\u00b7len", ",", "da\u00df", "im", "Wind", "von", "selbst", "die", "Fe\u00b7dern", "wal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "APPRART", "NN", "APPR", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df sie halb schon sind geflogen, halb gefallen.", "tokens": ["Und", "da\u00df", "sie", "halb", "schon", "sind", "ge\u00b7flo\u00b7gen", ",", "halb", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "VAFIN", "VVPP", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}