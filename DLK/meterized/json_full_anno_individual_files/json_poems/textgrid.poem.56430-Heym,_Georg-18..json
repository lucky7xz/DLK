{"textgrid.poem.56430": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schiffe schwanken vor der Wilden Sto\u00df.", "tokens": ["Die", "Schif\u00b7fe", "schwan\u00b7ken", "vor", "der", "Wil\u00b7den", "Sto\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Wasser tritt durch L\u00f6cher, die sie schlugen.", "tokens": ["Das", "Was\u00b7ser", "tritt", "durch", "L\u00f6\u00b7cher", ",", "die", "sie", "schlu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Schiffe krachen schon in allen Fugen.", "tokens": ["Die", "Schif\u00b7fe", "kra\u00b7chen", "schon", "in", "al\u00b7len", "Fu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die B\u00e4nke liegen ihren St\u00f6\u00dfen blo\u00df.", "tokens": ["Die", "B\u00e4n\u00b7ke", "lie\u00b7gen", "ih\u00b7ren", "St\u00f6\u00b7\u00dfen", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Ruder brechen sie wie trocknes Kraut.", "tokens": ["Die", "Ru\u00b7der", "bre\u00b7chen", "sie", "wie", "trock\u00b7nes", "Kraut", "."], "token_info"