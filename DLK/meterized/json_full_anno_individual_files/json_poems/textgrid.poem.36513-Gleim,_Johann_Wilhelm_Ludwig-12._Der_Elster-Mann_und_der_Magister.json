{"textgrid.poem.36513": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "12. Der Elster-Mann und der Magister", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und sah den Frager nicht,", "tokens": ["Und", "sah", "den", "Fra\u00b7ger", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und sah mich um, und sah ein narbigtes Gesicht,", "tokens": ["Und", "sah", "mich", "um", ",", "und", "sah", "ein", "nar\u00b7big\u00b7tes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An einem langen Mann, mit Mantel und mit Kragen,", "tokens": ["An", "ei\u00b7nem", "lan\u00b7gen", "Mann", ",", "mit", "Man\u00b7tel", "und", "mit", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und h\u00f6rt ihn sagen:", "tokens": ["Und", "h\u00f6rt", "ihn", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "\u00bbgenie, das ist die Wundereigenschaft", "tokens": ["\u00bb", "ge\u00b7nie", ",", "das", "ist", "die", "Wun\u00b7der\u00b7ei\u00b7gen\u00b7schaft"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "$,", "PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Seher Gottes, die die Kraft", "tokens": ["Der", "Se\u00b7her", "Got\u00b7tes", ",", "die", "die", "Kraft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu denken nicht empfangen haben,", "tokens": ["Zu", "den\u00b7ken", "nicht", "emp\u00b7fan\u00b7gen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie hinzulegen, sie an ihrem Wanderstab,", "tokens": ["Sie", "hin\u00b7zu\u00b7le\u00b7gen", ",", "sie", "an", "ih\u00b7rem", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie purpurrotes Band, zu tragen, und ins Grab", "tokens": ["Wie", "pur\u00b7pur\u00b7ro\u00b7tes", "Band", ",", "zu", "tra\u00b7gen", ",", "und", "ins", "Grab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie mit sich zu begraben!\u00ab", "tokens": ["Sie", "mit", "sich", "zu", "be\u00b7gra\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "PRF", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was ist Genie? erscholl noch siebenmal vom Dache.", "tokens": ["Was", "ist", "Ge\u00b7nie", "?", "e