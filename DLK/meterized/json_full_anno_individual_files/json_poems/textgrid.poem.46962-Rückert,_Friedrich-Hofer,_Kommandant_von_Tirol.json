{"textgrid.poem.46962": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hofer, Kommandant von Tirol", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Mantua von dem Walle", "tokens": ["Aus", "Man\u00b7tua", "von", "dem", "Wal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Komm' ich geschritten her,", "tokens": ["Komm'", "ich", "ge\u00b7schrit\u00b7ten", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo noch von meinem Falle", "tokens": ["Wo", "noch", "von", "mei\u00b7nem", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Fleck ist blutig sehr;", "tokens": ["Ein", "Fleck", "ist", "blu\u00b7tig", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Augen unverschlossen,", "tokens": ["Die", "Au\u00b7gen", "un\u00b7ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von der Franzosen Hand,", "tokens": ["Von", "der", "Fran\u00b7zo\u00b7sen", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ward ich allda erschossen,", "tokens": ["Ward", "ich", "all\u00b7da", "er\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich Tirols Kommandant.", "tokens": ["Ich", "Ti\u00b7rols", "Kom\u00b7man\u00b7dant", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im Jahre, da man setzte", "tokens": ["Im", "Jah\u00b7re", ",", "da", "man", "setz\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Insurrektion", "tokens": ["In", "In\u00b7sur\u00b7rek\u00b7ti\u00b7on"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tirol, das Schwerter wetzte", "tokens": ["Ti\u00b7rol", ",", "das", "Schwer\u00b7ter", "wetz\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr \u00d6streichs Kaiserthron,", "tokens": ["F\u00fcr", "\u00d6s\u00b7treichs", "Kai\u00b7ser\u00b7thron", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "War ich es, den erkannten", "tokens": ["War", "ich", "es", ",", "den", "er\u00b7kann\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "$,", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die H\u00e4upter der Partei", "tokens": ["Die", "H\u00e4up\u00b7ter", "der", "Par\u00b7tei"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Tirols Kommandanten,", "tokens": ["Als", "Ti\u00b7rols", "Kom\u00b7man\u00b7dan\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich's f\u00fcr \u00d6streich sei.", "tokens": ["Da\u00df", "ich's", "f\u00fcr", "\u00d6s\u00b7treich", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O \u00d6sterreich, ich habe", "tokens": ["O", "\u00d6s\u00b7ter\u00b7reich", ",", "ich", "ha\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Kommandantenschaft", "tokens": ["Die", "Kom\u00b7man\u00b7dan\u00b7ten\u00b7schaft"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bewahret bis zum Grabe", "tokens": ["Be\u00b7wah\u00b7ret", "bis", "zum", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr dich mit treuer Kraft;", "tokens": ["F\u00fcr", "dich", "mit", "treu\u00b7er", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es hat mich nicht verdrossen,", "tokens": ["Es", "hat", "mich", "nicht", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df als Verr\u00e4ter ich", "tokens": ["Da\u00df", "als", "Ver\u00b7r\u00e4\u00b7ter", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vom Feinde ward erschossen,", "tokens": ["Vom", "Fein\u00b7de", "ward", "er\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Weil ich es ward f\u00fcr dich.", "tokens": ["Weil", "ich", "es", "ward", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O \u00d6sterreich, ich habe", "tokens": ["O", "\u00d6s\u00b7ter\u00b7reich", ",", "ich", "ha\u00b7be"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Kommandantenschaft", "tokens": ["Die", "Kom\u00b7man\u00b7dan\u00b7ten\u00b7schaft"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bewahret auch im Grabe", "tokens": ["Be\u00b7wah\u00b7ret", "auch", "im", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr dich mit treuer Kraft;", "tokens": ["F\u00fcr", "dich", "mit", "treu\u00b7er", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mu\u00dft' auch mein Blut zerstieben", "tokens": ["Mu\u00dft'", "auch", "mein", "Blut", "zer\u00b7stie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf fremden Mauern wohl,", "tokens": ["Auf", "frem\u00b7den", "Mau\u00b7ern", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im Tod bin ich geblieben", "tokens": ["Im", "Tod", "bin", "ich", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kommandant von Tirol.", "tokens": ["Kom\u00b7man\u00b7dant", "von", "Ti\u00b7rol."], "token_info": ["word", "word", "abbreviation"], "pos": ["NN", "APPR", "NE"], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Ich hab' als treuer H\u00fcter,", "tokens": ["Ich", "hab'", "als", "treu\u00b7er", "H\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nachdem ich l\u00e4ngst erblich,", "tokens": ["Nach\u00b7dem", "ich", "l\u00e4ngst", "er\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beh\u00fctet die Gem\u00fcter,", "tokens": ["Be\u00b7h\u00fc\u00b7tet", "die", "Ge\u00b7m\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O \u00d6sterreich f\u00fcr dich.", "tokens": ["O", "\u00d6s\u00b7ter\u00b7reich", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als Geist bin ich geschritten", "tokens": ["Als", "Geist", "bin", "ich", "ge\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Stets dies mein Land hindurch,", "tokens": ["Stets", "dies", "mein", "Land", "hin\u00b7durch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PPOSAT", "NN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und habe unbestritten", "tokens": ["Und", "ha\u00b7be", "un\u00b7be\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bewahrt dir deine Burg.", "tokens": ["Be\u00b7wahrt", "dir", "dei\u00b7ne", "Burg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nun heut, da unser Hoffen", "tokens": ["Nun", "heut", ",", "da", "un\u00b7ser", "Hof\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gekommen ist zum Ziel,", "tokens": ["Ge\u00b7kom\u00b7men", "ist", "zum", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Tirol frei und offen", "tokens": ["Da\u00df", "Ti\u00b7rol", "frei", "und", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur\u00fcck an \u00d6streich fiel;", "tokens": ["Zu\u00b7r\u00fcck", "an", "\u00d6s\u00b7treich", "fiel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier von mir eingeh\u00e4ndigt", "tokens": ["Hier", "von", "mir", "ein\u00b7ge\u00b7h\u00e4n\u00b7digt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nimm hin das teure Pfand:", "tokens": ["Nimm", "hin", "das", "teu\u00b7re", "Pfand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Heut ist mein Amt geendigt", "tokens": ["Heut", "ist", "mein", "Amt", "ge\u00b7en\u00b7digt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als Tirols Kommandant.", "tokens": ["Als", "Ti\u00b7rols", "Kom\u00b7man\u00b7dant", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nimm hin dies Land der Treue,", "tokens": ["Nimm", "hin", "dies", "Land", "der", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PDS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das dein von Anfang war,", "tokens": ["Das", "dein", "von", "An\u00b7fang", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das dein jetzt ist aufs neue,", "tokens": ["Das", "dein", "jetzt", "ist", "aufs", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADV", "VAFIN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dein sei immerdar.", "tokens": ["Und", "dein"