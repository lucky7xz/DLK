{"dta.poem.13782": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00e4ger Abschied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "en:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was wir still gelobt im Wald,", "tokens": ["Was", "wir", "still", "ge\u00b7lobt", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollen's drau\u00dfen ehrlich halten,", "tokens": ["Wol\u00b7len's", "drau\u00b7\u00dfen", "ehr\u00b7lich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig bleiben treu die Alten:", "tokens": ["E\u00b7wig", "blei\u00b7ben", "treu", "die", "Al\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutsch Panier, das rauschend wallt,", "tokens": ["Deutsch", "Pa\u00b7nier", ",", "das", "rau\u00b7schend", "wallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebe wohl!", "tokens": ["Le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Schirm' dich Gott, du sch\u00f6ner Wald!", "tokens": ["Schirm'", "dich", "Gott", ",", "du", "sch\u00f6\u00b7ner", "Wald", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}