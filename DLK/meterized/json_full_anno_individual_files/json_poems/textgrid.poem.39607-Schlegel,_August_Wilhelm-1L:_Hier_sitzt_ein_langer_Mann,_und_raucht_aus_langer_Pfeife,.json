{"textgrid.poem.39607": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier sitzt ein langer Mann, und raucht aus langer Pfeife,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sitzt ein langer Mann, und raucht aus langer Pfeife,", "tokens": ["Hier", "sitzt", "ein", "lan\u00b7ger", "Mann", ",", "und", "raucht", "aus", "lan\u00b7ger", "Pfei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Dichter soll es sein, wie ich gewi\u00df erfuhr.", "tokens": ["Ein", "Dich\u00b7ter", "soll", "es", "sein", ",", "wie", "ich", "ge\u00b7wi\u00df", "er\u00b7fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Maler zeigte Sinn, wenn ich ihn recht begreife:", "tokens": ["Der", "Ma\u00b7ler", "zeig\u00b7te", "Sinn", ",", "wenn", "ich", "ihn", "recht", "be\u00b7grei\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Mann ist Nebending, die Pfeife Hauptfigur.", "tokens": ["Der", "Mann", "ist", "Ne\u00b7ben\u00b7ding", ",", "die", "Pfei\u00b7fe", "Haupt\u00b7fi\u00b7gur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Mann hat lebenslang an diesem Rohr gemolken,", "tokens": ["Der", "Mann", "hat", "le\u00b7bens\u00b7lang", "an", "die\u00b7sem", "Rohr", "ge\u00b7mol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er sog daraus hervor der Verse Nebelwolken.", "tokens": ["Er", "sog", "da\u00b7raus", "her\u00b7vor", "der", "Ver\u00b7se", "Ne\u00b7bel\u00b7wol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKVZ", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die lange Pfeife war ihm Muse, Kunst, Geschmack,", "tokens": ["Die", "lan\u00b7ge", "Pfei\u00b7fe", "war", "ihm", "Mu\u00b7se", ",", "Kunst", ",", "Ge\u00b7schmack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und alles riecht bei ihm nach ekelm Rauchtabak.", "tokens": ["Und", "al\u00b7les", "riecht", "bei", "ihm", "nach", "e\u00b7kelm", "Rauch\u00b7ta\u00b7bak", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hier sitzt ein langer Mann, und raucht aus langer Pfeife,", "tokens": ["Hier", "sitzt", "ein", "lan\u00b7ger", "Mann", ",", "und", "raucht", "aus"