{"textgrid.poem.38367": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ein Wahrheitslied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Gott der Herr geboren war,", "tokens": ["Als", "Gott", "der", "Herr", "ge\u00b7bo\u00b7ren", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da war es kalt,", "tokens": ["Da", "war", "es", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was sieht Maria am Wege stehn?", "tokens": ["Was", "sieht", "Ma\u00b7ria", "am", "We\u00b7ge", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Feigenbaum,", "tokens": ["Ein", "Fei\u00b7gen\u00b7baum", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Maria la\u00df du die Feigen noch stehn,", "tokens": ["Ma\u00b7ria", "la\u00df", "du", "die", "Fei\u00b7gen", "noch", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Wir haben noch drei\u00dfig Meilen zu gehn,", "tokens": ["Wir", "ha\u00b7ben", "noch", "drei\u00b7\u00dfig", "Mei\u00b7len", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Es wird uns sp\u00e4t.", "tokens": ["Es", "wird", "uns", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Und als Maria ins St\u00e4dtlein kam,", "tokens": ["Und", "als", "Ma\u00b7ria", "ins", "St\u00e4dt\u00b7lein", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor eine Th\u00fcr,", "tokens": ["Vor", "ei\u00b7ne", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da sprach sie zu dem B\u00e4uerlein,", "tokens": ["Da", "sprach", "sie", "zu", "dem", "B\u00e4u\u00b7er\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Behalt uns hier,", "tokens": ["Be\u00b7halt", "uns", "hier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wohl um das kleine Kindelein,", "tokens": ["Wohl", "um", "das", "klei\u00b7ne", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es m\u00f6cht dich warlich sonst gereun,", "tokens": ["Es", "m\u00f6cht", "dich", "war\u00b7lich", "sonst", "ge\u00b7reun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Nacht ist kalt.", "tokens": ["Die", "Nacht", "ist", "kalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Der Bauer sprach von Herzen ja,", "tokens": ["Der", "Bau\u00b7er", "sprach", "von", "Her\u00b7zen", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht in den Stall!", "tokens": ["Geht", "in", "den", "Stall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Als nun die halbe Mitternacht kam,", "tokens": ["Als", "nun", "die", "hal\u00b7be", "Mit\u00b7ter\u00b7nacht", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stand auf der Mann;", "tokens": ["Stand", "auf", "der", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wo seyd ihr dann, ihr armen Leut?", "tokens": ["Wo", "seyd", "ihr", "dann", ",", "ihr", "ar\u00b7men", "Leut", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ihr noch nicht erfroren seyd,", "tokens": ["Da\u00df", "ihr", "noch", "nicht", "er\u00b7fro\u00b7ren", "seyd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das wundert mich.", "tokens": ["Das", "wun\u00b7dert", "mich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der Bauer gieng da wieder ins Haus,", "tokens": ["Der", "Bau\u00b7er", "gieng", "da", "wie\u00b7der", "ins", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wohl aus der Scheuer,", "tokens": ["Wohl", "aus", "der", "Scheu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Steh auf mein Weib, mein liebes Weib,", "tokens": ["Steh", "auf", "mein", "Weib", ",", "mein", "lie\u00b7bes", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mach ein Feuer,", "tokens": ["Und", "mach", "ein", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Und mach ein gutes Feuerlein,", "tokens": ["Und", "mach", "ein", "gu\u00b7tes", "Feu\u00b7er\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df diese armen Leutelein", "tokens": ["Da\u00df", "die\u00b7se", "ar\u00b7men", "Leu\u00b7tel\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erw\u00e4rmen sich.", "tokens": ["Er\u00b7w\u00e4r\u00b7men", "sich", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und als Maria ins Haus hin kam,", "tokens": ["Und", "als", "Ma\u00b7ria", "ins", "Haus", "hin", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da war sie froh,", "tokens": ["Da", "war", "sie", "froh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Joseph, der war ein frommer Mann,", "tokens": ["Jo\u00b7se\u00b7ph", ",", "der", "war", "ein", "from\u00b7mer", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sein S\u00e4cklein holt;", "tokens": ["Sein", "S\u00e4ck\u00b7lein", "holt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Er nimmt heraus ein Kesselein,", "tokens": ["Er", "nimmt", "he\u00b7raus", "ein", "Kes\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Kind th\u00e4t ein bischen Schnee hinein,", "tokens": ["Das", "Kind", "th\u00e4t", "ein", "bi\u00b7schen", "Schnee", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und das sey Mehl.", "tokens": ["Und", "das", "sey", "Mehl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Es that ein wenig Eis hinein,", "tokens": ["Es", "that", "ein", "we\u00b7nig", "Eis", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und das sey Zucker,", "tokens": ["Und", "das", "sey", "Zu\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es that ein wenig Wasser drein,", "tokens": ["Es", "that", "ein", "we\u00b7nig", "Was\u00b7ser", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und das sey Milch;", "tokens": ["Und", "das", "sey", "Milch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sie hiengen den Kessel \u00fcbern Heerd,", "tokens": ["Sie", "hien\u00b7gen", "den", "Kes\u00b7sel", "\u00fc\u00b7bern", "Heerd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "An einen Hacken, ohn Beschwerd", "tokens": ["An", "ei\u00b7nen", "Ha\u00b7cken", ",", "ohn", "Be\u00b7schwerd"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das M\u00fc\u00dflein kocht.", "tokens": ["Das", "M\u00fc\u00df\u00b7lein", "kocht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ein L\u00f6ffel schnitzt der fromme Mann", "tokens": ["Ein", "L\u00f6f\u00b7fel", "schnitzt", "der", "from\u00b7me", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von einem Span,", "tokens": ["Von", "ei\u00b7nem", "Span", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der ward von lauter Helfenbein", "tokens": ["Der", "ward", "von", "lau\u00b7ter", "Hel\u00b7fen\u00b7bein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Diamant;", "tokens": ["Und", "Di\u00b7a\u00b7mant", ";"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Maria gab dem Kind den Brei,", "tokens": ["Ma\u00b7ria", "gab", "dem", "Kind", "den", "Brei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da sah man, da\u00df es Jesus sey,", "tokens": ["Da", "sah", "man", ",", "da\u00df", "es", "Je\u00b7sus", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "PPER", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Unter seinen Augen.", "tokens": ["Un\u00b7ter", "sei\u00b7nen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Als Gott der Herr geboren war,", "tokens": ["Als", "Gott", "der", "Herr", "ge\u00b7bo\u00b7ren", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da war es kalt,", "tokens": ["Da", "war", "es", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was sieht Maria am Wege stehn?", "tokens": ["Was", "sieht", "Ma\u00b7ria", "am", "We\u00b7ge", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Feigenbaum,", "tokens": ["Ein", "Fei\u00b7gen\u00b7baum", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Maria la\u00df du die Feigen noch stehn,", "tokens": ["Ma\u00b7ria", "la\u00df", "du", "die", "Fei\u00b7gen", "noch", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Wir haben noch drei\u00dfig Meilen zu gehn,", "tokens": ["Wir", "ha\u00b7ben", "noch", "drei\u00b7\u00dfig", "Mei\u00b7len", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "PT