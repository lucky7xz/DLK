{"dta.poem.9653": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Deutsche/ Se. Churf\u00fcrstl. Durchl.  \n zu Brandenburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Deutsche/ der durch seinen flei\u00df", "tokens": ["Der", "Deut\u00b7sche", "/", "der", "durch", "sei\u00b7nen", "flei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey den entbrandten krieges-flammen", "tokens": ["Bey", "den", "ent\u00b7brand\u00b7ten", "krie\u00b7ge\u00b7sflam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Das deutsche reich zu retten wei\u00df/", "tokens": ["Das", "deut\u00b7sche", "reich", "zu", "ret\u00b7ten", "wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und haupt und glieder h\u00e4lt zusammen/", "tokens": ["Und", "haupt", "und", "glie\u00b7der", "h\u00e4lt", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "L\u00e4\u00dft seine deutsche redligkeit", "tokens": ["L\u00e4\u00dft", "sei\u00b7ne", "deut\u00b7sche", "red\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In allen seinen thaten sp\u00fcren/", "tokens": ["In", "al\u00b7len", "sei\u00b7nen", "tha\u00b7ten", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und allerdings sein deutsches kleid", "tokens": ["Und", "al\u00b7ler\u00b7dings", "sein", "deut\u00b7sches", "kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mu\u00df uns der warheit \u00fcberf\u00fchren.", "tokens": ["Mu\u00df", "uns", "der", "war\u00b7heit", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}