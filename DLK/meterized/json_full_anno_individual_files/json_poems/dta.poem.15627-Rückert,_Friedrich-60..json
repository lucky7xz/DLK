{"dta.poem.15627": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am besten thust du, still Lehrmeinungen zu h\u00f6ren,", "tokens": ["Am", "bes\u00b7ten", "thust", "du", ",", "still", "Lehr\u00b7mei\u00b7nun\u00b7gen", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPER", "$,", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ohn' im Gedankengang den Meinenden zu st\u00f6ren.", "tokens": ["Ohn'", "im", "Ge\u00b7dan\u00b7ken\u00b7gang", "den", "Mei\u00b7nen\u00b7den", "zu", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Die innre Wahrheit macht dein Einwurf nur zunicht,", "tokens": ["Die", "inn\u00b7re", "Wahr\u00b7heit", "macht", "dein", "Ein\u00b7wurf", "nur", "zu\u00b7nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die jede Lehre hat und jegliches Gedicht.", "tokens": ["Die", "je\u00b7de", "Leh\u00b7re", "hat", "und", "jeg\u00b7li\u00b7ches", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die F\u00e4den hinderst du, lebendig sich zu schlingen,", "tokens": ["Die", "F\u00e4\u00b7den", "hin\u00b7derst", "du", ",", "le\u00b7ben\u00b7dig", "sich", "zu", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zusammenhangendes Geweb hervorzubringen.", "tokens": ["Zu\u00b7sam\u00b7men\u00b7han\u00b7gen\u00b7des", "Ge\u00b7web", "her\u00b7vor\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch bildender f\u00fcr dich, als an sich selbst die Meinung,", "tokens": ["Doch", "bil\u00b7den\u00b7der", "f\u00fcr", "dich", ",", "als", "an", "sich", "selbst", "die", "Mei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "$,", "KOUS", "APPR", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist des Zusammenhangs erfreuliche Erscheinung.", "tokens": ["Ist", "des", "Zu\u00b7sam\u00b7men\u00b7hangs", "er\u00b7freu\u00b7li\u00b7che", "Er\u00b7schei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}