{"textgrid.poem.47509": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wehe mir, mein Rosenkr\u00e4nzlein]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wehe mir, mein Rosenkr\u00e4nzlein,", "tokens": ["We\u00b7he", "mir", ",", "mein", "Ro\u00b7sen\u00b7kr\u00e4nz\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Weh, es ist entzwei gesprungen,", "tokens": ["Weh", ",", "es", "ist", "ent\u00b7zwei", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn zu hei\u00df um deine H\u00fcfte", "tokens": ["Denn", "zu", "hei\u00df", "um", "dei\u00b7ne", "H\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKA", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hatte sich mein Arm geschlungen.", "tokens": ["Hat\u00b7te", "sich", "mein", "Arm", "ge\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, wie soll ich zelleneinsam", "tokens": ["Ach", ",", "wie", "soll", "ich", "zel\u00b7len\u00b7ein\u00b7sam"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VMFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4ppische Gebete murmeln,", "tokens": ["L\u00e4p\u00b7pi\u00b7sche", "Ge\