{"dta.poem.1679": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die verkleidete Sch\u00e4ferinnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Hier schauet uns die Nimphen von dem Lande/", "tokens": ["Hier", "schau\u00b7et", "uns", "die", "Nim\u00b7phen", "von", "dem", "Lan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die wohl vergn\u00fcgt mit ihrem freyen Stande/", "tokens": ["Die", "wohl", "ver\u00b7gn\u00fcgt", "mit", "ih\u00b7rem", "frey\u00b7en", "Stan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch schlaue List und falsche Pracht", "tokens": ["Durch", "schlau\u00b7e", "List", "und", "fal\u00b7sche", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Niemand zu fangen seyn bedacht.", "tokens": ["Nie\u00b7mand", "zu", "fan\u00b7gen", "seyn", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein stoltzes Band mu\u00df unser Haubt beziehn/", "tokens": ["Kein", "stolt\u00b7zes", "Band", "mu\u00df", "un\u00b7ser", "Haubt", "be\u00b7ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kein theurer Staub ber\u00fchret unser Haar/", "tokens": ["Kein", "theu\u00b7rer", "Staub", "be\u00b7r\u00fch\u00b7ret", "un\u00b7ser", "Haar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gold/ Liljen/ Rosen die ihr nehmet wahr/", "tokens": ["Gold", "/", "Lil\u00b7jen", "/", "Ro\u00b7sen", "die", "ihr", "neh\u00b7met", "wahr", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "NN", "ART", "PPER", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Pflanzt die Natur mit eigner Hand dahin:", "tokens": ["Pflanzt", "die", "Na\u00b7tur", "mit", "eig\u00b7ner", "Hand", "da\u00b7hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der M\u00e4yen-Thau/ der reine Bronnen/", "tokens": ["Der", "M\u00e4yen\u00b7\u00b7T\u00b7hau", "/", "der", "rei\u00b7ne", "Bron\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Flutt der Spiegel-hellen Bach", "tokens": ["Die", "Flutt", "der", "Spie\u00b7gel\u00b7hel\u00b7len", "Bach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mu\u00df unsrer frischen Wangen Anstrich seyn.", "tokens": ["Mu\u00df", "uns\u00b7rer", "fri\u00b7schen", "Wan\u00b7gen", "An\u00b7strich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Weil fauler Schlaff verdunckelt andre Sonnen", "tokens": ["Weil", "fau\u00b7ler", "Schlaff", "ver\u00b7dun\u00b7ckelt", "and\u00b7re", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VVPP", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die ihrer Wollust hengen nach/", "tokens": ["Die", "ih\u00b7rer", "Wol\u00b7lust", "hen\u00b7gen", "nach", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und sch\u00f6ner Wangen Purpur bleicht/", "tokens": ["Und", "sch\u00f6\u00b7ner", "Wan\u00b7gen", "Pur\u00b7pur", "bleicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Besch\u00e4men wir durch unsern Schein", "tokens": ["Be\u00b7sch\u00e4\u00b7men", "wir", "durch", "un\u00b7sern", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Morgenr\u00f6the fr\u00fches Licht/", "tokens": ["Der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "fr\u00fc\u00b7hes", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df sich entf\u00e4rbt der Erd entbricht:", "tokens": ["Da\u00df", "sich", "ent\u00b7f\u00e4rbt", "der", "Erd", "ent\u00b7bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dem klaren Antlitz gleicht die Reinigkeit der Sinnen.", "tokens": ["Dem", "kla\u00b7ren", "Ant\u00b7litz", "gleicht", "die", "Rei\u00b7nig\u00b7keit", "der", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Kein tr\u00fcglich Wort/ kein abgestohlner Blick/", "tokens": ["Kein", "tr\u00fcg\u00b7lich", "Wort", "/", "kein", "ab\u00b7ge\u00b7stohl\u00b7ner", "Blick", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$(", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Kein Zucker falscher H\u00f6ffligkeit", "tokens": ["Kein", "Zu\u00b7cker", "fal\u00b7scher", "H\u00f6ff\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ist fremder Freyheit Fall und junger Hertzen Strick.", "tokens": ["Ist", "frem\u00b7der", "Frey\u00b7heit", "Fall", "und", "jun\u00b7ger", "Hert\u00b7zen", "Strick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "NN", "KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Buhler Kunst/ die diese Zeit", "tokens": ["Der", "Buh\u00b7ler", "Kunst", "/", "die", "die\u00b7se", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "An H\u00f6fen treibt/ wird man bey uns nicht innen.", "tokens": ["An", "H\u00f6\u00b7fen", "treibt", "/", "wird", "man", "bey", "uns", "nicht", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$(", "VAFIN", "PIS", "APPR", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.24": {"text": "Ein treues Hertz/ ein freyer Mutt", "tokens": ["Ein", "treu\u00b7es", "Hertz", "/", "ein", "frey\u00b7er", "Mutt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ist unsrer keuschen Seele gutt.", "tokens": ["Ist", "uns\u00b7rer", "keu\u00b7schen", "See\u00b7le", "gutt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Um Geld und Pracht stehn wir nicht feil/", "tokens": ["Um", "Geld", "und", "Pracht", "stehn", "wir", "nicht", "feil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Betrug und List hat hier kein Theil.", "tokens": ["Be\u00b7trug", "und", "List", "hat", "hier", "kein", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wer unser Hertze will erheben", "tokens": ["Wer", "un\u00b7ser", "Hert\u00b7ze", "will", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Mu\u00df seines um das unsre geben.", "tokens": ["Mu\u00df", "sei\u00b7nes", "um", "das", "uns\u00b7re", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}