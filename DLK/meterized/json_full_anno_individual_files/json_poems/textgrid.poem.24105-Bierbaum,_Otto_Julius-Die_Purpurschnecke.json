{"textgrid.poem.24105": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Die Purpurschnecke", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie eine Schnecke,", "tokens": ["Wie", "ei\u00b7ne", "Schne\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Tr\u00e4ge, langsam,", "tokens": ["Tr\u00e4\u00b7ge", ",", "lang\u00b7sam", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Schleicht das", "tokens": ["Schleicht", "das"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "\u00bbgl\u00fcck\u00ab ...", "tokens": ["\u00bb", "gl\u00fcck", "\u00ab", "..."], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "$(", "$("], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Mit wartendem, klopfendem Herzen steht", "tokens": ["Mit", "war\u00b7ten\u00b7dem", ",", "klop\u00b7fen\u00b7dem", "Her\u00b7zen", "steht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Mensch und breitet in Qual und Angst", "tokens": ["Der", "Mensch", "und", "brei\u00b7tet", "in", "Qual", "und", "Angst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Arme aus und schreit zum Himmel:", "tokens": ["Die", "Ar\u00b7me", "aus", "und", "schreit", "zum", "Him\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bboh komm, komm endlich,", "tokens": ["\u00bb", "oh", "komm", ",", "komm", "end\u00b7lich", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "L\u00f6se mich, l\u00f6se mich", "tokens": ["L\u00f6\u00b7se", "mich", ",", "l\u00f6\u00b7se", "mich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Aus Fesseln und Banden \u2013", "tokens": ["Aus", "Fes\u00b7seln", "und", "Ban\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ein Gl\u00fcckesl\u00e4cheln,", "tokens": ["Ein", "Gl\u00fc\u00b7ckes\u00b7l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Ein einziges nur,", "tokens": ["Ein", "ein\u00b7zi\u00b7ges", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Es w\u00fcrde mein Herz", "tokens": ["Es", "w\u00fcr\u00b7de", "mein", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Erw\u00e4rmen mit lachendem Leuchten,", "tokens": ["Er\u00b7w\u00e4r\u00b7men", "mit", "la\u00b7chen\u00b7dem", "Leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Wie Maiensonne nach Winters Frost die starre Erde!\u00ab ...", "tokens": ["Wie", "Mai\u00b7en\u00b7son\u00b7ne", "nach", "Win\u00b7ters", "Frost", "die", "star\u00b7re", "Er\u00b7de", "!", "\u00ab", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "NN", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Er wartet und fleht", "tokens": ["Er", "war\u00b7tet", "und", "fleht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Lange, lange,", "tokens": ["Lan\u00b7ge", ",", "lan\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und m\u00fcht sich ab im Geschirr des Lebens,", "tokens": ["Und", "m\u00fcht", "sich", "ab", "im", "Ge\u00b7schirr", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und keucht und keucht,", "tokens": ["Und", "keucht", "und", "keucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Gebunden, gepeitscht,", "tokens": ["Ge\u00b7bun\u00b7den", ",", "ge\u00b7peitscht", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "M\u00f6chte vorw\u00e4rts: hinauf!", "tokens": ["M\u00f6ch\u00b7te", "vor\u00b7w\u00e4rts", ":", "hin\u00b7auf", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "$.", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Hinauf! wo es strahlt", "tokens": ["Hin\u00b7auf", "!", "wo", "es", "strahlt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "PWAV", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Und l\u00e4chelt das Sch\u00f6ne,", "tokens": ["Und", "l\u00e4\u00b7chelt", "das", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Ruhige, Klare,", "tokens": ["Ru\u00b7hi\u00b7ge", ",", "Kla\u00b7re", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Immer Ersehnte ...", "tokens": ["Im\u00b7mer", "Er\u00b7sehn\u00b7te", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Aber das Gl\u00fcck,", "tokens": ["A\u00b7ber", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Kein st\u00fcrmischer Engel,", "tokens": ["Kein", "st\u00fcr\u00b7mi\u00b7scher", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ach, kein g\u00fctig gew\u00e4hrendes Weib,", "tokens": ["Ach", ",", "kein", "g\u00fc\u00b7tig", "ge\u00b7w\u00e4h\u00b7ren\u00b7des", "Weib", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Aber das Gl\u00fcck,", "tokens": ["A\u00b7ber", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Die purpurne Schnecke,", "tokens": ["Die", "pur\u00b7pur\u00b7ne", "Schne\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "R\u00fcckt nur m\u00fchsam,", "tokens": ["R\u00fcckt", "nur", "m\u00fch\u00b7sam", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "In langen Fristen,", "tokens": ["In", "lan\u00b7gen", "Fris\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Wenige Schritte", "tokens": ["We\u00b7ni\u00b7ge", "Schrit\u00b7te"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Vor ... und", "tokens": ["Vor", "...", "und"], "token_info": ["word", "punct", "word"], "pos": ["APPR", "$(", "KON"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Ihre ", "tokens": ["Ih\u00b7re"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Tasten kalt an eine starre,", "tokens": ["Tas\u00b7ten", "kalt", "an", "ei\u00b7ne", "star\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Augenleere Leiche im Grabe.", "tokens": ["Au\u00b7gen\u00b7lee\u00b7re", "Lei\u00b7che", "im", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Verfluchte Schnecke, o faules Gl\u00fcck!", "tokens": ["Ver\u00b7fluch\u00b7te", "Schne\u00b7cke", ",", "o", "fau\u00b7les", "Gl\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Indes du deinen schleimigen Weg", "tokens": ["In\u00b7des", "du", "dei\u00b7nen", "schlei\u00b7mi\u00b7gen", "Weg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Lautlos vorw\u00e4rts schlichest: da stob,", "tokens": ["Laut\u00b7los", "vor\u00b7w\u00e4rts", "schli\u00b7chest", ":", "da", "stob", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "$.", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Brauste, w\u00fctete, raste mit Heulen,", "tokens": ["Braus\u00b7te", ",", "w\u00fc\u00b7te\u00b7te", ",", "ras\u00b7te", "mit", "Heu\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Gewaltig schnelle mit Sturmes M\u00e4chten", "tokens": ["Ge\u00b7wal\u00b7tig", "schnel\u00b7le", "mit", "Stur\u00b7mes", "M\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Von allen Seiten die Schaar der Furien", "tokens": ["Von", "al\u00b7len", "Sei\u00b7ten", "die", "Schaar", "der", "Fu\u00b7ri\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Los auf den Armen.", "tokens": ["Los", "auf", "den", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die d\u00fcrren Weiber!", "tokens": ["Die", "d\u00fcr\u00b7ren", "Wei\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Hexengest\u00f6ber, grimmig jauchzendes ...", "tokens": ["He\u00b7xen\u00b7ge\u00b7st\u00f6\u00b7ber", ",", "grim\u00b7mig", "jauch\u00b7zen\u00b7des", "..."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJA", "$("], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Mit ihren Gei\u00dfeln schlugen sie ihn,", "tokens": ["Mit", "ih\u00b7ren", "Gei\u00b7\u00dfeln", "schlu\u00b7gen", "sie", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mit ihren Schlangen schreckten sie ihn,", "tokens": ["Mit", "ih\u00b7ren", "Schlan\u00b7gen", "schreck\u00b7ten", "sie", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Mit ihren modrigen Blicken trieben sie ihn", "tokens": ["Mit", "ih\u00b7ren", "mod\u00b7ri\u00b7gen", "Bli\u00b7cken", "trie\u00b7ben", "sie", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Durch bange Verzweiflung und Wahnsinnsnacht", "tokens": ["Durch", "ban\u00b7ge", "Ver\u00b7zwei\u00b7flung", "und", "Wahn\u00b7sinns\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "In den Tod.", "tokens": ["In", "den", "Tod", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Ein gehetztes, verendetes Wild \u2013", "tokens": ["Ein", "ge\u00b7hetz\u00b7tes", ",", "ver\u00b7en\u00b7de\u00b7tes", "Wild", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Im Grab", "tokens": ["Im", "Grab"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Stumm liegt er nun:", "tokens": ["Stumm", "liegt", "er", "nun", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Im Nichts.", "tokens": ["Im", "Nichts", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Im friedvollen, unbelebten Nichts", "tokens": ["Im", "fried\u00b7vol\u00b7len", ",", "un\u00b7be\u00b7leb\u00b7ten", "Nichts"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Ward ihm das Gl\u00fcck ...", "tokens": ["Ward", "ihm", "das", "Gl\u00fcck", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Die dunkelrote Pupurschnecke kriecht", "tokens": ["Die", "dun\u00b7kel\u00b7ro\u00b7te", "Pu\u00b7pur\u00b7schne\u00b7cke", "kriecht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ueber seiu Grab,", "tokens": ["Ue\u00b7ber", "sei\u00b7u", "Grab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Lautlos ...", "tokens": ["Laut\u00b7los", "..."], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "Wie eine Schnecke,", "tokens": ["Wie", "ei\u00b7ne", "Schne\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Tr\u00e4ge, langsam,", "tokens": ["Tr\u00e4\u00b7ge", ",", "lang\u00b7sam", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Schleicht das", "tokens": ["Schleicht", "das"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "\u00bbgl\u00fcck\u00ab ...", "tokens": ["\u00bb", "gl\u00fcck", "\u00ab", "..."], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "$(", "$("], "meter": "+", "measure": "single.up"}}, "stanza.10": {"line.1": {"text": "Mit wartendem, klopfendem Herzen steht", "tokens": ["Mit", "war\u00b7ten\u00b7dem", ",", "klop\u00b7fen\u00b7dem", "Her\u00b7zen", "steht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "lin