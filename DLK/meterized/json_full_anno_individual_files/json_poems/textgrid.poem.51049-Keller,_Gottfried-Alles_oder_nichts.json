{"textgrid.poem.51049": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Alles oder nichts", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, du bist frei, mein Volk, von Eisenketten,", "tokens": ["Ja", ",", "du", "bist", "frei", ",", "mein", "Volk", ",", "von", "Ei\u00b7sen\u00b7ket\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Frei von der H\u00f6rigkeiten alter Schande;", "tokens": ["Frei", "von", "der", "H\u00f6\u00b7rig\u00b7kei\u00b7ten", "al\u00b7ter", "Schan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Kein Hochgeborner schmiedet dir die Bande,", "tokens": ["Kein", "Hoch\u00b7ge\u00b7bor\u00b7ner", "schmie\u00b7det", "dir", "die", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie du liegen willst, darfst du dir betten!", "tokens": ["Und", "wie", "du", "lie\u00b7gen", "willst", ",", "darfst", "du", "dir", "bet\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVINF", "VMFIN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch nicht kann dies dich vor der Herrschaft retten,", "tokens": ["Doch", "nicht", "kann", "dies", "dich", "vor", "der", "Herr\u00b7schaft", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VMFIN", "PDS", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Die ohne Grenzen schleicht von Land zu Lande;", "tokens": ["Die", "oh\u00b7ne", "Gren\u00b7zen", "schleicht", "von", "Land", "zu", "Lan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein grimmer Wolf im weichen Lammsgewande,", "tokens": ["Ein", "grim\u00b7mer", "Wolf", "im", "wei\u00b7chen", "Lamms\u00b7ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schafft sie zum Lehn sich all' bewohnte St\u00e4tten.", "tokens": ["Schafft", "sie", "zum", "Lehn", "sich", "all'", "be\u00b7wohn\u00b7te", "St\u00e4t\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PRF", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn du nicht v\u00f6llig magst den Geist entbinden", "tokens": ["Wenn", "du", "nicht", "v\u00f6l\u00b7lig", "magst", "den", "Geist", "ent\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ihres Dunstes t\u00f6dlicher Umh\u00fcllung,", "tokens": ["Von", "ih\u00b7res", "Duns\u00b7tes", "t\u00f6d\u00b7li\u00b7cher", "Um\u00b7h\u00fcl\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht tapfer um der Seele Freiheit ringen:", "tokens": ["Nicht", "tap\u00b7fer", "um", "der", "See\u00b7le", "Frei\u00b7heit", "rin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So wird der Feind stets offne Tore finden,", "tokens": ["So", "wird", "der", "Feind", "stets", "off\u00b7ne", "To\u00b7re", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "All deinem Werke rauben die Erf\u00fcllung", "tokens": ["All", "dei\u00b7nem", "Wer\u00b7ke", "rau\u00b7ben", "die", "Er\u00b7f\u00fcl\u00b7lung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und jede Knechtschaft endlich wiederbringen!", "tokens": ["Und", "je\u00b7de", "Knecht\u00b7schaft", "end\u00b7lich", "wie\u00b7der\u00b7brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ja, du bist frei, mein Volk, von Eisenketten,", "tokens": ["Ja", ",", "du", "bist", "frei", ",", "mein", "Volk", ",", "von", "Ei\u00b7sen\u00b7ket\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"