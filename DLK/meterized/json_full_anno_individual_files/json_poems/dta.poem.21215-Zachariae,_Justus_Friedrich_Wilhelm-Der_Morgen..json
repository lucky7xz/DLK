{"dta.poem.21215": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Morgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Morgen k\u00f6mmt, mit ihm die Freu-", "tokens": ["Der", "Mor\u00b7gen", "k\u00f6mmt", ",", "mit", "ihm", "die", "Freu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "de!\nO sieh! mit blitzendem Geschmeide\nSchm\u00fcckt sich f\u00fcr dich das Feld.\nIndem du aus dem Meere steigest,", "tokens": ["de", "!", "O", "sieh", "!", "mit", "blit\u00b7zen\u00b7dem", "Ge\u00b7schmei\u00b7de", "Schm\u00fcckt", "sich", "f\u00fcr", "dich", "das", "Feld", ".", "In\u00b7dem", "du", "aus", "dem", "Mee\u00b7re", "stei\u00b7gest", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "PTKVZ", "$.", "APPR", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "$.", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-+--+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Und dich in Pomp den V\u00f6lkern zeigest,", "tokens": ["Und", "dich", "in", "Pomp", "den", "V\u00f6l\u00b7kern", "zei\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Frohlockt dir eine halbe Welt.", "tokens": ["Froh\u00b7lockt", "dir", "ei\u00b7ne", "hal\u00b7be", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der ganze Himmel schwimmt in Glanz.", "tokens": ["Der", "gan\u00b7ze", "Him\u00b7mel", "schwimmt", "in", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die g\u00fcldnen Stunden f\u00fchren ihren Tanz", "tokens": ["Die", "g\u00fcld\u00b7nen", "Stun\u00b7den", "f\u00fch\u00b7ren", "ih\u00b7ren", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um dich herum, und gr\u00fc\u00dfen, Sonne, dich!", "tokens": ["Um", "dich", "he\u00b7rum", ",", "und", "gr\u00fc\u00b7\u00dfen", ",", "Son\u00b7ne", ",", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUI", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "$,", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und alle Sph\u00e4ren klingen;", "tokens": ["Und", "al\u00b7le", "Sph\u00e4\u00b7ren", "klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und alle W\u00e4lder singen;", "tokens": ["Und", "al\u00b7le", "W\u00e4l\u00b7der", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und alle Harmonien dringen", "tokens": ["Und", "al\u00b7le", "Har\u00b7mo\u00b7ni\u00b7en", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf zum Olymp, und gr\u00fc\u00dfen, Sonne, dich.", "tokens": ["Auf", "zum", "O\u00b7lymp", ",", "und", "gr\u00fc\u00b7\u00dfen", ",", "Son\u00b7ne", ",", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$,", "KON", "VVFIN", "$,", "NN", "$,", "PPER", "$."], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.8": {"text": "Dir singt die helle Kriegstrompete", "tokens": ["Dir", "singt", "die", "hel\u00b7le", "Kriegs\u00b7trom\u00b7pe\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Jm waffenvollen Feld;", "tokens": ["Jm", "waf\u00b7fen\u00b7vol\u00b7len", "Feld", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Dir singt des Hirten sanfte Fl\u00f6te", "tokens": ["Dir", "singt", "des", "Hir\u00b7ten", "sanf\u00b7te", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Jm stillen Thal.", "tokens": ["Jm", "stil\u00b7len", "Thal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Dich gr\u00fc\u00dft, durch feyerliche Lieder,", "tokens": ["Dich", "gr\u00fc\u00dft", ",", "durch", "fey\u00b7er\u00b7li\u00b7che", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der Muselmann, der Heid, und Christ.", "tokens": ["Der", "Mu\u00b7sel\u00b7mann", ",", "der", "Heid", ",", "und", "Christ", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Doch du, o Christ, weih deine frommen Lieder", "tokens": ["Doch", "du", ",", "o", "Christ", ",", "weih", "dei\u00b7ne", "from\u00b7men", "Lie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Nur Jhm, der wundervoll das Nichts geb\u00e4hren", "tokens": ["Nur", "Jhm", ",", "der", "wun\u00b7der\u00b7voll", "das", "Nichts", "ge\u00b7b\u00e4h\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PRELS", "ADJD", "ART", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und Erden schuf, und Sonnen leuchten lie\u00df.", "tokens": ["Und", "Er\u00b7den", "schuf", ",", "und", "Son\u00b7nen", "leuch\u00b7ten", "lie\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "KON", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Allm\u00e4chtger! gro\u00df im Sonnenglanz,", "tokens": ["All\u00b7m\u00e4cht\u00b7ger", "!", "gro\u00df", "im", "Son\u00b7nen\u00b7glanz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gro\u00df in majest\u00e4tscher Nacht!", "tokens": ["Und", "gro\u00df", "in", "ma\u00b7jes\u00b7t\u00e4t\u00b7scher", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verschm\u00e4h nicht Morgenopfer ganz", "tokens": ["Ver\u00b7schm\u00e4h", "nicht", "Mor\u00b7ge\u00b7nop\u00b7fer", "ganz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Sterblichen gebracht.", "tokens": ["Von", "Sterb\u00b7li\u00b7chen", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jauchzt ihm voll Ehrfurcht, dunkle", "tokens": ["Jauchzt", "ihm", "voll", "Ehr\u00b7furcht", ",", "dunk\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "W\u00e4lder!\nJauchzt ihm, erwachte frohe Felder!\nJauchz' ihm lautwallend, Ocean!\nUnd du, o Mensch, o bet ihn an!", "tokens": ["W\u00e4l\u00b7der", "!", "Jauchzt", "ihm", ",", "er\u00b7wach\u00b7te", "fro\u00b7he", "Fel\u00b7der", "!", "Jauch\u00b7z'", "ihm", "laut\u00b7wal\u00b7lend", ",", "O\u00b7cean", "!", "Und", "du", ",", "o", "Mensch", ",", "o", "bet", "ihn", "an", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,", "VVFIN", "ADJA", "NN", "$.", "NN", "PPER", "ADJD", "$,", "NN", "$.", "KON", "PPER", "$,", "FM", "NN", "$,", "FM", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+-+-+--+---+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}}}}