{"textgrid.poem.61211": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Muttersprache, reichste aller Zungen,", "tokens": ["O", "Mut\u00b7ter\u00b7spra\u00b7che", ",", "reichs\u00b7te", "al\u00b7ler", "Zun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Lenzwind schmeichelnd, stark wie Wetterdr\u00f6hnen,", "tokens": ["Wie", "Lenz\u00b7wind", "schmei\u00b7chelnd", ",", "stark", "wie", "Wet\u00b7ter\u00b7dr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In deren dreimal benedeiten T\u00f6nen", "tokens": ["In", "de\u00b7ren", "drei\u00b7mal", "be\u00b7ne\u00b7dei\u00b7ten", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zuerst erfrischt das Wort des Herrn erklungen,", "tokens": ["Zu\u00b7erst", "er\u00b7frischt", "das", "Wort", "des", "Herrn", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit eh'rnen Banden h\u00e4ltst du uns umschlungen,", "tokens": ["Mit", "eh'r\u00b7nen", "Ban\u00b7den", "h\u00e4ltst", "du", "uns", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns alle, die du z\u00e4hlst zu deinen S\u00f6hnen,", "tokens": ["Uns", "al\u00b7le", ",", "die", "du", "z\u00e4hlst", "zu", "dei\u00b7nen", "S\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df keiner sich dem Machtspruch mag gew\u00f6hnen,", "tokens": ["Da\u00df", "kei\u00b7ner", "sich", "dem", "Machts\u00b7pruch", "mag", "ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ihm mit anderm Laut ins Ohr gedrungen.", "tokens": ["Der", "ihm", "mit", "an\u00b7derm", "Laut", "ins", "Ohr", "ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIS", "APPR", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun aber wollen dir die Weltgestalter", "tokens": ["Nun", "a\u00b7ber", "wol\u00b7len", "dir", "die", "Welt\u00b7ge\u00b7stal\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entziehn ein ganz Geschlecht nach ihren Launen,", "tokens": ["Ent\u00b7ziehn", "ein", "ganz", "Ge\u00b7schlecht", "nach", "ih\u00b7ren", "Lau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und d\u00e4nisch welschen soll's im neuen Alter.", "tokens": ["Und", "d\u00e4\u00b7nisch", "wel\u00b7schen", "soll's", "im", "neu\u00b7en", "Al\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VMFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl mag dich, Mutter, fassen drob ein Staunen,", "tokens": ["Wohl", "mag", "dich", ",", "Mut\u00b7ter", ",", "fas\u00b7sen", "drob", "ein", "Stau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch zage nicht! Nein, greif auf deinem Psalter", "tokens": ["Doch", "za\u00b7ge", "nicht", "!", "Nein", ",", "greif", "auf", "dei\u00b7nem", "Psal\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$.", "PTKANT", "$,", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein wehrhaft Lied, schmetternd wie Kriegsposaunen!", "tokens": ["Ein", "wehr\u00b7haft", "Lied", ",", "schmet\u00b7ternd", "wie", "Kriegs\u00b7po\u00b7sau\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "O Muttersprache, reichste aller Zungen,", "tokens": ["O", "Mut\u00b7ter\u00b7spra\u00b7che", ",", "reichs\u00b7te", "al\u00b7ler", "Zun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN",