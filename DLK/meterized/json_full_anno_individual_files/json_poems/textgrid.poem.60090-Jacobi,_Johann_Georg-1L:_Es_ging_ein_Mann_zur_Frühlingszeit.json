": "Da sucht' er nun mit Weh und Ach", "tokens": ["Da", "sucht'", "er", "nun", "mit", "Weh", "und", "Ach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wies man ihm den hellen Bach,", "tokens": ["Da", "wies", "man", "ihm", "den", "hel\u00b7len", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und drinn die goldne Schmerle;", "tokens": ["Und", "drinn", "die", "gold\u00b7ne", "Schmer\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nichts half der Bach im Sonnenglanz,", "tokens": ["Nichts", "half", "der", "Bach", "im", "Son\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Bache nichts der Schmerlen Tanz;", "tokens": ["Im", "Ba\u00b7che", "nichts", "der", "Schmer\u00b7len", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er suchte seine Perle.", "tokens": ["Er", "such\u00b7te", "sei\u00b7ne", "Per\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und suchen wird er immer so,", "tokens": ["Und", "su\u00b7chen", "wird", "er", "im\u00b7mer", "so", ","], "token_info": ["word", "word",