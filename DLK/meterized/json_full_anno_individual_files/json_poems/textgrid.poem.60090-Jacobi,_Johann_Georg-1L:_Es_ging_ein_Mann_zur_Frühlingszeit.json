 "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Berg auf und nieder mu\u00df er gehn,", "tokens": ["Berg", "auf", "und", "nie\u00b7der", "mu\u00df", "er", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis da\u00df er sie gefunden.", "tokens": ["Bis", "da\u00df", "er", "sie", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der arme Pilger! So wie er,", "tokens": ["Der", "ar\u00b7me", "Pil\u00b7ger", "!", "So", "wie", "er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "KOKOM", "PPER", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Geh' ich zur Fr\u00fchlingszeit umher", "tokens": ["Geh'", "ich", "zur", "Fr\u00fch\u00b7lings\u00b7zeit", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um Birke, Buch' und Erle;", "tokens": ["Um", "Bir\u00b7ke", ",", "Buch'", "und", "Er\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Mayen Wunder seh' ich nicht;", "tokens": ["Des", "Ma\u00b7yen", "Wun\u00b7der", "seh'", "ich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was aber, ach! was mir gebricht,", "tokens": ["Was", "a\u00b7ber", ",", "ach", "!", "was", "mir", "ge\u00b7bricht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "ITJ", "$.", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist mehr als eine Perle.", "tokens": ["Ist", "mehr", "als", "ei\u00b7ne", "Per\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Was mir gebricht, was ich verlor,", "tokens": ["Was", "mir", "ge\u00b7bricht", ",", "was", "ich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich zum h\u00f6chsten Gut erkor,", "tokens": ["Was", "ich", "zum", "h\u00f6chs\u00b7ten", "Gut", "er\u00b7kor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist Lieb' im treuen Herzen.", "tokens": ["Ist", "Lieb'", "im", "treu\u00b7en", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vergebens wall' ich auf und ab;", "tokens": ["Ver\u00b7ge\u00b7bens", "wall'", "ich", "auf", "und", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch find' ich einst ein k\u00fchles Grab,", "tokens": ["Doch", "find'", "ich", "einst", "ein", "k\u00fch\u00b7les", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das endet alle Schmerzen.", "tokens": ["Das", "en\u00b7det", "al\u00b7le", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}