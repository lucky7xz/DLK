{"dta.poem.16777": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "136.  Eben von derselben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Geh au\u00df/ so geht Gott ein: Stirb dir/ so lebstu GOtt:", "tokens": ["Geh", "au\u00df", "/", "so", "geht", "Gott", "ein", ":", "Stirb", "dir", "/", "so", "lebs\u00b7tu", "Gott", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$(", "ADV", "VVFIN", "NN", "PTKVZ", "$.", "VVIMP", "PPER", "$(", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sey nicht/ so ist es Er: thu nichts/ so gschicht\u2019 s Geboth.", "tokens": ["Sey", "nicht", "/", "so", "ist", "es", "Er", ":", "thu", "nichts", "/", "so", "gschicht'", "s", "Ge\u00b7both", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "$(", "ADV", "VAFIN", "PPER", "PPER", "$.", "VVFIN", "PIS", "$(", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}}}}