{"dta.poem.9091": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Thiergarten, auf einer Bank, sitz ich und rauche;", "tokens": ["Im", "Thier\u00b7gar\u00b7ten", ",", "auf", "ei\u00b7ner", "Bank", ",", "sitz", "ich", "und", "rau\u00b7che", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "VVIMP", "PPER", "KON", "VVFIN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "und freue mich \u00fcber die sch\u00f6ne Vormittagssonne.", "tokens": ["und", "freu\u00b7e", "mich", "\u00fc\u00b7ber", "die", "sch\u00f6\u00b7ne", "Vor\u00b7mit\u00b7tags\u00b7son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Vor mir, glitzernd, der Kanal:", "tokens": ["Vor", "mir", ",", "glit\u00b7zernd", ",", "der", "Ka\u00b7nal", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VVPP", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "den Himmel spiegelnd, beide Ufer leise schaukelnd.", "tokens": ["den", "Him\u00b7mel", "spie\u00b7gelnd", ",", "bei\u00b7de", "U\u00b7fer", "lei\u00b7se", "schau\u00b7kelnd", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PIAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ueber die Br\u00fccke, langsam Schritt, reitet ein Leutnant.", "tokens": ["Ue\u00b7ber", "die", "Br\u00fc\u00b7cke", ",", "lang\u00b7sam", "Schritt", ",", "rei\u00b7tet", "ein", "Leut\u00b7nant", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-++-++-", "measure": "iambic.septa.invert"}}, "stanza.4": {"line.1": {"text": "Unter ihm,", "tokens": ["Un\u00b7ter", "ihm", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "zwischen den dunklen, schwimmenden Kastanienkronen,", "tokens": ["zwi\u00b7schen", "den", "dunk\u00b7len", ",", "schwim\u00b7men\u00b7den", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7kro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "pfropfenzieherartig ins Wasser gedreht,", "tokens": ["pfrop\u00b7fen\u00b7zie\u00b7her\u00b7ar\u00b7tig", "ins", "Was\u00b7ser", "ge\u00b7dreht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "\u2014 den Kragen siegellackrot \u2014", "tokens": ["den", "Kra\u00b7gen", "sie\u00b7gel\u00b7lack\u00b7rot"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "sein Spiegelbild.", "tokens": ["sein", "Spie\u00b7gel\u00b7bild", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ein Kukuk", "tokens": ["Ein", "Ku\u00b7kuk"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "ruft.", "tokens": ["ruft", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}