{"textgrid.poem.42485": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Cluniacenser von Sanct Michael,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Cluniacenser von Sanct Michael,", "tokens": ["Die", "Clu\u00b7ni\u00b7a\u00b7cen\u00b7ser", "von", "Sanct", "Mic\u00b7hael", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das waren lustige Br\u00fcder;", "tokens": ["Das", "wa\u00b7ren", "lus\u00b7ti\u00b7ge", "Br\u00fc\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die tanzten und juchten kreuzfidel", "tokens": ["Die", "tanz\u00b7ten", "und", "juch\u00b7ten", "kreuz\u00b7fi\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit den Nonnen von Sanct L\u00fcder.", "tokens": ["Mit", "den", "Non\u00b7nen", "von", "Sanct", "L\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Abt tanzte selber weit voran,", "tokens": ["Der", "Abt", "tanz\u00b7te", "sel\u00b7ber", "weit", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein L\u00fcdrian sondergleichen.", "tokens": ["Ein", "L\u00fcd\u00b7ri\u00b7an", "son\u00b7derg\u00b7lei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einst spielten die M\u00f6nche dem wackern Mann", "tokens": ["Einst", "spiel\u00b7ten", "die", "M\u00f6n\u00b7che", "dem", "wa\u00b7ckern", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den tollsten von all ihren Streichen.", "tokens": ["Den", "tolls\u00b7ten", "von", "all", "ih\u00b7ren", "Strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Ein M\u00f6nch fand den Abt im Kloster nicht,", "tokens": ["Ein", "M\u00f6nch", "fand", "den", "Abt", "im", "Klos\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKNEG", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der war bei Nachtzeit verschwunden.", "tokens": ["Der", "war", "bei", "Nacht\u00b7zeit", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Warte, dir l\u00e4ut ich das j\u00fcngste Gericht,", "tokens": ["War\u00b7te", ",", "dir", "l\u00e4ut", "ich", "das", "j\u00fcngs\u00b7te", "Ge\u00b7richt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Gleich soll es die Glocke bekunden.", "tokens": ["Gleich", "soll", "es", "die", "Glo\u00b7cke", "be\u00b7kun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Das Sterbegl\u00f6ckchen, bimmeldi bim,", "tokens": ["Das", "Ster\u00b7be\u00b7gl\u00f6ck\u00b7chen", ",", "bim\u00b7mel\u00b7di", "bim", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "W\u00fctet wild und vermessen.", "tokens": ["W\u00fc\u00b7tet", "wild", "und", "ver\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Bim, bimmeldi, bimmeldi, schlimmer als schlimm,", "tokens": ["Bim", ",", "bim\u00b7mel\u00b7di", ",", "bim\u00b7mel\u00b7di", ",", "schlim\u00b7mer", "als", "schlimm", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der M\u00f6nch rei\u00dft am Strang wie besessen.", "tokens": ["Der", "M\u00f6nch", "rei\u00dft", "am", "Strang", "wie", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KOKOM", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Die Kutten laufen wie K\u00fcchlein her:", "tokens": ["Die", "Kut\u00b7ten", "lau\u00b7fen", "wie", "K\u00fcch\u00b7lein", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Misericordia sempiterna.", "tokens": ["Mi\u00b7se\u00b7ri\u00b7cor\u00b7dia", "sem\u00b7pi\u00b7ter\u00b7na", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Was ist denn los? fragts kreuz und quer.", "tokens": ["Was", "ist", "denn", "los", "?", "fragts", "kreuz", "und", "quer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PTKVZ", "$.", "VVFIN", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbabbas mortuus est in taberna.\u00ab", "tokens": ["\u00bb", "ab\u00b7bas", "mor\u00b7tu\u00b7us", "est", "in", "ta\u00b7ber\u00b7na", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Sie machen sich auf in die Stadt in Eil,", "tokens": ["Sie", "ma\u00b7chen", "sich", "auf", "in", "die", "Stadt", "in", "Eil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Alle kennen die Taverne.", "tokens": ["Al\u00b7le", "ken\u00b7nen", "die", "Ta\u00b7ver\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das Volk nimmt lachend am Zuge teil,", "tokens": ["Das", "Volk", "nimmt", "la\u00b7chend", "am", "Zu\u00b7ge", "teil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Just l\u00f6schte der Tag die Sterne.", "tokens": ["Just", "l\u00f6schte", "der", "Tag", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mortuus est in anima! schreit", "tokens": ["Mor\u00b7tu\u00b7us", "est", "in", "a\u00b7ni\u00b7ma", "!", "schreit"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Der M\u00f6nch schier unverdrossen.", "tokens": ["Der", "M\u00f6nch", "schier", "un\u00b7ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er schwingt das Rauchfa\u00df, er psalmodeit;", "tokens": ["Er", "schwingt", "das", "Rauch\u00b7fa\u00df", ",", "er", "psal\u00b7mo\u00b7deit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit plerren die Ordensgenossen.", "tokens": ["Mit", "pler\u00b7ren", "die", "Or\u00b7dens\u00b7ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Die feierliche Prozession", "tokens": ["Die", "fei\u00b7er\u00b7li\u00b7che", "Pro\u00b7zes\u00b7si\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ist ad bordellum gekommen.", "tokens": ["Ist", "ad", "bor\u00b7del\u00b7lum", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "FM", "FM", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da finden sie den saubern Patron;", "tokens": ["Da", "fin\u00b7den", "sie", "den", "sau\u00b7bern", "Pat\u00b7ron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dem wird sehr beklommen.", "tokens": ["Dem", "wird", "sehr", "be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Bei einer langen Hure lag", "tokens": ["Bei", "ei\u00b7ner", "lan\u00b7gen", "Hu\u00b7re", "lag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der geistliche Herr mit Vergn\u00fcgen.", "tokens": ["Der", "geist\u00b7li\u00b7che", "Herr", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Oh weh, es r\u00fchrt ihn fast der Schlag,", "tokens": ["Oh", "weh", ",", "es", "r\u00fchrt", "ihn", "fast", "der", "Schlag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er mu\u00df sich den Umst\u00e4nden f\u00fcgen.", "tokens": ["Er", "mu\u00df", "sich", "den", "Um\u00b7st\u00e4n\u00b7den", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nun geht es im Triumphschritt zur\u00fcck,", "tokens": ["Nun", "geht", "es", "im", "Tri\u00b7umphsc\u00b7hritt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Wallfahrt \u00e4fft Klagelieder.", "tokens": ["Die", "Wall\u00b7fahrt", "\u00e4fft", "Kla\u00b7ge\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dem w\u00fcrdigen Abt f\u00e4llt St\u00fcck um St\u00fcck", "tokens": ["Dem", "w\u00fcr\u00b7di\u00b7gen", "Abt", "f\u00e4llt", "St\u00fcck", "um", "St\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NE", "VVFIN", "NN", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von seiner Sauseele nieder.", "tokens": ["Von", "sei\u00b7ner", "Sau\u00b7see\u00b7le", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Bischof Waldemar h\u00f6rte bald den Verdru\u00df,", "tokens": ["Bi\u00b7schof", "Wal\u00b7de\u00b7mar", "h\u00f6r\u00b7te", "bald", "den", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "