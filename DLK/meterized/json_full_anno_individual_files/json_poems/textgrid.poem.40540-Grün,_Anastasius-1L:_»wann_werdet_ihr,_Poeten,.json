{"textgrid.poem.40540": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwann werdet ihr, Poeten,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwann werdet ihr, Poeten,", "tokens": ["\u00bb", "wann", "wer\u00b7det", "ihr", ",", "Po\u00b7et\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Dichtens einmal m\u00fcd'?", "tokens": ["Des", "Dich\u00b7tens", "ein\u00b7mal", "m\u00fcd'", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann wird einst ausgesungen", "tokens": ["Wann", "wird", "einst", "aus\u00b7ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das alte, ew'ge Lied?", "tokens": ["Das", "al\u00b7te", ",", "ew'\u00b7ge", "Lied", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ist nicht schon l\u00e4ngst zur Neige", "tokens": ["Ist", "nicht", "schon", "l\u00e4ngst", "zur", "Nei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Ueberflusses Horn?", "tokens": ["Des", "Ue\u00b7ber\u00b7flus\u00b7ses", "Horn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gepfl\u00fcckt nicht jede Blume,", "tokens": ["Ge\u00b7pfl\u00fcckt", "nicht", "je\u00b7de", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKNEG", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ersch\u00f6pft nicht jeder Born?\u00ab", "tokens": ["Er\u00b7sch\u00f6pft", "nicht", "je\u00b7der", "Born", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKNEG", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So lang der Sonnenwagen", "tokens": ["So", "lang", "der", "Son\u00b7nen\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Azurgleis noch zieht,", "tokens": ["Im", "A\u00b7zur\u00b7gleis", "noch", "zieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nur Ein Menschenantlitz", "tokens": ["Und", "nur", "Ein", "Men\u00b7schen\u00b7ant\u00b7litz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu ihm empor noch sieht;", "tokens": ["Zu", "ihm", "em\u00b7por", "noch", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So lang der Himmel St\u00fcrme", "tokens": ["So", "lang", "der", "Him\u00b7mel", "St\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Donnerkeile hegt,", "tokens": ["Und", "Don\u00b7ner\u00b7kei\u00b7le", "hegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bang vor ihrem Grimme", "tokens": ["Und", "bang", "vor", "ih\u00b7rem", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Herz noch zitternd schl\u00e4gt;", "tokens": ["Ein", "Herz", "noch", "zit\u00b7ternd", "schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So lang nach Ungewittern", "tokens": ["So", "lang", "nach", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Regenbogen spr\u00fcht,", "tokens": ["Ein", "Re\u00b7gen\u00b7bo\u00b7gen", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Busen noch dem Frieden", "tokens": ["Ein", "Bu\u00b7sen", "noch", "dem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und der Vers\u00f6hnung gl\u00fcht;", "tokens": ["Und", "der", "Ver\u00b7s\u00f6h\u00b7nung", "gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So lang die Nacht den Aether", "tokens": ["So", "lang", "die", "Nacht", "den", "A\u00b7e\u00b7ther"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit Sternensaat bes\u00e4t,", "tokens": ["Mit", "Ster\u00b7nen\u00b7saat", "be\u00b7s\u00e4t", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und noch Ein Mensch die Z\u00fcge", "tokens": ["Und", "noch", "Ein", "Mensch", "die", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der goldnen Schrift versteht;", "tokens": ["Der", "gold\u00b7nen", "Schrift", "ver\u00b7steht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So lang der Mond noch leuchtet,", "tokens": ["So", "lang", "der", "Mond", "noch", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Herz noch sehnt und f\u00fchlt;", "tokens": ["Ein", "Herz", "noch", "sehnt", "und", "f\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lang der Wald noch rauschet", "tokens": ["So", "lang", "der", "Wald", "noch", "rau\u00b7schet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und einen M\u00fcden k\u00fchlt;", "tokens": ["Und", "ei\u00b7nen", "M\u00fc\u00b7den", "k\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So lang noch Lenze gr\u00fcnen", "tokens": ["So", "lang", "noch", "Len\u00b7ze", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Rosenlauben bl\u00fchn,", "tokens": ["Und", "Ro\u00b7sen\u00b7lau\u00b7ben", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lang noch Wangen l\u00e4cheln", "tokens": ["So", "lang", "noch", "Wan\u00b7gen", "l\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Augen Freude spr\u00fchn;", "tokens": ["Und", "Au\u00b7gen", "Freu\u00b7de", "spr\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So lang noch Gr\u00e4ber trauern", "tokens": ["So", "lang", "noch", "Gr\u00e4\u00b7ber", "trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit den Cypressen dran,", "tokens": ["Mit", "den", "Cyp\u00b7res\u00b7sen", "dran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So lang Ein Aug' noch weinen,", "tokens": ["So", "lang", "Ein", "Aug'", "noch", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Herz noch brechen kann:", "tokens": ["Ein", "Herz", "noch", "bre\u00b7chen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "So lange wallt auf Erden", "tokens": ["So", "lan\u00b7ge", "wallt", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die G\u00f6ttin Poesie,", "tokens": ["Die", "G\u00f6t\u00b7tin", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und mit ihr wandelt jubelnd", "tokens": ["Und", "mit", "ihr", "wan\u00b7delt", "ju\u00b7belnd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wem sie die Weihe lieh.", "tokens": ["Wem", "sie", "die", "Wei\u00b7he", "lieh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und singend einst und jubelnd", "tokens": ["Und", "sin\u00b7gend", "einst", "und", "ju\u00b7belnd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchs alte Erdenhaus", "tokens": ["Durchs", "al\u00b7te", "Er\u00b7den\u00b7haus"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zieht als der letzte Dichter", "tokens": ["Zieht", "als", "der", "letz\u00b7te", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "K