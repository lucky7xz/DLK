{"textgrid.poem.36639": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du liebes theures Vaterland!", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liebes theures Vaterland!", "tokens": ["Du", "lie\u00b7bes", "theu\u00b7res", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich geno\u00df und was ich fand,", "tokens": ["Was", "ich", "ge\u00b7no\u00df", "und", "was", "ich", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das dank' ich deiner Liebe!", "tokens": ["Das", "dank'", "ich", "dei\u00b7ner", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es liebt der Mann dich nicht allein:", "tokens": ["Es", "liebt", "der", "Mann", "dich", "nicht", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir darf sich auch das M\u00e4dchen weih'n", "tokens": ["Dir", "darf", "sich", "auch", "das", "M\u00e4d\u00b7chen", "weih'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit heilig-zartem Triebe.", "tokens": ["Mit", "hei\u00b7lig\u00b7zar\u00b7tem", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du liebes theures Vaterland!", "tokens": ["Du", "lie\u00b7bes", "theu\u00b7res", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch bist du an des Abgrunds Rand,", "tokens": ["Noch", "bist", "du", "an", "des", "Ab\u00b7grunds", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch tr\u00e4gst du Band und Ketten!", "tokens": ["Noch", "tr\u00e4gst", "du", "Band", "und", "Ket\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach h\u00e4tt' ich Schwerdt und Lanzenschaft,", "tokens": ["Ach", "h\u00e4tt'", "ich", "Schwerdt", "und", "Lan\u00b7zen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach h\u00e4tt' ich St\u00e4rke, h\u00e4tt' ich Kraft,", "tokens": ["Ach", "h\u00e4tt'", "ich", "St\u00e4r\u00b7ke", ",", "h\u00e4tt'", "ich", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie wollt' ich dich erretten!", "tokens": ["Wie", "wollt'", "ich", "dich", "er\u00b7ret\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du liebes theures Vaterland!", "tokens": ["Du", "lie\u00b7bes", "theu\u00b7res", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meinem Muth, mit meiner Hand", "tokens": ["Mit", "mei\u00b7nem", "Muth", ",", "mit", "mei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kann ich nicht f\u00fcr dich streiten.", "tokens": ["Kann", "ich", "nicht", "f\u00fcr", "dich", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Hause sitz' und h\u00e4rm' ich mich,", "tokens": ["Zu", "Hau\u00b7se", "sitz'", "und", "h\u00e4rm'", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und weine manche Thr\u00e4n' um dich!", "tokens": ["Und", "wei\u00b7ne", "man\u00b7che", "Thr\u00e4n'", "um", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was hab' ich nicht zu leiden!", "tokens": ["Was", "hab'", "ich", "nicht", "zu", "lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du liebes theures Vaterland!", "tokens": ["Du", "lie\u00b7bes", "theu\u00b7res", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich trag' um dich ein Trau'rgewand,", "tokens": ["Ich", "trag'", "um", "dich", "ein", "Trau'r\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gehe nicht zum Tanze,", "tokens": ["Und", "ge\u00b7he", "nicht", "zum", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Tanz und Spiel, ins liebe Thal,", "tokens": ["Zu", "Tanz", "und", "Spiel", ",", "ins", "lie\u00b7be", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Lorbeerhayn, im Sonnenstrahl,", "tokens": ["Zum", "Lor\u00b7beer\u00b7hayn", ",", "im", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit heiterm Blumenkranze.", "tokens": ["Mit", "hei\u00b7term", "Blu\u00b7men\u00b7kran\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du liebes theures Vaterland!", "tokens": ["Du", "lie\u00b7bes", "theu\u00b7res", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Rosenblatt, kein farbig Band", "tokens": ["Kein", "Ro\u00b7sen\u00b7blatt", ",", "kein", "far\u00b7big", "Band"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schm\u00fcckt meine blonden Locken!", "tokens": ["Schm\u00fcckt", "mei\u00b7ne", "blon\u00b7den", "Lo\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor meinem Fenster steh' ich tr\u00fcb,", "tokens": ["Vor", "mei\u00b7nem", "Fens\u00b7ter", "steh'", "ich", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wein' auf euch, die ihr so lieb", "tokens": ["Und", "wein'", "auf", "euch", ",", "die", "ihr", "so", "lieb"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "PRELS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir winket, Blumenglocken.", "tokens": ["Mir", "win\u00b7ket", ",", "Blu\u00b7men\u00b7glo\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du liebes theures Vaterland!", "tokens": ["Du", "lie\u00b7bes", "theu\u00b7res", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann denk' ich, die er vor mir stand,", "tokens": ["Dann", "denk'", "ich", ",", "die", "er", "vor", "mir", "stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seiner hei\u00dfen K\u00fcsse!", "tokens": ["Und", "sei\u00b7ner", "hei\u00b7\u00dfen", "K\u00fcs\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seinen Arm, an seiner Brust!", "tokens": ["In", "sei\u00b7nen", "Arm", ",", "an", "sei\u00b7ner", "Brust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach welche Wonn', ach welche Lust", "tokens": ["Ach", "wel\u00b7che", "Wonn'", ",", "ach", "wel\u00b7che", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "PWAT", "NN", "$,", "ITJ", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich dir zu Liebe misse!", "tokens": ["Ich", "dir", "zu", "Lie\u00b7be", "mis\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du liebes theures Vaterland!", "tokens": ["Du", "lie\u00b7bes", "theu\u00b7res", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich geno\u00df und was ich fand,", "tokens": ["Was", "ich", "ge\u00b7no\u00df", "