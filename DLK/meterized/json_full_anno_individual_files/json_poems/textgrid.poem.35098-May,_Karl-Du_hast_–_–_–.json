{"textgrid.poem.35098": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Du hast \u2013 \u2013 \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast den Kopf zum Sinnen und zum Denken,", "tokens": ["Du", "hast", "den", "Kopf", "zum", "Sin\u00b7nen", "und", "zum", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kannst auf- und abw\u00e4rts dein Bestreben lenken;", "tokens": ["Kannst", "auf", "und", "ab\u00b7w\u00e4rts", "dein", "Be\u00b7stre\u00b7ben", "len\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "TRUNC", "KON", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df es zur H\u00f6he oder Tiefe gehen,", "tokens": ["La\u00df", "es", "zur", "H\u00f6\u00b7he", "o\u00b7der", "Tie\u00b7fe", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Doch harr' des Richters, ob du wirst bestehen.", "tokens": ["Doch", "harr'", "des", "Rich\u00b7ters", ",", "ob", "du", "wirst", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du hast das Herz; nie ist's im Leben still;", "tokens": ["Du", "hast", "das", "Herz", ";", "nie", "ist's", "im", "Le\u00b7ben", "still", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "ADV", "VAFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es schl\u00e4gt f\u00fcr gut und b\u00f6s, wof\u00fcr es will,", "tokens": ["Es", "schl\u00e4gt", "f\u00fcr", "gut", "und", "b\u00f6s", ",", "wo\u00b7f\u00fcr", "es", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "KON", "ADJD", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch ob es flei\u00dfig, noch so flei\u00dfig war,", "tokens": ["Doch", "ob", "es", "flei\u00b7\u00dfig", ",", "noch", "so", "flei\u00b7\u00dfig", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "$,", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Schl\u00e4ge Summe wird einst offenbar.", "tokens": ["Der", "Schl\u00e4\u00b7ge", "Sum\u00b7me", "wird", "einst", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du hast die H\u00e4nde, welche wirken sollen;", "tokens": ["Du", "hast", "die", "H\u00e4n\u00b7de", ",", "wel\u00b7che", "wir\u00b7ken", "sol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie k\u00f6nnen nehmen, geben, wie sie wollen,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "neh\u00b7men", ",", "ge\u00b7ben", ",", "wie", "sie", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVINF", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch selbst was sie verborgen hier getrieben,", "tokens": ["Doch", "selbst", "was", "sie", "ver\u00b7bor\u00b7gen", "hier", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "PPER", "VVPP", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das findest du einst Alles aufgeschrieben.", "tokens": ["Das", "fin\u00b7dest", "du", "einst", "Al\u00b7les", "auf\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du hast die F\u00fc\u00dfe, welche selten ruhn,", "tokens": ["Du", "hast", "die", "F\u00fc\u00b7\u00dfe", ",", "wel\u00b7che", "sel\u00b7ten", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kannst vorw\u00e4rts, r\u00fcckw\u00e4rts deine Schritte thun,", "tokens": ["Kannst", "vor\u00b7w\u00e4rts", ",", "r\u00fcck\u00b7w\u00e4rts", "dei\u00b7ne", "Schrit\u00b7te", "thun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und bist du ungewi\u00df, hast du die Knie',", "tokens": ["Und", "bist", "du", "un\u00b7ge\u0