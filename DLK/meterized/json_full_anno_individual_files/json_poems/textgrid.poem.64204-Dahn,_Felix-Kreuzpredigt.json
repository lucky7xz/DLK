{"textgrid.poem.64204": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Kreuzpredigt", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf! ruft es mit Posaunenschallen von Syria bis Thuleland,", "tokens": ["Auf", "!", "ruft", "es", "mit", "Po\u00b7sau\u00b7nen\u00b7schal\u00b7len", "von", "Sy\u00b7ria", "bis", "Thu\u00b7le\u00b7land", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "APPR", "NN", "APPR", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+---+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Auf, Pal\u00e4stina ist gefallen, Jerusalem in Heidenhand.", "tokens": ["Auf", ",", "Pa\u00b7l\u00e4s\u00b7ti\u00b7na", "ist", "ge\u00b7fal\u00b7len", ",", "Je\u00b7ru\u00b7sa\u00b7lem", "in", "Hei\u00b7den\u00b7hand", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NE", "VAFIN", "VVPP", "$,", "NE", "APPR", "NE", "$."], "meter": "+-+-+--+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Gesch\u00e4ndet sind die heil'gen St\u00e4tten, der Ro\u00dfschweif auf dem \u00d6lberg wallt,", "tokens": ["Ge\u00b7sch\u00e4n\u00b7det", "sind", "die", "heil'\u00b7gen", "St\u00e4t\u00b7ten", ",", "der", "Ro\u00df\u00b7schweif", "auf", "dem", "\u00d6l\u00b7berg", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Der fromme Pilger geht in Ketten, die Kirche Gottes tr\u00e4gt Gewalt.", "tokens": ["Der", "from\u00b7me", "Pil\u00b7ger", "geht", "in", "Ket\u00b7ten", ",", "die", "Kir\u00b7che", "Got\u00b7tes", "tr\u00e4gt", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,", "ART", "NN", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Des Sarazenen trunkne Lippe entweiht den Mund der Beterin,", "tokens": ["Des", "Sa\u00b7ra\u00b7ze\u00b7nen", "trunk\u00b7ne", "Lip\u00b7pe", "ent\u00b7weiht", "den", "Mund", "der", "Be\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Zu Bethlehem aus heil'ger Krippe sein Schlachtro\u00df f\u00fcttert Saladin.", "tokens": ["Zu", "Beth\u00b7le\u00b7hem", "aus", "heil'\u00b7ger", "Krip\u00b7pe", "sein", "Schlach\u00b7tro\u00df", "f\u00fct\u00b7tert", "Sa\u00b7la\u00b7din", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Ihr Ritter, s\u00fcnd'ge Schlachtenschl\u00e4ger um ird'schen Tand und Torenstreich,", "tokens": ["Ihr", "Rit\u00b7ter", ",", "s\u00fcn\u00b7d'\u00b7ge", "Schlach\u00b7ten\u00b7schl\u00e4\u00b7ger", "um", "ird'\u00b7schen", "Tand", "und", "To\u00b7rens\u00b7treich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "APPR", "ADJA", "NN", "KON", "NE", "$,"], "meter": "-+-+--+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line