{"dta.poem.10564": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Xi.  \n  Reinigung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Bleib' du in deiner Meerestiefe,", "tokens": ["Bleib'", "du", "in", "dei\u00b7ner", "Mee\u00b7res\u00b7tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wahnsinniger Traum,", "tokens": ["Wahn\u00b7sin\u00b7ni\u00b7ger", "Traum", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der du einst so manche Nacht", "tokens": ["Der", "du", "einst", "so", "man\u00b7che", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein Herz mit falschem Gl\u00fcck gequ\u00e4lt hast", "tokens": ["Mein", "Herz", "mit", "fal\u00b7schem", "Gl\u00fcck", "ge\u00b7qu\u00e4lt", "hast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und jetzt, als See-Gespenst,", "tokens": ["Und", "jetzt", ",", "als", "See\u00b7Ge\u00b7spenst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sogar am hellen Tag' mich bedrohest \u2014", "tokens": ["So\u00b7gar", "am", "hel\u00b7len", "Tag'", "mich", "be\u00b7dro\u00b7hest"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Bleib' Du dort unten, in Ewigkeit,", "tokens": ["Bleib'", "Du", "dort", "un\u00b7ten", ",", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und ich werfe noch zu dir hinab", "tokens": ["Und", "ich", "wer\u00b7fe", "noch", "zu", "dir", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "All meine Schmerzen und S\u00fcnden", "tokens": ["All", "mei\u00b7ne", "Schmer\u00b7zen", "und", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Und die Schellenkappe der Thorheit,", "tokens": ["Und", "die", "Schel\u00b7len\u00b7kap\u00b7pe", "der", "Thor\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Die so lange mein Haupt umklingelt,", "tokens": ["Die", "so", "lan\u00b7ge", "mein", "Haupt", "um\u00b7klin\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Und die kalte, glei\u00dfende Schlangenhaut", "tokens": ["Und", "die", "kal\u00b7te", ",", "glei\u00b7\u00dfen\u00b7de", "Schlan\u00b7gen\u00b7haut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Der Heuchelei,", "tokens": ["Der", "Heu\u00b7che\u00b7lei", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Die mir so lang' die Seele umwunden,", "tokens": ["Die", "mir", "so", "lang'", "die", "See\u00b7le", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Die kranke Seele,", "tokens": ["Die", "kran\u00b7ke", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Die gottverleugnende, engelverleugnende,", "tokens": ["Die", "gott\u00b7ver\u00b7leug\u00b7nen\u00b7de", ",", "en\u00b7gel\u00b7ver\u00b7leug\u00b7nen\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Unselige Seele \u2014", "tokens": ["Un\u00b7se\u00b7li\u00b7ge", "See\u00b7le"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Hoiho! hoiho! Da kommt der Wind!", "tokens": ["Hoi\u00b7ho", "!", "hoi\u00b7ho", "!", "Da", "kommt", "der", "Wind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ITJ", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Segel auf! Sie flattern und schwell'n;", "tokens": ["Die", "Se\u00b7gel", "auf", "!", "Sie", "flat\u00b7tern", "und", "schwell'n", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPER", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ueber die stillverderbliche Fl\u00e4che", "tokens": ["Ue\u00b7ber", "die", "still\u00b7ver\u00b7derb\u00b7li\u00b7che", "Fl\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Eilet das Schiff,", "tokens": ["Ei\u00b7let", "das", "Schiff", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und es jauchzt die befreite Seele.", "tokens": ["Und", "es", "jauchzt", "die", "be\u00b7frei\u00b7te", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}