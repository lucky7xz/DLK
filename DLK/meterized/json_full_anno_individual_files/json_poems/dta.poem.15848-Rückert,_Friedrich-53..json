{"dta.poem.15848": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zum reinen Sch\u00f6nen nicht vermagst du zu gelangen,", "tokens": ["Zum", "rei\u00b7nen", "Sch\u00f6\u00b7nen", "nicht", "ver\u00b7magst", "du", "zu", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKNEG", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da vom Fantastischen dein Geist noch ist gefangen.", "tokens": ["Da", "vom", "Fan\u00b7tas\u00b7ti\u00b7schen", "dein", "Geist", "noch", "ist", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Allein du bist noch jung, der Schaden scheinet klein,", "tokens": ["Al\u00b7lein", "du", "bist", "noch", "jung", ",", "der", "Scha\u00b7den", "schei\u00b7net", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn in dir selber w\u00e4chst die Kraft, dich zu befrein.", "tokens": ["Wenn", "in", "dir", "sel\u00b7ber", "w\u00e4chst", "die", "Kraft", ",", "dich", "zu", "be\u00b7fr\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch schlimmer ist: ich seh' in dir auch nicht die Kraft,", "tokens": ["Doch", "schlim\u00b7mer", "ist", ":", "ich", "seh'", "in", "dir", "auch", "nicht", "die", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$.", "PPER", "VVFIN", "APPR", "PPER", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dich befreien k\u00f6nnt' aus der Gefangenschaft.", "tokens": ["Die", "dich", "be\u00b7frei\u00b7en", "k\u00f6nnt'", "aus", "der", "Ge\u00b7fan\u00b7gen\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}}}}