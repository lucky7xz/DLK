{"textgrid.poem.51150": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2. O heiliger Augustin im Himmelssaal", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O heiliger Augustin im Himmelssaal,", "tokens": ["O", "hei\u00b7li\u00b7ger", "Au\u00b7gus\u00b7tin", "im", "Him\u00b7mels\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun werd ich glauben an deine Gnadenwahl;", "tokens": ["Nun", "werd", "ich", "glau\u00b7ben", "an", "dei\u00b7ne", "Gna\u00b7den\u00b7wahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn gleich dem Affen, der eine Tulpe h\u00e4lt,", "tokens": ["Denn", "gleich", "dem", "Af\u00b7fen", ",", "der", "ei\u00b7ne", "Tul\u00b7pe", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sah heut ich einen halten den Festpokal!", "tokens": ["Sah", "heut", "ich", "ei\u00b7nen", "hal\u00b7ten", "den", "Fest\u00b7po\u00b7kal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wie hat zerrei\u00dfend es mir ins Ohr gegellt,", "tokens": ["Wie", "hat", "zer\u00b7rei\u00b7\u00dfend", "es", "mir", "ins", "Ohr", "ge\u00b7gellt", ","],