{"dta.poem.14124": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Nachtblume .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "af:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00fcnsche wie die Wolken sind,", "tokens": ["W\u00fcn\u00b7sche", "wie", "die", "Wol\u00b7ken", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schiffen durch die stillen R\u00e4ume,", "tokens": ["Schif\u00b7fen", "durch", "die", "stil\u00b7len", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer erkennt im lauen Wind", "tokens": ["Wer", "er\u00b7kennt", "im", "lau\u00b7en", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob's Gedanken oder Tr\u00e4ume? \u2014", "tokens": ["Ob's", "Ge\u00b7dan\u00b7ken", "o\u00b7der", "Tr\u00e4u\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}