{"dta.poem.3084": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "129.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O f\u00fchle dich, mein Geist, von Geistern stets umgeben,", "tokens": ["O", "f\u00fch\u00b7le", "dich", ",", "mein", "Geist", ",", "von", "Geis\u00b7tern", "stets", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von guten Geistern, die dich \u00fcberall umschweben;", "tokens": ["Von", "gu\u00b7ten", "Geis\u00b7tern", ",", "die", "dich", "\u00fc\u00b7be\u00b7rall", "um\u00b7schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von guten Geistern der Natur, die Rosenkronen", "tokens": ["Von", "gu\u00b7ten", "Geis\u00b7tern", "der", "Na\u00b7tur", ",", "die", "Ro\u00b7sen\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Fr\u00fchling weben und in Lilienzelten wohnen;", "tokens": ["Dem", "Fr\u00fch\u00b7ling", "we\u00b7ben", "und", "in", "Li\u00b7li\u00b7en\u00b7zel\u00b7ten", "woh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Von guten Geistern, die in Himmeln Sterne leiten,", "tokens": ["Von", "gu\u00b7ten", "Geis\u00b7tern", ",", "die", "in", "Him\u00b7meln", "Ster\u00b7ne", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Morgenrothe vor und nach dem Sp\u00e4troth schreiten;", "tokens": ["Dem", "Mor\u00b7gen\u00b7ro\u00b7the", "vor", "und", "nach", "dem", "Sp\u00e4t\u00b7roth", "schrei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den guten Geistern, die der Menschen Sinne lenken", "tokens": ["Den", "gu\u00b7ten", "Geis\u00b7tern", ",", "die", "der", "Men\u00b7schen", "Sin\u00b7ne", "len\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle Seelen hie, die dein in Liebe denken;", "tokens": ["Und", "al\u00b7le", "See\u00b7len", "hie", ",", "die", "dein", "in", "Lie\u00b7be", "den\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "$,", "PRELS", "PPOSAT", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die du mit Lieb' hier sahst, die dort mit Lieb' hernieder", "tokens": ["Die", "du", "mit", "Lieb'", "hier", "sahst", ",", "die", "dort", "mit", "Lieb'", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "VVFIN", "$,", "PRELS", "ADV", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun sehn auf dich, und die du dort wirst sehen wieder.", "tokens": ["Nun", "sehn", "auf", "dich", ",", "und", "die", "du", "dort", "wirst", "se\u00b7hen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "KON", "PRELS", "PPER", "ADV", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wo gute Geister so in Schaaren dich umfahen,", "tokens": ["Wo", "gu\u00b7te", "Geis\u00b7ter", "so", "in", "Schaa\u00b7ren", "dich", "um\u00b7fa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darfst du nicht f\u00fcrchten, da\u00df zu nah die B\u00f6sen nahen,", "tokens": ["Darfst", "du", "nicht", "f\u00fcrch\u00b7ten", ",", "da\u00df", "zu", "nah", "die", "B\u00f6\u00b7sen", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "KOUS", "PTKA", "ADJD", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Geister der Begier, die dumpf in Raum und Zeit", "tokens": ["Die", "Geis\u00b7ter", "der", "Be\u00b7gier", ",", "die", "dumpf", "in", "Raum", "und", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Befangnen, eitler Lust und eitler Traurigkeit.", "tokens": ["Be\u00b7fang\u00b7nen", ",", "eit\u00b7ler", "Lust", "und", "eit\u00b7ler", "Trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}