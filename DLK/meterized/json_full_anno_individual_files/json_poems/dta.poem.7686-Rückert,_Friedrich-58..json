{"dta.poem.7686": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich mach', alt wie ich bin, zu lernen manchen Plan,", "tokens": ["Ich", "mach'", ",", "alt", "wie", "ich", "bin", ",", "zu", "ler\u00b7nen", "man\u00b7chen", "Plan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "KOKOM", "PPER", "VAFIN", "$,", "PTKZU", "VVINF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sp\u00e4t nachzuholen was ich zeitig nicht gethan.", "tokens": ["Sp\u00e4t", "nach\u00b7zu\u00b7ho\u00b7len", "was", "ich", "zei\u00b7tig", "nicht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVIZU", "PWS", "PPER", "ADJD", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich hoffe Schritt vor Schritt noch abzuthun, was jung", "tokens": ["Ich", "hof\u00b7fe", "Schritt", "vor", "Schritt", "noch", "ab\u00b7zu\u00b7thun", ",", "was", "jung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "ADV", "VVIZU", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich h\u00e4tte leichter abgethan mit einem Sprung.", "tokens": ["Ich", "h\u00e4t\u00b7te", "leich\u00b7ter", "ab\u00b7ge\u00b7than", "mit", "ei\u00b7nem", "Sprung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und k\u00e4me nun der Tod auch zwischen meinen Plan,", "tokens": ["Und", "k\u00e4\u00b7me", "nun", "der", "Tod", "auch", "zwi\u00b7schen", "mei\u00b7nen", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So w\u00e4re mit dem Sprung grad' alles abgethan.", "tokens": ["So", "w\u00e4\u00b7re", "mit", "dem", "Sprung", "grad'", "al\u00b7les", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}