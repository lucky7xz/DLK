{"textgrid.poem.24034": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heut Nacht war ich ein Reiter", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut Nacht war ich ein Reiter", "tokens": ["Heut", "Nacht", "war", "ich", "ein", "Rei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ritt Galopp, hussah,", "tokens": ["Und", "ritt", "Ga\u00b7lopp", ",", "hus\u00b7sah", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tausend Meilen und noch weiter,", "tokens": ["Tau\u00b7send", "Mei\u00b7len", "und", "noch", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ich ein Wunder sah.", "tokens": ["Als", "ich", "ein", "Wun\u00b7der", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was sah ich? Eine Leiter,", "tokens": ["Was", "sah", "ich", "?", "Ei\u00b7ne", "Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die stand im freien Felde da", "tokens": ["Die", "stand", "im", "frei\u00b7en", "Fel\u00b7de", "da"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ging bis in den Himmel.", "tokens": ["Und", "ging", "bis", "in", "den", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vom Pferd herabgeschwungen", "tokens": ["Vom", "Pferd", "her\u00b7ab\u00b7ge\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Spross' auf Sprosse hinauf.", "tokens": ["Und", "Spross'", "auf", "Spros\u00b7se", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Vom Himmel hats gesungen:", "tokens": ["Vom", "Him\u00b7mel", "hats", "ge\u00b7sun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm schnell, komm, komm! Herauf!", "tokens": ["Komm", "schnell", ",", "komm", ",", "komm", "!", "Her\u00b7auf", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "$,", "VVFIN", "$.", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da hab ich \u00fcbersprungen", "tokens": ["Da", "hab", "ich", "\u00fc\u00b7bers\u00b7prun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Viel Sprossen schnell im hei\u00dfen Lauf", "tokens": ["Viel", "Spros\u00b7sen", "schnell", "im", "hei\u00b7\u00dfen", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und stieg bis in den Himmel.", "tokens": ["Und", "stieg", "bis", "in", "den", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da, auf der h\u00f6chsten Sprossen,", "tokens": ["Da", ",", "auf", "der", "h\u00f6chs\u00b7ten", "Spros\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im blauen Blumenkleid,", "tokens": ["Im", "blau\u00b7en", "Blu\u00b7men\u00b7kleid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von lauter Licht umgossen,", "tokens": ["Von", "lau\u00b7ter", "Licht", "um\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Engel stand bereit.", "tokens": ["Ein", "En\u00b7gel", "stand", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den hab ich unverdrossen", "tokens": ["Den", "hab", "ich", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gek\u00fc\u00dft die halbe Ewigkeit.", "tokens": ["Ge\u00b7k\u00fc\u00dft", "die", "hal\u00b7be", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es k\u00fc\u00dft sich gut im Himmel.", "tokens": ["Es", "k\u00fc\u00dft", "sich", "gut", "im", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da kam ein pl\u00f6tzlich Lachen", "tokens": ["Da", "kam", "ein", "pl\u00f6tz\u00b7lich", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von meines Engels Mund:", "tokens": ["Von", "mei\u00b7nes", "En\u00b7gels", "Mund", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bboh, oh, was hei\u00dfe Sachen!", "tokens": ["\u00bb", "oh", ",", "oh", ",", "was", "hei\u00b7\u00dfe", "Sa\u00b7chen", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$,", "FM", "$,", "PWS", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er treibt es mir zu bunt!\u00ab", "tokens": ["Er", "treibt", "es", "mir", "zu", "bunt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es th\u00e4t die Leiter krachen,", "tokens": ["Es", "th\u00e4t", "die", "Lei\u00b7ter", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ich sa\u00df unten auf dem Grund", "tokens": ["Und", "ich", "sa\u00df", "un\u00b7ten", "auf", "dem", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und war nicht mehr im Himmel.", "tokens": ["Und", "war", "nicht", "mehr", "im", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch neben mir zur Seite", "tokens": ["Doch", "ne\u00b7ben", "mir", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der liebe Engel stand", "tokens": ["Der", "lie\u00b7be", "En\u00b7gel", "stand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im blauen Blumenkleid", "tokens": ["Im", "blau\u00b7en", "Blu\u00b7men\u00b7kleid"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und gab mir seine Hand.", "tokens": ["Und", "gab", "mir", "sei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Oh s\u00fc\u00dfe Augenweide:", "tokens": ["Oh", "s\u00fc\u00b7\u00dfe", "Au\u00b7gen\u00b7wei\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da hab ich ", "tokens": ["Da", "hab", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und war erst recht im Himmel.", "tokens": ["Und", "war", "erst", "recht", "im", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Von Engeln ein Gewimmel", "tokens": ["Von", "En\u00b7geln", "ein", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat hell mich ausgelacht,", "tokens": ["Hat", "hell", "mich", "aus\u00b7ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gekicher und Gebimmel \u2013", "tokens": ["Ge\u00b7ki\u00b7cher", "und", "Ge\u00b7bim\u00b7mel", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Engel, gute Nacht.", "tokens": ["Ihr", "En\u00b7gel", ",", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir ha'm auf einem Schimmel,", "tokens": ["Wir", "ha'm", "auf", "ei\u00b7nem", "Schim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ku\u00df, hussa, Ku\u00df, uns aufgemacht", "tokens": ["Ku\u00df", ",", "hus\u00b7sa", ",", "Ku\u00df", ",", "uns", "auf\u00b7ge\u00b7macht"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ITJ", "$,", "NN", "$,", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Direkt in unsern Himmel.", "toke