{"dta.poem.8296": {"metadata": {"author": {"name": "Milton, John", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n Verlohrne Paradies.  \n  Siebenter Gesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1763", "urn": "urn:nbn:de:kobv:b4-200905199734", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Steige vom Himmel herab, Urania \nNach dem Horaz Od. III. IV. 1. De-\nscende coelo etc. Urania hei\u00dft nach dem\nGriechischen himmlisch, da\u00df er also, wie\n\nzu Anfang des Gedichts, die himmli-\nsche Muse anruft. N.! Wenn\nich dich anders", "tokens": ["Stei\u00b7ge", "vom", "Him\u00b7mel", "her\u00b7ab", ",", "U\u00b7ra\u00b7nia", "Nach", "dem", "Ho\u00b7raz", "Od.", "I\u00b7iI", ".", "Iv", ".", "1.", "De", "scen\u00b7de", "coe\u00b7lo", "et\u00b7c.", "U\u00b7ra\u00b7nia", "hei\u00dft", "nach", "dem", "Grie\u00b7chi\u00b7schen", "himm\u00b7lisch", ",", "da\u00df", "er", "al\u00b7so", ",", "wie", "zu", "An\u00b7fang", "des", "Ge\u00b7dichts", ",", "die", "himm\u00b7li", "sche", "Mu\u00b7se", "an\u00b7ruft", ".", "N.", "!", "Wenn", "ich", "dich", "an\u00b7ders"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation", "word", "punct", "word", "punct", "ordinal", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "abbreviation", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ADV", "$,", "NE", "APPR", "ART", "NE", "NE", "CARD", "$.", "CARD", "$.", "ADJA", "TRUNC", "ADJA", "NE", "ADV", "NE", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,", "KOUS", "PPER", "ADV", "$,", "PWAV", "APPR", "NN", "ART", "NN", "$,", "ART", "TRUNC", "ADJA", "NN", "VVFIN", "$.", "NE", "$.", "KOUS", "PPER", "PRF", "ADV"], "meter": "+--+--+---+-+-+-+-+-+-+---+-+--+-+-+-+-+-+-+-+--+--+-+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Recht bey diesem Namen genannt. Der g\u00f6ttli-", "tokens": ["Recht", "bey", "die\u00b7sem", "Na\u00b7men", "ge\u00b7nannt", ".", "Der", "g\u00f6tt\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN", "VVPP", "$.", "ART", "TRUNC"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Folg\u2019 ich, indem ich verwegen weit \u00fcber den hohen ", "tokens": ["Fol\u00b7g'", "ich", ",", "in\u00b7dem", "ich", "ver\u00b7we\u00b7gen", "weit", "\u00fc\u00b7ber", "den", "ho\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Und weit \u00fcber den Flug der Schwingen des ", "tokens": ["Und", "weit", "\u00fc\u00b7ber", "den", "Flug", "der", "Schwin\u00b7gen", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "NN", "ART"], "meter": "-+---+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Nicht den Namen, dein Wesen ruf ich zum k\u00fchneren Lied an,", "tokens": ["Nicht", "den", "Na\u00b7men", ",", "dein", "We\u00b7sen", "ruf", "ich", "zum", "k\u00fch\u00b7ne\u00b7ren", "Lied", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Denn du bist keine der Musen ", "tokens": ["Denn", "du", "bist", "kei\u00b7ne", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gipfel nicht; sondern bist himmlisch gebohren. Bevor noch die H\u00fcgel", "tokens": ["Gip\u00b7fel", "nicht", ";", "son\u00b7dern", "bist", "himm\u00b7lisch", "ge\u00b7boh\u00b7ren", ".", "Be\u00b7vor", "noch", "die", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$.", "KON", "VAFIN", "ADJD", "VVPP", "$.", "NE", "ADV", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Sich erhoben, und Quellen gestr\u00f6mt, da hast du vertraut schon", "tokens": ["Sich", "er\u00b7ho\u00b7ben", ",", "und", "Quel\u00b7len", "ge\u00b7str\u00f6mt", ",", "da", "hast", "du", "ver\u00b7traut", "schon"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "VVPP", "$,", "KON", "NN", "VVPP", "$,", "ADV", "VAFIN", "PPER", "ADJD", "ADV"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Mit der ewigen Weisheit dich unterhalten; der Weisheit,", "tokens": ["Mit", "der", "e\u00b7wi\u00b7gen", "Weis\u00b7heit", "dich", "un\u00b7ter\u00b7hal\u00b7ten", ";", "der", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$.", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "10Deiner Schwester; und r\u00fchrtest mit ihr die himmlischen Saiten", "tokens": ["Schwes\u00b7ter", ";", "und", "r\u00fchr\u00b7test", "mit", "ihr", "die", "himm\u00b7li\u00b7schen", "Sai\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Vor dem allm\u00e4chtigen Vater, der an den harmonischen Liedern", "tokens": ["Vor", "dem", "all\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Va\u00b7ter", ",", "der", "an", "den", "har\u00b7mo\u00b7ni\u00b7schen", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.8": {"text": "Selbst sich erg\u00f6tzte. Geleitet durch dich, erk\u00fchnte mein Flug sich", "tokens": ["Selbst", "sich", "er\u00b7g\u00f6tz\u00b7te", ".", "Ge\u00b7lei\u00b7tet", "durch", "dich", ",", "er\u00b7k\u00fchn\u00b7te", "mein", "Flug", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "VVFIN", "$.", "VVPP", "APPR", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "PRF"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.9": {"text": "In den Himmel der Himmel hinauf zu steigen. Dort trank ich", "tokens": ["In", "den", "Him\u00b7mel", "der", "Him\u00b7mel", "hin\u00b7auf", "zu", "stei\u00b7gen", ".", "Dort", "trank", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$.", "ADV", "VVFIN", "PPER"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Als ein irdischer Gaft die empyreischen L\u00fcfte,", "tokens": ["Als", "ein", "ir\u00b7di\u00b7scher", "Gaft", "die", "em\u00b7py\u00b7rei\u00b7schen", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--++-+-", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Wieder zur Erde zur\u00fcck, von der ich entsprungen; damit ich", "tokens": ["Wie\u00b7der", "zur", "Er\u00b7de", "zu\u00b7r\u00fcck", ",", "von", "der", "ich", "ent\u00b7sprun\u00b7gen", ";", "da\u00b7mit", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "VVINF", "$.", "KOUS", "PPER"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Nicht, (wie ", "tokens": ["Nicht", ",", "(", "wie"], "token_info": ["word", "punct", "punct", "word"], "pos": ["PTKNEG", "$,", "$(", "KOKOM"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "St\u00fcrzte,) von diesem fliegenden Ro\u00df, das kein Z\u00fcgel regieret,", "tokens": ["St\u00fcrz\u00b7te", ",", ")", "von", "die\u00b7sem", "flie\u00b7gen\u00b7den", "Ro\u00df", ",", "das", "kein", "Z\u00fc\u00b7gel", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "$(", "APPR", "PDAT", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Abgeworfen, herunter falle, die Felder des ", "tokens": ["Ab\u00b7ge\u00b7wor\u00b7fen", ",", "her\u00b7un\u00b7ter", "fal\u00b7le", ",", "die", "Fel\u00b7der", "des"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "VVFIN", "$,", "ART", "NN", "ART"], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Noch die H\u00e4lfte des Lieds ist ungesungen, doch enger", "tokens": ["Noch", "die", "H\u00e4lf\u00b7te", "des", "Lieds", "ist", "un\u00b7ge\u00b7sun\u00b7gen", ",", "doch", "en\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "ADJD"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "In die sichtbare Sph\u00e4re des irdischen Tages beschr\u00e4nket.", "tokens": ["In", "die", "sicht\u00b7ba\u00b7re", "Sph\u00e4\u00b7re", "des", "ir\u00b7di\u00b7schen", "Ta\u00b7ges", "be\u00b7schr\u00e4n\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.17": {"text": "Da ich auf sterblichem Boden nun steh, und die k\u00fchnen Gedanken", "tokens": ["Da", "ich", "auf", "sterb\u00b7li\u00b7chem", "Bo\u00b7den", "nun", "steh", ",", "und", "die", "k\u00fch\u00b7nen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADV", "VVFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Nicht mehr \u00fcber den Pol hinaus entz\u00fcckt sind: so sing ich", "tokens": ["Nicht", "mehr", "\u00fc\u00b7ber", "den", "Pol", "hin\u00b7aus", "ent\u00b7z\u00fcckt", "sind", ":", "so", "sing", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "APZR", "VVPP", "VAFIN", "$.", "ADV", "VVFIN", "PPER"], "meter": "+-+--+-+-+-++-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Oder verstummt; ob ich gleich in \u00fcbele Tage ", "tokens": ["O\u00b7der", "ver\u00b7stummt", ";", "ob", "ich", "gleich", "in", "\u00fc\u00b7be\u00b7le", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.20": {"text": "Leider gefalln in \u00fcbele Tage, voll \u00fcbeler Zungen,", "tokens": ["Lei\u00b7der", "ge\u00b7falln", "in", "\u00fc\u00b7be\u00b7le", "Ta\u00b7ge", ",", "voll", "\u00fc\u00b7be\u00b7ler", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.21": {"text": "Sitzend in Finsterni\u00df, rund um mich her mit Gefahren umgeben,", "tokens": ["Sit\u00b7zend", "in", "Fins\u00b7ter\u00b7ni\u00df", ",", "rund", "um", "mich", "her", "mit", "Ge\u00b7fah\u00b7ren", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADJD", "APPR", "PRF", "APZR", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Einsam, verlassen; doch nicht allein, so lange du n\u00e4chtlich", "tokens": ["Ein\u00b7sam", ",", "ver\u00b7las\u00b7sen", ";", "doch", "nicht", "al\u00b7lein", ",", "so", "lan\u00b7ge", "du", "n\u00e4cht\u00b7lich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "VVPP", "$.", "ADV", "PTKNEG", "ADV", "$,", "ADV", "ADV", "PPER", "ADJD"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.23": {"text": "Ueberpurpert. Begeistre mein Lied, ", "tokens": ["Ue\u00b7ber\u00b7pur\u00b7pert", ".", "Be\u00b7geist\u00b7re", "mein", "Lied", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.24": {"text": "W\u00fcrdige H\u00f6rer finden, obgleich nur wenig der Edlen.", "tokens": ["W\u00fcr\u00b7di\u00b7ge", "H\u00f6\u00b7rer", "fin\u00b7den", ",", "ob\u00b7gleich", "nur", "we\u00b7nig", "der", "Ed\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,", "KOUS", "ADV", "PIS", "ART", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.25": {"text": "Aber verjage von mir den barbarischen Misklang des ", "tokens": ["A\u00b7ber", "ver\u00b7ja\u00b7ge", "von", "mir", "den", "bar\u00b7ba\u00b7ri\u00b7schen", "Mis\u00b7klang", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "ART"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Und der Schw\u00e4rmer des ", "tokens": ["Und", "der", "Schw\u00e4r\u00b7mer", "des"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART"], "meter": "--+-+", "measure": "anapaest.init"}, "line.27": {"text": "Wo selbst Fels und Hain zu seinen entz\u00fcckenden Liedern", "tokens": ["Wo", "selbst", "Fels", "und", "Hain", "zu", "sei\u00b7nen", "ent\u00b7z\u00fc\u00b7cken\u00b7den", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "NE", "KON", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Ohren hatten; bis endlich Geschrey und wildes Get\u00fcmmel", "tokens": ["Oh\u00b7ren", "hat\u00b7ten", ";", "bis", "end\u00b7lich", "Ge\u00b7schrey", "und", "wil\u00b7des", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$.", "KON", "ADV", "NN", "KON", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.29": {"text": "Leyer und Stimme bet\u00e4ubt; die Muse konnte den Sohn nicht", "tokens": ["Le\u00b7yer", "und", "Stim\u00b7me", "be\u00b7t\u00e4ubt", ";", "die", "Mu\u00b7se", "konn\u00b7te", "den", "Sohn", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVPP", "$.", "ART", "NN", "VMFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Sch\u00fctzen; doch also verla\u00df du nicht den, der itzo dich anruft,", "tokens": ["Sch\u00fct\u00b7zen", ";", "doch", "al\u00b7so", "ver\u00b7la\u00df", "du", "nicht", "den", ",", "der", "it\u00b7zo", "dich", "an\u00b7ruft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ADV", "VVFIN", "PPER", "PTKNEG", "ART", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Sage, was drauf, o G\u00f6ttinn, erfolgt, da so huldreich der Engel", "tokens": ["Sa\u00b7ge", ",", "was", "drauf", ",", "o", "G\u00f6t\u00b7tinn", ",", "er\u00b7folgt", ",", "da", "so", "huld\u00b7reich", "der", "En\u00b7gel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PAV", "$,", "FM", "NN", "$,", "VVFIN", "$,", "KOUS", "ADV", "ADJD", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Von dem Abfall und Streit der rebellischen Thronen erz\u00e4hlet,", "tokens": ["Von", "dem", "Ab\u00b7fall", "und", "Streit", "der", "re\u00b7bel\u00b7li\u00b7schen", "Thro\u00b7nen", "er\u00b7z\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Und ihn durch die\u00df schreckliche Beyspiel gewarnet, vor gleichem", "tokens": ["Und", "ihn", "durch", "die\u00df", "schreck\u00b7li\u00b7che", "Bey\u00b7sp