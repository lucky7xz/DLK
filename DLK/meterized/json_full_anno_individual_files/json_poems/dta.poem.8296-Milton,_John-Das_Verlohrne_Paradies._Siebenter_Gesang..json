, "$,", "KON", "ADJA", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "So verschieden von allem auf dieser niederen Erde,", "tokens": ["So", "ver\u00b7schie\u00b7den", "von", "al\u00b7lem", "auf", "die\u00b7ser", "nie\u00b7de\u00b7ren", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIS", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Hast du uns offenbart, o g\u00f6ttlicher Lehrer! Dich sandte", "tokens": ["Hast", "du", "uns", "of\u00b7fen\u00b7bart", ",", "o", "g\u00f6tt\u00b7li\u00b7cher", "Leh\u00b7rer", "!", "Dich", "sand\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "$,", "FM", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von dem Empyreum herab des Ewigen Gnade,", "tokens": ["Von", "dem", "Em\u00b7py\u00b7reum", "her\u00b7ab", "des", "E\u00b7wi\u00b7gen", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Uns in Zeiten vor Dingen zu warnen, die unser Verderben,", "tokens": ["Uns", "in", "Zei\u00b7ten", "vor", "Din\u00b7gen", "zu", "war\u00b7nen", ",", "die", "un\u00b7ser", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Da wir durch unsern Verstand sie nicht zu erreichen vermochten", "tokens": ["Da", "wir", "durch",