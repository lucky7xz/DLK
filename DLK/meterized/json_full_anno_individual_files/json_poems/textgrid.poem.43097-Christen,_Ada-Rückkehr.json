{"textgrid.poem.43097": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckkehr", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zuckt nicht die Achseln, gr\u00fc\u00dft nicht so h\u00f6hnisch", "tokens": ["Zuckt", "nicht", "die", "Ach\u00b7seln", ",", "gr\u00fc\u00dft", "nicht", "so", "h\u00f6h\u00b7nisch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,", "VVFIN", "PTKNEG", "ADV", "ADJD"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und wendet euch nicht sp\u00f6ttisch ab!", "tokens": ["Und", "wen\u00b7det", "euch", "nicht", "sp\u00f6t\u00b7tisch", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will kein Geld von euch entlehnen,", "tokens": ["Ich", "will", "kein", "Geld", "von", "euch", "ent\u00b7leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Will nicht zur\u00fcck, was ich euch gab.", "tokens": ["Will", "nicht", "zu\u00b7r\u00fcck", ",", "was", "ich", "euch", "gab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "PTKVZ", "$,", "PWS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht euern Liebsten mehr gef\u00e4hrlich", "tokens": ["Nicht", "eu\u00b7ern", "Liebs\u00b7ten", "mehr", "ge\u00b7f\u00e4hr\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich und nimmer eurem Ruhm;", "tokens": ["Bin", "ich", "und", "nim\u00b7mer", "eu\u00b7rem", "Ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Kummer nahm mir meine Sch\u00f6nheit", "tokens": ["Der", "Kum\u00b7mer", "nahm", "mir", "mei\u00b7ne", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und all mein Ungl\u00fcck macht mich dumm.", "tokens": ["Und", "all", "mein", "Un\u00b7gl\u00fcck", "macht", "mich", "dumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich komm' zu euch, weil fortgetrieben", "tokens": ["Ich", "komm'", "zu", "euch", ",", "weil", "fort\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom sichern Strand mein Lebensschiff;", "tokens": ["Vom", "si\u00b7chern", "Strand", "mein", "Le\u00b7bens\u00b7schiff", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ganz soll es scheitern, darum lenk' ich's", "tokens": ["Ganz", "soll", "es", "schei\u00b7tern", ",", "da\u00b7rum", "lenk'", "ich's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "PAV", "VVFIN", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur\u00fcck zu euch \u2013: ihr seid das Riff!", "tokens": ["Zu\u00b7r\u00fcck", "zu", "euch", "\u2013", ":", "ihr", "seid", "das", "Riff", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$(", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zuckt nicht die Achseln, gr\u00fc\u00dft nicht so h\u00f6hnisch", "tokens": ["Zuckt", "nicht", "die", "Ach\u00b7seln", ",", "gr\u00fc\u00dft", "nicht", "so", "h\u00f6h\u00b7nisch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,", "VVFIN", "PTKNEG", "ADV", "ADJD"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und wendet euch nicht sp\u00f6ttisch ab!", "tokens": ["Und", "wen\u00b7det", "euch", "nicht", "sp\u00f6t\u00b7tisch", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will kein Geld von euch entlehnen,", "tokens": ["Ich", "will", "kein", "Geld", "von", "euch", "ent\u00b7leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Will nicht zur\u00fcck, was ich euch gab.", "tokens": ["Will", "nicht", "zu\u00b7r\u00fcck", ",", "was", "ich", "euch", "gab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "PTKVZ", "$,", "PWS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "meas