{"textgrid.poem.50780": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei, wie das kracht! \u2013 Abscheuliches Geroll", "tokens": ["Ei", ",", "wie", "das", "kracht", "!", "\u2013", "Ab\u00b7scheu\u00b7li\u00b7ches", "Ge\u00b7roll"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "$,", "PWAV", "PDS", "VVFIN", "$.", "$(", "ADJA", "NN"], "meter": "+--+++-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Von Schutt und Erde, polternden Gebeinen!", "tokens": ["Von", "Schutt", "und", "Er\u00b7de", ",", "pol\u00b7tern\u00b7den", "Ge\u00b7bei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich kann nicht lachen und kann auch nicht weinen \u2013", "tokens": ["Ich", "kann", "nicht", "la\u00b7chen", "und", "kann", "auch", "nicht", "wei\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Es nimmt mich wunder, wie das enden soll!", "tokens": ["Es", "nimmt", "mich", "wun\u00b7der", ",", "wie", "das", "en\u00b7den", "soll", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "PWAV", "PDS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun wird es still. \u2013 Sie trollen sich nach Haus", "tokens": ["Nun", "wird", "es", "still", ".", "\u2013", "Sie", "trol\u00b7len", "sich", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und lassen mich hier sieben Fu\u00df tief liegen;", "tokens": ["Und", "las\u00b7sen", "mich", "hier", "sie\u00b7ben", "Fu\u00df", "tief", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "CARD", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun, Phantasie! la\u00df deine Adler fliegen,", "tokens": ["Nun", ",", "Phan\u00b7ta\u00b7sie", "!", "la\u00df", "dei\u00b7ne", "Ad\u00b7ler", "flie\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$.", "VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Hier schwingen sie wohl nimmer mich heraus!", "tokens": ["Hier", "schwin\u00b7gen", "sie", "wohl", "nim\u00b7mer", "mich", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das ist jetzt eine wunderliche Zeit!", "tokens": ["Das", "ist", "jetzt", "ei\u00b7ne", "wun\u00b7der\u00b7li\u00b7che", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im dunklen Grab kein Regen und kein R\u00fchren,", "tokens": ["Im", "dunk\u00b7len", "Grab", "kein", "Re\u00b7gen", "und", "kein", "R\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Indes der Geist, als Holzwurm, mag spazieren", "tokens": ["In\u00b7des", "der", "Geist", ",", "als", "Holz\u00b7wurm", ",", "mag", "spa\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "NN", "$,", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Tannenholz \u2013 ist das die Ewigkeit?", "tokens": ["Im", "Tan\u00b7nen\u00b7holz", "\u2013", "ist", "das", "die", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "VAFIN", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Menschen sind ein l\u00fcgnerisch Geschlecht,", "tokens": ["Die", "Men\u00b7schen", "sind", "ein", "l\u00fcg\u00b7ne\u00b7risch", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie haben selbst den Erdboden belogen,", "tokens": ["Sie", "ha\u00b7ben", "selbst", "den", "Erd\u00b7bo\u00b7den", "be\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den ernsten Moder schn\u00f6d mit mir betrogen \u2013", "tokens": ["Den", "erns\u00b7ten", "Mo\u00b7der", "schn\u00f6d", "mit", "mir", "be\u00b7tro\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "