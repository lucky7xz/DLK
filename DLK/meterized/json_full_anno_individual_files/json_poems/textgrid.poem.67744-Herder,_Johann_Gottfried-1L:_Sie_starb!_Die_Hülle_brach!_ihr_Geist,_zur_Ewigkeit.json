m", "nicht", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "KON", "VVFIN", "KON", "VVFIN", "$.", "NE", "$,", "PWS", "VVFIN", "PPER", "PTKNEG", "APPR"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und lernt, wie sterbend auch sein Mund erhaben sprach!", "tokens": ["Und", "lernt", ",", "wie", "ster\u00b7bend", "auch", "sein", "Mund", "er\u00b7ha\u00b7ben", "sprach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJD", "ADV", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sein bestes Monument sind stille Herzensthr\u00e4nen,", "tokens": ["Sein", "bes\u00b7tes", "Mo\u00b7nu\u00b7ment", "sind", "stil\u00b7le", "Her\u00b7zenst\u00b7hr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aufs edle Grab geweint, der Seufzer stilles Sehnen,", "tokens": ["Aufs", "ed\u00b7le", "Grab", "ge\u00b7weint", ",", "der", "Seuf\u00b7zer", "stil\u00b7les", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was in der Einsamkeit herauf der Engel tr\u00e4gt,", "tokens": ["Was", "in", "der", "Ein\u00b7sam\u00b7keit", "her\u00b7auf", "der", "En\u00b7gel", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "An seinen Busen dr\u00fcckt und vor den Herren legt.", "tokens": ["An", "sei\u00b7nen", "Bu\u00b7sen", "dr\u00fcckt", "und", "vor", "den", "Her\u00b7ren", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch auch die Thr\u00e4ne sieht ihr neuverkl\u00e4rter Blick", "tokens": ["Doch", "auch", "die", "Thr\u00e4\u00b7ne", "sieht", "ihr", "neu\u00b7ver\u00b7kl\u00e4r\u00b7ter", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit hoher Wehmuth an und w\u00fcnscht ihr \u2013 weises Gl\u00fcck!", "tokens": ["Mit", "ho\u00b7her", "Weh\u00b7muth", "an", "und", "w\u00fcnscht", "ihr", "\u2013", "wei\u00b7ses", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Erk\u00e4mpft ihr Trost von Dem, der jede Z\u00e4hr' verwischet,", "tokens": ["Er\u00b7k\u00e4mpft", "ihr", "Trost", "von", "Dem", ",", "der", "je\u00b7de", "Z\u00e4hr'", "ver\u00b7wi\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PDS", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sie z\u00e4hlt und in den Kelch der Leiden Wonne mischet.", "tokens": ["Sie", "z\u00e4hlt", "und", "in", "den", "Kelch", "der", "Lei\u00b7den", "Won\u00b7ne", "mi\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So w\u00fcnscht die Himmlische, stark, bis sie Gott erweicht;", "tokens": ["So", "w\u00fcnscht", "die", "Himm\u00b7li\u00b7sche", ",", "stark", ",", "bis", "sie", "Gott", "er\u00b7weicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "$,", "KOUS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und wo ein Engel w\u00fcnscht, da stirbt mein Trost und \u2013 schweigt.", "tokens": ["Und", "wo", "ein", "En\u00b7gel", "w\u00fcnscht", ",", "da", "stirbt", "mein", "Trost", "und", "\u2013", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "KON", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}