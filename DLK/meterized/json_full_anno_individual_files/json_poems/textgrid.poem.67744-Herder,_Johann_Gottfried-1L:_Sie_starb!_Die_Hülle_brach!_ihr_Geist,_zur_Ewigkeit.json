nct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVFIN", "$,", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn er im Feu'rgebet in Salem H\u00fctten baut.", "tokens": ["Wenn", "er", "im", "Feu'r\u00b7ge\u00b7bet", "in", "Sa\u00b7lem", "H\u00fct\u00b7ten", "baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er legt die B\u00fcrde ab, den Pilgerstab zerbrochen,", "tokens": ["Er", "legt", "die", "B\u00fcr\u00b7de", "ab", ",", "den", "Pil\u00b7ger\u00b7stab", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die ganze Pflicht erf\u00fcllt, den letzten Feind gerochen.", "tokens": ["Die", "gan\u00b7ze", "Pflicht", "er\u00b7f\u00fcllt", ",", "den", "letz\u00b7ten", "Feind", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Schon schallt sein Ohr den Engelston;", "tokens": ["Schon", "schallt", "sein", "Ohr", "den", "En\u00b7gel\u00b7ston", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fernher getroffen, h\u00f6rt und singt", "tokens": ["Fern\u00b7her", "ge\u00b7trof\u00b