"word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,", "APPR", "NE", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von nichts zur\u00fcckgezerrt, was hier im Leben blieb,", "tokens": ["Von", "nichts", "zu\u00b7r\u00fcck\u00b7ge\u00b7zerrt", ",", "was", "hier", "im", "Le\u00b7ben", "blieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil jedes Demantband das Alter l\u00e4ngst zerrieb!", "tokens": ["Weil", "je\u00b7des", "De\u00b7mant\u00b7band", "das", "Al\u00b7ter", "l\u00e4ngst", "zer\u00b7rieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kein Wunsch flammt mehr empor wie aus des Aetna Schlunde;", "tokens": ["Kein", "Wunsch", "flammt", "mehr", "em\u00b7por", "wie", "aus", "des", "A\u00b7et\u00b7na", "Schlun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "PTKVZ", "KOKOM", "APPR", "ART", "NE", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Der Sturm der Leidenschaft entschlief im stillen Grunde.", "tokens": ["Der", "Sturm", "der", "Lei\u00b7den\u00b7schaft", "ent\u00b7schlief", "im", "stil\u00b7len", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O, solch ein Augenblick ist (w\u00e4r' er mir gew\u00e4hrt!)", "tokens": ["O", ",", "solch", "ein", "Au\u00b7gen\u00b7blick", "ist", "(", "w\u00e4r'", "er", "mir", "ge\u00b7w\u00e4hrt", "!", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PIAT", "ART", "NN", "VAFIN", "$(", "VAFIN", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mehr als ein Leben voll, ja zehn voll Gl\u00fcckes werth.", "tokens": ["Mehr", "als", "ein", "Le\u00b7ben", "voll", ",", "ja", "zehn", "voll", "Gl\u00fc\u00b7ckes", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "ADJD", "$,", "ADV", "CARD", "ADJD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Halb Engel und halb Mensch (mein gro\u00dfes Maa\u00df will wanken),", "tokens": ["Halb", "En\u00b7gel", "und", "halb", "Mensch", "(", "mein", "gro\u00b7\u00dfes", "Maa\u00df", "will", "wan\u00b7ken", ")", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "KON", "ADJD", "NN", "$(", "PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$(", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieht sie in unsre Tief' vom h\u00f6chsten Erdgedanken", "tokens": ["Sieht", "sie", "in", "uns\u00b7re", "Tie\u00b7f'", "vom", "h\u00f6chs\u00b7ten", "Erd\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und blickt zu Gott empor, roth, wie einst Moses gl\u00fcht,", "tokens": ["Und", "blickt", "zu", "Gott", "em\u00b7por", ",", "roth", ",", "wie", "einst", "Mo\u00b7ses", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "ADJD", "$,", "PWAV", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn er, halb Mensch, halb Geist, den Unsichtbaren sieht.", "tokens": ["Wenn", "er", ",", "halb", "Mensch", ",", "halb", "Geist", ",", "den", "Un\u00b7sicht\u00b7ba\u00b7ren", "sieht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "NN", "$,", "ADJD", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O Schattenpunkt, umschr\u00e4nkt von Zeit und Ewigkeiten,", "tokens": ["O", "Schat\u00b7ten\u00b7punkt", ",", "um\u00b7schr\u00e4nkt", "von", "Zeit", "und", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie ist's, wenn man von Dir ein dunkel Feld von weiten", "tokens": ["Wie", "ist's", ",", "wenn", "man", "von", "Dir", "ein", "dun\u00b7kel", "Feld", "von", "wei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "$,", "KOUS", "PIS", "APPR", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Breit unabsehbar sieht, h\u00f6rt dunkler Fl\u00fcsse Ton", "tokens": ["Breit", "un\u00b7ab\u00b7seh\u00b7bar", "sieht", ",", "h\u00f6rt", "dunk\u00b7ler", "Fl\u