{"textgrid.poem.67744": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie starb! Die H\u00fclle brach! ihr Geist, zur Ewigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie starb! Die H\u00fclle brach! ihr Geist, zur Ewigkeit", "tokens": ["Sie", "starb", "!", "Die", "H\u00fcl\u00b7le", "brach", "!", "ihr", "Geist", ",", "zur", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "$.", "PPOSAT", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch langes Leben schon bereitet,", "tokens": ["Durch", "lan\u00b7ges", "Le\u00b7ben", "schon", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Entwand sich aus der H\u00fclle irdisch Kleid", "tokens": ["Ent\u00b7wand", "sich", "aus", "der", "H\u00fcl\u00b7le", "ir\u00b7disch", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und fuhr hinauf, vom Seraphim geleitet;", "tokens": ["Und", "fuhr", "hin\u00b7auf", ",", "vom", "Se\u00b7ra\u00b7phim", "ge\u00b7lei\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nur unten weint", "tokens": ["Nur", "un\u00b7ten", "weint"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Menschen-, Freund- und Kindesz\u00e4hre.", "tokens": ["Die", "Men\u00b7schen", ",", "Freun\u00b7d", "und", "Kin\u00b7des\u00b7z\u00e4h\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "$,", "TRUNC", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "O Du, des Alters Tod, der Menschheit Ehre,", "tokens": ["O", "Du", ",", "des", "Al\u00b7ters", "Tod", ",", "der", "Menschheit", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sei mir mein Lehrer und mein Freund!", "tokens": ["Sei", "mir", "mein", "Leh\u00b7rer", "und", "mein", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Welt liegt hinter ihr; selbst ihre Rosen waren", "tokens": ["Die", "Welt", "liegt", "hin\u00b7ter", "ihr", ";", "selbst", "ih\u00b7re", "Ro\u00b7sen", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "$.", "ADV", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umdornt, umringt ihr Gold von t\u00f6dtenden Gefahren;", "tokens": ["Um\u00b7dornt", ",", "um\u00b7ringt", "ihr", "Gold", "von", "t\u00f6d\u00b7ten\u00b7den", "Ge\u00b7fah\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch jetzt ist jede Dorn, die Blut von ihrer Wunde trug,", "tokens": ["Doch", "jetzt", "ist", "je\u00b7de", "Dorn", ",", "die", "Blut", "von", "ih\u00b7rer", "Wun\u00b7de", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ein bl\u00fchnder Rosenzweig im neuen Kronenschmuck.", "tokens": ["Ein", "bl\u00fchn\u00b7der", "Ro\u00b7sen\u00b7zweig", "im", "neu\u00b7en", "Kro\u00b7nen\u00b7schmuck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gez\u00e4hlt ward jede Thr\u00e4n', die sie der Menschheit zollte;", "tokens": ["Ge\u00b7z\u00e4hlt", "ward", "je\u00b7de", "Thr\u00e4n'", ",", "die", "sie", "der", "Menschheit", "zoll\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Jetzt ist's ein Perlenkranz, was ihr vom Auge rollte.", "tokens": ["Jetzt", "ist's", "ein", "Per\u00b7len\u00b7kranz", ",", "was", "ihr", "vom", "Au\u00b7ge", "roll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Lebens Trank hat sie gelabet und erfreut;", "tokens": ["Des", "Le\u00b7bens", "Trank", "hat", "sie", "ge\u00b7la\u00b7bet", "und", "er\u00b7freut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sein Gl\u00fcck, sein Wunsch und Thun ist \u2013 Eitelkeit.", "tokens": ["Sein", "Gl\u00fcck", ",", "sein", "Wunsch", "und", "Thun", "ist", "\u2013", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "VAFIN", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O, wie ruht sanft ihr Herz, von keinem Wurm zernaget,", "tokens": ["O", ",", "wie", "ruht", "sanft", "ihr", "Herz", ",", "von", "kei\u00b7nem", "Wurm", "zer\u00b7na\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von keinem Dolch zerw\u00fchlt, von Ottern ungeplaget,", "tokens": ["Von", "kei\u00b7nem", "Dolch", "zer\u00b7w\u00fchlt", ",", "von", "Ot\u00b7tern", "un\u00b7ge\u00b7pla\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,", "APPR", "NE", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von nichts zur\u00fcckgezerrt, was hier im Leben blieb,", "tokens": ["Von", "nichts", "zu\u00b7r\u00fcck\u00b7ge\u00b7zerrt", ",", "was", "hier", "im", "Le\u00b7ben", "blieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil jedes Demantband das Alter l\u00e4ngst zerrieb!", "tokens": ["Weil", "je\u00b7des", "De\u00b7mant\u00b7band", "das", "Al\u00b7ter", "l\u00e4ngst", "zer\u00b7rieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kein Wunsch flammt mehr empor wie aus des Aetna Schlunde;", "tokens": ["Kein", "Wunsch", "flammt", "mehr", "em\u00b7por", "wie", "aus", "des", "A\u00b7et\u00b7na", "Schlun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "PTKVZ", "KOKOM", "APPR", "ART", "NE", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Der Sturm der Leidenschaft entschlief im stillen Grunde.", "tokens": ["Der", "Sturm", "der", "Lei\u00b7den\u00b7schaft", "ent\u00b7schlief", "im", "stil\u00b7len", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O, solch ein Augenblick ist (w\u00e4r' er mir gew\u00e4hrt!)", "tokens": ["O", ",", "solch", "ein", "Au\u00b7gen\u00b7blick", "ist", "(", "w\u00e4r'", "er", "mir", "ge\u00b7w\u00e4hrt", "!", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PIAT", "ART", "NN", "VAFIN", "$(", "VAFIN", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mehr als ein Leben voll, ja zehn voll Gl\u00fcckes werth.", "tokens": ["Mehr", "als", "ein", "Le\u00b7ben", "voll", ",", "ja", "zehn", "voll", "Gl\u00fc\u00b7ckes", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "ADJD", "$,", "ADV", "CARD", "ADJD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Halb Engel und halb Mensch (mein gro\u00dfes Maa\u00df will wanken),", "tokens": ["Halb", "En\u00b7gel", "und", "halb", "Mensch", "(", "mein", "gro\u00b7\u00dfes", "Maa\u00df", "will", "wan\u00b7ken", ")", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "KON", "ADJD", "NN", "$(", "PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$(", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieht sie in unsre Tief' vom h\u00f6chsten Erdgedanken", "tokens": ["Sieht", "sie", "in", "uns\u00b7re", "Tie\u00b7f'", "vom", "h\u00f6chs\u00b7ten", "Erd\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und blickt zu Gott empor, roth, wie einst Moses gl\u00fcht,", "tokens": ["Und", "blickt", "zu", "Gott", "em\u00b7por", ",", "roth", ",", "wie", "einst", "Mo\u00b7ses", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "ADJD", "$,", "PWAV", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn er, halb Mensch, halb Geist, den Unsichtbaren sieht.", "tokens": ["Wenn", "er", ",", "halb", "Mensch", ",", "halb", "Geist", ",", "den", "Un\u00b7sicht\u00b7ba\u00b7ren", "sieht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "NN", "$,", "ADJD", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O Schattenpunkt, umschr\u00e4nkt von Zeit und Ewigkeiten,", "tokens": ["O", "Schat\u00b7ten\u00b7punkt", ",", "um\u00b7schr\u00e4nkt", "von", "Zeit", "und", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie ist's, wenn man von Dir ein dunkel Feld von weiten", "tokens": ["Wie", "ist's", ",", "wenn", "man", "von", "Dir", "ein", "dun\u00b7kel", "Feld", "von", "wei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "$,", "KOUS", "PIS", "APPR", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Breit unabsehbar sieht, h\u00f6rt dunkler Fl\u00fcsse Ton", "tokens": ["Breit", "un\u00b7ab\u00b7seh\u00b7bar", "sieht", ",", "h\u00f6rt", "dunk\u00b7ler", "Fl\u00fcs\u00b7se", "Ton"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN", "$,", "VVFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Geisterstimmen h\u00f6rt und sieht des Richters Thron!", "tokens": ["Und", "Geis\u00b7ters\u00b7tim\u00b7men", "h\u00f6rt", "und", "sieht", "des", "Rich\u00b7ters", "Thron", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O wohl, Dich kennt der Christ, der Ewigkeit nie scheute,", "tokens": ["O", "wohl", ",", "Dich", "kennt", "der", "Christ", ",", "der", "E\u00b7wig\u00b7keit", "nie", "scheu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie, weil er s' scheut, verflucht, oft, weil sie ihn erfreute,", "tokens": ["Nie", ",", "weil", "er", "s'", "scheut", ",", "ver\u00b7flucht", ",", "oft", ",", "weil", "sie", "ihn", "er\u00b7freu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "NE", "VVFIN", "$,", "VVPP", "$,", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Gew\u00fcnscht, gehofft, erfleht, oft schon entz\u00fcckt geschaut,", "tokens": ["Ge\u00b7w\u00fcnscht", ",", "ge\u00b7hofft", ",", "er\u00b7fleht", ",", "oft", "schon", "ent\u00b7z\u00fcckt", "ge\u00b7schaut", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVFIN", "$,", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn er im Feu'rgebet in Salem H\u00fctten baut.", "tokens": ["Wenn", "er", "im", "Feu'r\u00b7ge\u00b7bet", "in", "Sa\u00b7lem", "H\u00fct\u00b7ten", "baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er legt die B\u00fcrde ab, den Pilgerstab zerbrochen,", "tokens": ["Er", "legt", "die", "B\u00fcr\u00b7de", "ab", ",", "den", "Pil\u00b7ger\u00b7stab", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die ganze Pflicht erf\u00fcllt, den letzten Feind gerochen.", "tokens": ["Die", "gan\u00b7ze", "Pflicht", "er\u00b7f\u00fcllt", ",", "den", "letz\u00b7ten", "Feind", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Schon schallt sein Ohr den Engelston;", "tokens": ["Schon", "schallt", "sein", "Ohr", "den", "En\u00b7gel\u00b7ston", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fernher getroffen, h\u00f6rt und singt", "tokens": ["Fern\u00b7her", "ge\u00b7trof\u00b7fen", ",", "h\u00f6rt", "und", "singt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er mit das Lied, das ihm Triumph zuwinkt", "tokens": ["Er", "mit", "das", "Lied", ",", "das", "ihm", "Tri\u00b7umph", "zu\u00b7winkt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Ueberwinderthron!", "tokens": ["Vom", "Ue\u00b7ber\u00b7win\u00b7der\u00b7thron", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schallt, was kein Ohr geh\u00f6rt, kein Dichter nachgesungen:", "tokens": ["Schallt", ",", "was", "kein", "Ohr", "ge\u00b7h\u00f6rt", ",", "kein", "Dich\u00b7ter", "nach\u00b7ge\u00b7sun\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u00bbkomm her, o Knecht! Sieh Palmen, Dir errungen!\u00ab", "tokens": ["\u00bb", "komm", "her", ",", "o", "Knecht", "!", "Sieh", "Pal\u00b7men", ",", "Dir", "er\u00b7run\u00b7gen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "FM", "NN", "$.", "NE", "NN", "$,", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er sieht's und jauchzt und stirbt. O, wer jauchzt ihm nicht nach", "tokens": ["Er", "sieht's", "und", "jauchzt", "und", "stirbt", ".", "O", ",", "wer", "jauchzt", "ihm", "nicht", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "KON", "VVFIN", "KON", "VVFIN", "$.", "NE", "$,", "PWS", "VVFIN", "PPER", "PTKNEG", "APPR"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und lernt, wie sterbend auch sein Mund erhaben sprach!", "tokens": ["Und", "lernt", ",", "wie", "ster\u00b7bend", "auch", "sein", "Mund", "er\u00b7ha\u00b7ben", "sprach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJD", "ADV", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sein bestes Monument sind stille Herzensthr\u00e4nen,", "tokens": ["Sein", "bes\u00b7tes", "Mo\u00b7nu\u00b7ment", "sind", "stil\u00b7le", "Her\u00b7zenst\u00b7hr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Aufs edle Grab geweint, der Seufzer stilles Sehnen,", "tokens": ["Aufs", "ed\u00b7le", "Grab", "ge\u00b7weint", ",", "der", "Seuf\u00b7zer", "stil\u00b7les", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was in der Einsamkeit herauf der Engel tr\u00e4gt,", "tokens": ["Was", "in", "der", "Ein\u00b7sam\u00b7keit", "her\u00b7auf", "der", "En\u00b7gel", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "An seinen Busen dr\u00fcckt und vor den Herren legt.", "tokens": ["An", "sei\u00b7nen", "Bu\u00b7sen", "dr\u00fcckt", "und", "vor", "den", "Her\u00b7ren", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch auch 