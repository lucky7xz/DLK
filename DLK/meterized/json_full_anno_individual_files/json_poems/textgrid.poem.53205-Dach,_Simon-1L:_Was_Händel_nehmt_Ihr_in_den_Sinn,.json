{"textgrid.poem.53205": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was H\u00e4ndel nehmt Ihr in den Sinn,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was H\u00e4ndel nehmt Ihr in den Sinn,", "tokens": ["Was", "H\u00e4n\u00b7del", "nehmt", "Ihr", "in", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr H\u00e4ndel, dieser Zeiten?", "tokens": ["Herr", "H\u00e4n\u00b7del", ",", "die\u00b7ser", "Zei\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir haben H\u00e4ndel gnug vorhin,", "tokens": ["Wir", "ha\u00b7ben", "H\u00e4n\u00b7del", "gnug", "vor\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man kan sie kaum bestreiten:", "tokens": ["Man", "kan", "sie", "kaum", "be\u00b7strei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zuvor da alles sicher war", "tokens": ["Zu\u00b7vor", "da", "al\u00b7les", "si\u00b7cher", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Giengt Ihr ohn Liebes-Wandel;", "tokens": ["Giengt", "Ihr", "ohn", "Lie\u00b7bes\u00b7Wan\u00b7del", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Jetzt da sich regt die Kriegs-Gefahr,", "tokens": ["Jetzt", "da", "sich", "regt", "die", "Kriegs\u00b7Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Griefft Ihr zum Heyraht-Handel.", "tokens": ["Griefft", "Ihr", "zum", "Hey\u00b7raht\u00b7Han\u00b7del", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr meint, geht Ihr den Ehstand ein,", "tokens": ["Ihr", "meint", ",", "geht", "Ihr", "den", "Eh\u00b7stand", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr werdet sicher leben", "tokens": ["Ihr", "wer\u00b7det", "si\u00b7cher", "le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd \u00fcberall ohn H\u00e4ndel seyn;", "tokens": ["Vnd", "\u00fc\u00b7be\u00b7rall", "ohn", "H\u00e4n\u00b7del", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo irgends H\u00e4ndel schweben,", "tokens": ["Wo", "ir\u00b7gends", "H\u00e4n\u00b7del", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So wohnen sie der Heyraht bey,", "tokens": ["So", "woh\u00b7nen", "sie", "der", "Hey\u00b7raht", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ich es auch befinde,", "tokens": ["Wie", "ich", "es", "auch", "be\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ist alles sonst von H\u00e4ndeln frey,", "tokens": ["Ist", "al\u00b7les", "sonst", "von", "H\u00e4n\u00b7deln", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es macht sie das Gesinde.", "tokens": ["Es", "macht", "sie", "das", "Ge\u00b7sin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das f\u00fchret unternander Streit,", "tokens": ["Das", "f\u00fch\u00b7ret", "un\u00b7ter\u00b7nan\u00b7der", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist zwischen Mann und Frawen", "tokens": ["Ist", "zwi\u00b7schen", "Mann", "und", "Fra\u00b7wen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gleich alle Lieb' und Einigheit", "tokens": ["Gleich", "al\u00b7le", "Lieb'", "und", "Ei\u00b7nig\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd hertzliches Vertrawen,", "tokens": ["Vnd", "hertz\u00b7li\u00b7ches", "Ver\u00b7tra\u00b7wen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was Kriege werden da emp\u00f6rt,", "tokens": ["Was", "Krie\u00b7ge", "wer\u00b7den", "da", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Thut man nicht zu die Ohren,", "tokens": ["Thut", "man", "nicht", "zu", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Was Fl\u00fcche werden da geh\u00f6rt", "tokens": ["Was", "Fl\u00fc\u00b7che", "wer\u00b7den", "da", "ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd wie wird da geschworen?", "tokens": ["Vnd", "wie", "wird", "da", "ge\u00b7schwo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie offt wird da aus Vbermuth", "tokens": ["Wie", "offt", "wird", "da", "aus", "Vber\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Herrschaft widersprochen?", "tokens": ["Der", "Herr\u00b7schaft", "wi\u00b7der\u00b7spro\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo bleibt, da\u00df alles niemand thut", "tokens": ["Wo", "bleibt", ",", "da\u00df", "al\u00b7les", "nie\u00b7mand", "thut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "PIS", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wann etwas wird zerbrochen,", "tokens": ["Wann", "et\u00b7was", "wird", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verlohren, liederlich entwand,", "tokens": ["Ver\u00b7loh\u00b7ren", ",", "lie\u00b7der\u00b7lich", "ent\u00b7wand", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gelogen, au\u00dfgetragen,", "tokens": ["Ge\u00b7lo\u00b7gen", ",", "au\u00df\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Versaltzt, versotten und verbrandt,", "tokens": ["Ver\u00b7saltzt", ",", "ver\u00b7sot\u00b7ten", "und", "ver\u00b7brandt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd wer kan alles sagen?", "tokens": ["Vnd", "wer", "kan", "al\u00b7les", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Kranckheit-H\u00e4ndel lass' ich seyn,", "tokens": ["Die", "Kranck\u00b7heit\u00b7H\u00e4n\u00b7del", "lass'", "ich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die einen offt beschweren,", "tokens": ["Die", "ei\u00b7nen", "offt", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd bittre Thr\u00e4nen f\u00fcr den Wein", "tokens": ["Vnd", "bitt\u00b7re", "Thr\u00e4\u00b7nen", "f\u00fcr", "den", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Frewden uns gewehren,", "tokens": ["Der", "Frew\u00b7den", "uns", "ge\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mehr wenn ein Kind sich legen mu\u00df,", "tokens": ["Mehr", "wenn", "ein", "Kind", "sich", "le\u00b7gen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch offt wol gar verbleichen?", "tokens": ["Auch", "offt", "wol", "gar", "ver\u00b7blei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd wer wei\u00df alle den Verdru\u00df,", "tokens": ["Vnd", "wer", "wei\u00df", "al\u00b7le", "den", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die H\u00e4ndel zu erreichen?", "tokens": ["Die", "H\u00e4n\u00b7del", "zu", "er\u00b7rei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Noch m\u00fcssen Euch die H\u00e4ndel nicht", "tokens": ["Noch", "m\u00fcs\u00b7sen", "Euch", "die", "H\u00e4n\u00b7del", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Liebes-Handel legen,", "tokens": ["Den", "Lie\u00b7bes\u00b7Han\u00b7del", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herr H\u00e4ndel. Amors Vnterricht", "tokens": ["Herr", "H\u00e4n\u00b7del", ".", "A\u00b7mors", "Vn\u00b7ter\u00b7richt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$.", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan Ewer Hertz bewegen,", "tokens": ["Kan", "E\u00b7wer", "Hertz", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df Ihr auch wollt bi\u00df durch den Tod", "tokens": ["Da\u00df", "Ihr", "auch", "wollt", "bi\u00df", "durch", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Ewrer Liebe dringen", "tokens": ["Mit", "Ew\u00b7rer", "Lie\u00b7be", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd lasst Euch keiner Zeiten Noht", "tokens": ["Vnd", "lasst", "Euch", "kei\u00b7ner", "Zei\u00b7ten", "Noht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Ewren Vorsatz zwingen.", "tokens": ["In", "Ew\u00b7ren", "Vor\u00b7satz", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So macht denn H\u00e4ndel, wie Ihr wollt,", "tokens": ["So", "macht", "denn", "H\u00e4n\u00b7del", ",", "wie", "Ihr", "wollt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "NN", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wil nicht widersprechen,", "tokens": ["Ich", "wil", "nicht", "wi\u00b7der\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd seyd best\u00e4ndig, als Ihr sollt,", "tokens": ["Vnd", "seyd", "be\u00b7st\u00e4n\u00b7dig", ",", "als", "Ihr", "sollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er\u00e4ugt sich wo gebrechen:", "tokens": ["Er\u00b7\u00e4ugt", "sich", "wo", "ge\u00b7bre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PWAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Macht Euch das Gl\u00fcck der H\u00e4ndel viel,", "tokens": ["Macht", "Euch", "das", "Gl\u00fcck", "der", "H\u00e4n\u00b7del", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Macht durch Gebeht und Trawen", "tokens": ["Macht", "durch", "Ge\u00b7beht", "und", "Tra\u00b7wen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Dem Himmel H\u00e4ndel auch ohn Ziel,", "tokens": ["Dem", "Him\u00b7mel", "H\u00e4n\u00b7del", "auch", "ohn", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr werdet Segen schawen.", "tokens": ["Ihr", "wer\u00b7det", "Se\u00b7gen", "scha\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Was H\u00e4ndel nehmt Ihr in den Sinn,", "tokens": ["Was", "H\u00e4n\u00b7del", "nehmt", "Ihr", "in", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr H\u00e4ndel, dieser Zeiten?", "tokens": ["Herr", "H\u00e4n\u00b7del", ",", "die\u00b7ser", "Zei\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir haben H\u00e4ndel gnug vorhin,", "tokens": ["Wir", "ha\u00b7ben", "H\u00e4n\u00b7del", "gnug", "vor\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man kan sie kaum bestreiten: