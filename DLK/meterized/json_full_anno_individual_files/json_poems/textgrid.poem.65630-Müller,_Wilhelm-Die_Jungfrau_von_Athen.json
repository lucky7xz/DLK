{"textgrid.poem.65630": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Jungfrau von Athen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rosenstr\u00e4uche th\u00e4t ich pflanzen unter meinem Fensterlein,", "tokens": ["Ro\u00b7sen\u00b7str\u00e4u\u00b7che", "th\u00e4t", "ich", "pflan\u00b7zen", "un\u00b7ter", "mei\u00b7nem", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und sie bl\u00fchen und sie duften in die Kammer mir herein;", "tokens": ["Und", "sie", "bl\u00fc\u00b7hen", "und", "sie", "duf\u00b7ten", "in", "die", "Kam\u00b7mer", "mir", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und die Nachtigallen singen in den Zweigen Lieb' und Lust \u2013", "tokens": ["Und", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "sin\u00b7gen", "in", "den", "Zwei\u00b7gen", "Lieb'", "und", "Lust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Schweigt, ihr V\u00f6glein, noch ein Weilchen! \u2013 Ist es euch denn nicht bewu\u00dft,", "tokens": ["Schweigt", ",", "ihr", "V\u00f6\u00b7glein", ",", "noch", "ein", "Weil\u00b7chen", "!", "\u2013", "Ist", "es", "euch", "denn", "nicht", "be\u00b7wu\u00dft", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "ART", "NN", "$.", "$(", "VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Da\u00df mein Liebster ist gezogen in das Feld mit Lanz' und Schwert,", "tokens": ["Da\u00df", "mein", "Liebs\u00b7ter", "ist", "ge\u00b7zo\u00b7gen", "in", "das", "Feld", "mit", "Lanz'", "und", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "F\u00fcr das heil'ge Kreuz zu k\u00e4mpfen und f\u00fcr einen freien Herd?", "tokens": ["F\u00fcr", "das", "heil'\u00b7ge", "Kreuz", "zu", "k\u00e4mp\u00b7fen", "und", "f\u00fcr", "ei\u00b7nen", "frei\u00b7en", "Herd", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Saht ihr nicht, wie ich vom Halse meine Perlenschn\u00fcre band,", "tokens": ["Saht", "ihr", "nicht", ",", "wie", "ich", "vom", "Hal\u00b7se", "mei\u00b7ne", "Per\u00b7len\u00b7schn\u00fc\u00b7re", "band", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Und sie gab dem heil'gen Priester f\u00fcr das liebe Vaterland?", "tokens": ["Und", "sie", "gab", "dem", "heil'\u00b7gen", "Pries\u00b7ter", "f\u00fcr", "das", "lie\u00b7be", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Saht ihr nicht, da\u00df meine Haare ich seit Monden nicht geschm\u00fcckt?", "tokens": ["Saht", "ihr", "nicht", ",", "da\u00df", "mei\u00b7ne", "Haa\u00b7re", "ich", "seit", "Mon\u00b7den", "nicht", "ge\u00b7schm\u00fcckt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "PPER", "APPR", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Saht ihr wohl, da\u00df eine Rose ich so lange hier gepfl\u00fcckt?", "tokens": ["Saht", "ihr", "wohl", ",", "da\u00df", "ei\u00b7ne", "Ro\u00b7se", "ich", "so", "lan\u00b7ge", "hier", "ge\u00b7pfl\u00fcckt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Schweigt, ihr V\u00f6glein, noch ein Weilchen, bis der Liebste wiederkehrt,", "tokens": ["Schweigt", ",", "ihr", "V\u00f6\u00b7glein", ",", "noch", "ein", "Weil\u00b7chen", ",", "bis", "der", "Liebs\u00b7te", "wie\u00b7der\u00b7kehrt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Und uns neue, sch\u00f6ne Weisen zu der Freiheit Preise lehrt.", "tokens": ["Und", "uns", "neu\u00b7e", ",", "sch\u00f6\u00b7ne", "Wei\u00b7sen", "zu", "der", "Frei\u00b7heit", "Prei\u00b7se", "lehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "$,", "ADJA", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Bl\u00fcht, ihr Rosen, noch ein Weilchen, und ich bind' euch mir zum Kranz,", "tokens": ["Bl\u00fcht", ",", "ihr", "Ro\u00b7sen", ",", "noch", "ein", "Weil\u00b7chen", ",", "und", "ich", "bind'", "euch", "mir", "zum", "Kranz", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Wann den Siegern wir entgegen ziehn mit Sang und Spiel und Tanz!", "tokens": ["Wann", "den", "Sie\u00b7gern", "wir", "ent\u00b7ge\u00b7gen", "ziehn", "mit", "Sang", "und", "Spiel", "und", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "PTKVZ", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "---+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Ach, und kehrtest du, mein Liebster, mit den Andern nicht zur\u00fcck,", "tokens": ["Ach", ",", "und", "kehr\u00b7test", "du", ",", "mein", "Liebs\u00b7ter", ",", "mit", "den", "An\u00b7dern", "nicht", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "wor