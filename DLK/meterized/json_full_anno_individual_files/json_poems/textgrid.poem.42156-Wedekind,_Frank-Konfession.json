{"textgrid.poem.42156": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Konfession", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freudig schw\u00f6r ich es mit jedem Schwure", "tokens": ["Freu\u00b7dig", "schw\u00f6r", "ich", "es", "mit", "je\u00b7dem", "Schwu\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Vor der Allmacht, die mich z\u00fcchtigen kann;", "tokens": ["Vor", "der", "All\u00b7macht", ",", "die", "mich", "z\u00fcch\u00b7ti\u00b7gen", "kann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wie viel lieber w\u00e4r ich eine Hure", "tokens": ["Wie", "viel", "lie\u00b7ber", "w\u00e4r", "ich", "ei\u00b7ne", "Hu\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als an Ruhm und Gl\u00fcck der reichste Mann!", "tokens": ["Als", "an", "Ruhm", "und", "Gl\u00fcck", "der", "reichs\u00b7te", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Welt, in mir ging dir ein Weib verloren,", "tokens": ["Welt", ",", "in", "mir", "ging", "dir", "ein", "Weib", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Abgekl\u00e4rt und jeder Hemmung bar.", "tokens": ["Ab\u00b7ge\u00b7kl\u00e4rt", "und", "je\u00b7der", "Hem\u00b7mung", "bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wer war f\u00fcr den Liebesmarkt geboren", "tokens": ["Wer", "war", "f\u00fcr", "den", "Lie\u00b7bes\u00b7markt", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So wie ich daf\u00fcr geboren war?", "tokens": ["So", "wie", "ich", "da\u00b7f\u00fcr", "ge\u00b7bo\u00b7ren", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PAV", "VVPP", "VAFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Lebt ich nicht der Liebe treu ergeben", "tokens": ["Lebt", "ich", "nicht", "der", "Lie\u00b7be", "treu", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie es andre ihrem Handwerk sind?", "tokens": ["Wie", "es", "and\u00b7re", "ih\u00b7rem", "Hand\u00b7werk", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Liebt ich nur ein einzig Mal im Leben", "tokens": ["Liebt", "ich", "nur", "ein", "ein\u00b7zig", "Mal", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Irgendein bestimmtes Menschenkind?", "tokens": ["Ir\u00b7gend\u00b7ein", "be\u00b7stimm\u00b7tes", "Men\u00b7schen\u00b7kind", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Lieben? \u2013 Nein, das bringt kein Gl\u00fcck auf Erden.", "tokens": ["Lie\u00b7ben", "?", "\u2013", "Nein", ",", "das", "bringt", "kein", "Gl\u00fcck", "auf", "Er\u00b7den", "."], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$.", "$(", "PTKANT", "$,", "PDS", "VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lieben bringt Entw\u00fcrdigung und Neid.", "tokens": ["Lie\u00b7ben", "bringt", "Ent\u00b7w\u00fcr\u00b7di\u00b7gung", "und", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hei\u00df und oft und stark geliebt zu werden,", "tokens": ["Hei\u00df", "und", "oft", "und", "stark", "ge\u00b7liebt", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "KON", "ADJD", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das hei\u00dft Leben, das ist Seligkeit!", "tokens": ["Das", "hei\u00dft", "Le\u00b7ben", ",", "das", "ist", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,", "PDS", "VAFIN", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Oder sollte Schamgef\u00fchl mich hindern,", "tokens": ["O\u00b7der", "soll\u00b7te", "Scham\u00b7ge\u00b7f\u00fchl", "mich", "hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn sich erste Jugendkraft verliert,", "tokens": ["Wenn", "sich", "ers\u00b7te", "Ju\u00b7gend\u00b7kraft", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jeden noch so seltnen Schmerz zu lindern,", "tokens": ["Je\u00b7den", "noch", "so", "selt\u00b7nen", "Schmerz", "zu", "lin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADV", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Den verwegne Phantasie gebiert?", "tokens": ["Den", "ver\u00b7weg\u00b7ne", "Phan\u00b7ta\u00b7sie", "ge\u00b7biert", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Schamgef\u00fchl? \u2013 Ich hab es oft empfunden;", "tokens": ["Scham\u00b7ge\u00b7f\u00fchl", "?", "\u2013", "Ich", "hab", "es", "oft", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schamgef\u00fchl nach mancher edlen Tat;", "tokens": ["Scham\u00b7ge\u00b7f\u00fchl", "nach", "man\u00b7cher", "ed\u00b7len", "Tat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schamgef\u00fchl vor Klagen und vor Wunden;", "tokens": ["Scham\u00b7ge\u00b7f\u00fchl", "vor", "Kla\u00b7gen", "und", "vor", "Wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Scham, wenn endlich sich Belohnung naht.", "tokens": ["Scham", ",", "wenn", "end\u00b7lich", "sich", "Be\u00b7loh\u00b7nung", "naht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "PRF", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Aber Schamgef\u00fchl des K\u00f6rpers wegen,", "tokens": ["A\u00b7ber", "Scham\u00b7ge\u00b7f\u00fchl", "des", "K\u00f6r\u00b7pers", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "APPR", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der mit Wonnen \u00fcberreich begabt?", "tokens": ["Der", "mit", "Won\u00b7nen", "\u00fc\u00b7berr\u00b7eich", "be\u00b7gabt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Solch ein Undank hat mir fern gelegen,", "tokens": ["Solch", "ein", "Un\u00b7dank", "hat", "mir", "fern", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seit mich einst der erste Ku\u00df gelabt!", "tokens": ["Seit", "mich", "einst", "der", "ers\u00b7te", "Ku\u00df", "ge\u00b7labt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Und ein Leib, vom Scheitel bis zur Sohle", "tokens": ["Und", "ein", "Leib", ",", "vom", "Schei\u00b7tel", "bis", "zur", "Soh\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Allerw\u00e4rts als Hochgenu\u00df begehrt ...", "tokens": ["Al\u00b7ler\u00b7w\u00e4rts", "als", "Hoch\u00b7ge\u00b7nu\u00df", "be\u00b7gehrt", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Welchem reinern, k\u00f6stlichern Idole", "tokens": ["Wel\u00b7chem", "rei\u00b7nern", ",", "k\u00f6st\u00b7li\u00b7chern", "I\u00b7do\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nachzustreben, ist dies Dasein wert?", "tokens": ["Nach\u00b7zu\u00b7stre\u00b7ben", ",", "ist", "dies", "Da\u00b7sein", "wert", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PDS", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Wenn der Knie leiseste Bewegung", "tokens": ["Wenn", "der", "Knie", "lei\u00b7ses\u00b7te", "Be\u00b7we\u00b7gung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Krafterzeugend wirkt wie Feuersglut,", "tokens": ["Kraf\u00b7ter\u00b7zeu\u00b7gend", "wirkt", "wie", "Feu\u00b7ers\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Kraft, aus wonniger Erregung", "tokens": ["Und", "die", "Kraft", ",", "aus", "won\u00b7ni\u00b7ger", "Er\u00b7re\u00b7gung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sich zu \u00fcberbieten, nicht mehr ruht;", "tokens": ["Sich", "zu", "\u00fc\u00b7berb\u00b7ie\u00b7ten", ",", "nicht", "mehr", "ruht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Immer unverw\u00fcstlicher und s\u00fc\u00dfer,", "tokens": ["Im\u00b7mer", "un\u00b7ver\u00b7w\u00fcst\u00b7li\u00b7cher", "und", "s\u00fc\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Immer klarer im Genu\u00df geschaut,", "tokens": ["Im\u00b7mer", "kla\u00b7rer", "im", "Ge\u00b7nu\u00df", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df es statt vor Ohnmacht dem Genie\u00dfer", "tokens": ["Da\u00df", "es", "statt", "vor", "Ohn\u00b7macht", "dem", "Ge\u00b7nie\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Nur vor seiner Riesenst\u00e4rke graut ...", "tokens": ["Nur", "vor", "sei\u00b7ner", "Rie\u00b7sen\u00b7st\u00e4r\u00b7ke", "graut", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Welt, wenn ich von solchem Zauber tr\u00e4ume,", "tokens": ["Welt", ",", "wenn", "ich", "von", "sol\u00b7chem", "Zau\u00b7ber", "tr\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dann zerstiebt zu nichts, was ich getan;", "tokens": ["Dann", "zer\u00b7stiebt", "zu", "nichts", ",", "was", "ich", "ge\u00b7tan", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIS", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dann preis ich das Dasein und ich b\u00e4ume", "tokens": ["Dann", "preis", "ich", "das", "Da\u00b7sein", "und", "ich", "b\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "PPER", "VVFIN"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu den Sternen mich vor Gr\u00f6\u00dfenwahn! \u2013 \u2013 \u2013", "tokens": ["Zu", "den", "Ster\u00b7nen", "mich", "vor", "Gr\u00f6\u00b7\u00dfen\u00b7wahn", "!", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "APPR", "NN", "$.", "$(", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Unrecht w\u00e4r's, wollt ich der Welt verhehlen,", "tokens": ["Un\u00b7recht", "w\u00e4r's", ",", "wollt", "ich", "der", "Welt", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was mein Innerstes so wild entflammt,", "tokens": ["Was", "mein", "In\u00b7ners\u00b7tes", "so", "wild", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn vom Beifall vieler braver Seelen", "tokens": ["Denn", "vom", "Bei\u00b7fall", "vie\u00b7ler", "bra\u00b7ver", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Frag ich mich umsonst, woraus er stammt.", "tokens": ["Frag", "ich", "mich", "um\u00b7sonst", ",", "wo\u00b7raus", "er", "stammt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Freudig schw\u00f6r ich es mit jedem Schwure", "tokens": ["Freu\u00b7dig", "schw\u00f6r", "ich", "es", "mit", "je\u00b7dem", "Schwu\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Vor der Allmacht, die mich z\u00fcchtigen kann;", "tokens": ["Vor", "der", "All\u00b7macht", ",", "die", "mich", "z\u00fcch\u00b7ti\u00b7gen", "kann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wie viel lieber w\u00e4r ich eine Hure", "tokens": ["Wie", "viel", "lie\u00b7ber", "w\u00e4r", "ich", "ei\u00b7ne", "Hu\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als an Ruhm und Gl\u00fcck der reichste Mann!", "tokens": ["Als", "an", "Ruhm", "und", "Gl\u00fcck", "der", "reichs\u00b7te", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Welt, in mir ging dir ein Weib verloren,", "tokens": ["Welt", ",", "in", "mir", "ging", "dir", "ein", "Weib", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Abgekl\u00e4rt und jeder Hemmung bar.", "tokens": ["Ab\u00b7ge\u00b7kl\u00e4rt", "und", "je\u00b7der", "Hem\u00b7mung", "bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wer war f\u00fcr den Liebesmarkt geboren", "tokens": ["Wer", "war", "f\u00fcr", "den", "Lie\u00b7bes\u00b7markt", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So wie ich daf\u00fcr geboren war?", "tokens": ["So", "wie", "ich", "da\u00b7f\u00fcr", "ge\u00b7bo\u00b7ren", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PAV", "VVPP", "VAFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.15": {"line.1": {"text": "Lebt ich nicht der Liebe treu ergeben", "tokens": ["Lebt", "ich", "nicht", "der", "Lie\u00b7be", "treu", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie es andre ihrem Handwerk sind?", "tokens": ["Wie", "es", "and\u00b7re", "ih\u00b7rem", "Hand\u00b7werk", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Liebt ich nur ein einzig Mal im Leben", "tokens": ["Liebt", "ich", "nur", "ein", "ein\u00b7zig", "Mal", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Irgendein bestimmtes Menschenkind?", "tokens": ["Ir\u00b7gend\u00b7ein", "be\u00b7stimm\u00b7tes", "Men\u00b7schen\u00b7kind", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.16": {"line.1": {"text": "Lieben? \u2013 Nein, das bringt kein Gl\u00fcck auf Erden.", "tokens": ["Lie\u00b7ben", "?", "\u2013", "Nein", ",", "das", "bringt", "kein", "Gl\u00fcck", "auf", "Er\u00b7den", "."], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$.", "$(", "PTKANT", "$,", "PDS", "VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "l