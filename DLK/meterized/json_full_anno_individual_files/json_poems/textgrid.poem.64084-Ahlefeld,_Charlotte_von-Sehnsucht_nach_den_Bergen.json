{"textgrid.poem.64084": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht nach den Bergen", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf den Gipfeln erhabener Berge", "tokens": ["Auf", "den", "Gip\u00b7feln", "er\u00b7ha\u00b7be\u00b7ner", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "D\u00fcnken die Sorgen der Erde mir Zwerge,", "tokens": ["D\u00fcn\u00b7ken", "die", "Sor\u00b7gen", "der", "Er\u00b7de", "mir", "Zwer\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PPER", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Wenn sie im Thale als Riesen mir drohn.", "tokens": ["Wenn", "sie", "im", "Tha\u00b7le", "als", "Rie\u00b7sen", "mir", "drohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dort erheben die ernsten Gedanken", "tokens": ["Dort", "er\u00b7he\u00b7ben", "die", "erns\u00b7ten", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Sich \u00fcber des Schicksals dr\u00fcckende Schranken,", "tokens": ["Sich", "\u00fc\u00b7ber", "des", "Schick\u00b7sals", "dr\u00fc\u00b7cken\u00b7de", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Muthig dem Dunstkreis der Tiefe entflohn.", "tokens": ["Mut\u00b7hig", "dem", "Dunst\u00b7kreis", "der", "Tie\u00b7fe", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "O Ihr geliebten, Ihr herrlichen H\u00f6hen,", "tokens": ["O", "Ihr", "ge\u00b7lieb\u00b7ten", ",", "Ihr", "herr\u00b7li\u00b7chen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Werd' ich im Schmerz des Verlangens vergehen,", "tokens": ["Werd'", "ich", "im", "Schmerz", "des", "Ver\u00b7lan\u00b7gens", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Ehe mein Auge Euch wieder begr\u00fcsst? \u2013", "tokens": ["E\u00b7he", "mein", "Au\u00b7ge", "Euch", "wie\u00b7der", "be\u00b7gr\u00fcs\u00b7st", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wenn ich auf \u00f6der und formloser Haide", "tokens": ["Wenn", "ich", "auf", "\u00f6\u00b7der", "und", "form\u00b7lo\u00b7ser", "Hai\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Einsam die Qualen der Sehnsucht erleide,", "tokens"