{"dta.poem.8323": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Anrede  \n an die sp\u00e4ten Herbstblumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Kinder der Natur, die ihr so sp\u00e4t\nerscheint,", "tokens": ["Ihr", "Kin\u00b7der", "der", "Na\u00b7tur", ",", "die", "ihr", "so", "sp\u00e4t", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da man den Untergang des Pflan-", "tokens": ["Da", "man", "den", "Un\u00b7ter\u00b7gang", "des", "Pflan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum last ihr euch noch bei st\u00fcr-", "tokens": ["Wa\u00b7rum", "last", "ihr", "euch", "noch", "bei", "st\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des rauhend kalten Nords, beim", "tokens": ["Des", "rau\u00b7hend", "kal\u00b7ten", "Nords", ",", "beim"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jhr kommet kaum hervor, so seid ihr schon geplagt,", "tokens": ["Ihr", "kom\u00b7met", "kaum", "her\u00b7vor", ",", "so", "seid", "ihr", "schon", "ge\u00b7plagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr bl\u00fcht seid gestern erst, und stehet schon verzagt,", "tokens": ["Ihr", "bl\u00fcht", "seid", "ge\u00b7stern", "erst", ",", "und", "ste\u00b7het", "schon", "ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VAFIN", "ADV", "ADV", "$,", "KON", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und last das schlaffe Haupt der matten Bl\u00e4tter", "tokens": ["Und", "last", "das", "schlaf\u00b7fe", "Haupt", "der", "mat\u00b7ten", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mir deucht als wolt ihr uns mit euren Kopfe win-", "tokens": ["Mir", "deucht", "als", "wolt", "ihr", "uns", "mit", "eu\u00b7ren", "Kop\u00b7fe", "win"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df wir eur Elend sehn. Jhr alle kommet mir,", "tokens": ["Da\u00df", "wir", "eur", "E\u00b7lend", "sehn", ".", "Ihr", "al\u00b7le", "kom\u00b7met", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$.", "PPER", "PIS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als zarte Kinderchen, die kaum gebohren f\u00fcr,", "tokens": ["Als", "zar\u00b7te", "Kin\u00b7der\u00b7chen", ",", "die", "kaum", "ge\u00b7boh\u00b7ren", "f\u00fcr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "PRELS", "ADV", "VVPP", "APPR", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.11": {"text": "Und da ihr kaum aufbl\u00fcht, so m\u00fcst ihr euch in", "tokens": ["Und", "da", "ihr", "kaum", "auf\u00b7bl\u00fcht", ",", "so", "m\u00fcst", "ihr", "euch", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,", "ADV", "VMFIN", "PPER", "PRF", "APPR"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Nach euren Untergang schon wieder r\u00fckwerts sehnen.", "tokens": ["Nach", "eu\u00b7ren", "Un\u00b7ter\u00b7gang", "schon", "wie\u00b7der", "r\u00fck\u00b7werts", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jhr seid ein sch\u00f6nes Bild von unsrer Eitelkeit,", "tokens": ["Ihr", "seid", "ein", "sch\u00f6\u00b7nes", "Bild", "von", "uns\u00b7rer", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gebuhrt und Leben, Todt sind auch bei uns nicht", "tokens": ["Ge\u00b7buhrt", "und", "Le\u00b7ben", ",", "Todt", "sind", "auch", "bei", "uns", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "NN", "$,", "NN", "VAFIN", "ADV", "APPR", "PPER", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Der Anfang ist kaum da; so folget auch das Ende,", "tokens": ["Der", "An\u00b7fang", "ist", "kaum", "da", ";", "so", "fol\u00b7get", "auch", "das", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$.", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da faulet wiederum des Leibes Kunstgeb\u00e4nde.", "tokens": ["Da", "fau\u00b7let", "wie\u00b7de\u00b7rum", "des", "Lei\u00b7bes", "Kunst\u00b7ge\u00b7b\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der feuchte Herbst macht euch mit vielen Regen na\u00df,", "tokens": ["Der", "feuch\u00b7te", "Herbst", "macht", "euch", "mit", "vie\u00b7len", "Re\u00b7gen", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Wind der saugt euch aus, und macht euch bleich", "tokens": ["Der", "Wind", "der", "saugt", "euch", "aus", ",", "und", "macht", "euch", "bleich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und euer Anfang ist, der Anfang zu den Sterben,", "tokens": ["Und", "eu\u00b7er", "An\u00b7fang", "ist", ",", "der", "An\u00b7fang", "zu", "den", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Jhr bl\u00fchet sp\u00e4te auf, und m\u00fcsset fr\u00fch verderben,", "tokens": ["Ihr", "bl\u00fc\u00b7het", "sp\u00e4\u00b7te", "auf", ",", "und", "m\u00fcs\u00b7set", "fr\u00fch", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PTKVZ", "$,", "KON", "VMFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wir Menschen sehn euch an, nicht ohn Bewunde-", "tokens": ["Wir", "Men\u00b7schen", "sehn", "euch", "an", ",", "nicht", "ohn", "Be\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PTKNEG", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wir klagen da\u00df ihr seid so voller Aenderung,", "tokens": ["Wir", "kla\u00b7gen", "da\u00df", "ihr", "seid", "so", "vol\u00b7ler", "A\u00b7en\u00b7de\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Allein ihr rufft uns zu: beklaget euch nur selber,", "tokens": ["Al\u00b7lein", "ihr", "rufft", "uns", "zu", ":", "be\u00b7kla\u00b7get", "euch", "nur", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Eur erster Lebens-Schritt geht in die Sterbgew\u00f6l-", "tokens": ["Eur", "ers\u00b7ter", "Le\u00b7bens\u00b7Schritt", "geht", "in", "die", "Sterb\u00b7ge\u00b7w\u00f6l"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Eur erster Lebenshauch den ihr so seufzend zieht,", "tokens": ["Eur", "ers\u00b7ter", "Le\u00b7bens\u00b7hauch", "den", "ihr", "so", "seuf\u00b7zend", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Zeigt wie eur Athem dreinst mit Schmerzen aus euch", "tokens": ["Zeigt", "wie", "eur", "A\u00b7them", "dreinst", "mit", "Schmer\u00b7zen", "aus", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "PPOSAT", "NN", "ADV", "APPR", "NN", "APPR", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "Jhr weinet wenn ihr komt, ihr weinet bei dem Schei-", "tokens": ["Ihr", "wei\u00b7net", "wenn", "ihr", "komt", ",", "ihr", "wei\u00b7net", "bei", "dem", "Schei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Bei eures Lebens-Lust habt ihr auch vieles Leiden.", "tokens": ["Bei", "eu\u00b7res", "Le\u00b7bens\u00b7Lust", "habt", "ihr", "auch", "vie\u00b7les", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jm Reiche der Natur sind wir der lezte Rest,", "tokens": ["Jm", "Rei\u00b7che", "der", "Na\u00b7tur", "sind", "wir", "der", "lez\u00b7te", "Rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ART", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der vor der Winterszeit sich bl\u00fchend sehen l\u00e4st,", "tokens": ["Der", "vor", "der", "Win\u00b7ter\u00b7szeit", "sich", "bl\u00fc\u00b7hend", "se\u00b7hen", "l\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der Herbst raft uns hinweg, da wir erst sind ge-", "tokens": ["Der", "Herbst", "raft", "uns", "hin\u00b7weg", ",", "da", "wir", "erst", "sind", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APZR", "$,", "KOUS", "PPER", "ADV", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.32": {"text": "Jhr alle seid ja auch wie wir zum Tod erkohren.", "tokens": ["Ihr", "al\u00b7le", "seid", "ja", "auch", "wie", "wir", "zum", "Tod", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "ADV", "ADV", "KOKOM", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der Fr\u00fchling liefert euch in eurem ersten Jahr", "tokens": ["Der", "Fr\u00fch\u00b7ling", "lie\u00b7fert", "euch", "in", "eu\u00b7rem", "ers\u00b7ten", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Auch ofte wenn ihr lebt, zur schwarzen Todten-", "tokens": ["Auch", "of\u00b7te", "wenn", "ihr", "lebt", ",", "zur", "schwar\u00b7zen", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "PPER", "VVFIN", "$,", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Ich dachte noch dabei, so kan man allzeit sehen,", "tokens": ["Ich", "dach\u00b7te", "noch", "da\u00b7bei", ",", "so", "kan", "man", "all\u00b7zeit", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$,", "ADV", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wie wir den Blumen gleich, im Bl\u00fchen und Ver-", "tokens": ["Wie", "wir", "den", "Blu\u00b7men", "gleich", ",", "im", "Bl\u00fc\u00b7hen", "und", "Ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "$,", "APPRART", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}