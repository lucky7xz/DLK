{"dta.poem.5342": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "N\u00fctzliche Blumen-Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beschau, o Mensch, der Blumen-Pracht!", "tokens": ["Be\u00b7schau", ",", "o", "Mensch", ",", "der", "Blu\u00b7men\u00b7Pracht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nimm was in ihnen steckt in acht!", "tokens": ["Nimm", "was", "in", "ih\u00b7nen", "steckt", "in", "acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRELS", "APPR", "PPER", "VVFIN", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man kann von ihres Sch\u00f6pffers Macht", "tokens": ["Man", "kann", "von", "ih\u00b7res", "Sch\u00f6pf\u00b7fers", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nachricht in gef\u00e4rbten Lettern,", "tokens": ["Die", "Nach\u00b7richt", "in", "ge\u00b7f\u00e4rb\u00b7ten", "Let\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf ihren zart- und bunten Bl\u00e4ttern,", "tokens": ["Auf", "ih\u00b7ren", "zar\u00b7t", "und", "bun\u00b7ten", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In nett-verschrenckten Z\u00fcgen lesen.", "tokens": ["In", "net\u00b7tver\u00b7schren\u00b7ck\u00b7ten", "Z\u00fc\u00b7gen", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Um seine Weisheit zu erheben,", "tokens": ["Um", "sei\u00b7ne", "Weis\u00b7heit", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Scheint jede sich recht zu bestreben,", "tokens": ["Scheint", "je\u00b7de", "sich", "recht", "zu", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mit bunten Fingern ihn zu weisen,", "tokens": ["Mit", "bun\u00b7ten", "Fin\u00b7gern", "ihn", "zu", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mit stillem Munde GOtt zu preisen.", "tokens": ["Mit", "stil\u00b7lem", "Mun\u00b7de", "Gott", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Es lobt den Sch\u00f6pfer der Natur", "tokens": ["Es", "lobt", "den", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bald ihre zierliche Figur,", "tokens": ["Bald", "ih\u00b7re", "zier\u00b7li\u00b7che", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Bald ihre Farbe, bald ihr Gr\u00fcn,", "tokens": ["Bald", "ih\u00b7re", "Far\u00b7be", ",", "bald", "ihr", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und will uns gerne zu sich ziehn;", "tokens": ["Und", "will", "uns", "ger\u00b7ne", "zu", "sich", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Bald qvillet ein ambrirter Rauch,", "tokens": ["Bald", "qvil\u00b7let", "ein", "am\u00b7br\u00b7ir\u00b7ter", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Bald f\u00e4hrt ein angew\u00fcrtzter Hauch", "tokens": ["Bald", "f\u00e4hrt", "ein", "an\u00b7ge\u00b7w\u00fcrtz\u00b7ter", "Hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Aus ihrer Feuer-reichen Brust,", "tokens": ["Aus", "ih\u00b7rer", "Feu\u00b7er\u00b7rei\u00b7chen", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ver\u00e4ndert unsrer Augen Lust", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "uns\u00b7rer", "Au\u00b7gen", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und suchet den Geruch zu n\u00e4hren,", "tokens": ["Und", "su\u00b7chet", "den", "Ge\u00b7ruch", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und uns zu gleicher Zeit zu lehren,", "tokens": ["Und", "uns", "zu", "glei\u00b7cher", "Zeit", "zu", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Da\u00df nimmer von ihr selber nicht", "tokens": ["Da\u00df", "nim\u00b7mer", "von", "ihr", "sel\u00b7ber", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Sie so vortreflich riechen k\u00f6nne,", "tokens": ["Sie", "so", "vor\u00b7tre\u00b7flich", "rie\u00b7chen", "k\u00f6n\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und ein so kr\u00e4ftig Rauchwercks-Licht", "tokens": ["Und", "ein", "so", "kr\u00e4f\u00b7tig", "Rauchwercks\u00b7Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJD", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.24": {"text": "In ihr nicht von sich selber brenne.", "tokens": ["In", "ihr", "nicht", "von", "sich", "sel\u00b7ber", "bren\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Nun ist und riecht die Blume sch\u00f6n,", "tokens": ["Nun", "ist", "und", "riecht", "die", "Blu\u00b7me", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die Seele kann sie riechen, sehn;", "tokens": ["Die", "See\u00b7le", "kann", "sie", "rie\u00b7chen", ",", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Will sie sich denn nicht auch bestreben,", "tokens": ["Will", "sie", "sich", "denn", "nicht", "auch", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Die Kraft, die ihr geschenckt, das Dencken,", "tokens": ["Die", "Kraft", ",", "die", "ihr", "ge\u00b7schenckt", ",", "das", "Den\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "In Lust und Danck auf den zu lencken,", "tokens": ["In", "Lust", "und", "Danck", "auf", "den", "zu", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Der beiden Seyn und Kraft gegeben?", "tokens": ["Der", "bei\u00b7den", "Seyn", "und", "Kraft", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}