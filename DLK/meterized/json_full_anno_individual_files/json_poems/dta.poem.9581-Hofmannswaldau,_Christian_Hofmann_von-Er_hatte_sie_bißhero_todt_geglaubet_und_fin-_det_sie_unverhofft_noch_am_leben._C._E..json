{"dta.poem.9581": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er hatte sie bi\u00dfhero todt geglaubet/ und fin-  \n det sie unverhofft noch am leben.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jst di\u00df nicht die gestalt der trefflichsten Marinthen/", "tokens": ["Ist", "di\u00df", "nicht", "die", "ge\u00b7stalt", "der", "treff\u00b7lichs\u00b7ten", "Ma\u00b7rin\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die ich bi\u00dfher so hoch betrauret und beweint?", "tokens": ["Die", "ich", "bi\u00df\u00b7her", "so", "hoch", "be\u00b7trau\u00b7ret", "und", "be\u00b7weint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie geht di\u00df immer zu? wie ists mit mir gemeint?", "tokens": ["Wie", "geht", "di\u00df", "im\u00b7mer", "zu", "?", "wie", "ists", "mit", "mir", "ge\u00b7meint", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDS", "ADV", "PTKVZ", "$.", "PWAV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df sind die augen ja/ die mich so schnell entz\u00fcndten/", "tokens": ["Di\u00df", "sind", "die", "au\u00b7gen", "ja", "/", "die", "mich", "so", "schnell", "ent\u00b7z\u00fcnd\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$(", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und mit verborgner krafft mein lodrend hertz ergr\u00fcndten/", "tokens": ["Und", "mit", "ver\u00b7borg\u00b7ner", "krafft", "mein", "lod\u00b7rend", "hertz", "er\u00b7gr\u00fcnd\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Di\u00df ist der mund/ der gleich den vollen rosen scheint/", "tokens": ["Di\u00df", "ist", "der", "mund", "/", "der", "gleich", "den", "vol\u00b7len", "ro\u00b7sen", "scheint", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die wangen/ welchen schnee und liljen \u00e4hnlich seynd;", "tokens": ["Die", "wan\u00b7gen", "/", "wel\u00b7chen", "schnee", "und", "lil\u00b7jen", "\u00e4hn\u00b7lich", "seynd", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAT", "NN", "KON", "VVFIN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Di\u00df sind die h\u00e4nde/ die so manche kunst ersinnten.", "tokens": ["Di\u00df", "sind", "die", "h\u00e4n\u00b7de", "/", "die", "so", "man\u00b7che", "kunst", "er\u00b7sinn\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "$(", "ART", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie? oder fehl ich auch? gewi\u00df ich glaub es schier;", "tokens": ["Wie", "?", "o\u00b7der", "fehl", "ich", "auch", "?", "ge\u00b7wi\u00df", "ich", "glaub", "es", "schier", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VVFIN", "PPER", "ADV", "$.", "ADV", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch was? sie ist es doch! ich seh sie ja vor mir/", "tokens": ["Doch", "was", "?", "sie", "ist", "es", "doch", "!", "ich", "seh", "sie", "ja", "vor", "mir", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$.", "PPER", "VAFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Marinthe ist es selbst! o angenehme stunden!", "tokens": ["Ma\u00b7rin\u00b7the", "ist", "es", "selbst", "!", "o", "an\u00b7ge\u00b7neh\u00b7me", "stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$.", "FM", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nein! nein! sie ist nicht todt! o h\u00f6chst-begl\u00fcckter tag!", "tokens": ["Nein", "!", "nein", "!", "sie", "ist", "nicht", "todt", "!", "o", "h\u00f6chst\u00b7be\u00b7gl\u00fcck\u00b7ter", "tag", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PTKANT", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da ich/ Marinthe/ dich nun wieder schauen mag/", "tokens": ["Da", "ich", "/", "Ma\u00b7rin\u00b7the", "/", "dich", "nun", "wie\u00b7der", "schau\u00b7en", "mag", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "NE", "$(", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und so nach langer nacht dich/ meine sonne/ funden.", "tokens": ["Und", "so", "nach", "lan\u00b7ger", "nacht", "dich", "/", "mei\u00b7ne", "son\u00b7ne", "/", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "PPER", "$(", "PPOSAT", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}