{"textgrid.poem.59449": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Unbekannte", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Dorfe schon die Abendglocken klangen,", "tokens": ["Vom", "Dor\u00b7fe", "schon", "die", "A\u00b7bend\u00b7glo\u00b7cken", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die m\u00fcden V\u00f6glein gingen auch zur Ruh,", "tokens": ["Die", "m\u00fc\u00b7den", "V\u00f6\u00b7glein", "gin\u00b7gen", "auch", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur auf den Wiesen noch die Heimchen sangen", "tokens": ["Nur", "auf", "den", "Wie\u00b7sen", "noch", "die", "Heim\u00b7chen", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und von den Bergen rauscht' der Wald dazu;", "tokens": ["Und", "von", "den", "Ber\u00b7gen", "rauscht'", "der", "Wald", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da kam ein Wandrer durch die \u00c4hrenwogen,", "tokens": ["Da", "kam", "ein", "Wand\u00b7rer", "durch", "die", "\u00c4h\u00b7ren\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus fernen Landen schien er hergezogen.", "tokens": ["Aus", "fer\u00b7nen", "Lan\u00b7den", "schien", "er", "her\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vor seinem Hause, unter bl\u00fchnden Lauben", "tokens": ["Vor", "sei\u00b7nem", "Hau\u00b7se", ",", "un\u00b7ter", "bl\u00fchn\u00b7den", "Lau\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lud ihn ein Mann zum fr\u00f6hl'chen Rasten ein,", "tokens": ["Lud", "ihn", "ein", "Mann", "zum", "fr\u00f6hl'\u00b7chen", "Ras\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die junge Frau bracht Wein und Brot und Trauben,", "tokens": ["Die", "jun\u00b7ge", "Frau", "bracht", "Wein", "und", "Brot", "und", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Setzt dann, umspielt vom letzten Abendschein,", "tokens": ["Setzt", "dann", ",", "um\u00b7spielt", "vom", "letz\u00b7ten", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sich neben ihn und blickt halb scheu, halb lose,", "tokens": ["Sich", "ne\u00b7ben", "ihn", "und", "blickt", "halb", "scheu", ",", "halb", "lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "KON", "VVFIN", "ADJD", "ADJD", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein lockig Kn\u00e4blein l\u00e4chelnd auf dem Scho\u00dfe.", "tokens": ["Ein", "lo\u00b7ckig", "Kn\u00e4\u00b7blein", "l\u00e4\u00b7chelnd", "auf", "dem", "Scho\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr d\u00fcnkt, er w\u00e4r schon einst im Dorf gewesen,", "tokens": ["Ihr", "d\u00fcnkt", ",", "er", "w\u00e4r", "schon", "einst", "im", "Dorf", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADV", "APPRART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch so fremd und seltsam war die Tracht,", "tokens": ["Und", "doch", "so", "fremd", "und", "selt\u00b7sam", "war", "die", "Tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In seinen Mienen feur'ge Schrift zu lesen", "tokens": ["In", "sei\u00b7nen", "Mie\u00b7nen", "feur'\u00b7ge", "Schrift", "zu", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gleich Wetterleuchten fern bei stiller Nacht,", "tokens": ["Gleich", "Wet\u00b7ter\u00b7leuch\u00b7ten", "fern", "bei", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und traf sein Auge sie, wollt ihr fast grauen,", "tokens": ["Und", "traf", "sein", "Au\u00b7ge", "sie", ",", "wollt", "ihr", "fast", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "$,", "VMFIN", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Denn 's war, wie in den Himmelsgrund zu schauen.", "tokens": ["Denn", "'s", "war", ",", "wie", "in", "den", "Him\u00b7mels\u00b7grund", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "PWAV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Und wie sich k\u00fchler nun die Schatten breiten:", "tokens": ["Und", "wie", "sich", "k\u00fch\u00b7ler", "nun", "die", "Schat\u00b7ten", "brei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "ADJD", "ADV", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Berg Vesuv, der \u00fcber Tr\u00fcmmern raucht,", "tokens": ["Vom", "Berg", "Ve\u00b7suv", ",", "der", "\u00fc\u00b7ber", "Tr\u00fcm\u00b7mern", "raucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom blauen Meer, wo Schw\u00e4ne singend gleiten,", "tokens": ["Vom", "blau\u00b7en", "Meer", ",", "wo", "Schw\u00e4\u00b7ne", "sin\u00b7gend", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kristallnen Inseln, bl\u00fchend draus getaucht,", "tokens": ["Kris\u00b7tall\u00b7nen", "In\u00b7seln", ",", "bl\u00fc\u00b7hend", "draus", "ge\u00b7taucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Glocken, die im Meeresgrunde schlagen,", "tokens": ["Und", "Glo\u00b7cken", ",", "die", "im", "Mee\u00b7res\u00b7grun\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wu\u00dft wunderbar der sch\u00f6ne Gast zu sagen.", "tokens": ["Wu\u00dft", "wun\u00b7der\u00b7bar", "der", "sch\u00f6\u00b7ne", "Gast", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbhast viel erfahren, willst du ewig wandern?\u00ab", "tokens": ["\u00bb", "hast", "viel", "er\u00b7fah\u00b7ren", ",", "willst", "du", "e\u00b7wig", "wan\u00b7dern", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ADV", "VVINF", "$,", "VMFIN", "PPER", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sprach drauf sein Wirt mit herzlichem Vertraun,", "tokens": ["Sprach", "drauf", "sein", "Wirt", "mit", "herz\u00b7li\u00b7chem", "Ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbhier kannst du froh genie\u00dfen wie die andern,", "tokens": ["\u00bb", "hier", "kannst", "du", "froh", "ge\u00b7nie\u00b7\u00dfen", "wie", "die", "an\u00b7dern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "KOKOM", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am eignen Herd dein kleines G\u00e4rtchen baun,", "tokens": ["Am", "eig\u00b7nen", "Herd", "dein", "klei\u00b7nes", "G\u00e4rt\u00b7chen", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Nachbars T\u00f6chter haben reiche Truhen,", "tokens": ["Des", "Nach\u00b7bars", "T\u00f6ch\u00b7ter", "ha\u00b7ben", "rei\u00b7che", "Tru\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ruh endlich aus, brauchst nicht allein zu ruhen.\u00ab", "tokens": ["Ruh", "end\u00b7lich", "aus", ",", "brauchst", "nicht", "al\u00b7lein", "zu", "ru\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$,", "VVFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da stand der Wandrer auf, es bl\u00fchten Sterne", "tokens": ["Da", "stand", "der", "Wand\u00b7rer", "auf", ",", "es", "bl\u00fch\u00b7ten", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon aus dem Dunkel \u00fcberm stillen Land,", "tokens": ["Schon", "aus", "dem", "Dun\u00b7kel", "\u00fc\u00b7berm", "stil\u00b7len", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbgesegn euch Gott! mein Heimatland liegt ferne. \u2013\u00ab", "tokens": ["\u00bb", "ge\u00b7segn", "euch", "Gott", "!", "mein", "Hei\u00b7mat\u00b7land", "liegt", "fer\u00b7ne", ".", "\u2013", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVPP", "PPER", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und als er von den beiden sich gewandt,", "tokens": ["Und", "als", "er", "von", "den", "bei\u00b7den", "sich", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "PIAT", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kam himmlisch Klingen von der Waldeswiese \u2013", "tokens": ["Kam", "himm\u00b7lisch", "Klin\u00b7gen", "von", "der", "Wal\u00b7des\u00b7wie\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "