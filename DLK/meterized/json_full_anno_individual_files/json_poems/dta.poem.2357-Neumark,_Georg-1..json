{"dta.poem.2357": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.85", "no:0.14"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Zjon klaget aus der ma\u00dfen", "tokens": ["Zjon", "kla\u00b7get", "aus", "der", "ma\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zion die Betr\u00fcbte Die:", "tokens": ["Zi\u00b7on", "die", "Be\u00b7tr\u00fcb\u00b7te", "Die", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach der HErr hat mich verla\u00dfen/\nUnd vergisset meiner hiel", "tokens": ["Ach", "der", "Herr", "hat", "mich", "ver\u00b7la\u00b7\u00dfen", "/", "Und", "ver\u00b7gis\u00b7set", "mei\u00b7ner", "hiel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ART", "NN", "VAFIN", "PPER", "VVINF", "$(", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Gott lest mich fast untersinken/\nUnd im Ungl\u00fckssee ertrinken/", "tokens": ["Gott", "lest", "mich", "fast", "un\u00b7ter\u00b7sin\u00b7ken", "/", "Und", "im", "Un\u00b7gl\u00fcks\u00b7see", "er\u00b7trin\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVINF", "$(", "KON", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Lest mich stekken in der Pein/\nUnd wil nicht gen\u00e4dig seyn.", "tokens": ["Lest", "mich", "stek\u00b7ken", "in", "der", "Pein", "/", "Und", "wil", "nicht", "ge\u00b7n\u00e4\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$(", "KON", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich kan mich der hei\u00dfen Z\u00e4hren/", "tokens": ["Ich", "kan", "mich", "der", "hei\u00b7\u00dfen", "Z\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Meiner Augen herben Fluht/\nUnd des Weinens nicht erwehren/", "tokens": ["Mei\u00b7ner", "Au\u00b7gen", "her\u00b7ben", "Fluht", "/", "Und", "des", "Wei\u00b7nens", "nicht", "er\u00b7weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$(", "KON", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "So ist mein gequ\u00e4lter Muht", "tokens": ["So", "ist", "mein", "ge\u00b7qu\u00e4l\u00b7ter", "Muht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Betr\u00fcbn\u00fcs \u00fcberschwommen/", "tokens": ["Mit", "Be\u00b7tr\u00fcb\u00b7n\u00fcs", "\u00fc\u00b7bersc\u00b7hwom\u00b7men", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df mein Hertz mit Angst beklommen/", "tokens": ["Da\u00df", "mein", "Hertz", "mit", "Angst", "be\u00b7klom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "T\u00e4glich seuftzet/ \u00e4chtzt/ und schreiht:", "tokens": ["T\u00e4g\u00b7lich", "seuft\u00b7zet", "/", "\u00e4chtzt", "/", "und", "schreiht", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$(", "VVFIN", "$(", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo bleibt Gottes G\u00fctigkeit?", "tokens": ["Wo", "bleibt", "Got\u00b7tes", "G\u00fc\u00b7tig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kan ein Weib sich wol verbo\u00dfen/", "tokens": ["Kan", "ein", "Weib", "sich", "wol", "ver\u00b7bo\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So/ da\u00df Sie ihr liebes Kind", "tokens": ["So", "/", "da\u00df", "Sie", "ihr", "lie\u00b7bes", "Kind"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Solt' im Zorne gantz verstossen/\nUnd so grimmig seyn gesinnt/", "tokens": ["Solt'", "im", "Zor\u00b7ne", "gantz", "ver\u00b7stos\u00b7sen", "/", "Und", "so", "grim\u00b7mig", "seyn", "ge\u00b7sinnt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "ADV", "VVPP", "$(", "KON", "ADV", "ADJD", "VAINF", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df Sie sich nicht solt' erbarmen", "tokens": ["Da\u00df", "Sie", "sich", "nicht", "solt'", "er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es wieder\u00fcm \u00fcmarmen/", "tokens": ["Und", "es", "wie\u00b7de\u00b7r\u00fcm", "\u00fcm\u00b7ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn es seine H\u00e4ndchen ringt/\nUnd Jhr Hertz mit Thr\u00e4nen zwingt.", "tokens": ["Wenn", "es", "sei\u00b7ne", "H\u00e4nd\u00b7chen", "ringt", "/", "Und", "Ihr", "Hertz", "mit", "Thr\u00e4\u00b7nen", "zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$(", "KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Wenn es liegt auf seinen Kniehen/", "tokens": ["Wenn", "es", "liegt", "auf", "sei\u00b7nen", "Knie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und bekennet seine Schuld/", "tokens": ["Und", "be\u00b7ken\u00b7net", "sei\u00b7ne", "Schuld", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein! wie kan Sie sich entziehen", "tokens": ["Mein", "!", "wie", "kan", "Sie", "sich", "ent\u00b7zie\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "$.", "PWAV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df Sie Jhm nicht werde Huld?", "tokens": ["Da\u00df", "Sie", "Jhm", "nicht", "wer\u00b7de", "Huld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kan auch eine Mutter neiden/", "tokens": ["Kan", "auch", "ei\u00b7ne", "Mut\u00b7ter", "nei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das/ was Sie mit schwerem Leiden/", "tokens": ["Das", "/", "was", "Sie", "mit", "schwe\u00b7rem", "Lei\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit grosser Schmertzensmacht", "tokens": ["Und", "mit", "gros\u00b7ser", "Schmert\u00b7zens\u00b7macht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat aufdiese Welt gebracht.", "tokens": ["Hat", "auf\u00b7die\u00b7se", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch gesetzt/ da\u00df Jhr Gebl\u00fcte", "tokens": ["Doch", "ge\u00b7setzt", "/", "da\u00df", "Ihr", "Ge\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "$(", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So sehr unvers\u00f6hnlich bleibt/", "tokens": ["So", "sehr", "un\u00b7ver\u00b7s\u00f6hn\u00b7lich", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Jhr zorniges Gem\u00fchte/", "tokens": ["Und", "Ihr", "zor\u00b7ni\u00b7ges", "Ge\u00b7m\u00fch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solche Tyranney betreibt/", "tokens": ["Sol\u00b7che", "Ty\u00b7ran\u00b7ney", "be\u00b7treibt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So wil Ich doch Deiner d\u00e4nken/", "tokens": ["So", "wil", "Ich", "doch", "Dei\u00b7ner", "d\u00e4n\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und Mich G\u00fctig zu Dir lenken.", "tokens": ["Und", "Mich", "G\u00fc\u00b7tig", "zu", "Dir", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.7": {"text": "Deines Hertzens hei\u00dfes Flehn", "tokens": ["Dei\u00b7nes", "Hert\u00b7zens", "hei\u00b7\u00dfes", "Flehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hab' Ich gn\u00e4dig angesehn.", "tokens": ["Hab'", "Ich", "gn\u00e4\u00b7dig", "an\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df mein Kind von deinem Zagen/", "tokens": ["La\u00df", "mein", "Kind", "von", "dei\u00b7nem", "Za\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zion/ du Geliebte du/", "tokens": ["Zi\u00b7on", "/", "du", "Ge\u00b7lieb\u00b7te", "du", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "VVFIN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6r nur auf von deinem Klagen/", "tokens": ["H\u00f6r", "nur", "auf", "von", "dei\u00b7nem", "Kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stell dein traurigs Hertz zur Ruh/", "tokens": ["Stell", "dein", "trau\u00b7rigs", "Hertz", "zur", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sey geduldig/ meine Liebe/", "tokens": ["Sey", "ge\u00b7dul\u00b7dig", "/", "mei\u00b7ne", "Lie\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und dich nicht so sehr betr\u00fcbe/", "tokens": ["Und", "dich", "nicht", "so", "sehr", "be\u00b7tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ADV", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn dem Kreutz/ und die\u00df dein Leid/", "tokens": ["Denn", "dem", "Kreutz", "/", "und", "die\u00df", "dein", "Leid", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "KON", "PDS", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dienet dir zur Seeligkeit.", "tokens": ["Die\u00b7net", "dir", "zur", "See\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sey versichert meiner Gnaden/", "tokens": ["Sey", "ver\u00b7si\u00b7chert", "mei\u00b7ner", "Gna\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Traue meiner gro\u00dfen Macht/", "tokens": ["Trau\u00b7e", "mei\u00b7ner", "gro\u00b7\u00dfen", "Macht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn Ich seh' all deinen Schaden/", "tokens": ["Denn", "Ich", "seh'", "all", "dei\u00b7nen", "Scha\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich bin schon darauf bedacht/", "tokens": ["Ich", "bin", "schon", "da\u00b7rauf", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "VVPP", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da\u00df Ich werd' ein Mittel finden/", "tokens": ["Da\u00df", "Ich", "werd'", "ein", "Mit\u00b7tel", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich de\u00df Drangsals zu entbinden.", "tokens": ["Dich", "de\u00df", "Drangs\u00b7als", "zu", "ent\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Den\u0303 du bist mein werthes Pfand/", "tokens": ["De\u00f1", "du", "bist", "mein", "wert\u00b7hes", "Pfand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Eingeschrieben meiner Hand.", "tokens": ["Ein\u00b7ge\u00b7schrie\u00b7ben", "mei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}