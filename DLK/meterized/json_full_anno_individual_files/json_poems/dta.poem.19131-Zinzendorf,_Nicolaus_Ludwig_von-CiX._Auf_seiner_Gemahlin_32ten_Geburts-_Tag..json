{"dta.poem.19131": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CiX.  Auf seiner Gemahlin 32ten Geburts-  \n Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn man glauben solte,", "tokens": ["Wenn", "man", "glau\u00b7ben", "sol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Da\u00df die klugen Tichter,", "tokens": ["Da\u00df", "die", "klu\u00b7gen", "Tich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unsers Kampffes Splitter-Richter,", "tokens": ["Un\u00b7sers", "Kampf\u00b7fes", "Split\u00b7ter\u00b7Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Christi Creutz-Erk\u00e4nntni\u00df", "tokens": ["Chris\u00b7ti", "Creutz\u00b7Er\u00b7k\u00e4nnt\u00b7ni\u00df"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Uberseheu k\u00f6nnten,", "tokens": ["Ub\u00b7er\u00b7se\u00b7heu", "k\u00f6nn\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und sich billig weise nennten;", "tokens": ["Und", "sich", "bil\u00b7lig", "wei\u00b7se", "nenn\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00fcste man", "tokens": ["M\u00fcs\u00b7te", "man"], "token_info": ["word", "word"], "pos": ["VMFIN", "PIS"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Wohl den Plan", "tokens": ["Wohl", "den", "Plan"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Jhrer Kunst und Gaben", "tokens": ["Ih\u00b7rer", "Kunst", "und", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Nicht gesehen haben.", "tokens": ["Nicht", "ge\u00b7se\u00b7hen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ehmahls sprach ein Heyde", "tokens": ["Eh\u00b7mahls", "sprach", "ein", "Hey\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Von den Glaubens-Sachen:", "tokens": ["Von", "den", "Glau\u00b7bens\u00b7Sa\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Werdet ihr denn meiner lachen,", "tokens": ["Wer\u00b7det", "ihr", "denn", "mei\u00b7ner", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn mit diesem Leben", "tokens": ["Wenn", "mit", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Euer Geist verschwunden,", "tokens": ["Eu\u00b7er", "Geist", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wie ihr Weisen ausgefunden?", "tokens": ["Wie", "ihr", "Wei\u00b7sen", "aus\u00b7ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat mein Satz", "tokens": ["Hat", "mein", "Satz"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Aber Platz,", "tokens": ["A\u00b7ber", "Platz", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Wie wird euer Wagen", "tokens": ["Wie", "wird", "eu\u00b7er", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Meinen Spott ertragen?", "tokens": ["Mei\u00b7nen", "Spott", "er\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Christen m\u00f6chten weinen", "tokens": ["Chris\u00b7ten", "m\u00f6ch\u00b7ten", "wei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sterbliche zu kennen,", "tokens": ["Sterb\u00b7li\u00b7che", "zu", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Die sich Christen-Menschen nennen,", "tokens": ["Die", "sich", "Chris\u00b7ten\u00b7Men\u00b7schen", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die vor Christi Wahrheit,", "tokens": ["Die", "vor", "Chris\u00b7ti", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und dem Wort der Zeugen", "tokens": ["Und", "dem", "Wort", "der", "Zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sich mit Ehrerbietung neigen;", "tokens": ["Sich", "mit", "Ehr\u00b7er\u00b7bie\u00b7tung", "nei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fragt man nun,", "tokens": ["Fragt", "man", "nun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Was zu thun?", "tokens": ["Was", "zu", "thun", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Wissen sie die Lehren", "tokens": ["Wis\u00b7sen", "sie", "die", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Alle zu verkehren.", "tokens": ["Al\u00b7le", "zu", "ver\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Mit Privat-Personen,", "tokens": ["Mit", "Pri\u00b7vat\u00b7Per\u00b7so\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Die im \u00e4ussern Leben", "tokens": ["Die", "im", "\u00e4us\u00b7sern", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sich die Freyheit wolten geben;", "tokens": ["Sich", "die", "Frey\u00b7heit", "wol\u00b7ten", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So zusammen h\u00e4ngen,", "tokens": ["So", "zu\u00b7sam\u00b7men", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Allen Laut verlassen,", "tokens": ["Al\u00b7len", "Laut", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und dergleichen Schl\u00fcsse fassen:", "tokens": ["Und", "derg\u00b7lei\u00b7chen", "Schl\u00fcs\u00b7se", "fas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nein heist Ja!", "tokens": ["Nein", "heist", "Ja", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PTKANT", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Hier und da", "tokens": ["Hier", "und", "da"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KON", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "W\u00fcrden alle Knaben", "tokens": ["W\u00fcr\u00b7den", "al\u00b7le", "Kna\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Jhr Gesp\u00f6tte haben.", "tokens": ["Ihr", "Ge\u00b7sp\u00f6t\u00b7te", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "W\u00fcrden unter Menschen,", "tokens": ["W\u00fcr\u00b7den", "un\u00b7ter", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die zusammen wandeln,", "tokens": ["Die", "zu\u00b7sam\u00b7men", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und in einer Sprache handeln,", "tokens": ["Und", "in", "ei\u00b7ner", "Spra\u00b7che", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hundert tausend einig", "tokens": ["Hun\u00b7dert", "tau\u00b7send", "ei\u00b7nig"], "token_info": ["word", "word", "word"], "pos": ["CARD", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn sie reden solten", "tokens": ["Wenn", "sie", "re\u00b7den", "sol\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Da\u00df sies anders deuten wolten;", "tokens": ["Da\u00df", "sies", "an\u00b7ders", "deu\u00b7ten", "wol\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00f6chten sie", "tokens": ["M\u00f6ch\u00b7ten", "sie"], "token_info": ["word", "word"], "pos": ["VMFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Sich der M\u00fch,", "tokens": ["Sich", "der", "M\u00fch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Jhren Sinn zu sagen", "tokens": ["Ih\u00b7ren", "Sinn", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Nur zugleich entschlagen.", "tokens": ["Nur", "zu\u00b7gleich", "ent\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Aber, da\u00df die Seelen", "tokens": ["A\u00b7ber", ",", "da\u00df", "die", "See\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Eine Schrifft verdrehen", "tokens": ["Ei\u00b7ne", "Schrifft", "ver\u00b7dre\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eines, dem sie zugestehen,", "tokens": ["Ei\u00b7nes", ",", "dem", "sie", "zu\u00b7ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er sie vom Fluche", "tokens": ["Da\u00df", "er", "sie", "vom", "Flu\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mit sich selbst erkauffet,", "tokens": ["Mit", "sich", "selbst", "er\u00b7kauf\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wahrheit, drauff man alles tauffet:", "tokens": ["Wahr\u00b7heit", ",", "drauff", "man", "al\u00b7les", "tauf\u00b7fet", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PAV", "PIS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das kommt mir", "tokens": ["Das", "kommt", "mir"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Rassend f\u00fcr.", "tokens": ["Ras\u00b7send", "f\u00fcr", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "APPR", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Vor dergleichen Christen", "tokens": ["Vor", "derg\u00b7lei\u00b7chen", "Chris\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIS", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Lob ich Atheisten.", "tokens": ["Lob", "ich", "A\u00b7theis\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Weg mit diesen Bildern,", "tokens": ["Weg", "mit", "die\u00b7sen", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "So die Seelen qv\u00e4len,", "tokens": ["So", "die", "See\u00b7len", "qv\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die sich unsern HErrn erwehlen!", "tokens": ["Die", "sich", "un\u00b7sern", "Herrn", "er\u00b7weh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die grosse Frage:", "tokens": ["Auf", "die", "gros\u00b7se", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wo ist zum Exempel,", "tokens": ["Wo", "ist", "zum", "Ex\u00b7em\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auf der Welt ein Geistes-Tempel?", "tokens": ["Auf", "der", "Welt", "ein", "Geis\u00b7tes\u00b7Tem\u00b7pel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zeig\u2019 ich dich", "tokens": ["Zeig'", "ich", "dich"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "M\u00e4nniglich,", "tokens": ["M\u00e4n\u00b7nig\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Bild der wahren Ehe,", "tokens": ["Bild", "der", "wah\u00b7ren", "E\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Innigst-liebe Liebe!", "tokens": ["In\u00b7nigst\u00b7lie\u00b7be", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dein durchdringend Auge", "tokens": ["Dein", "durch\u00b7drin\u00b7gend", "Au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "VVPP", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sieht