{"textgrid.poem.37387": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um acht, als seine werte Sippe", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um acht, als seine werte Sippe", "tokens": ["Um", "acht", ",", "als", "sei\u00b7ne", "wer\u00b7te", "Sip\u00b7pe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch in den Federn schlummernd lag,", "tokens": ["Noch", "in", "den", "Fe\u00b7dern", "schlum\u00b7mernd", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begr\u00fc\u00dft er von der Felsenklippe", "tokens": ["Be\u00b7gr\u00fc\u00dft", "er", "von", "der", "Fel\u00b7sen\u00b7klip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bereits den neuen Fr\u00fchlingstag.", "tokens": ["Be\u00b7reits", "den", "neu\u00b7en", "Fr\u00fch\u00b7lings\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie die angenehme Sonne", "tokens": ["Und", "wie", "die", "an\u00b7ge\u00b7neh\u00b7me", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Liebreich zu ihm herniederschaut,", "tokens": ["Lieb\u00b7reich", "zu", "ihm", "her\u00b7nie\u00b7der\u00b7schaut", ","], "tok