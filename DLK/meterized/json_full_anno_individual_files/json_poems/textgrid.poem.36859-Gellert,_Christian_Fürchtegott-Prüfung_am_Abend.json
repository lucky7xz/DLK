{"textgrid.poem.36859": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Pr\u00fcfung am Abend", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag ist wieder hin, und diesen Teil des Lebens,", "tokens": ["Der", "Tag", "ist", "wie\u00b7der", "hin", ",", "und", "die\u00b7sen", "Teil", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,", "KON", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie hab ich ihn verbracht? Verstrich er mir vergebens?", "tokens": ["Wie", "hab", "ich", "ihn", "ver\u00b7bracht", "?", "Ver\u00b7strich", "er", "mir", "ver\u00b7ge\u00b7bens", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "VVPP", "$.", "ADJD", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hab ich mit allem Ernst dem Guten nachgestrebt?", "tokens": ["Hab", "ich", "mit", "al\u00b7lem", "Ernst", "dem", "Gu\u00b7ten", "nach\u00b7ge\u00b7strebt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PIS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hab ich vielleicht nur mir, nicht meiner Pflicht gelebt?", "tokens": ["Hab", "ich", "viel\u00b7leicht", "nur", "mir", ",", "nicht", "mei\u00b7ner", "Pflicht", "ge\u00b7lebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PPER", "$,", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "War's in der Furcht des Herrn, da\u00df ich ihn angefangen?", "tokens": ["Wa\u00b7r's", "in", "der", "Furcht", "des", "Herrn", ",", "da\u00df", "ich", "ihn", "an\u00b7ge\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit Dank und mit Gebet, mit eifrigem Verlangen,", "tokens": ["Mit", "Dank", "und", "mit", "Ge\u00b7bet", ",", "mit", "eif\u00b7ri\u00b7gem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als ein Gesch\u00f6pf von Gott der Tugend mich zu weihn,", "tokens": ["Als", "ein", "Ge\u00b7sch\u00f6pf", "von", "Gott", "der", "Tu\u00b7gend", "mich", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und z\u00fcchtig, und gerecht, und Gottes Freund zu sein?", "tokens": ["Und", "z\u00fcch\u00b7tig", ",", "und", "ge\u00b7recht", ",", "und", "Got\u00b7tes", "Freund", "zu", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KON", "ADJD", "$,", "KON", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hab ich in dem Beruf, den Gott mir angewiesen,", "tokens": ["Hab", "ich", "in", "dem", "Be\u00b7ruf", ",", "den", "Gott", "mir", "an\u00b7ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Eifer und durch Flei\u00df ihn, diesen Gott, gepriesen;", "tokens": ["Durch", "Ei\u00b7fer", "und", "durch", "Flei\u00df", "ihn", ",", "die\u00b7sen", "Gott", ",", "ge\u00b7prie\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PPER", "$,", "PDAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir und der Welt gen\u00fctzt, und jeden Dienst getan,", "tokens": ["Mir", "und", "der", "Welt", "ge\u00b7n\u00fctzt", ",", "und", "je\u00b7den", "Dienst", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "VVPP", "$,", "KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil ihn der Herr gebot, nicht weil mich Menschen sahn?", "tokens": ["Weil", "ihn", "der", "Herr", "ge\u00b7bot", ",", "nicht", "weil", "mich", "Men\u00b7schen", "sahn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PTKNEG", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie hab ich diesen Tag mein eigen Herz regieret?", "tokens": ["Wie", "hab", "ich", "die\u00b7sen", "Tag", "mein", "ei\u00b7gen", "Herz", "re\u00b7gie\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PDAT", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat mich im stillen oft ein Blick auf Gott ger\u00fchret?", "tokens": ["Hat", "mich", "im", "stil\u00b7len", "oft", "ein", "Blick", "auf", "Gott", "ge\u00b7r\u00fch\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erfreut ich mich des Herrn, der unser Flehn bemerkt?", "tokens": ["Er\u00b7freut", "ich", "mich", "des", "Herrn", ",", "der", "un\u00b7ser", "Flehn", "be\u00b7merkt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hab ich im Vertraun auf ihn mein Herz gest\u00e4rkt?", "tokens": ["Und", "hab", "ich", "im", "Ver\u00b7traun", "auf", "ihn", "mein", "Herz", "ge\u00b7st\u00e4rkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN", "APPR", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dacht ich bei dem Genu\u00df der G\u00fcter dieser Erden", "tokens": ["Dacht", "ich", "bei", "dem", "Ge\u00b7nu\u00df", "der", "G\u00fc\u00b7ter", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An den Allm\u00e4chtigen, durch den sie sind und werden?", "tokens": ["An", "den", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", ",", "durch", "den", "sie", "sind", "und", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "KON", "VAINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Verehrt ich ihn im Staub? Empfand ich seine Huld?", "tokens": ["Ver\u00b7ehrt", "ich", "ihn", "im", "Staub", "?", "Emp\u00b7fand", "ich", "sei\u00b7ne", "Huld", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "$.", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Trug ich das Gl\u00fcck mit Dank, den Unfall mit Geduld?", "tokens": ["Trug", "ich", "das", "Gl\u00fcck", "mit", "Dank", ",", "den", "Un\u00b7fall", "mit", "Ge\u00b7duld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "NN", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und wie geno\u00df mein Herz des Umgangs s\u00fc\u00dfe Stunden?", "tokens": ["Und", "wie", "ge\u00b7no\u00df", "mein", "Herz", "des", "Um\u00b7gangs", "s\u00fc\u00b7\u00dfe", "Stun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchlt ich der Freundschaft Gl\u00fcck, sprach ich, was ich empfunden?", "tokens": ["F\u00fchlt", "ich", "der", "Freund\u00b7schaft", "Gl\u00fcck", ",", "sprach", "ich", ",", "was", "ich", "emp\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "War auch mein Ernst noch sanft, mein Scherz noch unschuldsvoll?", "tokens": ["War", "auch", "mein", "Ernst", "noch", "sanft", ",", "mein", "Scherz", "noch", "un\u00b7schulds\u00b7voll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "$,", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hab ich nichts geredt, das ich bereuen soll?", "tokens": ["Und", "hab", "ich", "nichts", "ge\u00b7redt", ",", "das", "ich", "be\u00b7reu\u00b7en", "soll", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIS", "VVPP", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Hab ich die Meinigen durch Sorgfalt mir verpflichtet,", "tokens": ["Hab", "ich", "die", "Mei\u00b7ni\u00b7gen", "durch", "Sorg\u00b7falt", "mir", "ver\u00b7pflich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "PPOSS", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie durch mein Beispiel still zum Guten unterrichtet?", "tokens": ["Sie", "durch", "mein", "Bei\u00b7spiel", "still", "zum", "Gu\u00b7ten", "un\u00b7ter\u00b7rich\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "War zu des Mitleids Pflicht mein Herz nicht zu bequem?", "tokens": ["War", "zu", "des", "Mit\u00b7leids", "Pflicht", "mein", "Herz", "nicht", "zu", "be\u00b7quem", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "PPOSAT", "NN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Gl\u00fcck, das andre traf, war dies mir angenehm?", "tokens": ["Ein", "Gl\u00fcck", ",", "das", "and\u00b7re", "traf", ",", "war", "dies", "mir", "an\u00b7ge\u00b7nehm", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,", "VAFIN", "PDS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "War mir der Fehltritt leid, so bald ich ihn begangen?", "tokens": ["War", "mir", "der", "Fehl\u00b7tritt", "leid", ",", "so", "bald", "ich", "ihn", "be\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "$,", "ADV", "ADV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bestritt ich auch in mir ein unerlaubt Verlangen?", "tokens": ["Be\u00b7stritt", "ich", "auch", "in", "mir", "ein", "un\u00b7er\u00b7laubt", "Ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wenn in dieser Nacht Gott \u00fcber mich gebeut,", "tokens": ["Und", "wenn", "in", "die\u00b7ser", "Nacht", "Gott", "\u00fc\u00b7ber", "mich", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PDAT", "NN", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bin ich, vor ihm zu stehn, auch willig und bereit?", "tokens": ["Bin", "ich", ",", "vor", "ihm", "zu", "stehn", ",", "auch", "wil\u00b7lig", "und", "be\u00b7reit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Gott, der du alles wei\u00dft, was k\u00f6nnt ich dir verhehlen?", "tokens": ["Gott", ",", "der", "du", "al\u00b7les", "wei\u00dft", ",", "was", "k\u00f6nnt", "ich", "dir", "ver\u00b7heh\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PIS", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ich f\u00fchle t\u00e4glich noch die Schwachheit meiner Seelen.", "tokens": ["Ich", "f\u00fch\u00b7le", "t\u00e4g\u00b7lich", "noch", "die", "Schwach\u00b7heit", "mei\u00b7ner", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vergib durch Christi Blut mir die verletzte Pflicht;", "tokens": ["Ver\u00b7gib", "durch", "Chris\u00b7ti", "Blut", "mir", "die", "ver\u00b7letz\u00b7te", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NE", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vergib, und gehe du nicht mit mir ins Gericht.", "tokens": ["Ver\u00b7gib", ",", "und", "ge\u00b7he", "du", "nicht", "mit", "mir", "ins", "Ge\u00b7richt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Ja, du verzeihest dem, den seine S\u00fcnden kr\u00e4nken;", "tokens": ["Ja", ",", "du", "ver\u00b7zei\u00b7hest", "dem", ",", "den", "sei\u00b7ne", "S\u00fcn\u00b7den", "kr\u00e4n\u00b7ken", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du liebst Barmherzigkeit, und wirst auch mir sie schenken.", "tokens": ["Du", "liebst", "Barm\u00b7her\u00b7zig\u00b7keit", ",", "und", "wirst", "auch", "mir", "sie", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "VAFIN", "ADV", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch diese Nacht bist du der W\u00e4chter \u00fcber mir;", "tokens": ["A