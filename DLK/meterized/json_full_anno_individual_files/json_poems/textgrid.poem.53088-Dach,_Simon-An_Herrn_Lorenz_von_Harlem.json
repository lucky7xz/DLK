{"textgrid.poem.53088": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Lorenz von Harlem", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nvn ist ", "tokens": ["Nvn", "ist"], "token_info": ["word", "word"], "pos": ["NE", "VAFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Die Ihr die Singens-Kunst der wilden Barbarey", "tokens": ["Die", "Ihr", "die", "Sin\u00b7gens\u00b7Kunst", "der", "wil\u00b7den", "Bar\u00b7ba\u00b7rey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entreisset, vnd versteht, was an ", "tokens": ["E\u00b7ntreis\u00b7set", ",", "vnd", "ver\u00b7steht", ",", "was", "an"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "$,", "PRELS", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Ihr manch sch\u00f6nes Lied von Ihm wisst zu erzwingen.", "tokens": ["Wenn", "Ihr", "manch", "sch\u00f6\u00b7nes", "Lied", "von", "Ihm", "wisst", "zu", "er\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "APPR", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "O fahret fort, vnd lasst den thewren Mann noch singen!", "tokens": ["O", "fah\u00b7ret", "fort", ",", "vnd", "lasst", "den", "thew\u00b7ren", "Mann", "noch", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lang als er vns lebt, wird ", "tokens": ["So", "lang", "als", "er", "vns", "lebt", ",", "wird"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "PPER", "VVFIN", "$,", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "(es ist die Wahrheit ja, drumb red' ich auch so frey)", "tokens": ["(", "es", "ist", "die", "Wahr\u00b7heit", "ja", ",", "drumb", "red'", "ich", "auch", "so", "frey", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "PTKANT", "$,", "PAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcr, vnserm ", "tokens": ["F\u00fcr", ",", "vn\u00b7serm"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Gott tr\u00f6st vns nach der zeit. Eccarden rewet nicht", "tokens": ["Gott", "tr\u00f6st", "vns", "nach", "der", "zeit", ".", "Ec\u00b7car\u00b7den", "re\u00b7wet", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der M\u00fch', ", "tokens": ["Der", "M\u00fch'", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Was lieb- vnd k\u00fcnst-lichers ist, als was Er gemacht,", "tokens": ["Was", "lie\u00b7b", "vnd", "k\u00fcnst\u00b7li\u00b7chers", "ist", ",", "als", "was", "Er", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "TRUNC", "KON", "ADJA", "VAFIN", "$,", "KOUS", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn Er der Stimmen zwist in Eintracht hat gebracht?", "tokens": ["Wenn", "Er", "der", "Stim\u00b7men", "zwist", "in", "Ein\u00b7tracht", "hat", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vns d\u00fcncket da\u00df wir selbst der Engel Lieder h\u00f6ren.", "tokens": ["Vns", "d\u00fcn\u00b7cket", "da\u00df", "wir", "selbst", "der", "En\u00b7gel", "Lie\u00b7der", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nvn ist ", "tokens": ["Nvn", "ist"], "token_info": ["word", "word"], "pos": ["NE", "VAFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Die Ihr die Singens-Kunst der wilden Barbarey", "tokens": ["Die", "Ihr", "die", "Sin\u00b7gens\u00b7Kunst", "der", "wil\u00b7den", "Bar\u00b7ba\u00b7rey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entreisset, vnd versteht, was an ", "tokens": ["E\u00b7ntreis\u00b7set", ",", "vnd", "ver\u00b7steht", ",", "was", "an"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "$,", "PRELS", "APPR"], "m