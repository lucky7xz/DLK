{"textgrid.poem.39299": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn tiefe Trauer meine Seele kettet", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn tiefe Trauer meine Seele kettet", "tokens": ["Wenn", "tie\u00b7fe", "Trau\u00b7er", "mei\u00b7ne", "See\u00b7le", "ket\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An die Erinn'rung ", "tokens": ["An", "die", "Er\u00b7inn'\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "An hundert N\u00e4chte, schlummerlos durchwunden,", "tokens": ["An", "hun\u00b7dert", "N\u00e4ch\u00b7te", ",", "schlum\u00b7mer\u00b7los", "durch\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An Lager, die das Siechtum mir gebettet;", "tokens": ["An", "La\u00b7ger", ",", "die", "das", "Siech\u00b7tum", "mir", "ge\u00b7bet\u00b7tet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "An alles Gute, das ich hier verwettet,", "tokens": ["An", "al\u00b7les", "Gu\u00b7te", ",", "das", "ich", "hier", "ver\u00b7wet\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "