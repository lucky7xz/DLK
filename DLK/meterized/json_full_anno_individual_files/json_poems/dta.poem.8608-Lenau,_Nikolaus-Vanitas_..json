{"dta.poem.8608": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Vanitas   .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Eitles Trachten, eitles Ringen", "tokens": ["Eit\u00b7les", "Trach\u00b7ten", ",", "eit\u00b7les", "Rin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fri\u00dft dein bischen Leben auf,", "tokens": ["Fri\u00dft", "dein", "bi\u00b7schen", "Le\u00b7ben", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis die Abendglocken klingen,", "tokens": ["Bis", "die", "A\u00b7bend\u00b7glo\u00b7cken", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still dann steht der tolle Lauf.", "tokens": ["Still", "dann", "steht", "der", "tol\u00b7le", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gastlich bot dir auf der Reise", "tokens": ["Gast\u00b7lich", "bot", "dir", "auf", "der", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Natur ihr Heiligthum;", "tokens": ["Die", "Na\u00b7tur", "ihr", "Hei\u00b7lig\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch du st\u00e4ubtest fort im Gleise,", "tokens": ["Doch", "du", "st\u00e4ub\u00b7test", "fort", "im", "Glei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sahst nach ihr dich gar nicht um.", "tokens": ["Sahst", "nach", "ihr", "dich", "gar", "nicht", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PRF", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bl\u00fcthenduft und Nachtigallen,", "tokens": ["Bl\u00fc\u00b7then\u00b7duft", "und", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4dchenku\u00df und Freundeswort", "tokens": ["M\u00e4d\u00b7chen\u00b7ku\u00df", "und", "Freun\u00b7des\u00b7wort"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Riefen dich in ihre Hallen;", "tokens": ["Rie\u00b7fen", "dich", "in", "ih\u00b7re", "Hal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch du jagtest fort und fort.", "tokens": ["Doch", "du", "jag\u00b7test", "fort", "und", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Eine Th\u00f6rin dir zur Seite", "tokens": ["Ei\u00b7ne", "Th\u00f6\u00b7rin", "dir", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trieb mit dir ein arges Spiel,", "tokens": ["Trieb", "mit", "dir", "ein", "ar\u00b7ges", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wies dir stets ins graue Weite:", "tokens": ["Wies", "dir", "stets", "ins", "grau\u00b7e", "Wei\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201esiehst du, Freund, dort gl\u00e4nzt das Ziel!\u201c", "tokens": ["\u201e", "siehst", "du", ",", "Freund", ",", "dort", "gl\u00e4nzt", "das", "Ziel", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "War es Gold, war's Macht und Ehre,", "tokens": ["War", "es", "Gold", ",", "wa\u00b7r's", "Macht", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Was sie schmeichelnd dir verhie\u00df:", "tokens": ["Was", "sie", "schmei\u00b7chelnd", "dir", "ver\u00b7hie\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kunstgriff war's nur der Het\u00e4re,", "tokens": ["Kunst\u00b7griff", "wa\u00b7r's", "nur", "der", "He\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Eitel Tand ist das und die\u00df.", "tokens": ["Ei\u00b7tel", "Tand", "ist", "das", "und", "die\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ART", "KON", "PDS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sieh! noch winkt sie dir ins Weite,", "tokens": ["Sieh", "!", "noch", "winkt", "sie", "dir", "ins", "Wei\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und du wardst ein alter Knab!", "tokens": ["Und", "du", "wardst", "ein", "al\u00b7ter", "Knab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Nun entschl\u00fcpft dir dein Geleite,", "tokens": ["Nun", "ent\u00b7schl\u00fcpft", "dir", "dein", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du stehst allein \u2014 am Grab.", "tokens": ["Und", "du", "stehst", "al\u00b7lein", "am", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$(", "APPRART", "NN", "$."], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Kannst nicht trocknen mehr die Stirne,", "tokens": ["Kannst", "nicht", "trock\u00b7nen", "mehr", "die", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da du mit dem Tode ringst;", "tokens": ["Da", "du", "mit", "dem", "To\u00b7de", "ringst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rst nur ferne noch der Dirne", "tokens": ["H\u00f6rst", "nur", "fer\u00b7ne", "noch", "der", "Dir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hohngel\u00e4chter \u2014 und versinkst!", "tokens": ["Hohn\u00b7ge\u00b7l\u00e4ch\u00b7ter", "und", "ver\u00b7sinkst", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "KON", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}