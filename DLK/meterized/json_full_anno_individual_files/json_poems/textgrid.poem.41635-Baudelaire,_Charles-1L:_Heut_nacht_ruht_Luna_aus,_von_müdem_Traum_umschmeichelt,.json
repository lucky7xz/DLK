{"textgrid.poem.41635": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heut nacht ruht Luna aus, von m\u00fcdem Traum umschmeichelt,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut nacht ruht Luna aus, von m\u00fcdem Traum umschmeichelt,", "tokens": ["Heut", "nacht", "ruht", "Lu\u00b7na", "aus", ",", "von", "m\u00fc\u00b7dem", "Traum", "um\u00b7schmei\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie eine Sch\u00f6nheit sich in reiche Kissen schmiegt", "tokens": ["Wie", "ei\u00b7ne", "Sch\u00f6n\u00b7heit", "sich", "in", "rei\u00b7che", "Kis\u00b7sen", "schmiegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mit zerstreuter Hand hingleitend leise streichelt", "tokens": ["Und", "mit", "zer\u00b7streu\u00b7ter", "Hand", "hing\u00b7lei\u00b7tend", "lei\u00b7se", "strei\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Busens Linien, eh der Schlummer sie besiegt.", "tokens": ["Des", "Bu\u00b7sens", "Li\u00b7ni\u00b7en", ",", "eh", "der", "Schlum\u00b7mer", "sie", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+----+-+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Auf der Lawinen Pf\u00fchl, der gl\u00e4nzt in seidnem Lichte,", "tokens": ["Auf", "der", "La\u00b7wi\u00b7nen", "Pf\u00fchl", ",", "der", "gl\u00e4nzt", "in", "seid\u00b7nem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "L\u00e4\u00dft sie ersterbend sich in Ohnmacht untergehn", "tokens": ["L\u00e4\u00dft", "sie", "ers\u00b7ter\u00b7bend", "sich", "in", "Ohn\u00b7macht", "un\u00b7ter\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP", "PRF", "APPR", "NN", "VVINF"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und lenkt ihr Auge auf die wei\u00dfen Traumgesichte,", "tokens": ["Und", "lenkt", "ihr", "Au\u00b7ge", "auf", "die", "wei\u00b7\u00dfen", "Traum\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Bl\u00fctenkelchen gleich fern im Azur erstehn.", "tokens": ["Die", "Bl\u00fc\u00b7ten\u00b7kel\u00b7chen", "gleich", "fern", "im", "A\u00b7zur", "er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wann diesem Erdball sie, in ihrem m\u00fc\u00dfgen Sehnen,", "tokens": ["Wann", "die\u00b7sem", "Erd\u00b7ball", "sie", ",", "in", "ih\u00b7rem", "m\u00fc\u00df\u00b7gen", "Seh\u00b7nen", ","], "t