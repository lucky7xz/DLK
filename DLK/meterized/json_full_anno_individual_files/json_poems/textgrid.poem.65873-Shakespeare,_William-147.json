{"textgrid.poem.65873": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "147", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Lieben ist ein Fieber, es begehrt", "tokens": ["Mein", "Lie\u00b7ben", "ist", "ein", "Fie\u00b7ber", ",", "es", "be\u00b7gehrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ART", "NN", "$,", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur was die Krankheit fristet; all sein Sehnen", "tokens": ["Nur", "was", "die", "Krank\u00b7heit", "fris\u00b7tet", ";", "all", "sein", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PWS", "ART", "NN", "VVFIN", "$.", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geht auf den Zunder, der das \u00dcbel n\u00e4hrt,", "tokens": ["Geht", "auf", "den", "Zun\u00b7der", ",", "der", "das", "\u00dc\u00b7bel", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem kranken, launenhaften Reiz zu fr\u00f6nen.", "tokens": ["Dem", "kran\u00b7ken", ",", "lau\u00b7nen\u00b7haf\u00b7ten", "Reiz", "zu", "fr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vernunft, mein Liebesarzt, weil ich verschm\u00e4ht", "tokens": ["Ver\u00b7nunft", ",", "mein", "Lie\u00b7bes\u00b7arzt", ",", "weil", "ich", "ver\u00b