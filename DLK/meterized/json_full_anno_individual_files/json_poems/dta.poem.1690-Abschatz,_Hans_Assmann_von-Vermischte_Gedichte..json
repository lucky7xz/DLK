{"dta.poem.1690": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Tieffe See ist zu ergr\u00fcnden/", "tokens": ["Tief\u00b7fe", "See", "ist", "zu", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und des Himmels hohes Hau\u00df", "tokens": ["Und", "des", "Him\u00b7mels", "ho\u00b7hes", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Misst der kluge Stern-Witz aus;", "tokens": ["Misst", "der", "klu\u00b7ge", "Stern\u00b7Witz", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber wo sind die zu finden", "tokens": ["A\u00b7ber", "wo", "sind", "die", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VAFIN", "ART", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die des Frauenzimmers L\u00fcsten", "tokens": ["Die", "des", "Frau\u00b7en\u00b7zim\u00b7mers", "L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gr\u00fcndlich zu beschreiben w\u00fcsten?", "tokens": ["Gr\u00fcnd\u00b7lich", "zu", "be\u00b7schrei\u00b7ben", "w\u00fcs\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So die Worte was verneinen/", "tokens": ["So", "die", "Wor\u00b7te", "was", "ver\u00b7nei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Spricht das Hertze vielmahl ja/", "tokens": ["Spricht", "das", "Hert\u00b7ze", "viel\u00b7mahl", "ja", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVFIN", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und der gr\u00f6ste Sturm ist nah", "tokens": ["Und", "der", "gr\u00f6s\u00b7te", "Sturm", "ist", "nah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADJD"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Wenn die Augen heiter scheinen.", "tokens": ["Wenn", "die", "Au\u00b7gen", "hei\u00b7ter", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Gl\u00fccklich/ wer bey solchem Handeln", "tokens": ["Gl\u00fcck\u00b7lich", "/", "wer", "bey", "sol\u00b7chem", "Han\u00b7deln"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$(", "PWS", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Die Gedancken auch kan wandeln.", "tokens": ["Die", "Ge\u00b7dan\u00b7cken", "auch", "kan", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}