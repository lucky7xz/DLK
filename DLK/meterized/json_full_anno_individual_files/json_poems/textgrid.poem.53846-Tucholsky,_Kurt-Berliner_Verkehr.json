{"textgrid.poem.53846": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Berliner Verkehr", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bez\u00fcglich dem berliner Verkehr steht an jeder Ecke ein Mann, der m\u00fcllert", "tokens": ["Be\u00b7z\u00fcg\u00b7lich", "dem", "ber\u00b7li\u00b7ner", "Ver\u00b7kehr", "steht", "an", "je\u00b7der", "E\u00b7cke", "ein", "Mann", ",", "der", "m\u00fcl\u00b7lert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+--+--+-+--+--+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "und h\u00e4lt alle Autos und Kinderw\u00e4gen und Invaliden auf Rollen an.", "tokens": ["und", "h\u00e4lt", "al\u00b7le", "Au\u00b7tos", "und", "Kin\u00b7der\u00b7w\u00e4\u00b7gen", "und", "In\u00b7va\u00b7li\u00b7den", "auf", "Rol\u00b7len", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "KON", "NN", "KON", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+-+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wei\u00dfe Handschuhe heben sich, Lampen blinken, Signale blitzen,", "tokens": ["Wei\u00b7\u00dfe", "Hand\u00b7schu\u00b7he", "he\u00b7ben", "sich", ",", "Lam\u00b7pen", "blin\u00b7ken", ",", "Sig\u00b7na\u00b7le", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "$,", "NN", "VVINF", "$,", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "w\u00e4hrend gelangweilte Fahrg\u00e4ste in den Wagen sitzen,", "tokens": ["w\u00e4h\u00b7rend", "ge\u00b7lang\u00b7weil\u00b7te", "Fahr\u00b7g\u00e4s\u00b7te", "in", "den", "Wa\u00b7gen", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.5": {"text": "auch haben wir leuchtende Schildkr\u00f6ten, bitte sehr \u2013", "tokens": ["auch", "ha\u00b7ben", "wir", "leuch\u00b7ten\u00b7de", "Schild\u00b7kr\u00f6\u00b7ten", ",", "bit\u00b7te", "sehr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$,", "ADV", "ADV", "$("], "meter": "-+--+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "und das einzige, das uns noch fehlt, ist der Verkehr.", "tokens": ["und", "das", "ein\u00b7zi\u00b7ge", ",", "das", "uns", "noch", "fehlt", ",", "ist", "der", "Ver\u00b7kehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Aber wo nichts ist, haben nur S. M. der Kaiser das Recht verloren,", "tokens": ["A\u00b7ber", "wo", "nichts", "ist", ",", "ha\u00b7ben", "nur", "S.", "M.", "der", "Kai\u00b7ser", "das", "Recht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "abbreviation", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VAFIN", "$,", "VAFIN", "ADV", "NN", "NE", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+--+-+-", "measure": "iambic.septa.invert"}, "line.8": {"text": "nicht aber wir deutschen Organisatoren \u2013", "tokens": ["nicht", "a\u00b7ber", "wir", "deut\u00b7schen", "Or\u00b7ga\u00b7ni\u00b7sa\u00b7to\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPER", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "denn ist auch unser Wagenpark noch so klein:", "tokens": ["denn", "ist", "auch", "un\u00b7ser", "Wa\u00b7gen\u00b7park", "noch", "so", "klein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "organisiert mu\u00df sein.", "tokens": ["or\u00b7ga\u00b7ni\u00b7siert", "mu\u00df", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Es l\u00e4\u00dft uns nicht ruhen.", "tokens": ["Es", "l\u00e4\u00dft", "uns", "nicht", "ru\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Und genau so, wie wir dies tun,", "tokens": ["Und", "ge\u00b7nau", "so", ",", "wie", "wir", "dies", "tun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "PWAV", "PPER", "PDS", "VVINF", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "wie wir den nicht vorhandenen Verkehr in Klein-R\u00fclpsig und Gro\u00df-Berlin befestigen und organisieren,", "tokens": ["wie", "wir", "den", "nicht", "vor\u00b7han\u00b7de\u00b7nen", "Ver\u00b7kehr", "in", "Klein\u00b7R\u00fclp\u00b7sig", "und", "Gro\u00df\u00b7Ber\u00b7lin", "be\u00b7fes\u00b7ti\u00b7gen", "und", "or\u00b7ga\u00b7ni\u00b7sie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "PTKNEG", "ADJA", "NN", "APPR", "NE", "KON", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+---+--+--+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.14": {"text": "regieren", "tokens": ["re\u00b7gie\u00b7ren"], "token_info": ["word"], "pos": ["VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "wir im ganzen Lande umher.", "tokens": ["wir", "im", "gan\u00b7zen", "Lan\u00b7de", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Das deutsche Leben geh\u00f6rt dem Aktenverkehr.", "tokens": ["Das", "deut\u00b7sche", "Le\u00b7ben", "ge\u00b7h\u00f6rt", "dem", "Ak\u00b7ten\u00b7ver\u00b7kehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Wir organisieren Kleinkinder-G\u00e4rten und das Gro\u00df-Hamburger Hafenlogis,", "tokens": ["Wir", "or\u00b7ga\u00b7ni\u00b7sie\u00b7ren", "Klein\u00b7kin\u00b7der\u00b7G\u00e4r\u00b7ten", "und", "das", "Gro\u00df\u00b7Ham\u00b7bur\u00b7ger", "Ha\u00b7fen\u00b7lo\u00b7gis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+-+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.18": {"text": "den Radiumverbrauch auf dem Lande, den Weinbau in Ostpreu\u00dfen sowie", "tokens": ["den", "Ra\u00b7di\u00b7um\u00b7ver\u00b7brauch", "auf", "dem", "Lan\u00b7de", ",", "den", "Wein\u00b7bau", "in", "Ost\u00b7preu\u00b7\u00dfen", "so\u00b7wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "KON"], "meter": "-+-+-+--+--++-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.19": {"text": "Aufzucht von Ammen und Seidenraupen im Spreewald, auch desgleichen", "tokens": ["Auf\u00b7zucht", "von", "Am\u00b7men", "und", "Sei\u00b7den\u00b7rau\u00b7pen", "im", "Spree\u00b7wald", ",", "auch", "des\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPRART", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+--+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "den Wohnungsbau und die Reform der Aktenzeichen.", "tokens": ["den", "Woh\u00b7nungs\u00b7bau", "und", "die", "Re\u00b7form", "der", "Ak\u00b7ten\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sinn hat unser Tun keinen, in allen F\u00e4llen.", "tokens": ["Sinn", "hat", "un\u00b7ser", "Tun", "kei\u00b7nen", ",", "in", "al\u00b7len", "F\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "PIAT", "$,", "APPR", "PIAT", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Wir sind so, wie wir uns die Amerikaner vorstellen.", "tokens": ["Wir", "sind", "so", ",", "wie", "wir", "uns", "die", "A\u00b7me\u00b7ri\u00b7ka\u00b7ner", "vor\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text":