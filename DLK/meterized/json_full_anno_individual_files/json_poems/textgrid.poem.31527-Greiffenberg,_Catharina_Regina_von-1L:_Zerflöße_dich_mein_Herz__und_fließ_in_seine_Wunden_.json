{"textgrid.poem.31527": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zerfl\u00f6\u00dfe dich mein Herz / und flie\u00df in seine Wunden /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zerfl\u00f6\u00dfe dich mein Herz / und flie\u00df in seine Wunden /", "tokens": ["Zer\u00b7fl\u00f6\u00b7\u00dfe", "dich", "mein", "Herz", "/", "und", "flie\u00df", "in", "sei\u00b7ne", "Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$(", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "bey hei\u00dfer Liebesglut / f\u00fcll diese H\u00f6len zu.", "tokens": ["bey", "hei\u00b7\u00dfer", "Lie\u00b7bes\u00b7glut", "/", "f\u00fcll", "die\u00b7se", "H\u00f6\u00b7len", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "VVFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in seinem Schmerzen / such dein s\u00fc\u00dfe Seelen-Ruh:", "tokens": ["in", "sei\u00b7nem", "Schmer\u00b7zen", "/", "such", "dein", "s\u00fc\u00b7\u00dfe", "See\u00b7len\u00b7Ruh", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "VVIMP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "es wird der Himmel jetzt in diesen H\u00f6len funden.", "tokens": ["es", "wird", "der", "Him\u00b7mel", "jetzt", "in", "die\u00b7sen", "H\u00f6\u00b7len", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word