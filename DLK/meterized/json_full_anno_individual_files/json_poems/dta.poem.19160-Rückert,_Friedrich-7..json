{"dta.poem.19160": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchlingshimmel soll in Wolkend\u00fcnsten br\u00fcten,", "tokens": ["Der", "Fr\u00fch\u00b7lings\u00b7him\u00b7mel", "soll", "in", "Wol\u00b7ken\u00b7d\u00fcns\u00b7ten", "br\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis sich die Fr\u00fchlingsflur gef\u00fcllt mit Laub und Bl\u00fcten.", "tokens": ["Bis", "sich", "die", "Fr\u00fch\u00b7lings\u00b7flur", "ge\u00b7f\u00fcllt", "mit", "Laub", "und", "Bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n ist der blaue Raum, der wolkenlose, nur,", "tokens": ["Sch\u00f6n", "ist", "der", "blau\u00b7e", "Raum", ",", "der", "wol\u00b7ken\u00b7lo\u00b7se", ",", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "ADV", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wenn ihm entgegen bl\u00fcht die farbenreiche Flur.", "tokens": ["Wenn", "ihm", "ent\u00b7ge\u00b7gen", "bl\u00fcht", "die", "far\u00b7ben\u00b7rei\u00b7che", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch bis die Lebensf\u00fcll' erwacht im Erdgefild,", "tokens": ["Doch", "bis", "die", "Le\u00b7bens\u00b7f\u00fcll'", "er\u00b7wacht", "im", "Erd\u00b7ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei ein Ersatz mir ein fantastisch Wolkenbild.", "tokens": ["Sei", "ein", "Er\u00b7satz", "mir", "ein", "fan\u00b7tas\u00b7tisch", "Wol\u00b7ken\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}