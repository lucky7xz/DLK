{"dta.poem.11973": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Feuer brennt nicht hell an einem Scheit allein;", "tokens": ["Das", "Feu\u00b7er", "brennt", "nicht", "hell", "an", "ei\u00b7nem", "Scheit", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lerneifer z\u00fcndet erst sich an durch Lernverein.", "tokens": ["Lern\u00b7ei\u00b7fer", "z\u00fcn\u00b7det", "erst", "sich", "an", "durch", "Lern\u00b7ver\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PRF", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jemehr das K\u00e4lbchen saugt, jemehr das Euter quillt;", "tokens": ["Je\u00b7mehr", "das", "K\u00e4lb\u00b7chen", "saugt", ",", "je\u00b7mehr", "das", "Eu\u00b7ter", "quillt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je gr\u00f6\u00dfre Lernbegier, je lieber man sie stillt.", "tokens": ["Je", "gr\u00f6\u00df\u00b7re", "Lern\u00b7be\u00b7gier", ",", "je", "lie\u00b7ber", "man", "sie", "stillt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADV", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vom Lehrer fieng ich an, vom Mitgelehrten fuhr", "tokens": ["Vom", "Leh\u00b7rer", "fi\u00b7eng", "ich", "an", ",", "vom", "Mit\u00b7ge\u00b7lehr\u00b7ten", "fuhr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich fort zu lernen, aus lernt' ich vom Lehrling nur.", "tokens": ["Ich", "fort", "zu", "ler\u00b7nen", ",", "aus", "lernt'", "ich", "vom", "Lehr\u00b7ling", "nur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "PTKZU", "VVINF", "$,", "APPR", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}