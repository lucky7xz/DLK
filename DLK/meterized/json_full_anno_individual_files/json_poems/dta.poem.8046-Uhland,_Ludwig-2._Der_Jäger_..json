{"dta.poem.8046": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.  Der J\u00e4ger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich einsmals in den W\u00e4ldern", "tokens": ["Als", "ich", "eins\u00b7mals", "in", "den", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter einer Eiche stand,", "tokens": ["Hin\u00b7ter", "ei\u00b7ner", "Ei\u00b7che", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lauernd, oft mich vorw\u00e4rts legend,", "tokens": ["Lau\u00b7ernd", ",", "oft", "mich", "vor\u00b7w\u00e4rts", "le\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch die B\u00fcchse schon zur Hand:", "tokens": ["Auch", "die", "B\u00fcch\u00b7se", "schon", "zur", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da vernahm ich leichtes Rauschen", "tokens": ["Da", "ver\u00b7nahm", "ich", "leich\u00b7tes", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mein H\u00fcnerhund schlug an,", "tokens": ["Und", "mein", "H\u00fc\u00b7ner\u00b7hund", "schlug", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fertig hielt ich gleich die B\u00fcchse,", "tokens": ["Fer\u00b7tig", "hielt", "ich", "gleich", "die", "B\u00fcch\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Pa\u00dfte mit gespanntem Hahn:", "tokens": ["Pa\u00df\u00b7te", "mit", "ge\u00b7spann\u00b7tem", "Hahn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sieh! da kam nicht Reh noch Hase,", "tokens": ["Sieh", "!", "da", "kam", "nicht", "Reh", "noch", "Ha\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Kam ein Wild von sch\u00f6nrer Art,", "tokens": ["Kam", "ein", "Wild", "von", "sch\u00f6n\u00b7rer", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Trat ein M\u00e4gdlein aus den B\u00fcschen,", "tokens": ["Trat", "ein", "M\u00e4gd\u00b7lein", "aus", "den", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Jung und frisch, und lind und zart.", "tokens": ["Jung", "und", "frisch", ",", "und", "lind", "und", "zart", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "So von seltsamen Gewalten", "tokens": ["So", "von", "selt\u00b7sa\u00b7men", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Ward ich pl\u00f6tzlich \u00fcbermannt,", "tokens": ["Ward", "ich", "pl\u00f6tz\u00b7lich", "\u00fc\u00b7berm\u00b7annt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df ich fast vor eitel Liebe", "tokens": ["Da\u00df", "ich", "fast", "vor", "ei\u00b7tel", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Auf die Sch\u00f6nste losgebrannt.", "tokens": ["Auf", "die", "Sch\u00f6ns\u00b7te", "los\u00b7ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Immer geh\u2019 ich nun den F\u00e4hrten", "tokens": ["Im\u00b7mer", "geh'", "ich", "nun", "den", "F\u00e4hr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Dieses edeln Wildes nach", "tokens": ["Die\u00b7ses", "e\u00b7deln", "Wil\u00b7des", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Und vor seinem Lager steh\u2019 ich", "tokens": ["Und", "vor", "sei\u00b7nem", "La\u00b7ger", "steh'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Jeden Abend auf der Wach\u2019.", "tokens": ["Je\u00b7den", "A\u00b7bend", "auf", "der", "Wach'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Um es unverbl\u00fcmt zu sagen:", "tokens": ["Um", "es", "un\u00b7ver\u00b7bl\u00fcmt", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Vor der Lieblichsten Altan", "tokens": ["Vor", "der", "Lieb\u00b7lichs\u00b7ten", "Al\u00b7tan"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.23": {"text": "Steh\u2019 ich pflichtlich jeden Abend,", "tokens": ["Steh'", "ich", "pflicht\u00b7lich", "je\u00b7den", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Blicke traurig still hinan.", "tokens": ["Bli\u00b7cke", "trau\u00b7rig", "still", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Doch von solcher stummen Klage", "tokens": ["Doch", "von", "sol\u00b7cher", "stum\u00b7men", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Wird ihr gleich die Zeit zu lang,", "tokens": ["Wird", "ihr", "gleich", "die", "Zeit", "zu", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Lieder will sie s\u00fc\u00dfe Weisen,", "tokens": ["Lie\u00b7der", "will", "sie", "s\u00fc\u00b7\u00dfe", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Fl\u00f6tent\u00f6ne, Lautenklang.", "tokens": ["Fl\u00f6\u00b7ten\u00b7t\u00f6\u00b7ne", ",", "Lau\u00b7ten\u00b7klang", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Ach! das ist ein k\u00fcnstlich Locken,", "tokens": ["Ach", "!", "das", "ist", "ein", "k\u00fcnst\u00b7lich", "Lo\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Drin ich Waidmann nichts vermag,", "tokens": ["Drin", "ich", "Waid\u00b7mann", "nichts", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Nur den Kuckucksruf verstehend", "tokens": ["Nur", "den", "Ku\u00b7ckucks\u00b7ruf", "ver\u00b7ste\u00b7hend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Und den schlichten Wachtelschlag.", "tokens": ["Und", "den", "schlich\u00b7ten", "Wach\u00b7tel\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}