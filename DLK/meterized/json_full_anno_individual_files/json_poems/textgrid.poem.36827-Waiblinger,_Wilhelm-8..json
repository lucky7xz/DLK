word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Brust und Schulter, sind auch Arm' und Beine.", "tokens": ["Und", "Brust", "und", "Schul\u00b7ter", ",", "sind", "auch", "Arm'", "und", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "VAFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sein einz'ger Schmuck ist eine Wollenm\u00fctze,", "tokens": ["Sein", "einz'\u00b7ger", "Schmuck", "ist", "ei\u00b7ne", "Wol\u00b7len\u00b7m\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begl\u00fcckt ist er vielleicht in eines Kahnes,", "tokens": ["Be\u00b7gl\u00fcckt", "ist", "er", "viel\u00b7leicht", "in", "ei\u00b7nes", "Kah\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In einer H\u00fctte sparsamem Besitze.", "tokens": ["In", "ei\u00b7ner", "H\u00fct\u00b7te", "spar\u00b7sa\u00b7mem", "Be\u00b7sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein M\u00e4dchen ist die Sehnsucht seiner Jugend,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "ist", "die", "Sehn\u00b7sucht", "sei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihm getraut, so bringt's ihm frische Kinder,", "tokens": ["Und", "ihm", "ge\u00b7traut", ",", "so", "bringt's", "ihm", "fri\u00b7sche", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcbt bewu\u00dftlos eine strenge Tugend.", "tokens": ["Und", "\u00fcbt", "be\u00b7wu\u00dft\u00b7los", "ei\u00b7ne", "stren\u00b7ge", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Kleinen lernen bald die Kunst der Alten.", "tokens": ["Die", "Klei\u00b7nen", "ler\u00b7nen", "bald", "die", "Kunst", "der", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Netz zu ziehn, das Ruder keck zu f\u00fchren,", "tokens": ["Das", "Netz", "zu", "ziehn", ",", "das", "Ru\u00b7der", "keck", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Dienst des Boots ausdauernd zu verwalten.", "tokens": ["Den", "Dienst", "des", "Boots", "aus\u00b7dau\u00b7ernd", "zu", "ver\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-