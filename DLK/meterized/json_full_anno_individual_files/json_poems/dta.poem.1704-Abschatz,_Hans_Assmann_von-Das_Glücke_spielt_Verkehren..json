{"dta.poem.1704": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das Gl\u00fccke spielt Verkehren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Verkehren kan bald Lust/ und bald Verdr\u00fc\u00df gew\u00e4hren:", "tokens": ["Ver\u00b7keh\u00b7ren", "kan", "bald", "Lust", "/", "und", "bald", "Ver\u00b7dr\u00fc\u00df", "ge\u00b7w\u00e4h\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "NN", "$(", "KON", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer m\u00fc\u00dfig sizt am Brett/ tr\u00e4gt wenig aus dem Spiel/", "tokens": ["Wer", "m\u00fc\u00b7\u00dfig", "sizt", "am", "Brett", "/", "tr\u00e4gt", "we\u00b7nig", "aus", "dem", "Spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "APPRART", "NN", "$(", "VVFIN", "PIS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der beste Wurff ist nie zu wenig noch zu viel.", "tokens": ["Der", "bes\u00b7te", "Wurff", "ist", "nie", "zu", "we\u00b7nig", "noch", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKA", "PIS", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verkehret wie ihr wolt/ ihr m\u00fcst mit mir verkehren.", "tokens": ["Ver\u00b7keh\u00b7ret", "wie", "ihr", "wolt", "/", "ihr", "m\u00fcst", "mit", "mir", "ver\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPER", "VMFIN", "$(", "PPER", "VMFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}