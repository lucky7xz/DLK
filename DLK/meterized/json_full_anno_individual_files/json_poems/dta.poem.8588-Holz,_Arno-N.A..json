{"dta.poem.8588": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hinter den Brettern, die die Welt vernageln,", "tokens": ["Hin\u00b7ter", "den", "Bret\u00b7tern", ",", "die", "die", "Welt", "ver\u00b7na\u00b7geln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "sitzt ein Frosch mit goldnen Augen.", "tokens": ["sitzt", "ein", "Frosch", "mit", "gold\u00b7nen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schade!", "tokens": ["Scha\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Wenn ich jetzt dr\u00fcben s\u00e4sse,", "tokens": ["Wenn", "ich", "jetzt", "dr\u00fc\u00b7ben", "s\u00e4s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "w\u00e4re ich ein K\u00f6nigssohn.", "tokens": ["w\u00e4\u00b7re", "ich", "ein", "K\u00f6\u00b7nigs\u00b7sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "G\u00e4rten aus bl\u00fchenden Rosenlauben", "tokens": ["G\u00e4r\u00b7ten", "aus", "bl\u00fc\u00b7hen\u00b7den", "Ro\u00b7sen\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "funkelten,", "tokens": ["fun\u00b7kel\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Springbrunnen pl\u00e4tscherten,", "tokens": ["Spring\u00b7brun\u00b7nen", "pl\u00e4t\u00b7scher\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "in ihren weissen Armen wiegte mich eine Prinzessin . . .", "tokens": ["in", "ih\u00b7ren", "weis\u00b7sen", "Ar\u00b7men", "wieg\u00b7te", "mich", "ei\u00b7ne", "Prin\u00b7zes\u00b7sin", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Da, kuck, ein Astloch.", "tokens": ["Da", ",", "kuck", ",", "ein", "Ast\u00b7loch", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ich blinzle durch.", "tokens": ["Ich", "blinz\u00b7le", "durch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Eine gr\u00fcne Wiese mit Klapperkraut,", "tokens": ["Ei\u00b7ne", "gr\u00fc\u00b7ne", "Wie\u00b7se", "mit", "Klap\u00b7per\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "G\u00e4nse,", "tokens": ["G\u00e4n\u00b7se", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Schnips, der Hund,", "tokens": ["Schnips", ",", "der", "Hund", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "und dazu, stubsn\u00e4sig, Trine,", "tokens": ["und", "da\u00b7zu", ",", "stubs\u00b7n\u00e4\u00b7sig", ",", "Tri\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PAV", "$,", "ADJD", "$,", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "die, den Rock schon vorn zu kurz \u2014 Lichter zieht und Schmalzbrot kaut!", "tokens": ["die", ",", "den", "Rock", "schon", "vorn", "zu", "kurz", "Lich\u00b7ter", "zieht", "und", "Schmalz\u00b7brot", "kaut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "NN", "ADV", "ADV", "PTKA", "ADJD", "$(", "NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}