{"dta.poem.2071": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Affe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Scheinet nun von allen Thieren eins zu unsrer Lust", "tokens": ["Schei\u00b7net", "nun", "von", "al\u00b7len", "Thie\u00b7ren", "eins", "zu", "uns\u00b7rer", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "PIS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "geschaffen", "tokens": ["ge\u00b7schaf\u00b7fen"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und zum Vorwurf der Bewundrung, so sind es gewi\u00df", "tokens": ["Und", "zum", "Vor\u00b7wurf", "der", "Be\u00b7wund\u00b7rung", ",", "so", "sind", "es", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "die Affen.", "tokens": ["die", "Af\u00b7fen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Nicht nur in des K\u00f6rpers Bau, auch in Mienen und Ge-", "tokens": ["Nicht", "nur", "in", "des", "K\u00f6r\u00b7pers", "Bau", ",", "auch", "in", "Mie\u00b7nen", "und", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "NN", "$,", "ADV", "APPR", "NN", "KON", "TRUNC"], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "berden,", "tokens": ["ber\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "In behenden Handlungen, Munterkeit und Schlauigkeit,", "tokens": ["In", "be\u00b7hen\u00b7den", "Hand\u00b7lun\u00b7gen", ",", "Mun\u00b7ter\u00b7keit", "und", "Schlau\u00b7ig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "In der seltsamen Bewegung Arten und Verschiedenheit", "tokens": ["In", "der", "selt\u00b7sa\u00b7men", "Be\u00b7we\u00b7gung", "Ar\u00b7ten", "und", "Ver\u00b7schie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "KON", "NN"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "M\u00fcssen sie im ersten Grad fast von uns gerechnet werden:", "tokens": ["M\u00fcs\u00b7sen", "sie", "im", "ers\u00b7ten", "Grad", "fast", "von", "uns", "ge\u00b7rech\u00b7net", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Ja f\u00fcr einen Philosophen, da sein Thun so ungemein,", "tokens": ["Ja", "f\u00fcr", "ei\u00b7nen", "Phi\u00b7lo\u00b7so\u00b7phen", ",", "da", "sein", "Thun", "so", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "D\u00fcrft ein Aff zur Ueberlegung wohl ein w\u00fcrd\u2019ger Vorwurf", "tokens": ["D\u00fcrft", "ein", "Aff", "zur", "Ue\u00b7ber\u00b7le\u00b7gung", "wohl", "ein", "w\u00fcrd'\u00b7ger", "Vor\u00b7wurf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Wenn man recht die Trieb\u2019 erw\u00e4gt, die in Affen zu entdecken,", "tokens": ["Wenn", "man", "recht", "die", "Trieb'", "er\u00b7w\u00e4gt", ",", "die", "in", "Af\u00b7fen", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Wie so fern am Leib und Geist sich die F\u00e4higkeiten strecken,", "tokens": ["Wie", "so", "fern", "am", "Leib", "und", "Geist", "sich", "die", "F\u00e4\u00b7hig\u00b7kei\u00b7ten", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "APPRART", "NN", "KON", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Wird fast unsere Vernunft \u00fcber diese Thiere stutzen,", "tokens": ["Wird", "fast", "un\u00b7se\u00b7re", "Ver\u00b7nunft", "\u00fc\u00b7ber", "die\u00b7se", "Thie\u00b7re", "stut\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+---+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Und darinn von der Natur Ordnungen und Graden sehn,", "tokens": ["Und", "da\u00b7rinn", "von", "der", "Na\u00b7tur", "Ord\u00b7nun\u00b7gen", "und", "Gra\u00b7den", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APPR", "ART", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Wodurch gleichsam auch die Thiere sich zu uns noch mehr", "tokens": ["Wo\u00b7durch", "gleich\u00b7sam", "auch", "die", "Thie\u00b7re", "sich", "zu", "uns", "noch", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "PRF", "APPR", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.18": {"text": "erh\u00f6hn,", "tokens": ["er\u00b7h\u00f6hn", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Als es fast der Stolz erlaubet: Also kann ein Aff auch nutzen", "tokens": ["Als", "es", "fast", "der", "Stolz", "er\u00b7lau\u00b7bet", ":", "Al\u00b7so", "kann", "ein", "Aff", "auch", "nut\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$.", "ADV", "VMFIN", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Und zur Demuth gleichsam leiten. Billig f\u00e4llt hiebey uns ein:", "tokens": ["Und", "zur", "De\u00b7muth", "gleich\u00b7sam", "lei\u00b7ten", ".", "Bil\u00b7lig", "f\u00e4llt", "hie\u00b7bey", "uns", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "VVINF", "$.", "ADJD", "VVFIN", "ADV", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Was f\u00fcr eine Geisterleiter mu\u00df wohl nicht vorhanden seyn,", "tokens": ["Was", "f\u00fcr", "ei\u00b7ne", "Geis\u00b7ter\u00b7lei\u00b7ter", "mu\u00df", "wohl", "nicht", "vor\u00b7han\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VMFIN", "ADV", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Die von uns hinab- auch aufwerts mit so manchen Staffeln", "tokens": ["Die", "von", "uns", "hin\u00b7ab", "auch", "auf\u00b7werts", "mit", "so", "man\u00b7chen", "Staf\u00b7feln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "TRUNC", "ADV", "ADV", "APPR", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.23": {"text": "f\u00fchrt,", "tokens": ["f\u00fchrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Da\u00df weil wir kein End erblicken, die Vernunft sich fast", "tokens": ["Da\u00df", "weil", "wir", "kein", "End", "er\u00b7bli\u00b7cken", ",", "die", "Ver\u00b7nunft", "sich", "fast"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PIAT", "NN", "VVINF", "$,", "ART", "NN", "PRF", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.25": {"text": "verliert.", "tokens": ["ver\u00b7liert", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "Was sie all f\u00fcr Handlungen k\u00fcnstlich nachzuahmen wissen,", "tokens": ["Was", "sie", "all", "f\u00fcr", "Hand\u00b7lun\u00b7gen", "k\u00fcnst\u00b7lich", "nach\u00b7zu\u00b7ah\u00b7men", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PIAT", "APPR", "NN", "ADJD", "VVIZU", "VVFIN", "$,"], "meter": "+-+-++-+-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.27": {"text": "Auch was sie von selbsten thun, davon werd\u2019 ich schweigen", "tokens": ["Auch", "was", "sie", "von", "selbs\u00b7ten", "thun", ",", "da\u00b7von", "werd'", "ich", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "PPER", "APPR", "ADV", "VVINF", "$,", "PAV", "VAFIN", "PPER", "VVINF"], "meter": "-+--+-+----+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "m\u00fcssen,", "tokens": ["m\u00fcs\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Weil die Vielheit gar zu gro\u00df. Dieses macht, da\u00df ich allein", "tokens": ["Weil", "die", "Viel\u00b7heit", "gar", "zu", "gro\u00df", ".", "Die\u00b7ses", "macht", ",", "da\u00df", "ich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "PTKA", "ADJD", "$.", "PDS", "VVFIN", "$,", "KOUS", "PPER", "ADV"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.30": {"text": "Zur Betrachtung, da\u00df auch sie uns zu Gut erschaffen seyn,", "tokens": ["Zur", "Be\u00b7trach\u00b7tung", ",", "da\u00df", "auch", "sie", "uns", "zu", "Gut", "er\u00b7schaf\u00b7fen", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ADV", "PPER", "PRF", "PTKA", "ADJD", "VVPP", "VAINF", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Zur Bewunderung und Dank, wie ich schuldig bin, mich kehre", "tokens": ["Zur", "Be\u00b7wun\u00b7de\u00b7rung", "und", "Dank", ",", "wie", "ich", "schul\u00b7dig", "bin", ",", "mich", "keh\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.32": {"text": "Und so, wie in allen Werken, auch in ihm, den Sch\u00f6pfer ehre.", "tokens": ["Und", "so", ",", "wie", "in", "al\u00b7len", "Wer\u00b7ken", ",", "auch", "in", "ihm", ",", "den", "Sch\u00f6p\u00b7fer", "eh\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "APPR", "PIAT", "NN", "$,", "ADV", "APPR", "PPER", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}