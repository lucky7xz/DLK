{"textgrid.poem.56993": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die beiden Flaschen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Flaschen stehn auf einer Bank,", "tokens": ["Zwei", "Fla\u00b7schen", "stehn", "auf", "ei\u00b7ner", "Bank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die eine dick, die andre schlank.", "tokens": ["die", "ei\u00b7ne", "dick", ",", "die", "and\u00b7re", "schlank", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie m\u00f6chte gerne heiraten.", "tokens": ["Sie", "m\u00f6ch\u00b7te", "ger\u00b7ne", "hei\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Doch wer soll ihnen beiraten?", "tokens": ["Doch", "wer", "soll", "ih\u00b7nen", "bei\u00b7ra\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit ihrem Doppel-Auge leiden", "tokens": ["Mit", "ih\u00b7rem", "Dop\u00b7pel\u00b7Au\u00b7ge", "lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sie auf zum blauen Firmament ...", "tokens": ["sie", "auf", "zum", "blau\u00b7en", "Fir\u00b7ma\u00b7ment", "..."], "token_inf