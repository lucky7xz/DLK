{"textgrid.poem.64263": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Aufruf der K\u00f6nigin Augusta", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es brach das Herz der edeln Dulderin!", "tokens": ["Es", "brach", "das", "Herz", "der", "e\u00b7deln", "Dul\u00b7de\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht sah Luise noch das Morgenrot,", "tokens": ["Nicht", "sah", "Lu\u00b7i\u00b7se", "noch", "das", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "NE", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das flammend aufstieg dort in Preu\u00dfens Ostmark.", "tokens": ["Das", "flam\u00b7mend", "auf\u00b7stieg", "dort", "in", "Preu\u00b7\u00dfens", "Ost\u00b7mark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "ADV", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jedoch ihr Geist lebt in den Frauen fort", "tokens": ["Je\u00b7doch", "ihr", "Geist", "lebt", "in", "den", "Frau\u00b7en", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Hohenzollernhauses: Heil Augusta!", "tokens": ["Des", "Ho\u00b7hen\u00b7zol\u00b7lern\u00b7hau\u00b7ses", ":", "Heil", "Au\u00b7gus\u00b7ta", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sie mahnte laut: \u00bbDas Vaterland erwartet,", "tokens": ["Sie", "mahn\u00b7te", "laut", ":", "\u00bb", "Das", "Va\u00b7ter\u00b7land", "er\u00b7war\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "$(", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df alle Frau'n bereit sind, ihre Pflicht", "tokens": ["Da\u00df", "al\u00b7le", "Frau'n", "be\u00b7reit", "sind", ",", "ih\u00b7re", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "VAFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zu tun!\u00ab \u2013 Und wie ihr Wort, so war ihr Werk.", "tokens": ["Zu", "tun", "!", "\u00ab", "\u2013", "Und", "wie", "ihr", "Wort", ",", "so", "war", "ihr", "Werk", "."], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "$(", "$(", "KON", "PWAV", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es brach das Herz der edeln Dulderin!", "tokens": ["Es", "brach", "das", "Herz", "der", "e\u00b7deln", "Dul\u00b7de\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht sah Luise noch das Morgenrot,", "tokens": ["Nicht", "sah", "Lu\u00b7i\u00b7se", "noch", "das", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "NE", "ADV", "ART", "NN