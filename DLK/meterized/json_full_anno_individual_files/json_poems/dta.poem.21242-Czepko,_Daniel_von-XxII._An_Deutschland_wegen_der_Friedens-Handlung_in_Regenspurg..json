{"dta.poem.21242": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "XxII.  \n  An Deutschland  \n wegen der Friedens-Handlung in Regenspurg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "O Deutschland schaw wie trew dein Haubt vor dich gewacht/", "tokens": ["O", "Deutschland", "schaw", "wie", "trew", "dein", "Haubt", "vor", "dich", "ge\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "KOKOM", "ADJD", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Damit in Sicherheit das ", "tokens": ["Da\u00b7mit", "in", "Si\u00b7cher\u00b7heit", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein ", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Drumb vbergiebest du ", "tokens": ["Drumb", "vber\u00b7gie\u00b7best", "du"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Der V\u00f6lcker Furcht vnd Hold/ der Reiche Nutz vnd Heil/", "tokens": ["Der", "V\u00f6l\u00b7cker", "Furcht", "vnd", "Hold", "/", "der", "Rei\u00b7che", "Nutz", "vnd", "Heil", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NE", "$(", "ART", "NE", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die m\u00fcssen voller Frewd\u2019 einander jtzt begegnen.", "tokens": ["Die", "m\u00fcs\u00b7sen", "vol\u00b7ler", "Fre\u00b7wd'", "ein\u00b7an\u00b7der", "jtzt", "be\u00b7geg\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dann ", "tokens": ["Dann"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Vnd vntern L\u00e4ndern hofft ein jedes seinen Theil.", "tokens": ["Vnd", "vn\u00b7tern", "L\u00e4n\u00b7dern", "hofft", "ein", "je\u00b7des", "sei\u00b7nen", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du hast nichts eingeb\u00fcst/ als eintzig deine Krafft/", "tokens": ["Du", "hast", "nichts", "ein\u00b7ge\u00b7b\u00fcst", "/", "als", "eint\u00b7zig", "dei\u00b7ne", "Krafft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVFIN", "$(", "KOUS", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dich zu Bodem stie\u00df. Anjtzt kanst du genesen.", "tokens": ["Die", "dich", "zu", "Bo\u00b7dem", "stie\u00df", ".", "Anjtzt", "kanst", "du", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$.", "NN", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+----", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Der Frieden ist die Glutt/ den man Vns wird verlesen/", "tokens": ["Der", "Frie\u00b7den", "ist", "die", "Glutt", "/", "den", "man", "Vns", "wird", "ver\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ART", "PIS", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch die dein alter Stand sich auff das new erschafft.", "tokens": ["Durch", "die", "dein", "al\u00b7ter", "Stand", "sich", "auff", "das", "new", "er\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "PRF", "APPR", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O Ph\u00f6nix deines Gl&#367;cks! Ich sehe trefflich hoch", "tokens": ["O", "Ph\u00f6\u00b7nix", "dei\u00b7nes", "Gl", "&#367;", "cks", "!", "Ich", "se\u00b7he", "treff\u00b7lich", "hoch"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "PPOSAT", "NN", "$(", "NE", "$.", "PPER", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Wolfarth Morgenr\u00f6th in ", "tokens": ["Der", "Wolf\u00b7arth", "Mor\u00b7gen\u00b7r\u00f6th", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sehe Streit vnd Schlacht die ", "tokens": ["Ich", "se\u00b7he", "Streit", "vnd", "Schlacht", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jtzt wirst du Deutsch/ vor wahrst du aller V\u00f6lcker Gloch.", "tokens": ["Jtzt", "wirst", "du", "Deutsch", "/", "vor", "wahrst", "du", "al\u00b7ler", "V\u00f6l\u00b7cker", "Gloch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$(", "APPR", "VAFIN", "PPER", "PIAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}