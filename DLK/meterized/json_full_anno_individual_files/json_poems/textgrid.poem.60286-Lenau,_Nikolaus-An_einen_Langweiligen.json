{"textgrid.poem.60286": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An einen Langweiligen", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unnahbar sind die M\u00e4chte, unbezwingbar,", "tokens": ["Un\u00b7nah\u00b7bar", "sind", "die", "M\u00e4ch\u00b7te", ",", "un\u00b7be\u00b7zwing\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die dir getreu, gleich Sklaven, schwerbejochten,", "tokens": ["Die", "dir", "ge\u00b7treu", ",", "gleich", "Skla\u00b7ven", ",", "schwer\u00b7be\u00b7joch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "$,", "ADV", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An deine Ferse, deinen Wink geflochten,", "tokens": ["An", "dei\u00b7ne", "Fer\u00b7se", ",", "dei\u00b7nen", "Wink", "ge\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu m\u00e4chtig schier, als da\u00df sie mir besingbar.", "tokens": ["Zu", "m\u00e4ch\u00b7tig", "schier", ",", "als", "da\u00df", "sie", "mir", "be\u00b7sing\u00b7bar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Mein Saitenspiel auch darf nur zagend hoffen,", "tokens": ["Mein", "Sai\u00b7ten\u00b7spiel", "auch", "darf", "nur", "za\u00b7gend", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von ihrem Sieg zu bleiben ungetroffen.", "tokens": ["Von", "ih\u00b7rem", "Sieg", "zu", "blei\u00b7ben", "un\u00b7ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch Tyrannei ist Mutter der Emp\u00f6rung;", "tokens": ["Doch", "Ty\u00b7ran\u00b7nei", "ist", "Mut\u00b7ter", "der", "Em\u00b7p\u00f6\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drum wagt ich einst mit lustigen Gesellen,", "tokens": ["Drum", "wagt", "ich", "einst", "mit", "lus\u00b7ti\u00b7gen", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gemacht, den Kater Cato selbst zu prellen", "tokens": ["Ge\u00b7macht", ",", "den", "Ka\u00b7ter", "Ca\u00b7to", "selbst", "zu", "prel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "NE", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um einen Schwank, \u2013 wir wagten die Verschw\u00f6rung.", "tokens": ["Um", "ei\u00b7nen", "Schwank", ",", "\u2013", "wir", "wag\u00b7ten", "die", "Ver\u00b7schw\u00f6\u00b7rung", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Uns in der Schenk an deinen Tisch zu setzen,", "tokens": ["Uns", "in", "der", "Schenk", "an", "dei\u00b7nen", "Tisch", "zu", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Scherz und Witz dich einmal scharf zu hetzen.", "tokens": ["Mit", "Scherz", "und", "Witz", "dich", "ein\u00b7mal", "scharf", "zu", "het\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Weh uns! da quoll der Murmelbach der Rede", "tokens": ["Weh", "uns", "!", "da", "quoll", "der", "Mur\u00b7mel\u00b7bach", "der", "Re\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hervor aus deines Kopfes finstrer Nacht,", "tokens": ["Her\u00b7vor", "aus", "dei\u00b7nes", "Kop\u00b7fes", "finst\u00b7rer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und unsre pl\u00e4nkelnde Vorpostenwacht,", "tokens": ["Und", "uns\u00b7re", "pl\u00e4n\u00b7keln\u00b7de", "Vor\u00b7pos\u00b7ten\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Scherz, der Witz erlagen in der Fehde;", "tokens": ["Der", "Scherz", ",", "der", "Witz", "er\u00b7la\u00b7gen", "in", "der", "Feh\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von Wassergeistern ward der Witz umnebelt,", "tokens": ["Von", "Was\u00b7ser\u00b7geis\u00b7tern", "ward", "der", "Witz", "um\u00b7ne\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Von ihnen ward im Hui! der Scherz geknebelt.", "tokens": ["Von", "ih\u00b7nen", "ward", "im", "Hui", "!", "der", "Scherz", "ge\u00b7kne\u00b7belt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "APPRART", "FM", "$.", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da trat, f\u00fcr uns zu Schmach und argem Spotte,", "tokens": ["Da", "trat", ",", "f\u00fcr", "uns", "zu", "Schmach", "und", "ar\u00b7gem", "Spot\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "PPER", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die hohe F\u00fcrstin der D\u00e4monenschar,", "tokens": ["Die", "ho\u00b7he", "F\u00fcrs\u00b7tin", "der", "D\u00e4\u00b7mo\u00b7nen\u00b7schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3