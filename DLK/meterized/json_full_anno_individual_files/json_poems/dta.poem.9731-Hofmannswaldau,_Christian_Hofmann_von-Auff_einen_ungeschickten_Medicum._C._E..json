{"dta.poem.9731": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff einen ungeschickten Medicum.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bullus ist ein artzt geworden und versteht das hand-", "tokens": ["Bul\u00b7lus", "ist", "ein", "artzt", "ge\u00b7wor\u00b7den", "und", "ver\u00b7steht", "das", "han\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "VVPP", "VAPP", "KON", "VVFIN", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "werck nicht/", "tokens": ["werck", "nicht", "/"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Lesen hat er nie gelernet/ und das schreiben will ihm fehlen/", "tokens": ["Le\u00b7sen", "hat", "er", "nie", "ge\u00b7ler\u00b7net", "/", "und", "das", "schrei\u00b7ben", "will", "ihm", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$(", "KON", "PDS", "VVINF", "VMFIN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Gleichwohl kan er lauter gl\u00fcck unter seine curen zehlen.", "tokens": ["Gleich\u00b7wohl", "kan", "er", "lau\u00b7ter", "gl\u00fcck", "un\u00b7ter", "sei\u00b7ne", "cu\u00b7ren", "zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Die recepte/ die er giebet/ sind von andren eingericht.", "tokens": ["Die", "re\u00b7cep\u00b7te", "/", "die", "er", "gie\u00b7bet", "/", "sind", "von", "an\u00b7dren", "ein\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "PRELS", "PPER", "VVFIN", "$(", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Vor das fieber/ vor den stein/ vor die stiche zu dem hertzen", "tokens": ["Vor", "das", "fie\u00b7ber", "/", "vor", "den", "stein", "/", "vor", "die", "sti\u00b7che", "zu", "dem", "hert\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$(", "APPR", "ART", "NN", "$(", "APPR", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Tr\u00e4gt er beyde s\u00e4cke voll/ und bestillt die gr\u00f6ste schmertzen.", "tokens": ["Tr\u00e4gt", "er", "bey\u00b7de", "s\u00e4\u00b7cke", "voll", "/", "und", "be\u00b7stillt", "die", "gr\u00f6s\u00b7te", "schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "VVFIN", "ADJD", "$(", "KON", "VVFIN", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Julep/ edle hertzens-pulver/ pillen/ safft und elixir/", "tokens": ["Ju\u00b7lep", "/", "ed\u00b7le", "hert\u00b7zens\u00b7pul\u00b7ver", "/", "pil\u00b7len", "/", "safft", "und", "e\u00b7li\u00b7xir", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADJA", "NN", "$(", "VVINF", "$(", "VVFIN", "KON", "PPER", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Zieht er/ wie das gl\u00fcck es f\u00fcget/ aus der taschen bald herf\u00fcr/", "tokens": ["Zieht", "er", "/", "wie", "das", "gl\u00fcck", "es", "f\u00fc\u00b7get", "/", "aus", "der", "ta\u00b7schen", "bald", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOKOM", "PDS", "VVFIN", "PPER", "VVFIN", "$(", "APPR", "ART", "ADJA", "ADV", "ADV", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Schickt es in die apotheck/ und so bald es eingenommen/", "tokens": ["Schickt", "es", "in", "die", "a\u00b7pot\u00b7heck", "/", "und", "so", "bald", "es", "ein\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$(", "KON", "ADV", "ADV", "PPER", "VVPP", "$("], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.11": {"text": "Spricht er zu dem krancken drauff: GOtt la\u00df alles wol be-", "tokens": ["Spricht", "er", "zu", "dem", "kran\u00b7cken", "drauff", ":", "Gott", "la\u00df", "al\u00b7les", "wol", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$.", "NN", "VVFIN", "PIS", "ADV", "TRUNC"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "kommen.", "tokens": ["kom\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}