{"dta.poem.18643": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Cantata.  \n  U berdas unbest\u00e4ndige  G l\u00fccke", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Meine Sonn' ist gantz verschwunden/", "tokens": ["Mei\u00b7ne", "Sonn'", "ist", "gantz", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mich martert stete Nacht.", "tokens": ["Und", "mich", "mar\u00b7tert", "ste\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! wie schmertzlich sind die Stunden/", "tokens": ["Ach", "!", "wie", "schmertz\u00b7lich", "sind", "die", "Stun\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo kein Freuden-Morgen lacht!", "tokens": ["Wo", "kein", "Freu\u00b7den\u00b7Mor\u00b7gen", "lacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich mu\u00df bey den Finsternissen", "tokens": ["Ich", "mu\u00df", "bey", "den", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Schon mein Grab im Leben k\u00fcssen.", "tokens": ["Schon", "mein", "Grab", "im", "Le\u00b7ben", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So Gl\u00fcck als Zeit/ ja Sonn- und Monden-Licht/", "tokens": ["So", "Gl\u00fcck", "als", "Zeit", "/", "ja", "Sonn", "und", "Mon\u00b7den\u00b7Licht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "$(", "ADV", "TRUNC", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und alles ist dem Wechsel untergeben;", "tokens": ["Und", "al\u00b7les", "ist", "dem", "Wech\u00b7sel", "un\u00b7ter\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch nur mein Ungl\u00fcck nicht/", "tokens": ["Doch", "nur", "mein", "Un\u00b7gl\u00fcck", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denn mein bet\u00fcbtes Leben/", "tokens": ["Denn", "mein", "be\u00b7t\u00fcb\u00b7tes", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ist immer einerley.", "tokens": ["Ist", "im\u00b7mer", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach Himmel! Was sprichst du?", "tokens": ["Ach", "Him\u00b7mel", "!", "Was", "sprichst", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Da\u00df ich verstossen sey?", "tokens": ["Da\u00df", "ich", "ver\u00b7stos\u00b7sen", "sey", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ja/ Ja/ du schliest dich vor mir zu.", "tokens": ["Ja", "/", "Ja", "/", "du", "schliest", "dich", "vor", "mir", "zu", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PTKANT", "$(", "PPER", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn andre sich im Fr\u00fchling weiden/", "tokens": ["Wenn", "and\u00b7re", "sich", "im", "Fr\u00fch\u00b7ling", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "L\u00e4\u00dft du mir steten Winter leiden/", "tokens": ["L\u00e4\u00dft", "du", "mir", "ste\u00b7ten", "Win\u00b7ter", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und dein beliebter Schein", "tokens": ["Und", "dein", "be\u00b7lieb\u00b7ter", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Mu\u00df mir verschwunden seyn.", "tokens": ["Mu\u00df", "mir", "ver\u00b7schwun\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch halt/ es heist zu weit gegangen/", "tokens": ["Doch", "halt", "/", "es", "heist", "zu", "weit", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man dich tadlen wil:", "tokens": ["Wenn", "man", "dich", "tad\u00b7len", "wil", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ein Kluger h\u00e4lt gedultig still/", "tokens": ["Ein", "Klu\u00b7ger", "h\u00e4lt", "ge\u00b7dul\u00b7tig", "still", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn sieht man gleich/ wie du es angefangen/", "tokens": ["Denn", "sieht", "man", "gleich", "/", "wie", "du", "es", "an\u00b7ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "$(", "PWAV", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So sieht man doch das Ende nicht", "tokens": ["So", "sieht", "man", "doch", "das", "En\u00b7de", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das noch vielleicht von was Vergu\u00fcgtes spricht.", "tokens": ["Das", "noch", "viel\u00b7leicht", "von", "was", "Ver\u00b7gu\u00fcg\u00b7tes", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "APPR", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du kanst blitzen und auch lachen/", "tokens": ["Du", "kanst", "blit\u00b7zen", "und", "auch", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und aus Dornen Rosen machen.", "tokens": ["Und", "aus", "Dor\u00b7nen", "Ro\u00b7sen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sol ich hoffen/ oder nicht?", "tokens": ["Sol", "ich", "hof\u00b7fen", "/", "o\u00b7der", "nicht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$(", "KON", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmel zeige meiner Seelen/", "tokens": ["Him\u00b7mel", "zei\u00b7ge", "mei\u00b7ner", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was mir deine Gunst verspricht/", "tokens": ["Was", "mir", "dei\u00b7ne", "Gunst", "ver\u00b7spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder wilst du mich nur qv\u00e4len/", "tokens": ["O\u00b7der", "wilst", "du", "mich", "nur", "qv\u00e4\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach so ende meine Noth", "tokens": ["Ach", "so", "en\u00b7de", "mei\u00b7ne", "Noth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch den Todt.", "tokens": ["Durch", "den", "Todt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Allein/ ist di\u00df nicht ein Verlangen/", "tokens": ["Al\u00b7lein", "/", "ist", "di\u00df", "nicht", "ein", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VAFIN", "PDS", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das nach Verzweifflung schmeckt?", "tokens": ["Das", "nach", "Ver\u00b7zweif\u00b7flung", "schmeckt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Angst und Noth die Seele h\u00e4lt \u00fcmfangen/", "tokens": ["Wenn", "Angst", "und", "Noth", "die", "See\u00b7le", "h\u00e4lt", "\u00fcm\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So w\u00fcnscht man sich im Tode hingestreckt;", "tokens": ["So", "w\u00fcnscht", "man", "sich", "im", "To\u00b7de", "hin\u00b7ge\u00b7streckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch wenn wir Freuden-Rosen brechen/", "tokens": ["Doch", "wenn", "wir", "Freu\u00b7den\u00b7Ro\u00b7sen", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wil man nichts von Sterben sprechen.", "tokens": ["So", "wil", "man", "nichts", "von", "Ster\u00b7ben", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PIS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schertzen im Gl\u00fccke/ sich qv\u00e4len im Leiden/", "tokens": ["Schert\u00b7zen", "im", "Gl\u00fc\u00b7cke", "/", "sich", "qv\u00e4\u00b7len", "im", "Lei\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$(", "PRF", "VVFIN", "APPRART", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Heisset allein", "tokens": ["Heis\u00b7set", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["NE", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sclavisch zu seyn.", "tokens": ["Scla\u00b7visch", "zu", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VAINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Aber im blitzen die Augen zu weiden/", "tokens": ["A\u00b7ber", "im", "blit\u00b7zen", "die", "Au\u00b7gen", "zu", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Lachen wenn man sich mit Schmertzen sol kleiden/", "tokens": ["La\u00b7chen", "wenn", "man", "sich", "mit", "Schmert\u00b7zen", "sol", "klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PIS", "PRF", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Bleibet nur himmlischen Seelen gemein.", "tokens": ["Blei\u00b7bet", "nur", "himm\u00b7li\u00b7schen", "See\u00b7len", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Ob das Gel\u00fccke", "tokens": ["Ob", "das", "Ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mich gleich vom Gipffel rei\u00dft/", "tokens": ["Mich", "gleich", "vom", "Gipf\u00b7fel", "rei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So glaub' ich doch/ da\u00df mein Geschicke", "tokens": ["So", "glaub'", "ich", "doch", "/", "da\u00df", "mein", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich einmahl wieder steigen heist.", "tokens": ["Mich", "ein\u00b7mahl", "wie\u00b7der", "stei\u00b7gen", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein Sturm wirfft Cedern nieder/", "tokens": ["Ein", "Sturm", "wirfft", "Ce\u00b7dern", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch weil die Wurtzel bleibt/", "tokens": ["Doch", "weil", "die", "Wurt\u00b7zel", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So gr\u00fcnt die Hoffnung wieder/", "tokens": ["So", "gr\u00fcnt", "die", "Hoff\u00b7nung", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ihre Krafft von neuen Wachsthum treibt/", "tokens": ["Da\u00df", "ih\u00b7re", "Krafft", "von", "neu\u00b7en", "Wach\u00b7sthum", "treibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Hoffnung mu\u00df allein", "tokens": ["Die", "Hoff\u00b7nung", "mu\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Des Lebens s\u00fcsse Nahrung seyn:", "tokens": ["Des", "Le\u00b7bens", "s\u00fcs\u00b7se", "Nah\u00b7rung", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum sol sie meinen Leben", "tokens": ["Drum", "sol", "sie", "mei\u00b7nen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch das Vergn\u00fcgen geben.", "tokens": ["Auch", "das", "Ver\u00b7gn\u00fc\u00b7gen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VV