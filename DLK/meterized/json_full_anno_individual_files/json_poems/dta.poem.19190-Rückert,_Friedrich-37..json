{"dta.poem.19190": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Apfel f\u00e4llt, gereift, in seines G\u00e4rtners Hand;", "tokens": ["Der", "Ap\u00b7fel", "f\u00e4llt", ",", "ge\u00b7reift", ",", "in", "sei\u00b7nes", "G\u00e4rt\u00b7ners", "Hand", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So f\u00e4llt in Gott ein Geist, der seine Reife fand.", "tokens": ["So", "f\u00e4llt", "in", "Gott", "ein", "Geist", ",", "der", "sei\u00b7ne", "Rei\u00b7fe", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wol f\u00e4llt ein Apfel auch, zu fr\u00fch vom Sturm gebrochen,", "tokens": ["Wol", "f\u00e4llt", "ein", "Ap\u00b7fel", "auch", ",", "zu", "fr\u00fch", "vom", "Sturm", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,", "PTKA", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Von Willk\u00fcr abgepfl\u00fcckt, oder vom Wurm gestochen.", "tokens": ["Von", "Will\u00b7k\u00fcr", "ab\u00b7ge\u00b7pfl\u00fcckt", ",", "o\u00b7der", "vom", "Wurm", "ge\u00b7sto\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "KON", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch hierin ist der Baum im engen Gartenraum", "tokens": ["Doch", "hie\u00b7rin", "ist", "der", "Baum", "im", "en\u00b7gen", "Gar\u00b7ten\u00b7raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hoch \u00fcberragt und weit vom Weltengartenbaum,", "tokens": ["Hoch", "\u00fc\u00b7berr\u00b7agt", "und", "weit", "vom", "Wel\u00b7ten\u00b7gar\u00b7ten\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den solch ein G\u00e4rtner zieht, der auch dem Sturm befihlt,", "tokens": ["Den", "solch", "ein", "G\u00e4rt\u00b7ner", "zieht", ",", "der", "auch", "dem", "Sturm", "be\u00b7fihlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den keine Willk\u00fcr st\u00f6rt, kein schwacher Wurm bestihlt;", "tokens": ["Den", "kei\u00b7ne", "Will\u00b7k\u00fcr", "st\u00f6rt", ",", "kein", "schwa\u00b7cher", "Wurm", "be\u00b7stihlt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und sicher f\u00fchlest du's: von ihm wird hingenommen", "tokens": ["Und", "si\u00b7cher", "f\u00fch\u00b7lest", "du's", ":", "von", "ihm", "wird", "hin\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "$.", "APPR", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Heile jede Frucht, wann ihre Zeit gekommen.", "tokens": ["Zum", "Hei\u00b7le", "je\u00b7de", "Frucht", ",", "wann", "ih\u00b7re", "Zeit", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Villeicht erschien sie dir von au\u00dfen nicht gestreift,", "tokens": ["Vil\u00b7leicht", "er\u00b7schien", "sie", "dir", "von", "au\u00b7\u00dfen", "nicht", "ge\u00b7streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPR", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie aber, glaub' es mir, war innerlich gereift.", "tokens": ["Sie", "a\u00b7ber", ",", "glaub'", "es", "mir", ",", "war", "in\u00b7ner\u00b7lich", "ge\u00b7reift", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "VVFIN", "PPER", "PPER", "$,", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}