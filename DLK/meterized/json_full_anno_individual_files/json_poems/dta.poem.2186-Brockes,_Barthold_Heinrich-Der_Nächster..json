{"dta.poem.2186": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der N\u00e4chster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du fragst, wie jener Pharis\u00e4er, mit Recht: wer doch", "tokens": ["Du", "fragst", ",", "wie", "je\u00b7ner", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", ",", "mit", "Recht", ":", "wer", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PDAT", "NN", "$,", "APPR", "NN", "$.", "PWS", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "dein N\u00e4chster sey?", "tokens": ["dein", "N\u00e4chs\u00b7ter", "sey", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Drauf f\u00fcg\u2019 ich dieses der Erkl\u00e4rung von jenem Sama-", "tokens": ["Drauf", "f\u00fcg'", "ich", "die\u00b7ses", "der", "Er\u00b7kl\u00e4\u00b7rung", "von", "je\u00b7nem", "Sa\u00b7ma"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PDAT", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "riter bey:", "tokens": ["ri\u00b7ter", "bey", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Es sind die alle deine N\u00e4chsten, die auf dem Zirkel die-", "tokens": ["Es", "sind", "die", "al\u00b7le", "dei\u00b7ne", "N\u00e4chs\u00b7ten", ",", "die", "auf", "dem", "Zir\u00b7kel", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": 