{"dta.poem.18138": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Pr\u00fcffung  der  Uebers etzung  \n von Horazens Dichtkunst.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Habt ihr denn recht gew\u00e4hlt, so d\u00f6rft ihr euch nicht", "tokens": ["Habt", "ihr", "denn", "recht", "ge\u00b7w\u00e4hlt", ",", "so", "d\u00f6rft", "ihr", "euch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "qu\u00e4len,", "tokens": ["qu\u00e4\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Euch wird an Worten nicht, noch auch an Ordnung", "tokens": ["Euch", "wird", "an", "Wor\u00b7ten", "nicht", ",", "noch", "auch", "an", "Ord\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PTKNEG", "$,", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "fehlen.", "tokens": ["feh\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}