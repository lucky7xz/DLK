{"textgrid.poem.63878": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft am Tische dieser Welt nur auch kein Kostver\u00e4chter sein.", "tokens": ["Du", "mu\u00dft", "am", "Ti\u00b7sche", "die\u00b7ser", "Welt", "nur", "auch", "kein", "Kost\u00b7ver\u00b7\u00e4ch\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "PDAT", "NN", "ADV", "ADV", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Denk, wenn ein Mahl dir nicht behagt, es k\u00f6nnte leicht noch schlechter sein.", "tokens": ["Denk", ",", "wenn", "ein", "Mahl", "dir", "nicht", "be\u00b7hagt", ",", "es", "k\u00f6nn\u00b7te", "leicht", "noch", "schlech\u00b7ter", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PPER", "PTKNEG", "VVPP", "$,", "PPER", "VMFIN", "ADJD", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Nimm auch mit trocknem Brot vorlieb und einem Trunk aus klarem Quell;", "tokens": ["Nimm", "auch", "mit", "trock\u00b7nem", "Brot", "vor\u00b7lieb", "und", "ei\u00b7nem", "Trunk", "aus", "kla\u00b7rem", "Quell", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN", "ADJD", "KON", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "'s ist besser, n\u00fcchtern, als ein schwer von Fuseltrank Bezechter sein.", "tokens": ["'s", "ist", "bes\u00b7ser", ",", "n\u00fcch\u00b7tern", ",", "als", "ein", "schwer", "von", "Fu\u00b7sel\u00b7trank", "Be\u00b7zech\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJD", "$,", "KOUS", "ART", "ADJD", "APPR", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Nur h\u00fct' dich vor gef\u00e4lschter Kost, auch noch so zierlich aufgetischt;", "tokens": ["Nur", "h\u00fct'", "dich", "vor", "ge\u00b7f\u00e4lschter", "Kost", ",", "auch", "noch", "so", "zier\u00b7lich", "auf\u00b7ge\u00b7tischt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,", "ADV", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Die Kr\u00e4fte, die du st\u00e4rken willst, sie w\u00fcrden nur geschw\u00e4chter sein.", "tokens": ["Die", "Kr\u00e4f\u00b7te", ",", "die", "du", "st\u00e4r\u00b7ken", "willst", ",", "sie", "w\u00fcr\u00b7den", "nur", "ge\u00b7schw\u00e4ch\u00b7ter", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Erlogne Weisheit, Br\u00fcderschaft mit solchen, die ver\u00e4chtlich sind,", "tokens": ["Er\u00b7log\u00b7ne", "Weis\u00b7heit", ",", "Br\u00fc\u00b7der\u00b7schaft", "mit", "sol\u00b7chen", ",", "die", "ver\u00b7\u00e4cht\u00b7lich", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "APPR", "PIAT", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Eh sie dich selbst ver\u00e4chtlich macht, mu\u00dft stolz du ihr Ver\u00e4chter sein.", "tokens": ["Eh", "sie", "dich", "selbst", "ver\u00b7\u00e4cht\u00b7lich", "macht", ",", "mu\u00dft", "stolz", "du", "ihr", "Ver\u00b7\u00e4ch\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$,", "VMFIN", "ADJD", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.9": {"text": "Doch halt auch zu Geringern dich, wenn sie nur wackre Leute sind;", "tokens": ["Doch", "halt", "auch", "zu", "Ge\u00b7rin\u00b7gern", "dich", ",", "wenn", "sie", "nur", "wack\u00b7re", "Leu\u00b7te", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.10": {"text": "Je dumpfer ihr Verstand, ihr Herz wird oft nur um so echter sein.", "tokens": ["Je", "dum\u00b7pfer", "ihr", "Ver\u00b7stand", ",", "ihr", "Herz", "wird", "oft", "nur", "um", "so", "ech\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.11": {"text": "Bleib aber fern, wo man sich spreizt, wo das Gemeine vornehm tut,", "tokens": ["Bleib", "a\u00b7ber", "fern", ",", "wo", "man", "sich", "spreizt", ",", "wo", "das", "Ge\u00b7mei\u00b7ne", "vor\u00b7nehm", "tut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,", "PWAV", "PIS", "PRF", "VVFIN", "$,", "PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.12": {"text": "Und Schranzen- oder Streberpack la\u00df stets dir zum Gel\u00e4chter sein.", "tokens": ["Und", "Schran\u00b7zen", "o\u00b7der", "Stre\u00b7ber\u00b7pack", "la\u00df", "stets", "dir", "zum", "Ge\u00b7l\u00e4ch\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KON", "NN", "VVIMP", "ADV", "PPER", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.13": {"text": "So wirst du, wenn du schwerlich auch emporkommst und Karri\u00e8re machst,", "tokens": ["So", "wirst", "du", ",", "wenn", "du", "schwer\u00b7lich", "auch", "em\u00b7por\u00b7kommst", "und", "Kar\u00b7ri\u00e8\u00b7re", "machst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "ADV", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.14": {"text": "Doch deines eignen Genius getreuer, frommer W\u00e4chter sein.", "tokens": ["Doch", "dei\u00b7nes", "eig\u00b7nen", "Ge\u00b7nius", "ge\u00b7treu\u00b7er", ",", "from\u00b7mer", "W\u00e4ch\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADJD", "$,", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Du mu\u00dft am Tische d