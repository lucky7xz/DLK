{"dta.poem.7485": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XlI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eeins \u2014 zwei \u2014 drei \u2014 vier \u2014 nun, eine h\u00fcbsche Schar!", "tokens": ["\u201e", "eins", "zwei", "drei", "vier", "nun", ",", "ei\u00b7ne", "h\u00fcb\u00b7sche", "Schar", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "$(", "CARD", "$(", "CARD", "$(", "CARD", "$(", "ADV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein guter Freund, Ihr treibt das Ding ins Gro\u00dfe;", "tokens": ["Mein", "gu\u00b7ter", "Freund", ",", "Ihr", "treibt", "das", "Ding", "ins", "Gro\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Heut' ist es diese, Morgen jene Rose:", "tokens": ["Heut'", "ist", "es", "die\u00b7se", ",", "Mor\u00b7gen", "je\u00b7ne", "Ro\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "$,", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Eurem Herzen steht es sonderbar.\u201c", "tokens": ["Mit", "Eu\u00b7rem", "Her\u00b7zen", "steht", "es", "son\u00b7der\u00b7bar", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Dichter ist der Sultan Scheriar,", "tokens": ["Der", "Dich\u00b7ter", "ist", "der", "Sul\u00b7tan", "Sche\u00b7ri\u00b7ar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und liebt, wie dieser Herr, das Grandiose;", "tokens": ["Und", "liebt", ",", "wie", "die\u00b7ser", "Herr", ",", "das", "Gran\u00b7di\u00b7o\u00b7se", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PDAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis er Scheherezaden ward gewahr.", "tokens": ["Bis", "er", "Sche\u00b7he\u00b7re\u00b7za\u00b7den", "ward", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich sah wohl manch ein sch\u00f6nes Angesicht,", "tokens": ["Ich", "sah", "wohl", "manch", "ein", "sch\u00f6\u00b7nes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das ich besungen und belobt; nur schade,", "tokens": ["Das", "ich", "be\u00b7sun\u00b7gen", "und", "be\u00b7lobt", ";", "nur", "scha\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVINF", "KON", "VVFIN", "$.", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das, was ich suchte, war es immer nicht.", "tokens": ["Das", ",", "was", "ich", "such\u00b7te", ",", "war", "es", "im\u00b7mer", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und Alles, Alles mord' ich ohne Gnade,", "tokens": ["Und", "Al\u00b7les", ",", "Al\u00b7les", "mord'", "ich", "oh\u00b7ne", "Gna\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "VMFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was meinem Ideale widerspricht:", "tokens": ["Was", "mei\u00b7nem", "I\u00b7dea\u00b7le", "wi\u00b7der\u00b7spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wann kommst Du endlich, o Scheherezade?", "tokens": ["Wann", "kommst", "Du", "end\u00b7lich", ",", "o", "Sche\u00b7he\u00b7re\u00b7za\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}