{"dta.poem.21594": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Es lebt ein Gott \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ees lebt ein Gott, der Sch\u00f6pfer des Weltenrunds,\u201c", "tokens": ["\u201e", "es", "lebt", "ein", "Gott", ",", "der", "Sch\u00f6p\u00b7fer", "des", "Wel\u00b7ten\u00b7runds", ",", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So sagen sie. Doch, geben sie Kunde auch,", "tokens": ["So", "sa\u00b7gen", "sie", ".", "Doch", ",", "ge\u00b7ben", "sie", "Kun\u00b7de", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KON", "$,", "VVFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ob von dem Funkeln, das den einen", "tokens": ["Ob", "von", "dem", "Fun\u00b7keln", ",", "das", "den", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PRELS", "ART", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tropfen im Meere des Alls umleuchtet,", "tokens": ["Trop\u00b7fen", "im", "Mee\u00b7re", "des", "Alls", "um\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ob er vom Ringen menschlicher Nichtigkeit", "tokens": ["Ob", "er", "vom", "Rin\u00b7gen", "menschli\u00b7cher", "Nich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jemals vernahm? \u201eAllm\u00e4chtig und liebevoll", "tokens": ["Je\u00b7mals", "ver\u00b7nahm", "?", "\u201e", "All\u00b7m\u00e4ch\u00b7tig", "und", "lie\u00b7be\u00b7voll"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "$(", "ADJD", "KON", "ADJD"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ist er, vor seinen Vaterblicken", "tokens": ["Ist", "er", ",", "vor", "sei\u00b7nen", "Va\u00b7ter\u00b7bli\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Birgt im unendlichen Raum sich Niemand,", "tokens": ["Birgt", "im", "un\u00b7end\u00b7li\u00b7chen", "Raum", "sich", "Nie\u00b7mand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PRF", "PIS", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Kein Schmerz ist ihm, kein Jubel der Freude fremd,", "tokens": ["Kein", "Schmerz", "ist", "ihm", ",", "kein", "Ju\u00b7bel", "der", "Freu\u00b7de", "fremd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$,", "PIAT", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Gott der Liebe nennen ihn Alle ja.\u201c \u2014", "tokens": ["Den", "Gott", "der", "Lie\u00b7be", "nen\u00b7nen", "ihn", "Al\u00b7le", "ja", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "PIS", "ADV", "$.", "$(", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So sieht er also dieser Erde", "tokens": ["So", "sieht", "er", "al\u00b7so", "die\u00b7ser", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimmer ermessene Jammerw\u00fcste?", "tokens": ["Nim\u00b7mer", "er\u00b7mes\u00b7se\u00b7ne", "Jam\u00b7mer\u00b7w\u00fcs\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Er sieht das Edle unter den Fu\u00df gestampft", "tokens": ["Er", "sieht", "das", "Ed\u00b7le", "un\u00b7ter", "den", "Fu\u00df", "ge\u00b7stampft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Tiefgemeinen? Siehet in Qual und Staub", "tokens": ["Des", "Tief\u00b7ge\u00b7mei\u00b7nen", "?", "Sie\u00b7het", "in", "Qual", "und", "Staub"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sich w\u00e4lzen Millionen Herzen,", "tokens": ["Sich", "w\u00e4l\u00b7zen", "Mil\u00b7lion\u00b7en", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Blutend, gemartert ein qualschweres Dasein?", "tokens": ["Blu\u00b7tend", ",", "ge\u00b7mar\u00b7tert", "ein", "qual\u00b7schwe\u00b7res", "Da\u00b7sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Und endets nicht? Und tr\u00fcmmert und schmettert nicht", "tokens": ["Und", "en\u00b7dets", "nicht", "?", "Und", "tr\u00fcm\u00b7mert", "und", "schmet\u00b7tert", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "$.", "KON", "VVFIN", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Welt in\u2019s wahnlos friedliche Nichts zur\u00fcck? \u2014", "tokens": ["Die", "Welt", "in's", "wahn\u00b7los", "fried\u00b7li\u00b7che", "Nichts", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "ADJD", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Gott grausamer w\u00e4r\u2019 er wahrlich,", "tokens": ["Den", "Gott", "grau\u00b7sa\u00b7mer", "w\u00e4r'", "er", "wahr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als der verworfenste Menschenbube!", "tokens": ["Als", "der", "ver\u00b7wor\u00b7fens\u00b7te", "Men\u00b7schen\u00b7bu\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}