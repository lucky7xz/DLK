info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "APPR", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Wie man die G\u00f6tter empf\u00e4ngt, so begr\u00fc\u00dfte jeder mit Andacht,", "tokens": ["Wie", "man", "die", "G\u00f6t\u00b7ter", "emp\u00b7f\u00e4ngt", ",", "so", "be\u00b7gr\u00fc\u00df\u00b7te", "je\u00b7der", "mit", "An\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Was der Genius ihm, redend und bildend, erschuf.", "tokens": ["Was", "der", "Ge\u00b7nius", "ihm", ",", "re\u00b7dend", "und", "bil\u00b7dend", ",", "er\u00b7schuf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "$,", "VVPP", "KON", "ADJD", "$,", "VVFIN", "$."], "meter": "---+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "An der Glut des Gesangs entflammten des H\u00f6rers Gef\u00fchle,", "tokens": ["An", "der", "Glut", "des", "Ge\u00b7sangs", "ent\u00b7flamm\u00b7ten", "des", "H\u00f6\u00b7rers", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "An des H\u00f6rers Gef\u00fchl n\u00e4hrte der S\u00e4nger die Glut.", "tokens": ["An", "des", "H\u00f6\u00b7rers", "Ge\u00b7f\u00fchl", "n\u00e4hr\u00b7te", "der", "S\u00e4n\u00b7ger", "die", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "wo