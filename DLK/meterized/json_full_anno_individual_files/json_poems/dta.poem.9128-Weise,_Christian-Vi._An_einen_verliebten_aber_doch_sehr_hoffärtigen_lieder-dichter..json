{"dta.poem.9128": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n An einen verliebten/ aber doch sehr hoff\u00e4rtigen  \n lieder-dichter.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein freund/ was fluchst du auf die sterne/", "tokens": ["Mein", "freund", "/", "was", "fluchst", "du", "auf", "die", "ster\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was klagst du ihr verh\u00e4ngni\u00df an/", "tokens": ["Was", "klagst", "du", "ihr", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als h\u00e4tten sie dir aus der ferne", "tokens": ["Als", "h\u00e4t\u00b7ten", "sie", "dir", "aus", "der", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "PPER", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So grossen \u00fcberlast gethan?", "tokens": ["So", "gros\u00b7sen", "\u00fc\u00b7berl\u00b7ast", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach bilde dir auf ihren schein", "tokens": ["Ach", "bil\u00b7de", "dir", "auf", "ih\u00b7ren", "schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch nicht dergleichen h\u00e4ndel ein.", "tokens": ["Doch", "nicht", "derg\u00b7lei\u00b7chen", "h\u00e4n\u00b7del", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Die sterne/ welche droben schimmern/", "tokens": ["Die", "ster\u00b7ne", "/", "wel\u00b7che", "dro\u00b7ben", "schim\u00b7mern", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "PRELS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind warlich viel zu stoltz darzu/", "tokens": ["Sind", "war\u00b7lich", "viel", "zu", "stoltz", "dar\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKA", "ADJD", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als da\u00df sie sich darum bek\u00fcmmern/", "tokens": ["Als", "da\u00df", "sie", "sich", "da\u00b7rum", "be\u00b7k\u00fcm\u00b7mern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was ich allhie auf erden thu:", "tokens": ["Was", "ich", "all\u00b7hie", "auf", "er\u00b7den", "thu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Drum dencke da\u00df dir auch so ist/", "tokens": ["Drum", "den\u00b7cke", "da\u00df", "dir", "auch", "so", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KOUS", "PPER", "ADV", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dieweil du meines gleichen bist.", "tokens": ["Die\u00b7weil", "du", "mei\u00b7nes", "glei\u00b7chen", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Es sind ja freylich andre sachen", "tokens": ["Es", "sind", "ja", "frey\u00b7lich", "and\u00b7re", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der hohen satzung unterthan:", "tokens": ["Der", "ho\u00b7hen", "sat\u00b7zung", "un\u00b7ter\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch darffst du nicht die rechnung machen", "tokens": ["Doch", "darffst", "du", "nicht", "die", "rech\u00b7nung", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als gieng es dich ingleichen an:", "tokens": ["Als", "gieng", "es", "dich", "in\u00b7glei\u00b7chen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn was dein schwacher sinn begehrt/", "tokens": ["Denn", "was", "dein", "schwa\u00b7cher", "sinn", "be\u00b7gehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist offt kaum deiner sorgen wehrt.", "tokens": ["Ist", "offt", "kaum", "dei\u00b7ner", "sor\u00b7gen", "wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Da heist ein irrthum der gedancken", "tokens": ["Da", "heist", "ein", "irr\u00b7thum", "der", "ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Alsbald der sterne gauckelspiel/", "tokens": ["Als\u00b7bald", "der", "ster\u00b7ne", "gau\u00b7ckel\u00b7spiel", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn die sehnsucht aus den schrancken", "tokens": ["Und", "wenn", "die", "sehn\u00b7sucht", "aus", "den", "schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PDS", "VVFIN", "APPR", "ART", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der klugheit sich entreissen will/", "tokens": ["Der", "klug\u00b7heit", "sich", "en\u00b7treis\u00b7sen", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So mu\u00df der unbewie\u00dfne zwang", "tokens": ["So", "mu\u00df", "der", "un\u00b7be\u00b7wie\u00df\u00b7ne", "zwang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des gl\u00fcckes vor die richterbanck.", "tokens": ["Des", "gl\u00fc\u00b7ckes", "vor", "die", "rich\u00b7ter\u00b7banck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Wann wir begehrten was wir solten/", "tokens": ["Wann", "wir", "be\u00b7gehr\u00b7ten", "was", "wir", "sol\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PWS", "PPER", "VMFIN", "$("], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So st\u00fcnd es allenthalben gut/", "tokens": ["So", "st\u00fcnd", "es", "al\u00b7len\u00b7thal\u00b7ben", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wir bek\u00e4men was wir wolten;", "tokens": ["Und", "wir", "be\u00b7k\u00e4\u00b7men", "was", "wir", "wol\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hingegen wo man das nicht thut/", "tokens": ["Hin\u00b7ge\u00b7gen", "wo", "man", "das", "nicht", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PIS", "PDS", "PTKNEG", "VVFIN", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Da qv\u00e4lt man sich mit ungedult:", "tokens": ["Da", "qv\u00e4lt", "man", "sich", "mit", "un\u00b7ge\u00b7dult", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPR", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch hat der himmel keine schuld.", "tokens": ["Doch", "hat", "der", "him\u00b7mel", "kei\u00b7ne", "schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Mein freund/ der stern ist im gehirne/", "tokens": ["Mein", "freund", "/", "der", "stern", "ist", "im", "ge\u00b7hir\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "ADJA", "VAFIN", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der dir so ungl\u00fcckselig strahlt/", "tokens": ["Der", "dir", "so", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "strahlt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hinter deiner eignen stirne", "tokens": ["Und", "hin\u00b7ter", "dei\u00b7ner", "eig\u00b7nen", "stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Steht dein verh\u00e4ngn\u00fc\u00df abgemahlt.", "tokens": ["Steht", "dein", "ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "ab\u00b7ge\u00b7mahlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der himmel l\u00e4st dich wohl zur ruh/", "tokens": ["Der", "him\u00b7mel", "l\u00e4st", "dich", "wohl", "zur", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sprich du nur selbst dein ja darzu.", "tokens": ["Sprich", "du", "nur", "selbst", "dein", "ja", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PPOSAT", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}