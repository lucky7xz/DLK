{"textgrid.poem.58522": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "560. Ursprung des B\u00f6sen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach nein, du wesentliches Gut,", "tokens": ["Ach", "nein", ",", "du", "we\u00b7sent\u00b7li\u00b7ches", "Gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das B\u00f6se kann aus ", "tokens": ["Das", "B\u00f6\u00b7se", "kann", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wer seine Liebe dir entnommen", "tokens": ["Wer", "sei\u00b7ne", "Lie\u00b7be", "dir", "ent\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seinen ", "tokens": ["Und", "sei\u00b7nen"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Der ist es, der die S\u00fcnde macht,", "tokens": ["Der", "ist", "es", ",", "der", "die", "S\u00fcn\u00b7de", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der bringt sich selbst in Not und Schmerzen,", "tokens": ["Der", "bringt", "sich", "selbst", "in", "Not", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der baut ihm selbst die H\u00f6ll' im Herzen \u2013", "tokens": ["Der", "baut", "ihm", "selbst", "die", "H\u00f6ll'", "im", "Her\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wer 's Licht verl\u00e4\u00dft, ist in der Nacht.", "tokens": ["Wer", "'s", "Licht", "ver\u00b7l\u00e4\u00dft", ",", "ist", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$,", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Ich bin dein Str\u00e4hlchen, meine Sonn',", "tokens": ["Ich", "bin", "dein", "Str\u00e4hl\u00b7chen", ",", "mei\u00b7ne", "Sonn'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "La\u00df mich von dir mich nimmer scheiden,", "tokens": ["La\u00df", "mich", "von", "dir", "mich", "nim\u00b7mer", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So bleib' ich frei von S\u00fcnd' und Leiden", "tokens": ["So", "bleib'", "ich", "frei", "von", "S\u00fcnd'", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und leb' in deines Lichtes Wonn'!", "tokens": ["Und", "leb'", "