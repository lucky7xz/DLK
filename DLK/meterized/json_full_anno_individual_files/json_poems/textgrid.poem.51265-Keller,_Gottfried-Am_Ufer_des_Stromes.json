], "pos": ["NE", "KON", "ADJD", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und siehst auch mein ergrauendes Haar?", "tokens": ["Und", "siehst", "auch", "mein", "er\u00b7grau\u00b7en\u00b7des", "Haar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.14": {"line.1": {"text": "Befiel' mich ein F\u00fcnklein Lieb zu ihr,", "tokens": ["Be\u00b7fiel'", "mich", "ein", "F\u00fcn\u00b7klein", "Lieb", "zu", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "APPR", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Laut rief' ich es von der Stelle hier,", "tokens": ["Laut", "rie\u00b7f'", "ich", "es", "von", "der", "Stel\u00b7le", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rief's laut in der Wellen rauschenden Gang,", "tokens": ["Rie\u00b7f's", "laut", "in", "der", "Wel\u00b7len", "rau\u00b7schen\u00b7den", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "---+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mich d\u00fcnkt' es der allersch\u00f6nste Gesang!", "tokens": ["Mich", "d\u00fcnkt'", "es", "der", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Le