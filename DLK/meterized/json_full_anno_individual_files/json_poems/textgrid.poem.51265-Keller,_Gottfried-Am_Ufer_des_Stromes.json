 "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich glaube, sie ahnt' es und l\u00e4chelte fein,", "tokens": ["Ich", "glau\u00b7be", ",", "sie", "ahnt'", "es", "und", "l\u00e4\u00b7chel\u00b7te", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch wu\u00dft ich nicht: sang's in ihr ja oder nein.", "tokens": ["Doch", "wu\u00dft", "ich", "nicht", ":", "sang's", "in", "ihr", "ja", "o\u00b7der", "nein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "APPR", "PPER", "ADV", "KON", "PTKANT", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.17": {"line.1": {"text": "Der Sommer war warm und der Winter kalt,", "tokens": ["Der", "Som\u00b7mer", "war", "warm", "und", "der", "Win\u00b7ter", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Zeit verging, und wir wurden alt;", "tokens": ["Die", "Zeit", "ver\u00b7ging", ",", "und", "wir", "wur\u00b7den", "alt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als ich zum letzten Mal sie sah,", "tokens": ["Als", "ich", "zum", "letz\u00b7ten", "Mal", "sie", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter