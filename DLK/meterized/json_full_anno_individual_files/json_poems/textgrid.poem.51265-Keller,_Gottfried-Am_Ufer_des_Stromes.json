{"textgrid.poem.51265": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Am Ufer des Stromes", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Graulockig ein Mann und ein blonder Kam'rad", "tokens": ["Grau\u00b7lo\u00b7ckig", "ein", "Mann", "und", "ein", "blon\u00b7der", "Kam'\u00b7rad"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Spazieren an flie\u00dfenden Wassers Gestad;", "tokens": ["Spa\u00b7zie\u00b7ren", "an", "flie\u00b7\u00dfen\u00b7den", "Was\u00b7sers", "Ge\u00b7stad", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der \u00c4ltere kehrt sich zum Jungen und spricht:", "tokens": ["Der", "\u00c4l\u00b7te\u00b7re", "kehrt", "sich", "zum", "Jun\u00b7gen", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u00bbwas schneidest du f\u00fcr ein betr\u00fcbtes Gesicht?\u00ab \u2013", "tokens": ["\u00bb", "was", "schnei\u00b7dest", "du", "f\u00fcr", "ein", "be\u00b7tr\u00fcb\u00b7tes", "Ge\u00b7sicht", "?", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "\u00bblieb fand ich ein M\u00e4dchen und hab ihm's gesagt,", "tokens": ["\u00bb", "lieb", "fand", "ich", "ein", "M\u00e4d\u00b7chen", "und", "hab", "ihm's", "ge\u00b7sagt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "ART", "NN", "KON", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sie fl\u00fcstert ein Nein, kaum da\u00df ich gefragt,", "tokens": ["Sie", "fl\u00fcs\u00b7tert", "ein", "Nein", ",", "kaum", "da\u00df", "ich", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PTKANT", "$,", "ADV", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und alles im Nu \u2013 nun beklemmt's mir die Brust,", "tokens": ["Und", "al\u00b7les", "im", "Nu", "\u2013", "nun", "be\u00b7klemmt's", "mir", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPRART", "ADV", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da\u00df Herz ich und Mund nicht zu halten gewu\u00dft!\u00ab", "tokens": ["Da\u00df", "Herz", "ich", "und", "Mund", "nicht", "zu", "hal\u00b7ten", "ge\u00b7wu\u00dft", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "PPER", "KON", "NN", "PTKNEG", "PTKZU", "VVINF", "VVPP", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Und jener erwidert: \u00bbDes F\u00e4hrmanns Magd", "tokens": ["Und", "je\u00b7ner", "er\u00b7wi\u00b7dert", ":", "\u00bb", "Des", "F\u00e4hr\u00b7manns", "Magd"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "VVPP", "$.", "$(", "ART", "NN", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Siehst du, die \u00fcber dem Strome ragt,", "tokens": ["Siehst", "du", ",", "die", "\u00fc\u00b7ber", "dem", "Stro\u00b7me", "ragt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gering und arm und der Zierde bar,", "tokens": ["Ge\u00b7ring", "und", "arm", "und", "der", "Zier\u00b7de", "bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und siehst auch mein ergrauendes Haar?", "tokens": ["Und", "siehst", "auch", "mein", "er\u00b7grau\u00b7en\u00b7des", "Haar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Befiel' mich ein F\u00fcnklein Lieb zu ihr,", "tokens": ["Be\u00b7fiel'", "mich", "ein", "F\u00fcn\u00b7klein", "Lieb", "zu", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "APPR", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Laut rief' ich es von der Stelle hier,", "tokens": ["Laut", "rie\u00b7f'", "ich", "es", "von", "der", "Stel\u00b7le", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rief's laut in der Wellen rauschenden Gang,", "tokens": ["Rie\u00b7f's", "laut", "in", "der", "Wel\u00b7len", "rau\u00b7schen\u00b7den", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "---+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mich d\u00fcnkt' es der allersch\u00f6nste Gesang!", "tokens": ["Mich", "d\u00fcnkt'", "es", "der", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Leicht schlug mir in meiner Jugend das Herz,", "tokens": ["Leicht", "schlug", "mir", "in", "mei\u00b7ner", "Ju\u00b7gend", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und m\u00fc\u00dfig schweifte der Blick allw\u00e4rts;", "tokens": ["Und", "m\u00fc\u00b7\u00dfig", "schweif\u00b7te", "der", "Blick", "all\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+++-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Rasch hab ich so manches Gest\u00e4ndnis gemacht,", "tokens": ["Rasch", "hab", "ich", "so", "man\u00b7ches", "Ge\u00b7st\u00e4nd\u00b7nis", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "++--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die ein' hat geweint und die andre gelacht.", "tokens": ["Die", "ein'", "hat", "ge\u00b7weint", "und", "die", "and\u00b7re", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "VVPP", "KON", "ART", "ADJA", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Bei ", "tokens": ["Bei"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Wie sehr es auch sterbend im Busen gezuckt;", "tokens": ["Wie", "sehr", "es", "auch", "ster\u00b7bend", "im", "Bu\u00b7sen", "ge\u00b7zuckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich glaube, sie ahnt' es und l\u00e4chelte fein,", "tokens": ["Ich", "glau\u00b7be", ",", "sie", "ahnt'", "es", "und", "l\u00e4\u00b7chel\u00b7te", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch wu\u00dft ich nicht: sang's in ihr ja oder nein.", "tokens": ["Doch", "wu\u00dft", "ich", "nicht", ":", "sang's", "in", "ihr", "ja", "o\u00b7der", "nein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "APPR", "PPER", "ADV", "KON", "PTKANT", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Der Sommer war warm und der Winter kalt,", "tokens": ["Der", "Som\u00b7mer", "war", "warm", "und", "der", "Win\u00b7ter", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Zeit verging, und wir wurden alt;", "tokens": ["Die", "Zeit", "ver\u00b7ging", ",", "und", "wir", "wur\u00b7den", "alt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als ich zum letzten Mal sie sah,", "tokens": ["Als", "ich", "zum", "letz\u00b7ten", "Mal", "sie", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lag sie im Leichenschmucke da.", "tokens": ["Lag", "sie", "im", "Lei\u00b7chen\u00b7schmu\u00b7cke", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Fest waren die Augen zugetan,", "tokens": ["Fest", "wa\u00b7ren", "die", "Au\u00b7gen", "zu\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie schauten nicht mich noch die Welt mehr an;", "tokens": ["Sie", "schau\u00b7ten", "nicht", "mich", "noch", "die", "Welt", "mehr", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPER", "ADV", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch auf dem Munde bleich und tot,", "tokens": ["Doch", "auf", "dem", "Mun\u00b7de", "bleich", "und", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da l\u00e4chelt's noch leise wie ein Spott.", "tokens": ["Da", "l\u00e4\u00b7chelt's", "noch", "lei\u00b7se", "wie", "ein", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Mir lispelt's im Ohre: 'O tr\u00e4ger Mann,", "tokens": ["Mir", "lis\u00b7pelt's", "im", "Oh\u00b7re", ":", "'o", "tr\u00e4\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der so mit Worten geizen kann!", "tokens": ["Der", "so", "mit", "Wor\u00b7ten", "gei\u00b7zen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hattest den Schl\u00fcssel zum seligen Haus,", "tokens": ["Du", "hat\u00b7test", "den", "Schl\u00fcs\u00b7sel", "zum", "se\u00b7li\u00b7gen", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wo fliegen die Engel hinein und hinaus!", "tokens": ["Wo", "flie\u00b7gen", "die", "En\u00b7gel", "hin\u00b7ein", "und", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Du hattest den Schl\u00fcssel zum goldenen Schrein", "tokens": ["Du", "hat\u00b7test", "den", "Schl\u00fcs\u00b7sel", "zum", "gol\u00b7de\u00b7nen", "Schrein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "F\u00fcr alle zwei beide, nun lieg ich allein!'", "tokens": ["F\u00fcr", "al\u00b7le", "zwei", "bei\u00b7de", ",", "nun", "lieg", "ich", "al\u00b7lein", "!", "'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "CARD", "PIS", "$,", "ADV", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da donnert' die Orgel, da psaltert' der Chor,", "tokens": ["Da", "don\u00b7nert'", "die", "Or\u00b7gel", ",", "da", "psal\u00b7tert'", "der", "Chor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und sie trugen hinaus, was ich elend verlor!\u00ab", "tokens": ["Und", "sie", "tru\u00b7gen", "hin\u00b7aus", ",", "was", "ich", "e\u00b7lend", "ver\u00b7lor", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$.", "$("], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.11": {"line.1": {"text": "Graulockig ein Mann und ein blonder Kam'rad", "tokens": ["Grau\u00b7lo\u00b7ckig", "ein", "Mann", "und", "ein", "blon\u00b7der", "Kam'\u00b7rad"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Spazieren an flie\u00dfenden Wassers Gestad;", "tokens": ["Spa\u00b7zie\u00b7ren", "an", "flie\u00b7\u00dfen\u00b7den", "Was\u00b7sers", "Ge\u00b7stad", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der \u00c4ltere kehrt sich zum Jungen und spricht:", "tokens": ["Der", "\u00c4l\u00b7te\u00b7re", "kehrt", "sich", "zum", "Jun\u00b7gen", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u00bbwas schneidest du f\u00fcr ein betr\u00fcbtes Gesicht?\u00ab \u2013", "tokens": ["\u00bb", "was", "schnei\u00b7dest", "du", "f\u00fcr", "ein", "be\u00b7tr\u00fcb\u00b7tes", "Ge\u00b7sicht", "?", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.12": {"line.1": {"text": "\u00bblieb fand ich ein M\u00e4dchen und hab ihm's gesagt,", "tokens": ["\u00bb", "lieb", "fand", "ich", "ein", "M\u00e4d\u00b7chen", "und", "hab", "ihm's", "ge\u00b7sagt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "ART", "NN", "KON", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sie fl\u00fcstert ein Nein, kaum da\u00df ich gefragt,", "tokens": ["Sie", "fl\u00fcs\u00b7tert", "ein", "Nein", ",", "kaum", "da\u00df", "ich", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", 