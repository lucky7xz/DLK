{"textgrid.poem.65305": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Meeresstille", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wirf Rosenbl\u00e4tter in die Fluth:", "tokens": ["Wirf", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "in", "die", "Fluth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie ist so spiegelglatt.", "tokens": ["Sie", "ist", "so", "spie\u00b7gel\u00b7glatt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich fische sie mit meinem Hut", "tokens": ["Ich", "fi\u00b7sche", "sie", "mit", "mei\u00b7nem", "Hut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fcsse jedes Blatt.", "tokens": ["Und", "k\u00fcs\u00b7se", "je\u00b7des", "Blatt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und streust du Bl\u00e4tter auf das Meer,", "tokens": ["Und", "streust", "du", "Bl\u00e4t\u00b7ter", "auf", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schaust du selbst hinein \u2013", "tokens": ["So", "schaust", "du", "selbst", "hin\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann schwimmen zwischen ihnen her", "tokens": ["Dann", "schwim\u00b7men", "zwi\u00b7schen", "ih\u00b7nen", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vier volle R\u00f6selein.", "tokens": ["Vier", "vol\u00b7le", "R\u00f6\u00b7se\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Wangen und die Lippen dein,", "tokens": ["Die", "Wan\u00b7gen", "und", "die", "Lip\u00b7pen", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie mein' ich alle vier.", "tokens": ["Sie", "mein'", "ich", "al\u00b7le", "vier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, schw\u00e4mmen diese R\u00f6selein", "tokens": ["Ach", ",", "schw\u00e4m\u00b7men", "die\u00b7se", "R\u00f6\u00b7se\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch auch heran zu mir!", "tokens": ["Doch", "auch", "he\u00b7ran", "zu", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wirf Rosenbl\u00e4tter in die Fluth:", "tokens": ["Wirf", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "in", "die", "Fluth", ":"], "token_info": ["word", "word", "word", "word", "word",