{"dta.poem.21264": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein Gesch\u00fctz.  \n Berlin, den 3 October 1760.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "O du, dem gl&#252;hend Eisen, donnernd\nFeuer", "tokens": ["O", "du", ",", "dem", "gl", "&#252;", "hend", "Ei\u00b7sen", ",", "don\u00b7nernd", "Feu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "XML_entity", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "ADJD", "$(", "ADJD", "NN", "$,", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus offnem Aetnaschlunde flammt,", "tokens": ["Aus", "off\u00b7nem", "A\u00b7e\u00b7tnasc\u00b7hlun\u00b7de", "flammt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die frommen Dichter zu zerschmettern,", "tokens": ["Die", "from\u00b7men", "Dich\u00b7ter", "zu", "zer\u00b7schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das aus der H\u00f6lle stammt!", "tokens": ["Das", "aus", "der", "H\u00f6l\u00b7le", "stammt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer zur Verheerung bl\u00fchender Ge-", "tokens": ["Wer", "zur", "Ver\u00b7hee\u00b7rung", "bl\u00fc\u00b7hen\u00b7der", "Ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich an das Sonnenlicht gebracht,", "tokens": ["Dich", "an", "das", "Son\u00b7nen\u00b7licht", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat ohne Reue seine Mutter, seine", "tokens": ["Hat", "oh\u00b7ne", "Reu\u00b7e", "sei\u00b7ne", "Mut\u00b7ter", ",", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "APPR", "NN", "PPOSAT", "NN", "$,", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Frohlockend umgebracht.", "tokens": ["Froh\u00b7lo\u00b7ckend", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ganz nahe war ich schon dem Styx,", "tokens": ["Ganz", "na\u00b7he", "war", "ich", "schon", "dem", "Styx", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem giftgeschwollnen Cerberus;", "tokens": ["Dem", "gift\u00b7ge\u00b7schwoll\u00b7nen", "Cer\u00b7be\u00b7rus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6rte schon das Rad Ixions rasseln,", "tokens": ["Ich", "h\u00f6r\u00b7te", "schon", "das", "Rad", "I\u00b7xions", "ras\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NE", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Brut des Danaus,", "tokens": ["Die", "Brut", "des", "Da\u00b7naus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Verdammt zum Spott bey bodenlosen", "tokens": ["Ver\u00b7dammt", "zum", "Spott", "bey", "bo\u00b7den\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Minos Antlitz, und das Feld", "tokens": ["Und", "Mi\u00b7nos", "Ant\u00b7litz", ",", "und", "das", "Feld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "NE", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Elysiens; den grossen Ahnherrn eines", "tokens": ["E\u00b7ly\u00b7si\u00b7ens", ";", "den", "gros\u00b7sen", "Ahn\u00b7herrn", "ei\u00b7nes"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Urenkels, und sein Zelt", "tokens": ["Ur\u00b7en\u00b7kels", ",", "und", "sein", "Zelt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Voll tapfrer Brennen sah ich: ihre", "tokens": ["Voll", "tapf\u00b7rer", "Bren\u00b7nen", "sah", "ich", ":", "ih\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "PPER", "$.", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Fest bey jedem Freudenmahl", "tokens": ["Ihr", "Fest", "bey", "je\u00b7dem", "Freu\u00b7den\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist er, der wider sechs Monarchen sicht,", "tokens": ["Ist", "er", ",", "der", "wi\u00b7der", "sechs", "Mon\u00b7ar\u00b7chen", "sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Satrapen ohne Zahl.", "tokens": ["Sa\u00b7tra\u00b7pen", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Schon s\u00e4ng\u2019 ich seine j\u00fcngste That: wie", "tokens": ["Schon", "s\u00e4ng'", "ich", "sei\u00b7ne", "j\u00fcngs\u00b7te", "That", ":", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$.", "PWAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Meer von Feinden ihn umfieng,", "tokens": ["Ein", "Meer", "von", "Fein\u00b7den", "ihn", "um\u00b7fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er aber seinen Weg hindurch auf zehen-", "tokens": ["Er", "a\u00b7ber", "sei\u00b7nen", "Weg", "hin\u00b7durch", "auf", "ze\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "PAV", "APPR", "TRUNC"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zertretnen Schedeln gieng.", "tokens": ["Zer\u00b7tret\u00b7nen", "Sche\u00b7deln", "gieng", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Alc\u00e4us w\u00fcrde jetzt mein Lied be-", "tokens": ["Al\u00b7c\u00e4us", "w\u00fcr\u00b7de", "jetzt", "mein", "Lied", "be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "PPOSAT", "NN", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon s\u00e4h ich C\u00e4sarn lauschend nahn,", "tokens": ["Schon", "s\u00e4h", "ich", "C\u00e4\u00b7sarn", "lau\u00b7schend", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihm den weisen Antonin, und den", "tokens": ["Mit", "ihm", "den", "wei\u00b7sen", "An\u00b7to\u00b7nin", ",", "und", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,", "KON", "ART"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gefeyrten Julian.", "tokens": ["Ge\u00b7feyr\u00b7ten", "Ju\u00b7li\u00b7an", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Allein Merkur stand neben mir, und", "tokens": ["Al\u00b7lein", "Mer\u00b7kur", "stand", "ne\u00b7ben", "mir", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "NE", "VVFIN", "APPR", "PPER", "$,", "KON"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Durch seinen wunderbaren Stab", "tokens": ["Durch", "sei\u00b7nen", "wun\u00b7der\u00b7ba\u00b7ren", "Stab"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Ball, der mich ins Reich der Nacht", "tokens": ["Den", "Ball", ",", "der", "mich", "ins", "Reich", "der", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von meinen Schl\u00e4fen ab.", "tokens": ["Von", "mei\u00b7nen", "Schl\u00e4\u00b7fen", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Denn ich soll noch die Laute st\u00e4rker", "tokens": ["Denn", "ich", "soll", "noch", "die", "Lau\u00b7te", "st\u00e4r\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wann er durch Weihrauchwolken zeucht,", "tokens": ["Wann", "er", "durch", "Weih\u00b7rauch\u00b7wol\u00b7ken", "zeucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kriegesfurie gefesselt an dem Wa-", "tokens": ["Die", "Krie\u00b7ges\u00b7fu\u00b7rie", "ge\u00b7fes\u00b7selt", "an", "dem", "Wa"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des Ueberwinders keucht;", "tokens": ["Des", "Ue\u00b7ber\u00b7win\u00b7ders", "keucht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wann er, auf einem Throne von", "tokens": ["Wann", "er", ",", "auf", "ei\u00b7nem", "Thro\u00b7ne", "von"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rund um sich her der K\u00fcnste Kranz,", "tokens": ["Rund", "um", "sich", "her", "der", "K\u00fcns\u00b7te", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "APZR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und wir im Musentempel seine Siege", "tokens": ["Und", "wir", "im", "Mu\u00b7sen\u00b7tem\u00b7pel", "sei\u00b7ne", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Versteckt in Spiel und Tanz;", "tokens": ["Ver\u00b7steckt", "in", "Spiel", "und", "Tanz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wann er, ein Gott Osir! durch unsre", "tokens": ["Wann", "er", ",", "ein", "Gott", "O\u00b7sir", "!", "durch", "uns\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "NE", "$.", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im seligsten Triumphe f\u00e4hrt,", "tokens": ["Im", "se\u00b7ligs\u00b7ten", "Tri\u00b7um\u00b7phe", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Indess der Ueberfluss auf jede seiner", "tokens": ["In\u00b7dess", "der", "Ue\u00b7ber\u00b7fluss", "auf", "je\u00b7de", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PIAT", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein ganzes F\u00fcllhorn leert.", "tokens": ["Ein", "gan\u00b7zes", "F\u00fcll\u00b7horn", "leert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}