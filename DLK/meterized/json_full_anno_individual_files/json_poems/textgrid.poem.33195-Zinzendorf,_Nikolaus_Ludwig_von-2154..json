{"textgrid.poem.33195": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2154.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der die braut hat, das ist der br\u00e4utigam.", "tokens": ["Der", "die", "braut", "hat", ",", "das", "ist", "der", "br\u00e4u\u00b7ti\u00b7gam", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VVFIN", "VAFIN", "$,", "PDS", "VAFIN", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Itzt komme ich auf dinge, hilf Gott, da\u00df mirs gelinge, etc.", "tokens": ["Itzt", "kom\u00b7me", "ich", "auf", "din\u00b7ge", ",", "hilf", "Gott", ",", "da\u00df", "mirs", "ge\u00b7lin\u00b7ge", ",", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "VVIMP", "NN", "$,", "KOUS", "NE", "VVFIN", "$,", "ADV"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wir sind dazu geschaffen, in seinem arm zu schlafen, weil alle seelen seine seyn.", "tokens": ["Wir", "sind", "da\u00b7zu", "ge\u00b7schaf\u00b7fen", ",", "in", "sei\u00b7nem", "arm", "zu", "schla\u00b7fen", ",", "weil", "al\u00b7le", "see\u00b7len", "sei\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$,", "APPR", "PPOSAT", "ADJD", "PTKZU", "VVINF", "$,", "KOUS", "PIS", "VVFIN", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+--+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Da schied er sie von beyden, das schw\u00e4chste muste scheiden, etc.", "tokens": ["Da", "schied", "er", "sie", "von", "bey\u00b7den", ",", "das", "schw\u00e4chs\u00b7te", "mus\u00b7te", "schei\u00b7den", ",", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PIAT", "$,", "ART", "ADJA", "VMFIN", "VVINF", "$,", "ADV"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Wie bring ichs doch zuwegen, da\u00df ich die ehe-segen, etc.", "tokens": ["Wie", "bring", "ichs", "doch", "zu\u00b7we\u00b7gen", ",", "da\u00df", "ich", "die", "e\u00b7he\u00b7se\u00b7gen", ",", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["PWAV", "VVFIN", "PIS", "ADV", "VVINF", "$,", "KOUS", "PPER", "ART", "ADJA", "$,", "ADV"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Und Gott das keusche L\u00e4mmlein bl\u00e4\u00dft auf das ehe-fl\u00e4mmlein, etc.", "tokens": ["Und", "Gott", "das", "keu\u00b7sche", "L\u00e4mm\u00b7lein", "bl\u00e4\u00dft", "auf", "das", "e\u00b7he\u00b7fl\u00e4mm\u00b7lein", ",", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Da\u00df wir das p\u00fcnctlein treffen, und nicht wie Satans affen, etc.", "tokens": ["Da\u00df", "wir", "das", "p\u00fcnct\u00b7lein", "tref\u00b7fen", ",", "und", "nicht", "wie", "Sa\u00b7tans", "af\u00b7fen", ",", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["KOUS",