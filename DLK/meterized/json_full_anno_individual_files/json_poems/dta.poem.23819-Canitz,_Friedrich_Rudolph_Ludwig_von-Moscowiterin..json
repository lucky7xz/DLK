{"dta.poem.23819": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Moscowiterin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ist der Wunder-Peltz beh\u00e4ngt mit hundert Schw\u00e4n-", "tokens": ["Wer", "ist", "der", "Wun\u00b7der\u00b7Peltz", "be\u00b7h\u00e4ngt", "mit", "hun\u00b7dert", "Schw\u00e4n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVFIN", "APPR", "CARD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die uns der K\u00fcrschner hohlt von Moskau kalten Gr\u00e4ntze\u0303?", "tokens": ["Die", "uns", "der", "K\u00fcrschner", "hohlt", "von", "Mos\u00b7kau", "kal\u00b7ten", "Gr\u00e4nt\u00b7z\u1ebd", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Man sagt da\u00df Pr\u00fcgeln dort der Liebe Zeichen sey/", "tokens": ["Man", "sagt", "da\u00df", "Pr\u00fc\u00b7geln", "dort", "der", "Lie\u00b7be", "Zei\u00b7chen", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOUS", "NN", "ADV", "ART", "NN", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Warum schl\u00e4gt ihr der Mann nicht Arm und Bein", "tokens": ["Wa\u00b7rum", "schl\u00e4gt", "ihr", "der", "Mann", "nicht", "Arm", "und", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word", "word",