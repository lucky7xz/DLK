{"textgrid.poem.50947": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und als die Nixe den Fischer gefa\u00dft,", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als die Nixe den Fischer gefa\u00dft,", "tokens": ["Und", "als", "die", "Ni\u00b7xe", "den", "Fi\u00b7scher", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da machte sie sich abseiten;", "tokens": ["Da", "mach\u00b7te", "sie", "sich", "ab\u00b7sei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Sie schwamm hinaus mit l\u00fcsterner Hast,", "tokens": ["Sie", "schwamm", "hin\u00b7aus", "mit", "l\u00fcs\u00b7ter\u00b7ner", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hinaus in die n\u00e4chtlichen Weiten.", "tokens": ["Hin\u00b7aus", "in", "die", "n\u00e4cht\u00b7li\u00b7chen", "Wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Sie schwamm in gewaltigen Kreisen herum,", "tokens": ["Sie", "schwamm", "in", "ge\u00b7wal\u00b7ti\u00b7gen", "Krei\u00b7sen", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-