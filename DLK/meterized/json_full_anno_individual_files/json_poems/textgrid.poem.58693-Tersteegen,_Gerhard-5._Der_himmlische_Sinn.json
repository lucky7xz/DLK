{"textgrid.poem.58693": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "5. Der himmlische Sinn", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ist denn nun nichts mehr f\u00fcr meinen Geist auf Erden,", "tokens": ["So", "ist", "denn", "nun", "nichts", "mehr", "f\u00fcr", "mei\u00b7nen", "Geist", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PIS", "ADV", "APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein himmlischer Magnet ber\u00fchrt und ziehet mich,", "tokens": ["Mein", "himm\u00b7li\u00b7scher", "Mag\u00b7net", "be\u00b7r\u00fchrt", "und", "zie\u00b7het", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "KON", "VVFIN", "PPER", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mein Herze, Sinn und Sucht will er allein f\u00fcr sich \u2013", "tokens": ["Mein", "Her\u00b7ze", ",", "Sinn", "und", "Sucht", "will", "er", "al\u00b7lein", "f\u00fcr", "sich", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$,", "NN", "KON", "VVFIN", "VMFIN", "PPER", "ADV", "APPR", "PRF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da, wo mein Schatz ist, mu\u00df mein Herz gefunden werden.", "tokens": ["Da", ",", "wo", "mein", "Schatz", "ist", ",", "mu\u00df", "mein", "Herz", "ge\u00b7fun\u00b7den", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "w