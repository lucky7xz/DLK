{"dta.poem.8826": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "V .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Entfuhr auch damals manches Ach, entfuhr auch manche", "tokens": ["Ent\u00b7fuhr", "auch", "da\u00b7mals", "man\u00b7ches", "Ach", ",", "ent\u00b7fuhr", "auch", "man\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PIS", "ITJ", "$,", "VVFIN", "ADV", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es brachte jedes L\u00fcftchen mir aus deinen Locken s\u00fc\u00dfen", "tokens": ["Es", "brach\u00b7te", "je\u00b7des", "L\u00fcft\u00b7chen", "mir", "aus", "dei\u00b7nen", "Lo\u00b7cken", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Und Rede stand dein blitzend Aug', so schien's, auf", "tokens": ["Und", "Re\u00b7de", "stand", "dein", "blit\u00b7zend", "Aug'", ",", "so", "schien's", ",", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "ADJD", "NN", "$,", "ADV", "ADJA", "$,", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "An deiner Stimme hing ich fest, an deiner Lippen", "tokens": ["An", "dei\u00b7ner", "Stim\u00b7me", "hing", "ich", "fest", ",", "an", "dei\u00b7ner", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Musik, bey der mein Herz geh\u00fcpft, wo flohst du hin,", "tokens": ["Mu\u00b7sik", ",", "bey", "der", "mein", "Herz", "ge\u00b7h\u00fcpft", ",", "wo", "flohst", "du", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "PPOSAT", "NN", "VVPP", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Da mir die leeren Hoffnungen gestoben in die leere Luft,", "tokens": ["Da", "mir", "die", "lee\u00b7ren", "Hoff\u00b7nun\u00b7gen", "ge\u00b7sto\u00b7ben", "in", "die", "lee\u00b7re", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Der Tr\u00f6ster unberufne Schaar, wie wird sie nun zur", "tokens": ["Der", "Tr\u00f6s\u00b7ter", "un\u00b7be\u00b7ruf\u00b7ne", "Schaar", ",", "wie", "wird", "sie", "nun", "zur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "APPRART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An einer sch\u00f6nen Brust zu ruh'n, das ist ein Trost,", "tokens": ["An", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Brust", "zu", "ruh'n", ",", "das", "ist", "ein", "Trost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es ist verha\u00dft mein eigen Selbst in jeder andern Lage", "tokens": ["Es", "ist", "ver\u00b7ha\u00dft", "mein", "ei\u00b7gen", "Selbst", "in", "je\u00b7der", "an\u00b7dern", "La\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "PPOSAT", "NN", "ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}