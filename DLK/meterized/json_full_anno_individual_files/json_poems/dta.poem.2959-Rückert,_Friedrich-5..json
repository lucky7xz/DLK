{"dta.poem.2959": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Willst du geheiliget, verg\u00f6ttert seyn in Schriften,", "tokens": ["Willst", "du", "ge\u00b7hei\u00b7li\u00b7get", ",", "ver\u00b7g\u00f6t\u00b7tert", "seyn", "in", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "VVPP", "VAINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00dft du neue Lehr' und neuen Glauben stiften.", "tokens": ["So", "mu\u00dft", "du", "neu\u00b7e", "Lehr'", "und", "neu\u00b7en", "Glau\u00b7ben", "stif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch Ehre v\u00f6llig rein ist solchem nicht verliehn;", "tokens": ["Doch", "Eh\u00b7re", "v\u00f6l\u00b7lig", "rein", "ist", "sol\u00b7chem", "nicht", "ver\u00b7liehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "ADJD", "VAFIN", "PIAT", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ihn sein Anhang lobt, schelten die Gegner ihn.", "tokens": ["Weil", "ihn", "sein", "An\u00b7hang", "lobt", ",", "schel\u00b7ten", "die", "Geg\u00b7ner", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch der, nach welchem Schul' und Sekte sich nicht nennt,", "tokens": ["Doch", "der", ",", "nach", "wel\u00b7chem", "Schul'", "und", "Sek\u00b7te", "sich", "nicht", "nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "APPR", "PWAT", "NN", "KON", "VVFIN", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mag hoffen da\u00df zuletzt ihn jede anerkennt.", "tokens": ["Mag", "hof\u00b7fen", "da\u00df", "zu\u00b7letzt", "ihn", "je\u00b7de", "an\u00b7er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KOUS", "ADV", "PPER", "PIAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}