{"dta.poem.16824": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "183.  In der mitten siht man alles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Setz dich in Mittelpunet/ so sihstu alls zugleich/", "tokens": ["Setz", "dich", "in", "Mit\u00b7tel\u00b7pu\u00b7net", "/", "so", "sih\u00b7stu", "alls", "zu\u00b7gleich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NE", "$(", "ADV", "ADJD", "KOKOM", "ADV", "$("], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Was jetz und dann geschicht/ hier und im Him\u0304elreich.", "tokens": ["Was", "jetz", "und", "dann", "ge\u00b7schicht", "/", "hier", "und", "im", "Him\u0304el\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KON", "ADV", "VVPP", "$(", "ADV", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}