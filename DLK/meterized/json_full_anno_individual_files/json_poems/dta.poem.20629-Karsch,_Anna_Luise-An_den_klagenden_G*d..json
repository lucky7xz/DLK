{"dta.poem.20629": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den klagenden G*d.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sprich, welcher Gott soll Dich besch\u00fctzen,                 ", "tokens": ["Sprich", ",", "wel\u00b7cher", "Gott", "soll", "Dich", "be\u00b7sch\u00fct\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob Ph\u00f6bus, oder Jupiter?", "tokens": ["Ob", "Ph\u00f6\u00b7bus", ",", "o\u00b7der", "Ju\u00b7pi\u00b7ter", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "KON", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Du schw\u00f6rst, Dein Herz wird keine mehr besitzen,", "tokens": ["Du", "schw\u00f6rst", ",", "Dein", "Herz", "wird", "kei\u00b7ne", "mehr", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "PIAT", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn sie sch\u00f6n wie Venus w\u00e4r.", "tokens": ["Und", "wenn", "sie", "sch\u00f6n", "wie", "Ve\u00b7nus", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "KOKOM", "NE", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du f\u00fcrchtest nicht den Bogentr\u00e4ger,", "tokens": ["Du", "f\u00fcrch\u00b7test", "nicht", "den", "Bo\u00b7gen\u00b7tr\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem doch kein Weiser widerstand,", "tokens": ["Dem", "doch", "kein", "Wei\u00b7ser", "wi\u00b7der\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der unterm L\u00f6wenfell den Hyderschl\u00e4ger,", "tokens": ["Der", "un\u00b7term", "L\u00f6\u00b7wen\u00b7fell", "den", "Hy\u00b7der\u00b7schl\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Sohn Alkmenens, \u00fcberwand?", "tokens": ["Den", "Sohn", "A\u00b7lkme\u00b7nens", ",", "\u00fc\u00b7ber\u00b7wand", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verbirg Dich unter Leichenh\u00fcgeln,", "tokens": ["Ver\u00b7birg", "Dich", "un\u00b7ter", "Lei\u00b7chen\u00b7h\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wirf Dich auf den Marmorstein,", "tokens": ["Und", "wirf", "Dich", "auf", "den", "Mar\u00b7mor\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den unsichtbar zween Genius versiegeln", "tokens": ["Den", "un\u00b7sicht\u00b7bar", "zween", "Ge\u00b7nius", "ver\u00b7sie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Elys\u00e4er Blumen streun;", "tokens": ["Und", "E\u00b7ly\u00b7s\u00e4\u00b7er", "Blu\u00b7men", "streun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch schleichet sich in Deinem Herzen", "tokens": ["Doch", "schlei\u00b7chet", "sich", "in", "Dei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Empfindung ein, die Du verschwurst,", "tokens": ["Emp\u00b7fin\u00b7dung", "ein", ",", "die", "Du", "ver\u00b7schwurst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Du, bet\u00e4ubt von Tyrannei der Schmerzen,", "tokens": ["Als", "Du", ",", "be\u00b7t\u00e4ubt", "von", "Ty\u00b7ran\u00b7nei", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unbillig mit Dir selbst verfuhrst.", "tokens": ["Un\u00b7bil\u00b7lig", "mit", "Dir", "selbst", "ver\u00b7fuhrst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Du, von des Greises Alter ferne,", "tokens": ["Du", ",", "von", "des", "Grei\u00b7ses", "Al\u00b7ter", "fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Folgst Adelgundens Schatten nicht,", "tokens": ["Folgst", "A\u00b7del\u00b7gun\u00b7dens", "Schat\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich reizen noch zween Augensterne,", "tokens": ["Dich", "rei\u00b7zen", "noch", "zween", "Au\u00b7gens\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich lockt ein bl\u00fchend Angesicht.", "tokens": ["Dich", "lockt", "ein", "bl\u00fc\u00b7hend", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dir l\u00e4chelt des Verstandes Morgen", "tokens": ["Dir", "l\u00e4\u00b7chelt", "des", "Ver\u00b7stan\u00b7des", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus faltenloser Stirne zu,", "tokens": ["Aus", "fal\u00b7ten\u00b7lo\u00b7ser", "Stir\u00b7ne", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In jedem Blick ist Amors Pfeil verborgen,", "tokens": ["In", "je\u00b7dem", "Blick", "ist", "A\u00b7mors", "Pfeil", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jeden Blick empfindest Du.", "tokens": ["Und", "je\u00b7den", "Blick", "emp\u00b7fin\u00b7dest", "Du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Erzittre G*d! ich wei\u00dfage", "tokens": ["Er\u00b7zitt\u00b7re", "G", "*", "d", "!", "ich", "wei\u00b7\u00dfa\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["XY", "XY", "XY", "XY", "$.", "PPER", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mehr als Apollo\u2019s Pythia,", "tokens": ["Mehr", "als", "A\u00b7pol\u00b7lo's", "Py\u00b7thia", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vor des Gottes Tempel ohne Frage", "tokens": ["Die", "vor", "des", "Got\u00b7tes", "Tem\u00b7pel", "oh\u00b7ne", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Weltbezwingers Namen sah,", "tokens": ["Des", "Welt\u00b7be\u00b7zwin\u00b7gers", "Na\u00b7men", "sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf jugendlicher Stirn geschrieben,", "tokens": ["Auf", "ju\u00b7gend\u00b7li\u00b7cher", "Stirn", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und rief: \u201eWer kann dir widerstehn?\u201e", "tokens": ["Und", "rief", ":", "\u201e", "Wer", "kann", "dir", "wi\u00b7der\u00b7stehn", "?", "\u201e"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PWS", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch die Th\u00fcr, die sonst versperrt geblieben,", "tokens": ["Und", "durch", "die", "Th\u00fcr", ",", "die", "sonst", "ver\u00b7sperrt", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihn zum Altar lie\u00df opfern gehn.", "tokens": ["Ihn", "zum", "Al\u00b7tar", "lie\u00df", "op\u00b7fern", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}