, "da", "er", "hert\u00b7zog", "ist", "im", "ho\u00b7hen", "en\u00b7gel\u00b7or\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "PPER", "VVFIN", "VAFIN", "APPRART", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du auff der erden bist zu seinem bilde worden.", "tokens": ["Du", "auff", "der", "er\u00b7den", "bist", "zu", "sei\u00b7nem", "bil\u00b7de", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Drum auff/ und r\u00fcste dich/ zeuch schmertz und thr\u00e4nen ein!", "tokens": ["Drum", "auff", "/", "und", "r\u00fcs\u00b7te", "dich", "/", "zeuch", "schmertz", "und", "thr\u00e4\u00b7nen", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "$(", "KON", "VVFIN", "PPER", "$(", "VVIMP", "ADJD", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fiedre deinen geist/ dem vater nachzufliegen.", "tokens": ["Und", "fied\u00b7re", "dei\u00b7nen", "geist", "/", "dem", "va\u00b7ter", "nach\u00b7zu\u00b7flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Philippus kan auch noch in seinem tode siegen/", "tokens": ["Phil\u00b7ip\u00b7pus", "kan", "auch", "noch", "in", "sei\u00b7nem", "to\u00b7de", "sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil er sich kleiner sieht als Alexandern seyn.", "tokens": ["Weil", "er", "sich", "klei\u00b7ner", "sieht", "als", "A\u00b7lex\u00b7an\u00b7dern", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "KOKOM", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Schweden neundter Carl verdoppelt ruhm und leben/", "tokens": ["Der", "Schwe\u00b7den", "neund\u00b7ter", "Carl", "ver\u00b7dop\u00b7pelt", "ruhm", "und", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NE", "VVFIN", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil muth und tugend auch aus seinem Adolph blitzt:", "tokens": ["Weil", "muth", "und", "tu\u00b7gend", "auch", "aus", "sei\u00b7nem", "A\u00b7dolph", "blitzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wird dein vater auch in aller augen schweben/", "tokens": ["So", "wird", "dein", "va\u00b7ter", "auch", "in", "al\u00b7ler", "au\u00b7gen", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil seiner strahlen gold auff deiner stirne sitzt;", "tokens": ["Weil", "sei\u00b7ner", "strah\u00b7len", "gold", "auff", "dei\u00b7ner", "stir\u00b7ne", "sitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die nach-welt aber wird aus deinen thaten lesen:", "tokens": ["Die", "nach\u00b7welt", "a\u00b7ber", "wird", "aus", "dei\u00b7nen", "tha\u00b7ten", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPR", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df nur ein unterscheid im namen sey gewesen.", "tokens": ["Da\u00df", "nur", "ein", "un\u00b7ter\u00b7scheid", "im", "na\u00b7men", "sey", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Da\u00df himmel und gestirn der seelen ursprung sey/", "tokens": ["Da\u00df", "him\u00b7mel", "und", "ge\u00b7stirn", "der", "see\u00b7len", "ur\u00b7sprung", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df durch vier sterne sie zur erden abw\u00e4rts fliessen/", "tokens": ["Da\u00df", "durch", "vier", "ster\u00b7ne", "sie", "zur", "er\u00b7den", "ab\u00b7w\u00e4rts", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "CARD", "VVFIN", "PPER", "APPRART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und so viel staffeln auch mu\u00df wieder auffw\u00e4rts schiessen/", "tokens": ["Und", "so", "viel", "staf\u00b7feln", "auch", "mu\u00df", "wie\u00b7der", "auf\u00b7fw\u00e4rts", "schies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "ADV", "VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schrieb ihr/ doch ohne grund/ schon l\u00e4ngst Egypten bey.", "tokens": ["Schrieb", "ihr", "/", "doch", "oh\u00b7ne", "grund", "/", "schon", "l\u00e4ngst", "E\u00b7gyp\u00b7ten", "bey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "ADV", "APPR", "NN", "$(", "ADV", "ADV", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Heut aber hat der bruch der kurtzen lebens-stunden/", "tokens": ["Heut", "a\u00b7ber", "hat", "der", "bruch", "der", "kurt\u00b7zen", "le\u00b7bens\u00b7stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der deines vaters leib in grau\u00df und asche legt/", "tokens": ["Der", "dei\u00b7nes", "va\u00b7ters", "leib", "in", "grau\u00df", "und", "asc\u00b7he", "legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "APPR", "NE", "KON", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zwar dir/ durchlauchster Printz/ mit thr\u00e4nen-saltz und wunden/", "tokens": ["Zwar", "dir", "/", "durch\u00b7lauchs\u00b7ter