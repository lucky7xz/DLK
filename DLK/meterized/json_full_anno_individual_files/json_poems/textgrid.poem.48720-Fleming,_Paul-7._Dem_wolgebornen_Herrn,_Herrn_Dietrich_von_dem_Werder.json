{"textgrid.poem.48720": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7. Dem wolgebornen Herrn, Herrn Dietrich von dem Werder", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sagts Jerusalem, es sagets Krieg und Sieg", "tokens": ["Es", "sagts", "Je\u00b7ru\u00b7sa\u00b7lem", ",", "es", "sa\u00b7gets", "Krieg", "und", "Sieg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "$,", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und hundert anders mehr, was, werter Held, dein Dichten", "tokens": ["und", "hun\u00b7dert", "an\u00b7ders", "mehr", ",", "was", ",", "wer\u00b7ter", "Held", ",", "dein", "Dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "CARD", "ADV", "ADV", "$,", "PRELS", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und dein Verrichten sei. Du giebest den Geschichten", "tokens": ["und", "dein", "Ver\u00b7rich\u00b7ten", "sei", ".", "Du", "gie\u00b7best", "den", "Ge\u00b7schich\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ihr Leben durch dein Tun, machst, da\u00df dein Sieg und Krieg", "tokens": ["ihr", "Le\u00b7ben", "durch", "dein", "Tun", ",", "machst", ",", "da\u00df", "dein", "Sieg", "und", "Krieg"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APP