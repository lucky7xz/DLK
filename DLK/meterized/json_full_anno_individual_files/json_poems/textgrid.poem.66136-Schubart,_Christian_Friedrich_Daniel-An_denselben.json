{"textgrid.poem.66136": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An denselben", "genre": "verse", "period": "N.A.", "pub_year": 1766, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich armer, durstiger Poet,", "tokens": ["Ich", "ar\u00b7mer", ",", "durs\u00b7ti\u00b7ger", "Po\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der fast aus Hunger betteln geht,", "tokens": ["Der", "fast", "aus", "Hun\u00b7ger", "bet\u00b7teln", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer schwach und niemals satt", "tokens": ["Und", "im\u00b7mer", "schwach", "und", "nie\u00b7mals", "satt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Chaos in dem Beutel hat.", "tokens": ["Ein", "Chaos", "in", "dem", "Beu\u00b7tel", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ein armer Mann, ein kranker Mann,", "tokens": ["Ein", "ar\u00b7mer", "Mann", ",", "ein", "kran\u00b7ker", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der hektisch von dem Busen keuchet", "tokens": ["Der", "hek\u00b7tisch", "von", "dem", "Bu\u00b7sen", "keuc\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wie ein Greis am Stabe schleichet", "tokens": ["Und", "wie", "ein", "Greis", "am", "Sta\u00b7be", "schlei\u00b7chet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und kaum die Rippe decken kann. \u2013", "tokens": ["Und", "kaum", "die", "Rip\u00b7pe", "de\u00b7cken", "kann", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein Knabe wie ein Pavian,", "tokens": ["Ein", "Kna\u00b7be", "wie", "ein", "Pa\u00b7vi\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der bietet mir nur alle Vierteljahre", "tokens": ["Der", "bie\u00b7tet", "mir", "nur", "al\u00b7le", "Vier\u00b7tel\u00b7jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Mit kothichtem Gesicht und mit zerzaustem Haare", "tokens": ["Mit", "ko\u00b7thich\u00b7tem", "Ge\u00b7sicht", "und", "mit", "zer\u00b7zaus\u00b7tem", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Zw\u00f6lf kupferrothe Kreuzer an.", "tokens": ["Zw\u00f6lf", "kup\u00b7fer\u00b7ro\u00b7the", "Kreu\u00b7zer", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ach Bruder! hier gebricht's", "tokens": ["Ach", "Bru\u00b7der", "!", "hier", "ge\u00b7bricht's"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ITJ", "NN", "$.", "ADV", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Am Geld, des Menschen bester Kraft,", "tokens": ["Am", "Geld", ",", "des", "Men\u00b7schen", "bes\u00b7ter", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die aus den Eseln Doktor schafft \u2013", "tokens": ["Die", "aus", "den", "E\u00b7seln", "Dok\u00b7tor", "schafft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Drum nehme nur vor deinen Rebensaft", "tokens": ["Drum", "neh\u00b7me", "nur", "vor", "dei\u00b7nen", "Re\u00b7ben\u00b7saft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Dies wenige! \u2013 \u2013 Ha, Schwager, \u2013 nichts.", "tokens": ["Dies", "we\u00b7ni\u00b7ge", "!", "\u2013", "\u2013", "Ha", ",", "Schwa\u00b7ger", ",", "\u2013", "nichts", "."], "token_info": ["word", "word", "punct", "punct", "punct", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["PDS", "PIS", "$.", "$(", "$(", "ITJ", "$,", "NN", "$,", "$(", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich armer, durstiger Poet,", "tokens": ["Ich", "ar\u00b7mer", ",", "durs\u00b7ti\u00b7ger", "Po\u00b7et", ","], "tok