{"dta.poem.21890": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XlVI,", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Das Frauen-zimmer ist ja sonst dem Geize", "tokens": ["Das", "Frau\u00b7en\u00b7zim\u00b7mer", "ist", "ja", "sonst", "dem", "Gei\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wie k\u00f6mmt es denn/ da\u00df Fillis nicht so viel ein-", "tokens": ["wie", "k\u00f6mmt", "es", "denn", "/", "da\u00df", "Fil\u00b7lis", "nicht", "so", "viel", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "NE", "PTKNEG", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ich kabe wenig. Dennoch ist ihr di\u00df mehr", "tokens": ["ich", "ka\u00b7be", "we\u00b7nig", ".", "Den\u00b7noch", "ist", "ihr", "di\u00df", "mehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$.", "ADV", "VAFIN", "PPER", "PDS", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "als wenn mit Amalt\u00e4en Horn selbst ihr Fi-", "tokens": ["als", "wenn", "mit", "A\u00b7mal\u00b7t\u00e4\u00b7en", "Horn", "selbst", "ihr", "Fi"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "APPR", "NE", "NE", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}