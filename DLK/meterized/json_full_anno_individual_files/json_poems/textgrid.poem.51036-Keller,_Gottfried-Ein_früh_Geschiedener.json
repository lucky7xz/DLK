{"textgrid.poem.51036": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ein fr\u00fch Geschiedener", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er war geschaffen, durch das All zu schweifen", "tokens": ["Er", "war", "ge\u00b7schaf\u00b7fen", ",", "durch", "das", "All", "zu", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit hellem Mute und gest\u00e4hlten Sinnen,", "tokens": ["Mit", "hel\u00b7lem", "Mu\u00b7te", "und", "ge\u00b7st\u00e4hl\u00b7ten", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu lauschen, wo des Lebens Quellen rinnen,", "tokens": ["Zu", "lau\u00b7schen", ",", "wo", "des", "Le\u00b7bens", "Quel\u00b7len", "rin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und forschend jeden Abgrund zu durchstreifen.", "tokens": ["Und", "for\u00b7schend", "je\u00b7den", "Ab\u00b7grund", "zu", "durch\u00b7strei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hinaus, hin\u00fcber, wo die Palmen reifen,", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7\u00fc\u00b7ber", ",", "wo", "die", "Pal\u00b7men", "rei\u00b7fen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zog es ihn m\u00e4chtig jeden Lenz von hinnen;", "tokens": ["Zog", "es", "ihn", "m\u00e4ch\u00b7tig", "je\u00b7den", "Lenz", "von", "hin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "PIAT", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von des Planeten h\u00f6chsten Gletscherzinnen", "tokens": ["Von", "des", "Pla\u00b7ne\u00b7ten", "h\u00f6chs\u00b7ten", "Glet\u00b7scher\u00b7zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Gel\u00fcstet's ihn, den \u00c4ther zu ergreifen.", "tokens": ["Ge\u00b7l\u00fcs\u00b7tet's", "ihn", ",", "den", "\u00c4\u00b7ther", "zu", "er\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er blieb gefesselt an das tiefe Moor:", "tokens": ["Er", "blieb", "ge\u00b7fes\u00b7selt", "an", "das", "tie\u00b7fe", "Moor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Theologie, die Notdurft zu erwerben,", "tokens": ["Theo\u00b7lo\u00b7gie", ",", "die", "Not\u00b7durft", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Im Nacken hart der Armut scharfe Klauen.", "tokens": ["Im", "Na\u00b7cken", "hart", "der", "Ar\u00b7mut", "schar\u00b7fe", "Klau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da \u00f6ffnet ihm der Tod das Sonnentor,", "tokens": ["Da", "\u00f6ff\u00b7net", "ihm", "der", "Tod", "das", "Son\u00b7nen\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der J\u00fcngling s\u00e4umte nicht, das Licht zu schauen", "tokens": ["Der", "J\u00fcng\u00b7ling", "s\u00e4um\u00b7te", "nicht", ",", "das", "Licht", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und jungfr\u00e4ulichen Geistes hier zu sterben.", "tokens": ["Und", "jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Geis\u00b7tes", "hier", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Er war geschaffen, durch das All zu schweifen", "tokens": ["Er", "war", "ge\u00b7schaf\u00b7fen", ",", "durch", "das", "All", "zu", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"tex