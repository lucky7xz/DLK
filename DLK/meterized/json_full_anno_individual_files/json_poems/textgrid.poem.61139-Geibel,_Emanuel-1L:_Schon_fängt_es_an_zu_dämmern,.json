{"textgrid.poem.61139": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon f\u00e4ngt es an zu d\u00e4mmern,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon f\u00e4ngt es an zu d\u00e4mmern,", "tokens": ["Schon", "f\u00e4ngt", "es", "an", "zu", "d\u00e4m\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Mond als Hirt erwacht", "tokens": ["Der", "Mond", "als", "Hirt", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOUS", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und singt den Wolkenl\u00e4mmern", "tokens": ["Und", "singt", "den", "Wol\u00b7ken\u00b7l\u00e4m\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Lied zur guten Nacht;", "tokens": ["Ein", "Lied", "zur", "gu\u00b7ten", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wie er singt so leise,", "tokens": ["Und", "wie", "er", "singt", "so", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da dringt vom Sternenkreise", "tokens": ["Da", "dringt", "vom", "Ster\u00b7nen\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Schall ins Ohr mir sacht:", "tokens": ["Der", "Schall", "ins", "Ohr", "mir", "sacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Schlafet in Ruh'! schlafet in Ruh'!", "tokens": ["Schla\u00b7fet", "in", "Ruh'", "!", "schla\u00b7fet", "in", "Ruh'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$.", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Vor\u00fcber der Tag und sein Schall;", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "der", "Tag", "und", "sein", "Schall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Die Liebe Gottes deckt euch zu", "tokens": ["Die", "Lie\u00b7be", "Got\u00b7tes", "deckt", "euch", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "All\u00fcberall.", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Nun suchen in den Zweigen", "tokens": ["Nun", "su\u00b7chen", "in", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Nest die V\u00f6gelein,", "tokens": ["Ihr", "Nest", "die", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Halm' und Blumen neigen", "tokens": ["Die", "Halm'", "und", "Blu\u00b7men", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Haupt im Mondenschein,", "tokens": ["Das", "Haupt", "im", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und selbst des M\u00fchlbachs Wellen", "tokens": ["Und", "selbst", "des", "M\u00fchl\u00b7bachs", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lassen das wilde Schwellen", "tokens": ["Las\u00b7sen", "das", "wil\u00b7de", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und schlummern murmelnd ein.", "tokens": ["Und", "schlum\u00b7mern", "mur\u00b7melnd", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Schlafet in Ruh', schlafet in Ruh'!", "tokens": ["Schla\u00b7fet", "in", "Ruh'", ",", "schla\u00b7fet", "in", "Ruh'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Vor\u00fcber der Tag und sein Schall;", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "der", "Tag", "und", "sein", "Schall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Die Liebe Gottes deckt euch zu", "tokens": ["Die", "Lie\u00b7be", "Got\u00b7tes", "deckt", "euch", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "All\u00fcberall.", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Von T\u00fcr zu T\u00fcre wallet", "tokens": ["Von", "T\u00fcr", "zu", "T\u00fc\u00b7re", "wal\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Traum, ein lieber Gast,", "tokens": ["Der", "Traum", ",", "ein", "lie\u00b7ber", "Gast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Harfenspiel verhallet", "tokens": ["Das", "Har\u00b7fen\u00b7spiel", "ver\u00b7hal\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im schimmernden Palast,", "tokens": ["Im", "schim\u00b7mern\u00b7den", "Pa\u00b7last", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Nachen schl\u00e4ft der Ferge,", "tokens": ["Im", "Na\u00b7chen", "schl\u00e4ft", "der", "Fer\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Hirten auf dem Berge", "tokens": ["Die", "Hir\u00b7ten", "auf", "dem", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Halten ums Feuer Rast.", "tokens": ["Hal\u00b7ten", "ums", "Feu\u00b7er", "Rast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Schlafet in Ruh', schlafet in Ruh'!", "tokens": ["Schla\u00b7fet", "in", "Ruh'", ",", "schla\u00b7fet", "in", "Ruh'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Vor\u00fcber der Tag und sein Schall;", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "der", "Tag", "und", "sein", "Schall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Die Liebes Gottes deckt euch zu", "tokens": ["Die", "Lie\u00b7bes", "Got\u00b7tes", "deckt", "euch", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "All\u00fcberall.", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und wie nun alle Kerzen", "tokens": ["Und", "wie", "nun", "al\u00b7le", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verl\u00f6schen durch die Nacht,", "tokens": ["Ver\u00b7l\u00f6\u00b7schen", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schweigen auch die Schmerzen,", "tokens": ["Da", "schwei\u00b7gen", "auch", "die", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonn' und Tag gebracht;", "tokens": ["Die", "Sonn'", "und", "Tag", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lind s\u00e4useln die Zypressen,", "tokens": ["Lind", "s\u00e4u\u00b7seln", "die", "Zyp\u00b7res\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Ein seliges Vergessen", "tokens": ["Ein", "se\u00b7li\u00b7ges", "Ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Durchweht die L\u00fcfte sacht.", "tokens": ["Durch\u00b7weht", "die", "L\u00fcf\u00b7te", "sacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Schlafet in Ruh', schlafet in Ruh'!", "tokens": ["Schla\u00b7fet", "in", "Ruh'", ",", "schla\u00b7fet", "in", "Ruh'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Vor\u00fcber der Tag und sein Schall;", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "der", "Tag", "und", "sein", "Schall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Die Liebe Gottes deckt euch zu", "tokens": ["Die", "Lie\u00b7be", "Got\u00b7tes", "deckt", "euch", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "All\u00fcberall.", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und wo von hei\u00dfen Tr\u00e4nen", "tokens": ["Und", "wo", "von", "hei\u00b7\u00dfen", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein schmachtend Auge bl\u00fcht,", "tokens": ["Ein", "schmach\u00b7tend", "Au\u00b7ge", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wo in bangem Sehnen", "tokens": ["Und", "wo", "in", "ban\u00b7gem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein liebend Herz vergl\u00fcht,", "tokens": ["Ein", "lie\u00b7bend", "Herz", "ver\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Traum kommt leis und linde", "tokens": ["Der", "Traum", "kommt", "leis", "und", "lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und singt dem kranken Kinde",