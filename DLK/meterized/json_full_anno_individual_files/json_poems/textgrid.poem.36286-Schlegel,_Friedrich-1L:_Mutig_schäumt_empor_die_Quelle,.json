{"textgrid.poem.36286": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mutig sch\u00e4umt empor die Quelle,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutig sch\u00e4umt empor die Quelle,", "tokens": ["Mu\u00b7tig", "sch\u00e4umt", "em\u00b7por", "die", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprudelt silberhelle,", "tokens": ["Spru\u00b7delt", "sil\u00b7ber\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hingezogen", "tokens": ["Hin\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Schimmern durch das Gr\u00fcn die Wogen;", "tokens": ["Schim\u00b7mern", "durch", "das", "Gr\u00fcn", "die", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00e4chtig dehnt der Strom sich, rei\u00dfend schnelle;", "tokens": ["M\u00e4ch\u00b7tig", "dehnt", "der", "Strom", "sich", ",", "rei\u00b7\u00dfend", "schnel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "$,", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "K\u00fchner umgebogen,", "tokens": ["K\u00fch\u00b7ner", "um\u00b7ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df die Flut so reicher schwelle,", "tokens": ["Da\u00df", "die", "Flut", "so", "rei\u00b7cher", "schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis zum Meere hin die stark' entflogen,", "tokens": ["Bis", "zum", "Mee\u00b7re", "hin", "die", "stark'", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wo dann endlos rauscht die Welle,", "tokens": ["Wo", "dann", "end\u00b7los", "rauscht", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Brausend ew'ge Fluten durch einander wogen.", "tokens": ["Brau\u00b7send", "ew'\u00b7ge", "Flu\u00b7ten", "durch", "ein\u00b7an\u00b7der", "wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Also, die vom Himmel stammen,", "tokens": ["Al\u00b7so", ",", "die", "vom", "Him\u00b7mel", "stam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die Liebesflammen", "tokens": ["Auch", "die", "Lie\u00b7bes\u00b7flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Im Gem\u00fcte,", "tokens": ["Im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Spielen erst um Fr\u00fchlingsbl\u00fcte;", "tokens": ["Spie\u00b7len", "erst", "um", "Fr\u00fch\u00b7lings\u00b7bl\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mutiger das B\u00f6se zu verdammen,", "tokens": ["Mu\u00b7ti\u00b7ger", "das", "B\u00f6\u00b7se", "zu", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie es wild auch w\u00fcte,", "tokens": ["Wie", "es", "wild", "auch", "w\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Schlagen Herzen dann zusammen", "tokens": ["Schla\u00b7gen", "Her\u00b7zen", "dann", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00fchn, wer frei dem Vaterland ergl\u00fchte;", "tokens": ["K\u00fchn", ",", "wer", "frei", "dem", "Va\u00b7ter\u00b7land", "er\u00b7gl\u00fch\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Bis noch reiner sie entflammen,", "tokens": ["Bis", "noch", "rei\u00b7ner", "sie", "ent\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mitverschlungen in das Meer der ew'gen G\u00fcte.", "tokens": ["Mit\u00b7ver\u00b7schlun\u00b7gen", "in", "das", "Meer", "der", "ew'\u00b7gen", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Diese Lieder und Ges\u00e4nge,", "tokens": ["Die\u00b7se", "Lie\u00b7der", "und", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieber Jugend Kl\u00e4nge,", "tokens": ["Lie\u00b7ber", "Ju\u00b7gend", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Erst nur Spiele", "tokens": ["Erst", "nur", "Spie\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Streben bald zu lichterm Ziele;", "tokens": ["Stre\u00b7ben", "bald", "zu", "lich\u00b7term", "Zie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fchn empor sich windend aus der Enge", "tokens": ["K\u00fchn", "em\u00b7por", "sich", "win\u00b7dend", "aus", "der", "En\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "PRF", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Spielender Gef\u00fchle;", "tokens": ["Spie\u00b7len\u00b7der", "Ge\u00b7f\u00fch\u00b7le", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Abw\u00e4rts von der bl\u00f6den Menge,", "tokens": ["Ab\u00b7w\u00e4rts", "von", "der", "bl\u00f6\u00b7den", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Neu entz\u00fcndend mut'ger Herzen viele,", "tokens": ["Neu", "ent\u00b7z\u00fcn\u00b7dend", "mut'\u00b7ger", "Her\u00b7zen", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ADJA", "NN", "PIS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Rei\u00dft vom irdischen Gedr\u00e4nge", "tokens": ["Rei\u00dft", "vom", "ir\u00b7di\u00b7schen", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aufw\u00e4rts der Gesang den Geist zum Flammenziele.", "tokens": ["Auf\u00b7w\u00e4rts", "der", "Ge\u00b7sang", "den", "Geist", "zum", "Flam\u00b7men\u00b7zie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Die in Liebe du entz\u00fcndet,", "tokens": ["Die", "in", "Lie\u00b7be", "du", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig mir verb\u00fcndet,", "tokens": ["E\u00b7wig", "mir", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Lust und Klagen", "tokens": ["Lust", "und", "Kla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Teilend, alles wolltest wagen;", "tokens": ["Tei\u00b7lend", ",", "al\u00b7les", "woll\u00b7test", "wa\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PIS", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe ist es, was das Lied verk\u00fcndet,", "tokens": ["Lie\u00b7be", "ist", "es", ",", "was", "das", "Lied", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie in Fr\u00fchlingstagen", "tokens": ["Wie", "in", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sch\u00f6ne Freude sich entz\u00fcndet.", "tokens": ["Sch\u00f6\u00b7ne", "Freu\u00b7de", "sich", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6her noch beginnt der Mut zu schlagen,", "tokens": ["H\u00f6\u00b7her", "noch", "be\u00b7ginnt", "der", "Mut", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wenn die Hoffnung, neu begr\u00fcndet,", "tokens": ["Wenn", "die", "Hoff\u00b7nung", ",", "neu", "be\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "All' uns will vereint zum Meer der Liebe tragen.", "tokens": ["All'", "uns", "will", "ver\u00b7eint", "zum", "Meer", "der", "Lie\u00b7be", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VMFIN", "VVPP", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Als die Zeit in Ha\u00df entbrannte,", "tokens": ["Als", "die", "Zeit", "in", "Ha\u00df", "ent\u00b7brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keinen Frieden kannte,", "tokens": ["Kei\u00b7nen", "Frie\u00b7den", "kann\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fern vertrieben,", "tokens": ["Fern", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "War es nicht dein treues Lieben,", "tokens": ["War", "es", "nicht", "dein", "treu\u00b7es", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo der Mut zur Hoffnung sich ermannte?", "tokens": ["Wo", "der", "Mut", "zur", "Hoff\u00b7nung", "sich", "er\u00b7mann\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sind umhergetrieben,", "tokens": ["Sind", "um\u00b7her\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ehrend was die Welt verkannte,", "tokens": ["Eh\u00b7rend", "was", "die", "Welt", "ver\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wir nicht folgend unsers Herzens Trieben,", "tokens": ["Wir", "nicht", "fol\u00b7gend", "un\u00b7sers", "Her\u00b7zens", "Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Welchen Wahn man immer nannte,", "tokens": ["Wel\u00b7chen", "Wahn", "man", "im\u00b7mer", "nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PIS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Treu dem rechten Vaterlande stets geblieben?", "tokens": ["Treu", "dem", "rech\u00b7ten", "Va\u00b7ter\u00b7lan\u00b7de", "stets", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Was von Lust und Schmerz bezwungen,", "tokens": ["Was", "von", "Lust", "und", "Schmerz", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mutig ich gesungen,", "tokens": ["Mu\u00b7tig", "ich", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Was dem vollen", "tokens": ["Was", "dem", "vol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Herzen sch\u00f6pferisch entquollen;", "tokens": ["Her\u00b7zen", "sch\u00f6p\u00b7fe\u00b7risch", "ent\u00b7quol\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was sich spielend erst durchs Tal geschlungen,", "tokens": ["Was", "sich", "spie\u00b7lend", "erst", "durchs", "Tal", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADJD", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dann zum Strom erschwollen", "tokens": ["Dann", "zum", "Strom", "er\u00b7schwol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Um das Vaterland geschwungen;", "tokens": ["Um", "das", "Va\u00b7ter\u00b7land", "ge\u00b7schwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Soll den Dank der Liebe freudig zollen,", "tokens": ["Soll", "den", "Dank", "der", "Lie\u00b7be", "freu\u00b7dig", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Weil durch Liebe nur gelungen,", "tokens": ["Weil", "durch", "Lie\u00b7be", "nur", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word