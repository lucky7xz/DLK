al", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADJD", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dann zum Strom erschwollen", "tokens": ["Dann", "zum", "Strom", "er\u00b7schwol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Um das Vaterland geschwungen;", "tokens": ["Um", "das", "Va\u00b7ter\u00b7land", "ge\u00b7schwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Soll den Dank der Liebe freudig zollen,", "tokens": ["Soll", "den", "Dank", "der", "Lie\u00b7be", "freu\u00b7dig", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Weil durch Liebe nur gelungen,", "tokens": ["Weil", "durch", "Lie\u00b7be", "nur", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Was auf k\u00fchner Fahrt zum Ziel uns f\u00fchren sollen.", "tokens": ["Was", "auf", "k\u00fch\u00b7ner", "Fahrt", "zum", "Ziel", "uns", "f\u00fch\u00b7ren", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "APPRART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.14": {"line.1": {"text": "Fahrt, die wir vereint bestanden,", "tokens": ["Fahrt", ",", "die", "wir", "ver\u00b7eint"