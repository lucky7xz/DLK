 ["NN", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie in Fr\u00fchlingstagen", "tokens": ["Wie", "in", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sch\u00f6ne Freude sich entz\u00fcndet.", "tokens": ["Sch\u00f6\u00b7ne", "Freu\u00b7de", "sich", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6her noch beginnt der Mut zu schlagen,", "tokens": ["H\u00f6\u00b7her", "noch", "be\u00b7ginnt", "der", "Mut", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wenn die Hoffnung, neu begr\u00fcndet,", "tokens": ["Wenn", "die", "Hoff\u00b7nung", ",", "neu", "be\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "All' uns will vereint zum Meer der Liebe tragen.", "tokens": ["All'", "uns", "will", "ver\u00b7eint", "zum", "Meer", "der", "Lie\u00b7be", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "VMFIN", "VVPP", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.12": {"line.1": {"text": "Als die Zeit in Ha\u00df entbrannte,", "tokens": ["Als", "die", "Zeit", "in", "Ha\u00df", "ent\u00b7brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keinen Frieden kannte,", "tokens": ["Kei\u00b7nen", "Frie\u00b7den", "kann\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Fern vertrieben,", "tokens": ["Fern", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "War es nicht dein treues Lieben,", "tokens": ["War", "es", "nicht", "dein", "treu\u00b7es", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo der Mut zur Hoffnung sich ermannte?", "tokens": ["Wo", "der", "Mut", "zur", "Hoff\u00b7nung", "sich", "er\u00b7mann\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sind umhergetrieben,", "tokens": ["Sind", "um\u00b7her\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ehrend was die Welt verkannte,", "tokens": ["Eh\u00b7rend", "was", "die", "Welt", "ver\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wir nicht folgend unsers Herzens Trieben,", "tokens": ["Wir", "nicht", "fol\u00b7gend", "un\u00b7sers", "Her\u00b7zens", "Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "w