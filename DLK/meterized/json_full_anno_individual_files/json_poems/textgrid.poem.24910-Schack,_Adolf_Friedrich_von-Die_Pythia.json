{"textgrid.poem.24910": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Pythia", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von des Mummius loher Fackel war zu Staub Korinth geworden,", "tokens": ["Von", "des", "Mum\u00b7mi\u00b7us", "lo\u00b7her", "Fa\u00b7ckel", "war", "zu", "Staub", "Ko\u00b7rinth", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VAFIN", "APPR", "NN", "NE", "VAPP", "$,"], "meter": "+-+--+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Und der Freiheit altes Bollwerk lag gest\u00fcrzt durch R\u00f6merhorden;", "tokens": ["Und", "der", "Frei\u00b7heit", "al\u00b7tes", "Boll\u00b7werk", "lag", "ge\u00b7st\u00fcrzt", "durch", "R\u00f6\u00b7mer\u00b7hor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aber noch bek\u00e4mpften Hellas' St\u00e4mme sich in ew'gem Hadern,", "tokens": ["A\u00b7ber", "noch", "be\u00b7k\u00e4mpf\u00b7ten", "Hel\u00b7las'", "St\u00e4m\u00b7me", "sich", "in", "ew'\u00b7gem", "Ha\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "NN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Tr\u00e4nkten noch den Mutterboden mit dem Blut der eignen Adern.", "tokens": ["Tr\u00e4nk\u00b7ten", "noch", "den", "Mut\u00b7ter\u00b7bo\u00b7den", "mit", "dem", "Blut", "der", "eig\u00b7nen", "A\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Und im Tempel Delphis standen die Gesandten der Entzweiten,", "tokens": ["Und", "im", "Tem\u00b7pel", "Del\u00b7phis", "stan\u00b7den", "die", "Ge\u00b7sand\u00b7ten", "der", "Ent\u00b7zwei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NE", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Um Apollos Spruch zu h\u00f6ren, eh sie ihren Kampf erneuten;", "tokens": ["Um", "A\u00b7pol\u00b7los", "Spruch", "zu", "h\u00f6\u00b7ren", ",", "eh", "sie", "ih\u00b7ren", "Kampf", "er\u00b7neu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "NN", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Auf dem Dreifu\u00df ruht die Pythia, vor dem Gott dahingesunken,", "tokens": ["Auf", "dem", "Drei\u00b7fu\u00df", "ruht", "die", "Py\u00b7thia", ",", "vor", "dem", "Gott", "da\u00b7hin\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und ihr Haupt erhebt sich m\u00e4hlich, von dem Geist der Zukunft trunken.", "tokens": ["Und", "ihr", "Haupt", "er\u00b7hebt", "sich", "m\u00e4h\u00b7lich", ",", "von", "dem", "Geist", "der", "Zu\u00b7kunft", "trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "ADJD", "$,", "APPR", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Da ert\u00f6nen Donnerschl\u00e4ge, da\u00df die Tempelmauern zittern,", "tokens": ["Da", "er\u00b7t\u00f6\u00b7nen", "Don\u00b7ner\u00b7schl\u00e4\u00b7ge", ",", "da\u00df", "die", "Tem\u00b7pel\u00b7mau\u00b7ern", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Lodernd zuckt ein Blitzstrahl nieder, schl\u00e4gt das S\u00e4ulendach zu Splittern,", "tokens": ["Lo\u00b7dernd", "zuckt", "ein", "Blitz\u00b7strahl", "nie\u00b7der", ",", "schl\u00e4gt", "das", "S\u00e4u\u00b7len\u00b7dach", "zu", "Split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und die Seherin, verzweifelnd, st\u00fcrzt vom Sitze: \u00bbWeh, Hellenen!", "tokens": ["Und", "die", "Se\u00b7he\u00b7rin", ",", "ver\u00b7zwei\u00b7felnd", ",", "st\u00fcrzt", "vom", "Sit\u00b7ze", ":", "\u00bb", "Weh", ",", "Hel\u00b7le\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "VVPP", "$,", "VVFIN", "APPRART", "NN", "$.", "$(", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Unter euch wie einen Abgrund seh' ich die Vernichtung g\u00e4hnen!", "tokens": ["Un\u00b7ter", "euch", "wie", "ei\u00b7nen", "Ab\u00b7grund", "seh'", "ich", "die", "Ver\u00b7nich\u00b7tung", "g\u00e4h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOKOM", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Alle, die ihr euch befehdet im jahrhundertlangen Kampfe,", "tokens": ["Al\u00b7le", ",", "die", "ihr", "euch", "be\u00b7feh\u00b7det", "im", "jahr\u00b7hun\u00b7dert\u00b7lan\u00b7gen", "Kamp\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "H\u00f6r' ich untergehend \u00e4chzen in demselben Todeskrampfe;", "tokens": ["H\u00f6r'", "ich", "un\u00b7ter\u00b7ge\u00b7hend", "\u00e4ch\u00b7zen", "in", "dem\u00b7sel\u00b7ben", "To\u00b7des\u00b7kramp\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aus der Erde selbst erschallen dumpf ans Ohr mir Klaget\u00f6ne,", "tokens": ["Aus", "der", "Er\u00b7de", "selbst", "er\u00b7schal\u00b7len", "dumpf", "ans", "Ohr", "mir", "Kla\u00b7ge\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "ADJD", "APPRART", "NN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Gleich dem Jammerruf der Muttter an den Leichen ihrer S\u00f6hne.", "tokens": ["Gleich", "dem", "Jam\u00b7mer\u00b7ruf", "der", "Mutt\u00b7ter", "an", "den", "Lei\u00b7chen", "ih\u00b7rer", "S\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Zahllos wie die Wogen, wenn den Isthmus st\u00fcrmen beide Meere,", "tokens": ["Zahl\u00b7los", "wie", "die", "Wo\u00b7gen", ",", "wenn", "den", "Isth\u00b7mus", "st\u00fcr\u00b7men", "bei\u00b7de", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "W\u00e4lzen durch die Bergesschluchten sich heran die Scythenheere;", "tokens": ["W\u00e4l\u00b7zen", "durch", "die", "Ber\u00b7ges\u00b7schluch\u00b7ten", "sich", "he\u00b7ran", "die", "Scyt\u00b7hen\u00b7hee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PRF", "APZR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und ihr Atem ist Zerst\u00f6rung; auf dem Lauf, dem sturmgetragnen,", "tokens": ["Und", "ihr", "A\u00b7tem", "ist", "Zer\u00b7st\u00f6\u00b7rung", ";", "auf", "dem", "Lauf", ",", "dem", "sturm\u00b7ge\u00b7trag\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "$.", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Leuchten ihnen loh'nde St\u00e4dte \u00fcber Haufen der Erschlagnen.", "tokens": ["Leuch\u00b7ten", "ih\u00b7nen", "loh'n\u00b7de", "St\u00e4d\u00b7te", "\u00fc\u00b7ber", "Hau\u00b7fen", "der", "Er\u00b7schlag\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Raubgev\u00f6gel, leichenwitternd, folgt dem Zuge der Barbaren;", "tokens": ["Raub\u00b7ge\u00b7v\u00f6\u00b7gel", ",", "lei\u00b7chen\u00b7wit\u00b7ternd", ",", "folgt", "dem", "Zu\u00b7ge", "der", "Bar\u00b7ba\u00b7ren", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "An der Rosse Schweife binden sie die Jungfrau mit den Haaren;", "tokens": ["An", "der", "Ros\u00b7se", "Schwei\u00b7fe", "bin\u00b7den", "sie", "die", "Jung\u00b7frau", "mit", "den", "Haa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "An das Haus Kronions selber legen sie verruchte H\u00e4nde,", "tokens": ["An", "das", "Haus", "Kro\u00b7nions", "sel\u00b7ber", "le\u00b7gen", "sie", "ver\u00b7ruch\u00b7te", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Schleudern auf das Haus des Gottes lachend ihre Feuerbr\u00e4nde.", "tokens": ["Schleu\u00b7dern", "auf", "das", "Haus", "des", "Got\u00b7tes", "la\u00b7chend", "ih\u00b7re", "Feu\u00b7er\u00b7br\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Haltet ein, Vermess'ne! Seht ihr nicht den Donnrer auf den Zinnen", "tokens": ["Hal\u00b7tet", "ein", ",", "Ver\u00b7mess'\u00b7ne", "!", "Seht", "ihr", "nicht", "den", "Donn\u00b7rer", "auf", "den", "Zin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Mit dem Blitzstrahl in der Rechten, dem die Frevler nicht entrinnen?", "tokens": ["Mit", "dem", "Blitz\u00b7strahl", "in", "der", "Rech\u00b7ten", ",", "dem", "die", "Frev\u00b7ler", "nicht", "ent\u00b7rin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nein, umsonst! Die G\u00f6tter starben, und der Tempel sinkt zu Tr\u00fcmmern;", "tokens": ["Nein", ",", "um\u00b7sonst", "!", "Die", "G\u00f6t\u00b7ter", "star\u00b7ben", ",", "und", "der", "Tem\u00b7pel", "sinkt", "zu", "Tr\u00fcm\u00b7mern", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "$.", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nur zermalmte Marmorbilder h\u00f6r' ich aus dem Schutte wimmern.", "tokens": ["Nur", "zer\u00b7malm\u00b7te", "Mar\u00b7mor\u00b7bil\u00b7der", "h\u00f6r'", "ich", "aus", "dem", "Schut\u00b7te", "wim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Nicht ein Stein bleibt auf dem Steine; hingeschmettert von den Keulen", "tokens": ["Nicht", "ein", "Stein", "bleibt", "auf", "dem", "Stei\u00b7ne", ";", "hin\u00b7ge\u00b7schmet\u00b7tert", "von", "den", "Keu\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "St\u00fcrzen auf die letzten Griechen ihrer letzten Tempel S\u00e4ulen;", "tokens": ["St\u00fcr\u00b7zen", "auf", "die", "letz\u00b7ten", "Grie\u00b7chen", "ih\u00b7rer", "letz\u00b7ten", "Tem\u00b7pel", "S\u00e4u\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und aus Rennbahn und Theater mit verl\u00f6schendem Geflacker", "tokens": ["Und", "aus", "Renn\u00b7bahn", "und", "The\u00b7a\u00b7ter", "mit", "ver\u00b7l\u00f6\u00b7schen\u00b7dem", "Ge\u00b7fla\u00b7cker"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wirft die Flamme blassen Schimmer auf den gro\u00dfen Totenacker.", "tokens": ["Wirft", "die", "Flam\u00b7me", "blas\u00b7sen", "Schim\u00b7mer", "auf", "den", "gro\u00b7\u00dfen", "To\u00b7te\u00b7na\u00b7cker", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Stolzes Volk, einst Weltgebieter! Dich mit allen deinen St\u00e4mmen", "tokens": ["Stol\u00b7zes", "Volk", ",", "einst", "Welt\u00b7ge\u00b7bie\u00b7ter", "!", "Dich", "mit", "al\u00b7len", "dei\u00b7nen", "St\u00e4m\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "NN", "$.", "PPER", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wird die Sturmflut der Vernichtung weg vom Erdenboden schwemmen;", "tokens": ["Wird", "die", "Sturm\u00b7flut", "der", "Ver\u00b7nich\u00b7tung", "weg", "vom", "Er\u00b7den\u00b7bo\u00b7den", "schwem\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Selbst dein Name wird verschwinden, nur auf Gr\u00e4bern wird man lesen", "tokens": ["Selbst", "dein", "Na\u00b7me", "wird", "ver\u00b7schwin\u00b7den", ",", "nur", "auf", "Gr\u00e4\u00b7bern", "wird", "man", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "VVINF", "$,", "ADV", "APPR", "NN", "VAFIN", "PIS", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und in deiner Geister Werken, da\u00df ein Hellas je gew