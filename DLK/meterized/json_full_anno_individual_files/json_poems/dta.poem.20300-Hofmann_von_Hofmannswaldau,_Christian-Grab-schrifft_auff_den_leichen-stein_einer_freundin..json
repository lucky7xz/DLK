{"dta.poem.20300": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Grab-schrifft auff den leichen-stein  \n einer freundin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein stern der tugenden/ die sonne dieser stadt/", "tokens": ["Ein", "stern", "der", "tu\u00b7gen\u00b7den", "/", "die", "son\u00b7ne", "die\u00b7ser", "stadt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein engel/ wenn man will den namen recht erwegen/", "tokens": ["Ein", "en\u00b7gel", "/", "wenn", "man", "will", "den", "na\u00b7men", "recht", "er\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PIS", "VMFIN", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein licht/ so in der welt mit lust geschienen hat/", "tokens": ["Ein", "licht", "/", "so", "in", "der", "welt", "mit", "lust", "ge\u00b7schie\u00b7nen", "hat", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "ART", "NN", "APPR", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mu\u00df sich dem tode nun zu seinen f\u00fcssen legen.", "tokens": ["Mu\u00df", "sich", "dem", "to\u00b7de", "nun", "zu", "sei\u00b7nen", "f\u00fcs\u00b7sen", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein leser/ li\u00df doch recht/ was ich dir kund gethan;", "tokens": ["Mein", "le\u00b7ser", "/", "li\u00df", "doch", "recht", "/", "was", "ich", "dir", "kund", "ge\u00b7than", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "ADV", "ADJD", "$(", "PWS", "PPER", "PPER", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich habe viel gesagt/ noch aber mehr verschwiegen;", "tokens": ["Ich", "ha\u00b7be", "viel", "ge\u00b7sagt", "/", "noch", "a\u00b7ber", "mehr", "ver\u00b7schwie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$(", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo hier stern/ sonne/ licht und engel wohnen kan/", "tokens": ["Wo", "hier", "stern", "/", "son\u00b7ne", "/", "licht", "und", "en\u00b7gel", "woh\u00b7nen", "kan", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVINF", "$(", "XY", "$(", "NN", "KON", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So mu\u00df der himmel ja in diesem grabe liegen.", "tokens": ["So", "mu\u00df", "der", "him\u00b7mel", "ja", "in", "die\u00b7sem", "gra\u00b7be", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}