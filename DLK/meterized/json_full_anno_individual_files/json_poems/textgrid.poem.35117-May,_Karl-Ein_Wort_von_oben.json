be!", "tokens": ["Da\u00df", "euch", "der", "Zwei\u00b7fel", "nicht", "aus", "ihm", "ver\u00b7trie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O, athmet diese Gottesliebe ein,", "tokens": ["O", ",", "ath\u00b7met", "die\u00b7se", "Got\u00b7tes\u00b7lie\u00b7be", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und gebt mit jedem Wort und Blick sie weiter,", "tokens": ["Und", "gebt", "mit", "je\u00b7dem", "Wort", "und", "Blick", "sie", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "KON", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So werdet ihr bald hoch gestiegen sein", "tokens": ["So", "wer\u00b7det", "ihr", "bald", "hoch", "ge\u00b7stie\u00b7gen", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auf \u2013 \u2013 ich wills sagen! \u2013 \u2013 auf der Himmelsleiter.", "tokens": ["Auf", "\u2013", "\u2013", "ich", "wills", "sa\u00b7gen", "!", "\u2013", "\u2013", "auf", "der", "Him\u00b7mels\u00b7lei\u00b7ter", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "$(", "PPER", "VMFIN", "VVINF", "$.", "$(", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich ging zum Himmel ein, doch bin i