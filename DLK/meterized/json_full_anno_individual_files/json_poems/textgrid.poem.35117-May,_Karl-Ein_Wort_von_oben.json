{"textgrid.poem.35117": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Ein Wort von oben", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging zum Himmel ein, doch bin ich euch nicht fern;", "tokens": ["Ich", "ging", "zum", "Him\u00b7mel", "ein", ",", "doch", "bin", "ich", "euch", "nicht", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihr ihn auch nicht seht, den Garten Eden.", "tokens": ["Wenn", "ihr", "ihn", "auch", "nicht", "seht", ",", "den", "Gar\u00b7ten", "E\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es reicht der Himmel ja von Stern zu Stern,", "tokens": ["Es", "reicht", "der", "Him\u00b7mel", "ja", "von", "Stern", "zu", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umfassend auch den irdischen Planeten.", "tokens": ["Um\u00b7fas\u00b7send", "auch", "den", "ir\u00b7di\u00b7schen", "Pla\u00b7ne\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Vor Gott sind alle, alle Welten gleich,", "tokens": ["Vor", "Gott", "sind", "al\u00b7le", ",", "al\u00b7le", "Wel\u00b7ten", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "$,", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die er f\u00fcr seine Selgen vorbereitet,", "tokens": ["Die", "er", "f\u00fcr", "sei\u00b7ne", "Sel\u00b7gen", "vor\u00b7be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und weil es ohne Ende ist, sein Reich,", "tokens": ["Und", "weil", "es", "oh\u00b7ne", "En\u00b7de", "ist", ",", "sein", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "VAFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So liegt es auch um euch rings ausgebreitet.", "tokens": ["So", "liegt", "es", "auch", "um", "euch", "rings", "aus\u00b7ge\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich ging zum Himmel ein, doch bin ich euch nicht fern;", "tokens": ["Ich", "ging", "zum", "Him\u00b7mel", "ein", ",", "doch", "bin", "ich", "euch", "nicht", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denkt ja nicht, da\u00df ich euch entzogen werde.", "tokens": ["Denkt", "ja", "nicht", ",", "da\u00df", "ich", "euch", "ent\u00b7zo\u00b7gen", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Himmel ist die Herrlichkeit des Herrn,", "tokens": ["Der", "Him\u00b7mel", "ist", "die", "Herr\u00b7lich\u00b7keit", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und diese leuchtet euch auch auf der Erde.", "tokens": ["Und", "die\u00b7se", "leuch\u00b7tet", "euch", "auch", "auf", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es sieht zwar euer Aug das Wunder nicht,", "tokens": ["Es", "sieht", "zwar", "eu\u00b7er", "Aug", "das", "Wun\u00b7der", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zu dem ich selig meine Blicke leite,", "tokens": ["Zu", "dem", "ich", "se\u00b7lig", "mei\u00b7ne", "Bli\u00b7cke", "lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch wenn mein Herz f\u00fcr euch zum Vater spricht,", "tokens": ["Doch", "wenn", "mein", "Herz", "f\u00fcr", "euch", "zum", "Va\u00b7ter",