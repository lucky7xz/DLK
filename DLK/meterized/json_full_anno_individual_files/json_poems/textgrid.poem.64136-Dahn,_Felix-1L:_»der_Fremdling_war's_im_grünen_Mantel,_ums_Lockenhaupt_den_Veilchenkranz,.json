{"textgrid.poem.64136": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbder Fremdling war's im gr\u00fcnen Mantel, ums Lockenhaupt den Veilchenkranz,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbder Fremdling war's im gr\u00fcnen Mantel, ums Lockenhaupt den Veilchenkranz,", "tokens": ["\u00bb", "der", "Fremd\u00b7ling", "wa\u00b7r's", "im", "gr\u00fc\u00b7nen", "Man\u00b7tel", ",", "ums", "Lo\u00b7cken\u00b7haupt", "den", "Veil\u00b7chen\u00b7kranz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Er hat bet\u00f6rt die K\u00f6nigstochter, die er gef\u00fchrt im Maientanz.", "tokens": ["Er", "hat", "be\u00b7t\u00f6rt", "die", "K\u00f6\u00b7nig\u00b7stoch\u00b7ter", ",", "die", "er", "ge\u00b7f\u00fchrt", "im", "Mai\u00b7en\u00b7tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Er kam, man wei\u00df es nicht, von wannen, er schied und niemand wei\u00df, wohin.", "tokens": ["Er", "kam", ",", "man", "wei\u00df", "es", "nicht", ",", "von", "wan\u00b7nen", ",", "er", "schied", "und", "nie\u00b7mand", "wei\u00df", ",", "wo\u00b7hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "ADV", "$,", "PPER", "VVFIN", "KON", "PIS", "VVFIN", "$,", "PWAV", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Du bist betrogen, sch\u00f6n Haralda, und Schmach und Tod ist dein Gewinn.\u00ab", "tokens": ["Du", "bist", "be\u00b7tro\u00b7gen", ",", "sch\u00f6n", "Ha\u00b7ral\u00b7da", ",", "und", "Schmach", "und", "Tod", "ist", "dein", "Ge\u00b7winn", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADJD", "NE", "$,", "KON", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "So klagt das Volk; doch K\u00f6nig Olaf, der finstre, klagt und drohet nicht.", "tokens": ["So", "klagt", "das", "Volk", ";", "doch", "K\u00f6\u00b7nig", "O\u00b7laf", ",", "der", "finst\u00b7re", ",", "klagt", "und", "dro\u00b7het", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "NN", "NE", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ein Grab l\u00e4\u00dft er im Walde graben, durch Eis und Schnee der Spaten bricht.", "tokens": ["Ein", "Grab", "l\u00e4\u00dft", "er", "im", "Wal\u00b7de", "gra\u00b7ben", ",", "durch", "Eis", "und", "Schnee", "der", "Spa\u00b7ten", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,", "APPR", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+---+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Im Fr\u00fchm\u00e4rz ist's: kahl stehn die B\u00e4ume, kein Vogelruf, Eis deckt den Quell,", "tokens": ["Im", "Fr\u00fch\u00b7m\u00e4rz", "ist's", ":", "kahl", "stehn", "die", "B\u00e4u\u00b7me", ",", "kein", "Vo\u00b7gel\u00b7ruf", ",", "Eis", "deckt", "den", "Quell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$.", "ADJD", "VVFIN", "ART", "NN", "$,", "PIAT", "NN", "$,", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Rings alles starr: nur hoch am Himmel zieht's hin wie Fr\u00fchlingswolken hell.", "tokens": ["Rings", "al\u00b7les", "starr", ":", "nur", "hoch", "am", "Him\u00b7mel", "zieht's", "hin", "wie", "Fr\u00fch\u00b7lings\u00b7wol\u00b7ken", "hell", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADJD", "$.", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "PTKVZ", "KOKOM", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Und schweigend f\u00fchrt vor allem Volke sein Kind er an den dunkeln Schlund:", "tokens": ["Und", "schwei\u00b7gend", "f\u00fchrt", "vor", "al\u00b7lem", "Vol\u00b7ke", "sein", "Kind", "er", "an", "den", "dun\u00b7keln", "Schlund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PIS", "NN", "PPOSAT", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "\u00bblebendig sei mit deiner Schande verschlungen von der Erde Grund,", "tokens": ["\u00bb", "le\u00b7ben\u00b7dig", "sei", "mit", "dei\u00b7ner", "Schan\u00b7de", "ver\u00b7schlun\u00b7gen", "von", "der", "Er\u00b7de", "Grund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Sagst du mir nicht des Frevlers Namen und wo ihn trifft mein Strafgericht.\u00ab", "tokens": ["Sagst", "du", "mir", "nicht", "des", "Frev\u00b7lers", "Na\u00b7men", "und", "wo", "ihn", "trifft", "mein", "Straf\u00b7ge\u00b7richt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "ART", "NN", "NN", "KON", "PWAV", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Doch sie schlug auf die sch\u00f6nen Augen und sprach in Ruh': \u00bbIch wei\u00df es nicht!", "tokens": ["Doch", "sie", "schlug", "auf", "die", "sch\u00f6\u00b7nen", "Au\u00b7gen", "und", "sprach", "in", "Ruh'", ":", "\u00bb", "Ich", "wei\u00df", "es", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "APPR", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "--+--+-+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Ich wei\u00df nur, da\u00df er ist mein Gatte und da\u00df er wiederkehret mir:", "tokens": ["Ich", "wei\u00df", "nur", ",", "da\u00df", "er", "ist", "mein", "Gat\u00b7te", "und", "da\u00df", "er", "wie\u00b7der\u00b7keh\u00b7ret", "mir", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VAFIN", "PPOSAT", "NN", "KON", "KOUS", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Er schlang von gelben Schl\u00fcsselblumen den Reif um meine Rechte hier,", "tokens": ["Er", "schlang", "von", "gel\u00b7ben", "Schl\u00fcs\u00b7sel\u00b7blu\u00b7men", "den", "Reif", "um", "mei\u00b7ne", "Rech\u00b7te", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Und sprach: \u00bbAuf Monde bannt das Schicksal mich fern von dir, geliebte Frau,", "tokens": ["Und", "sprach", ":", "\u00bb", "Auf", "Mon\u00b7de", "bannt", "das", "Schick\u00b7sal", "mich", "fern", "von", "dir", ",", "ge\u00b7lieb\u00b7te", "Frau", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "APPR", "NE", "VVFIN", "ART", "NN", "PPER", "ADJD", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Doch wann die Schl\u00fcsselblumen wieder, die gelben, sprie\u00dfen auf der Au,", "tokens": ["Doch", "wann", "die", "Schl\u00fcs\u00b7sel\u00b7blu\u00b7men", "wie\u00b7der", ",", "die", "gel\u00b7ben", ",", "sprie\u00b7\u00dfen", "auf", "der", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "$,", "PRELS", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.9": {"line.1": {"text": "Dann kehr' ich dir zur\u00fcck so sicher, als Sonn' und Mond am Himmel gehn.\u00ab", "tokens": ["Dann", "kehr'", "ich", "dir", "zu\u00b7r\u00fcck", "so", "si\u00b7cher", ",", "als", "Sonn'", "und", "Mond", "am", "Him\u00b7mel", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$,", "KOUS", "NN", "KON", "NN", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Schon hab' ich heut' aus Schnee und Eise das erste Veilchen lauschen sehn,", "tokens": ["Schon", "hab'", "ich", "heut'", "aus", "Schnee", "und", "Ei\u00b7se", "das", "ers\u00b7te", "Veil\u00b7chen", "lau\u00b7schen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.10": {"line.1": {"text": "Nun kommt er bald!\u00ab \u2013 \u00bbDu willst noch h\u00f6hnen?\u00ab ruft da der K\u00f6nig zornesbleich,", "tokens": ["Nun", "kommt", "er", "bald", "!", "\u00ab", "\u2013", "\u00bb", "Du", "willst", "noch", "h\u00f6h\u00b7nen", "?", "\u00ab", "ruft", "da", "der", "K\u00f6\u00b7nig", "zor\u00b7nes\u00b7bleich", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "$(", "$(", "$(", "PPER", "VMFIN", "ADV", "VVINF", "$.", "$(", "VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "\u00bbhinab mit dir!\u00ab \u2013 Schon setzt die Holde den wei\u00dfen Fu\u00df ins Totenreich: \u2013", "tokens": ["\u00bb", "hin\u00b7ab", "mit", "dir", "!", "\u00ab", "\u2013", "Schon", "setzt", "die", "Hol\u00b7de", "den", "wei\u00b7\u00dfen", "Fu\u00df", "ins", "To\u00b7ten\u00b7reich", ":", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "APPR", "PPER", "$.", "$(", "$(", "ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "Da pl\u00f6tzlich rauscht es in den L\u00fcften, es blitzt, es donnert, braust und weht,", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "rauscht", "es", "in", "den", "L\u00fcf\u00b7ten", ",", "es", "blitzt", ",", "es", "don\u00b7nert", ",", "braust", "und", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ein warmer Hauch wie Veilchend\u00fcfte berauschend durch die Wipfel geht,", "tokens": ["Ein", "war\u00b7mer", "Hauch", "wie", "Veil\u00b7chen\u00b7d\u00fcf\u00b7te", "be\u00b7rau\u00b7schend", "durch", "die", "Wip\u00b7fel", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "NN", "VVPP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.12": {"line.1": {"text": "Hie Sonnenschein, dort Regenbogen, ein Schwalbenflug, er zwitschert hell,", "tokens": ["Hie", "Son\u00b7nen\u00b7schein", ",", "dort", "Re\u00b7gen\u00b7bo\u00b7gen", ",", "ein", "Schwal\u00b7ben\u00b7flug", ",", "er", "zwit\u00b7schert", "hell", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Der Rasen gr\u00fcnt, die B\u00fcsche knospen und aus dem Eise bricht der Quell.", "tokens": ["Der", "Ra\u00b7sen", "gr\u00fcnt", ",", "die", "B\u00fc\u00b7sche", "knos\u00b7pen", "und", "aus", "dem", "Ei\u00b7se", "bricht", "der", "Quell", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.13": {"line.1": {"text": "Die Erde bebt und aus dem Grabe, umstrahlt von lichtem G\u00f6tterglanz,", "tokens": ["Die", "Er\u00b7de", "bebt", "und", "aus", "dem", "Gra\u00b7be", ",", "um\u00b7strahlt", "von", "lich\u00b7tem", "G\u00f6t\u00b7ter\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Der Fremdling steigt in gr\u00fcnem Mantel und auf dem Haupt den Veilchenkranz.", "tokens": ["Der", "Fremd\u00b7ling", "steigt", "in", "gr\u00fc\u00b7nem", "Man\u00b7tel", "und", "auf", "dem", "Haupt", "den", "Veil\u00b7chen\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.14": {"line.1": {"text": "\u00bbgott Baldur!\u00ab rufen Volk und K\u00f6nig und sinken bebend in die Knie,", "tokens": ["\u00bb", "gott", "Bal\u00b7dur", "!", "\u00ab", "ru\u00b7fen", "Volk", "und", "K\u00f6\u00b7nig", "und", "sin\u00b7ken", "be\u00b7bend", "in", "die", "Knie", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$.", "$(", "VVFIN", "NN", "KON", "NN", "KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Er aber fa\u00dft die Hand Haraldas und zu den Sternen schweben sie.", "tokens": ["Er", "a\u00b7ber", "fa\u00dft", "die", "Hand", "Ha\u00b7ral\u00b7das", "und", "zu", "den", "Ster\u00b7nen", "schwe\u00b7ben", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "NE", "KON", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-++-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}}, "stanza.15": {"line.1": {"text": "\u00bbder Fremdling war's im gr\u00fcnen Mantel, ums Lockenhaupt den Veilchenkranz,", "tokens": ["\u00bb", "der", "Fremd\u00b7ling", "wa\u00b7r's", "im", "gr\u00fc\u00b7nen", "Man\u00b7tel", ",", "ums", "Lo\u00b7cken\u00b7haupt", "den", "Veil\u00b7chen\u00b7kranz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Er hat bet\u00f6rt die K\u00f6nigstochter, die er gef\u00fchrt im Maientanz.", "tokens": ["Er", "hat", "be\u00b7t\u00f6rt", "die", "K\u00f6\u00b7nig\u00b7stoch\u00b7ter", ",", "die", "er", "ge\u00b7f\u00fchrt", "im", "Mai\u00b7en\u00b7tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.16": {"line.1": {"text": "Er kam, man wei\u00df es nicht, von wannen, er schied und niemand wei\u00df, wohin.", "tokens": ["Er", "kam", ",", "man", "wei\u00df", "es", "nicht", ",", "von", "wan\u00b7nen", ",", "er", "schied", "und", "nie\u00b7mand", "wei\u00df", ",", "wo\u00b7hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "ADV", "$,", "PPER", "VVFIN", "KON", "PIS", "VVFIN", "$,", "PWAV", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Du bist betrogen, sch\u00f6n Haralda, und Schmach und Tod ist dein Gewinn.\u00ab", "tokens": ["Du", "bist", "be\u00b7tro\u00b7gen", ",", "sch\u00f6n", "Ha\u00b7ral\u00b7da", ",", "und", "Schmach", "und", "Tod", "ist", "dein", "Ge\u00b7winn", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADJD", "NE", "$,", "KON", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.17": {"line.1": {"text": "So klagt das Volk; doch K\u00f6nig Olaf, der finstre, klagt und drohet nicht.", "tokens": ["So", "klagt", "das", "Volk", ";", "doch", "K\u00f6\u00b7nig", "O\u00b7laf", ",", "der", "finst\u00b7re", ",", "klagt", "und", "dro\u00b7het", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "NN", "NE", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ein Grab l\u00e4\u00dft er im Walde graben, durch Eis und Schnee der Spaten bricht.", "tokens": ["Ein", "Grab", "l\u00e4\u00dft", "er", "im", "Wal\u00b7de", "gra\u00b7ben", ",", "durch", "Eis", "und", "Schnee", "der", "Spa\u00b7ten", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,", "APPR", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+---+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.18": {"line.1": {"text": "Im Fr\u00fchm\u00e4rz ist's: kahl stehn die B\u00e4ume, kein Vogelruf, Eis deckt den Quell,", "tokens": ["Im", "Fr\u00fch\u00b7m\u00e4rz", "ist's", ":", "kahl", "stehn", "die", "B\u00e4u\u00b7me", ",", "kein", "Vo\u00b7gel\u00b7ruf", ",", "Eis", "deckt", "den", "Quell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$.", "ADJD", "VVFIN", "ART", "NN", "$,", "PIAT", "NN", "$,", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Rings alles starr: nur hoch am Himmel zieht's hin wie Fr\u00fchlingswolken hell.", "tokens": ["Rings", "al\u00b7les", "starr", ":", "nur", "hoch", "am", "Him\u00b7mel", "zieht's", "hin", "wie", "Fr\u00fch\u00b7lings\u00b7wol\u00b7ken", "hell", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADJD", "$.", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "PTKVZ", "KOKOM", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.19": {"line.1": {"text": "Und schweigend f\u00fchrt vor allem Volke sein Kind er an den dunkeln Schlund:", "tokens": ["Und", "schwei\u00b7gend", "f\u00fchrt", "vor", "al\u00b7lem", "Vol\u00b7ke", "sein", "Kind", "er", "an", "den", "dun\u00b7keln", "Schlund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PIS", "NN", "PPOSAT", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "\u00bblebendig sei mit deiner Schande verschlungen von der Erde Grund,", "tokens": ["\u00bb", "le\u00b7ben\u00b7dig", "sei", "mit", "dei\u00b7ner", "Schan\u00b7de", "ver\u00b7schlun\u00b7gen", "von", "der", "Er\u00b7de", "Grund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.20": {"line.1": {"text": "Sagst du mir nicht des Frevlers Namen und wo ihn trifft mein Strafgericht.\u00ab", "tokens": ["Sagst", "du", "mir", "nicht", "des", "Frev\u00b7lers", "Na\u00b7men", "und", "wo", "ihn", "trifft", "mein", "Straf\u00b7ge\u00b7richt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "ART", "NN", "NN", "KON", "PWAV", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Doch sie schlug auf die sch\u00f6nen Augen und sprach in Ruh': \u00bbIch wei\u00df es nicht!", "tokens": ["Doch", "sie", "schlug", "auf", "die", "sch\u00f6\u00b7nen", "Au\u00b7gen", "und", "sprach", "in", "Ruh'", ":", "\u00bb", "Ich", "wei\u00df", "es", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "APPR", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "--+--+-+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.21": {"line.1": {"text": "Ich wei\u00df nur, da\u00df er ist mein Gatte und da\u00df er wiederkehret mir:", "tokens": ["Ich", "wei\u00df", "nur", ",", "da\u00df", "er", "ist", "mein", "Gat\u00b7te", "und", "da\u00df", "er", "wie\u00b7der\u00b7keh\u00b7ret", "mir", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VAFIN", "PPOSAT", "NN", "KON", "KOUS", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Er schlang von gelben Schl\u00fcsselblumen den Reif um meine Rechte hier,", "tokens": ["Er", "schlang", "von", "gel\u00b7ben", "Schl\u00fcs\u00b7sel\u00b7blu\u00b7men", "den", "Reif", "um", "mei\u00b7ne", "Rech\u00b7te", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.22": {"line.1": {"text": "Und sprach: \u00bbAuf Monde bannt das Schicksal mich fern von dir, geliebte Frau,", "tokens": ["Und", "sprach", ":", "\u00bb", "Auf", "Mon\u00b7de", "bannt", "das", "Schick\u00b7sal", "mich", "fern", "von", "dir", ",", "ge\u00b7lieb\u00b7te", "Frau", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "APPR", "NE", "VVFIN", "ART", "NN", "PPER", "ADJD", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Doch wann die Schl\u00fcsselblumen wieder, die gelben, sprie\u00dfen auf der Au,", "tokens": ["Doch", "wann", "die", "Schl\u00fcs\u00b7sel\u00b7blu\u00b7men", "wie\u00b7der", ",", "die", "gel\u00b7ben", ",", "sprie\u00b7\u00dfen", "auf", "der", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "$,", "PRELS", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.23": {"line.1": {"text": "Dann kehr' ich dir zur\u00fcck so sicher, als Sonn' und Mond am Himmel gehn.\u00ab", "tokens": ["Dann", "kehr'", "ich", "dir", "zu\u00b7r\u00fcck", "so", "si\u00b7cher", ",", "als", "Sonn'", "und", "Mond", "am", "Him\u00b7mel", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$,", "KOUS", "NN", "KON", "NN", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Schon hab' ich heut' aus Schnee und Eise das erste Veilchen lauschen sehn,", "tokens": ["Schon", "hab'", "ich", "heut'", "aus", "Schnee", "und", "Ei\u0