{"textgrid.poem.33872": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die treuen Br\u00fcder", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sind zwei treue Br\u00fcder,", "tokens": ["Es", "sind", "zwei", "treu\u00b7e", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die zieh'n in den Streit hinaus,", "tokens": ["Die", "zieh'n", "in", "den", "Streit", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch reden sie hin und wieder,", "tokens": ["Noch", "re\u00b7den", "sie", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da schmettert's den Einen darnieder,", "tokens": ["Da", "schmet\u00b7tert's", "den", "Ei\u00b7nen", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ART", "PAV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der And're sieht's mit Graus.", "tokens": ["Der", "An\u00b7d'\u00b7re", "sieht's", "mit", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Bruder in seinem Blute", "tokens": ["Der", "Bru\u00b7der", "in", "sei\u00b7nem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Erregt ihm bittern Schmerz;", "tokens": ["Er\u00b7regt", "ihm", "bit\u00b7tern", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ihn der Tod ereilte,", "tokens": ["Da\u00df", "ihn", "der", "Tod", "er\u00b7eil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bevor er den Kampf noch theilte,", "tokens": ["Be\u00b7vor", "er", "den", "Kampf", "noch", "theil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zerrei\u00dft ihm ganz das Herz.", "tokens": ["Zer\u00b7rei\u00dft", "ihm", "ganz", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Sterbende blickt freundlich", "tokens": ["Der", "Ster\u00b7ben\u00b7de", "blickt", "freund\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Noch einmal auf zu ihm,", "tokens": ["Noch", "ein\u00b7mal", "auf", "zu", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann greift er, als w\u00e4r' er der Alte,", "tokens": ["Dann", "greift", "er", ",", "als", "w\u00e4r'", "er", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zur B\u00fcchse, die noch nicht knallte,", "tokens": ["Zur", "B\u00fcch\u00b7se", ",", "die", "noch", "nicht", "knall\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dr\u00fcckt ab mit Ungest\u00fcm.", "tokens": ["Dr\u00fcckt", "ab", "mit", "Un\u00b7ge\u00b7st\u00fcm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun bricht er wieder zusammen", "tokens": ["Nun", "bricht", "er", "wie\u00b7der", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und l\u00e4chelt, und ist todt. \u2013", "tokens": ["Und", "l\u00e4\u00b7chelt", ",", "und", "ist", "todt", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der And're, als er sich wandte,", "tokens": ["Der", "An\u00b7d'\u00b7re", ",", "als", "er", "sich", "wand\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sah einen Feind im Sande,", "tokens": ["Sah", "ei\u00b7nen", "Feind", "im", "San\u00b7de", ","], "token_info": ["wor