{"dta.poem.20096": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Junker Dampf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Aus einem edlen Stamme", "tokens": ["Aus", "ei\u00b7nem", "ed\u00b7len", "Stam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entspro\u00df der Junker Dampf:", "tokens": ["Ent\u00b7spro\u00df", "der", "Jun\u00b7ker", "Dampf", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Wasser und die Flamme", "tokens": ["Das", "Was\u00b7ser", "und", "die", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erzeugten ihn im Kampf;", "tokens": ["Er\u00b7zeug\u00b7ten", "ihn", "im", "Kampf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch hin und her getragen,", "tokens": ["Doch", "hin", "und", "her", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Spielball jedem Wind,", "tokens": ["Ein", "Spiel\u00b7ball", "je\u00b7dem", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schien aus der Art geschlagen", "tokens": ["Schien", "aus", "der", "Art", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Elementenkind.", "tokens": ["Das", "E\u00b7le\u00b7men\u00b7ten\u00b7kind", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ja, frei an F\u00fc\u00df\u2019 und H\u00e4nden", "tokens": ["Ja", ",", "frei", "an", "F\u00fc\u00df'", "und", "H\u00e4n\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist er ein lockrer Fant,", "tokens": ["Ist", "er", "ein", "lock\u00b7rer", "Fant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Doch hinter Kerkerw\u00e4nden", "tokens": ["Doch", "hin\u00b7ter", "Ker\u00b7ker\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da wird er ein Gigant:", "tokens": ["Da", "wird", "er", "ein", "Gi\u00b7gant", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In tausend Tr\u00fcmmerreste", "tokens": ["In", "tau\u00b7send", "Tr\u00fcm\u00b7mer\u00b7res\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zerschl\u00e4gt er jede Haft,", "tokens": ["Zer\u00b7schl\u00e4gt", "er", "je\u00b7de", "Haft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit ihrer Dicht\u2019 und Feste", "tokens": ["Mit", "ih\u00b7rer", "Dicht'", "und", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "W\u00e4chst seine Riesenkraft.", "tokens": ["W\u00e4chst", "sei\u00b7ne", "Rie\u00b7sen\u00b7kraft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Selbst da, wo seiner Zelle", "tokens": ["Selbst", "da", ",", "wo", "sei\u00b7ner", "Zel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein schmales Pf\u00f6rtchen blieb,", "tokens": ["Ein", "schma\u00b7les", "Pf\u00f6rt\u00b7chen", "blieb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ringt er nach Luft und Helle", "tokens": ["Ringt", "er", "nach", "Luft", "und", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mit solchem Sturmestrieb,", "tokens": ["Mit", "sol\u00b7chem", "Stur\u00b7me\u00b7strieb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df, wenn ihn beim Entwischen", "tokens": ["Da\u00df", ",", "wenn", "ihn", "beim", "Ent\u00b7wi\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Thores Enge hemmt,", "tokens": ["Des", "Tho\u00b7res", "En\u00b7ge", "hemmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Kerker, unter Zischen,", "tokens": ["Den", "Ker\u00b7ker", ",", "un\u00b7ter", "Zi\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er auf die Schulter klemmt;", "tokens": ["Er", "auf", "die", "Schul\u00b7ter", "klemmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und so, trotz eh\u2019rner Fessel", "tokens": ["Und", "so", ",", "trotz", "eh'r\u00b7ner", "Fes\u00b7sel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An F\u00fc\u00dfen noch und Hand,", "tokens": ["An", "F\u00fc\u00b7\u00dfen", "noch", "und", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rei\u00dft er den Kerkerkessel", "tokens": ["Rei\u00dft", "er", "den", "Ker\u00b7ker\u00b7kes\u00b7sel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Im Fluge mit durch\u2019s Land,", "tokens": ["Im", "Flu\u00b7ge", "mit", "durch's", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Rei\u00dft ganze H\u00e4userreihen", "tokens": ["Rei\u00dft", "gan\u00b7ze", "H\u00e4u\u00b7ser\u00b7rei\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit fort, wie Wirbelwind,", "tokens": ["Mit", "fort", ",", "wie", "Wir\u00b7bel\u00b7wind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PTKVZ", "$,", "PWAV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis wieder er im Freien", "tokens": ["Bis", "wie\u00b7der", "er", "im", "Frei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nichts, als \u2014 ein spielend Kind.", "tokens": ["Nichts", ",", "als", "ein", "spie\u00b7lend", "Kind", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "$(", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}