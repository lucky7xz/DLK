{"dta.poem.6874": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sind unsre Tr\u00e4ume nun ein gar zu schwach Exempel", "tokens": ["Sind", "uns\u00b7re", "Tr\u00e4u\u00b7me", "nun", "ein", "gar", "zu", "schwach", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ART", "ADV", "PTKA", "ADJD", "NN"], "meter": "-+-+--++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zu zeigen, da\u00df der Leib nichts hat, als das Bewegen,", "tokens": ["Zu", "zei\u00b7gen", ",", "da\u00df", "der", "Leib", "nichts", "hat", ",", "als", "das", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "PIS", "VAFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df die Seele blos der Sinnen Sitz und Tempel;", "tokens": ["Und", "da\u00df", "die", "See\u00b7le", "blos", "der", "Sin\u00b7nen", "Sitz", "und", "Tem\u00b7pel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lasset uns bedachtsam \u00fcberlegen", "tokens": ["So", "las\u00b7set", "uns", "be\u00b7dacht\u00b7sam", "\u00fc\u00b7berl\u00b7e\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Dinge, so in uns des Fiebers Gluht erregen.", "tokens": ["Die", "Din\u00b7ge", ",", "so", "in", "uns", "des", "Fie\u00b7bers", "Gluht", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Lasst uns die Vorw\u00fcrff\u2019 einst, die sie uns weiset, sehn,", "tokens": ["Lasst", "uns", "die", "Vor\u00b7w\u00fcrff'", "einst", ",", "die", "sie", "uns", "wei\u00b7set", ",", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn unser Haupt durch sie erhitzte D\u00fcnste f\u00fcllen:", "tokens": ["Wenn", "un\u00b7ser", "Haupt", "durch", "sie", "er\u00b7hitz\u00b7te", "D\u00fcns\u00b7te", "f\u00fcl\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Man seh\u2019 gewaltsame Vergehungen entstehn,", "tokens": ["Man", "seh'", "ge\u00b7walt\u00b7sa\u00b7me", "Ver\u00b7ge\u00b7hun\u00b7gen", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Von Wahnwitz, Raserey, auch von der Schw\u00e4rmer Grillen,", "tokens": ["Von", "Wahn\u00b7witz", ",", "Ra\u00b7se\u00b7rey", ",", "auch", "von", "der", "Schw\u00e4r\u00b7mer", "Gril\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "$,", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wann von dem aufgebrachten Blut", "tokens": ["Wann", "von", "dem", "auf\u00b7ge\u00b7brach\u00b7ten", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das voller wilden Hitz und Gluht,", "tokens": ["Das", "vol\u00b7ler", "wil\u00b7den", "Hitz", "und", "Gluht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die schnellen Trieb\u2019 ins Hirn verwirrte Geister treiben,", "tokens": ["Die", "schnel\u00b7len", "Trieb'", "ins", "Hirn", "ver\u00b7wirr\u00b7te", "Geis\u00b7ter", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die sich an des Gesichts so zarte Nerven reiben.", "tokens": ["Die", "sich", "an", "des", "Ge\u00b7sichts", "so", "zar\u00b7te", "Ner\u00b7ven", "rei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Welch Schreck-Gespenster-Heer nimmt uns die Sinnen ein,", "tokens": ["Welch", "Schreck\u00b7Ge\u00b7spens\u00b7ter\u00b7Heer", "nimmt", "uns", "die", "Sin\u00b7nen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das uns noch st\u00e4rcker r\u00fchrt,", "tokens": ["Das", "uns", "noch", "st\u00e4r\u00b7cker", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Als Sachen, die man leiblich sp\u00fcrt,", "tokens": ["Als", "Sa\u00b7chen", ",", "die", "man", "leib\u00b7lich", "sp\u00fcrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und w\u00fcrcklich gegenw\u00e4rtig seyn.", "tokens": ["Und", "w\u00fcrck\u00b7lich", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}