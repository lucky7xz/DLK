{"textgrid.poem.36502": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1. Der Maulwurf und der Hamster", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In die gef\u00fcllte Speisekammer", "tokens": ["In", "die", "ge\u00b7f\u00fcll\u00b7te", "Spei\u00b7se\u00b7kam\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Hamsters grub sich einst ein Maulwurf ein.", "tokens": ["Des", "Hams\u00b7ters", "grub", "sich", "einst", "ein", "Maul\u00b7wurf", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier, sprach er klagend, hier wird meines Hungers Jammer", "tokens": ["Hier", ",", "sprach", "er", "kla\u00b7gend", ",", "hier", "wird", "mei\u00b7nes", "Hun\u00b7gers", "Jam\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "VVPP", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Einmal am Ende sein!", "tokens": ["Ein\u00b7mal", "am", "En\u00b7de", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Acht Tage schon hab' ich gegraben,", "tokens": ["Acht", "Ta\u00b7ge", "schon", "hab'", "ich", "ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und nichts gefunden, mich zu laben;", "tokens": ["Und", "nichts", "ge\u00b7fun\u00b7den", ",", "mich", "zu", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVPP", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gottlob! \u2013", "tokens": ["Gott\u00b7lob", "!", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Indem er's sagt, entsteht", "tokens": ["In\u00b7dem", "er's", "sagt", ",", "ent\u00b7steht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ein gro\u00dfer L\u00e4rm, der Hamster kommt gesprungen;", "tokens": ["Ein", "gro\u00b7\u00dfer", "L\u00e4rm", ",", "der", "Hams\u00b7ter", "kommt", "ge\u00b7sprun\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "\u00bbdieb! R\u00e4uber! M\u00f6rder!\u00ab \u2013 Gnade fleht", "tokens": ["\u00bb", "dieb", "!", "R\u00e4u\u00b7ber", "!", "M\u00f6r\u00b7der", "!", "\u00ab", "\u2013", "Gna\u00b7de", "fleht"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word"], "pos": ["$(", "VVIMP", "$.", "NN", "$.", "NN", "$.", "$(", "$(", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der arme Hungrige! Gek\u00e4mpfet und gerungen", "tokens": ["Der", "ar\u00b7me", "Hung\u00b7ri\u00b7ge", "!", "Ge\u00b7k\u00e4mp\u00b7fet", "und", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "KON", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auf Leben und auf Tod, wie in Amerika,", "tokens": ["Auf", "Le\u00b7ben", "und", "auf", "Tod", ",", "wie", "in", "A\u00b7me\u00b7ri\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "PWAV", "APPR", "NE", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Wird in der allzuengen Kammer. \u2013", "tokens": ["Wird", "in", "der", "all\u00b7zu\u00b7en\u00b7gen", "Kam\u00b7mer", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der arme Maulwurf stirbt und endigt seinen Jammer.", "tokens": ["Der", "ar\u00b7me", "Maul\u00b7wurf", "stirbt", "und", "en\u00b7digt", "sei\u00b7nen", "Jam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So w\u00e4ren, wenn's geschah,", "tokens": ["So", "w\u00e4\u00b7ren", ",", "wenn's", "ge\u00b7schah", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Tiere ja", "tokens": ["Die", "Tie\u00b7re", "ja"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So grausam wie die Menschen? Nein!", "tokens": ["So", "grau\u00b7sam", "wie", "die", "Men\u00b7schen", "?", "Nein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "