, "noch", "bl\u00fcht", "dein", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Cephalus, erwache doch!", "tokens": ["Ce\u00b7pha\u00b7lus", ",", "er\u00b7wa\u00b7che", "doch", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald, bald k\u00f6mmt der Tag zur\u00fccke,", "tokens": ["Bald", ",", "bald", "k\u00f6mmt", "der", "Tag", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein Gew\u00f6lk nur deckt ihn noch!", "tokens": ["Ein", "Ge\u00b7w\u00f6lk", "nur", "deckt", "ihn", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und du weist, vor seinem Blicke", "tokens": ["Und", "du", "weist", ",", "vor", "sei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flieht Auror': erwache doch!", "tokens": ["Flieht", "Au\u00b7ror'", ":", "er\u00b7wa\u00b7che", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "VVFIN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "So sprach sie: und der Silberwagen", "tokens": ["So", "sprach", "sie", ":", "und", "der", "Sil\u00b7ber\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bringt schon vom fern den Gott getragen,", "tokens": ["Bringt", "schon", "vom", "fern", "den", "Gott", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der auf die Welt sein Licht ergie\u00dft:", "tokens": ["Der", "auf", "die", "Welt", "sein", "Licht", "er\u00b7gie\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein naher Glanz entschlie\u00dft,", "tokens": ["Sein", "na\u00b7her", "Glanz", "ent\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wiewohl zu sp\u00e4t, des J\u00fcnglings Augenl\u00fcder:", "tokens": ["Wie\u00b7wohl", "zu", "sp\u00e4t", ",", "des", "J\u00fcng\u00b7lings", "Au\u00b7gen\u00b7l\u00fc\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKA", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sein Gl\u00fcck war da, und sieh, es floh auch wieder!", "tokens": ["Sein", "Gl\u00fcck", "war", "da", ",", "und", "sieh", ",", "es", "floh", "auch", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "KON", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er wachet, sieht sie, schreyt ihr nach:", "tokens": ["Er", "wa\u00b7chet", ",", "sieht", "sie", ",", "schreyt", "ihr", "nach", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Weinen ist umsonst, vergebens ist sein Ach!", "tokens": ["Sein", "Wei\u00b7nen", "ist", "um\u00b7sonst", ",", "ver\u00b7ge\u00b7bens", "ist", "sein", "Ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie flieht, und l\u00e4\u00dft zu seinen Schmerzen", "tokens": ["Sie", "flieht", ",", "und", "l\u00e4\u00dft", "zu", "sei\u00b7nen", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das Bild von einem kurz bese\u00dfnen Gl\u00fcck,", "tokens": ["Das", "Bild", "von", "ei\u00b7nem", "kurz", "be\u00b7se\u00df\u00b7nen", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das er verschlief, zur\u00fcck.", "tokens": ["Das", "er", "ver\u00b7schlief", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "So straft die Lieb oft junge Herzen:", "tokens": ["So", "straft", "die", "Lieb", "oft", "jun\u00b7ge", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ihr jungen Herzen merkt, merkt ja wohl sein Geschick!", "tokens": ["Ihr", "jun\u00b7gen", "Her\u00b7zen", "merkt", ",", "merkt", "ja", "wohl", "sein", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Erwartet nie den sp\u00e4ten Morgen,", "tokens": ["Er\u00b7war\u00b7tet", "nie", "den", "sp\u00e4\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wacht ja, so bald Aurore wacht:", "tokens": ["Wacht", "ja", ",", "so", "bald", "Au\u00b7ro\u00b7re", "wacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "ADV", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Liebe Macht bleibt euch verborgen,", "tokens": ["Der", "Lie\u00b7be", "Macht", "bleibt", "euch", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn euch der Schlummer f\u00fchllos macht.", "tokens": ["Wenn", "euch", "der", "Schlum\u00b7mer", "f\u00fchl\u00b7los", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "m