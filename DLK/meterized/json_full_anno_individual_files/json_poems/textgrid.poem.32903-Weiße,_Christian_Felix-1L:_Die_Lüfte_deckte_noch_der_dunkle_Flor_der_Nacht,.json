{"textgrid.poem.32903": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die L\u00fcfte deckte noch der dunkle Flor der Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die L\u00fcfte deckte noch der dunkle Flor der Nacht,", "tokens": ["Die", "L\u00fcf\u00b7te", "deck\u00b7te", "noch", "der", "dunk\u00b7le", "Flor", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt erleuchtete nur der Diane Pracht,", "tokens": ["Die", "Welt", "er\u00b7leuch\u00b7te\u00b7te", "nur", "der", "Di\u00b7a\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als schon von Orients entfernten hei\u00dfen Fl\u00fc\u00dfen", "tokens": ["Als", "schon", "von", "O\u00b7rients", "ent\u00b7fern\u00b7ten", "hei\u00b7\u00dfen", "Fl\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Aurore, durch der Liebe Macht,", "tokens": ["Au\u00b7ro\u00b7re", ",", "durch", "der", "Lie\u00b7be", "Macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem sanften Schlaf entrissen,", "tokens": ["Dem", "sanf\u00b7ten", "Schlaf", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nach ihrem liebsten Cephal gieng,", "tokens": ["Nach", "ih\u00b7rem", "liebs\u00b7ten", "Ce\u00b7phal", "gieng", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den noch des Schlafes Arm umfieng.", "tokens": ["Den", "noch", "des", "Schla\u00b7fes", "Arm", "um\u00b7fi\u00b7eng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Sie nahet sich: Furcht, Zweifel und Entz\u00fccken", "tokens": ["Sie", "na\u00b7het", "sich", ":", "Furcht", ",", "Zwei\u00b7fel", "und", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$.", "NN", "$,", "NN", "KON", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Entdecken sich in ihren Blicken,", "tokens": ["Ent\u00b7de\u00b7cken", "sich", "in", "ih\u00b7ren", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da sie den holden J\u00fcngling sieht:", "tokens": ["Da", "sie", "den", "hol\u00b7den", "J\u00fcng\u00b7ling", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Liebe Brand, von dem sie gl\u00fcht,", "tokens": ["Der", "Lie\u00b7be", "Brand", ",", "von", "dem", "sie", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Erkl\u00e4rt sich schamhaft durch die\u00df Lied.", "tokens": ["Er\u00b7kl\u00e4rt", "sich", "scham\u00b7haft", "durch", "die\u00df", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "PDS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr Fl\u00fcsse, rauschet ganz gelinde!", "tokens": ["Ihr", "Fl\u00fcs\u00b7se", ",", "rau\u00b7schet", "ganz", "ge\u00b7lin\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weht sanft und k\u00fchl, ihr Fr\u00fchlingswinde!", "tokens": ["Weht", "sanft", "und", "k\u00fchl", ",", "ihr", "Fr\u00fch\u00b7lings\u00b7win\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr V\u00f6gel, d\u00e4mpft die Melodien!", "tokens": ["Ihr", "V\u00f6\u00b7gel", ",", "d\u00e4mpft", "die", "Me\u00b7lo\u00b7dien", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verliehrt kein Blatt, ihr stillen B\u00e4ume,", "tokens": ["Ver\u00b7liehrt", "kein", "Blatt", ",", "ihr", "stil\u00b7len", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wieg ihn, o Schlaf, in sanfte Tr\u00e4ume!", "tokens": ["Wieg", "ihn", ",", "o", "Schlaf", ",", "in", "sanf\u00b7te", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Liebesg\u00f6tter, wacht um ihn!", "tokens": ["Ihr", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", ",", "wacht", "um", "ihn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Allein, was sag ich? nein: die blinde Z\u00e4rtlichkeit", "tokens": ["Al\u00b7lein", ",", "was", "sag", "ich", "?", "nein", ":", "die", "blin\u00b7de", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWS", "VVFIN", "PPER", "$.", "PTKANT", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verf\u00fchret mich zu weit.", "tokens": ["Ver\u00b7f\u00fch\u00b7ret", "mich", "zu", "weit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leichtsinniger, ist die\u00df dein Sehnen,", "tokens": ["Leicht\u00b7sin\u00b7ni\u00b7ger", ",", "ist", "die\u00df", "dein", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PDS", "PPOSAT", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Da\u00df dich der Schlaf besiegt?", "tokens": ["Da\u00df", "dich", "der", "Schlaf", "be\u00b7siegt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und seufzet so dein Arm nach seiner Sch\u00f6nen,", "tokens": ["Und", "seuf\u00b7zet", "so", "dein", "Arm", "nach", "sei\u00b7ner", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df er auf weichem Moo\u00df hier sinkend kraftlos liegt?", "tokens": ["Da\u00df", "er", "auf", "wei\u00b7chem", "Moo\u00df", "hier", "sin\u00b7kend", "kraft\u00b7los", "liegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So soll ich ungek\u00fc\u00dft hier einsam bey dir stehen,", "tokens": ["So", "soll", "ich", "un\u00b7ge\u00b7k\u00fc\u00dft", "hier", "ein\u00b7sam", "bey", "dir", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dich dem Schlaf in Armen sehen? \u2013 \u2013", "tokens": ["Und", "dich", "dem", "Schlaf", "in", "Ar\u00b7men", "se\u00b7hen", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vortrefflich! ey wie sehr", "tokens": ["Vor\u00b7treff\u00b7lich", "!", "ey", "wie", "sehr"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "NE", "KOKOM", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Hoffst du auf meine Wiederkehr!", "tokens": ["Hoffst", "du", "auf", "mei\u00b7ne", "Wie\u00b7der\u00b7kehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Cephalus! noch bl\u00fcht dein Gl\u00fccke!", "tokens": ["Ce\u00b7pha\u00b7lus", "!", "noch", "bl\u00fcht", "dein", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Cephalus, erwache doch!", "tokens": ["Ce\u00b7pha\u00b7lus", ",", "er\u00b7wa\u00b7che", "doch", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald, bald k\u00f6mmt der Tag zur\u00fccke,", "tokens": ["Bald", ",", "bald", "k\u00f6mmt", "der", "Tag", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein Gew\u00f6lk nur deckt ihn noch!", "tokens": ["Ein", "Ge\u00b7w\u00f6lk", "nur", "deckt", "ihn", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und du weist, vor seinem Blicke", "tokens": ["Und", "du", "weist", ",", "vor", "sei\u00b7nem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flieht Auror': erwache doch!", "tokens": ["Flieht", "Au\u00b7ror'", ":", "er\u00b7wa\u00b7che", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "VVFIN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "So sprach sie: und der Silberwagen", "tokens": ["So", "sprach", "sie", ":", "und", "der", "Sil\u00b7ber\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bringt schon vom fern den Gott getragen,", "tokens": ["Bringt", "schon", "vom", "fern", "den", "Gott", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der auf die Welt sein Licht ergie\u00dft:", "tokens": ["Der", "auf", "die", "Welt", "sein", "Licht", "er\u00b7gie\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein naher Glanz entschlie\u00dft,", "tokens": ["Sein", "na\u00b7her", "Glanz", "ent\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wiewohl zu sp\u00e4t, des J\u00fcnglings Augenl\u00fcder:", "tokens": ["Wie\u00b7wohl", "zu", "sp\u00e4t", ",", "des", "J\u00fcng\u00b7lings", "Au\u00b7gen\u00b7l\u00fc\u00b7der", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKA", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sein Gl\u00fcck war da, und sieh, es floh auch wieder!", "tokens": ["Sein", "Gl\u00fcck", "war", "da", ",", "und", "sieh", ",", "es", "floh", "auch", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "KON", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er wachet, sieht sie, schreyt ihr nach:", "tokens": ["Er", "wa\u00b7chet", ",", "sieht", "sie", ",", "schreyt", "ihr", "nach", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Weinen ist umsonst, vergebens ist sein Ach!", "tokens": ["Sein", "Wei\u00b7nen", "ist", "um\u00b7sonst", ",", "ver\u00b7ge\u00b7bens", "ist", "sein", "Ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie flieht, und l\u00e4\u00dft zu seinen Schmerzen", "tokens": ["Sie", "flieht", ",", "und", "l\u00e4\u00dft", "zu", "sei\u00b7nen", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das Bild von einem kurz bese\u00dfnen Gl\u00fcck,", "tokens": ["Das", "Bild", "von", "ei\u00b7nem", "kurz", "be\u00b7se\u00df\u00b7nen", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das er verschlief, zur\u00fcck.", "tokens": ["Das", "er", "ver\u00b7schlief", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "So straft die Lieb oft junge Herzen:", "tokens": ["So", "straft", "die", "Lieb", "oft", "jun\u00b7ge", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ihr jungen Herzen merkt, merkt ja wohl sein Geschick!", "tokens": ["Ihr", "jun\u00b7gen", "Her\u00b7zen", "merkt", ",", "merkt", "ja", "wohl", "sein", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Erwartet nie den sp\u00e4ten Morgen,", "tokens": ["Er\u00b7war\u00b7tet", "nie", "den", "sp\u00e4\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wacht ja, so bald Aurore wacht:", "tokens": ["Wacht", "ja", ",", "so", "bald", "Au\u00b7ro\u00b7re", "wacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "ADV", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Liebe Macht bleibt euch verborgen,", "tokens": ["Der", "Lie\u00b7be", "Macht", "bleibt", "euch", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn euch der Schlummer f\u00fchllos macht.", "tokens": ["Wenn", "euch", "der", "Schlum\u00b7mer", "f\u00fchl\u00b7los", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sch\u00e4ferstunde flieht von hinnen,", "tokens": ["Die", "Sch\u00e4\u00b7fer\u00b7stun\u00b7de", "flieht", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als wie ein West streicht sie vorbey,", "tokens": ["Als", "wie", "ein", "West", "streicht", "sie", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und hinterl\u00e4\u00dft den tr\u00e4gen Sinnen", "tokens": ["Und", "hin\u00b7ter\u00b7l\u00e4\u00dft", "den", "tr\u00e4\u00b7gen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verschlafner Liebe Gram und Reu.", "tokens": ["Ver\u00b7schlaf\u00b7ner", "Lie\u00b7be", "Gram", "und", "Reu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die L\u00fcfte deckte noch der dunkle Flor der Nacht,", "tokens": ["Die", "L\u00fcf\u00b7te", "deck\u00b7te", "noch", "der", "dunk\u00b7le", "Flor", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt erleuchtete nur der Diane Pracht,", "tokens": ["Die", "Welt", "er\u00b7leuch\u00b7te\u00b7te", "nur", "der", "Di\u00b7a\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als schon von Orients entfernten hei\u00dfen Fl\u00fc\u00dfen", "tokens": ["Als", "schon", "von", "O\u00b7rients", "ent\u00b7fern\u00b7ten", "hei\u00b7\u00dfen", "Fl\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Aurore, durch der Liebe Macht,", "tokens": ["Au\u00b7ro\u00b7re", ",", "durch", "der", "Lie\u00b7be", "Macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem sanften Schlaf entrissen,", "tokens": ["Dem", "sanf\u00b7ten", "Schlaf", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nach ihrem liebsten Cephal gieng,", "tokens": ["Nach", "ih\u00b7rem", "liebs\u00b7ten", "Ce\u00b7phal", "gieng", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den noch des Schlafes Arm umfieng.", "