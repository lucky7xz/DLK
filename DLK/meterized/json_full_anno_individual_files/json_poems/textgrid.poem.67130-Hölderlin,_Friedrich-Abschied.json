{"textgrid.poem.67130": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich sterbe mit Schmach, wenn an den Frechen nicht", "tokens": ["Wenn", "ich", "ster\u00b7be", "mit", "Schmach", ",", "wenn", "an", "den", "Fre\u00b7chen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$,", "KOUS", "APPR", "ART", "NN", "PTKNEG"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Meine Seele sich r\u00e4cht, wenn ich hinunter bin,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "sich", "r\u00e4cht", ",", "wenn", "ich", "hin\u00b7un\u00b7ter", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Von des Genius Feinden", "tokens": ["Von", "des", "Ge\u00b7nius", "Fein\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "\u00dcberwunden, ins feige Grab,", "tokens": ["\u00dc\u00b7berw\u00b7un\u00b7den", ",", "ins", "fei\u00b7ge", "Grab", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Dann vergi\u00df mich, o dann rette vom Untergang", "tokens": ["Dann", "ver\u00b7gi\u00df", "mich", ",", "o", "dann", "ret\u00b7te", "vom", "Un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "$,", "FM", "ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Meinen Namen auch du, g\u00fctiges Herz! nicht mehr,", "tokens": ["Mei\u00b7nen", "Na\u00b7men", "auch", "du", ",", "g\u00fc\u00b7ti\u00b7ges", "Herz", "!", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PPER", "$,", "ADJA", "NN", "$.", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dann err\u00f6te, die du mir", "tokens": ["Dann", "er\u00b7r\u00f6\u00b7te", ",", "die", "du", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hold gewesen, doch eher nicht!", "tokens": ["Hold", "ge\u00b7we\u00b7sen", ",", "doch", "e\u00b7her", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAPP", "$,", "ADV", "ADV", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Aber wei\u00df ich es nicht? Wehe! du liebender", "tokens": ["A\u00b7ber", "wei\u00df", "ich", "es", "nicht", "?", "We\u00b7he", "!", "du", "lie\u00b7ben\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "$.", "NN", "$.", "PPER", "ADJA"], "meter": "+-+---+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Schutzgeist! ferne von dir spielen zerrei\u00dfend bald", "tokens": ["Schutz\u00b7geist", "!", "fer\u00b7ne", "von", "dir", "spie\u00b7len", "zer\u00b7rei\u00b7\u00dfend", "bald"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "APPR", "PPER",