{"textgrid.poem.36780": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Aqu\u00e4dukte", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sage mir doch, was hab' ich mir wohl von den B\u00f6gen zu denken,", "tokens": ["Sa\u00b7ge", "mir", "doch", ",", "was", "hab'", "ich", "mir", "wohl", "von", "den", "B\u00f6\u00b7gen", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PWS", "VAFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Die so entsetzlich hinaus in die Campagna sich ziehn?", "tokens": ["Die", "so", "ent\u00b7setz\u00b7lich", "hin\u00b7aus", "in", "die", "Cam\u00b7pag\u00b7na", "sich", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADV", "APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "\u00bbaqu\u00e4dukte nennt man's, mein Freund, womit man in Rom so", "tokens": ["\u00bb", "a\u00b7qu\u00e4d\u00b7uk\u00b7te", "nennt", "man's", ",", "mein", "Freund", ",", "wo\u00b7mit", "man", "in", "Rom", "so"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "VVFIN", "PIS", "$,", "PPOSAT", "NN", "$,", "PWAV", "PIS", "APPR", "NE", "ADV"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wie auf dem deutschen Parna\u00df alle die Zeitungen kriegt.\u00ab", "tokens": ["Wie", "auf", "dem", "deut\u00b7schen", "Par\u00b7na\u00df", "al\u00b7le", "die", "Zei\u00b7tun\u00b7gen", "krie