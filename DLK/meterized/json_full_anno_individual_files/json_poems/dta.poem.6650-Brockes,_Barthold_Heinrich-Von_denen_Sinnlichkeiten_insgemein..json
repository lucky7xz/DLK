{"dta.poem.6650": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Handlung, die man offt in diesen C\u00f6rpern sp\u00fcrt,", "tokens": ["Die", "Hand\u00b7lung", ",", "die", "man", "offt", "in", "die\u00b7sen", "C\u00f6r\u00b7pern", "sp\u00fcrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist nicht diejenige, die uns ber\u00fchrt.", "tokens": ["Ist", "nicht", "die\u00b7je\u00b7ni\u00b7ge", ",", "die", "uns", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PDS", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn wir offt C\u00f6rper sehn,", "tokens": ["Wenn", "wir", "offt", "C\u00f6r\u00b7per", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NE", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die sich bald jagen, bald umfahen,", "tokens": ["Die", "sich", "bald", "ja\u00b7gen", ",", "bald", "um\u00b7fa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bald hoch, bald niedrig gehn,", "tokens": ["Bald", "hoch", ",", "bald", "nied\u00b7rig", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bald sich entfernen, bald sich nahen;", "tokens": ["Bald", "sich", "ent\u00b7fer\u00b7nen", ",", "bald", "sich", "na\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVINF", "$,", "ADV", "PRF", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist es ein Wind, ein Rad, so die Machine ziehen.", "tokens": ["Ist", "es", "ein", "Wind", ",", "ein", "Rad", ",", "so", "die", "Ma\u00b7chi\u00b7ne", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das heisst nicht: mit Bedacht sich suchen und sich fliehen.", "tokens": ["Das", "heisst", "nicht", ":", "mit", "Be\u00b7dacht", "sich", "su\u00b7chen", "und", "sich", "flie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "$.", "APPR", "NN", "PRF", "VVINF", "KON", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}