{"textgrid.poem.25033": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch tr\u00e4umt' ich von den Alpenwanderungen,", "tokens": ["Noch", "tr\u00e4umt'", "ich", "von", "den", "Al\u00b7pen\u00b7wan\u00b7de\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo ich mit den Lawinen Zwiesprach hielt,", "tokens": ["Wo", "ich", "mit", "den", "La\u00b7wi\u00b7nen", "Zwie\u00b7sprach", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Von Rosen, die hoch ob dem Thale", "tokens": ["Von", "Ro\u00b7sen", ",", "die", "hoch", "ob", "dem", "Tha\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "KOUS", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Morgen gr\u00fc\u00dft mit erstem Strahle,", "tokens": ["Der", "Mor\u00b7gen", "gr\u00fc\u00dft", "mit", "ers\u00b7tem", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und von der Ceder, sturmgeschwungen,", "tokens": ["Und", "von", "der", "Ce\u00b7der", ",", "sturm\u00b7ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die t\u00e4ndelnd mit dem Blitze spielt.", "tokens": ["Die", "t\u00e4n\u00b7delnd", "mit", "dem", "Blit\u00b7ze", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch nun von Ceder wie von Alpenrose,", "tokens": ["Doch", "nun", "von", "Ce\u00b7der", "wie", "von", "Al\u00b7pen\u00b7ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verstummen mu\u00df in meinem Lied der Preis,", "tokens": ["Ver\u00b7stum\u00b7men", "mu\u00df", "in", "mei\u00b7nem", "Lied", "der", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seit ich im Thale dich, das zarte", "tokens": ["Seit", "ich", "im", "Tha\u00b7le", "dich", ",", "das", "zar\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00e4rzveilchen, holderbl\u00fcht, gewahrte,", "tokens": ["M\u00e4rz\u00b7veil\u00b7chen", ",", "hol\u00b7der\u00b7bl\u00fcht", ",", "ge\u00b7wahr\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das still sich birgt im niedern Moose", "tokens": ["Das", "still", "sich", "birgt", "im", "nie\u00b7dern", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word", "