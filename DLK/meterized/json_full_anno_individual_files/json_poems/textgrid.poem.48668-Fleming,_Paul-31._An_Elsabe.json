", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "mit Lachen mag umfassen.", "tokens": ["mit", "La\u00b7chen", "mag", "um\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Di\u00df soll f\u00fcr diese kurze Pein", "tokens": ["Di\u00df", "soll", "f\u00fcr", "die\u00b7se", "kur\u00b7ze", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "uns ewig unsre Freude sein.", "tokens": ["uns", "e\u00b7wig", "uns\u00b7re", "Freu\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Eilt, lauft, ihr tr\u00fcben Tage,", "tokens": ["Eilt", ",", "lauft", ",", "ihr", "tr\u00fc\u00b7ben", "Ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "eilt, lauft, vorbei.", "tokens": ["eilt", ",", "lauft", ",", "vor\u00b7bei", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Eilt, macht mich frei", "tokens": ["Eilt", ",", "macht", "mich", "frei"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "von aller meiner Plage.", "tokens": ["von", "al\u00b7ler", "mei\u00b7ner", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Eilt, kommt ihr hellen Stunden ihr,", "tokens": ["Eilt", ",", "kommt", "ihr", "hel\u00b7len", "Stun\u00b7den", "ihr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die mich gew\u00e4ren aller Zier.", "tokens": ["die", "mich", "ge\u00b7w\u00e4\u00b7ren", "al\u00b7ler", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}