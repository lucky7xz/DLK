{"dta.poem.19411": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "144.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Behalte, was ich hier dir nicht will vorenthalten,", "tokens": ["Be\u00b7hal\u00b7te", ",", "was", "ich", "hier", "dir", "nicht", "will", "vor\u00b7ent\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "ADV", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vier Lehren, die nicht sind in jedem Ohr enthalten.", "tokens": ["Vier", "Leh\u00b7ren", ",", "die", "nicht", "sind", "in", "je\u00b7dem", "Ohr", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PTKNEG", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dir geben einen Halt, im Leben einen Stab,", "tokens": ["Dir", "ge\u00b7ben", "ei\u00b7nen", "Halt", ",", "im", "Le\u00b7ben", "ei\u00b7nen", "Stab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Worte vier: Halt ein! Halt aus! Halt an! Halt ab!", "tokens": ["Der", "Wor\u00b7te", "vier", ":", "Halt", "ein", "!", "Halt", "aus", "!", "Halt", "an", "!", "Halt", "ab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "$.", "VVIMP", "PTKVZ", "$.", "VVIMP", "PTKVZ", "$.", "VVIMP", "PTKVZ", "$.", "VVIMP", "PTKVZ", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Halt ein den Zorn, die Gier, und jede Leidenschaft;", "tokens": ["Halt", "ein", "den", "Zorn", ",", "die", "Gier", ",", "und", "je\u00b7de", "Lei\u00b7den\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ART", "NN", "$,", "ART", "NN", "$,", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Halt aus, was dich betrifft, mit starker Seelenkraft.", "tokens": ["Halt", "aus", ",", "was", "dich", "be\u00b7tr\u00b7ifft", ",", "mit", "star\u00b7ker", "See\u00b7len\u00b7kraft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}}}}}