{"dta.poem.13255": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Fr\u00f6mmigkeit bewog inzwischen das Gesicht,", "tokens": ["Die", "Fr\u00f6m\u00b7mig\u00b7keit", "be\u00b7wog", "in\u00b7zwi\u00b7schen", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie wann man innerlich von etwas andern spricht;", "tokens": ["Wie", "wann", "man", "in\u00b7ner\u00b7lich", "von", "et\u00b7was", "an\u00b7dern", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWAV", "PIS", "ADJD", "APPR", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie fieng auch endlich an: \u201eIch kann mich nicht entbrechen,", "tokens": ["Sie", "fi\u00b7eng", "auch", "end\u00b7lich", "an", ":", "\u201e", "Ich", "kann", "mich", "nicht", "ent\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$.", "$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "\u201evon einer Wichtigkeit, ", "tokens": ["\u201e", "von", "ei\u00b7ner", "Wich\u00b7tig\u00b7keit", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201ekaum stund\u2019 ich im Gemach, wo dieser Zepter war,", "tokens": ["\u201e", "kaum", "stund'", "ich", "im", "Ge\u00b7mach", ",", "wo", "die\u00b7ser", "Zep\u00b7ter", "war", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "PWAV", "PDAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "310\u201dSo h\u00f6rt' ich auf dem Plaz von einer frohen Schaar", "tokens": ["\"", "So", "h\u00f6rt'", "ich", "auf", "dem", "Plaz", "von", "ei\u00b7ner", "fro\u00b7hen", "Schaar"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eein jauchzendes Geschrey. Ich lief hinab zu sehen,", "tokens": ["\u201e", "ein", "jauch\u00b7zen\u00b7des", "Ge\u00b7schrey", ".", "Ich", "lief", "hin\u00b7ab", "zu", "se\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eso fand ich in der Burg das Volck versammelt stehen.", "tokens": ["\u201e", "so", "fand", "ich", "in", "der", "Burg", "das", "Volck", "ver\u00b7sam\u00b7melt", "ste\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ees sprung, frolockte, rieff und wies oft nach der H\u00f6h,", "tokens": ["\u201e", "es", "sprung", ",", "fro\u00b7lock\u00b7te", ",", "rieff", "und", "wies", "oft", "nach", "der", "H\u00f6h", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eals ob am Himmels-Krei\u00df ein Wunder-Zeichen steh.", "tokens": ["\u201e", "als", "ob", "am", "Him\u00b7mels\u00b7Krei\u00df", "ein", "Wun\u00b7der\u00b7Zei\u00b7chen", "steh", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "315\u201dEtwelche sungen gar: (so fern ich recht vernommen)", "tokens": ["\"", "Et\u00b7wel\u00b7che", "sun\u00b7gen", "gar", ":(", "so", "fern", "ich", "recht", "ver\u00b7nom\u00b7men", ")"], "token_info": ["punct", "word", "word", "word", "emoticon", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "ADV", "$(", "ADV", "ADJD", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eder Vorboth ist schon hier; das Gl\u00fcck wird auch bald kom\u0303en!", "tokens": ["\u201e", "der", "Vor\u00b7both", "ist", "schon", "hier", ";", "das", "Gl\u00fcck", "wird", "auch", "bald", "kom\u0303en", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADV", "ADV", "$.", "ART", "NN", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}