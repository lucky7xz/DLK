{"textgrid.poem.65817": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "91", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der prahlt mit seinem Adel, der mit Kunst,", "tokens": ["Der", "prahlt", "mit", "sei\u00b7nem", "A\u00b7del", ",", "der", "mit", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Reichtum jener, der mit Leibeskraft;", "tokens": ["Mit", "Reich\u00b7tum", "je\u00b7ner", ",", "der", "mit", "Lei\u00b7bes\u00b7kraft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "$,", "PRELS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Kleidern, wie auch Mode sie verhunzt,", "tokens": ["Mit", "Klei\u00b7dern", ",", "wie", "auch", "Mo\u00b7de", "sie", "ver\u00b7hunzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ADV", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Falk' und Hund, mit stolzer Reiterschaft;", "tokens": ["Mit", "Fal\u00b7k'", "und", "Hund", ",", "mit", "stol\u00b7zer", "Rei\u00b7ter\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und jeder Laun' ist ihre Lust gegeben,", "tokens": ["Und", "je\u00b7der", "Laun'", "ist", "ih\u00b7re", "Lust", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Worin sie gern vor andern sich behagt.", "tokens": ["Wo\u00b7rin", "sie", "gern", "vor", "an\u00b7dern", "sich", "be\u00b7hagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich aber mag nach solchen Ziel nicht streben,", "tokens": ["Ich", "a\u00b7ber", "mag", "nach", "sol\u00b7chen", "Ziel", "nicht", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "APPR", "PIAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Weil mir ein H\u00f6chstes \u00fcber alle ragt.", "tokens": ["Weil", "mir", "ein", "H\u00f6chs\u00b7tes", "\u00fc\u00b7ber", "al\u00b7le", "ragt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dein Herz ist h\u00f6her mir als hohes Blut,", "tokens": ["Dein", "Herz", "ist", "h\u00f6\u00b7her", "mir", "als", "ho\u00b7hes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "PPER", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Teurer als Gold, Gew\u00e4nder, edle Steine,", "tokens": ["Teu\u00b7rer", "als", "Gold", ",", "Ge\u00b7w\u00e4n\u00b7der", ",", "ed\u00b7le", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Begl\u00fcckender als Pferd- und Hundebrut,", "tokens": ["Be\u00b7gl\u00fc\u00b7cken\u00b7der", "als", "Pferd", "und", "Hun\u00b7de\u00b7brut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und hab' ich dich, ist aller Stolz der meine.", "tokens": ["Und", "hab'", "ich", "dich", ",", "ist", "al\u00b7ler", "Stolz", "der", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "$,", "VAFIN", "PIAT", "NN", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Unselig darin nur, da\u00df du mir's ganz", "tokens": ["Un\u00b7se\u00b7lig", "da\u00b7rin", "nur", ",", "da\u00df", "du", "mir's", "ganz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PAV", "ADV", "$,", "KOUS", "PPER", "NE", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Entziehn, und mich h\u00f6chst elend machen kannst!", "tokens": ["Ent\u00b7ziehn", ",", "und", "mich", "h\u00f6chst", "e\u00b7lend", "ma\u00b7chen", "kannst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der prahlt mit seinem Adel, der mit Kunst,", "tokens": ["Der", "prahlt", "mit", "sei\u00b7nem", "A\u00b7del", ",", "der", "mit", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-