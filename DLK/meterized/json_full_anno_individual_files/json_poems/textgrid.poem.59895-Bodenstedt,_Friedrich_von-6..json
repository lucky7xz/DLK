{"textgrid.poem.59895": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist doch Mirza-Jussuf ein vielbeles'ner Mann!", "tokens": ["Was", "ist", "doch", "Mir\u00b7za\u00b7Jus\u00b7suf", "ein", "viel\u00b7be\u00b7les'\u00b7ner", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Bald liest er den Hafis, bald liest er den Koran,", "tokens": ["Bald", "liest", "er", "den", "Ha\u00b7fis", ",", "bald", "liest", "er", "den", "Ko\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Bald Oschami und Chakani und bald den Gulistan.", "tokens": ["Bald", "O\u00b7scha\u00b7mi", "und", "Cha\u00b7ka\u00b7ni", "und", "bald", "den", "Gu\u00b7lis\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "NE", "KON", "ADV", "ART", "NN", "$."], "meter": "-+---+-+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Hier stiehlt er sich ein Bild und eine Blume dort,", "tokens": ["Hier", "stiehlt", "er", "sich", "ein", "Bild", "und", "ei\u00b7ne", "Blu\u00b7me", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "KON", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier einen sch\u00f6nen Gedanken und dort ein sch\u00f6nes Wort.", "tokens": ["Hier", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Ge\u00b7dan\u00b7ken", "und", "dort", "ein", "sch\u00f6\u00b7nes", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Was schon geschaffen ist, das schafft er wieder um,", "tokens": ["Was", "schon", "ge\u00b7schaf\u00b7fen", "ist", ",", "das", "schafft", "er", "wie\u00b7der", "um", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die ganze Welt setzt er in seine Lieder um", "tokens": ["Die", "gan\u00b7ze", "Welt", "setzt", "er", "in", "sei\u00b7ne", "Lie\u00b7der", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und h\u00e4ngt zu eignem Schmuck fremdes Gefieder um.", "tokens": ["Und", "h\u00e4ngt", "zu", "eig\u00b7nem", "Schmuck", "frem\u00b7des", "Ge\u00b7fie\u00b7der", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Damit macht er sich breit und nennt das Poesie.", "tokens": ["Da\u00b7mit", "macht", "er", "sich", "breit", "und", "nennt", "das", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADJD", "KON", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Wie anders dichtet doch und lebt Mirza-Schaffy!", "tokens": ["Wie", "an\u00b7ders", "dich\u00b7tet", "doch", "und", "lebt", "Mir\u00b7za\u00b7Schaffy", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "KON", "VVFIN", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ein Leuchtstern ist sein Herz, ein Garten seine Brust,", "tokens": ["Ein", "Leuchts\u00b7tern", "ist", "sein", "Herz", ",", "ein", "Gar\u00b7ten", "sei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo alles gl\u00fcht und duftet von frischer Bl\u00fctenlust.", "tokens": ["Wo", "al\u00b7les", "gl\u00fcht", "und", "duf\u00b7tet", "von", "fri\u00b7scher", "Bl\u00fc\u00b7ten\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und bei des eignen Schaffens unw\u00fcchsiger Gew\u00f6hnung", "tokens": ["Und", "bei", "des", "eig\u00b7nen", "Schaf\u00b7fens", "un\u00b7w\u00fcch\u00b7si\u00b7ger", "Ge\u00b7w\u00f6h\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Vergi\u00dft er auch den Klang, die Formvollendung nicht;", "tokens": ["Ver\u00b7gi\u00dft", "er", "auch", "den", "Klang", ",", "die", "Form\u00b7vol\