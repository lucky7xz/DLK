{"dta.poem.6791": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Derselben W\u00fcrckung nun, was das Gesicht angeht,", "tokens": ["Der\u00b7sel\u00b7ben", "W\u00fcr\u00b7ckung", "nun", ",", "was", "das", "Ge\u00b7sicht", "an\u00b7geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist, da\u00df ihr Grund dem Strahl des Lichtes widersteht,", "tokens": ["Ist", ",", "da\u00df", "ihr", "Grund", "dem", "Strahl", "des", "Lich\u00b7tes", "wi\u00b7der\u00b7steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df dieser Strahl im Punct, woselbst er r\u00fcckwerts springet,", "tokens": ["Da\u00df", "die\u00b7ser", "Strahl", "im", "Punct", ",", "wo\u00b7selbst", "er", "r\u00fcck\u00b7werts", "sprin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "APPRART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denselben Winckel macht, mit dem, der in ihn dringet.", "tokens": ["Den\u00b7sel\u00b7ben", "Win\u00b7ckel", "macht", ",", "mit", "dem", ",", "der", "in", "ihn", "drin\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "$,", "APPR", "ART", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jm platten Spiegel bleibt ein Vorwurff, wie er ist.", "tokens": ["Jm", "plat\u00b7ten", "Spie\u00b7gel", "bleibt", "ein", "Vor\u00b7wurff", ",", "wie", "er", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}