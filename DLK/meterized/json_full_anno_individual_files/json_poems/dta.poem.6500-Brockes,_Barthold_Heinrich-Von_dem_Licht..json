{"dta.poem.6500": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne, das Gestirn, die Flamme, welche brennt,", "tokens": ["Die", "Son\u00b7ne", ",", "das", "Ge\u00b7stirn", ",", "die", "Flam\u00b7me", ",", "wel\u00b7che", "brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind Vorw\u00fcrf\u2019, die man licht und helle nennt.", "tokens": ["Sind", "Vor\u00b7w\u00fcr\u00b7f'", ",", "die", "man", "licht", "und", "hel\u00b7le", "nennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "PIS", "NN", "KON", "ADJA", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es kennet unser Geist die Krafft von ihrem Schein,", "tokens": ["Es", "ken\u00b7net", "un\u00b7ser", "Geist", "die", "Krafft", "von", "ih\u00b7rem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir forschen hier nach dem, was solche w\u00fcrcklich seyn.", "tokens": ["Wir", "for\u00b7schen", "hier", "nach", "dem", ",", "was", "sol\u00b7che", "w\u00fcrck\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "$,", "PRELS", "PIAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Worinn kan sonst die Krafft von einem Stoff bestehen,", "tokens": ["Wo\u00b7rinn", "kan", "sonst", "die", "Krafft", "von", "ei\u00b7nem", "Stoff", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der alles helle macht, durch den wir sehen;", "tokens": ["Der", "al\u00b7les", "hel\u00b7le", "macht", ",", "durch", "den", "wir", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "VVFIN", "$,", "APPR", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als blos in einer leicht- und schnellen Fertigkeit?", "tokens": ["Als", "blos", "in", "ei\u00b7ner", "leicht", "und", "schnel\u00b7len", "Fer\u00b7tig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Man mu\u00df den Vorwurf hier wohl unterscheiden", "tokens": ["Man", "mu\u00df", "den", "Vor\u00b7wurf", "hier", "wohl", "un\u00b7ter\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Vom Eindruck, den wir durch ihn leiden.", "tokens": ["Vom", "Ein\u00b7druck", ",", "den", "wir", "durch", "ihn", "lei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}