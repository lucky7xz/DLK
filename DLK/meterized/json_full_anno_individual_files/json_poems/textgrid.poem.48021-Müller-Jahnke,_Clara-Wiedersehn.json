{"textgrid.poem.48021": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Wiedersehn", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aufblitzen im goldigen Sonnenstrahl", "tokens": ["Auf\u00b7blit\u00b7zen", "im", "gol\u00b7di\u00b7gen", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Millionen glitzernder Sterne \u2013", "tokens": ["Mil\u00b7lion\u00b7en", "glit\u00b7zern\u00b7der", "Ster\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "durch schneeige Fl\u00e4chen braust der Zug", "tokens": ["durch", "schne\u00b7ei\u00b7ge", "Fl\u00e4\u00b7chen", "braust", "der", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "herbei aus d\u00e4mmernder Ferne.", "tokens": ["her\u00b7bei", "aus", "d\u00e4m\u00b7mern\u00b7der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er keucht und st\u00f6hnt \u2013 und es gellt ein Pfiff,", "tokens": ["Er", "keucht", "und", "st\u00f6hnt", "\u2013", "und", "es", "gellt", "ein", "Pfiff", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$(", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ein hastiges Dr\u00e4