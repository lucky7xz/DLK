{"textgrid.poem.25324": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Marsyas sang.", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Marsyas sang.", "tokens": ["Mar\u00b7syas", "sang", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Erst war es nur ein fl\u00fcchtig Lied", "tokens": ["Erst", "war", "es", "nur", "ein", "fl\u00fcch\u00b7tig", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wie Windeshauch der weich das Laub durchzieht", "tokens": ["wie", "Win\u00b7des\u00b7hauch", "der", "weich", "das", "Laub", "durch\u00b7zieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wie Tropfenrieseln", "tokens": ["wie", "Trop\u00b7fen\u00b7rie\u00b7seln"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "wie ein Bach der unter Kr\u00e4utern rinnt", "tokens": ["wie", "ein", "Bach", "der", "un\u00b7ter", "Kr\u00e4u\u00b7tern", "rinnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "wie Regen dann und Wolkenbruch und Wind", "tokens": ["wie", "Re\u00b7gen", "dann", "und", "Wol\u00b7ken\u00b7bruch", "und", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADV", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "dann wie der Sturm dann wie das wilde Meer \u2013", "tokens": ["dann", "wie", "der", "Sturm", "dann", "wie", "das", "wil\u00b7de", "Meer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADV", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "dann Schweigen ... heller wieder schwebt daher", "tokens": ["dann", "Schwei\u00b7gen", "...", "hel\u00b7ler", "wie\u00b7der", "schwebt", "da\u00b7her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$(", "ADJD", "ADV", "VVFIN", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "zu unserm Ohr zitternd der Fl\u00f6te Klang", "tokens": ["zu", "un\u00b7serm", "Ohr", "zit\u00b7ternd", "der", "Fl\u00f6\u00b7te", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "ART", "NN", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.10": {"text": "wie Fichtens\u00e4useln wie ein Immensang ...", "tokens": ["wie", "Fich\u00b7ten\u00b7s\u00e4u\u00b7seln", "wie", "ein", "Im\u00b7men\u00b7sang", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und wie er tr\u00e4umend in den Abend bl\u00e4st sein Lied", "tokens": ["Und", "wie", "er", "tr\u00e4u\u00b7mend", "in", "den", "A\u00b7bend", "bl\u00e4st", "sein", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "erlischt die Sonne hinter Moor und Ried.", "tokens": ["er\u00b7lischt", "die", "Son\u00b7ne", "hin\u00b7ter", "Moor", "und", "Ried", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Starr stand Apollo und das Licht zerging", "tokens": ["Starr", "stand", "A\u00b7pol\u00b7lo", "und", "das", "Licht", "zer\u00b7ging"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "KON", "ART", "NN", "VVFIN"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.14": {"text": "um seinen Leib und d\u00fcstrer Schatten hing", "tokens": ["um", "sei\u00b7nen", "Leib", "und", "d\u00fcst\u00b7rer", "Schat\u00b7ten", "hing"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "sich um ihn tief. Und pl\u00f6tzlich schien er ganz", "tokens": ["sich", "um", "ihn", "tief", ".", "Und", "pl\u00f6tz\u00b7lich", "schien", "er", "ganz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPER", "ADJD", "$.", "KON", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "von Nacht umronnen.", "tokens": ["von", "Nacht", "um\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Doch Marsyas vom letzten Glast umsponnen", "tokens": ["Doch", "Mar\u00b7syas", "vom", "letz\u00b7ten", "Glast", "um\u00b7spon\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "der Sonne die sein Antlitz purpurn \u00fcberflo\u00