{"textgrid.poem.47244": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Himmelschl\u00fcsselchen ist genannt ein goldnes", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Himmelschl\u00fcsselchen ist genannt ein goldnes", "tokens": ["Him\u00b7mel\u00b7schl\u00fcs\u00b7sel\u00b7chen", "ist", "ge\u00b7nannt", "ein", "gold\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP", "ART", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Feingebildetes Bl\u00fcmchen auf der Wiese,", "tokens": ["Fein\u00b7ge\u00b7bil\u00b7de\u00b7tes", "Bl\u00fcm\u00b7chen", "auf", "der", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Weil den Himmel auf Erden sieht die Unschuld", "tokens": ["Weil", "den", "Him\u00b7mel", "auf", "Er\u00b7den", "sieht", "die", "Un\u00b7schuld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aufgeschlossen im Fr\u00fchling unter Blumen.", "tokens": ["Auf\u00b7ge\u00b7schlos\u00b7sen", "im", "Fr\u00fch\u00b7ling", "un\u00b7ter", "Blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "\u00bbhimmelschl\u00fcsselchen nenn' ich\u00ab, sprach ein J\u00fcngling,", "tokens": ["\u00bb", "him\u00b7mel\u00b7schl\u00fcs\u00b7sel\u00b7chen", "nenn'", "ich", "\u00ab", ",", "sprach", "ein", "J\u00fcng\u00b7ling", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VVFIN", "PPER", "$(", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "\u00bbdich mit eigenem Rechte, weil ein Himmel", "tokens": ["\u00bb", "dich", "mit", "ei\u00b7ge\u00b7nem", "Rech\u00b7te", ",", "weil", "ein", "Him\u00b7mel"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "APPR", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Mir auf Erden, ein Herz, sich aufgeschlossen,", "tokens": ["Mir", "auf", "Er\u00b7den", ",", "ein", "Herz", ",", "sich", "auf\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "ART", "NN", "$,", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein geliebtes, im Fr\u00fchling, als zum ersten", "tokens": ["Ein", "ge\u00b7lieb\u00b7tes", ",", "im", "Fr\u00fch\u00b7ling", ",", "als", "zum", "ers\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "APPRART", "NN", "$,", "KOUS", "APPRART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Kranz ich sch\u00fcchtern dich wand mit andern Blumen.", "tokens": ["Kranz", "ich", "sch\u00fcch\u00b7tern", "dich", "wand", "mit", "an\u00b7dern", "Blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Himmelschl\u00fcsselchen! den mir aufgeschlossnen", "tokens": ["Him\u00b7mel\u00b