{"textgrid.poem.41279": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Gothilas", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der stolze Gothilas, ein neugedruckter Dichter,", "tokens": ["Der", "stol\u00b7ze", "Go\u00b7thi\u00b7las", ",", "ein", "neu\u00b7ge\u00b7druck\u00b7ter", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Geist von starker Zeugungskraft,", "tokens": ["Ein", "Geist", "von", "star\u00b7ker", "Zeu\u00b7gungs\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fand, seiner Einsicht nach, den Glauben fehlerhaft,", "tokens": ["Fand", ",", "sei\u00b7ner", "Ein\u00b7sicht", "nach", ",", "den", "Glau\u00b7ben", "feh\u00b7ler\u00b7haft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "PTKVZ", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ward des Christenthums unb\u00e4rt'ger Winkelrichter.", "tokens": ["Und", "ward", "des", "Chris\u00b7ten\u00b7thums", "un\u00b7b\u00e4rt'\u00b7ger", "Win\u00b7kel\u00b7rich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er qu\u00e4lte sein Gehirn, die Werkstatt fr\u00fcher List,", "tokens": ["Er", "qu\u00e4l\u00b7te", "sein", "Ge\u00b7hirn", ",", "die", "Werk\u00b7statt", "fr\u00fc\u00b7her", "List", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dir, o Spinoza, nachzu\u00e4ffen:", "tokens": ["Dir", ",", "o", "Spi\u00b7no\u00b7za", ",", "nach\u00b7zu\u00b7\u00e4f\u00b7fen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "FM", "FM", "$,", "VVIZU", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Als ein unsterblicher Deist,", "tokens": ["Als", "ein", "uns\u00b7terb\u00b7li\u00b7cher", "De\u00b7ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der kleinen Ketzer Schwarm dereinst zu \u00fcbertreffen!", "tokens": ["Der", "klei\u00b7nen", "Ket\u00b7zer", "Schwarm", "de\u00b7reinst", "zu", "\u00fc\u00b7bert\u00b7ref\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dies Kl\u00fcgeln ward sein liebster Zeitvertreib;", "tokens": ["Dies", "Kl\u00fc\u00b7geln", "ward", "sein", "liebs\u00b7ter", "Zeit\u00b7ver\u00b7treib", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Doch, da er lange g'nug dem Himmel Hohn gesprochen,", "tokens": ["Doch", ",", "da", "er", "lan\u00b7ge", "g'\u00b7nug", "dem", "Him\u00b7mel", "Hohn", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "wor