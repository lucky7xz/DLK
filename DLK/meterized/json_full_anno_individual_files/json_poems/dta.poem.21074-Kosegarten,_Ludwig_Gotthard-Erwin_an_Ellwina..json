{"dta.poem.21074": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Erwin an Ellwina.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich denk' an dich und holde Fantasieen,               ", "tokens": ["Ich", "denk'", "an", "dich", "und", "hol\u00b7de", "Fan\u00b7ta\u00b7si\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und rosenfarbne Tr\u00e4ume schmeicheln mir.", "tokens": ["Und", "ro\u00b7sen\u00b7farb\u00b7ne", "Tr\u00e4u\u00b7me", "schmei\u00b7cheln", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein liebelechzend Herz zerschmilzt in Elegieen,", "tokens": ["Mein", "lie\u00b7be\u00b7lech\u00b7zend", "Herz", "zer\u00b7schmilzt", "in", "E\u00b7le\u00b7gi\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und jede Fiber t\u00f6nt von dir,", "tokens": ["Und", "je\u00b7de", "Fi\u00b7ber", "t\u00f6nt", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und jeder Muskel zuckt, dich zu erstreben,", "tokens": ["Und", "je\u00b7der", "Mus\u00b7kel", "zuckt", ",", "dich", "zu", "er\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sehnsucht Sturmwind fasst mich stark und wild,", "tokens": ["Der", "Sehn\u00b7sucht", "Sturm\u00b7wind", "fasst", "mich", "stark", "und", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sch\u00fcttelt meinen Bau, dass seine Pfeiler beben,", "tokens": ["Und", "sch\u00fct\u00b7telt", "mei\u00b7nen", "Bau", ",", "dass", "sei\u00b7ne", "Pfei\u00b7ler", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und meiner Kr\u00e4fte Fluth erschwillt.", "tokens": ["Und", "mei\u00b7ner", "Kr\u00e4f\u00b7te", "Fluth", "er\u00b7schwillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Getragen von dem Fittig der Gedanken,", "tokens": ["Ge\u00b7tra\u00b7gen", "von", "dem", "Fit\u00b7tig", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gehoben von der Welle der Begier,", "tokens": ["Ge\u00b7ho\u00b7ben", "von", "der", "Wel\u00b7le", "der", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entschwingt der freye Geist sich k\u00fchn des Raumes", "tokens": ["Ent\u00b7schwingt", "der", "frey\u00b7e", "Geist", "sich", "k\u00fchn", "des", "Rau\u00b7mes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PRF", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schranken,", "tokens": ["Schran\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und pflegt Vertraulichkeit mit dir.", "tokens": ["Und", "pflegt", "Ver\u00b7trau\u00b7lich\u00b7keit", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch ach, wenn ich nun wirklich zu dir", "tokens": ["Doch", "ach", ",", "wenn", "ich", "nun", "wirk\u00b7lich", "zu", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "KOUS", "PPER", "ADV", "ADJD", "APPR", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "fliege,", "tokens": ["flie\u00b7ge", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "So \u00fcberw\u00e4ltigt mich geheimes Graun.", "tokens": ["So", "\u00fc\u00b7berw\u00b7\u00e4l\u00b7tigt", "mich", "ge\u00b7hei\u00b7mes", "Graun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Bl\u00f6de wagt es nicht, die seelenvollen Z\u00fcge,", "tokens": ["Der", "Bl\u00f6\u00b7de", "wagt", "es", "nicht", ",", "die", "see\u00b7len\u00b7vol\u00b7len", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das klare Antlitz anzuschaun.", "tokens": ["Das", "kla\u00b7re", "Ant\u00b7litz", "an\u00b7zu\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mein Blick bebt bange vor dem deinen nieder.", "tokens": ["Mein", "Blick", "bebt", "ban\u00b7ge", "vor", "dem", "dei\u00b7nen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "PPOSAT", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ferne steh' ich tr\u00e4umend. Jedes Wort,", "tokens": ["Von", "fer\u00b7ne", "steh'", "ich", "tr\u00e4u\u00b7mend", ".", "Je\u00b7des", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "VVPP", "$.", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das dir ents\u00e4uselt, klingt aus meinem Innern", "tokens": ["Das", "dir", "ent\u00b7s\u00e4u\u00b7selt", ",", "klingt", "aus", "mei\u00b7nem", "In\u00b7nern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wieder,", "tokens": ["wie\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ein liebelispelnder Accord.", "tokens": ["Ein", "lie\u00b7be\u00b7lis\u00b7peln\u00b7der", "Ac\u00b7cord", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Wenn im Vor\u00fcberfliehn dein Kleid mich streifet,", "tokens": ["Wenn", "im", "Vor\u00b7\u00fc\u00b7berf\u00b7liehn", "dein", "Kleid", "mich", "strei\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein irrend Auge meines blinzelnd fasst,", "tokens": ["Dein", "ir\u00b7rend", "Au\u00b7ge", "mei\u00b7nes", "blin\u00b7zelnd", "fasst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "NN", "PPOSAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein himmelheller Blick den meinigen ergreifet,", "tokens": ["Dein", "him\u00b7mel\u00b7hel\u00b7ler", "Blick", "den", "mei\u00b7ni\u00b7gen", "er\u00b7grei\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "PPOSS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So stockt die Rede. Wechselnd blasst", "tokens": ["So", "stockt", "die", "Re\u00b7de", ".", "Wech\u00b7selnd", "blasst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und feuert mir die Wange. Nebel flirren", "tokens": ["Und", "feu\u00b7ert", "mir", "die", "Wan\u00b7ge", ".", "Ne\u00b7bel", "flir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$.", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor meinen Augen; jeder Umriss schwankt,", "tokens": ["Vor", "mei\u00b7nen", "Au\u00b7gen", ";", "je\u00b7der", "Um\u00b7riss", "schwankt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es schwindelt der Begriff in ausganglosen Irren,", "tokens": ["Es", "schwin\u00b7delt", "der", "Be\u00b7griff", "in", "aus\u00b7gan\u00b7glo\u00b7sen", "Ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und rings die Feste rollt und wankt.", "tokens": ["Und", "rings", "die", "Fes\u00b7te", "rollt", "und", "wankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch, wenn du schonend deine Macht ge-", "tokens": ["Doch", ",", "wenn", "du", "scho\u00b7nend", "dei\u00b7ne", "Macht", "ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "brauchest,", "tokens": ["brau\u00b7chest", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mir mild und g\u00fctig in das Auge blickst,", "tokens": ["Mir", "mild", "und", "g\u00fc\u00b7tig", "in", "das", "Au\u00b7ge", "blickst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein leises \u201eDenke mein!\u201c mir in die Seele hau-", "tokens": ["Ein", "lei\u00b7ses", "\u201e", "Den\u00b7ke", "mein", "!", "\u201c", "mir", "in", "die", "See\u00b7le", "hau"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$(", "VVIMP", "PPOSAT", "$.", "$(", "PPER", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "chest,", "tokens": ["chest", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Und scheidend mir die H\u00e4nde dr\u00fcckst:", "tokens": ["Und", "schei\u00b7dend", "mir", "die", "H\u00e4n\u00b7de", "dr\u00fcckst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie selig f\u00fchl' ich dann mich, \u00fcberselig;", "tokens": ["Wie", "se\u00b7lig", "f\u00fchl'", "ich", "dann", "mich", ",", "\u00fc\u00b7ber\u00b7se\u00b7lig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "PPER", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Heros, w\u00e4hn' ich, sey ich, sey ein Gott!", "tokens": ["Ein", "He\u00b7ros", ",", "w\u00e4hn'", "ich", ",", "sey", "ich", ",", "sey", "ein", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVIMP", "PPER", "$,", "VAFIN", "PPER", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und blutete f\u00fcr dich mit Freuden, s\u00e4nke fr\u00f6hlich", "tokens": ["Und", "blu\u00b7te\u00b7te", "f\u00fcr", "dich", "mit", "Freu\u00b7den", ",", "s\u00e4n\u00b7ke", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "NN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcr dich, Geliebte, in den Tod!", "tokens": ["F\u00fcr", "dich", ",", "Ge\u00b7lieb\u00b7te", ",", "in", "den", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Dann k\u00f6nnt' ich alles dulden, alles tragen,", "tokens": ["Dann", "k\u00f6nnt'", "ich", "al\u00b7les", "dul\u00b7den", ",", "al\u00b7les", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF", "$,", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Bande Schmach, der Kerker Finsterniss;", "tokens": ["Der", "Ban\u00b7de", "Schmach", ",", "der", "Ker\u00b7ker", "Fins\u00b7ter\u00b7niss", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich trotzet', um f\u00fcr dich mein Alles hinzuwagen,", "tokens": ["Ich", "trot\u00b7zet'", ",", "um", "f\u00fcr", "dich", "mein", "Al\u00b7les", "hin\u00b7zu\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "APPR", "PPER", "PPOSAT", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Parzen und der Nemesis.", "tokens": ["Den", "Par\u00b7zen", "und", "der", "Ne\u00b7me\u00b7sis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dann m\u00f6cht' ich gern die ganze Welt be-", "tokens": ["Dann", "m\u00f6cht'", "ich", "gern", "die", "gan\u00b7ze", "Welt", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "gl\u00fccken,", "tokens": ["gl\u00fc\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Den Todfeind m\u00f6cht' ich br\u00fcderlich umfahn,", "tokens": ["Den", "Tod\u00b7feind", "m\u00f6cht'", "ich", "br\u00fc\u00b7der\u00b7lich", "um\u00b7fahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die starre H\u00f6lle selbst an meinen Busen dr\u00fccken,", "tokens": ["Die", "star\u00b7re", "H\u00f6l\u00b7le", "selbst", "an", "mei\u00b7nen", "Bu\u00b7sen", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und rettend mich dem Orkus nahn.", "tokens": ["Und", "ret\u00b7tend", "mich", "dem", "Or\u00b7kus", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Das Sieb der Danaiden m\u00f6cht' ich f\u00fcllen,", "tokens": ["Da