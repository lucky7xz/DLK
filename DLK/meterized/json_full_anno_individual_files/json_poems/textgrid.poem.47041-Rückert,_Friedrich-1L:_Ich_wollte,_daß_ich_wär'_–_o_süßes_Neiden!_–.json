{"textgrid.poem.47041": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wollte, da\u00df ich w\u00e4r' \u2013 o s\u00fc\u00dfes Neiden! \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollte, da\u00df ich w\u00e4r' \u2013 o s\u00fc\u00dfes Neiden! \u2013", "tokens": ["Ich", "woll\u00b7te", ",", "da\u00df", "ich", "w\u00e4r'", "\u2013", "o", "s\u00fc\u00b7\u00dfes", "Nei\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "VAFIN", "$(", "FM", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Spiegel mit dem blanken Angesichte;", "tokens": ["Dein", "Spie\u00b7gel", "mit", "dem", "blan\u00b7ken", "An\u00b7ge\u00b7sich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So w\u00fcrd' ich doch an deines Auges Lichte", "tokens": ["So", "w\u00fcrd'", "ich", "doch", "an", "dei\u00b7nes", "Au\u00b7ges", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Viel \u00f6fter mich als jetzo k\u00f6nnen weiden.", "tokens": ["Viel", "\u00f6f\u00b7ter", "mich", "als", "jet\u00b7zo", "k\u00f6n\u00b7nen", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "KOKOM", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich wollte, da\u00df ich w\u00e4r' \u2013 o bittres Leiden! \u2013", "tokens": ["Ich", "woll\u00b7te", ",", "da\u00df", "ich", "w\u00e4r'", "\u2013", "o", "bitt\u00b7res", "Lei\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "VAFIN", "$(", "FM", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Schatten, der vor deinem Glanz zu nichte", "tokens": ["Dein", "Schat\u00b7ten", ",", "der", "vor", "dei\u00b7nem", "Glanz", "zu", "nich\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "APPR", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nie wird; so w\u00fcrd' ich, gleich dem dunklen Wichte,", "tokens": ["Nie", "wird", ";", "so", "w\u00fcrd'", "ich", ",", "gleich", "dem", "dunk\u00b7len", "Wich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$.", "ADV", "VAFIN", "PPER", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von deinem Leibe brauchen nie zu scheiden.", "tokens": ["Von", "dei\u00b7nem", "Lei\u00b7be", "brau\u00b7chen", "nie", "zu", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich wollte, da\u00df ich nur dein L\u00e4mmchen w\u00e4re,", "tokens": ["Ich", "woll\u00b7te", ",", "da\u00df", "ich", "nur", "dein", "L\u00e4mm\u00b7chen", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", 