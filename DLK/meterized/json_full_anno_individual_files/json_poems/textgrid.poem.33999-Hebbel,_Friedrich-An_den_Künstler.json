{"textgrid.poem.33999": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An den K\u00fcnstler", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob du auch bilden magst, was unverg\u00e4nglich", "tokens": ["Ob", "du", "auch", "bil\u00b7den", "magst", ",", "was", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch alle Zeiten wandeln soll und gl\u00e4nzen,", "tokens": ["Durch", "al\u00b7le", "Zei\u00b7ten", "wan\u00b7deln", "soll", "und", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "VMFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wird dich die, in der du lebst, nicht kr\u00e4nzen,", "tokens": ["Doch", "wird", "dich", "die", ",", "in", "der", "du", "lebst", ",", "nicht", "kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie wird dir trotzen, stumpf und unempf\u00e4nglich.", "tokens": ["Sie", "wird", "dir", "trot\u00b7zen", ",", "stumpf", "und", "un\u00b7emp\u00b7f\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Menschheit, schon an sich so unzul\u00e4nglich,", "tokens": ["Die", "Menschheit", ",", "schon", "an", "sich", "so", "un\u00b7zu\u00b7l\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kann sich in ihren enggesteckten Gr\u00e4nzen", "tokens": ["Kann", "sich", "in", "ih\u00b7ren", "eng\u00b7ge\u00b7steck\u00b7ten", "Gr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht einmal aus dem Zauberquell erg\u00e4nzen,", "tokens": ["Nicht", "ein\u00b7mal", "aus", "dem", "Zau\u00b7ber\u00b7quell", "er\u00b7g\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der aus ihr selbst hervor bricht, \u00fcberschw\u00e4nglich.", "tokens": ["Der", "aus", "ihr", "selbst", "her\u00b7vor", "bricht", ",", "\u00fc\u00b7bersc\u00b7hw\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADV", "PTKVZ", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Beklage es, doch einzig ihrethalben,", "tokens": ["Be\u00b7kla\u00b7ge", "es", ",", "doch", "ein\u00b7zig", "ih\u00b7re\u00b7thal\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mit dem Nicht-Genie\u00dfen dies Verkennen", "tokens": ["Die", "mit", "dem", "Nicht\u00b7Ge\u00b7ni\u00b7e\u00b7\u00dfen", "dies", "Ver\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PDS", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Zu theuer b\u00fc\u00dft, und nimmer deinetwegen;", "tokens": ["Zu", "theu\u00b7er", "b\u00fc\u00dft", ",", "und", "nim\u00b7mer", "dei\u00b7net\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "$,", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn, wollte sie dich gleich zum K\u00f6nig salben,", "tokens": ["Denn", ",", "woll\u00b7te", "sie", "dich", "gleich", "zum", "K\u00f6\u00b7nig", "sal\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So w\u00fcrden dich die Zweifel nicht mehr brennen,", "tokens": ["So", "w\u00fcr\u00b7den", "dich", "die", "Zwei\u00b7fel", "nicht", "mehr", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch die du zahlst f\u00fcr aller G\u00f6tter Segen!", "tokens": ["Durch", "die", "du", "zahlst", "f\u00fcr", "al\u00b7ler", "G\u00f6t\u00b7ter", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ob du auch bilden magst, was unverg\u00e4nglich", "tokens": ["Ob", "du", "auch",