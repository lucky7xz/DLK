], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Republik, die Republik!", "tokens": ["Die", "Re\u00b7pub\u00b7lik", ",", "die", "Re\u00b7pub\u00b7lik", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ankeuchten die Berichte:", "tokens": ["An\u00b7keuch\u00b7ten", "die", "Be\u00b7rich\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Atmezug, ein Wink, ein Blick,", "tokens": ["Ein", "At\u00b7me\u00b7zug", ",", "ein", "Wink", ",", "ein", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Handumdrehn \u2013 die Republik!", "tokens": ["Ein", "Han\u00b7dum\u00b7drehn", "\u2013", "die", "Re\u00b7pub\u00b7lik", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So dichtet die Geschichte!", "tokens": ["So", "dich\u00b7tet", "die", "Ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Republik, die Republik!", "tokens": ["Die", "Re\u00b7pub\u00b7lik", ",", "die", "Re\u00b7pub\u00b7lik", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Republik,