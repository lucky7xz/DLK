{"dta.poem.2118": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nothwendigkeit Gottes Werke  \n zu betrachten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6n gef\u00e4rbten Blumen bl\u00fchn,", "tokens": ["Die", "sch\u00f6n", "ge\u00b7f\u00e4rb\u00b7ten", "Blu\u00b7men", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kraut, W\u00e4lder, Gras, und Felder gr\u00fcnen.", "tokens": ["Kraut", ",", "W\u00e4l\u00b7der", ",", "Gras", ",", "und", "Fel\u00b7der", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie nun, Menschen zu vergn\u00fcgen, und, in der Lust,", "tokens": ["Wenn", "sie", "nun", ",", "Men\u00b7schen", "zu", "ver\u00b7gn\u00fc\u00b7gen", ",", "und", ",", "in", "der", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "NN", "PTKZU", "VVINF", "$,", "KON", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "zu Gott zu ziehn", "tokens": ["zu", "Gott", "zu", "ziehn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Durch fr\u00f6liche Bewunderung und durch Erkenntlichkeit,", "tokens": ["Durch", "fr\u00f6\u00b7li\u00b7che", "Be\u00b7wun\u00b7de\u00b7rung", "und", "durch", "Er\u00b7kennt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "nicht dienen;", "tokens": ["nicht", "die\u00b7nen", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "So saget mir, ihr Menschen, doch: f\u00fcr wen sie bl\u00fchn?", "tokens": ["So", "sa\u00b7get", "mir", ",", "ihr", "Men\u00b7schen", ",", "doch", ":", "f\u00fcr", "wen", "sie", "bl\u00fchn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "$.", "APPR", "PWS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "f\u00fcr wen sie gr\u00fcnen?", "tokens": ["f\u00fcr", "wen", "sie", "gr\u00fc\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}