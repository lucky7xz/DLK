{"textgrid.poem.63640": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatt einmal gar treffliche Talente:", "tokens": ["Ich", "hatt", "ein\u00b7mal", "gar", "treff\u00b7li\u00b7che", "Ta\u00b7len\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Goldsterne schnitzeln und die Lichter z\u00fcnden", "tokens": ["Gold\u00b7ster\u00b7ne", "schnit\u00b7zeln", "und", "die", "Lich\u00b7ter", "z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Weihnachtsbaum und mit der Glocke k\u00fcnden,", "tokens": ["Am", "Weih\u00b7nachts\u00b7baum", "und", "mit", "der", "Glo\u00b7cke", "k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df man die T\u00fcr nun endlich st\u00fcrmen k\u00f6nnte.", "tokens": ["Da\u00df", "man", "die", "T\u00fcr", "nun", "end\u00b7lich", "st\u00fcr\u00b7men", "k\u00f6nn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich wu\u00dft' auch, wie man Festungen berennte,", "tokens": ["Ich", "wu\u00dft'", "auch", ",", "wie", "man", "Fes\u00b7tun\u00b7gen", "be\u00b7renn\u00b7te", ","], "token_info": ["