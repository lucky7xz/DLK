{"textgrid.poem.36478": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An des Prinzen Friederichs von Preu\u00dfen K\u00f6nigliche Hoheit", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indes, da\u00df ", "tokens": ["In\u00b7des", ",", "da\u00df"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Nachdem der Feinde Stolz den Frieden ihm versagt,", "tokens": ["Nach\u00b7dem", "der", "Fein\u00b7de", "Stolz", "den", "Frie\u00b7den", "ihm", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und in gerechtem Krieg, (denn andern f\u00fchrt er nicht,)", "tokens": ["Und", "in", "ge\u00b7rech\u00b7tem", "Krieg", ",", "(", "denn", "an\u00b7dern", "f\u00fchrt", "er", "nicht", ",", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "$(", "KON", "PIS", "VVFIN", "PPER", "PTKNEG", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Ihm der Patriot, Dein gro\u00dfer Vater, ficht,", "tokens": ["Mit", "Ihm", "der", "Pat\u00b7ri\u00b7ot", ",", "Dein", "gro\u00b7\u00dfer", "Va\u00b7ter", ",", "ficht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und schl\u00e4gt und \u00fcberwindet;", "tokens": ["Und", "schl\u00e4gt", "und", "\u00fc\u00b7berw\u00b7in\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Indes, o ", "tokens": ["In\u00b7des", ",", "o"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "FM"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Der Weisheit und der Wahrheit zu,", "tokens": ["Der", "Weis\u00b7heit", "und", "der", "Wahr\u00b7heit", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die man nicht oft bei Hofe findet.", "tokens": ["Die", "man", "nicht", "oft", "bei", "Ho\u00b7fe", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Du pr\u00e4gst Dir ihre Lehren ein,", "tokens": ["Du", "pr\u00e4gst", "Dir", "ih\u00b7re", "Leh\u00b7ren", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bedenkst, wie schwer es ist, ein Landesvater sein!", "tokens": ["Be\u00b7denkst", ",", "wie", "schwer", "es", "ist", ",", "ein", "Lan\u00b7des\u00b7va\u00b7ter", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein Mentor,", "tokens": ["Dein", "Men\u00b7tor", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Rei\u00dft, ein geschworner Feind der niedern Schmeichelei,", "tokens": ["Rei\u00dft", ",", "ein", "ge\u00b7schwor\u00b7ner", "Feind", "der", "nie\u00b7dern", "Schmei\u00b7che\u00b7lei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Herrn der Erde, frei,", "tokens": ["Den", "Herrn", "der", "Er\u00b7de", ",", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Larve vom Gesichte,", "tokens": ["Die", "Lar\u00b7ve", "vom", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sagt Dir bei jeglichem, was er gewesen sei:", "tokens": ["Sagt", "Dir", "bei", "jeg\u00b7li\u00b7chem", ",", "was", "er", "ge\u00b7we\u00b7sen", "sei", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "$,", "PWS", "PPER", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Domitian, ein Feind der Tugend und des Rechts!", "tokens": ["Do\u00b7mi\u00b7ti\u00b7an", ",", "ein", "Feind", "der", "Tu\u00b7gend", "und", "des", "Rechts", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Aurel, die Lust des menschlichen Geschlechts!", "tokens": ["Au\u00b7rel", ",", "die", "Lust", "des", "menschli\u00b7chen", "Ge\u00b7schlechts", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Wenn er die F\u00fcrsten straft, und ihrer Tyrannei", "tokens": ["Wenn", "er", "die", "F\u00fcrs\u00b7ten", "straft", ",", "und", "ih\u00b7rer", "Ty\u00b7ran\u00b7nei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wohlverdienten Namen giebt,", "tokens": ["Die", "wohl\u00b7ver\u00b7dien\u00b7ten", "Na\u00b7men", "giebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wirst Du zornig, sagst, ", "tokens": ["Dann", "wirst", "Du", "zor\u00b7nig", ",", "sagst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Dein Exempel ist, und willst dann so geschwind,", "tokens": ["Der", "Dein", "Ex\u00b7em\u00b7pel", "ist", ",", "und", "willst", "dann", "so", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$,", "KON", "VMFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie Er zu Felde geht, und eine Schlacht gewinnt,", "tokens": ["Wie", "Er", "zu", "Fel\u00b7de", "geht", ",", "und", "ei\u00b7ne", "Schlacht", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Landesv\u00e4ter alle kennen,", "tokens": ["Die", "Lan\u00b7des\u00b7v\u00e4\u00b7ter", "al\u00b7le", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Deines Hauses Ehre sind!", "tokens": ["Die", "Dei\u00b7nes", "Hau\u00b7ses", "Eh\u00b7re", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein Mentor bittet Dich, mit ihm zu gehn,", "tokens": ["Dein", "Men\u00b7tor", "bit\u00b7tet", "Dich", ",", "mit", "ihm", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4\u00dft sie Dich in einem Tempel sehn,", "tokens": ["Und", "l\u00e4\u00dft", "sie", "Dich", "in", "ei\u00b7nem", "Tem\u00b7pel", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In welchem ", "tokens": ["In", "wel\u00b7chem"], "token_info": ["word", "word"], "pos": ["APPR", "PWAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Mit welcher Er das Vaterland", "tokens": ["Mit", "wel\u00b7cher", "Er", "das", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verteidigt, Er, der sieggewohnte Held,", "tokens": ["Ver\u00b7tei\u00b7digt", ",", "Er", ",", "der", "sieg\u00b7ge\u00b7wohn\u00b7te", "Held", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie hat den K\u00f6nigen zum Muster aufgestellt!", "tokens": ["Sie", "hat", "den", "K\u00f6\u00b7ni\u00b7gen", "zum", "Mus\u00b7ter", "auf\u00b7ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Darf ich, o ", "tokens": ["Darf", "ich", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "$,", "FM"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Den g\u00f6ttlichen Gedanken nachzusagen,", "tokens": ["Den", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken", "nach\u00b7zu\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Du in diesem Tempel denkst?", "tokens": ["Den", "Du", "in", "die\u00b7sem", "Tem\u00b7pel", "denkst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du stehst vor ihren Bildern, lenkst", "tokens": ["Du", "stehst", "vor", "ih\u00b7ren", "Bil\u00b7dern", ",", "lenkst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Blick auf ihre gro\u00dfen Namen,", "tokens": ["Den", "Blick", "auf", "ih\u00b7re", "gro\u00b7\u00dfen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denkst Deine gro\u00dfe Pflicht,", "tokens": ["Denkst", "Dei\u00b7ne", "gro\u00b7\u00dfe", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den Helden allen nachzuahmen,", "tokens": ["Den", "Hel\u00b7den", "al\u00b7len", "nach\u00b7zu\u00b7ah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Willen im Gesicht!", "tokens": ["Den", "Wil\u00b7len", "im", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ach! h\u00e4tten alle Menschen solchen Willen,", "tokens": ["Ach", "!", "h\u00e4t\u00b7ten", "al\u00b7le", "Men\u00b7schen", "sol\u00b7chen", "Wil\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PIAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann d\u00fcrften wir die Wahrheit nicht verh\u00fcllen;", "tokens": ["Dann", "d\u00fcrf\u00b7ten", "wir", "die", "Wahr\u00b7heit", "nicht", "ver\u00b7h\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir brauchten keine lehrende Gedich