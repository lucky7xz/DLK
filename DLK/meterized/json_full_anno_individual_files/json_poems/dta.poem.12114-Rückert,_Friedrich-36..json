{"dta.poem.12114": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Regen geht herab in Str\u00f6men, landerquickend;", "tokens": ["Der", "Re\u00b7gen", "geht", "her\u00b7ab", "in", "Str\u00f6\u00b7men", ",", "lan\u00b7der\u00b7qui\u00b7ckend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie oft erflehtest du daheim ihn, aufwertsblickend!", "tokens": ["Wie", "oft", "er\u00b7fleh\u00b7test", "du", "da\u00b7heim", "ihn", ",", "auf\u00b7werts\u00b7bli\u00b7ckend", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADV", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im fremden Lande nun verw\u00fcnschest du den Segen,", "tokens": ["Im", "frem\u00b7den", "Lan\u00b7de", "nun", "ver\u00b7w\u00fcn\u00b7schest", "du", "den", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er dem Wanderer zum Koth wird auf den Wegen.", "tokens": ["Weil", "er", "dem", "Wan\u00b7de\u00b7rer", "zum", "Koth", "wird", "auf", "den", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du hast f\u00fcr die Natur und alle Kreatur", "tokens": ["Du", "hast", "f\u00fcr", "die", "Na\u00b7tur", "und", "al\u00b7le", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein menschlich Mitgef\u00fchl in deinem Kreise nur.", "tokens": ["Ein", "menschlich", "Mit\u00b7ge\u00b7f\u00fchl", "in", "dei\u00b7nem", "Krei\u00b7se", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}