{"dta.poem.8340": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken \u00fcber einem Weinstok  \n mit gereifften Trauben zur  \n Herbstszeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was seh ich da an Garten W\u00e4nden", "tokens": ["Was", "seh", "ich", "da", "an", "Gar\u00b7ten", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor strahlende Tapecerei?", "tokens": ["Vor", "strah\u00b7len\u00b7de", "Ta\u00b7pe\u00b7ce\u00b7rei", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mir deucht, da\u00df sie von denen H\u00e4n-", "tokens": ["Mir", "deucht", ",", "da\u00df", "sie", "von", "de\u00b7nen", "H\u00e4n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PRELS", "TRUNC"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Des Sch\u00f6pfers, auch gewebet sey.", "tokens": ["Des", "Sch\u00f6p\u00b7fers", ",", "auch", "ge\u00b7we\u00b7bet", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein d\u00fcrrer Stock voll schlanker Reben,", "tokens": ["Ein", "d\u00fcr\u00b7rer", "Stock", "voll", "schlan\u00b7ker", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kan solche Augenweide geben,", "tokens": ["Kan", "sol\u00b7che", "Au\u00b7gen\u00b7wei\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie ist das nicht erquiklich sch\u00f6n?", "tokens": ["Wie", "ist", "das", "nicht", "er\u00b7quik\u00b7lich", "sch\u00f6n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PDS", "PTKNEG", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ach! la\u00df mich HErr zu deinem Ruhme,", "tokens": ["Ach", "!", "la\u00df", "mich", "Herr", "zu", "dei\u00b7nem", "Ruh\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auch in der G\u00e4rten Heiligthume,", "tokens": ["Auch", "in", "der", "G\u00e4r\u00b7ten", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des Weinstocks Lust und Schmuck besehn.", "tokens": ["Des", "Wein\u00b7stocks", "Lust", "und", "Schmuck", "be\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Anblik zeuget die Gedanken:", "tokens": ["Der", "An\u00b7blik", "zeu\u00b7get", "die", "Ge\u00b7dan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein d\u00fcrrer Stamm bringt solche Pracht,", "tokens": ["Ein", "d\u00fcr\u00b7rer", "Stamm", "bringt", "sol\u00b7che", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er mit den geschlungnen Ranken,", "tokens": ["Wenn", "er", "mit", "den", "ge\u00b7schlung\u00b7nen", "Ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die G\u00e4rtenw\u00e4nde gr\u00fcnend macht.", "tokens": ["Die", "G\u00e4r\u00b7ten\u00b7w\u00e4n\u00b7de", "gr\u00fc\u00b7nend", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kunst mu\u00df sie in Ordnung binden,", "tokens": ["Die", "Kunst", "mu\u00df", "sie", "in", "Ord\u00b7nung", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sie sich in die H\u00f6he winden,", "tokens": ["Wie", "sie", "sich", "in", "die", "H\u00f6\u00b7he", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So sinds Tapeten der Natur", "tokens": ["So", "sinds", "Ta\u00b7pe\u00b7ten", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die mit den durchgeflochtnen Zweigen,", "tokens": ["Die", "mit", "den", "durch\u00b7ge\u00b7flocht\u00b7nen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "In Bl\u00e4tterreicher Anmuth zeigen,", "tokens": ["In", "Bl\u00e4t\u00b7ter\u00b7rei\u00b7cher", "An\u00b7muth", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Manch Herzerfreuliche Figur.", "tokens": ["Manch", "Her\u00b7zer\u00b7freu\u00b7li\u00b7che", "Fi\u00b7gur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jhr die ihr eurer Wohnung Zimmer,", "tokens": ["Ihr", "die", "ihr", "eu\u00b7rer", "Woh\u00b7nung", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit solchen Dekken ausgelegt,", "tokens": ["Mit", "sol\u00b7chen", "Dek\u00b7ken", "aus\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Woraus ein Strahlenreicher Schimmer,", "tokens": ["Wo\u00b7raus", "ein", "Strah\u00b7len\u00b7rei\u00b7cher", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In unsrer Augen Spiegel schl\u00e4gt.", "tokens": ["In", "uns\u00b7rer", "Au\u00b7gen", "Spie\u00b7gel", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr m\u00f6gt mit Stikwerk, Mahlereien,", "tokens": ["Ihr", "m\u00f6gt", "mit", "Stik\u00b7werk", ",", "Mah\u00b7le\u00b7rei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Anblik suchen zu erfreuen,", "tokens": ["Den", "An\u00b7blik", "su\u00b7chen", "zu", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die W\u00e4nde mit den Bildern ziern,", "tokens": ["Die", "W\u00e4n\u00b7de", "mit", "den", "Bil\u00b7dern", "zi\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es mu\u00df doch das, was k\u00fcnstlich gl\u00e4nzet,", "tokens": ["Es", "mu\u00df", "doch", "das", ",", "was", "k\u00fcnst\u00b7lich", "gl\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PDS", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vor dem, was die Natur bekr\u00e4nzet,", "tokens": ["Vor", "dem", ",", "was", "die", "Na\u00b7tur", "be\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Pracht der Farben bald verliehrn.", "tokens": ["Die", "Pracht", "der", "Far\u00b7ben", "bald", "ver\u00b7liehrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Seht an, den Weinstok mit dem Laube,", "tokens": ["Seht", "an", ",", "den", "Weins\u00b7tok", "mit", "dem", "Lau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie er sich um die Pf\u00e4le schlingt,", "tokens": ["Wie", "er", "sich", "um", "die", "Pf\u00e4\u00b7le", "schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie dadurch die Purpurtraube", "tokens": ["Und", "wie", "da\u00b7durch", "die", "Pur\u00b7pur\u00b7trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In sanfftgemischten Glanze dringt.", "tokens": ["In", "sanfft\u00b7ge\u00b7mischten", "Glan\u00b7ze", "dringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hier sind lebendige Figuren,", "tokens": ["Hier", "sind", "le\u00b7ben\u00b7di\u00b7ge", "Fi\u00b7gu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schlankigte gezogne Spuren:", "tokens": ["Und", "schlan\u00b7kig\u00b7te", "ge\u00b7zog\u00b7ne", "Spu\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Rankenwerk nat\u00fcrlich sch\u00f6n,", "tokens": ["Ein", "Ran\u00b7ken\u00b7werk", "na\u00b7t\u00fcr\u00b7lich", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da eines sich durchs andre bieget,", "tokens": ["Da", "ei\u00b7nes", "sich", "durchs", "and\u00b7re", "bie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPRART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein Blat gleichsam auf andren lieget,", "tokens": ["Ein", "Blat", "gleich\u00b7sam", "auf", "an\u00b7dren", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie sich der Reben Gabeln drehn.", "tokens": ["Wie", "sich", "der", "Re\u00b7ben", "Ga\u00b7beln", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jhr schm\u00fckket mit gemahlten Kr\u00e4nzen,", "tokens": ["Ihr", "schm\u00fck\u00b7ket", "mit", "ge\u00b7mahl\u00b7ten", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Majest\u00e4tschen H\u00e4user an,", "tokens": ["Die", "Ma\u00b7jes\u00b7t\u00e4t\u00b7schen", "H\u00e4u\u00b7ser", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo man ein schimmernd g\u00fcldnes Gl\u00e4nzen,", "tokens": ["Wo", "man", "ein", "schim\u00b7mernd", "g\u00fcld\u00b7nes", "Gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was herrliches bewundern kan:", "tokens": ["Was", "herr\u00b7li\u00b7ches", "be\u00b7wun\u00b7dern", "kan", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch die Pracht mu\u00df sich verdunkeln,", "tokens": ["Je\u00b7doch", "die", "Pracht", "mu\u00df", "sich", "ver\u00b7dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bei der erhabnen Trauben Funkeln,", "tokens": ["Bei", "der", "er\u00b7hab\u00b7nen", "Trau\u00b7ben", "Fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die gr\u00fcnlich, roth und Purpurbraun,", "tokens": ["Die", "gr\u00fcn\u00b7lich", ",", "roth", "und", "Pur\u00b7pur\u00b7braun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In sch\u00f6n geformter Ordnung hangen,", "tokens": ["In", "sch\u00f6n", "ge\u00b7form\u00b7ter", "Ord\u00b7nung", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da aller Edelsteine Prangen,", "tokens": ["Da", "al\u00b7ler", "E\u00b7del\u00b7stei\u00b7ne", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In ihren Beerleins anzuschaun.", "tokens": ["In", "ih\u00b7ren", "Be\u00b7er\u00b7leins", "an\u00b7zu\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Wie herrlich sind sie ausgezieret,", "tokens": ["Wie", "herr\u00b7lich", "sind", "sie", "aus\u00b7ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man die Trauben recht erblikt,", "tokens": ["Wenn", "man", "die", "Trau\u00b7ben", "recht", "er\u00b7blikt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wie Cristallen klar poliret,", "tokens": ["Die", "wie", "Cris\u00b7tal\u00b7len", "klar", "po\u00b7li\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gef\u00fcllten Perlen gleich geschm\u00fckt;", "tokens": ["Ge\u00b7f\u00fcll\u00b7ten", "Per\u00b7len", "gleich", "ge\u00b7schm\u00fckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Nectarsafft der darin schwimmet,", "tokens": ["Der", "Nec\u00b7tar\u00b7safft", "der", "da\u00b7rin", "schwim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "p