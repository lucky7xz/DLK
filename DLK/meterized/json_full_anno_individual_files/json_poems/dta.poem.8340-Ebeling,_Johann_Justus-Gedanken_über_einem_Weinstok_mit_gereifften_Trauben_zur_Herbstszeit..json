er", "Gott", "!", "hast", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dis Weinstocks sch\u00f6nen Nectertrank:", "tokens": ["Dis", "Wein\u00b7stocks", "sch\u00f6\u00b7nen", "Nec\u00b7ter\u00b7trank", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Damit wir bei vergn\u00fcgten Leben,", "tokens": ["Da\u00b7mit", "wir", "bei", "ver\u00b7gn\u00fcg\u00b7ten", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir opferten Lob, Preis und Dank:", "tokens": ["Dir", "op\u00b7fer\u00b7ten", "Lob", ",", "Preis", "und", "Dank", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Ich sehe jezt bei welken Laube,", "tokens": ["Ich", "se\u00b7he", "jezt", "bei", "wel\u00b7ken", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die durch die Sonn gereiffte Traube,", "tokens": ["Die", "durch", "die", "Sonn", "ge\u00b7reiff\u00b7te", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich schmekke ihre S\u00fcssigkeit,", "tokens": ["Ich", "schmek\u00b7ke", "ih\u00b7re", "S\u00fcs\u00b7sig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und drin mit n\u00fcchternen Gem\u00fcthe,", "tokens": ["Und", "drin", "mit", "n\u00fcch\u00b7ter\u00b7nen", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auch deine grosse Wunderg\u00fcte,", "tokens": ["Auch", "dei\u00b7ne", "gros\u00b7se", "Wun\u00b7der\u00b7g\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die uns den Lebenssafft anbeut.", "tokens": ["Die", "uns", "den", "Le\u00b7bens\u00b7safft", "an\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Der Weinbau ist wie man erfahren,", "tokens": ["Der", "Wein\u00b7bau", "ist", "wie", "man", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "PIS", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "An denen Bergen wol gedeit,", "tokens": ["An", "de\u00b7nen", "Ber\u00b7gen", "wol", "ge\u00b7deit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast dadurch zu vielen Jahren,", "tokens": ["Du", "hast", "da\u00b7durch", "zu", "vie\u00b7len", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "APPR", "PIAT", "NN"