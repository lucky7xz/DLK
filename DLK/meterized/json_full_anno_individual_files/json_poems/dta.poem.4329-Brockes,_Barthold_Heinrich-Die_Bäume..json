{"dta.poem.4329": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die B\u00e4ume.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum ver\u00e4nderst du dein sanft- und mildes Wesen,", "tokens": ["Wa\u00b7rum", "ver\u00b7\u00e4n\u00b7derst", "du", "dein", "sanft", "und", "mil\u00b7des", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geliebte Luft, warum verkehrst du dich?", "tokens": ["Ge\u00b7lieb\u00b7te", "Luft", ",", "wa\u00b7rum", "ver\u00b7kehrst", "du", "dich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Warum ver\u00e4ndert sich", "tokens": ["Wa\u00b7rum", "ver\u00b7\u00e4n\u00b7dert", "sich"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dein S\u00e4useln in ein heftig Sausen,", "tokens": ["Dein", "S\u00e4u\u00b7seln", "in", "ein", "hef\u00b7tig", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Lispeln in ein wildes Brausen?", "tokens": ["Dein", "Lis\u00b7peln", "in", "ein", "wil\u00b7des", "Brau\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Warum wird dein sapphirnes Blau", "tokens": ["Wa\u00b7rum", "wird", "dein", "sap\u00b7phir\u00b7nes", "Blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Jtzt falb und dunkel- grau?", "tokens": ["Jtzt", "falb", "und", "dun\u00b7kel", "grau", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "TRUNC", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wie k\u00f6mmt es, da\u00df du uns entlaubest?", "tokens": ["Wie", "k\u00f6mmt", "es", ",", "da\u00df", "du", "uns", "ent\u00b7lau\u00b7best", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Du raubst dir deinen Schmuck, indem du uns beraubest,", "tokens": ["Du", "raubst", "dir", "dei\u00b7nen", "Schmuck", ",", "in\u00b7dem", "du", "uns", "be\u00b7rau\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da wir ja fast f\u00fcr dich allein", "tokens": ["Da", "wir", "ja", "fast", "f\u00fcr", "dich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So sch\u00f6n geziert gewesen seyn.", "tokens": ["So", "sch\u00f6n", "ge\u00b7ziert", "ge\u00b7we\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dein Reich war durch die gr\u00fcne Pracht,", "tokens": ["Dein", "Reich", "war", "durch", "die", "gr\u00fc\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die wir hervorgebracht,", "tokens": ["Die", "wir", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Ja lustig, angenehm und sch\u00f6n,", "tokens": ["Ja", "lus\u00b7tig", ",", "an\u00b7ge\u00b7nehm", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und sch\u00f6ner fast, als wir selbst, anzusehn.", "tokens": ["Und", "sch\u00f6\u00b7ner", "fast", ",", "als", "wir", "selbst", ",", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "KOUS", "PPER", "ADV", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Besinne dich, nimm uns nicht unser Kleid,", "tokens": ["Be\u00b7sin\u00b7ne", "dich", ",", "nimm", "uns", "nicht", "un\u00b7ser", "Kleid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVIMP", "PPER", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Das dich zugleich geziert, mit solcher Heftigkeit.", "tokens": ["Das", "dich", "zu\u00b7gleich", "ge\u00b7ziert", ",", "mit", "sol\u00b7cher", "Hef\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVPP", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}