{"textgrid.poem.48806": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "31. An die Koppel", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Lustplatz der Natur, von dem als einer Zinnen", "tokens": ["Du", "Lust\u00b7platz", "der", "Na\u00b7tur", ",", "von", "dem", "als", "ei\u00b7ner", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "$,", "APPR", "ART", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sie hier die Dryaden sieht bei den Faunen gehn,", "tokens": ["sie", "hier", "die", "Dry\u00b7a\u00b7den", "sieht", "bei", "den", "Fau\u00b7nen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "da die ger\u00fchmte Stadt in g\u00fcldnen Spitzen stehn,", "tokens": ["da", "die", "ge\u00b7r\u00fchm\u00b7te", "Stadt", "in", "g\u00fcld\u00b7nen", "Spit\u00b7zen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dort ein durchflo\u00dfnes Feld mit vielen s\u00fc\u00dfen Br\u00fcnnen;", "tokens": ["dort", "ein", "durch\u00b7flo\u00df\u00b7nes", "Feld", "mit", "vie\u00b7len", "s\u00fc\u00b7\u00dfen", "Br\u00fcn\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measur