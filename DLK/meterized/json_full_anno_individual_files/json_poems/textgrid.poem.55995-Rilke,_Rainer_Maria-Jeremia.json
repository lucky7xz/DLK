{"textgrid.poem.55995": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Jeremia", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einmal war ich weich wie fr\u00fcher Weizen,", "tokens": ["Ein\u00b7mal", "war", "ich", "weich", "wie", "fr\u00fc\u00b7her", "Wei\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KOKOM", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "doch, du Rasender, du hast vermocht,", "tokens": ["doch", ",", "du", "Ra\u00b7sen\u00b7der", ",", "du", "hast", "ver\u00b7mocht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "mir das hingehaltne Herz zu reizen,", "tokens": ["mir", "das", "hin\u00b7ge\u00b7halt\u00b7ne", "Herz", "zu", "rei\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "da\u00df es jetzt wie eines L\u00f6wen kocht.", "tokens": ["da\u00df", "es", "jetzt", "wie", "ei\u00b7nes", "L\u00f6\u00b7wen", "kocht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Welchen Mund hast du mir zugemutet,", "tokens": ["Wel\u00b7chen", "Mund", "hast", "du", "mir", "zu\u00b7ge\u00b7mu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "damals, da ich fast ein Knabe war:", "tokens": ["da\u00b7mals", ",", "da", "ich", "fast", "ein", "Kna\u00b7be", "war", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "eine Wunde wurde er: nun blutet", "tokens": ["ei\u00b7ne", "Wun\u00b7de", "wur\u00b7de", "er", ":", "nun", "blu\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "aus ihm Ungl\u00fccksjahr um Ungl\u00fccksjahr.", "tokens": ["aus", "ihm", "Un\u00b7gl\u00fccks\u00b7jahr", "um", "Un\u00b7gl\u00fccks\u00b7jahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "T\u00e4glich t\u00f6nte ich von neuen N\u00f6ten,", "tokens": ["T\u00e4g\u00b7lich", "t\u00f6n\u00b7te", "ich", "von", "neu\u00b7en", "N\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "die du, Uners\u00e4ttlicher, ersannst,", "tokens": ["die", "du", ",", "Un\u00b7er\u00b7s\u00e4tt\u00b7li\u00b7cher", ",", "er\u00b7sannst", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und sie konnten mir den Mund nicht t\u00f6ten;", "tokens": ["und", "sie", "konn\u00b7ten", "mir", "den", "Mund", "nicht", "t\u00f6\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "sieh du zu, wie du ihn stillen kannst,", "tokens": ["sieh", "du", "zu", ",", "wie", "du", "ihn", "stil\u00b7len", "kannst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "wenn, die wir zersto\u00dfen und zerst\u00f6ren,", "tokens": ["wenn", ",", "die", "wir", "zer\u00b7sto\u00b7\u00dfen", "und", "zer\u00b7st\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "erst verloren sind und fernverlaufen", "tokens": ["erst", "ver\u00b7lo\u00b7ren", "sind", "und", "fern\u00b7ver\u00b7lau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und vergangen sind in der Gefahr:", "tokens": ["und", "ver\u00b7gan\u00b7gen", "sind", "in", "der", "Ge\u00b7fahr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "denn dann will ich in den Tr\u00fcmmerhaufen", "tokens": ["denn", "dann", "will", "ich", "in", "den", "Tr\u00fcm\u00b7mer\u00b7hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure