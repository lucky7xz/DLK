{"dta.poem.7600": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Mehl zu sichten, braucht man Siebe, gro\u00df und kleiner;", "tokens": ["Das", "Mehl", "zu", "sich\u00b7ten", ",", "braucht", "man", "Sie\u00b7be", ",", "gro\u00df", "und", "klei\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "PIS", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch je mehr Sieb' es geht, je feiner ists und reiner.", "tokens": ["Durch", "je", "mehr", "Sieb'", "es", "geht", ",", "je", "fei\u00b7ner", "ists", "und", "rei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VAFIN", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das ist das gr\u00f6bste, was im ersten Sieb sich fieng,", "tokens": ["Das", "ist", "das", "gr\u00f6bs\u00b7te", ",", "was", "im", "ers\u00b7ten", "Sieb", "sich", "fi\u00b7eng", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "$,", "PRELS", "APPRART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Und das vorz\u00fcglichste, was durch das feinste gieng.", "tokens": ["Und", "das", "vor\u00b7z\u00fcg\u00b7lichs\u00b7te", ",", "was", "durch", "das", "feins\u00b7te", "gieng", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "PRELS", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "--+----+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Auch Perlen sichtet man in mehr als einem Sieb,", "tokens": ["Auch", "Per\u00b7len", "sich\u00b7tet", "man", "in", "mehr", "als", "ei\u00b7nem", "Sieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PIS", "APPR", "PIAT", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist die beste, die im ersten hangen blieb.", "tokens": ["Doch", "ist", "die", "bes\u00b7te", ",", "die", "im", "ers\u00b7ten", "han\u00b7gen", "blieb", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "$,", "PRELS", "APPRART", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Je schlechter nur, jemehr durch Siebe sie gegangen,", "tokens": ["Je", "schlech\u00b7ter", "nur", ",", "je\u00b7mehr", "durch", "Sie\u00b7be", "sie", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "ADV", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bleiben die schlechtesten zuletzt im feinsten hangen.", "tokens": ["Blei\u00b7ben", "die", "schlech\u00b7tes\u00b7ten", "zu\u00b7letzt", "im", "feins\u00b7ten", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADV", "APPRART", "ADJA", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Wenn du die Perle bist, sei lieber gro\u00df als klein;", "tokens": ["Wenn", "du", "die", "Per\u00b7le", "bist", ",", "sei", "lie\u00b7ber", "gro\u00df", "als", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "VAFIN", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wenn du Mehl bist, kannst du fein genug nicht seyn.", "tokens": ["Doch", "wenn", "du", "Mehl", "bist", ",", "kannst", "du", "fein", "ge\u00b7nug", "nicht", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VAFIN", "$,", "VMFIN", "PPER", "ADJD", "ADV", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}