{"textgrid.poem.56757": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich tr\u00e4umt einmal, ich l\u00e4g, ein blasser Knabe,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tr\u00e4umt einmal, ich l\u00e4g, ein blasser Knabe,", "tokens": ["Ich", "tr\u00e4umt", "ein\u00b7mal", ",", "ich", "l\u00e4g", ",", "ein", "blas\u00b7ser", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in einem Kahne schlafend ausgestreckt,", "tokens": ["in", "ei\u00b7nem", "Kah\u00b7ne", "schla\u00b7fend", "aus\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und meiner Lider fein Geweb durchflammte", "tokens": ["und", "mei\u00b7ner", "Li\u00b7der", "fein", "Ge\u00b7web", "durch\u00b7flamm\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der hohen Nacht geheimnisvoller Glanz.", "tokens": ["der", "ho\u00b7hen", "Nacht", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7ler", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und all mein Innres wurde Licht und Schimmer,", "tokens": ["Und", "all", "mein", "Inn\u00b7res", "wur\u00b7de", "Licht", "und", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und ein Entz\u00fccken, das ich nie gekannt,", "tokens": ["und", "ein", "Ent\u00b7z\u00fc\u00b7cken", ",", "das", "ich", "nie", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "durchgl\u00fchte mich und hob mein ganzes Wesen", "tokens": ["durch\u00b7gl\u00fch\u00b7te", "mich", "und", "hob", "mein", "gan\u00b7zes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "in eine h\u00f6here Ordnung der Natur.", "tokens": ["in", "ei\u00b7ne", "h\u00f6\u00b7he\u00b7re", "Ord\u00b7nung", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ein leises T\u00f6nen hielt mich hold umfangen,", "tokens": ["Ein", "lei\u00b7ses", "T\u00f6\u00b7nen", "hielt", "mich", "hold", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "als zitterte in jedem Sternenstrahl", "tokens": ["als", "zit\u00b7ter\u00b7te", "in", "je\u00b7dem", "Ster\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "der Ton der Heimat, die ihn hergesendet.", "tokens": ["der", "Ton", "der", "Hei\u00b7mat", ",", "die", "ihn", "her\u00b7ge\u00b7sen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "und lie\u00df die andern mehr und mehr verstummen", "tokens": ["und", "lie\u00df", "die", "an\u00b7dern", "mehr", "und", "mehr", "ver\u00b7stum\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADV", "KON", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "und tat sich auseinander wie der Kelch", "tokens": ["und", "tat", "sich", "aus\u00b7ein\u00b7an\u00b7der", "wie", "der", "Kelch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "der K\u00f6nigin der Nacht und offenbarte", "tokens": ["der", "K\u00f6\u00b7ni\u00b7gin", "der", "Nacht", "und", "of\u00b7fen\u00b7bar\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "auf seinem Grunde mir sein s\u00fc\u00dfes Lied ...", "tokens": ["auf", "sei\u00b7nem", "Grun\u00b7de", "mir", "sein", "s\u00fc\u00b7\u00dfes", "Lied", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbwir gr\u00fc\u00dfen dich in deine stillen N\u00e4chte,", "tokens": ["\u00bb", "wir", "gr\u00fc\u00b7\u00dfen", "dich", "in", "dei\u00b7ne", "stil\u00b7len", "N\u00e4ch\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "als deiner Zukunft tr\u00f6stliche Gew\u00e4hr,", "tokens": ["als", "dei\u00b7ner", "Zu\u00b7kunft", "tr\u00f6st\u00b7li\u00b7che", "Ge\u00b7w\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "es schalten ungeheure Willensm\u00e4chte", "tokens": ["es", "schal\u00b7ten", "un\u00b7ge\u00b7heu\u00b7re", "Wil\u00b7lens\u00b7m\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in unsrer Tage blindem Ungef\u00e4hr.", "tokens": ["in", "uns\u00b7rer", "Ta\u00b7ge", "blin\u00b7dem", "Un\u00b7ge\u00b7f\u00e4hr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie ziehn dich von Gestaltung zu Gestaltung,", "tokens": ["Sie", "ziehn", "dich", "von", "Ge\u00b7stal\u00b7tung", "zu", "Ge\u00b7stal\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "heut schleppst du dich noch schweren Schrittes hin,", "tokens": ["heut", "schleppst", "du", "dich", "noch", "schwe\u00b7ren", "Schrit\u00b7tes", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "doch bald begabt dich freiere Entfaltung", "tokens": ["doch", "bald", "be\u00b7gabt", "dich", "frei\u00b7e\u00b7re", "Ent\u00b7fal\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "mit reicherer Natur und h\u00f6herm Sinn.", "tokens": ["mit", "rei\u00b7che\u00b7rer", "Na\u00b7tur", "und", "h\u00f6\u00b7herm", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So wandeln wir auf leichten T\u00e4nzerf\u00fc\u00dfen,", "tokens": ["So", "wan\u00b7deln", "wir", "auf", "leich\u00b7ten", "T\u00e4n\u00b7zer\u00b7f\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "die wir dereinst auch dein Geschick geteilt,", "tokens": ["die", "wir", "de\u00b7reinst", "auch", "dein", "Ge\u00b7schick", "ge\u00b7teilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "und d\u00fcrfen dich mit einem Liede gr\u00fc\u00dfen,", "tokens": ["und", "d\u00fcr\u00b7fen", "dich", "mit", "ei\u00b7nem", "Lie\u00b7de", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "das dich auf Strahlen unsres Sterns ereilt.", "tokens": ["das", "dich", "auf", "Strah\u00b7len", "uns\u00b7res", "Sterns", "er\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Oh fl\u00fcchte bald nach unsern Lustgefilden,", "tokens": ["Oh", "fl\u00fcch\u00b7te", "bald", "nach", "un\u00b7sern", "Lust\u00b7ge\u00b7fil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "und la\u00df der kalten Erde grauen Dunst,", "tokens": ["und", "la\u00df", "der", "kal\u00b7ten", "Er\u00b7de", "grau\u00b7en", "Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Oh s\u00e4hst du, zu welch g\u00f6ttlichen Gebilden", "tokens": ["Oh", "s\u00e4hst", "du", ",", "zu", "welch", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7bil\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "$,", "APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "uns schuf des Schicksals hei\u00df ersehnte Gunst!", "tokens": ["uns", "schuf", "des", "Schick\u00b7sals", "hei\u00df", "er\u00b7sehn\u00b7te", "Gunst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Auf Blumen wandeln wir wie leichte Falter,", "tokens": ["Auf", "Blu\u00b7men", "wan\u00b7deln", "wir", "wie", "leich\u00b7te", "Fal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "aus Fr\u00fcchten saugen wir der Kr\u00e4fte Saft,", "tokens": ["aus", "Fr\u00fcch\u00b7ten", "sau\u00b7gen", "wir", "der", "Kr\u00e4f\u00b7te", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "uns ficht kein Elend an, zerbricht kein Alter,", "tokens": ["uns", "ficht", "kein", "E\u00b7lend", "an", ",", "zer\u00b7bricht", "kein", "Al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "der fr\u00fchern Leiden l\u00e4chelt unsre Kraft.", "tokens": ["der", "fr\u00fc\u00b7hern", "Lei\u00b7den", "l\u00e4\u00b7chelt", "uns\u00b7re", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Denn allzu sch\u00f6n, als da\u00df wir uns entzweiten,", "tokens": ["Denn", "all\u00b7zu", "sch\u00f6n", ",", "als", "da\u00df", "wir", "uns", "ent\u00b7zwei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKA", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "erschuf uns das Gestirn, das uns gebar, \u2013", "tokens": ["er\u00b7schuf", "uns", "das", "Ge\u00b7stirn", ",", "das", "uns", "ge\u00b7bar", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "wir k\u00f6nnen uns nicht Schmerz und Not bereiten,", "tokens": ["wir", "k\u00f6n\u00b7nen", "uns", "nicht", "Schmerz", "und", "Not", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "die Sch\u00f6nheit macht uns aller Feindschaft bar!", "tokens": ["die", "Sch\u00f6n\u00b7heit", "macht", "uns", "al\u00b7ler", "Feind\u00b7schaft", "bar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Wir lieben uns aus tiefsten Herzensgr\u00fcnden,", "tokens": ["Wir", "lie\u00b7ben", "uns", "aus", "tiefs\u00b7ten", "Her\u00b7zens\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "wir trinken unsres Anblicks Gl\u00fcck und Huld,", "tokens": ["wir", "trin\u00b7ken", "uns\u00b7res", "An\u00b7blicks", "Gl\u00fcck", "und", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "wir wissen nichts wie ihr von fahlen S\u00fcnden,", "tokens": ["wir", "wis\u00b7sen", "nichts", "wie", "ihr", "von", "fah\u00b7len", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "und keinen \u00e4ngstigt das Gespenst der Schuld.", "tokens": ["und", "kei\u00b7nen", "\u00e4ngs\u00b7tigt", "das", "Ge\u00b7spenst", "der", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Oh komm! da\u00df sich die dornenlose Rose", "tokens": ["Oh", "komm", "!", "da\u00df", "sich", "die", "dor\u00b7nen\u00b7lo\u00b7se", "Ro\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$.", "KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "auch deiner Schl\u00e4fe duftend schmiegen kann!", "tokens": ["auch", "dei\u00b7ner", "Schl\u00e4\u00b7fe", "duf\u00b7tend", "schmie\u00b7gen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Die sch\u00f6nste Schwester diene deinem Lose", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Schwes\u00b7ter", "die\u00b7ne", "dei\u00b7nem", "Lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "und schenke dich dem sch\u00f6nsten Mann \u2013 oh komm \u2013!\u00ab", "tokens": ["und", "schen\u00b7ke", "dich", "dem", "sch\u00f6ns\u00b7ten", "Mann", "\u2013", "oh", "komm", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$(", "ADJD", "VVFIN", "$(", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Da unterbrach ein dumpfer Glockenton", "tokens": ["Da", "un\u00