{"textgrid.poem.36919": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun, adeliger Schuft, Verbrecher,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun, adeliger Schuft, Verbrecher,", "tokens": ["Nun", ",", "a\u00b7de\u00b7li\u00b7ger", "Schuft", ",", "Ver\u00b7bre\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der frech betrogen und geraubt,", "tokens": ["Der", "frech", "be\u00b7tro\u00b7gen", "und", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun ist's, den Unsern dich zu nennen,", "tokens": ["Nun", "ist's", ",", "den", "Un\u00b7sern", "dich", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns b\u00fcrgerlichem Pack erlaubt.", "tokens": ["Uns", "b\u00fcr\u00b7ger\u00b7li\u00b7chem", "Pack", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem Beispiel deiner Ahnen folgend,", "tokens": ["Dem", "Bei\u00b7spiel", "dei\u00b7ner", "Ah\u00b7nen", "fol\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Traf dich so hartes Mi\u00dfgeschick!", "tokens": ["Traf", "dich", "so", "har\u00b7tes", "Mi\u00df\u00b7ge\u00b7schick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt nimmt man R\u00e4ubern ihren Adel \u2013", "tokens": ["Jetzt", "nimmt", "man", "R\u00e4u\u00b7bern", "ih\u00b7ren", "A\u00b7del", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So geht die sch\u00f6ne Zeit zur\u00fcck!", "tokens": ["So", "geht", "die", "sch\u00f6\u00b7ne", "Zeit", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun, Schurke, bist du unser worden,", "tokens": ["Nun", ",", "Schur\u00b7ke", ",", "bist", "du", "un\u00b7ser", "wor\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da Schmach an deinem Namen klebt;", "tokens": ["Da", "Schmach", "an", "dei\u00b7nem", "Na\u00b7men", "klebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wir sind die gebornen Schurken,", "tokens": ["Denn", "wir", "sind", "die", "ge\u00b7bor\u00b7nen", "Schur\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von denen Staat und K\u00f6nig lebt.", "tokens": ["Von", "de\u00b7nen", "Staat", "und", "K\u00f6\u00b7nig", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch tr\u00f6ste dich und strebe weiter,", "tokens": ["Doch", "tr\u00f6s\u00b7te", "dich", "und", "stre\u00b7be", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verfolge deinen Galgenlauf!", "tokens": ["Ver\u00b7fol\u00b7ge", "dei\u00b7nen", "Gal\u00b7gen\u00b7lauf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann, aus der b\u00fcrgerlichen Sph\u00e4re,", "tokens": ["Dann", ",", "aus", "der", "b\u00fcr\u00b7ger\u00b7li\u00b7chen", "Sph\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zieht man dich wiederum hinauf.", "tokens": ["Zieht", "man", "dich", "wie\u00b7de\u00b7rum", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun, adeliger Schuft, Verbrecher,", "tokens": ["Nun", ",", "a\u00b7de\u00b7li\u00b7ger", "Schuft", ",", "Ver\u00b7bre\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der frech betrogen und geraubt,", "token