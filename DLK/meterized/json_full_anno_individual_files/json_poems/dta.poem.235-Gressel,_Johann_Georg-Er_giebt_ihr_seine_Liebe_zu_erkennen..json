{"dta.poem.235": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er giebt ihr seine Liebe zu erkennen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Durch deiner Augen-Pracht geliebte Sch\u00f6ne!", "tokens": ["Durch", "dei\u00b7ner", "Au\u00b7gen\u00b7Pracht", "ge\u00b7lieb\u00b7te", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast du mein Hertz verletzt/ und mich verwundt/", "tokens": ["Hast", "du", "mein", "Hertz", "ver\u00b7letzt", "/", "und", "mich", "ver\u00b7wundt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$(", "KON", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum g\u00f6nne/ da\u00df es auch sich nach dir sehne", "tokens": ["Drum", "g\u00f6n\u00b7ne", "/", "da\u00df", "es", "auch", "sich", "nach", "dir", "seh\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$(", "KOUS", "PPER", "ADV", "PRF", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch einen holden Blick wird es gesundt.", "tokens": ["Durch", "ei\u00b7nen", "hol\u00b7den", "Blick", "wird", "es", "ge\u00b7sundt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Eh bin ich nicht vergn\u00fcgt/", "tokens": ["Eh", "bin", "ich", "nicht", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bis ich dein Hertz bestegt/", "tokens": ["Bis", "ich", "dein", "Hertz", "bes\u00b7tegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Darf ich dein eigen seyn?", "tokens": ["Darf", "ich", "dein", "ei\u00b7gen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "So legt sich meine Pein/", "tokens": ["So", "legt", "sich", "mei\u00b7ne", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und ist vorbey:", "tokens": ["Und", "ist", "vor\u00b7bey", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Es schmecket meine Brust/", "tokens": ["Es", "schme\u00b7cket", "mei\u00b7ne", "Brust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Vergn\u00fcgte Himmels-Lust/", "tokens": ["Ver\u00b7gn\u00fcg\u00b7te", "Him\u00b7mels\u00b7Lust", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Wenn mich dein Mund bek\u00fcst/", "tokens": ["Wenn", "mich", "dein", "Mund", "be\u00b7k\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Und meine Quaal vers\u00fc\u00dft", "tokens": ["Und", "mei\u00b7ne", "Qua\u00b7al", "ver\u00b7s\u00fc\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Durch wahre Treu.", "tokens": ["Durch", "wah\u00b7re", "Treu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mein Wahl-Spruch lautet so: Ich will dich lieben/", "tokens": ["Mein", "Wahl\u00b7Spruch", "lau\u00b7tet", "so", ":", "Ich", "will", "dich", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$.", "PPER", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So lange mir die Lufft das Leben schenckt/", "tokens": ["So", "lan\u00b7ge", "mir", "die", "Lufft", "das", "Le\u00b7ben", "schenckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein Nahm ist mir so tieff ins Hertz geschrieben/", "tokens": ["Dein", "Nahm", "ist", "mir", "so", "tieff", "ins", "Hertz", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df es so Tag als Nacht an dich gedenckt.", "tokens": ["Da\u00df", "es", "so", "Tag", "als", "Nacht", "an", "dich", "ge\u00b7denckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "KOUS", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich ehre deine Pracht", "tokens": ["Ich", "eh\u00b7re", "dei\u00b7ne", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die mich zum Sclaven macht/", "tokens": ["Die", "mich", "zum", "Scla\u00b7ven", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dein sch\u00f6nes Angesicht", "tokens": ["Dein", "sch\u00f6\u00b7nes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df weicht im minsten nicht", "tokens": ["Da\u00df", "weicht", "im", "mins\u00b7ten", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPRART", "ADJA", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dem Paradies", "tokens": ["Dem", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "La\u00df deiner Augen-Schein", "tokens": ["La\u00df", "dei\u00b7ner", "Au\u00b7gen\u00b7Schein"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Mir holde Sternen seyn/", "tokens": ["Mir", "hol\u00b7de", "Ster\u00b7nen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "So f\u00fchret mein ", "tokens": ["So", "f\u00fch\u00b7ret", "mein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Mich auf die rechte Stra\u00df/", "tokens": ["Mich", "auf", "die", "rech\u00b7te", "Stra\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Zum g\u00fcldnem Vlie\u00df.", "tokens": ["Zum", "g\u00fcld\u00b7nem", "Vlie\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}