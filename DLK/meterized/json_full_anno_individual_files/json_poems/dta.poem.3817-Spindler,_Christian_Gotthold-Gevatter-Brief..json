{"dta.poem.3817": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Gevatter-Brief.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "So hat demnach die Zeit, die angenehmsten", "tokens": ["So", "hat", "dem\u00b7nach", "die", "Zeit", ",", "die", "an\u00b7ge\u00b7nehms\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PAV", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stunden,", "tokens": ["Stun\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mir meinen liebsten Schatz mit einem Sohn ent-", "tokens": ["Mir", "mei\u00b7nen", "liebs\u00b7ten", "Schatz", "mit", "ei\u00b7nem", "Sohn", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "bunden.", "tokens": ["bun\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Da liegt das arme Kind, mein Hertz und meine Lust,", "tokens": ["Da", "liegt", "das", "ar\u00b7me", "Kind", ",", "mein", "Hertz", "und", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jedoch durch Adams Fall, in seinem S\u00fcnden-Wust.", "tokens": ["Je\u00b7doch", "durch", "A\u00b7dams", "Fall", ",", "in", "sei\u00b7nem", "S\u00fcn\u00b7den\u00b7Wust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So kan es ja das Bad der heilsam vollen Gnaden,", "tokens": ["So", "kan", "es", "ja", "das", "Bad", "der", "heil\u00b7sam", "vol\u00b7len", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NE", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bey seiner S\u00fcnden-Noth auf keine Art entrathen.", "tokens": ["Bey", "sei\u00b7ner", "S\u00fcn\u00b7den\u00b7Noth", "auf", "kei\u00b7ne", "Art", "ent\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gott thut ihm seine Huld und seine Liebe kund,", "tokens": ["Gott", "thut", "ihm", "sei\u00b7ne", "Huld", "und", "sei\u00b7ne", "Lie\u00b7be", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er nimmt es willig auf in seinen Gnaden-Bund.", "tokens": ["Er", "nimmt", "es", "wil\u00b7lig", "auf", "in", "sei\u00b7nen", "Gna\u00b7den\u00b7Bund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Soll diese Handlung nun ihr fernres Ziel erreichen,", "tokens": ["Soll", "die\u00b7se", "Hand\u00b7lung", "nun", "ihr", "fern\u00b7res", "Ziel", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So brauchet man hierzu auch treu erbetne Zeugen.", "tokens": ["So", "brau\u00b7chet", "man", "hier\u00b7zu", "auch", "treu", "er\u00b7bet\u00b7ne", "Zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PAV", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vor die\u00dfmahl solst nun du der ", "tokens": ["Vor", "die\u00df\u00b7mahl", "solst", "nun", "du", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VMFIN", "ADV", "PPER", "ART"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.14": {"text": "Sprich bey mir, als mein Freund, und Herr Ge-", "tokens": ["Sprich", "bey", "mir", ",", "als", "mein", "Freund", ",", "und", "Herr", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPER", "$,", "KOUS", "PPOSAT", "NN", "$,", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "vatter, ein.", "tokens": ["vat\u00b7ter", ",", "ein", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Du must das arme Kind bey deinem Gott vertreten,", "tokens": ["Du", "must", "das", "ar\u00b7me", "Kind", "bey", "dei\u00b7nem", "Gott", "ver\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du must, nach Pathen-Pflicht, inbr\u00fcnstig vor ihn", "tokens": ["Du", "must", ",", "nach", "Pa\u00b7then\u00b7Pflicht", ",", "in\u00b7br\u00fcns\u00b7tig", "vor", "ihn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "APPR", "NN", "$,", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "beten.", "tokens": ["be\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Um drey Uhr, Nachmittags, wird alles vor sich gehn,", "tokens": ["Um", "drey", "Uhr", ",", "Nach\u00b7mit\u00b7tags", ",", "wird", "al\u00b7les", "vor", "sich", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "ADV", "$,", "VAFIN", "PIS", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den\u0303 suchst du, als sein Path, ihm flei\u00dfig beyzustehn.", "tokens": ["De\u00f1", "suchst", "du", ",", "als", "sein", "Path", ",", "ihm", "flei\u00b7\u00dfig", "bey\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "$,", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "O! flehe doch zu GOtt bey seinem Wunder-Bade,", "tokens": ["O", "!", "fle\u00b7he", "doch", "zu", "Gott", "bey", "sei\u00b7nem", "Wun\u00b7der\u00b7Ba\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Befiehl es seiner Huld, befiehl es seiner Gnade.", "tokens": ["Be\u00b7fiehl", "es", "sei\u00b7ner", "Huld", ",", "be\u00b7fiehl", "es", "sei\u00b7ner", "Gna\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und hast du di\u00df gethan, so komme in mein Hau\u00df,", "tokens": ["Und", "hast", "du", "di\u00df", "ge\u00b7than", ",", "so", "kom\u00b7me", "in", "mein", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PDS", "VVPP", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Nim\u0303 williglich vorlieb mit einem Kindtauffschmau\u00df.", "tokens": ["Nim\u0303", "wil\u00b7lig\u00b7lich", "vor\u00b7lieb", "mit", "ei\u00b7nem", "Kind\u00b7tauff\u00b7schmau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Freund, ist er gleich sehr schlecht, du wirst es doch", "tokens": ["Freund", ",", "ist", "er", "gleich", "sehr", "schlecht", ",", "du", "wirst", "es", "doch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "PPER", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "nicht achten,", "tokens": ["nicht", "ach\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Du wirst mein Kindtauffs Mahl (ich wei\u00df es) nicht", "tokens": ["Du", "wirst", "mein", "Kind\u00b7tauffs", "Mahl", "(", "ich", "wei\u00df", "es", ")", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN", "$(", "PPER", "VVFIN", "PPER", "$(", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "verachten.", "tokens": ["ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "Nimm also diesen Brief in aller Liebe hin,", "tokens": ["Nimm", "al\u00b7so", "die\u00b7sen", "Brief", "in", "al\u00b7ler", "Lie\u00b7be", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PDAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Von mir, der ich dein Freund, und nun Gevatter bin.", "tokens": ["Von", "mir", ",", "der", "ich", "dein", "Freund", ",", "und", "nun", "Ge\u00b7vat\u00b7ter", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPER", "PPOSAT", "NN", "$,", "KON", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}