{"textgrid.poem.39779": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Tischgebet", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast uns aufgesetzet", "tokens": ["Du", "hast", "uns", "auf\u00b7ge\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von deinem guten Wein:", "tokens": ["Von", "dei\u00b7nem", "gu\u00b7ten", "Wein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn wir uns dran geletzet,", "tokens": ["Wenn", "wir", "uns", "dran", "ge\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df, Herr, es uns gedeihn!", "tokens": ["La\u00df", ",", "Herr", ",", "es", "uns", "ge\u00b7deihn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du l\u00e4ssest es nicht fehlen", "tokens": ["Du", "l\u00e4s\u00b7sest", "es", "nicht", "feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An Liedern hell und gut,", "tokens": ["An", "Lie\u00b7dern", "hell", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So gieb uns frische Kehlen,", "tokens": ["So", "gieb", "uns", "fri\u00b7sche", "Keh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und frohen Liedermut!", "tokens": ["Und", "fro\u00b7hen", "Lie\u00b7der\u00b7mut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wem du zu den Reben", "tokens": ["Und", "wem", "du", "zu", "den", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und zu dem lust'gen Sang", "tokens": ["Und", "zu", "dem", "lust'\u00b7gen", "Sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Liebchen wolltest geben,", "tokens": ["Ein", "Lieb\u00b7chen", "woll\u00b7test", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem la\u00df es noch recht lang!", "tokens": ["Dem", "la\u00df", "es", "noch", "recht", "lang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In Z\u00fcchten und in Ehren", "tokens": ["In", "Z\u00fcch\u00b7ten", "und", "in", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bewahr' ihm ihren Kranz;", "tokens": ["Be\u00b7wahr'", "ihm", "ih\u00b7ren", "Kranz", ";"], "token_inf