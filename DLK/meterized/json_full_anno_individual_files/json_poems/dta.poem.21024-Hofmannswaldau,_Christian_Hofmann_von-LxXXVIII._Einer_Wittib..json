{"dta.poem.21024": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXVIII.  \n Einer Wittib.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein sch\u00f6nes Schif/ das ohne Ladung lag/", "tokens": ["Ich", "war", "ein", "sch\u00f6\u00b7nes", "Schif", "/", "das", "oh\u00b7ne", "La\u00b7dung", "lag", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "PDS", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es plagte mich die Nacht/ es kr\u00e4nckte mich der Tag.", "tokens": ["Es", "plag\u00b7te", "mich", "die", "Nacht", "/", "es", "kr\u00e4nck\u00b7te", "mich", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$(", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier ist nicht Licht genung/ mich deutlich zu verste-", "tokens": ["Hier", "ist", "nicht", "Licht", "ge\u00b7nung", "/", "mich", "deut\u00b7lich", "zu", "vers\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "ADV", "$(", "PPER", "ADJD", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "hen/ (hen.", "tokens": ["hen", "/", "(", "hen", "."], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "$(", "VVINF", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.5": {"text": "Weil mir der Mast gebrach/ mu\u00df ich zu Grunde ge-", "tokens": ["Weil", "mir", "der", "Mast", "ge\u00b7brach", "/", "mu\u00df", "ich", "zu", "Grun\u00b7de", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$(", "VMFIN", "PPER", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}