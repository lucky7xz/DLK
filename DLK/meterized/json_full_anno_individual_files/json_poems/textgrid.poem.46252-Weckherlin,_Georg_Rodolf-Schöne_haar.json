{"textgrid.poem.46252": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6ne haar", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O der lieb liebstes garn, der sch\u00f6nsten sch\u00f6nste haar,", "tokens": ["O", "der", "lieb", "liebs\u00b7tes", "garn", ",", "der", "sch\u00f6ns\u00b7ten", "sch\u00f6ns\u00b7te", "haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJD", "ADJA", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "wan scherzend in dem luft ihr schon bandlos umflieget,", "tokens": ["wan", "scher\u00b7zend", "in", "dem", "luft", "ihr", "schon", "band\u00b7los", "um\u00b7flie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+----+--++-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "befind ich doch alsbald, da\u00df ihr mein herz betrieget,", "tokens": ["be\u00b7find", "ich", "doch", "als\u00b7bald", ",", "da\u00df", "ihr", "mein", "herz", "be\u00b7trie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und da\u00df, je freier ihr, je gr\u00f6\u00dfer mein gefahr.", "tokens": ["und", "da\u00df", ",", "je", "frei\u00b7er", "ihr", ",", "je", "gr\u00f6\u00b7\u00dfer", "mein", "ge\u00b7fahr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "