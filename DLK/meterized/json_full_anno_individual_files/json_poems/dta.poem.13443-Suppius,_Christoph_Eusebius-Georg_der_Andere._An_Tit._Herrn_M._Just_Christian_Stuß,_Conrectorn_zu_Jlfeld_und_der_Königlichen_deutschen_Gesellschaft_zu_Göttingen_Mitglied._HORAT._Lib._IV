{"dta.poem.13443": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Georg der Andere.  \n An Tit. Herrn  \n  M.  Just Christian Stu\u00df,  \n Conrectorn zu Jlfeld und der K\u00f6niglichen deutschen  \n Gesellschaft zu G\u00f6ttingen Mitglied.  \n   \n  HORAT.   Lib. IV. Od. 14.  \n  Te, non pauentis funera Galliae,  \n  Duraeque tellus audit Iberiae:  \n  Te caede gaudentes Sicambri  \n  Compositis venerantur armis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Du sahest ihn, mein Stu\u00df! vor dessen Drohen", "tokens": ["Du", "sa\u00b7hest", "ihn", ",", "mein", "Stu\u00df", "!", "vor", "des\u00b7sen", "Dro\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bey Dettingen der Feinde Schaaren flohen,", "tokens": ["Bey", "Det\u00b7tin\u00b7gen", "der", "Fein\u00b7de", "Schaa\u00b7ren", "flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es sch\u00fctzte sie der Rhein noch aus Erbarmen", "tokens": ["Es", "sch\u00fctz\u00b7te", "sie", "der", "Rhein", "noch", "aus", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NE", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er aber hie\u00df ihr Haupt-Panier nun schwingen,", "tokens": ["Er", "a\u00b7ber", "hie\u00df", "ihr", "Haup\u00b7tPa\u00b7nier", "nun", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und von Triumph dich, frohes Deutschland singen,", "tokens": ["Und", "von", "Tri\u00b7umph", "dich", ",", "fro\u00b7hes", "Deutschland", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Von dannen eilend, um der Wellen W\u00fcten", "tokens": ["Von", "dan\u00b7nen", "ei\u00b7lend", ",", "um", "der", "Wel\u00b7len", "W\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVPP", "$,", "KOUI", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die unter seinen Stab sich murmelnd beugen,", "tokens": ["Die", "un\u00b7ter", "sei\u00b7nen", "Stab", "sich", "mur\u00b7melnd", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und der Emp\u00f6rung nicht mehr g\u00fcnstig zeigen,", "tokens": ["Und", "der", "Em\u00b7p\u00f6\u00b7rung", "nicht", "mehr", "g\u00fcns\u00b7tig", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie wiegen ihn sanft zum Gehorsamszeichen", "tokens": ["Sie", "wie\u00b7gen", "ihn", "sanft", "zum", "Ge\u00b7hor\u00b7sams\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Um, wenn er winkt, auf ihren nassen R\u00fccken", "tokens": ["Um", ",", "wenn", "er", "winkt", ",", "auf", "ih\u00b7ren", "nas\u00b7sen", "R\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Manch Indien nach Engelland zu schicken,", "tokens": ["Manch", "In\u00b7di\u00b7en", "nach", "En\u00b7gel\u00b7land", "zu", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In seiner V\u00f6lker angef\u00fcllten Speichern", "tokens": ["In", "sei\u00b7ner", "V\u00f6l\u00b7ker", "an\u00b7ge\u00b7f\u00fcll\u00b7ten", "Spei\u00b7chern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Freund! preise mit mir Jhn, den Held im Heere,", "tokens": ["Freund", "!", "prei\u00b7se", "mit", "mir", "Jhn", ",", "den", "Held", "im", "Hee\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "APPR", "PPER", "PPER", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der L\u00e4nder Vater und den Gott der Meere,", "tokens": ["Der", "L\u00e4n\u00b7der", "Va\u00b7ter", "und", "den", "Gott", "der", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Georg den Andern, mit erhabnem Tone,", "tokens": ["Ge\u00b7org", "den", "An\u00b7dern", ",", "mit", "er\u00b7hab\u00b7nem", "To\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Herrschsucht stolzer Muth sinkt vor Jhm nieder,", "tokens": ["Der", "Herrschsucht", "stol\u00b7zer", "Muth", "sinkt", "vor", "Jhm", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem Adler schafft Er seine Blitze wieder,", "tokens": ["Dem", "Ad\u00b7ler", "schafft", "Er", "sei\u00b7ne", "Blit\u00b7ze", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bedr\u00e4ngte Freyheit fleht Jhn aus dem Wetter,", "tokens": ["Be\u00b7dr\u00e4ng\u00b7te", "Frey\u00b7heit", "fleht", "Jhn", "aus", "dem", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Dar\u00fcber tobt die Zwietracht; welch ein Rasen!", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "tobt", "die", "Zwiet\u00b7racht", ";", "welch", "ein", "Ra\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$.", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie sucht Jhm selbst ein Feuer aufzublasen,", "tokens": ["Sie", "sucht", "Jhm", "selbst", "ein", "Feu\u00b7er", "auf\u00b7zu\u00b7bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Cadmus k\u00f6mmt, um Saamen auszus\u00e4en,", "tokens": ["Und", "Cad\u00b7mus", "k\u00f6mmt", ",", "um", "Saa\u00b7men", "aus\u00b7zu\u00b7s\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "KOUI", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein Heer erw\u00e4chst aus felsichten Geb\u00fcrgen,", "tokens": ["Ein", "Heer", "er\u00b7w\u00e4chst", "aus", "fel\u00b7sich\u00b7ten", "Ge\u00b7b\u00fcr\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ger\u00fcstet, seine Sippschaft selbst zu w\u00fcrgen,", "tokens": ["Ge\u00b7r\u00fcs\u00b7tet", ",", "sei\u00b7ne", "Sipp\u00b7schaft", "selbst", "zu", "w\u00fcr\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es wird zerst\u00e4ubt! halb liegt es auf dem Boden", "tokens": ["Es", "wird", "zer\u00b7st\u00e4ubt", "!", "halb", "liegt", "es", "auf", "dem", "Bo\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Held! Cumberland! mit deines Vaters Keulen", "tokens": ["Held", "!", "Cum\u00b7ber\u00b7land", "!", "mit", "dei\u00b7nes", "Va\u00b7ters", "Keu\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewapnet, o! wohin willst Du doch eilen?", "tokens": ["Ge\u00b7wap\u00b7net", ",", "o", "!", "wo\u00b7hin", "willst", "Du", "doch", "ei\u00b7len", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "FM", "$.", "PWAV", "VMFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sprich nur ein Wort, so liegen sie bey Haufen,", "tokens": ["Sprich", "nur", "ein", "Wort", ",", "so", "lie\u00b7gen", "sie", "bey", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wie triumphirt Georg! Er wird noch gr\u00f6sser,", "tokens": ["Wie", "tri\u00b7um\u00b7phirt", "Ge\u00b7org", "!", "Er", "wird", "noch", "gr\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Ocean weiht Jhm sein ganz Gew\u00e4sser,", "tokens": ["Der", "O\u00b7cean", "weiht", "Jhm", "sein", "ganz", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "ADV", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er unterwirft des K\u00f6nigs Macht und G\u00fcte", "tokens": ["Er", "un\u00b7ter\u00b7wirft", "des", "K\u00f6\u00b7nigs", "Macht", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Um fernerhin der Freyheit zu geniessen,", "tokens": ["Um", "fer\u00b7ner\u00b7hin", "der", "Frey\u00b7heit", "zu", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Erde nach Gewohnheit zu umfliessen,", "tokens": ["Die", "Er\u00b7de", "nach", "Ge\u00b7wohn\u00b7heit", "zu", "um\u00b7flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deren Reichthum wie mit Mutterh\u00e4nden", "tokens": ["Und", "de\u00b7ren", "Reicht\u00b7hum", "wie", "mit", "Mut\u00b7ter\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRELAT", "NN", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Drum kommen nun auf seinen gr\u00fcnen Wogen", "tokens": ["Drum", "kom\u00b7men", "nun", "auf", "sei\u00b7nen", "gr\u00fc\u00b7nen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Britten Heere schwimmend angezogen,", "tokens": ["Der", "Brit\u00b7ten", "Hee\u00b7re", "schwim\u00b7mend", "an\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des K\u00f6nigs Donner funkeln an der Seite,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Don\u00b7ner", "fun\u00b7keln", "an", "der", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Der Feinde Segel sehn das, und sie beben,", "tokens": ["Der", "Fein\u00b7de", "Se\u00b7gel", "sehn", "das", ",", "und", "sie", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PDS", "$,", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn diese Schl\u00f6sser noch in Wolken schweben,", "tokens": ["Wenn", "die\u00b7se", "Schl\u00f6s\u00b7ser", "noch", "in", "Wol\u00b7ken", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Britten schleudern auf sie los mit Blitzen,", "tokens": ["Die", "Brit\u00b7ten", "schleu\u00b7dern", "auf", "sie", "los", "mit", "Blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Empf\u00e4nget sie kein unbekannt Gestade,", "tokens": ["Emp\u00b7f\u00e4n\u00b7get", "sie", "kein", "un\u00b7be\u00b7kannt", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So ist ihr Trost des Ueberwinders Gnade,", "tokens": ["So", "ist", "ihr", "Trost", "des", "Ue\u00b7ber\u00b7win\u00b7ders", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch welche sie, dem Untergang entnommen,", "tokens": ["Durch", "wel\u00b7che", "sie", ",", "dem", "Un\u00b7ter\u00b7gang", "ent\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Wie glorreich hat die Majest\u00e4t der Britten", "tokens": ["Wie", "glor\u00b7reich", "hat", "die", "Ma\u00b7jes\u00b7t\u00e4t", "der", "Brit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Manch muthiges Geschwader schon bestritten!", "tokens": ["Manch", "mut\u00b7hi\u00b7ges", "Ge\u00b7schwa\u00b7der", "schon", "be\u00b7strit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sobald Sie einem Seeheld nur befohlen,", "tokens": ["So\u00b7bald", "Sie", "ei\u00b7nem", "Se\u00b7e\u00b7held", "nur", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.16": {"line.1": {"text": "Dort steiget Norris auf des Nachbars K\u0