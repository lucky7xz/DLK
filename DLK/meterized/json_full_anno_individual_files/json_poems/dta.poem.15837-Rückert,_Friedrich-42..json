{"dta.poem.15837": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch ein gesegnet Jahr! wie sch\u00f6n der Fr\u00fchling war!", "tokens": ["Welch", "ein", "ge\u00b7seg\u00b7net", "Jahr", "!", "wie", "sch\u00f6n", "der", "Fr\u00fch\u00b7ling", "war", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "VVPP", "NN", "$.", "PWAV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun aber bringt der Herbst geschwellte F\u00fcllen dar.", "tokens": ["Nun", "a\u00b7ber", "bringt", "der", "Herbst", "ge\u00b7schwell\u00b7te", "F\u00fcl\u00b7len", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An Aesten Birn' und Pflaum', und Trauben an den Reben;", "tokens": ["An", "A\u00b7es\u00b7ten", "Birn'", "und", "Pflaum'", ",", "und", "Trau\u00b7ben", "an", "den", "Re\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch Bl\u00fcten, seh' ich recht? erschlie\u00dfen sich daneben!", "tokens": ["Doch", "Bl\u00fc\u00b7ten", ",", "seh'", "ich", "recht", "?", "er\u00b7schlie\u00b7\u00dfen", "sich", "da\u00b7ne\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VVFIN", "PPER", "ADJD", "$.", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Vom Fr\u00fchlingsnachspiel wird der reife Herbst versch\u00f6nt;", "tokens": ["Vom", "Fr\u00fch\u00b7lings\u00b7nach\u00b7spiel", "wird", "der", "rei\u00b7fe", "Herbst", "ver\u00b7sch\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Wunder ist die Frucht von Bl\u00fcten \u00fcberkr\u00f6nt.", "tokens": ["Ein", "Wun\u00b7der", "ist", "die", "Frucht", "von", "Bl\u00fc\u00b7ten", "\u00fc\u00b7ber\u00b7kr\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das ist ein Alter, das, wie wenige, dich erfreut,", "tokens": ["Das", "ist", "ein", "Al\u00b7ter", ",", "das", ",", "wie", "we\u00b7ni\u00b7ge", ",", "dich", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PDS", "$,", "PWAV", "PIS", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bei deinen Fr\u00fcchten, Herz, die Bl\u00fcten dir erneut.", "tokens": ["Bei", "dei\u00b7nen", "Fr\u00fcch\u00b7ten", ",", "Herz", ",", "die", "Bl\u00fc\u00b7ten", "dir", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}