{"dta.poem.18586": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "U ber das zu  D ublin in  M ar-  \n mor gehauene Bildni\u00df Wilhelm des  \n  III.  K\u00f6nigs in Engelland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Hier blitzt der tapffre Held/ mit dem die Erden pranget/", "tokens": ["Hier", "blitzt", "der", "tapf\u00b7fre", "Held", "/", "mit", "dem", "die", "Er\u00b7den", "pran\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "APPR", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor dem die stoltze See vergn\u00fcgt die Segel streicht.", "tokens": ["Vor", "dem", "die", "stolt\u00b7ze", "See", "ver\u00b7gn\u00fcgt", "die", "Se\u00b7gel", "streicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Freyheit/ Ehr und Ruhm den Britten hat erlanget/", "tokens": ["Der", "Frey\u00b7heit", "/", "Ehr", "und", "Ruhm", "den", "Brit\u00b7ten", "hat", "er\u00b7lan\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "KON", "NN", "ART", "NN", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der in Batavien allein der Sonnen gleicht.", "tokens": ["Der", "in", "Ba\u00b7ta\u00b7vi\u00b7en", "al\u00b7lein", "der", "Son\u00b7nen", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es leget Hercules vor ihm die Keulen nieder/", "tokens": ["Es", "le\u00b7get", "Her\u00b7cu\u00b7les", "vor", "ihm", "die", "Keu\u00b7len", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-++-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Weil seine tapffre Faust nicht \u00fcber diese kam.", "tokens": ["Weil", "sei\u00b7ne", "tapf\u00b7fre", "Faust", "nicht", "\u00fc\u00b7ber", "die\u00b7se", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "APPR", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er st\u00e4rckt den tapffren Arm durch sieben tapffre Glieder/", "tokens": ["Er", "st\u00e4rckt", "den", "tapf\u00b7fren", "Arm", "durch", "sie\u00b7ben", "tapf\u00b7fre", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So kennt ein blinder auch den gro", "tokens": ["So", "kennt", "ein", "blin\u00b7der", "auch", "den", "gro"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}