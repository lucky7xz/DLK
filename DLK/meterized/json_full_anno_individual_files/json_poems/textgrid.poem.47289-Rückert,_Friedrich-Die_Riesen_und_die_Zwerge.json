{"textgrid.poem.47289": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Riesen und die Zwerge", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ging die Riesentochter, zu haben einen Spa\u00df,", "tokens": ["Es", "ging", "die", "Rie\u00b7sen\u00b7toch\u00b7ter", ",", "zu", "ha\u00b7ben", "ei\u00b7nen", "Spa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PTKZU", "VAINF", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Herab vom hohen Schlosse, wo Vater Riese sa\u00df.", "tokens": ["Her\u00b7ab", "vom", "ho\u00b7hen", "Schlos\u00b7se", ",", "wo", "Va\u00b7ter", "Rie\u00b7se", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "PWAV", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da fand sie in dem Thale die Ochsen und den Pflug,", "tokens": ["Da", "fand", "sie", "in", "dem", "Tha\u00b7le", "die", "Och\u00b7sen", "und", "den", "Pflug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dahinter auch den Bauern, der schien ihr klein genug.", "tokens": ["Da\u00b7hin\u00b7ter", "auch", "den", "Bau\u00b7ern", ",", "der", "schien", "ihr", "klein", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die Riesen und die Zwerge!", "tokens": ["Die", "Rie\u00b7sen", "und", "die", "Zwer\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Pflug, Ochsen und den Bauern, es war ihr nicht zu gro\u00df,", "tokens": ["Pflug", ",", "Och\u00b7sen", "und", "den", "Bau\u00b7ern", ",", "es", "war", "ihr", "nicht", "zu", "gro\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie fa\u00dft's in ihre Sch\u00fcrze und trug's aufs Riesenschlo\u00df.", "tokens": ["Sie", "fa\u00dft's", "in", "ih\u00b7re", "Sch\u00fcr\u00b7ze", "und", "trug's", "aufs", "Rie\u00b7sen\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da fragte Vater Riese: \u00bbWas hast du, Kind, gemacht?\u00ab", "tokens": ["Da", "frag\u00b7te", "Va\u00b7ter", "Rie\u00b7se", ":", "\u00bb", "Was", "hast", "du", ",", "Kind", ",", "ge\u00b7macht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "$.", "$(", "PWS", "VAFIN", "PPER", "$,", "NN", "$,", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie sprach: \u00bbEin sch\u00f6nes Spielzeug ha