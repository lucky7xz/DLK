{"dta.poem.985": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "E. R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Oft scheint es so als ob wir unsre besten                 ", "tokens": ["Oft", "scheint", "es", "so", "als", "ob", "wir", "uns\u00b7re", "bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOKOM", "KOUS", "PPER", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhebungen mit ihren s&#252;ssen reizen                 ", "tokens": ["Er\u00b7he\u00b7bun\u00b7gen", "mit", "ih\u00b7ren", "s", "&#252;", "ssen", "rei\u00b7zen"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "FM", "FM", "FM", "VVINF"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aus fr&#252;her fr&#252;he holen und mit resten                 ", "tokens": ["Aus", "fr", "&#252;", "her", "fr", "&#252;", "he", "ho\u00b7len", "und", "mit", "res\u00b7ten"], "token_info": ["word", "word", "XML_entity", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["APPR", "FM", "FM", "FM", "FM", "FM", "FM", "VVINF", "KON", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die \u00f6de ganzer lebensr&#228;ume heizen               ", "tokens": ["Die", "\u00f6\u00b7de", "gan\u00b7zer", "le\u00b7bensr", "&#228;", "u\u00b7me", "hei\u00b7zen"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ART", "FM", "FM", "FM", "FM", "FM", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bald so dass h\u00f6chster schatz 