{"dta.poem.6474": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch St\u00f6ss\u2019 und Dr\u00fcckungen, die sie den L\u00fcfften geben,", "tokens": ["Durch", "St\u00f6ss'", "und", "Dr\u00fc\u00b7ckun\u00b7gen", ",", "die", "sie", "den", "L\u00fcff\u00b7ten", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So um sie schweben,", "tokens": ["So", "um", "sie", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die f\u00e4hig, da\u00df sie sich denselben einverleiben,", "tokens": ["Die", "f\u00e4\u00b7hig", ",", "da\u00df", "sie", "sich", "den\u00b7sel\u00b7ben", "ein\u00b7ver\u00b7lei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "KOUS", "PPER", "PRF", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und welche sie in Circkeln treiben.", "tokens": ["Und", "wel\u00b7che", "sie", "in", "Cir\u00b7ckeln", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}