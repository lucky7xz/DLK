{"dta.poem.13930": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist die alte Halle,", "tokens": ["Das", "ist", "die", "al\u00b7te", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier sang ich manchesmal,", "tokens": ["Hier", "sang", "ich", "man\u00b7ches\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die hohen Ritter alle", "tokens": ["Die", "ho\u00b7hen", "Rit\u00b7ter", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rings um mich her im Saal.", "tokens": ["Rings", "um", "mich", "her", "im", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APZR", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}