{"dta.poem.21606": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch steh\u2019 ich entr\u00fcckt dem Erdenget\u00fcmmel,", "tokens": ["Hoch", "steh'", "ich", "ent\u00b7r\u00fcckt", "dem", "Er\u00b7den\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Durchschauert vor st\u00fcrmender Wonne erhabener Einsamkeit;", "tokens": ["Durch\u00b7schau\u00b7ert", "vor", "st\u00fcr\u00b7men\u00b7der", "Won\u00b7ne", "er\u00b7ha\u00b7be\u00b7ner", "Ein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+-+", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Zu niederem Kraut zusammengeb\u00fcckt gr\u00fcnt drunten", "tokens": ["Zu", "nie\u00b7de\u00b7rem", "Kraut", "zu\u00b7sam\u00b7men\u00b7ge\u00b7b\u00fcckt", "gr\u00fcnt", "drun\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VVFIN", "ADV"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In tiefentlegener Thaleseinsenkung", "tokens": ["In", "tie\u00b7fent\u00b7le\u00b7ge\u00b7ner", "Tha\u00b7le\u00b7sein\u00b7sen\u00b7kung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Des stattlichen Buchwalds hochkronige Ebne;", "tokens": ["Des", "statt\u00b7li\u00b7chen", "Buch\u00b7walds", "hoch\u00b7kro\u00b7ni\u00b7ge", "Eb\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Rings in der Runde schwellen empor die Bergesalt\u00e4re,", "tokens": ["Rings", "in", "der", "Run\u00b7de", "schwel\u00b7len", "em\u00b7por", "die", "Ber\u00b7ge\u00b7sal\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Des Himmels Schale mit trotzigem Nacken zu durchbrechen begehrend,", "tokens": ["Des", "Him\u00b7mels", "Scha\u00b7le", "mit", "trot\u00b7zi\u00b7gem", "Na\u00b7cken", "zu", "durch\u00b7bre\u00b7chen", "be\u00b7geh\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+--+--+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Und um ihre stolzen Glieder lichtwei\u00df geschlungen", "tokens": ["Und", "um", "ih\u00b7re", "stol\u00b7zen", "Glie\u00b7der", "licht\u00b7wei\u00df", "ge\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "ADJD", "VVPP"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wallen hernieder des ewigen Schnees prangende Strahlengewinde.", "tokens": ["Wal\u00b7len", "her\u00b7nie\u00b7der", "des", "e\u00b7wi\u00b7gen", "Schnees", "pran\u00b7gen\u00b7de", "Strah\u00b7len\u00b7ge\u00b7win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+--+---+--+--+-", "measure": "dactylic.tri.plus"}, "line.10": {"text": "Aber wie ich so schaue durch schimmernd krystallene Aethergefilde,", "tokens": ["A\u00b7ber", "wie", "ich", "so", "schau\u00b7e", "durch", "schim\u00b7mernd", "krys\u00b7tal\u00b7le\u00b7ne", "A\u00b7e\u00b7ther\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+-+-+---+-", "measure": "iambic.octa.plus.invert"}, "line.11": {"text": "Verlorenen Blicks hintr\u00e4ume, auf Fl\u00fcgeln sonnigen Strahlengef\u00e4hrtes", "tokens": ["Ver\u00b7lo\u00b7re\u00b7nen", "Blicks", "hin\u00b7tr\u00e4u\u00b7me", ",", "auf", "Fl\u00fc\u00b7geln", "son\u00b7ni\u00b7gen", "Strah\u00b7len\u00b7ge\u00b7f\u00e4hr\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "APPR", "NN", "ADJA", "NN"], "meter": "-+--+-+--+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Unermessene R\u00e4ume himmlischer L\u00fcfte durchwallend,", "tokens": ["Un\u00b7er\u00b7mes\u00b7se\u00b7ne", "R\u00e4u\u00b7me", "himm\u00b7li\u00b7scher", "L\u00fcf\u00b7te", "durch\u00b7wal\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.13": {"text": "Ist derweilen ein sturmgepr\u00fcftes, d\u00fcster gefaltetes Wolkenmeer", "tokens": ["Ist", "der\u00b7wei\u00b7len", "ein", "sturm\u00b7ge\u00b7pr\u00fcf\u00b7tes", ",", "d\u00fcs\u00b7ter", "ge\u00b7fal\u00b7te\u00b7tes", "Wol\u00b7ken\u00b7meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$,", "ADJD", "ADJA", "NN"], "meter": "+-+--+-+-+--+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.14": {"text": "Aus ferner Tiefe des Thales emporgebrandet hinter der Bergwand", "tokens": ["Aus", "fer\u00b7ner", "Tie\u00b7fe", "des", "Tha\u00b7les", "em\u00b7por\u00b7ge\u00b7bran\u00b7det", "hin\u00b7ter", "der", "Berg\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+--+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.15": {"text": "Und schl\u00e4gt sein wild entfesseltes Wogengewand", "tokens": ["Und", "schl\u00e4gt", "sein", "wild", "ent\u00b7fes\u00b7sel\u00b7tes", "Wo\u00b7gen\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Rings um zeittrotzender Felsen erzene Panzer.", "tokens": ["Rings", "um", "zeit\u00b7trot\u00b7zen\u00b7der", "Fel\u00b7sen", "er\u00b7ze\u00b7ne", "Pan\u00b7zer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.17": {"text": "Bald furchen des Blitzstrahls flammende Schwerter die zitternden L\u00fcfte,", "tokens": ["Bald", "fur\u00b7chen", "des", "Blitz\u00b7strahls", "flam\u00b7men\u00b7de", "Schwer\u00b7ter", "die", "zit\u00b7tern\u00b7den", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Mit tosendem Donnerschlage die Wetterlawinen zusammen.", "tokens": ["Mit", "to\u00b7sen\u00b7dem", "Don\u00b7ner\u00b7schla\u00b7ge", "die", "Wet\u00b7ter\u00b7la\u00b7wi\u00b7nen", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Und zum angsterbebenden Erdball sauset", "tokens": ["Und", "zum", "angs\u00b7ter\u00b7be\u00b7ben\u00b7den", "Erd\u00b7ball", "sau\u00b7set"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "--+-+--+-++", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Des Hagels schneidender Geiselhieb nieder. \u2014", "tokens": ["Des", "Ha\u00b7gels", "schnei\u00b7den\u00b7der", "Gei\u00b7sel\u00b7hieb", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Wahrlich, o Menschheit, durch deine Tempel, durch deine Seelen", "tokens": ["Wahr\u00b7lich", ",", "o", "Menschheit", ",", "durch", "dei\u00b7ne", "Tem\u00b7pel", ",", "durch", "dei\u00b7ne", "See\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Mu\u00df auch also jagen ein machtvoll zertr\u00fcmmerndes Wettergew\u00f6lk,", "tokens": ["Mu\u00df", "auch", "al\u00b7so", "ja\u00b7gen", "ein", "macht\u00b7voll", "zer\u00b7tr\u00fcm\u00b7mern\u00b7des", "Wet\u00b7ter\u00b7ge\u00b7w\u00f6lk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+--+", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "In den Staub mu\u00dft du sinken", "tokens": ["In", "den", "Staub", "mu\u00dft", "du", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Und niederbeugen die hoffart-eitele Stirn;", "tokens": ["Und", "nie\u00b7der\u00b7beu\u00b7gen", "die", "hoff\u00b7ar\u00b7tei\u00b7te\u00b7le", "Stirn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Beim S\u00fcndenmahle jauchzest du,", "tokens": ["Beim", "S\u00fcn\u00b7den\u00b7mah\u00b7le", "jauch\u00b7zest", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Bl\u00fcthenumkr\u00e4nzt, duftberauscht,", "tokens": ["Bl\u00fc\u00b7the\u00b7num\u00b7kr\u00e4nzt", ",", "duft\u00b7be\u00b7rauscht", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Als ekle Dirne wird verfeilscht", "tokens": ["Als", "ek\u00b7le", "Dir\u00b7ne", "wird", "ver\u00b7feilscht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Jed\u2019 himmelentsprossenes Gut", "tokens": ["Jed'", "him\u00b7me\u00b7lents\u00b7pros\u00b7se\u00b7nes", "Gut"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.29": {"text": "Und schwerer Tag um Tag", "tokens": ["Und", "schwe\u00b7rer", "Tag", "um", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "W\u00e4lzest du die Frevellast dir auf die Seele.", "tokens": ["W\u00e4l\u00b7zest", "du", "die", "Fre\u00b7vel\u00b7last", "dir", "auf", "die", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.31": {"text": "\u2014 Aber ich will dich rei\u00dfen aus matter S\u00fcndenverkommni\u00df,", "tokens": ["A\u00b7ber", "ich", "will", "dich", "rei\u00b7\u00dfen", "aus", "mat\u00b7ter", "S\u00fcn\u00b7den\u00b7ver\u00b7komm\u00b7ni\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Hebe empor dein erdw\u00e4rts gewendetes Auge,", "tokens": ["He\u00b7be", "em\u00b7por", "dein", "erd\u00b7w\u00e4rts", "ge\u00b7wen\u00b7de\u00b7tes", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.33": {"text": "Heb\u2019 es empor zu den wankenden Bergen,", "tokens": ["Heb'", "es", "em\u00b7por", "zu", "den", "wan\u00b7ken\u00b7den", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.34": {"text": "Siehe, wie die Blitze zerschmelzen der Felsen eiserne G\u00fcrtel; \u2014", "tokens": ["Sie\u00b7he", ",", "wie", "die", "Blit\u00b7ze", "zer\u00b7schmel\u00b7zen", "der", "Fel\u00b7sen", "ei\u00b7ser\u00b7ne", "G\u00fcr\u00b7tel", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "$,", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+-+--+-", "measure": "amphibrach.tetra.plus"}, "line.35": {"text": "Ja, mit Sturmgebrause werd\u2019 ich \u00fcber dich kommen,", "tokens": ["Ja", ",", "mit", "Sturm\u00b7ge\u00b7brau\u00b7se", "werd'", "ich", "\u00fc\u00b7ber", "dich", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "NN", "VAFIN", "PPER", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Mit Sturmgebrause aus verfluchter S\u00fcndennacht dich zwingen,", "tokens": ["Mit", "Sturm\u00b7ge\u00b7brau\u00b7se", "aus", "ver\u00b7fluch\u00b7ter", "S\u00fcn\u00b7den\u00b7nacht", "dich", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.37": {"text": "Mehr sollst du zittern dann als das schwankende Rohr", "tokens": ["Mehr", "sollst", "du", "zit\u00b7tern", "dann", "als", "das", "schwan\u00b7ken\u00b7de", "Rohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "VVFIN", "ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Am windge\u00f6ffneten Ufer!", "tokens": ["Am", "wind\u00b7ge\u00f6ff\u00b7ne\u00b7ten", "U\u00b7fer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Durch mich wird dir das Heil das Herz durchleuchten,", "tokens": ["Durch", "mich", "wird", "dir", "das", "Heil", "das", "Herz", "durch\u00b7leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-