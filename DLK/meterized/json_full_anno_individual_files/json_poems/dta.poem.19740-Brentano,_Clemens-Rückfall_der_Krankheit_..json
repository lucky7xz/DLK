{"dta.poem.19740": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckfall der Krankheit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich denn sterben,               ", "tokens": ["Soll", "ich", "denn", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bin noch so jung?", "tokens": ["Bin", "noch", "so", "jung", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wenn das mein Vater w\u00fc\u00dft,", "tokens": ["Wenn", "das", "mein", "Va\u00b7ter", "w\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich schon sterben m\u00fc\u00dft,", "tokens": ["Da\u00df", "ich", "schon", "ster\u00b7ben", "m\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er th\u00e4t sich kr\u00e4nken", "tokens": ["Er", "th\u00e4t", "sich", "kr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bis in den Tod.", "tokens": ["Bis", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "++-+", "measure": "iambic.di"}, "line.7": {"text": "Wenn es die Mutter w\u00fc\u00dft,", "tokens": ["Wenn", "es", "die", "Mut\u00b7ter", "w\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wenn es die Schwester w\u00fc\u00dft,", "tokens": ["Wenn", "es", "die", "Schwes\u00b7ter", "w\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Th\u00e4ten sich h\u00e4rmen", "tokens": ["Th\u00e4\u00b7ten", "sich", "h\u00e4r\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Bis in den Tod.", "tokens": ["Bis", "in", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "++-+", "measure": "iambic.di"}, "line.11": {"text": "Wenn es mein M\u00e4del w\u00fc\u00dft,", "tokens": ["Wenn", "es", "mein", "M\u00e4\u00b7del", "w\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Da\u00df ich schon sterben m\u00fc\u00dft,", "tokens": ["Da\u00df", "ich", "schon", "ster\u00b7ben", "m\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Sie th\u00e4t sich kr\u00e4nken", "tokens": ["Sie", "th\u00e4t", "sich", "kr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Mit mir ins Grab.", "tokens": ["Mit", "mir", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}