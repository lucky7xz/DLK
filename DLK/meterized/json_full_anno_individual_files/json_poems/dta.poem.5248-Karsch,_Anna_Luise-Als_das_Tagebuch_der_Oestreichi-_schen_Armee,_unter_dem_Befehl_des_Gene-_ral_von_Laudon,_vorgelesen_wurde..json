{"dta.poem.5248": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Als das Tagebuch der Oestreichi-  \n schen Armee, unter dem Befehl des Gene-  \n ral von Laudon, vorgelesen wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Dich, grosser Friedrich, sing ich nicht;", "tokens": ["Dich", ",", "gros\u00b7ser", "Fried\u00b7rich", ",", "sing", "ich", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NE", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df deine Feinde ruhig bleiben,", "tokens": ["Da\u00df", "dei\u00b7ne", "Fein\u00b7de", "ru\u00b7hig", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihre Furchtsamkeit beschreiben,", "tokens": ["Und", "ih\u00b7re", "Furcht\u00b7sam\u00b7keit", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist dein Lobgedicht!", "tokens": ["Das", "ist", "dein", "Lob\u00b7ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}