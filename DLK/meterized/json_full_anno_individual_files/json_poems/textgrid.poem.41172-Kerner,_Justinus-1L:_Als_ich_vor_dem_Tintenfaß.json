{"textgrid.poem.41172": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich vor dem Tintenfa\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich vor dem Tintenfa\u00df", "tokens": ["Als", "ich", "vor", "dem", "Tin\u00b7ten\u00b7fa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder mit der Feder sa\u00df", "tokens": ["Wie\u00b7der", "mit", "der", "Fe\u00b7der", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Und mit solcher tief gestochen", "tokens": ["Und", "mit", "sol\u00b7cher", "tief", "ge\u00b7sto\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Tinte bis zum Satz,", "tokens": ["In", "die", "Tin\u00b7te", "bis", "zum", "Satz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kam etwas heraufgekrochen,", "tokens": ["Kam", "et\u00b7was", "her\u00b7auf\u00b7ge\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie der Schwanz von einer Katz'.", "tokens": ["Wie", "der", "Schwanz", "von", "ei\u00b7ner", "Katz'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mir doch ward es immer b\u00e4nger,", "tokens": ["Mir", "doch", "ward", "es", "im\u00b7mer", "b\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn das Ding wurd' immer l\u00e4nger,", "tokens": ["Denn", "das", "Ding", "wurd'", "im\u00b7mer", "l\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Gar zu lang f\u00fcr eine Maus,", "tokens": ["Gar", "zu", "lang", "f\u00fcr", "ei\u00b7ne", "Maus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und der Teufel kroch heraus.", "tokens": ["Und", "der", "Teu\u00b7fel", "kroch", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.11": {"text": "Erst macht er drei Reverenzen,", "tokens": ["Erst", "macht", "er", "drei", "Re\u00b7ve\u00b7ren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "CARD", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.12": {"text": "Schlingend mit dem Schwanze Ringe,", "tokens": ["Schlin\u00b7gend", "mit", "dem", "Schwan\u00b7ze", "Rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und erz\u00e4hlt mir Wunderdinge", "tokens": ["Und", "er\u00b7z\u00e4hlt", "mir", "Wun\u00b7der\u00b7din\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Von sich, um vor mir zu gl\u00e4nzen,", "tokens": ["Von", "sich", ",", "um", "vor", "mir", "zu", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "$,", "KOUI", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df er einst gewesen sei", "tokens": ["Da\u00df", "er", "einst", "ge\u00b7we\u00b7sen", "sei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VAPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "In Neapels Hofkanzlei.", "tokens": ["In", "Nea\u00b7pels", "Hof\u00b7kanz\u00b7lei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "\u00bbjetzt bin ich (Sie werden's merken)\u00ab,", "tokens": ["\u00bb", "jetzt", "bin", "ich", "(", "Sie", "wer\u00b7den's", "mer\u00b7ken", ")", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "$(", "PPER", "VAFIN", "VVINF", "$(", "$(", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.18": {"text": "Spricht er, \u00bbnun an andrer Stelle,", "tokens": ["Spricht", "er", ",", "\u00bb", "nun", "an", "an\u00b7drer", "Stel\u00b7le", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "$(", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "(jedem wird nach seinen Werken),", "tokens": ["(", "je\u00b7dem", "wird", "nach", "sei\u00b7nen", "Wer\u00b7ken", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIS", "VAFIN", "APPR", "PPOSAT", "NN", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ein klein wenig in der H\u00f6lle.", "tokens": ["Ein", "klein", "we\u00b7nig", "in", "der", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Einstens war ich gro\u00df und reich,", "tokens": ["Eins\u00b7tens", "war", "ich", "gro\u00df", "und", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Jetzt, um's kurz zu sagen gleich,", "tokens": ["Jetzt", ",", "um's", "kurz", "zu", "sa\u00b7gen", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "ADJD", "PTKZU", "VVINF", "ADV", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.23": {"text": "Bin ich zwar ein armer Schlucker,", "tokens": ["Bin", "ich", "zwar", "ein", "ar\u00b7mer", "Schlu\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Doch ein emsiger Geselle", "tokens": ["Doch", "ein", "em\u00b7si\u00b7ger", "Ge\u00b7sel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Und der Druckerschw\u00e4rze Reiber", "tokens": ["Und", "der", "Dru\u00b7cker\u00b7schw\u00e4r\u00b7ze", "Rei\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Von des Satans Hofbuchdrucker,", "tokens": ["Von", "des", "Sa\u00b7tans", "Hof\u00b7buch\u00b7dru\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wollte Ihnen sagen schnell:", "tokens": ["Woll\u00b7te", "Ih\u00b7nen", "sa\u00b7gen", "schnell", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Da\u00df f\u00fcr schwarze H\u00f6llenleiber", "tokens": ["Da\u00df", "f\u00fcr", "schwar\u00b7ze", "H\u00f6l\u00b7len\u00b7lei\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Ihre Tinte ist zu hell,", "tokens": ["Ih\u00b7re", "Tin\u00b7te", "ist", "zu", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Werde, um sie schwarz zu frischen,", "tokens": ["Wer\u00b7de", ",", "um", "sie", "schwarz", "zu", "fri\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUI", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Sie mit Druckerschw\u00e4rze mischen.\u00ab \u2013", "tokens": ["Sie", "mit", "Dru\u00b7cker\u00b7schw\u00e4r\u00b7ze", "mi\u00b7schen", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "\u00bbfort!\u00ab rief ich, vor Zorn ganz bla\u00df,", "tokens": ["\u00bb", "fort", "!", "\u00ab", "rief", "ich", ",", "vor", "Zorn", "ganz", "bla\u00df", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "VVFIN", "PPER", "$,", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "\u00bbmeinst du nicht, ich merk' nicht, da\u00df", "tokens": ["\u00bb", "meinst", "du", "nicht", ",", "ich", "merk'", "nicht", ",", "da\u00df"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Du der vor'ge Teufel, nur", "tokens": ["Du", "der", "vor'\u00b7ge", "Teu\u00b7fel", ",", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Mit ver\u00e4nderter Figur,", "tokens": ["Mit", "ver\u00b7\u00e4n\u00b7der\u00b7ter", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.36": {"text": "Der hinaus zum Schornstein fuhr.", "tokens": ["Der", "hin\u00b7aus", "zum", "Schorn\u00b7stein", "fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Lie\u00df' ich mich vom Zorn hinrei\u00dfen,", "tokens": ["Lie\u00df'", "ich", "mich", "vom", "Zorn", "hin\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "W\u00fcrd' ich dir das Tintenfa\u00df", "tokens": ["W\u00fcrd'", "ich", "dir", "das", "Tin\u00b7ten\u00b7fa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Doch genug f\u00fcr dich ist \u2013 ", "tokens": ["Doch", "ge\u00b7nug", "f\u00fcr", "dich", "ist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VAFIN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.40": {"text": "Drauf hab' ich ein Kreuz geschlagen,", "tokens": ["Drauf", "hab'", "ich", "ein", "Kreuz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.41": {"text": "Was die Teufel nicht ertragen,", "tokens": ["Was", "die", "Teu\u00b7fel", "nicht", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Da ward schnell er d\u00fcnner noch,", "tokens": ["Da", "ward", "schnell", "er", "d\u00fcn\u00b7ner", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PPER", "ADJD", "ADV", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.43": {"text": "D\u00fcnner als der Spinne Waden,", "tokens": ["D\u00fcn\u00b7ner", "als", "der", "Spin\u00b7ne", "Wa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Und als schwarzer, h\u00e4r'ger Faden", "tokens": ["Und", "als", "schwar\u00b7zer", ",", "h\u00e4r'\u00b7ger", "Fa\u00b7den"], "token_info": ["word", "w