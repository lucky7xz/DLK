{"dta.poem.21836": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n  Das mi\u00dftrauliche Alter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Wo der Teufel nicht kommt hin/", "tokens": ["Wo", "der", "Teu\u00b7fel", "nicht", "kommt", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKNEG", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "mu\u00df er alte Weiber senden/", "tokens": ["mu\u00df", "er", "al\u00b7te", "Wei\u00b7ber", "sen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "jezo st\u00fcnd\u2019 erf\u00fclle mein Sinn/", "tokens": ["je\u00b7zo", "st\u00fcnd'", "er\u00b7f\u00fcl\u00b7le", "mein", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und das Gl\u00fckk\u2019 in meinen H\u00e4nden/", "tokens": ["und", "das", "Gl\u00fc\u00b7kk'", "in", "mei\u00b7nen", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "--++-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "kommt ein alter H\u00f6llen-Hund", "tokens": ["kommt", "ein", "al\u00b7ter", "H\u00f6l\u00b7len\u00b7Hund"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "und verst\u00f6rt mir alles Wesen.", "tokens": ["und", "ver\u00b7st\u00f6rt", "mir", "al\u00b7les", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In Avernus roten Schlund", "tokens": ["In", "A\u00b7ver\u00b7nus", "ro\u00b7ten", "Schlund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "mit dem d\u00fcrren Donnerbesen.", "tokens": ["mit", "dem", "d\u00fcr\u00b7ren", "Don\u00b7ner\u00b7be\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alter schimpfft zwar niemand nicht/", "tokens": ["Al\u00b7ter", "schimpfft", "zwar", "nie\u00b7mand", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PIS", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wo es nu den Jungen traute/", "tokens": ["wo", "es", "nu", "den", "Jun\u00b7gen", "trau\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wo sein sorgliches Gesicht/", "tokens": ["wo", "sein", "sorg\u00b7li\u00b7ches", "Ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Sch\u00f6nen zarter Mund", "tokens": ["Mei\u00b7ner", "Sch\u00f6\u00b7nen", "zar\u00b7ter", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "fiel auff mich mit tausend K\u00fcssen/", "tokens": ["fiel", "auff", "mich", "mit", "tau\u00b7send", "K\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "APPR", "CARD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "was mir weiter war vergunnt/", "tokens": ["was", "mir", "wei\u00b7ter", "war", "ver\u00b7gunnt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "mu\u00df ich um der Alten missen.", "tokens": ["mu\u00df", "ich", "um", "der", "Al\u00b7ten", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kunnstu denn nicht di\u00dfmahl ruhn/", "tokens": ["Kunns\u00b7tu", "denn", "nicht", "di\u00df\u00b7mahl", "ruhn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "ADV", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df du uns zerreist die Karten?", "tokens": ["da\u00df", "du", "uns", "zer\u00b7reist", "die", "Kar\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "hastu weiter nichts zu tuhn/", "tokens": ["has\u00b7tu", "wei\u00b7ter", "nichts", "zu", "tuhn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "nicht der Spindel abzuwarten?", "tokens": ["nicht", "der", "Spin\u00b7del", "ab\u00b7zu\u00b7war\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Flikk den alten Belz vielmehr", "tokens": ["Flikk", "den", "al\u00b7ten", "Belz", "viel\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "und bestell das Todten-Hemde.", "tokens": ["und", "be\u00b7stell", "das", "Tod\u00b7ten\u00b7Hem\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was verbeusstu/ da\u00df wol ehr", "tokens": ["Was", "ver\u00b7beus\u00b7stu", "/", "da\u00df", "wol", "ehr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "dir nicht ist gewesen fremde.", "tokens": ["dir", "nicht", "ist", "ge\u00b7we\u00b7sen", "frem\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VAFIN", "VAPP", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df die Jugend fr\u00f6lich sein/", "tokens": ["La\u00df", "die", "Ju\u00b7gend", "fr\u00f6\u00b7lich", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJD", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "weil die Geister noch sich r\u00fchren.", "tokens": ["weil", "die", "Geis\u00b7ter", "noch", "sich", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Wangen fallen ein", "tokens": ["Wenn", "die", "Wan\u00b7gen", "fal\u00b7len", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und die Z\u00e4hne sich verlieren/", "tokens": ["und", "die", "Z\u00e4h\u00b7ne", "sich", "ver\u00b7lie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn die Brust verwelket steht/", "tokens": ["wenn", "die", "Brust", "ver\u00b7wel\u00b7ket", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "und der Glieder Blut erkaltet", "tokens": ["und", "der", "Glie\u00b7der", "Blut", "er\u00b7kal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "aller Muht zu dr\u00fcmmern geht", "tokens": ["al\u00b7ler", "Muht", "zu", "dr\u00fcm\u00b7mern", "geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "nnd der ganze Leib veraltet.", "tokens": ["nnd", "der", "gan\u00b7ze", "Leib", "ver\u00b7al\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}