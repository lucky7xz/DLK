{"dta.poem.13507": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "IiI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was willst auf dieser Station", "tokens": ["Was", "willst", "auf", "die\u00b7ser", "Sta\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So breit dich niederlassen!", "tokens": ["So", "breit", "dich", "nie\u00b7der\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie bald nicht bl\u00e4st der Postillon,", "tokens": ["Wie", "bald", "nicht", "bl\u00e4st", "der", "Pos\u00b7til\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du mu\u00dft doch alles lassen.", "tokens": ["Du", "mu\u00dft", "doch", "al\u00b7les", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}