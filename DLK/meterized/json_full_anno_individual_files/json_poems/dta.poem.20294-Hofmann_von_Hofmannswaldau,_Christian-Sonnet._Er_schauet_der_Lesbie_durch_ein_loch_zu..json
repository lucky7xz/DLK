{"dta.poem.20294": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n Er schauet der Lesbie durch ein  \n loch zu.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es dachte Lesbie sie s\u00e4sse gantz allein/", "tokens": ["Es", "dach\u00b7te", "Les\u00b7bie", "sie", "s\u00e4s\u00b7se", "gantz", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PPER", "VVFIN", "ADV", "ADV", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Indem sie wohl verwahrt die fenster und die th\u00fcren;", "tokens": ["In\u00b7dem", "sie", "wohl", "ver\u00b7wahrt", "die", "fens\u00b7ter", "und", "die", "th\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch lie\u00df sich Sylvius den geilen f\u00fcrwitz f\u00fchren/", "tokens": ["Doch", "lie\u00df", "sich", "Syl\u00b7vius", "den", "gei\u00b7len", "f\u00fcr\u00b7witz", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NE", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und schaute durch ein loch in ihr gemach hinein.", "tokens": ["Und", "schau\u00b7te", "durch", "ein", "loch", "in", "ihr", "ge\u00b7mach", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADV", "APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auff ihrem lincken knie lag ihr das rechte bein/", "tokens": ["Auff", "ih\u00b7rem", "lin\u00b7cken", "knie", "lag", "ihr", "das", "rech\u00b7te", "bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die hand war h\u00f6chst bem\u00fcht/ den schuch ihr zuzuschn\u00fcren/", "tokens": ["Die", "hand", "war", "h\u00f6chst", "be\u00b7m\u00fcht", "/", "den", "schuch", "ihr", "zu\u00b7zu\u00b7schn\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$(", "ART", "VVFIN", "PPER", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er schaute/ wie der mo\u00df zinnober wei\u00df zu zieren/", "tokens": ["Er", "schau\u00b7te", "/", "wie", "der", "mo\u00df", "zin\u00b7no\u00b7ber", "wei\u00df", "zu", "zie\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "ART", "NN", "ADV", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wo Cupido will mit lust gewieget seyn.", "tokens": ["Und", "wo", "Cu\u00b7pi\u00b7do", "will", "mit", "lust", "ge\u00b7wie\u00b7get", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "VMFIN", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es ruffte Sylvius: wie zierlich sind die waden", "tokens": ["Es", "ruff\u00b7te", "Syl\u00b7vius", ":", "wie", "zier\u00b7lich", "sind", "die", "wa\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "$.", "PWAV", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Mit warmen schnee bedeckt/ mit helffenbein beladen!", "tokens": ["Mit", "war\u00b7men", "schnee", "be\u00b7deckt", "/", "mit", "helf\u00b7fen\u00b7bein", "be\u00b7la\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$(", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er sahe selbst den ort/ wo seine hoffnung stund.", "tokens": ["Er", "sa\u00b7he", "selbst", "den", "ort", "/", "wo", "sei\u00b7ne", "hoff\u00b7nung", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es lachte Sylvius/ sie sprach: du bist verlohren/", "tokens": ["Es", "lach\u00b7te", "Syl\u00b7vius", "/", "sie", "sprach", ":", "du", "bist", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$(", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Zum schmertzen bist du dir/ und mir zur pein erkohren:", "tokens": ["Zum", "schmert\u00b7zen", "bist", "du", "dir", "/", "und", "mir", "zur", "pein", "er\u00b7koh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "VAFIN", "PPER", "PPER", "$(", "KON", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Denn deine hoffnung hat ja gar zu schlechten grund.", "tokens": ["Denn", "dei\u00b7ne", "hoff\u00b7nung", "hat", "ja", "gar", "zu", "schlech\u00b7ten", "grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}