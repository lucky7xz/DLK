{"textgrid.poem.49872": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Inschrift auf eine Uhr mit den drei Horen", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am langsamsten von allen G\u00f6ttern wandeln wir,", "tokens": ["Am", "lang\u00b7sams\u00b7ten", "von", "al\u00b7len", "G\u00f6t\u00b7tern", "wan\u00b7deln", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit Bl\u00e4tterkronen sch\u00f6n geschm\u00fcckte, schweigsame.", "tokens": ["Mit", "Bl\u00e4t\u00b7ter\u00b7kro\u00b7nen", "sch\u00f6n", "ge\u00b7schm\u00fcck\u00b7te", ",", "schweig\u00b7sa\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wer uns ehrt und wem wir selber g\u00fcnstig sind,", "tokens": ["Doch", "wer", "uns", "ehrt", "und", "wem", "wir", "sel\u00b7ber", "g\u00fcns\u00b7tig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "KON", "PWS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil er die Anmut liebet und das heilge Ma\u00df,", "tokens": ["Weil", "er", "die", "An\u00b7mut", "lie\u00b7bet", "und", "das", "heil\u00b7ge", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vor dessen Augen schweben wir im leichten Tanz", "tokens": ["Vor", "des\u00b7sen", "Au\u00b7gen", "schwe\u00b7ben", "wir", "im", "leich\u00b7ten", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und machen mannigfaltig ihm den langen Tag.", "tokens": ["Und", "ma\u00b7chen", "man\u00b7nig\u00b7fal\u00b7tig", "ihm", "den", "lan\u00b7gen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Am langsamsten von allen G\u00f6ttern wandeln wir,", "tokens": ["Am", "lang\u00b7sams\u00b7ten", "von", "al\u00b7len", "G\u00f6t\u00b7tern", "wan\u00b7deln", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit Bl\u00e4tterkronen sch\u00f6n geschm\u00fcckte, schweigsame.", "tokens": ["Mit", "Bl\u00e4t\u00b7ter\u00b7kro\u00b7nen", "sch\u00f6n", "ge\u00b7schm\u00fcck\u00b7te", ",", "schweig\u00b7sa\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wer uns ehrt und wem wir selber g\u00fcnstig sind,", "tokens": ["Doch", "wer", "uns", "ehrt", "und", "wem", "wir", "sel\u00b7ber", "g\u00fcns\u00b7tig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "KON",