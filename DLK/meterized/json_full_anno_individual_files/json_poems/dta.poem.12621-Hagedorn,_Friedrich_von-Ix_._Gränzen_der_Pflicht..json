{"dta.poem.12621": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Ix .  \n  Gr\u00e4nzen der Pflicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus Beyfall und gewohnten Gr\u00fcnden", "tokens": ["Aus", "Bey\u00b7fall", "und", "ge\u00b7wohn\u00b7ten", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Menschen recht vern\u00fcnftig finden,", "tokens": ["Nur", "Men\u00b7schen", "recht", "ver\u00b7n\u00fcnf\u00b7tig", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das will die Pflicht:", "tokens": ["Das", "will", "die", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Doch manche Menschen, die wir kennen,", "tokens": ["Doch", "man\u00b7che", "Men\u00b7schen", ",", "die", "wir", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Viel kl\u00fcger, als die Thiere, nennen,", "tokens": ["Viel", "kl\u00fc\u00b7ger", ",", "als", "die", "Thie\u00b7re", ",", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das will sie nicht.", "tokens": ["Das", "will", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die seltnen F\u00fcrsten G\u00f6tter heissen,", "tokens": ["Die", "selt\u00b7nen", "F\u00fcrs\u00b7ten", "G\u00f6t\u00b7ter", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sich der Menschen-Huld befleissen,", "tokens": ["Die", "sich", "der", "Men\u00b7schen\u00b7Huld", "be\u00b7fleis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das will die Pflicht:", "tokens": ["Das", "will", "die", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Doch die mit G\u00f6tter-Namen zieren,", "tokens": ["Doch", "die", "mit", "G\u00f6t\u00b7ter\u00b7Na\u00b7men", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die weibisch oder wild regieren,", "tokens": ["Die", "wei\u00b7bisch", "o\u00b7der", "wild", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das will sie nicht.", "tokens": ["Das", "will", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Nicht widersprechen und sich schmiegen,", "tokens": ["Nicht", "wi\u00b7der\u00b7spre\u00b7chen", "und", "sich", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "KON", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wann grosse M\u00e4nner pr\u00e4chtig l\u00fcgen,", "tokens": ["Wann", "gros\u00b7se", "M\u00e4n\u00b7ner", "pr\u00e4ch\u00b7tig", "l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das will die Pflicht:", "tokens": ["Das", "will", "die", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Doch glauben was sie uns erzehlen,", "tokens": ["Doch", "glau\u00b7ben", "was", "sie", "uns", "er\u00b7zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch glauben wo Beweise fehlen,", "tokens": ["Doch", "glau\u00b7ben", "wo", "Be\u00b7wei\u00b7se", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das will sie nicht.", "tokens": ["Das", "will", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der Neuern Kunst und Witz verehren,", "tokens": ["Der", "Neu\u00b7ern", "Kunst", "und", "Witz", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zumahl wann sie durch Muster lehren,", "tokens": ["Zu\u00b7mahl", "wann", "sie", "durch", "Mus\u00b7ter", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das will die Pflicht:", "tokens": ["Das", "will", "die", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Allein den grossen Geist der Alten", "tokens": ["Al\u00b7lein", "den", "gros\u00b7sen", "Geist", "der", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr unsrer Zeiten Antheil halten,", "tokens": ["F\u00fcr", "uns\u00b7rer", "Zei\u00b7ten", "An\u00b7theil", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das will sie nicht.", "tokens": ["Das", "will", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Der Welt das Wasser anzupreisen,", "tokens": ["Der", "Welt", "das", "Was\u00b7ser", "an\u00b7zu\u00b7prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erlaubt man Aerzten oder Weisen,", "tokens": ["Er\u00b7laubt", "man", "A\u00b7erz\u00b7ten", "o\u00b7der", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das will die Pflicht:", "tokens": ["Das", "will", "die", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Allein des Vorrangs dich berauben,", "tokens": ["Al\u00b7lein", "des", "Vor\u00b7rangs", "dich", "be\u00b7rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du freudenvoller Saft der Trauben!", "tokens": ["Du", "freu\u00b7den\u00b7vol\u00b7ler", "Saft", "der", "Trau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das will sie nicht.", "tokens": ["Das", "will", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Die frommen Blicke nicht verschm\u00e4hen,", "tokens": ["Die", "from\u00b7men", "Bli\u00b7cke", "nicht", "ver\u00b7schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo wir nur Zucht und Unschuld sehen,", "tokens": ["Wo", "wir", "nur", "Zucht", "und", "Un\u00b7schuld", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das will die Pflicht:", "tokens": ["Das", "will", "die", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Doch deren Vorzugs-Recht verkennen,", "tokens": ["Doch", "de\u00b7ren", "Vor\u00b7zugs\u00b7Recht", "ver\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In welchen Lust und Jugend brennen,", "tokens": ["In", "wel\u00b7chen", "Lust", "und", "Ju\u00b7gend", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das will sie nicht.", "tokens": ["Das", "will", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Die scharfen M\u00fctter nicht belachen,", "tokens": ["Die", "schar\u00b7fen", "M\u00fct\u00b7ter", "nicht", "be\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die schlaue T\u00f6chter stets bewachen,", "tokens": ["Die", "schlau\u00b7e", "T\u00f6ch\u00b7ter", "stets", "be\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das will die Pflicht:", "tokens": ["Das", "will", "die", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Allein der T\u00f6chter List verrathen,", "tokens": ["Al\u00b7lein", "der", "T\u00f6ch\u00b7ter", "List", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die das thun was die M\u00fctter thaten,", "tokens": ["Die", "das", "thun", "was", "die", "M\u00fct\u00b7ter", "tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "VVINF", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das will sie nicht.", "tokens": ["Das", "will", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Den Alten, die uns bessern k\u00f6nnen,", "tokens": ["Den", "Al\u00b7ten", ",", "die", "uns", "bes\u00b7sern", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mehr Zehenden an Jahren g\u00f6nnen,", "tokens": ["Mehr", "Ze\u00b7hen\u00b7den", "an", "Jah\u00b7ren", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das will die Pflicht:", "tokens": ["Das", "will", "die", "Pflicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Allein zu ihrem l\u00e4ngern Leben", "tokens": ["Al\u00b7lein", "zu", "ih\u00b7rem", "l\u00e4n\u00b7gern", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von unserm eine Stunde geben,", "tokens": ["Von", "un\u00b7serm", "ei\u00b7ne", "Stun\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das will sie nicht.", "tokens": ["Das", "will", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}