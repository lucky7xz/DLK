{"dta.poem.74": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An den Herrn  Autorem.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wo reine Liebes-Glut den holden Scepter f\u00fchret/", "tokens": ["Wo", "rei\u00b7ne", "Lie\u00b7bes\u00b7Glut", "den", "hol\u00b7den", "Scep\u00b7ter", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ist die Tichterey ein treuer Unterthan/", "tokens": ["Da", "ist", "die", "Tich\u00b7te\u00b7rey", "ein", "treu\u00b7er", "Un\u00b7ter\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wo dieselbe erst nur das Clavier ber\u00fchret/", "tokens": ["Und", "wo", "die\u00b7sel\u00b7be", "erst", "nur", "das", "Cla\u00b7vier", "be\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PDAT", "ADV", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Da giebet jene gleich den Thon vernehmlich an.", "tokens": ["Da", "gie\u00b7bet", "je\u00b7ne", "gleich", "den", "Thon", "ver\u00b7nehm\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Weil seine Leber Ey\u00df/ das Hertze eisern ist", "tokens": ["Weil", "sei\u00b7ne", "Le\u00b7ber", "Ey\u00df", "/", "das", "Hert\u00b7ze", "ei\u00b7sern", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$(", "PDS", "VVFIN", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Wenn er ein sch\u00f6nes Weib nur erst einmahl gek\u00fcsst.", "tokens": ["Wenn", "er", "ein", "sch\u00f6\u00b7nes", "Weib", "nur", "erst", "ein\u00b7mahl", "ge\u00b7k\u00fcsst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Freund/ es zeugt dein Buch von Lieben und von Wissen/", "tokens": ["Mein", "Freund", "/", "es", "zeugt", "dein", "Buch", "von", "Lie\u00b7ben", "und", "von", "Wis\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Apollo r\u00fchmt den Flei\u00df/ er kennt dein Wissen wol.", "tokens": ["A\u00b7pol\u00b7lo", "r\u00fchmt", "den", "Flei\u00df", "/", "er", "kennt", "dein", "Wis\u00b7sen", "wol", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.11": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Da\u00df dich vor deine M\u00e4h ein jeder lieben soll.", "tokens": ["Da\u00df", "dich", "vor", "dei\u00b7ne", "M\u00e4h", "ein", "je\u00b7der", "lie\u00b7ben", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}