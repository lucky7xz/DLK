{"textgrid.poem.52268": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Ein Lied vom Tode", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf den H\u00fcgeln steht er im Morgenrot,", "tokens": ["Auf", "den", "H\u00fc\u00b7geln", "steht", "er", "im", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Das gez\u00fcckte Schwert in der sehn'gen Hand.", "tokens": ["Das", "ge\u00b7z\u00fcck\u00b7te", "Schwert", "in", "der", "sehn'\u00b7gen", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbwer ich bin? Ich bin der Befreiertod!", "tokens": ["\u00bb", "wer", "ich", "bin", "?", "Ich", "bin", "der", "Be\u00b7frei\u00b7er\u00b7tod", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "VAFIN", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bin der Tod f\u00fcr die Menschheit, das Vaterland!", "tokens": ["Bin", "der", "Tod", "f\u00fcr", "die", "Menschheit", ",", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Der den Greis und das Kind auf die Bahre legt \u2013", "tokens": ["Der", "den", "Greis", "und", "das", "Kind", "auf", "die", "Bah\u00b7re", "legt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KON", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Nein, der eiserne St\u00fcrmer im Kampfgew\u00fchl,", "tokens": ["Nein", ",", "der", "ei\u00b7ser\u00b7ne", "St\u00fcr\u00b7mer", "im", "Kampf\u00b7ge\u00b7w\u00fchl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Der den Mann und den trotzigen J\u00fcngling erschl\u00e4gt!", "tokens": ["Der", "den", "Mann", "und", "den", "trot\u00b7zi\u00b7gen", "J\u00fcng\u00b7ling", "er\u00b7schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Unterm blauen lustigen Himmelszelt,", "tokens": ["Un\u00b7term", "blau\u00b7en", "lus\u00b7ti\u00b7gen", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da durchflieg' ich, da licht' ich die jauchzenden Reihn;", "tokens": ["Da", "durch\u00b7flieg'", "ich", ",", "da", "licht'", "ich", "die", "jauch\u00b7zen\u00b7den", "Reihn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Da werf' ich sie hin auf das Ackerfeld,", "tokens": ["Da", "wer\u00b7f'", "ich", "sie", "hin", "auf", "das", "A\u00b7cker\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Auf die Blumenflur, auf den Pflasterstein!", "tokens": ["Auf", "die", "Blu\u00b7men\u00b7flur", ",", "auf", "den", "Pflas\u00b7ter\u00b7stein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "O, wie stirbt es sich sch\u00f6n in der Kraft, im Zorn:", "tokens": ["O", ",", "wie", "stirbt", "es", "sich", "sch\u00f6n", "in", "der", "Kraft", ",", "im", "Zorn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Sie liegen, emporgewandt den Blick;", "tokens": ["Sie", "lie\u00b7gen", ",", "em\u00b7por\u00b7ge\u00b7wandt", "den", "Blick", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sie liegen, die Todeswunde vorn", "tokens": ["Sie", "lie\u00b7gen", ",", "die", "To\u00b7des\u00b7wun\u00b7de", "vorn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und das bleiche, blutige Haupt im Genick!", "tokens": ["Und", "das", "blei\u00b7che", ",", "blu\u00b7ti\u00b7ge", "Haupt", "im", "Ge\u00b7nick", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "So lagen die Tapfern an Wien und Spree;", "tokens": ["So", "la\u00b7gen", "die", "Tap\u00b7fern", "an", "Wi\u00b7en", "und", "Spree", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "KON", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So lagen die Turner am Eiderflu\u00df;", "tokens": ["So", "la\u00b7gen", "die", "Tur\u00b7ner", "am", "Ei\u00b7der\u00b7flu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "So lagen auf jener Schwarzwaldh\u00f6h'", "tokens": ["So", "la\u00b7gen", "auf", "je\u00b7ner", "Schwa\u00b7rz\u00b7wald\u00b7h\u00f6h'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Freistaatm\u00e4nner, gef\u00e4llt vom Schu\u00df.", "tokens": ["Die", "Frei\u00b7staat\u00b7m\u00e4n\u00b7ner", ",", "ge\u00b7f\u00e4llt", "vom", "Schu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So liegen und lagen sie hundertweis,", "tokens": ["So", "lie\u00b7gen", "und", "la\u00b7gen", "sie", "hun\u00b7dert\u00b7weis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die der M\u00e4rz gefordert und der April;", "tokens": ["Die", "der", "M\u00e4rz", "ge\u00b7for\u00b7dert", "und", "der", "Ap\u00b7ril", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "KON", "ART", "NN", "$."], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.7": {"text": "So findet sie liegen die Rose des Mais,", "tokens": ["So", "fin\u00b7det", "sie", "lie\u00b7gen", "die", "Ro\u00b7se", "des", "Mais", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Da\u00df ihr Grab sie bekr\u00e4nze freundlich und still!", "tokens": ["Da\u00df", "ihr", "Grab", "sie", "be\u00b7kr\u00e4n\u00b7ze", "freund\u00b7lich", "und", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Die Rose des Mais! \u2013 Ja, was bringt der Mai?", "tokens": ["Die", "Ro\u00b7se", "des", "Mais", "!", "\u2013", "Ja", ",", "was", "bringt", "der", "Mai", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "$(", "PTKANT", "$,", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich will es euch sagen: Hieb und Stich!", "tokens": ["Ich", "will", "es", "euch", "sa\u00b7gen", ":", "Hieb", "und", "Stich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "VVINF", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich will es euch sagen: Trompetenschrei,", "tokens": ["Ich", "will", "es", "euch", "sa\u00b7gen", ":", "Trom\u00b7pe\u00b7ten\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "VVINF", "$.", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Knatternde Salven und abermals mich!", "tokens": ["Knat\u00b7tern\u00b7de", "Sal\u00b7ven", "und", "a\u00b7ber\u00b7mals", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADV", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Denn ihr sollt euch gr\u00fcndlich und ganz befrein,", "tokens": ["Denn", "ihr", "sollt", "euch", "gr\u00fcnd\u00b7lich", "und", "ganz", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADJD", "KON", "ADV", "PTKVZ", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und das leuchtende Gold, da\u00df die Fahn' euch schm\u00fcckt,", "tokens": ["Und", "das", "leuch\u00b7ten\u00b7de", "Gold", ",", "da\u00df", "die", "Fahn'", "euch", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Sei die ", "tokens": ["Sei", "die"], "token_info": ["word", "word"], "pos": ["VAFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Die des Kammerdieners Livree bestickt!", "tokens": ["Die", "des", "Kam\u00b7mer\u00b7die\u00b7ners", "Liv\u00b7ree", "be\u00b7stickt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NE", "VVPP", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ja, ihr habt, was ihr tatet, nur halb getan! \u2013", "tokens": ["Ja", ",", "ihr", "habt", ",", "was", "ihr", "ta\u00b7tet", ",", "nur", "halb", "ge\u00b7tan", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VVPP", "$.", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wer ist, der die Kugel hemmen darf?", "tokens": ["Wer", "ist", ",", "der", "die", "Ku\u00b7gel", "hem\u00b7men", "darf", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie roll' und sie donnre auf ihrer Bahn,", "tokens": ["Sie", "roll'", "und", "sie", "donn\u00b7re", "auf", "ih\u00b7rer", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Bis sie ", "tokens": ["Bis", "sie"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Euch hei\u00dft 'Rebell' der entschiedne Mann,", "tokens": ["Euch", "hei\u00dft", "'", "Re\u00b7bell", "'", "der", "ent\u00b7schied\u00b7ne", "Mann", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NE", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der die ", "tokens": ["Der", "die"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Ei, wie man so bald vergessen kann,", "tokens": ["Ei", ",", "wie", "man", "so", "bald", "ver\u00b7ges\u00b7sen", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PIS", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Da\u00df von ", "tokens": ["Da\u00df", "von"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "'demokratische Basis', die 'breiteste' gar!", "tokens": ["'", "de\u00b7mo\u00b7kra\u00b7ti\u00b7sche", "Ba\u00b7sis", "'", ",", "die", "'", "brei\u00b7tes\u00b7te", "'", "gar", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "PRELS", "NE", "$(", "$,", "PRELS", "$(", "ADJA", "$(", "ADV", "$."], "meter": "--+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "'parlament' und 'Verfassung', 'Kaiser und Reich'!", "tokens": ["'", "par\u00b7la\u00b7ment", "'", "und", "'", "Ver\u00b7fas\u00b7sung", "'", ",", "'", "Kai\u00b7ser", "und", "Reich", "'", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "$(", "KON", "$(", "NN", "$(", "$,", "$(", "NN", "KON", "NN", "$(", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Von dem allen ist nur das eine klar:", "tokens": ["Von", "dem", "al\u00b7len", "ist", "nur", "das", "ei\u00b7ne", "klar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "VAFIN", "ADV", "ART", "ART", "ADJD", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Einer 'Basis' bed\u00fcrft ihr \u2013 ja wohl, ", "tokens": ["Ei\u00b7ner", "'", "Ba\u00b7sis", "'", "be\u00b7d\u00fcrft", "ihr", "\u2013", "ja", "wohl", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "$(", "NE", "$(", "VVFIN", "PPER", "$(", "ADV", "ADV", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Eines 'breitesten', drauf ihr breit euch macht!", "tokens": ["Ei\u00b7nes", "'", "brei\u00b7tes\u00b7ten", "'", ",", "drauf", "ihr", "breit", "euch", "macht", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "VVINF", "$(", "$,", "PAV", "PPER", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ihr wollt nur ein Jahr, das wie Drei\u00dfig blitzt \u2013", "tokens": ["Ihr", "wollt", "nur", "ein", "Jahr", ",", "das", "wie", "Drei\u00b7\u00dfig", "blitzt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "$,", "PRELS", "KOKOM", "CARD", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Ihr wollt kein Gewitter von Vierzig und acht!", "tokens": ["Ihr", "wollt", "kein", "Ge\u00b7wit\u00b7ter", "von", "Vier\u00b7zig", "und", "acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPR", "CARD", "KON", "CARD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Doch wir ", "tokens": ["Doch", "wir"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Und das Wetter ist da, und ihr haltet's nicht auf!", "tokens": ["Und", "das", "Wet\u00b7ter", "ist", "da", ",", "u