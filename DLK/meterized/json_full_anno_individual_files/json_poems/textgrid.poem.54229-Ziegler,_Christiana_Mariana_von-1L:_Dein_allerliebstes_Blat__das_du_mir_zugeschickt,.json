{"textgrid.poem.54229": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein allerliebstes Blat / das du mir zugeschickt,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein allerliebstes Blat / das du mir zugeschickt,", "tokens": ["Dein", "al\u00b7ler\u00b7liebs\u00b7tes", "Blat", "/", "das", "du", "mir", "zu\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat mein erstorbenes Hertz recht wiederum erquickt.", "tokens": ["Hat", "mein", "ers\u00b7tor\u00b7be\u00b7nes", "Hertz", "recht", "wie\u00b7de\u00b7rum", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Mu\u00df ich gleich, liebstes Kind, von dir nach frembden L\u00e4ndern,", "tokens": ["Mu\u00df", "ich", "gleich", ",", "liebs\u00b7tes", "Kind", ",", "von", "dir", "nach", "fremb\u00b7den", "L\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird doch die frembde Lufft mein vorges Hertz nicht \u00e4ndern,", "tokens": ["Wird", "doch", "die", "fremb\u00b7de", "Lufft", "mein", "vor\u00b7ges", "Hertz", "nicht", "\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil \u00fcberall dein Knecht die Fessel mit sich tr\u00e4gt,", "tokens": ["Weil", "\u00fc\u00b7be\u00b7rall", "dein", "Knecht", "die", "Fes\u00b7sel", "mit", "sich", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die deine Lieblichkeit ihm l\u00e4ngstens angelegt,", "tokens": ["Die", "dei\u00b7ne", "Lieb\u00b7lich\u00b7keit", "ihm", "l\u00e4ngs\u00b7tens", "an\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er bleibet dir getreu und wird der Deine heissen,", "tokens": ["Er", "blei\u00b7bet", "dir", "ge\u00b7treu", "und", "wird", "der", "Dei\u00b7ne", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VAFIN", "ART", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja Pluto mag mein Hertz in tausend St\u00fccken reissen.", "tokens": ["Ja", "Plu\u00b7to", "mag", "mein", "Hertz", "in", "tau\u00b7send", "St\u00fc\u00b7cken", "reis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "VMFIN", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dafern es Sch\u00f6nste, dir nicht gantz allein verbleibt", "tokens": ["Da\u00b7fern", "es", "Sch\u00f6ns\u00b7te", ",", "dir", "nicht", "gantz", "al\u00b7lein", "ver\u00b7bleibt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "$,", "PPER", "PTKNEG", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und bis ins kalte Grab sich dir ergeben schreibt.", "tokens": ["Und", "bis", "ins", "kal\u00b7te", "Grab", "sich", "dir", "er\u00b7ge\u00b7ben", "schreibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "PRF", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach fasse dich daher, verbanne Gram und Schmertzen,", "tokens": ["Ach", "fas\u00b7se", "dich", "da\u00b7her", ",", "ver\u00b7ban\u00b7ne", "Gram", "und", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PRF", "PAV", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verbei\u00df der Z\u00e4hren Saltz, liebst du mich noch von Hertzen.", "tokens": ["Ver\u00b7bei\u00df", "der", "Z\u00e4h\u00b7ren", "Saltz", ",", "liebst", "du", "mich", "noch", "von", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein Schmertz verdoppelt sich sonst durch der Thr\u00e4nen See,", "tokens": ["Mein", "Schmertz", "ver\u00b7dop\u00b7pelt", "sich", "sonst", "durch", "der", "Thr\u00e4\u00b7nen", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und deinen Seladon geschieht dadurch zu weh.", "tokens": ["Und", "dei\u00b7nen", "Se\u00b7la\u00b7don", "ge\u00b7schieht", "da\u00b7durch", "zu", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PAV", "PTKZU", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was hilfft die Wehmuth dir? was nutzen deine Klagen?", "tokens": ["Was", "hilfft", "die", "Weh\u00b7muth", "dir", "?", "was", "nut\u00b7zen", "dei\u00b7ne", "Kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man mu\u00df des Himmels Schlu\u00df, mein Kind, gedultig tragen.", "tokens": ["Man", "mu\u00df", "des", "Him\u00b7mels", "Schlu\u00df", ",", "mein", "Kind", ",", "ge\u00b7dul\u00b7tig", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "NN", "$,", "PPOSAT", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich leide mehr als du, erweg ich meine Noth,", "tokens": ["Ich", "lei\u00b7de", "mehr", "als", "du", ",", "er\u00b7weg", "ich", "mei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "PPER", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und gienge, glaub es mir, viel lieber in den Tod.", "tokens": ["Und", "gien\u00b7ge", ",", "glaub", "es", "mir", ",", "viel", "lie\u00b7ber", "in", "den", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich soll, bedenck es selbst, dein sch\u00f6nes Auge missen", "tokens": ["Ich", "soll", ",", "be\u00b7denck", "es", "selbst", ",", "dein", "sch\u00f6\u00b7nes", "Au\u00b7ge", "mis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und dich im Bilde nur, nicht mehr pers\u00f6nlich, k\u00fcssen.", "tokens": ["Und", "dich", "im", "Bil\u00b7de", "nur", ",", "nicht", "mehr", "per\u00b7s\u00f6n\u00b7lich", ",", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "ADV", "$,", "PTKNEG", "ADV", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Entsetzlicher Verlust! Raub, dem nichts gleichen kan!", "tokens": ["Ent\u00b7setz\u00b7li\u00b7cher", "Ver\u00b7lust", "!", "Raub", ",", "dem", "nichts", "glei\u00b7chen", "kan", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Wie gerne, d\u00fcrfft ich nur, g\u00e4b ich mein Reisen an!", "tokens": ["Wie", "ger\u00b7ne", ",", "d\u00fcrfft", "ich", "nur", ",", "g\u00e4b", "ich", "mein", "Rei\u00b7sen", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "VMFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn deines gleichen zeigt mir wohl kein Theil der Erden,", "tokens": ["Denn", "dei\u00b7nes", "glei\u00b7chen", "zeigt", "mir", "wohl", "kein", "Theil", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "PPER", "ADV", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Drum dencke, wie mir mu\u00df darbey zu Muthe werden,", "tokens": ["Drum", "den\u00b7cke", ",", "wie", "mir", "mu\u00df", "dar\u00b7bey", "zu", "Mu\u00b7the", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "PAV", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da Post und Horn mich rufft, davon das Ohr mir g\u00e4llt,", "tokens": ["Da", "Post", "und", "Horn", "mich", "rufft", ",", "da\u00b7von", "das", "Ohr", "mir", "g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$,", "PAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weil meiner Seuffzer Schall zugleich darein mit f\u00e4llt.", "tokens": ["Weil", "mei\u00b7ner", "Seuff\u00b7zer", "Schall", "zu\u00b7gleich", "da\u00b7rein", "mit", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADV", "PAV", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mein Hertz wird Centner schwer; ists m\u00f6glich da\u00df ein Wagen", "tokens": ["Mein", "Hertz", "wird", "Cent\u00b7ner", "schwer", ";", "ists", "m\u00f6g\u00b7lich", "da\u00df", "ein", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADJD", "$.", "VAFIN", "ADJD", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dergleichen Lasten kan so weit auf Achsen tragen?", "tokens": ["Derg\u00b7lei\u00b7chen", "Las\u00b7ten", "kan", "so", "weit", "auf", "Ach\u00b7sen", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VMFIN", "ADV", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein Engel, lebe wohl, nunmehro fahr ich fort,", "tokens": ["Mein", "En\u00b7gel", ",", "le\u00b7be", "wohl", ",", "nun\u00b7meh\u00b7ro", "fahr", "ich", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die Feder sinckt mir hin, ich kan von Schmertz kein Wort", "tokens": ["Die", "Fe\u00b7der", "sinckt", "mir", "hin", ",", "ich", "kan", "von", "Schmertz", "kein", "Wort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VMFIN", "APPR", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Zu deiner Lindrung mehr und Trost als so viel schreiben:", "tokens": ["Zu", "dei\u00b7ner", "Lind\u00b7rung", "mehr", "und", "Trost", "als", "so", "viel", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "KON", "NN", "KOKOM", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dein Seladon wird dir auf ewig treu verbleiben.", "tokens": ["Dein", "Se\u00b7la\u00b7don", "wird", "dir", "auf", "e\u00b7wig", "treu", "ver\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "APPR", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein allerliebstes Blat / das du mir zugeschickt,", "tokens": ["Dein", "al\u00b7ler\u00b7liebs\u00b7tes", "Blat", "/", "das", "du", "mir", "zu\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat mein erstorbenes Hertz recht wiederum erquickt.", "tokens": ["Hat", "mein", "ers\u00b7tor\u00b7be\u00b7nes", "Hertz", "recht", "wie\u00b7de\u00b7rum", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Mu\u00df ich gleich, liebstes Kind, von dir nach frembden L\u00e4ndern,", "tokens": ["Mu\u00df", "ich", "gleich", ",", "liebs\u00b7tes", "Kind", ",", "von", "dir", "nach", "fremb\u00b7den", "L\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird doch die frembde Lufft mein vorges Hertz nicht \u00e4ndern,", "tokens": ["Wird", "doch", "die", "fremb\u00b7de", "Lufft", "mein", "vor\u00b7ges", "Hertz", "nicht", "\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil \u00fcberall dein Knecht die Fessel mit sich tr\u00e4gt,", "tokens": ["Weil", "\u00fc\u00b7be\u00b7rall", "dein", "Knecht", "die", "Fes\u00b7sel", "mit", "sich", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die deine Lieblichkeit ihm l\u00e4ngstens angelegt,", "tokens": ["Die", "dei\u00b7ne", "Lieb\u00b7lich\u00b7keit", "ihm", "l\u00e4ngs\u00b7tens", "an\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er bleibet dir getreu und wird der Deine heissen,", "tokens": ["Er", "blei\u00b7bet", "dir", "ge\u00b7treu", "und", "wird", "der", "Dei\u00b7ne", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VAFIN", "ART", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja Pluto mag mein Hertz in tausend St\u00fccken reissen.", "tokens": ["Ja", "Plu\u00b7to", "mag", "mein", "Hertz", "in", "tau\u00b7send", "St\u00fc\u00b7cken", "reis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "VMFIN", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dafern es Sch\u00f6nste, dir nicht gantz allein verbleibt", "tokens": ["Da\u00b7fern", "es", "Sch\u00f6ns\u00b7te", ",", "dir", "nicht", "gantz", "al\u00b7lein", "ver\u00b7bleibt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "$,", "PPER", "PTKNEG", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und bis ins kalte Grab sich dir ergeben schreibt.", "tokens": ["Und", "bis", "ins", "kal\u00b7te", "Grab", "sich", "dir", "er\u00b7ge\u00b7ben", "schreibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "PRF", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach fasse dich daher, verbanne Gram und Schmertzen,", "tokens": ["Ach", "fas\u00b7se", "dich", "da\u00b7her", ",", "ver\u00b7ban\u00b7ne", "Gram", "und", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PRF", "PAV", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verbei\u00df der Z\u00e4hren Saltz, liebst du mich noch von Hertzen.", "tokens": ["Ver\u00b7bei\u00df", "der", "Z\u00e4h\u00b7ren", "Saltz", ",", "liebst", "du", "mich", "noch", "von", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein Schmertz verdoppelt sich sonst durch der Thr\u00e4nen See,", "tokens": ["Mein", "Schmertz", "ver\u00b7dop\u00b7pelt", "sich", "sonst", "durch", "der", "Thr\u00e4\u00b7nen", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und deinen Seladon geschieht dadurch zu weh.", "tokens": ["Und", "dei\u00b7nen", "Se\u00b7la\u00b7don", "ge\u00b7schieht", "da\u00b7durch", "zu", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PAV", "PTKZU", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was hilfft die Wehmuth dir? was nutzen deine Klagen?", "tokens": ["Was", "hilfft", "die", "Weh\u00b7muth", "dir", "?", "was", "nut\u00b7zen", "dei\u00b7ne", "Kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man mu\u00df des Himmels Schlu\u00df, mein Kind, gedultig tragen.", "tokens": ["Man", "mu\u00df", "des", "Him\u00b7mels", "Schlu\u00df", ",", "mein", "Kind", ",", "ge\u00b7dul\u00b7tig", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "NN", "$,", "PPOSAT", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich leide mehr als du, erweg ich meine Noth,", "tokens": ["Ich", "lei\u00b7de", "mehr", "als", "du", ",", "er\u00b7weg", "ich", "mei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "PPER", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und gienge, glaub es mir, viel lieber in den Tod.", "tokens": ["Und", "gien\u00b7ge", ",", "glaub", "es", "mir", ",", "viel", "lie\u00b7ber", "in", "den", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$,", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich soll, bedenck es selbst, dein sch\u00f6nes Auge missen", "tokens": ["Ich", "soll", ",", "be\u00b7denck", "es", "selbst", ",", "dein", "sch\u00f6\u00b7nes", "Au\u00b7ge", "mis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und dich im Bilde nur, nicht mehr pers\u00f6nlich, k\u00fcssen.", "tokens": ["Und", "dich", "im", "Bil\u00b7de", "nur", ",", "nicht", "mehr", "per\u00b7s\u00f6n\u00b7lich", ",", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "ADV", "$,", "PTKNEG", "ADV", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Entsetzlicher Verlust! Raub, dem nichts gleichen kan!", "tokens": ["Ent\u00b7setz\u00b7li\u00b7cher", "Ver\u00b7lust", "!", "Raub", ",", "dem", "nichts", "glei\u00b7chen", "kan", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Wie gerne, d\u00fcrfft ich nur, g\u00e4b ich mein Reisen an!", "tokens": ["Wie", "ger\u00b7ne", ",", "d\u00fcrfft", "ich", "nur", ",", "g\u00e4b", "ich", "mein", "Rei\u00b7sen", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "VMFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn deines gleichen zeigt mir wohl kein Theil der Erden,", "tokens": ["Denn", "dei\u00b7nes", "glei\u00b7chen", "zeigt", "mir", "wohl", "kein", "Theil", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "PPER", "ADV", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Drum dencke, wie mir mu\u00df darbey zu Muthe werden,", "tokens": ["Drum", "den\u00b7cke", ",", "wie", "mir", "mu\u00df", "dar\u00b7bey", "zu", "Mu\u00b7the", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "PAV", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da Post und Horn mich rufft, davon das Ohr mir g\u00e4llt,", "tokens": ["Da", "Post", "und", "Horn", "mich", "rufft", ",", "da\u00b7von", "das", "Ohr", "mir", "g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$,", "PAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weil meiner Seuffzer Schall zugleich darein mit f\u00e4llt.", "tokens": ["Weil", "mei\u00b7ner", "Seuff\u00b7zer", "Schall", "zu\u00b7gleich", "da\u00b7rein", "mit", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADV", "PAV", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mein Hertz wird Centner schwer; ists m\u00f6glich da\u00df ein Wagen", "tokens": ["Mein", "Hertz", "wird", "Cent\u00b7ner", "schwer", ";", "ists", "m\u00f6g\u00b7lich", "da\u00df", "ein", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADJD", "$.", "VAFIN", "ADJD", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dergleichen Lasten kan so weit auf Achsen tragen?", "tokens": ["Derg\u00b7lei\u00b7chen", "Las\u00b7ten", "kan", "so", "weit", "auf", "Ach\u00b7sen", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VMFIN", "ADV", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein Engel, lebe wohl, nunmehro fahr ich fort,", "tokens": ["Mein", "En\u00b7gel", ",", "le\u00b7be", "wohl", ",", "nun\u00b7meh\u00b7ro", "fahr", "ich", "fort", ","], "token_i