{"dta.poem.10607": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma an die Asterien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ob schon dein rother Mundt ist einer Rosen gleich/", "tokens": ["Ob", "schon", "dein", "ro\u00b7ther", "Mundt", "ist", "ei\u00b7ner", "Ro\u00b7sen", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo er wirdt andre sich zuk\u00fcssen vnderfangen/", "tokens": ["Wo", "er", "wirdt", "and\u00b7re", "sich", "zu\u00b7k\u00fcs\u00b7sen", "vn\u00b7der\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "PIS", "PRF", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So w\u00fcnsch ich/ da\u00df er doch werd also wei\u00df vnd bleich/", "tokens": ["So", "w\u00fcnsch", "ich", "/", "da\u00df", "er", "doch", "werd", "al\u00b7so", "wei\u00df", "vnd", "bleich", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "VAFIN", "ADV", "VVFIN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als mir von Liebes Pein sein worden meine Wangen.", "tokens": ["Als", "mir", "von", "Lie\u00b7bes", "Pein", "sein", "wor\u00b7den", "mei\u00b7ne", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VAINF", "VAPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}