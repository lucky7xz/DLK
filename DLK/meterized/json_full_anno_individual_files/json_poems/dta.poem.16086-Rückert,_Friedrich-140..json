{"dta.poem.16086": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "140.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wirst du beide los, die dich zudringlich plagen?", "tokens": ["Wie", "wirst", "du", "bei\u00b7de", "los", ",", "die", "dich", "zu\u00b7dring\u00b7lich", "pla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PIS", "PTKVZ", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sag jedem: schon hab' ichs dem andern abgeschlagen;", "tokens": ["Sag", "je\u00b7dem", ":", "schon", "hab'", "ichs", "dem", "an\u00b7dern", "ab\u00b7ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$.", "ADV", "VAFIN", "PIS", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn ichs dir gew\u00e4hrt', er w\u00fcrd' es \u00fcbel nehmen.", "tokens": ["Und", "wenn", "ichs", "dir", "ge\u00b7w\u00e4hrt'", ",", "er", "w\u00fcrd'", "es", "\u00fc\u00b7bel", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "VVPP", "$,", "PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So werden alle zwei zum Abzug sich bequemen.", "tokens": ["So", "wer\u00b7den", "al\u00b7le", "zwei", "zum", "Ab\u00b7zug", "sich", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "p