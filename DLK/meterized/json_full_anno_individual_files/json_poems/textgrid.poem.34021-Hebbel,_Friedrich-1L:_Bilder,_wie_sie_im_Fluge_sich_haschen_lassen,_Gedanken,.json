{"textgrid.poem.34021": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bilder, wie sie im Fluge sich haschen lassen, Gedanken,", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bilder, wie sie im Fluge sich haschen lassen, Gedanken,", "tokens": ["Bil\u00b7der", ",", "wie", "sie", "im", "Flu\u00b7ge", "sich", "ha\u00b7schen", "las\u00b7sen", ",", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "APPRART", "NN", "PRF", "VVINF", "VVINF", "$,", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Welche sich runden in sich, mancher geschichtliche Strich,", "tokens": ["Wel\u00b7che", "sich", "run\u00b7den", "in", "sich", ",", "man\u00b7cher", "ge\u00b7schicht\u00b7li\u00b7che", "Strich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADJD", "APPR", "PRF", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Auch zuweilen ein Hauch, der, leise schwellend, den Busen", "tokens": ["Auch", "zu\u00b7wei\u00b7len", "ein", "Hauch", ",", "der", ",", "lei\u00b7se", "schwel\u00b7lend", ",", "den", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "PRELS", "$,", "ADJD", "VVPP", "$,", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Hebt und wieder verl\u00e4\u00dft, eh' er ein Lied noch beseelt,", "tokens": ["Hebt", "und", "wie\u00b7der", "ver\u00b7l\u00e4\u00dft", ",", "eh'", "er", "ein", "Lied", "noch", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Und dazwischen, doch selten, die K\u00f6pfe von Schelmen und Wichten,", "tokens": ["Und", "da\u00b7zwi\u00b7schen", ",", "doch", "sel\u00b7ten", ",", "die", "K\u00f6p\u00b7fe", "von", "Schel\u00b7men", "und", "Wich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "ADV", "ADJD", "$,", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Wie man, genagelt an's Thor, Eulen und Dohlen erblickt,", "tokens": ["Wie", "man", ",", "ge\u00b7na\u00b7gelt", "an's", "Thor", ",", "Eu\u00b7len", "und", "Doh\u00b7len", "er\u00b7blickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "VVPP", "APPRART", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Alles aber im Vers, wie Schiller und Goethe ihn bauten,", "tokens": ["Al\u00b7les", "a\u00b7ber", "im", "Vers", ",", "wie", "Schil\u00b7ler", "und", "Goe\u00b7the", "ihn", "bau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPRART", "NN", "$,", "PWAV", "NE", "KON", "NE", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Schm\u00e4h'n ihn auch Platen und Vo\u00df, weil er der deutscheste ist!", "tokens": ["Schm\u00e4h'n", "ihn", "auch", "Pla\u00b7ten", "und", "Vo\u00df", ",", "weil", "er", "der", "deut\u00b7sches\u00b7te", "ist", "!"], "token_info