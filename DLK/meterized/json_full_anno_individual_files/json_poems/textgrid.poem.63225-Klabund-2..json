{"textgrid.poem.63225": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch sind voll gr\u00fcnem Laube die Platanen.", "tokens": ["Noch", "sind", "voll", "gr\u00fc\u00b7nem", "Lau\u00b7be", "die", "Pla\u00b7ta\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Reben h\u00e4ngen an den St\u00f6cken schwer.", "tokens": ["Die", "Re\u00b7ben", "h\u00e4n\u00b7gen", "an", "den", "St\u00f6\u00b7cken", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Menschen frieren in den Eisenbahnen", "tokens": ["Die", "Men\u00b7schen", "frie\u00b7ren", "in", "den", "Ei\u00b7sen\u00b7bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Voll Ahnung fr\u00fchen Winters allzusehr.", "tokens": ["Voll", "Ah\u00b7nung", "fr\u00fc\u00b7hen", "Win\u00b7ters", "all\u00b7zu\u00b7sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja: morgen ist die letzte Traubenlesung;", "tokens": ["Ja", ":", "mor\u00b7gen", "ist", "die", "letz\u00b7te", "Trau\u00b7ben\u00b7le\u00b7sung", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "VAFIN", "ART",