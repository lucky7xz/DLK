{"dta.poem.19776": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Iv.   Die Feder im Bart .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht aber lang zu dieser Zeit                 ", "tokens": ["Nicht", "a\u00b7ber", "lang", "zu", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im ganzen Land ist gro\u00dfe Freud,", "tokens": ["Im", "gan\u00b7zen", "Land", "ist", "gro\u00b7\u00dfe", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Herzog Heinrich ist zur\u00fcck,", "tokens": ["Der", "Her\u00b7zog", "Hein\u00b7rich", "ist", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat gest\u00f6rt der Freier Gl\u00fcck,", "tokens": ["Und", "hat", "ge\u00b7st\u00f6rt", "der", "Frei\u00b7er", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nach dem Me\u00dfhau\u00df in der Stadt,", "tokens": ["Und", "nach", "dem", "Me\u00df\u00b7hau\u00df", "in", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er allen Adel zu sich bat.", "tokens": ["Er", "al\u00b7len", "A\u00b7del", "zu", "sich", "bat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch Thedel kam im neuen Kleid,", "tokens": ["Auch", "The\u00b7del", "kam", "im", "neu\u00b7en", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Herzog ihn erkannt von weit,", "tokens": ["Der", "Her\u00b7zog", "ihn", "er\u00b7kannt", "von", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Auch gab ihm seine Gnad die Hand,", "tokens": ["Auch", "gab", "ihm", "sei\u00b7ne", "Gnad", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und dankte ihm, wie allbebkannt.", "tokens": ["Und", "dank\u00b7te", "ihm", ",", "wie", "all\u00b7beb\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie assen, tranken allzumal,", "tokens": ["Sie", "as\u00b7sen", ",", "tran\u00b7ken", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und waren guter Ding im Saal,", "tokens": ["Und", "wa\u00b7ren", "gu\u00b7ter", "Ding", "im", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Auch \u00fcber Essen ward gesungen,", "tokens": ["Auch", "\u00fc\u00b7ber", "Es\u00b7sen", "ward", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Darnach gerungen und gesprungen,", "tokens": ["Dar\u00b7nach", "ge\u00b7run\u00b7gen", "und", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Getanzt, gefochten und tornirt,", "tokens": ["Ge\u00b7tanzt", ",", "ge\u00b7foch\u00b7ten", "und", "tor\u00b7nirt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Auf Trommel und auf Pfeif hofirt;", "tokens": ["Auf", "Trom\u00b7mel", "und", "auf", "Pfeif", "ho\u00b7firt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Herr Thedel wollt dabey stets seyn,", "tokens": ["Herr", "The\u00b7del", "wollt", "da\u00b7bey", "stets", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "PAV", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und sollts ihm kosten Arm und Bein.", "tokens": ["Und", "sollts", "ihm", "kos\u00b7ten", "Arm", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Im Rennen, Torniern und Stechen,", "tokens": ["Im", "Ren\u00b7nen", ",", "Tor\u00b7ni\u00b7ern", "und", "Ste\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Im Schwerdt und Spie\u00df zerbrechen", "tokens": ["Im", "Schwerdt", "und", "Spie\u00df", "zer\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Ward keiner mehr gesehen,", "tokens": ["Ward", "kei\u00b7ner", "mehr", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Der ihn noch wollt bestehen.", "tokens": ["Der", "ihn", "noch", "wollt", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Es rief ein jeder Edelmann,", "tokens": ["Es", "rief", "ein", "je\u00b7der", "E\u00b7del\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Da\u00df er das beste hab gethan.", "tokens": ["Da\u00df", "er", "das", "bes\u00b7te", "hab", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Der Herzog gab ein Kleinod fein,", "tokens": ["Der", "Her\u00b7zog", "gab", "ein", "Klei\u00b7nod", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Gemacht aus Gold und Edelstein,", "tokens": ["Ge\u00b7macht", "aus", "Gold", "und", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und sagt, da\u00df er Gefallen hab", "tokens": ["Und", "sagt", ",", "da\u00df", "er", "Ge\u00b7fal\u00b7len", "hab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "An seinem Ro\u00df, schwarz wie ein Rab,", "tokens": ["An", "sei\u00b7nem", "Ro\u00df", ",", "schwarz", "wie", "ein", "Rab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Weil er von seinem schwarzen Pferd", "tokens": ["Weil", "er", "von", "sei\u00b7nem", "schwar\u00b7zen", "Pferd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Noch nie gefallen auf die Erd.", "tokens": ["Noch", "nie", "ge\u00b7fal\u00b7len", "auf", "die", "Erd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Herr Thedel sprach: \u201eEs ist dies Pferd", "tokens": ["Herr", "The\u00b7del", "sprach", ":", "\u201e", "Es", "ist", "dies", "Pferd"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "\u201eweils Nachricht bracht der F\u00fcrstin werth,", "tokens": ["\u201e", "weils", "Nach\u00b7richt", "bracht", "der", "F\u00fcrs\u00b7tin", "werth", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "\u201evon euch Herr Herzog mir sehr theuer,", "tokens": ["\u201e", "von", "euch", "Herr", "Her\u00b7zog", "mir", "sehr", "theu\u00b7er", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "NN", "NE", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "\u201edrum hassens ihre R\u00e4th und Freyer.\u201c", "tokens": ["\u201e", "drum", "has\u00b7sens", "ih\u00b7re", "R\u00e4\u00b7th", "und", "Frey\u00b7er", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "ADV", "PPOSAT", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.35": {"text": "Der F\u00fcrst fing ihn zu loben an,", "tokens": ["Der", "F\u00fcrst", "fing", "ihn", "zu", "lo\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und pries ihn da vor jedermann.", "tokens": ["Und", "pries", "ihn", "da", "vor", "je\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Ein Jungfr\u00e4ulein reicht ihm den Kranz", "tokens": ["Ein", "Jung\u00b7fr\u00e4u\u00b7lein", "reicht", "ihm", "den", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.38": {"text": "Und f\u00fchret ihn so drat zum Tanz,", "tokens": ["Und", "f\u00fch\u00b7ret", "ihn", "so", "drat", "zum", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und wie er zu dem Tanz hintrat", "tokens": ["Und", "wie", "er", "zu", "dem", "Tanz", "hin\u00b7trat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Gedacht er in dem Herzen drat:", "tokens": ["Ge\u00b7dacht", "er", "in", "dem", "Her\u00b7zen", "drat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "\u201eich dank dir Gott zu dieser Frist,", "tokens": ["\u201e", "ich", "dank", "dir", "Gott", "zu", "die\u00b7ser", "Frist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "APPR", "PPER", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "\u201eda\u00df du mein H\u00fclf und Tr\u00f6ster bist,", "tokens": ["\u201e", "da\u00df", "du", "mein", "H\u00fclf", "und", "Tr\u00f6s\u00b7ter", "bist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPOSAT", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "\u201eherr Jesu Christ, Lob, Ehr und Preis,", "tokens": ["\u201e", "herr", "Je\u00b7su", "Christ", ",", "Lob", ",", "Ehr", "und", "Preis", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "NE", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.44": {"text": "\u201edem heilgen Geist in gleicher Weis!\u201c", "tokens": ["\u201e", "dem", "heil\u00b7gen", "Geist", "in", "glei\u00b7cher", "Weis", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Als nun der Thedel unverfehrt", "tokens": ["Als", "nun", "der", "The\u00b7del", "un\u00b7ver\u00b7fehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Vor andern ward so hochgeehrt,", "tokens": ["Vor", "an\u00b7dern", "ward", "so", "hoch\u00b7geehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Da ward ein Neider aus dem Freund,", "tokens": ["Da", "ward", "ein", "Nei\u00b7der", "aus", "dem", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Der wollt ihm schlimmer als der Feind,", "tokens": ["Der", "wollt", "ihm", "schlim\u00b7mer", "als", "der", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Der Herzog fragt: \u201eOb Unverfehrt", "tokens": ["Der", "Her\u00b7zog", "fragt", ":", "\u201e", "Ob", "Un\u00b7ver\u00b7fehrt"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NE", "VVFIN", "$.", "$(", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "\u201ewohl irgend zu erschrecken w\u00e4r?", "tokens": ["\u201e", "wohl", "ir\u00b7gend", "zu", "er\u00b7schre\u00b7cken", "w\u00e4r", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Der Neider sprach: Ich hab eins funden,", "tokens": ["Der", "Nei\u00b7der", "sprach", ":", "Ich", "hab", "eins", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Wenn morgen kommt zur Kirch die Stunde", "tokens": ["Wenn", "mor\u00b7gen", "kommt", "zur", "Kirch", "die", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Steckt eine Feder d\u00fcnn und klein", "tokens": ["Steckt", "ei\u00b7ne", "Fe\u00b7der", "d\u00fcnn", "und", "klein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "In eures Bartes Haar hinein,", "tokens": ["In", "eu\u00b7res", "Bar\u00b7tes", "Haar", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-