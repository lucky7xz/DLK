{"textgrid.poem.39257": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "Zueignung", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich die Kl\u00e4nge fassen tiefgewaltig,", "tokens": ["Wenn", "sich", "die", "Kl\u00e4n\u00b7ge", "fas\u00b7sen", "tief\u00b7ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Schlachtendonner himmelanw\u00e4rts streben,", "tokens": ["Im", "Schlach\u00b7ten\u00b7don\u00b7ner", "him\u00b7me\u00b7lan\u00b7w\u00e4rts", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Heldenbilder m\u00e4chtig, traumgestaltig,", "tokens": ["Und", "Hel\u00b7den\u00b7bil\u00b7der", "m\u00e4ch\u00b7tig", ",", "traum\u00b7ge\u00b7stal\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus tiefer Nacht die rost'gen Klingen heben,", "tokens": ["Aus", "tie\u00b7fer", "Nacht", "die", "rost'\u00b7gen", "Klin\u00b7gen", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da zieht durchs Herz leishauchend, liederhaltig,", "tokens": ["Da", "zieht", "durchs", "Herz", "leis\u00b7hau\u00b7chend", ",", "lie\u00b7der\u00b7hal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein tief geheimnisvolles Zauberleben,", "tokens": ["Ein", "tief", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7les", "Zau\u00b7ber\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da treibt mich's auf, der Helden Wert zu singen,", "tokens": ["Da", "treibt", "mich's", "auf", ",", "der", "Hel\u00b7den", "Wert", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$,", "ART", "NN", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Schlacht Getos und zartes Minnedringen.", "tokens": ["Der", "Schlacht", "Ge\u00b7tos", "und", "zar\u00b7tes", "Min\u00b7ne\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nimm denn der Harfe mutig Kind zur Gabe,", "tokens": ["Nimm", "denn", "der", "Har\u00b7fe", "mu\u00b7tig", "Kind", "zur", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du! dem ich weihte meine ersten Tr\u00e4ume,", "tokens": ["Du", "!", "dem", "ich", "weih\u00b7te", "mei\u00b7ne", "ers\u00b7ten", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PRELS", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du Vater, dem der liederfrohe Knabe", "tokens": ["Du", "Va\u00b7ter", ",", "dem", "der", "lie\u00b7der\u00b7fro\u00b7he", "Kna\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So oft gerauscht des Herzens Wellensch\u00e4ume,", "tokens": ["So", "oft", "ge\u00b7rauscht", "des", "Her\u00b7zens", "Wel\u00b7len\u00b7sch\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es ist das Lied des S\u00e4ngers beste Habe,", "tokens": ["Es", "ist", "das", "Lied", "des", "S\u00e4n\u00b7gers", "bes\u00b7te", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein sei die Bl\u00fcte meiner Lenzesb\u00e4ume,", "tokens": ["Dein", "sei", "die", "Bl\u00fc\u00b7te", "mei\u00b7ner", "Len\u00b7zes\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dein sei des Kampfes und des Friedens Weise,", "tokens": ["Dein", "sei", "des", "Kamp\u00b7fes", "und", "des", "Frie\u00b7dens", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dein sei das Gold im blauen Liedergleise.", "tokens": ["Dein", "sei", "das", "Gold", "im", "blau\u00b7en", "Lie\u00b7der