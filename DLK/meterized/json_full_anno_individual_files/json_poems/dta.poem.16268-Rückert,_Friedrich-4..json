{"dta.poem.16268": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Freundes denkend, wenn ich Gl\u00fcckliches erstrebt,", "tokens": ["Des", "Freun\u00b7des", "den\u00b7kend", ",", "wenn", "ich", "Gl\u00fcck\u00b7li\u00b7ches", "er\u00b7strebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprech' ich: O h\u00e4ttest du doch dieses miterlebt!", "tokens": ["Sprech'", "ich", ":", "O", "h\u00e4t\u00b7test", "du", "doch", "die\u00b7ses", "mi\u00b7ter\u00b7lebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "NE", "VAFIN", "PPER", "ADV", "PDAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dann seiner denkend, wenn mich dr\u00fccket eine Last,", "tokens": ["Dann", "sei\u00b7ner", "den\u00b7kend", ",", "wenn", "mich", "dr\u00fc\u00b7cket", "ei\u00b7ne", "Last", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprech' ich: O gl\u00fccklich, da\u00df du's miterlebt nicht hast!", "tokens": ["Sprech'", "ich", ":", "O", "gl\u00fcck\u00b7lich", ",", "da\u00df", "du's", "mi\u00b7ter\u00b7lebt", "nicht", "hast", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "NE", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist zu bedauern, ist zu preisen, wer geschieden?", "tokens": ["Ist", "zu", "be\u00b7dau\u00b7ern", ",", "ist", "zu", "prei\u00b7sen", ",", "wer", "ge\u00b7schie\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKZU", "VVINF", "$,", "VAFIN", "PTKZU", "VVINF", "$,", "PWS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er hingieng und du noch dabist, sei zufrieden!", "tokens": ["Da\u00df", "er", "hin\u00b7gieng", "und", "du", "noch", "da\u00b7bist", ",", "sei", "zu\u00b7frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "PPER", "ADV", "PAV", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}