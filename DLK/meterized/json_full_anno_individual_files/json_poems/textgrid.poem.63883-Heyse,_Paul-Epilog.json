{"textgrid.poem.63883": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Epilog", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was an Weisheit ich erspart, seit ich zu Verstand gekommen,", "tokens": ["Was", "an", "Weis\u00b7heit", "ich", "er\u00b7spart", ",", "seit", "ich", "zu", "Ver\u00b7stand", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "VVPP", "$,", "KOUS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hab' ich gern im Reim bewahrt, nur zu eignem Nutz und Frommen.", "tokens": ["Hab'", "ich", "gern", "im", "Reim", "be\u00b7wahrt", ",", "nur", "zu", "eig\u00b7nem", "Nutz", "und", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Denn so manchen Biedermann wird so manches dran befremden", "tokens": ["Denn", "so", "man\u00b7chen", "Bie\u00b7der\u00b7mann", "wird", "so", "man\u00b7ches", "dran", "be\u00b7frem\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "NN", "VAFIN", "ADV", "PIS", "PAV", "VVINF"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und wohl oft in ihren Bart murmeln \u00bbKetzerei!\u00ab die Frommen.", "tokens": ["Und", "wohl", "oft", "in", "ih\u00b7ren", "Bart", "mur\u00b7meln", "\u00bb", "Ket\u00b7ze\u00b7rei", "!", "\u00ab", "die", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$(", "NN", "$.", "$(", "ART", "NN", "$."], "meter": "--+-+-++-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Doch ein kleines Kapital, das man pr\u00e4gt mit eignem Stempel,", "tokens": ["Doch", "ein", "klei\u00b7nes", "Ka\u00b7pi\u00b7tal", ",", "das", "man", "pr\u00e4gt", "mit", "eig\u00b7nem", "Stem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Nicht nur blind zusammenscharrt, ist zur Zeit der Not willkommen.", "tokens": ["Nicht", "nur", "blind", "zu\u00b7sam\u00b7men\u00b7scharrt", ",", "ist", "zur", "Zeit", "der", "Not", "will\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VVPP", "$,", "VAFIN", "APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Wohl nicht alles wiegt wie Gold, manches ist nur Scheidem\u00fcnze,", "tokens": ["Wohl", "nicht", "al\u00b7les", "wiegt", "wie", "Gold", ",", "man\u00b7ches", "ist", "nur", "Schei\u00b7de\u00b7m\u00fcn\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PIS", "VVFIN", "KOKOM", "NN", "$,", "PIS", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Doch kein falscher Groschen ward wissentlich drin aufgenommen.", "tokens": ["Doch", "kein", "fal\u00b7scher", "Gro\u00b7schen", "ward", "wis\u00b7sent\u00b7lich", "drin", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und so reicht es immerhin, lernte man nur hauszuhalten,", "tokens": ["Und", "so", "reicht", "es", "im\u00b7mer\u00b7hin", ",", "lern\u00b7te", "man", "nur", "haus\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "PIS", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Auf der irdischen Wanderfahrt leidlich damit auszukommen.", "tokens": ["Auf", "der", "ir\u00b7di\u00b7schen", "Wan\u00b7der\u00b7fahrt", "leid\u00b7lich", "da\u00b7mit", "aus\u00b7zu\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "PAV", "VVIZU", "$."], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Was an Weisheit ich erspart, seit ich zu Verstand gekommen,", "tokens": ["Was", "an", "Weis\u00b7heit", "ich", "er\u00b7spart", ",", "seit", "ich", "zu", "Ver\u00b7stand", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "VVPP", "$,", "KOUS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hab' ich gern im Reim bewahrt, nur zu eignem Nutz und Frommen.", "tokens": ["Hab'", "ich", "gern", "im", "Reim", "be\u00b7wahrt", ",", "nur", "zu", "eig\u00b7nem", "Nutz", "und", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "wo