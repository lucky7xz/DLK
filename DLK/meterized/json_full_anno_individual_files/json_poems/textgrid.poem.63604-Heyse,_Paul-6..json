{"textgrid.poem.63604": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch eh' der H\u00fcgel gr\u00fcnt auf deinem Grab,", "tokens": ["Noch", "eh'", "der", "H\u00fc\u00b7gel", "gr\u00fcnt", "auf", "dei\u00b7nem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Eh' jener Kr\u00e4nze bleicher Schmuck vermodert,", "tokens": ["Eh'", "je\u00b7ner", "Kr\u00e4n\u00b7ze", "blei\u00b7cher", "Schmuck", "ver\u00b7mo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die man dir mitgab in die Nacht hinab,", "tokens": ["Die", "man", "dir", "mit\u00b7gab", "in", "die", "Nacht", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie? all die Glut der Schmerzen schon verlodert?", "tokens": ["Wie", "?", "all", "die", "Glut", "der", "Schmer\u00b7zen", "schon", "ver\u00b7lo\u00b7dert", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PIAT", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Augen trocken, k\u00fchl der Herzen Schlag,", "tokens": ["Die", "Au\u00b7gen", "tro\u00b7cken", ",", "k\u00fchl", "der", "Her\u00b7zen", "Schlag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als w\u00e4re nichts geschehn, was Tr\u00e4nen fordert?", "tokens": ["Als", "w\u00e4\u00b7re", "nichts", "ge\u00b7schehn", ",", "was", "Tr\u00e4\u00b7nen", "for\u00b7dert", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIS", "VVPP", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "O ihr, da er noch auf der Bahre lag,", "tokens": ["O", "ihr", ",", "da", "er", "noch", "auf", "der", "Bah\u00b7re", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "An Jammer uners\u00e4ttlich, wie so eilig", "tokens": ["An", "Jam\u00b7mer", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", ",", "wie", "so", "ei\u00b7lig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "$,", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Verleidet' euch das Leid der lust'ge Tag!", "tokens": ["Ver\u00b7lei\u00b7det'", "euch", "das", "Leid", "der", "lust'\u00b7ge", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Im Wechsel euch bet\u00e4uben m\u00fc\u00dft ihr freilich,", "tokens": ["Im", "Wech\u00b7sel", "euch", "be\u00b7t\u00e4u\u00b7ben", "m\u00fc\u00dft", "ihr", "frei\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Denn an die eigne Flachheit mahnt euch bald", "tokens": ["Denn", "an", "die", "eig\u00b7ne", "Flach\u00b7heit", "mahnt", "euch", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ein jedes Wehgef\u00fchl, das tief und heilig.", "tokens": ["Ein", "je\u00b7des", "Weh\u00b7ge\u00b7f\u00fchl", ",", "das", "tief", "und", "hei\u00b7lig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Drum habt ihr eure Spr\u00fcchlein maunigfalt,", "tokens": ["Drum", "habt", "ihr", "eu\u00b7re", "Spr\u00fcch\u00b7lein", "mau\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Daraus ihr lernt: ein Tor, wer nicht genie\u00dfe", "tokens": ["Da\u00b7raus", "ihr", "lernt", ":", "ein", "Tor", ",", "wer", "nicht", "ge\u00b7nie\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PPER", "VVFIN", "$.", "ART", "NN", "$,", "PWS", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Des Augenblicks buntgaukelnde Gestalt,", "tokens": ["Des", "Au\u00b7gen\u00b7blicks", "bunt\u00b7gau\u00b7keln\u00b7de", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Vom Strom nicht trinke, der so rasch verflie\u00dfe,", "tokens": ["Vom", "Strom", "nicht", "trin\u00b7ke", ",", "der", "so", "rasch", "ver\u00b7flie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVFIN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Vers\u00e4umend eines Sonnenblickes Gunst,", "tokens": ["Ver\u00b7s\u00e4u\u00b7mend", "ei\u00b7nes", "Son\u00b7nen\u00b7bli\u00b7ckes", "Gunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Fruchtlosen Gram fest an den Busen schlie\u00dfe.", "tokens": ["Frucht\u00b7lo\u00b7sen", "Gram", "fest", "an", "den", "Bu\u00b7sen", "schlie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.19": {"text": "Dem lebenden Geschlecht sei Gr\u00e4berdunst.", "tokens": ["Dem", "le\u00b7ben\u00b7den", "Ge\u00b7schlecht", "sei", "Gr\u00e4\u00b7ber\u00b7dunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "So geht denn hin und kehrt in schn\u00f6der Eile", "tokens": ["So", "geht", "denn", "hin", "und", "kehrt", "in", "schn\u00f6\u00b7der", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Zu nicht'gem Tagwerk, das euch wichtig scheint,", "tokens": ["Zu", "nicht'\u00b7gem", "Tag\u00b7werk", ",", "das", "euch", "wich\u00b7tig", "scheint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Indes ich still bei meinem Toten weile.", "tokens": ["In\u00b7des", "ich", "still", "bei", "mei\u00b7nem", "To\u00b7ten", "wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ich habe meinen Gram nicht ", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7nen", "Gram", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Wie ihr, nicht aus den Augen ihn versch\u00fcttet;", "tokens": ["Wie", "ihr", ",", "nicht", "aus", "den", "Au\u00b7gen", "ihn", "ver\u00b7sch\u00fct\u00b7tet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PTKNEG", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.25": {"text": "Zu tief mit meinem Blut ist er vereint.", "tokens": ["Zu", "tief", "mit", "mei\u00b7nem", "Blut", "ist", "er", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Nichts hab' ich mehr, das noch zur Not verkittet", "tokens": ["Nichts", "hab'", "ich", "mehr", ",", "das", "noch", "zur", "Not", "ver\u00b7kit\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die St\u00fccke des zerbrochnen Seins, als ihn,", "tokens": ["Die", "St\u00fc\u00b7cke", "des", "zer\u00b7broch\u00b7nen", "Seins", ",", "als", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Der ganz die Seele f\u00fcllt, obschon zerr\u00fcttet.", "tokens": ["Der", "ganz", "die", "See\u00b7le", "f\u00fcllt", ",", "ob\u00b7schon", "zer\u00b7r\u00fct\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,", "KOUS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Nicht will ich feige mir und ihm entfliehn,", "tokens": ["Nicht", "will", "ich", "fei\u00b7ge", "mir", "und", "ihm", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "VVFIN", "PPER", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Will ", "tokens": ["Will"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Da mir der Andacht hohe Kraft verliehn.", "tokens": ["Da", "mir", "der", "An\u00b7dacht", "ho\u00b7he", "Kraft", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Denn Frevel d\u00fcnkt mich, da\u00df man sich entschlage", "tokens": ["Denn", "Fre\u00b7vel", "d\u00fcnkt", "mich", ",", "da\u00df", "man", "sich", "ent\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "KOUS", "PIS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Der Pflicht des Danks, mit Schmerzen ", "tokens": ["Der", "Pflicht", "des", "Danks", ",", "mit", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Die man geliebt mit innigem Herzensschlage.", "tokens": ["Die", "man", "ge\u00b7liebt", "mit", "in\u00b7ni\u00b7gem", "Her\u00b7zens\u00b7schla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.35": {"text": "Die frommen Alten lehrt' es ihr Gewissen,", "tokens": ["Die", "from\u00b7men", "Al\u00b7ten", "lehrt'", "es", "ihr", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Dem Gram sein Recht zu geben, wie der Freude,", "tokens": ["Dem", "Gram", "sein", "Recht", "zu", "ge\u00b7ben", ",", "wie", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und das Volk Gottes hat sein Kleid zerrissen.", "tokens": ["Und", "das", "Volk", "Got\u00b7tes", "hat", "sein", "Kleid", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Nur ihr, die ihr der Selbstsucht Wahngeb\u00e4ude", "tokens": ["Nur", "ihr", ",", "die", "ihr", "der", "Selbst\u00b7sucht", "Wahn\u00b7ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Auft\u00fcrmt, ihr nennt zu kostbar die Sekunde,", "tokens": ["Auf\u00b7t\u00fcrmt", ",", "ihr", "nennt", "zu", "kost\u00b7bar", "die", "Se\u00b7kun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Die man an hoffnungsloses Weh vergeude.", "tokens": ["Die", "man", "an", "hoff\u00b7nungs\u00b7lo\u00b7ses", "Weh", "ver\u00b7geu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "O nun versteh ich, was mit stummem Munde", "tokens": ["O", "nun", "ver\u00b7steh", "ich", ",", "was", "mit", "stum\u00b7mem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Du mir gesagt, mein Liebling, als mit St\u00f6hnen", "tokens": ["Du", "mir", "ge\u00b7sagt", ",", "mein", "Lieb\u00b7ling", ",", "als", "mit", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PPER", "VVPP", "$,", "PPOSAT", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Und Schluchzen dich umgab die dichte Runde.", "tokens": ["Und", "Schluch\u00b7zen", "dich", "um\u00b7gab", "die", "dich\u00b7te", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Dein Schweigen schien ihr Klaggeheul zu h\u00f6hnen,", "tokens": ["Dein", "Schwei\u00b7gen", "schien", "ihr", "Klag\u00b7ge\u00b7heul", "zu", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Als w\u00fc\u00dftest du, der k\u00e4rglichste Gewinn", "tokens": ["Als", "w\u00fc\u00df\u00b7test", "du", ",", "der", "k\u00e4rg\u00b7lichs\u00b7te", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Wird morgen sie mit ", "tokens": ["Wird", "mor\u00b7gen", "sie", "mit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.47": {"text": "Die Augen, dunkel starrend vor sich hin,", "tokens": ["Die", "Au\u00b7gen", ",", "dun\u00b7kel", "star\u00b7rend", "vor", "sich", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADJD", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Bekannten: Wohl mir, da\u00df ich dieser Erde,", "tokens": ["Be\u00b7kann\u00b7ten", ":", "Wohl", "mir", ",", "da\u00df", "ich", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word