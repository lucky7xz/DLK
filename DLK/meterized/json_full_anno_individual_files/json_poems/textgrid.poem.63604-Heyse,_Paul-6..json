ie", "ihr", ",", "nicht", "aus", "den", "Au\u00b7gen", "ihn", "ver\u00b7sch\u00fct\u00b7tet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PTKNEG", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.25": {"text": "Zu tief mit meinem Blut ist er vereint.", "tokens": ["Zu", "tief", "mit", "mei\u00b7nem", "Blut", "ist", "er", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Nichts hab' ich mehr, das noch zur Not verkittet", "tokens": ["Nichts", "hab'", "ich", "mehr", ",", "das", "noch", "zur", "Not", "ver\u00b7kit\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die St\u00fccke des zerbrochnen Seins, als ihn,", "tokens": ["Die", "St\u00fc\u00b7cke", "des", "zer\u00b7broch\u00b7nen", "Seins", ",", "als", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Der ganz die Seele f\u00fcllt, obschon zerr\u00fcttet.", "tokens": ["Der", "ganz", "die", "See\u00b7le", "f\u00fcllt", ",", "ob\u00b7schon", "zer\u00b7r\u00fct\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,", "KOUS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Nicht will ich feige mir und ihm entfliehn,", "tokens": ["Nicht", "will", "ich", "fei\u00b7ge", "mir", "und", "ihm", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "VVFIN", "PPER", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Will ", "tokens": ["Will"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Da mir der Andacht hohe Kraft verliehn.", "tokens": ["Da", "mir", "der", "An\u00b7dacht", "ho\u00b7he", "Kraft", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Denn Frevel d\u00fcnkt mich, da\u00df man sich entschlage", "tokens": ["Denn", "Fre\u00b7vel", "d\u00fcnkt", "mich", ",", "da\u00df", "man", "sich", "ent\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "KOUS", "PIS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Der Pflicht des Danks, mit Schmerzen ", "tokens": ["Der", "Pflicht", "des", "Danks", ",", "mit", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Die man geliebt mit innigem Herzensschlage.", "tokens": ["Die", "man", "ge\u00b7liebt", "mit", "in\u00b7ni\u00b7gem", "Her\u00b7zens\u00b7schla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.35": {"text": "Die frommen Alten lehrt' es ihr Gewissen,", "tokens": ["Die", "from\u00b7men", "Al\u00b7ten", "lehrt'", "es", "ihr", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Dem Gram sein Recht zu geben, wie der Freude,", "tokens": ["Dem", "Gram", "sein", "Recht", "zu", "ge\u00b7ben", ",", "wie", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und das Volk Gottes hat sein Kleid zerrissen.", "tokens": ["Und", "das", "Volk", "Got\u00b7tes", "hat", "sein", "Kleid", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Nur ihr, die ihr der Selbstsucht Wahngeb\u00e4ude", "tokens": ["Nur", "ihr", ",", "die", "ihr", "der", "Selbst\u00b7sucht", "Wahn\u00b7ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Auft\u00fcrmt, ihr nennt zu kostbar die Sekunde,", "tokens": ["Auf\u00b7t\u00fcrmt", ",", "ihr", "nennt", "zu", "kost\u00b7bar", "die", "Se\u00b7kun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Die man an hoffnungsloses Weh vergeude.", "tokens": ["Die", "man", "an", "hoff\u00b7nungs\u00b7lo\u00b7ses", "Weh", "ver\u00b7geu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "O nun versteh ich, was mit stummem Munde", "tokens": ["O", "nun", "ver\u00b7steh", "ich", ",", "was", "mit", "stum\u00b7mem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Du mir gesagt, mein Liebling, als mit St\u00f6hnen", "tokens": ["Du", "mir", "ge\u00b7sagt", ",", "mein", "Lieb\u00b7ling", ",", "als", "mit", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PPER", "VVPP", "$,", "PPOSAT", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Und Schluchzen dich umgab die dichte Runde.", "tokens": ["Und", "Schluch\u00b7zen", "dich", "um\u00b7gab", "die", "dich\u00b7te", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Dein Schweigen schien ihr Klaggeheul zu h\u00f6hnen,", "tokens": ["Dein", "Schwei\u00b7gen", "schien", "ihr", "Klag\u00b7ge\u00b7heul", "zu", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Als w\u00fc\u00dftest du, der k\u00e4rglichste Gewinn", "tokens": ["Als", "w\u00fc\u00df\u00b7test", "du", ",", "der", "k\u00e4rg\u00b7lichs\u00b7te", "Ge\u00b7winn"], "token_info": ["word", "w