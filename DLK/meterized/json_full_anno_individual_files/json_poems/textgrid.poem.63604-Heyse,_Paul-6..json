o", "k\u00fchl", "und", "stolz", ",", "es", "bang\u00b7te", "mir", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "Als ob ich selbst von dir verachtet werde.", "tokens": ["Als", "ob", "ich", "selbst", "von", "dir", "ver\u00b7ach\u00b7tet", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.53": {"text": "Nein, Liebling, mich nur aus der dumpfen Schar", "tokens": ["Nein", ",", "Lieb\u00b7ling", ",", "mich", "nur", "aus", "der", "dum\u00b7pfen", "Schar"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.54": {"text": "Sollst du getreu und deiner wert erfinden;", "tokens": ["Sollst", "du", "ge\u00b7treu", "und", "dei\u00b7ner", "wert", "er\u00b7fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "KON", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.55": {"text": "Denn was dein L\u00e4cheln meinem Leben war,", "tokens": ["Denn", "was", "dein", "L\u00e4\u00b7cheln", "mei\u00b7nem", "Le\u00b7ben", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.56": {"text": "Wird mit dem letzten Hauch nur mir entschwinden!", "tokens": ["Wird", "mit", "dem", "letz\u00b7ten", "Hauch", "nur