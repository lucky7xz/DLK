{"dta.poem.3538": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wir verstehen sie nicht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Schneider h\u00e4tt ein b\u00f6ses Weib,               ", "tokens": ["Ein", "Schnei\u00b7der", "h\u00e4tt", "ein", "b\u00f6\u00b7ses", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vorwitzig, stolz, doch fein von Leib,", "tokens": ["Vor\u00b7wit\u00b7zig", ",", "stolz", ",", "doch", "fein", "von", "Leib", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sehr eigenwillig, frech und steil,", "tokens": ["Sehr", "ei\u00b7gen\u00b7wil\u00b7lig", ",", "frech", "und", "steil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Trug ihre Ehr auch ziemlich feil,", "tokens": ["Trug", "ih\u00b7re", "Ehr", "auch", "ziem\u00b7lich", "feil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stets ihrem Mann zuwieder lebte,", "tokens": ["Stets", "ih\u00b7rem", "Mann", "zu\u00b7wie\u00b7der", "leb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In allem Guten wiederstrebte;", "tokens": ["In", "al\u00b7lem", "Gu\u00b7ten", "wie\u00b7der\u00b7streb\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Ding er ihr befehlen kunnt,", "tokens": ["Kein", "Ding", "er", "ihr", "be\u00b7feh\u00b7len", "kunnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Allzeit sie das unrecht verstund.", "tokens": ["All\u00b7zeit", "sie", "das", "un\u00b7recht", "ver\u00b7stund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Sie sollt ihm einstens bringen Wachs,", "tokens": ["Sie", "sollt", "ihm", "eins\u00b7tens", "brin\u00b7gen", "Wachs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kam sie heim und brachte Flachs;", "tokens": ["Da", "kam", "sie", "heim", "und", "brach\u00b7te", "Flachs", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch einmal schickt er sie nach Zwirn,", "tokens": ["Noch", "ein\u00b7mal", "schickt", "er", "sie", "nach", "Zwirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da brachte sie statt dessen Birn.", "tokens": ["Da", "brach\u00b7te", "sie", "statt", "des\u00b7sen", "Birn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRELAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Sie sollte weisse Seide holen,", "tokens": ["Sie", "soll\u00b7te", "weis\u00b7se", "Sei\u00b7de", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie brachte Saiten unbefohlen;", "tokens": ["Sie", "brach\u00b7te", "Sai\u00b7ten", "un\u00b7be\u00b7foh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie sollt ihm holen eine Scheer,", "tokens": ["Sie", "sollt", "ihm", "ho\u00b7len", "ei\u00b7ne", "Scheer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie bracht daher viel Schweineschmeer.", "tokens": ["Sie", "bracht", "da\u00b7her", "viel", "Schwei\u00b7ne\u00b7schmeer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er sprach einmal zu ihr mit Flei\u00df,", "tokens": ["Er", "sprach", "ein\u00b7mal", "zu", "ihr", "mit", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Mach eilends mir ein Eisen hei\u00df;", "tokens": ["Mach", "ei\u00b7lends", "mir", "ein", "Ei\u00b7sen", "hei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie lie\u00df ein Eisen machen bald,", "tokens": ["Sie", "lie\u00df", "ein", "Ei\u00b7sen", "ma\u00b7chen", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schmied brachts hin, da war es kalt.", "tokens": ["Der", "Schmied", "brachts", "hin", ",", "da", "war", "es", "kalt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er sprach: Ich hab zuvor genug Eisen,", "tokens": ["Er", "sprach", ":", "Ich", "hab", "zu\u00b7vor", "ge\u00b7nug", "Ei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich hab kein neues machen hei\u00dfen;", "tokens": ["Ich", "hab", "kein", "neu\u00b7es", "ma\u00b7chen", "hei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Weib mich nimmer recht versteht,", "tokens": ["Mein", "Weib", "mich", "nim\u00b7mer", "recht", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit allem sie den Krebsgang geht.", "tokens": ["Mit", "al\u00b7lem", "sie", "den", "Krebs\u00b7gang", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Einst sprach er: Gieb mir her die Ell.", "tokens": ["Einst", "sprach", "er", ":", "Gieb", "mir", "her", "die", "Ell", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVIMP", "PPER", "ADV", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da bracht sie ihm Lissabonisch Oehl;", "tokens": ["Da", "bracht", "sie", "ihm", "Lis\u00b7sa\u00b7bo\u00b7nisch", "O\u00b7ehl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mehr sagt er: Dieses Kleid zertrenn,", "tokens": ["Mehr", "sagt", "er", ":", "Die\u00b7ses", "Kleid", "zer\u00b7trenn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$.", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sie verstand, das Kleid verbrenn.", "tokens": ["Und", "sie", "ver\u00b7stand", ",", "das", "Kleid", "ver\u00b7brenn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Alsbald warf sie dasselb ins Feuer,", "tokens": ["Als\u00b7bald", "warf", "sie", "das\u00b7selb", "ins", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das kam den Schneider gar sehr theuer;", "tokens": ["Das", "kam", "den", "Schnei\u00b7der", "gar", "sehr", "theu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NE", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er hie\u00df sie bringen ander Tuch", "tokens": ["Er", "hie\u00df", "sie", "brin\u00b7gen", "an\u00b7der", "Tuch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Kleid, sie aber bracht ein Buch.", "tokens": ["Zum", "Kleid", ",", "sie", "a\u00b7ber", "bracht", "ein", "Buch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er hie\u00df sie fr\u00fcher aufzustehn,", "tokens": ["Er", "hie\u00df", "sie", "fr\u00fc\u00b7her", "auf\u00b7zu\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Predigt in die Kirch zu gehn;", "tokens": ["Zur", "Pre\u00b7digt", "in", "die", "Kirch", "zu", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kinder \u00fcberbringen hin,", "tokens": ["Die", "Kin\u00b7der", "\u00fc\u00b7berb\u00b7rin\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Schule was zu lernen drinn.", "tokens": ["Zur", "Schu\u00b7le", "was", "zu", "ler\u00b7nen", "drinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kinder in die Kirch sie f\u00fchrte,", "tokens": ["Die", "Kin\u00b7der", "in", "die", "Kirch", "sie", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie aber in der Schul studierte;", "tokens": ["Sie", "a\u00b7ber", "in", "der", "Schul", "stu\u00b7dier\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Einst folgt er ihr nach auf dem Fu\u00df,", "tokens": ["Einst", "folgt", "er", "ihr", "nach", "auf", "dem", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sah was, das ihm bracht Verdru\u00df.", "tokens": ["Und", "sah", "was", ",", "das", "ihm", "bracht", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "PRELS", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Als sie zu Hause wieder kam,", "tokens": ["Als", "sie", "zu", "Hau\u00b7se", "wie\u00b7der", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschwind er die Flachshechel nahm;", "tokens": ["Ge\u00b7schwind", "er", "die", "Flachs\u00b7he\u00b7chel", "nahm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Schlug ihr damit den Kopf und Leib,", "tokens": ["Schlug", "ihr", "da\u00b7mit", "den", "Kopf", "und", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O weh! was thust du, sprach das Weib.", "tokens": ["O", "weh", "!", "was", "thust", "du", ",", "sprach", "das", "Weib", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er sprach: Ich mu\u00df mich nur bem\u00fchen,", "tokens": ["Er", "sprach", ":", "Ich", "mu\u00df", "mich", "nur", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Flachs fein durch die Hechel ziehen:", "tokens": ["Den", "Flachs", "fein", "durch", "die", "He\u00b7chel", "zie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie rief o weh, weh meine Stirn!", "tokens": ["Sie", "rief", "o", "weh", ",", "weh", "mei\u00b7ne", "Stirn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "FM", "PTKVZ", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er sprach: ich speise dich mit Birn.", "tokens": ["Er", "sprach", ":", "ich", "spei\u00b7se", "dich", "mit", "Birn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie rief o weh, mein R\u00fcck und Seit,", "tokens": ["Sie", "rief", "o", "weh", ",", "mein", "R\u00fcck", "und", "Seit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "FM", "PTKVZ", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sprach: wie klingt die Zittersait,", "tokens": ["Er", "sprach", ":", "wie", "klingt", "die", "Zit\u00b7ter\u00b7sait", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schrie: schlag mich doch nicht so sehr;", "tokens": ["Sie", "schrie", ":", "schlag", "mich", "doch", "nicht", "so", "sehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sprach: das Leder darf viel Schmeer.", "tokens": ["Er", "sprach", ":", "das", "Le\u00b7der", "darf", "viel", "Schmeer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VMFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie bat: 