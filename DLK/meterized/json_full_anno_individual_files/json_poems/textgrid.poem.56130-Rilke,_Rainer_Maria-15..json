{"textgrid.poem.56130": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wartet..., das schmeckt... Schon ists auf der Flucht.", "tokens": ["War\u00b7tet", "...", ",", "das", "schmeckt", "...", "Schon", "ists", "auf", "der", "Flucht", "."], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "$,", "PDS", "VVFIN", "$(", "ADV", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-++-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "..... Wenig Musik nur, ein Stampfen, ein Summen \u2013:", "tokens": [".....", "We\u00b7nig", "Mu\u00b7sik", "nur", ",", "ein", "Stamp\u00b7fen", ",", "ein", "Sum\u00b7men", "\u2013", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT", "NN", "ADV", "$,", "ART", "NN", "$,", "ART", "NN", "$(", "$."], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "M\u00e4dchen, ihr warmen, M\u00e4dchen, ihr stummen,", "tokens": ["M\u00e4d\u00b7chen", ",", "ihr", "war\u00b7men", ",", "M\u00e4d\u00b7chen", ",", "ihr", "stum\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "tanzt den Geschmack der erfahrenen Frucht!", "tokens": ["tanzt", "den", "Ge\u00b7schmack", "der", "er\u00b7fah\u00b7re\u00b7nen", "Frucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "