{"dta.poem.18780": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Hatem .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne kommt! Ein Prachterscheinen!", "tokens": ["Die", "Son\u00b7ne", "kommt", "!", "Ein", "Prach\u00b7ter\u00b7schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sichelmond umklammert sie.", "tokens": ["Der", "Si\u00b7chel\u00b7mond", "um\u00b7klam\u00b7mert", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer konnte solch ein Paar vereinen?", "tokens": ["Wer", "konn\u00b7te", "solch", "ein", "Paar", "ver\u00b7ei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIAT", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies R\u00e4thsel wie erkl\u00e4rt sich\u2019s? Wie?", "tokens": ["Dies", "R\u00e4th\u00b7sel", "wie", "er\u00b7kl\u00e4rt", "sich's", "?", "Wie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "NN", "KOKOM", "VVFIN", "PIS", "$.", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Sultan konnt\u2019 es, er verm\u00e4hlte", "tokens": ["Der", "Sul\u00b7tan", "konnt'", "es", ",", "er", "ver\u00b7m\u00e4hl\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das allerh\u00f6chste Weltenpaar,", "tokens": ["Das", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Wel\u00b7ten\u00b7paar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um zu bezeichnen Auserw\u00e4hlte,", "tokens": ["Um", "zu", "be\u00b7zeich\u00b7nen", "Au\u00b7ser\u00b7w\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die tapfersten der treuen Schaar.", "tokens": ["Die", "tap\u00b7fers\u00b7ten", "der", "treu\u00b7en", "Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Auch sey\u2019s ein Bild von unsrer Wonne!", "tokens": ["Auch", "sey's", "ein", "Bild", "von", "uns\u00b7rer", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon seh ich wieder mich und dich,", "tokens": ["Schon", "seh", "ich", "wie\u00b7der", "mich", "und", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "KON", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du nennst mich, Liebchen, deine Sonne,", "tokens": ["Du", "nennst", "mich", ",", "Lieb\u00b7chen", ",", "dei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Komm, s\u00fcsser Mond, umklammre mich!", "tokens": ["Komm", ",", "s\u00fcs\u00b7ser", "Mond", ",", "um\u00b7klamm\u00b7re", "mich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}