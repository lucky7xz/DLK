{"dta.poem.13084": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df nahm die Bilder-Kunst mit scheelen Augen an:", "tokens": ["Die\u00df", "nahm", "die", "Bil\u00b7der\u00b7Kunst", "mit", "schee\u00b7len", "Au\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201everzeih mir, sagte sie, da\u00df ich kaum h\u00f6ren kann", "tokens": ["\u201e", "ver\u00b7zeih", "mir", ",", "sag\u00b7te", "sie", ",", "da\u00df", "ich", "kaum", "h\u00f6\u00b7ren", "kann"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewie deine Kunst sich prahlt: wer lobte dann die Grillen,", "tokens": ["\u201e", "wie", "dei\u00b7ne", "Kunst", "sich", "prahlt", ":", "wer", "lob\u00b7te", "dann", "die", "Gril\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "PPOSAT", "NN", "PRF", "VVFIN", "$.", "PWS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "160\u201dMit denen du verlangst die Nachwelt anzuf\u00fcllen?", "tokens": ["\"", "Mit", "de\u00b7nen", "du", "ver\u00b7langst", "die", "Nach\u00b7welt", "an\u00b7zu\u00b7f\u00fcl\u00b7len", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "VVFIN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewas heissen Kalch und Sand? wie prangen Stein\u2019 und Maur?", "tokens": ["\u201e", "was", "heis\u00b7sen", "Kalch", "und", "Sand", "?", "wie", "pran\u00b7gen", "Stein'", "und", "Maur", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADJA", "NN", "KON", "NN", "$.", "PWAV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eo da\u00df ich deine M\u00fch und Arbeit nicht bedaur!", "tokens": ["\u201e", "o", "da\u00df", "ich", "dei\u00b7ne", "M\u00fch", "und", "Ar\u00b7beit", "nicht", "be\u00b7daur", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "KOUS", "PPER", "PPOSAT", "NN", "KON", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eein rauher Ziegel-Berg wem tr\u00e4gt er Ruhm und Ehren?", "tokens": ["\u201e", "ein", "rau\u00b7her", "Zie\u00b7gel\u00b7Berg", "wem", "tr\u00e4gt", "er", "Ruhm", "und", "Eh\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "PWS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ef\u00fcr wen ist er gebaut? wer wird den Nahmen h\u00f6ren,", "tokens": ["\u201e", "f\u00fcr", "wen", "ist", "er", "ge\u00b7baut", "?", "wer", "wird", "den", "Nah\u00b7men", "h\u00f6\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PWS", "VAFIN", "PPER", "VVPP", "$.", "PWS", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "165\u201dDen etwann schon der Rost der Zeit hat ausgefeilt?", "tokens": ["\"", "Den", "et\u00b7wann", "schon", "der", "Rost", "der", "Zeit", "hat", "aus\u00b7ge\u00b7feilt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "ADV", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201esag, wessen Ehre war derselbe zugetheilt?", "tokens": ["\u201e", "sag", ",", "wes\u00b7sen", "Eh\u00b7re", "war", "der\u00b7sel\u00b7be", "zu\u00b7ge\u00b7theilt", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ADJA", "NN", "VAFIN", "PDAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ewas die Nachk\u00f6mmlinge dadurch zu wissen haben,", "tokens": ["\u201e", "was", "die", "Nach\u00b7k\u00f6mm\u00b7lin\u00b7ge", "da\u00b7durch", "zu", "wis\u00b7sen", "ha\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ART", "NN", "PAV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "\u201enicht wahr, es ist in Sand, in Kalch und Stein vergraben.", "tokens": ["\u201e", "nicht", "wahr", ",", "es", "ist", "in", "Sand", ",", "in", "Kalch", "und", "Stein", "ver\u00b7gra\u00b7ben", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}