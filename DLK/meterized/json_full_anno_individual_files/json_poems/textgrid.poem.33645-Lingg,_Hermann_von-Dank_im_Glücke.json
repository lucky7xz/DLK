{"textgrid.poem.33645": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Dank im Gl\u00fccke", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergi\u00df es, da\u00df du einst im Scho\u00df", "tokens": ["Ver\u00b7gi\u00df", "es", ",", "da\u00df", "du", "einst", "im", "Scho\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Armut bist gelegen,", "tokens": ["Der", "Ar\u00b7mut", "bist", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und da\u00df des Jammers Tr\u00e4ne flo\u00df", "tokens": ["Und", "da\u00df", "des", "Jam\u00b7mers", "Tr\u00e4\u00b7ne", "flo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deinen Morgensegen!", "tokens": ["In", "dei\u00b7nen", "Mor\u00b7gen\u00b7se\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vergi\u00df es, da du gl\u00fccklich bist,", "tokens": ["Ver\u00b7gi\u00df", "es", ",", "da", "du", "gl\u00fcck\u00b7lich", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Tr\u00e4ume man am Tag vergi\u00dft.", "tokens": ["Wie", "Tr\u00e4u\u00b7me", "man", "am", "Tag", "ver\u00b7gi\u00dft",