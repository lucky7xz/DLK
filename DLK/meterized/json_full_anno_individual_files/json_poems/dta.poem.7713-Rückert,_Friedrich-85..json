{"dta.poem.7713": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um Eines ist das Thier vom Menschen zu beneiden,", "tokens": ["Um", "Ei\u00b7nes", "ist", "das", "Thier", "vom", "Men\u00b7schen", "zu", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIS", "VAFIN", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df es nicht sorgen darf, wie es sich solle kleiden.", "tokens": ["Da\u00df", "es", "nicht", "sor\u00b7gen", "darf", ",", "wie", "es", "sich", "sol\u00b7le", "klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "PWAV", "PPER", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Winter w\u00e4chst sein Pelz, im Sommer h\u00e4r't er sich,", "tokens": ["Im", "Win\u00b7ter", "w\u00e4chst", "sein", "Pelz", ",", "im", "Som\u00b7mer", "h\u00e4r't", "er", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "APPRART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Jahrzeit stets gem\u00e4\u00df und jedem Himmelstrich.", "tokens": ["Der", "Jahr\u00b7zeit", "stets", "ge\u00b7m\u00e4\u00df", "und", "je\u00b7dem", "Him\u00b7mel\u00b7strich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Kleid veraltet nie und kommt nicht aus der Mode,", "tokens": ["Das", "Kleid", "ver\u00b7al\u00b7tet", "nie", "und", "kommt", "nicht", "aus", "der", "Mo\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit der Geburt wird's angelegt und ab im Tode.", "tokens": ["Mit", "der", "Ge\u00b7burt", "wird's", "an\u00b7ge\u00b7legt", "und", "ab", "im", "To\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "KON", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Kein Wechsel ist erlaubt mit Purpur, Gold und Seide;", "tokens": ["Kein", "Wech\u00b7sel", "ist", "er\u00b7laubt", "mit", "Pur\u00b7pur", ",", "Gold", "und", "Sei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "APPR", "NN", "$,", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der Verschwendung bleibt kein Anla\u00df noch dem Neide.", "tokens": ["Und", "der", "Ver\u00b7schwen\u00b7dung", "bleibt", "kein", "An\u00b7la\u00df", "noch", "dem", "Nei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vom Thiere gilts allein: das Kleid macht nicht den Mann;", "tokens": ["Vom", "Thie\u00b7re", "gilts", "al\u00b7lein", ":", "das", "Kleid", "macht", "nicht", "den", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "$.", "ART", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil keins vorm andern sich durchs Kleid auszeichnen kann.", "tokens": ["Weil", "keins", "vorm", "an\u00b7dern", "sich", "durchs", "Kleid", "aus\u00b7zeich\u00b7nen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "APPRART", "ADJA", "PRF", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}