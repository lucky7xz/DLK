{"dta.poem.909": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "TrOMPETEN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Unter verschnittenen Weiden, wo braune Kinder spielen", "tokens": ["Un\u00b7ter", "ver\u00b7schnit\u00b7te\u00b7nen", "Wei\u00b7den", ",", "wo", "brau\u00b7ne", "Kin\u00b7der", "spie\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN", "VVINF"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.2": {"text": "Und Bl\u00e4tter treiben, t\u00f6nen Trompeten. Ein Kirchhofsschauer.", "tokens": ["Und", "Bl\u00e4t\u00b7ter", "trei\u00b7ben", ",", "t\u00f6\u00b7nen", "Trom\u00b7pe\u00b7ten", ".", "Ein", "Kirch\u00b7hofs\u00b7schau\u00b7er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "ADJA", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-++--+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Fahnen von Scharlach st\u00fcrzen durch des Ahorns Trauer,", "tokens": ["Fah\u00b7nen", "von", "Schar\u00b7lach", "st\u00fcr\u00b7zen", "durch", "des", "A\u00b7horns", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Reiter entlang an Roggenfeldern, leeren M\u00fchlen.", "tokens": ["Rei\u00b7ter", "ent\u00b7lang", "an", "Rog\u00b7gen\u00b7fel\u00b7dern", ",", "lee\u00b7ren", "M\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPO", "APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Oder Hirten singen nachts und Hirsche treten", "tokens": ["O\u00b7der", "Hir\u00b7ten", "sin\u00b7gen", "nachts", "und", "Hir\u00b7sche", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADV", "KON", "NN", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "In den Kreis ihrer Feuer, des Hains uralte Trauer,", "tokens": ["In", "den", "Kreis", "ih\u00b7rer", "Feu\u00b7er", ",", "des", "Hains", "ur\u00b7al\u00b7te", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Tanzende heben sich von einer schwarzen Mauer;", "tokens": ["Tan\u00b7zen\u00b7de", "he\u00b7ben", "sich", "von", "ei\u00b7ner", "schwar\u00b7zen", "Mau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Fahnen von Scharlach, Lachen, Wahnsinn, Trompeten.", "tokens": ["Fah\u00b7nen", "von", "Schar\u00b7lach", ",", "La\u00b7chen", ",", "Wahn\u00b7sinn", ",", "Trom\u00b7pe\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}}}}}