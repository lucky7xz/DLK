{"textgrid.poem.34131": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ariost", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reizend, wie du, hat Keiner die Thorheit der Welt uns geschildert;", "tokens": ["Rei\u00b7zend", ",", "wie", "du", ",", "hat", "Kei\u00b7ner", "die", "Thor\u00b7heit", "der", "Welt", "uns", "ge\u00b7schil\u00b7dert", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPER", "$,", "VAFIN", "PIS", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Ward dein Gedicht dir belohnt, ward der Verstand dir versagt.", "tokens": ["Ward", "dein", "Ge\u00b7dicht", "dir", "be\u00b7lohnt", ",", "ward", "der", "Ver\u00b7stand", "dir", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$,", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihn zu verlieren, ist schlimm, so hei\u00dft es, ihn nicht zu bekommen,", "tokens": ["Ihn", "zu", "ver\u00b7lie\u00b7ren", ",", "ist", "schlimm", ",", "so", "hei\u00dft", "es", ",", "ihn", "nicht", "zu", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "$,", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ist das einzige Gl\u00fcck, welches die G\u00f6tter verleih