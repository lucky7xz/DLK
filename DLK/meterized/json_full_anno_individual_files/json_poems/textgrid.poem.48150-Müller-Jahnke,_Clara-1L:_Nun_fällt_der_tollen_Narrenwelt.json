{"textgrid.poem.48150": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun f\u00e4llt der tollen Narrenwelt", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun f\u00e4llt der tollen Narrenwelt", "tokens": ["Nun", "f\u00e4llt", "der", "tol\u00b7len", "Nar\u00b7ren\u00b7welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das bunte Kleid in Lumpen, \u2013", "tokens": ["das", "bun\u00b7te", "Kleid", "in", "Lum\u00b7pen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und klirrend auf den Estrich schellt", "tokens": ["und", "klir\u00b7rend", "auf", "den", "Est\u00b7rich", "schellt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Freude voller Humpen.", "tokens": ["der", "Freu\u00b7de", "vol\u00b7ler", "Hum\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lautkrachend springt ins Schlo\u00df das Tor,", "tokens": ["Laut\u00b7kra\u00b7chend", "springt", "ins", "Schlo\u00df", "das", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "kein Lichtschein mehr am Fenster \u2013", "tokens": ["kein", "Licht\u00b7schein", "mehr", "am", "Fens\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "ein grauer Morgen kriecht empor,", "tokens": ["ein", "grau\u00b7er", "Mor\u00b7gen", "kriecht", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "der Morgen der Gespenster.", "tokens": ["der", "Mor\u00b7gen", "der", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da ist im tiefen Stra\u00dfenstaub", "tokens": ["Da", "ist", "im", "tie\u00b7fen", "Stra\u00b7\u00dfen\u00b7staub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein stolzes Weib gestanden \u2013", "tokens": ["ein", "stol\u00b7zes", "Weib", "ge\u00b7stan\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "von ihrem Odem rauscht das Laub,", "tokens": ["von", "ih\u00b7rem", "O\u00b7dem", "rauscht", "das", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "des Meeres Wogen branden.", "tokens": ["des", "Mee\u00b7res", "Wo\u00b7gen", "bran\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie reckt sich in die Fr\u00fchlingspracht", "tokens": ["Sie", "reckt", "sich", "in", "die", "Fr\u00fch\u00b7lings\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mit herrischer Geb\u00e4rde:", "tokens": ["mit", "her\u00b7ri\u00b7scher", "Ge\u00b7b\u00e4r\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "mein ist, was bl\u00fcht und weint und lacht \u2013", "tokens": ["mein", "ist", ",", "was", "bl\u00fcht", "und", "weint", "und", "lacht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "$,", "PWS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "mein ist die ganze Erde!", "tokens": ["mein", "ist", "die", "gan\u00b7ze", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was bimmelt ihr vom Kirchenturm", "tokens": ["Was", "bim\u00b7melt", "ihr", "vom", "Kir\u00b7chen\u00b7turm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und predigt Reu und Bu\u00dfe?", "tokens": ["und", "pre\u00b7digt", "Reu", "und", "Bu\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr seid das Sandkorn vor dem Sturm,", "tokens": ["Ihr", "seid", "das", "Sand\u00b7korn", "vor", "dem", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Staub mir unterm Fu\u00dfe.", "tokens": ["der", "Staub", "mir", "un\u00b7term", "Fu\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was schiert mich eurer S\u00fcnde Scham", "tokens": ["Was", "schiert", "mich", "eu\u00b7rer", "S\u00fcn\u00b7de", "Scham"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und eurer H\u00f6lle Flammen?", "tokens": ["und", "eu\u00b7rer", "H\u00f6l\u00b7le", "Flam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich blas den ganzen Maskenkram", "tokens": ["Ich", "blas", "den", "gan\u00b7zen", "Mas\u00b7ken\u00b7kram"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "mit einem Hauch zusammen.", "tokens": ["mit", "ei\u00b7nem", "Hauch", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mir gilt die Dirne unterm Tor,", "tokens": ["Mir", "gilt", "die", "Dir\u00b7ne", "un\u00b7term", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das H\u00fcndlein in der Gossen", "tokens": ["das", "H\u00fcnd\u00b7lein", "in", "der", "Gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "mehr als der sch\u00f6nste Damenflor", "tokens": ["mehr", "als", "der", "sch\u00f6ns\u00b7te", "Da\u00b7men\u00b7flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in euren Staatskarossen.", "tokens": ["in", "eu\u00b7ren", "Staats\u00b7ka\u00b7ros\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Blumen und Konfettischlacht?", "tokens": ["Und", "Blu\u00b7men", "und", "Kon\u00b7fet\u00b7tischlacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wie j\u00e4h verstummt die Harfe,", "tokens": ["Wie", "j\u00e4h", "ver\u00b7stummt", "die", "Har\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "verspr\u00fcht der Witz, verbla\u00dft die Pracht,", "tokens": ["ver\u00b7spr\u00fcht", "der", "Witz", ",", "ver\u00b7bla\u00dft", "die", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "l\u00f6st meine Hand die Larve.", "tokens": ["l\u00f6st", "mei\u00b7ne", "Hand", "die", "Lar\u00b7ve", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Mir gilt des Bettlers hohle Hand", "tokens": ["Mir", "gilt", "des", "Bett\u00b7lers", "hoh\u00b7le", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und gramzerfressne Miene", "tokens": ["und", "gram\u00b7zer\u00b7fress\u00b7ne", "Mie\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "mehr als der F\u00fcrstenh\u00f6fe Tand", "tokens": ["mehr", "als", "der", "F\u00fcrs\u00b7ten\u00b7h\u00f6\u00b7fe", "Tand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und blutige Hermeline. \u2013", "tokens": ["und", "blu\u00b7ti\u00b7ge", "Her\u00b7me\u00b7li\u00b7ne", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und tobt im Ost der Schwertertanz,", "tokens": ["Und", "tobt", "im", "Ost", "der", "Schwer\u00b7ter\u00b7tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und saust das Blei, das rasche \u2013", "tokens": ["und", "saust", "das", "Blei", ",", "das", "ra\u00b7sche", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "auf aller Kronen Faschingsglanz", "tokens": ["auf", "al\u00b7ler", "Kro\u00b7nen", "Fa\u00b7schings\u00b7glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "streu ich die Handvoll Asche!", "tokens": ["streu", "ich", "die", "Hand\u00b7voll", "A\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ob Kirchen- oder Festungssturm,", "tokens": ["Ob", "Kir\u00b7chen", "o\u00b7der", "Fes\u00b7tungs\u00b7sturm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sie wanken beid auf Erden", "tokens": ["sie", "wan\u00b7ken", "beid", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und werden einst vom Wirbelsturm", "tokens": ["und", "wer\u00b7den", "einst", "vom", "Wir\u00b7bels\u00b7turm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zu Staub zerblasen werden.", "tokens": ["zu", "Staub", "zer\u00b7bla\u00b7sen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und rei\u00dft der letzten Narretei", "tokens": ["Und", "rei\u00dft", "der", "letz\u00b7ten", "Nar\u00b7re\u00b7tei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der bunte Rock in Fetzen,", "tokens": ["der", "bun\u00b7te", "Rock", "in", "Fet\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "dann soll die Menschheit, 