{"dta.poem.15691": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du den Blinden siehst, den armen Mann, den kranken,", "tokens": ["Wenn", "du", "den", "Blin\u00b7den", "siehst", ",", "den", "ar\u00b7men", "Mann", ",", "den", "kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach d\u00fcrft'ger Gab' umher an seinem Stabe wanken;", "tokens": ["Nach", "d\u00fcrft'\u00b7ger", "Gab'", "um\u00b7her", "an", "sei\u00b7nem", "Sta\u00b7be", "wan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Bedachtest du dabei, womit du das, o Kind,", "tokens": ["Be\u00b7dach\u00b7test", "du", "da\u00b7bei", ",", "wo\u00b7mit", "du", "das", ",", "o", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "$,", "PWAV", "PPER", "PDS", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verdienst, da\u00df du nicht auch bist arm und krank und blind?", "tokens": ["Ver\u00b7dienst", ",", "da\u00df", "du", "nicht", "auch", "bist", "arm", "und", "krank", "und", "blind", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht dein Verdienst ist das, erkenne Gottes Gnaden,", "tokens": ["Nicht", "dein", "Ver\u00b7dienst", "ist", "das", ",", "er\u00b7ken\u00b7ne", "Got\u00b7tes", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VAFIN", "PDS", "$,", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und klage nicht, da\u00df du bist anders auch beladen.", "tokens": ["Und", "kla\u00b7ge", "nicht", ",", "da\u00df", "du", "bist", "an\u00b7ders", "auch", "be\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie k\u00f6nntest du vor Scham ganz sorglos aufrecht stehn,", "tokens": ["Wie", "k\u00f6nn\u00b7test", "du", "vor", "Scham", "ganz", "sorg\u00b7los", "auf\u00b7recht", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "NN", "ADV", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und s\u00e4hest so in Staub geb\u00fcckt den Bruder gehn!", "tokens": ["Und", "s\u00e4\u00b7hest", "so", "in", "Staub", "ge\u00b7b\u00fcckt", "den", "Bru\u00b7der", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "VVPP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}