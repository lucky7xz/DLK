{"dta.poem.13765": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Mahnung .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Entschlie\u00df Dich wie du kannst nun, doch das merke:", "tokens": ["Ent\u00b7schlie\u00df", "Dich", "wie", "du", "kannst", "nun", ",", "doch", "das", "mer\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "PPER", "VMFIN", "ADV", "$,", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer in der Noth nichts mag, als Lauten r\u00fchren,", "tokens": ["Wer", "in", "der", "Noth", "nichts", "mag", ",", "als", "Lau\u00b7ten", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PIS", "VMFIN", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "De\u00df Hand dereinst w\u00e4chst mahnend aus dem Grabe.", "tokens": ["De\u00df", "Hand", "de\u00b7reinst", "w\u00e4chst", "mah\u00b7nend", "aus", "dem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}