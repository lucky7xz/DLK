{"dta.poem.23525": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die deutschgesinnte Polizei.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weg mit w\u00e4lschem Ungeschmack", "tokens": ["Weg", "mit", "w\u00e4l\u00b7schem", "Un\u00b7ge\u00b7schmack"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dem schamlos offnen Frack!", "tokens": ["Und", "dem", "scham\u00b7los", "off\u00b7nen", "Frack", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutscher Rock und deutsch Baret,", "tokens": ["Deut\u00b7scher", "Rock", "und", "deutsch", "Ba\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei, wie steht's so fein und nett!", "tokens": ["Ei", ",", "wie", "steht's", "so", "fein", "und", "nett", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also sprach man Tag und Nacht", "tokens": ["Al\u00b7so", "sprach", "man", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der Leipziger Freiheitsschlacht,", "tokens": ["Nach", "der", "Leip\u00b7zi\u00b7ger", "Frei\u00b7heits\u00b7schlacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Doch behielt im ganzen Land", "tokens": ["Doch", "be\u00b7hielt", "im", "gan\u00b7zen", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets der Frack die Oberhand.", "tokens": ["Stets", "der", "Frack", "die", "O\u00b7ber\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bald auch hing man an den Pflock", "tokens": ["Bald", "auch", "hing", "man", "an", "den", "Pflock"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hie und da den deutschen Rock;", "tokens": ["Hie", "und", "da", "den", "deut\u00b7schen", "Rock", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur der Bruder Studio", "tokens": ["Nur", "der", "Bru\u00b7der", "Stu\u00b7dio"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Machte noch damit Halloh.", "tokens": ["Mach\u00b7te", "noch", "da\u00b7mit", "Hal\u00b7loh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PAV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und nun kam die Polizei", "tokens": ["Und", "nun", "kam", "die", "Po\u00b7li\u00b7zei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie sprach: es ist vorbei!", "tokens": ["Und", "sie", "sprach", ":", "es", "ist", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutsche Tracht ist Tand und Schein,", "tokens": ["Deut\u00b7sche", "Tracht", "ist", "Tand", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutsch von Herzen sollt ihr sein!", "tokens": ["Deutsch", "von", "Her\u00b7zen", "sollt", "ihr", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}