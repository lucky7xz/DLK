{"textgrid.poem.60960": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Selbstlob", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? mich selbst je h\u00e4tt ich gelobt? Wo? Wann? Es entdeckte", "tokens": ["Wie", "?", "mich", "selbst", "je", "h\u00e4tt", "ich", "ge\u00b7lobt", "?", "Wo", "?", "Wann", "?", "Es", "ent\u00b7deck\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "$.", "PPER", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "$.", "PWAV", "$.", "PWAV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Irgend ein Mensch jemals eitle Gedanken in mir?", "tokens": ["Ir\u00b7gend", "ein", "Mensch", "je\u00b7mals", "eit\u00b7le", "Ge\u00b7dan\u00b7ken", "in", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Nicht mich selber, ich r\u00fchmte den Genius, welcher besucht mich,", "tokens": ["Nicht", "mich", "sel\u00b7ber", ",", "ich", "r\u00fchm\u00b7te", "den", "Ge\u00b7nius", ",", "wel\u00b7cher", "be\u00b7sucht", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Nicht mein sterbliches, mein fl\u00fcchtiges, irdisches Nichts!", "tokens": ["Nicht", "mein", "sterb\u00b7li\u00b7ches", ",", "mein", "fl\u00fcch\u00b7ti\u00b7ges", ",", "ir\u00b7di\u00b7sches", "Nichts", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Weil ich bescheiden und still mich selbst f\u00fcr viel zu gering hielt,", "tokens": ["Weil", "ich", "be\u00b7schei\u00b7den", "und", "still", "mich", "selbst", "f\u00fcr", "viel", "zu", "ge\u00b7ring", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "PPER", "ADV", "APPR", "ADV", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Staunt ich in meinem Gem\u00fct \u00fcber den g\u00f6ttlichen Gast.", "tokens": ["Staunt", "ich", "in", "mei\u00b7nem", "Ge\u00b7m\u00fct", "\u00fc\u00b7ber", "den", "g\u00f6tt\u00b7li\u00b7chen", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie? mich selbst je h\u00e4tt ich gelobt? Wo? Wann? Es entdeckte", "tokens": ["Wie", "?", "mich", "selbst", "je", "h\u00e4tt", "ich", "ge\u00b7lobt", "?", "Wo", "?", "Wann", "?", "Es", "ent\u00b7deck\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "$.", "PPER", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "$.", "PWAV", "$.", "PWAV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Irgend ein Mensch jemals eitle Gedanken in mir?", "tokens": ["Ir\u00b7gend", "ein", "Mensch", "je\u00b7mals", "eit\u00b7le", "Ge\u00b7dan