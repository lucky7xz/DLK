{"textgrid.poem.60256": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Trauer", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blumen, V\u00f6gel, duftend, singend,", "tokens": ["Blu\u00b7men", ",", "V\u00f6\u00b7gel", ",", "duf\u00b7tend", ",", "sin\u00b7gend", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVPP", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seid doch nicht so ausgelassen,", "tokens": ["Seid", "doch", "nicht", "so", "aus\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ungest\u00fcm ans Herz mir dringend;", "tokens": ["Un\u00b7ge\u00b7st\u00fcm", "ans", "Herz", "mir", "drin\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft allein mich ziehn die Stra\u00dfen!", "tokens": ["La\u00dft", "al\u00b7lein", "mich", "ziehn", "die", "Stra\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Vieles ist vor\u00fcbergangen,", "tokens": ["Vie\u00b7les", "ist", "vor\u00b7\u00fc\u00b7ber\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit wir uns zuletzt begegnet,", "tokens": ["Seit", "wir", "uns", "zu\u00b7le