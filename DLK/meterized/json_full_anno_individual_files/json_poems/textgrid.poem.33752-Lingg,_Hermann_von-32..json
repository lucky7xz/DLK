{"textgrid.poem.33752": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichg\u00fcltig seh' ich's jetzt, wie von der Linde", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "seh'", "ich's", "jetzt", ",", "wie", "von", "der", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PIS", "ADV", "$,", "PWAV", "APPR", "ART", "NE"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das welke Laub weht, auch die D\u00e4mmerstunden,", "tokens": ["Das", "wel\u00b7ke", "Laub", "weht", ",", "auch", "die", "D\u00e4m\u00b7mer\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sonst ich ohne Wehmut nie empfunden,", "tokens": ["Die", "sonst", "ich", "oh\u00b7ne", "Weh\u00b7mut", "nie", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind mir gleichg\u00fcltig jetzt wie Schnee und Winde.", "tokens": ["Sind", "mir", "gleich\u00b7g\u00fcl\u00b7tig", "jetzt", "wie", "Schnee", "und", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ob eine Zeit, ob dies ob das entschwinde,", "tokens": ["Ob", "ei\u00b7ne", "Zeit", ",", "ob", "dies", 