{"dta.poem.360": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Kurtze Antwort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Talasso deine Schrifft ist wohl zu recht gebracht/", "tokens": ["Ta\u00b7las\u00b7so", "dei\u00b7ne", "Schrifft", "ist", "wohl", "zu", "recht", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich k\u00fcsse deinen Schlu\u00df/ und mir gef\u00e4llt dein Lieben/", "tokens": ["Ich", "k\u00fcs\u00b7se", "dei\u00b7nen", "Schlu\u00df", "/", "und", "mir", "ge\u00b7f\u00e4llt", "dein", "Lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich bin bereit zu thun was du mir vorgeschrieben/", "tokens": ["Ich", "bin", "be\u00b7reit", "zu", "thun", "was", "du", "mir", "vor\u00b7ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PTKZU", "VVINF", "PWS", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich folge dem Geschick/ und des Verh\u00e4ngni\u00df Macht/", "tokens": ["Ich", "fol\u00b7ge", "dem", "Ge\u00b7schick", "/", "und", "des", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "Macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "KON", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die mir/ mein Schatz/ befiehlt mein Hertz dir zu zuneigen/", "tokens": ["Die", "mir", "/", "mein", "Schatz", "/", "be\u00b7fiehlt", "mein", "Hertz", "dir", "zu", "zu\u00b7nei\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "PPOSAT", "NN", "$(", "VVFIN", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zeigt sich nun deines treu? so bin ich schon dein eigen.", "tokens": ["Zeigt", "sich", "nun", "dei\u00b7nes", "treu", "?", "so", "bin", "ich", "schon", "dein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PPOSAT", "ADJD", "$.", "ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}