{"dta.poem.2749": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die keusche Lorbeern/ dem Grabe  \n  J. S. g. S. gewidmet/ den 18. August.  1680.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jihr keusche Lorbeern ihr/ die ihr noch nie verm\u00e4hlet/", "tokens": ["Jihr", "keu\u00b7sche", "Lor\u00b7beern", "ihr", "/", "die", "ihr", "noch", "nie", "ver\u00b7m\u00e4h\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "$(", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer Jungfern bleibt von unerme\u00dfner Zeit/", "tokens": ["Und", "im\u00b7mer", "Jung\u00b7fern", "bleibt", "von", "un\u00b7er\u00b7me\u00df\u00b7ner", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die seinem Heiligthum Apollo zugezehlet/", "tokens": ["Die", "sei\u00b7nem", "Hei\u00b7lig\u00b7thum", "A\u00b7pol\u00b7lo", "zu\u00b7ge\u00b7zeh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit welchen noch sein Haar und Scheitel wird erfreut;", "tokens": ["Mit", "wel\u00b7chen", "noch", "sein", "Haar", "und", "Schei\u00b7tel", "wird", "er\u00b7freut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADV", "PPOSAT", "NN", "KON", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Umbgebet dieses Grab/ beschattet itzt die Bahre/", "tokens": ["U\u00b7mbge\u00b7bet", "die\u00b7ses", "Grab", "/", "be\u00b7schat\u00b7tet", "itzt", "die", "Bah\u00b7re", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$(", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Schenckt derer Einsamkeit noch eure gr\u00fcne Nacht.", "tokens": ["Schenckt", "de\u00b7rer", "Ein\u00b7sam\u00b7keit", "noch", "eu\u00b7re", "gr\u00fc\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Vor stundet ihr ein Schmuck nur in der G\u00f6tter Haare/", "tokens": ["Vor", "stun\u00b7det", "ihr", "ein", "Schmuck", "nur", "in", "der", "G\u00f6t\u00b7ter", "Haa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jetzt hat der blasse Tod euch Wermuth gleich gemacht.", "tokens": ["Jetzt", "hat", "der", "blas\u00b7se", "Tod", "euch", "Wer\u00b7muth", "gleich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Flecht Aest\u2019 in Aeste/ flecht ihr unverlobten Zweige/", "tokens": ["Flecht", "A\u00b7est'", "in", "A\u00b7es\u00b7te", "/", "flecht", "ihr", "un\u00b7ver\u00b7lob\u00b7ten", "Zwei\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$(", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Jhr baut ein Schlaffgemach voll Friede/ voller Ruh/", "tokens": ["Ihr", "baut", "ein", "Schlaff\u00b7ge\u00b7mach", "voll", "Frie\u00b7de", "/", "vol\u00b7ler", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und da\u00df kein scharffer Nord in eure Bl\u00e4tter steige/", "tokens": ["Und", "da\u00df", "kein", "scharf\u00b7fer", "Nord", "in", "eu\u00b7re", "Bl\u00e4t\u00b7ter", "stei\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So deckt euch Zephyr selbst mit seinen Fl\u00fcgeln zu.", "tokens": ["So", "deckt", "euch", "Ze\u00b7phyr", "selbst", "mit", "sei\u00b7nen", "Fl\u00fc\u00b7geln", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier ist nicht Daphne Flucht und Ph\u00f6bus heisse Flammen/", "tokens": ["Hier", "ist", "nicht", "Daph\u00b7ne", "Flucht", "und", "Ph\u00f6\u00b7bus", "heis\u00b7se", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "NN", "KON", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit solchen Fabeln ist mein Reimschlu\u00df nicht erf\u00fcllt/", "tokens": ["Mit", "sol\u00b7chen", "Fa\u00b7beln", "ist", "mein", "Reim\u00b7schlu\u00df", "nicht", "er\u00b7f\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die/ so die Eitelkeit der Welt pflag zu verdammen/", "tokens": ["Die", "/", "so", "die", "Ei\u00b7tel\u00b7keit", "der", "Welt", "pflag", "zu", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ligt in dem Leichen-Tuch gleich vielen eingeh\u00fcllt.", "tokens": ["Ligt", "in", "dem", "Lei\u00b7chen\u00b7Tuch", "gleich", "vie\u00b7len", "ein\u00b7ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hier w\u00fcnsch ich euren Wald und eure stille Schatten/", "tokens": ["Hier", "w\u00fcnsch", "ich", "eu\u00b7ren", "Wald", "und", "eu\u00b7re", "stil\u00b7le", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und di\u00df verdient die Hand die euch so sehr gehegt.", "tokens": ["Und", "di\u00df", "ver\u00b7dient", "die", "Hand", "die", "euch", "so", "sehr", "ge\u00b7hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Kommt derer Einsamkeit auch in der Grufft zu statten/", "tokens": ["Kommt", "de\u00b7rer", "Ein\u00b7sam\u00b7keit", "auch", "in", "der", "Grufft", "zu", "stat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.20": {"text": "Die nichts im Leben mehr als Einsamkeit gepflegt.", "tokens": ["Die", "nichts", "im", "Le\u00b7ben", "mehr", "als", "Ein\u00b7sam\u00b7keit", "ge\u00b7pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "ADV", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Ruh ihr h\u00f6chstes Gut/ GOtt ihren Schatz geheissen/", "tokens": ["Die", "Ruh", "ihr", "h\u00f6chs\u00b7tes", "Gut", "/", "Gott", "ih\u00b7ren", "Schatz", "ge\u00b7heis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$(", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nicht in der Eitelkeit verwickelt ihren Fu\u00df.", "tokens": ["Nicht", "in", "der", "Ei\u00b7tel\u00b7keit", "ver\u00b7wi\u00b7ckelt", "ih\u00b7ren", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ver\u00e4chtlich angeschaut der Welt ihr falsches Gleissen/", "tokens": ["Ver\u00b7\u00e4cht\u00b7lich", "an\u00b7ge\u00b7schaut", "der", "Welt", "ihr", "fal\u00b7sches", "Gleis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So einem Nebel gleich im Anfang schwinden mu\u00df.", "tokens": ["So", "ei\u00b7nem", "Ne\u00b7bel", "gleich", "im", "An\u00b7fang", "schwin\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die edle ", "tokens": ["Die", "ed\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Sie meynt da\u00df sie kein Lentz gleich eurem Zierath schm\u00fcckt.", "tokens": ["Sie", "meynt", "da\u00df", "sie", "kein", "Lentz", "gleich", "eu\u00b7rem", "Zie\u00b7rath", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "PIAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sie will auch nur bey euch ihr Sieges-B\u00e4ume/ wohnen", "tokens": ["Sie", "will", "auch", "nur", "bey", "euch", "ihr", "Sie\u00b7ges\u00b7B\u00e4u\u00b7me", "/", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PPER", "PPOSAT", "NN", "$(", "VVINF"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Weil ihr bey Lebens-Zeit vielf\u00e4ltig sie erquickt.", "tokens": ["Weil", "ihr", "bey", "Le\u00b7bens\u00b7Zeit", "viel\u00b7f\u00e4l\u00b7tig", "sie", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jtzt findet sie die Ruh/ hier war sie nicht zu finden/", "tokens": ["Jtzt", "fin\u00b7det", "sie", "die", "Ruh", "/", "hier", "war", "sie", "nicht", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "ADV", "VAFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wie eingezogen auch ihr gantzes Leben hie\u00df!", "tokens": ["Wie", "ein\u00b7ge\u00b7zo\u00b7gen", "auch", "ihr", "gant\u00b7zes", "Le\u00b7ben", "hie\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sie wird kein Sorgen-Netz und Kummer-Garn mehr binden", "tokens": ["Sie", "wird", "kein", "Sor\u00b7gen\u00b7Netz", "und", "Kum\u00b7mer\u00b7Garn", "mehr", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Noch Kranckheit/ die ihr offt von Kr\u00e4fften wenig lie\u00df.", "tokens": ["Noch", "Kran\u00b7ck\u00b7heit", "/", "die", "ihr", "offt", "von", "Kr\u00e4ff\u00b7ten", "we\u00b7nig", "lie\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "PRELS", "PPER", "ADV", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Und weil ihr ja der Welt vermummte Heucheleyen/", "tokens": ["Und", "weil", "ihr", "ja", "der", "Welt", "ver\u00b7mumm\u00b7te", "Heu\u00b7che\u00b7le\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.34": {"text": "Mehr als zu wol bekandt/ so stie\u00df ihr himmlisch Sinn", "tokens": ["Mehr", "als", "zu", "wol", "be\u00b7kandt", "/", "so", "stie\u00df", "ihr", "himm\u00b7lisch", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "APPR", "ADV", "ADJD", "$(", "ADV", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Gro\u00dfm\u00fcthig von sich aus/ was Menschen kan erfreuen/", "tokens": ["Gro\u00df\u00b7m\u00fct\u00b7hig", "von", "sich", "aus", "/", "was", "Men\u00b7schen", "kan", "er\u00b7freu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "APPR", "$(", "PWS", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Worumb der meiste Theil offt gibt den Himmel hin.", "tokens": ["Wo\u00b7rumb", "der", "meis\u00b7te", "Theil", "offt", "gibt", "den", "Him\u00b7mel", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und wie ihr Lorbeern wachst auch bey der gr\u00f6sten Hitze/", "tokens": ["Und", "wie", "ihr", "Lor\u00b7beern", "wachst", "auch", "bey", "der", "gr\u00f6s\u00b7ten", "Hit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und mitten in dem Frost in voller Bl\u00fcthe steht/", "tokens": ["Und", "mit\u00b7ten", "in", "dem", "Frost", "in", "vol\u00b7ler", "Bl\u00fc\u00b7the", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie die Best\u00e4ndigkeit verlacht des Donners Blitze/", "tokens": ["Wie", "die", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "ver\u00b7lacht", "des", "Don\u00b7ners", "Blit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wie scharff auf sie der Sturm von allen Wettern geht:", "tokens": ["Wie", "scharff", "auf", "sie", "der", "Sturm", "von", "al\u00b7len", "Wet\u00b7tern", "geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So trug sie auch ihr Creutz/ was andern eine B\u00fcrde/", "tokens": ["So", "trug", "sie", "auch", "ihr", "Creutz", "/", "was", "an\u00b7dern", "ei\u00b7ne", "B\u00fcr\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", 