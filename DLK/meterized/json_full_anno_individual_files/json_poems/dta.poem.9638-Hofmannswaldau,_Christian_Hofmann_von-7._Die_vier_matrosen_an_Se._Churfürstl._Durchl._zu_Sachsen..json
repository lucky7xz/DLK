{"dta.poem.9638": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "7.  \n  Die vier matrosen an Se. Churf\u00fcrstl.  \n Durchl. zu Sachsen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verzeih uns junger held/ da\u00df wir matrosen fragen:", "tokens": ["Ver\u00b7zeih", "uns", "jun\u00b7ger", "held", "/", "da\u00df", "wir", "mat\u00b7ro\u00b7sen", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VVFIN", "$(", "KOUS", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "[du kanst es uns vielleicht aus der erfahrung sagen/]", "tokens": ["du", "kanst", "es", "uns", "viel\u00b7leicht", "aus", "der", "er\u00b7fah\u00b7rung", "sa\u00b7gen", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$(", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Gleicht uns die liebe nicht auff unsern ruder-b\u00e4ncken?", "tokens": ["Gleicht", "uns", "die", "lie\u00b7be", "nicht", "auff", "un\u00b7sern", "ru\u00b7der\u00b7b\u00e4n\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jm rudern kehren wir den r\u00fccken nach dem port;", "tokens": ["Jm", "ru\u00b7dern", "keh\u00b7ren", "wir", "den", "r\u00fc\u00b7cken", "nach", "dem", "port", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und der ist doch der ort/", "tokens": ["Und", "der", "ist", "doch", "der", "ort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wohin wir unser schiff mit allen kr\u00e4fften lencken.", "tokens": ["Wo\u00b7hin", "wir", "un\u00b7ser", "schiff", "mit", "al\u00b7len", "kr\u00e4ff\u00b7ten", "len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "VVFIN", "APPR", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die schlauen liebenden/ die machens freylich so/", "tokens": ["Die", "schlau\u00b7en", "lie\u00b7ben\u00b7den", "/", "die", "ma\u00b7chens", "frey\u00b7lich", "so", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "VVFIN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie stellen sich gar kalt/ und brennen lichter-loh:", "tokens": ["Sie", "stel\u00b7len", "sich", "gar", "kalt", "/", "und", "bren\u00b7nen", "lich\u00b7ter\u00b7loh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "$(", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie sehen nicht dahin/ wohin sie doch gedencken.", "tokens": ["Sie", "se\u00b7hen", "nicht", "da\u00b7hin", "/", "wo\u00b7hin", "sie", "doch", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PAV", "$(", "PWAV", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}