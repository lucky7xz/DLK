{"dta.poem.16084": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "138.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erwirb ein Gut, da\u00df du es einem Erben lassest,", "tokens": ["Er\u00b7wirb", "ein", "Gut", ",", "da\u00df", "du", "es", "ei\u00b7nem", "Er\u00b7ben", "las\u00b7sest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einen Namen, der ihn schm\u00fcckt, wann du erblassest.", "tokens": ["Und", "ei\u00b7nen", "Na\u00b7men", ",", "der", "ihn", "schm\u00fcckt", ",", "wann", "du", "er\u00b7blas\u00b7sest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie wenig, was ein Mensch von dieser Welt genie\u00dft,", "tokens": ["Wie", "we\u00b7nig", ",", "was", "ein", "Mensch", "von", "die\u00b7ser", "Welt", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn seine Spanne Zeit die Zukunft nicht umschlie\u00dft.", "tokens": ["Wenn", "sei\u00b7ne", "Span\u00b7ne", "Zeit", "die", "Zu\u00b7kunft", "nicht", "um\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Genie\u00dfen wird dein Kind, was du nicht hast genossen;", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", "wird", "dein", "Kind", ",", "was", "du", "nicht", "hast", "ge\u00b7nos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "PWS", "PPER", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In diesem Traume sind die Augen sanft geschlossen.", "tokens": ["In", "die\u00b7sem", "Trau\u00b7me", "sind", "die", "Au\u00b7gen", "sanft", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}