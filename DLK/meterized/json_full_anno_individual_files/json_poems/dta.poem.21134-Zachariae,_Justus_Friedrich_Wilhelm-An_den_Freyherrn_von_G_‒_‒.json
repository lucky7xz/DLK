{"dta.poem.21134": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An den Freyherrn von G \u2012 \u2012", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der du in Acten versenkt, verwirrte Processe durch-\nw\u00fchlest,", "tokens": ["Der", "du", "in", "Ac\u00b7ten", "ver\u00b7senkt", ",", "ver\u00b7wirr\u00b7te", "Pro\u00b7ces\u00b7se", "durch", "w\u00fch\u00b7lest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "$,", "VVFIN", "NN", "TRUNC", "VVFIN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und deine Leyer vergessen hast;", "tokens": ["Und", "dei\u00b7ne", "Le\u00b7yer", "ver\u00b7ges\u00b7sen", "hast", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "G \u2014, opfre nicht stets auf dem Altare der Themis,", "tokens": ["G", ",", "opf\u00b7re", "nicht", "stets", "auf", "dem", "Al\u00b7ta\u00b7re", "der", "The\u00b7mis", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "$,", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Und flieh die staubichte Canzeley.", "tokens": ["Und", "flieh", "die", "stau\u00b7bich\u00b7te", "Can\u00b7ze\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Die Musen vertragen sich nicht mit Advokaten und", "tokens": ["Die", "Mu\u00b7sen", "ver\u00b7tra\u00b7gen", "sich", "nicht", "mit", "Ad\u00b7vo\u00b7ka\u00b7ten", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "KON"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie fliehn Archive voll Actenstaub.", "tokens": ["Sie", "fliehn", "Ar\u00b7chi\u00b7ve", "voll", "Ac\u00b7ten\u00b7staub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht oft dringt sich der Geschmack bis zu dem rechten-", "tokens": ["Nicht", "oft", "dringt", "sich", "der", "Ge\u00b7schmack", "bis", "zu", "dem", "rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "APPR", "ART", "TRUNC"], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Das von der Zanksucht der Menschen lebt.", "tokens": ["Das", "von", "der", "Zank\u00b7sucht", "der", "Men\u00b7schen", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und du, du suchtest vielleicht den hohen Trieb zu er-", "tokens": ["Und", "du", ",", "du", "such\u00b7test", "viel\u00b7leicht", "den", "ho\u00b7hen", "Trieb", "zu", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der dich zum Tempel der Zukunft f\u00fchrt?", "tokens": ["Der", "dich", "zum", "Tem\u00b7pel", "der", "Zu\u00b7kunft", "f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nein, dazu bist du zu gro\u00df! Auf! stimme von neuem", "tokens": ["Nein", ",", "da\u00b7zu", "bist", "du", "zu", "gro\u00df", "!", "Auf", "!", "stim\u00b7me", "von", "neu\u00b7em"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PAV", "VAFIN", "PPER", "PTKA", "ADJD", "$.", "APPR", "$.", "VVFIN", "APPR", "ADJA"], "meter": "+---+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der oft der Leinenstrand zugeh\u00f6rt.", "tokens": ["Der", "oft", "der", "Lei\u00b7nen\u00b7strand", "zu\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Schnell geht dies Leben dahin, und man vergi\u00dft nach", "tokens": ["Schnell", "geht", "dies", "Le\u00b7ben", "da\u00b7hin", ",", "und", "man", "ver\u00b7gi\u00dft", "nach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PDS", "NN", "PAV", "$,", "KON", "PIS", "VVFIN", "APPR"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Selbst Helden ohne des Dichters Kunst.", "tokens": ["Selbst", "Hel\u00b7den", "oh\u00b7ne", "des", "Dich\u00b7ters", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Viel Millionen sind Staub; l\u00e4ngst sind die Namen", "tokens": ["Viel", "Mil\u00b7lion\u00b7en", "sind", "Staub", ";", "l\u00e4ngst", "sind", "die", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "NN", "$.", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch lebt Homerus und Flaccus noch.", "tokens": ["Doch", "lebt", "Ho\u00b7me\u00b7rus", "und", "Flac\u00b7cus", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "KON", "NE", "ADV", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}}}}