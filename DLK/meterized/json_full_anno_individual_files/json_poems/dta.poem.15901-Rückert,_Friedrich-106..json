{"dta.poem.15901": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "106.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wenig oder viel des Sch\u00f6nen mir gelang,", "tokens": ["Wie", "we\u00b7nig", "o\u00b7der", "viel", "des", "Sch\u00f6\u00b7nen", "mir", "ge\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "KON", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erscheint mir doch am Ziel naturgem\u00e4\u00df mein Gang.", "tokens": ["Er\u00b7scheint", "mir", "doch", "am", "Ziel", "na\u00b7tur\u00b7ge\u00b7m\u00e4\u00df", "mein", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich sehe, da\u00df ich bin vom Schauen ausgegangen,", "tokens": ["Ich", "se\u00b7he", ",", "da\u00df", "ich", "bin", "vom", "Schau\u00b7en", "aus\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um durchs Empfinden hin zum Denken zu gelangen.", "tokens": ["Um", "durchs", "Emp\u00b7fin\u00b7den", "hin", "zum", "Den\u00b7ken", "zu", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPRART", "NN", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "me