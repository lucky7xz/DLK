{"textgrid.poem.55860": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nacht, stille Nacht, in die verwoben sind", "tokens": ["Nacht", ",", "stil\u00b7le", "Nacht", ",", "in", "die", "ver\u00b7wo\u00b7ben", "sind"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "APPR", "ART", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ganz wei\u00dfe Dinge, rote, bunte Dinge,", "tokens": ["ganz", "wei\u00b7\u00dfe", "Din\u00b7ge", ",", "ro\u00b7te", ",", "bun\u00b7te", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "verstreute Farben, die erhoben sind", "tokens": ["ver\u00b7streu\u00b7te", "Far\u00b7ben", ",", "die", "er\u00b7ho\u00b7ben", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "zu Einem Dunkel Einer Stille, \u2013 bringe", "tokens": ["zu", "Ei\u00b7nem", "Dun\u00b7kel", "Ei\u00b7ner", "Stil\u00b7le", ",", "\u2013", "brin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "$(", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "doch mich auch in Beziehung zu dem Vielen,", "tokens": ["doch", "mich", "auch", "in", "Be\u00b7zie\u00b7hung", "zu", "dem", "Vie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "das du erwirbst und \u00fcberredest. Spielen", "tokens": ["das", "du", "er\u00b7wirbst", "und", "\u00fc\u00b7berr\u00b7e\u00b7dest", ".", "Spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "VVFIN", "KON", "VVFIN", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "denn meine Sinne noch zu sehr mit Licht?", "tokens": ["denn", "mei\u00b7ne", "Sin\u00b7ne", "noch", "zu", "sehr", "mit", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PTKA", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "W\u00fcrde sich denn mein Angesicht", "tokens": ["W\u00fcr\u00b7de", "sich", "denn", "mein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ADV", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "noch immer st\u00f6rend von den Gegenst\u00e4nden", "tokens": ["noch", "im\u00b7mer", "st\u00f6\u00b7rend", "von", "den", "Ge\u00b7gen\u00b7st\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "abheben? Urteile nach meinen H\u00e4nden:", "tokens": ["ab\u00b7he\u00b7ben", "?", "Ur\u00b7tei\u00b7le", "nach", "mei\u00b7nen", "H\u00e4n\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$.", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Liegen sie nicht wie Werkzeug da und Ding?", "tokens": ["Lie\u00b7gen", "sie", "nicht", "wie", "Werk\u00b7zeug", "da", "und", "Ding", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "KOKOM", "NN", "ADV", "KON", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.12": {"text": "Ist nicht der Ring selbst schlicht", "tokens": ["Ist", "nicht", "der", "Ring", "selbst", "schlicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "an meiner Hand, und liegt das Licht", "tokens": ["an", "mei\u00b7ner", "Hand", ",", "und", "liegt", "das", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "nicht ganz so, voll Vertrauen, \u00fcber ihnen, \u2013", "tokens": ["nicht", "ganz", "so", ",", "voll", "Ver\u00b7trau\u00b7en", ",", "\u00fc\u00b7ber", "ih\u00b7nen", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "$,", "ADJD", "NN", "$,", "APPR", "PPER", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "als ob sie Wege w\u00e4ren, die, beschienen,", "tokens": ["als", "ob", "sie", "We\u00b7ge", "w\u00e4\u00b7ren", ",", "die", ",", "be\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "NN", "VAFIN", "$,", "PRELS", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "nicht anders sich verzweigen, als im Dunkel?...", "tokens": ["nicht", "an\u00b7ders", "sich", "ver\u00b7zwei\u00b7gen", ",", "als", "im", "Dun\u00b7kel", "?", "..."], "token_info": ["word", "word", "word", "wo