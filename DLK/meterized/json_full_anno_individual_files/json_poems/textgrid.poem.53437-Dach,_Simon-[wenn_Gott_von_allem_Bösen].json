{"textgrid.poem.53437": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[wenn Gott von allem B\u00f6sen]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Gott von allem B\u00f6sen", "tokens": ["Wenn", "Gott", "von", "al\u00b7lem", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PIS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und dieser Lebens-Noht", "tokens": ["Und", "die\u00b7ser", "Le\u00b7bens\u00b7Noht"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird meine Seel' erl\u00f6sen", "tokens": ["Wird", "mei\u00b7ne", "Seel'", "er\u00b7l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch einen selign Tod,", "tokens": ["Durch", "ei\u00b7nen", "se\u00b7lign", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich werd' auffgenommen", "tokens": ["Da\u00df", "ich", "werd'", "auff\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gro\u00df, herrlich, himmlisch, rein,", "tokens": ["Gro\u00df", ",", "herr\u00b7lich", ",", "himm\u00b7lisch", ",", "rein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hoch in die Zahl der Frommen,", "tokens": ["Hoch", "in", "die", "Zahl", "der", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie selig werd' ich seyn.", "tokens": ["Wie", "se\u00b7lig", "werd'", "ich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Mund wird nichts als lachen,", "tokens": ["Mein", "Mund", "wird", "nichts", "als", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "KOKOM", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und meiner Zungen Klang", "tokens": ["Und", "mei\u00b7ner", "Zun\u00b7gen", "Klang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird nichts als Lieder machen,", "tokens": ["Wird", "nichts", "als", "Lie\u00b7der", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gott unserm Heyl zu Danck,", "tokens": ["Gott", "un\u00b7serm", "Heyl", "zu", "Danck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihm werd' ich Ehre bringen,", "tokens": ["Ihm", "werd'", "ich", "Eh\u00b7re", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von seiner Wercke Zahl", "tokens": ["Von", "sei\u00b7ner", "Wer\u00b7cke", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wird heilig wieder klingen", "tokens": ["Wird", "hei\u00b7lig", "wie\u00b7der", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der gantze Himmels-Saal.", "tokens": ["Der", "gant\u00b7ze", "Him\u00b7mels\u00b7Saal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Herr, wende mein Verlangen,", "tokens": ["Herr", ",", "wen\u00b7de", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich der Bande frey,", "tokens": ["Da\u00df", "ich", "der", "Ban\u00b7de", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darin ich bin gefangen", "tokens": ["Da\u00b7rin", "ich", "bin", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und gantz mein eigen sey:", "tokens": ["Und", "gantz", "mein", "ei\u00b7gen", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So lang ich hie mu\u00df leben,", "tokens": ["So", "lang", "ich", "hie", "mu\u00df", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So bin ich immerzu", "tokens": ["So", "bin", "ich", "im\u00b7mer\u00b7zu"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit S\u00fcnden nur umbgeben", "tokens": ["Mit", "S\u00fcn\u00b7den", "nur", "umb\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und finde keine Ruh.", "tokens": ["Und", "fin\u00b7de", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was dein Gesetz mir zeiget,", "tokens": ["Was", "dein", "Ge\u00b7setz", "mir", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Belustigt meinen Geist,", "tokens": ["Be\u00b7lus\u00b7tigt", "mei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ist mein Fleisch geneiget", "tokens": ["Doch", "ist", "mein", "Fleisch", "ge\u00b7nei\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Argen allermeist,", "tokens": ["Zum", "Ar\u00b7gen", "al\u00b7ler\u00b7meist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich kann mich offt nicht retten", "tokens": ["Ich", "kann", "mich", "offt", "nicht", "ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr W\u00fcnschen und Begier,", "tokens": ["F\u00fcr", "W\u00fcn\u00b7schen", "und", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und schrey in diesen Ketten:", "tokens": ["Und", "schrey", "in", "die\u00b7sen", "Ket\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ach Gott, wer hilffet mir.", "tokens": ["Ach", "Gott", ",", "wer", "hilf\u00b7fet", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vom Jammer den ich treibe,", "tokens": ["Vom", "Jam\u00b7mer", "den", "ich", "trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von meines Fleisches Streit,", "tokens": ["Von", "mei\u00b7nes", "Flei\u00b7sches", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dieses Todes Leibe", "tokens": ["Und", "die\u00b7ses", "To\u00b7des", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist niemand der mich freyt?", "tokens": ["Ist", "nie\u00b7mand", "der", "mich", "freyt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wil ich alles leiden,", "tokens": ["Doch", "wil", "ich", "al\u00b7les", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn du, O Gott, nur nicht", "tokens": ["Wenn", "du", ",", "O", "Gott", ",", "nur", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NE", "NN", "$,", "ADV", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dich wollest von mir scheiden", "tokens": ["Dich", "wol\u00b7lest", "von", "mir", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit deinem Angesicht.", "tokens": ["Mit", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "La\u00df deinen Geist mich st\u00e4rken,", "tokens": ["La\u00df", "dei\u00b7nen", "Geist", "mich", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mach da\u00df ich \u00fcberall", "tokens": ["Mach", "da\u00df", "ich", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "PPER", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Kan seinen Beystand mercken,", "tokens": ["Kan", "sei\u00b7nen", "Beys\u00b7tand", "mer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So f\u00fcrcht' ich keinen Fall,", "tokens": ["So", "f\u00fcrcht'", "ich", "kei\u00b7nen", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und ob ich lang mu\u00df weinen,", "tokens": ["Und", "ob", "ich", "lang", "mu\u00df", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So wird die Sonne mir", "tokens": ["So", "wird", "die", "Son\u00b7ne", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Um so viel heller scheinen", "tokens": ["Um", "so", "viel", "hel\u00b7ler", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit unbew\u00f6lckter Zier.", "tokens": ["Mit", "un\u00b7be\u00b7w\u00f6lck\u00b7ter", "Zier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hie mu\u00df ich Samen streuen", "tokens": ["Hie", "mu\u00df", "ich", "Sa\u00b7men", "streu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Mit Thr\u00e4nen vieler Pein,", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "vie\u00b7ler", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort werd' ich Wonne meyen,", "tokens": ["Dort", "werd'", "ich", "Won\u00b7ne", "me\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Der Ende nie wird seyn:", "tokens": ["Der", "En\u00b7de", "nie", "wird", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hie mu\u00df ich traurig singen", "tokens": ["Hie", "mu\u00df", "ich", "trau\u00b7rig", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und klagen meine Zeit,", "tokens": ["Und", "kla\u00b7gen", "mei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dort werd' ich Garben bringen", "tokens": ["Dort", "werd'", "ich", "Gar\u00b7ben", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In ewger Herrligkeit.", "tokens": ["In", "ew\u00b7ger", "Herr\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wenn Gott von allem B\u00f6sen", "tokens": ["Wenn", "Gott", "von", "al\u00b7lem", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PIS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und dieser Lebens-Noht", "tokens": ["Und", "die\u00b7ser", "Le\u00b7bens\u00b7Noht"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird meine Seel' erl\u00f6sen", "tokens": ["Wird", "mei\u00b7ne", "Seel'", "er\u00b7l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch einen selign Tod,", "tokens": ["Durch", "ei\u00b7nen", "se\u00b7lign", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich werd' auffgenommen", "tokens": ["Da\u00df", "ich", "werd'", "auff\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gro\u00df, herrlich, himmlisch, rein,", "tokens": ["Gro\u00df", ",", "herr\u00b7lich", ",", "himm\u00b7lisch", ",", "rein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hoch in die Zahl der Frommen,", "tokens": ["Hoch", "in", "die", "Zahl", "der", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie selig werd' ich seyn.", "tokens": ["Wie", "se\u00b7lig", "werd'", "ich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mein Mund wird nichts als lachen,", "tokens": ["Mein", "Mund", "wird", "nichts", "als", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "KOKOM", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und meiner Zungen Klang", "tokens": ["Und", "mei\u00b7ner", "Zun\u00b7gen", "Klang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird nichts als Lieder machen,", "tokens": ["Wird", "nichts", "als", "Lie\u00b7der", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gott unserm Heyl zu Danck,", "tokens": ["Gott", "un\u00b7serm", "Heyl", "zu", "Danck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihm werd' ich Ehre bringen,", "tokens": ["Ihm", "werd'", "ich", "Eh\u00b7re", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von seiner Wercke Zahl", "tokens": ["Von", "sei\u00b7ner", "Wer\u00b7cke", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wird heilig wieder klingen", "tokens": ["Wird", "hei\u00b7lig", "wie\u00b7der", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der gantze Himmels-Saal.", "tokens": ["Der", "gant\u00b7ze", "Him\u00b7mels\u00b7Saal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Herr, we