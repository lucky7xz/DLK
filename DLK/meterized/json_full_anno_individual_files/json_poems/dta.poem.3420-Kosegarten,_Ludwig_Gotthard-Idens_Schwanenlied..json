{"dta.poem.3420": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Idens Schwanenlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie schaust du aus dem Nebelflor,", "tokens": ["Wie", "schaust", "du", "aus", "dem", "Ne\u00b7bel\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Sonne, bleich und m\u00fcde!", "tokens": ["O", "Son\u00b7ne", ",", "bleich", "und", "m\u00fc\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es schwirrt der Heimchen heis'rer Chor", "tokens": ["Es", "schwirrt", "der", "Heim\u00b7chen", "heis'\u00b7rer", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meinem Schwanenliede.", "tokens": ["Zu", "mei\u00b7nem", "Schwa\u00b7nen\u00b7lie\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es girrt die scheidende Natur", "tokens": ["Es", "girrt", "die", "schei\u00b7den\u00b7de", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Lebewohl so traurig;", "tokens": ["Ihr", "Le\u00b7be\u00b7wohl", "so", "trau\u00b7rig", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es stehen Busch und Wald und Flur", "tokens": ["Es", "ste\u00b7hen", "Busch", "und", "Wald", "und", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So trostlos und so schaurig.", "tokens": ["So", "trost\u00b7los", "und", "so", "schau\u00b7rig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Entbl\u00e4ttert steht der Erlenhayn,", "tokens": ["Ent\u00b7bl\u00e4t\u00b7tert", "steht", "der", "Er\u00b7len\u00b7hayn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entlaubt der traute Garten,", "tokens": ["Ent\u00b7laubt", "der", "trau\u00b7te", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo Er und ich im Mondenschein", "tokens": ["Wo", "Er", "und", "ich", "im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "KON", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einander bang' erharrten;", "tokens": ["Ein\u00b7an\u00b7der", "bang'", "er\u00b7harr\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wo Er und ich im Mondenblitz,", "tokens": ["Wo", "Er", "und", "ich", "im", "Mon\u00b7den\u00b7blitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KON", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schirm der Linde sassen,", "tokens": ["Im", "Schirm", "der", "Lin\u00b7de", "sas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und auf des Rasens weichem Sitz", "tokens": ["Und", "auf", "des", "Ra\u00b7sens", "wei\u00b7chem", "Sitz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der \u00f6den Welt vergassen;", "tokens": ["Der", "\u00f6\u00b7den", "Welt", "ver\u00b7gas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wo ich, gelehnt an seine Brust,", "tokens": ["Wo", "ich", ",", "ge\u00b7lehnt", "an", "sei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In s\u00fcsse Tr\u00e4ume nickte,", "tokens": ["In", "s\u00fcs\u00b7se", "Tr\u00e4u\u00b7me", "nick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und holder Wahn, und Edens Lust", "tokens": ["Und", "hol\u00b7der", "Wahn", ",", "und", "E\u00b7dens", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Tr\u00e4umende durchz\u00fcckte.", "tokens": ["Die", "Tr\u00e4u\u00b7men\u00b7de", "durch\u00b7z\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Und schimmerte des Aufgangs Glanz", "tokens": ["Und", "schim\u00b7mer\u00b7te", "des", "Auf\u00b7gangs", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch die verschwiegnen Linden,", "tokens": ["Durch", "die", "ver\u00b7schwieg\u00b7nen", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Pflegt' ich den sch\u00f6nsten Rosenkranz", "tokens": ["Pflegt'", "ich", "den", "sch\u00f6ns\u00b7ten", "Ro\u00b7sen\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm um den Hut zu winden.", "tokens": ["Ihm", "um", "den", "Hut", "zu", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch keine Kr\u00e4nze wird hinfort", "tokens": ["Doch", "kei\u00b7ne", "Kr\u00e4n\u00b7ze", "wird", "hin\u00b7fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein M\u00e4dchen, Freund, dir winden.", "tokens": ["Dein", "M\u00e4d\u00b7chen", ",", "Freund", ",", "dir", "win\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Denn unsre Blumen sind verdorrt,", "tokens": ["Denn", "uns\u00b7re", "Blu\u00b7men", "sind", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entlaubt sind unsre Linden.", "tokens": ["Ent\u00b7laubt", "sind", "uns\u00b7re", "Lin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihr Rosen, die der rauhe Ost", "tokens": ["Ihr", "Ro\u00b7sen", ",", "die", "der", "rau\u00b7he", "Ost"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrem Knospen pfl\u00fcckte;", "tokens": ["In", "ih\u00b7rem", "Knos\u00b7pen", "pfl\u00fcck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Nelken, die der fr\u00fche Frost,", "tokens": ["Ihr", "Nel\u00b7ken", ",", "die", "der", "fr\u00fc\u00b7he", "Frost", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Halbaufgeschlossen, knickte;", "tokens": ["Halb\u00b7auf\u00b7ge\u00b7schlos\u00b7sen", ",", "knick\u00b7te", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ist euer Loos nicht auch mein Loos?", "tokens": ["Ist", "eu\u00b7er", "Loos", "nicht", "auch", "mein", "Loos", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seyd ihr nicht, was ich werde?", "tokens": ["Seyd", "ihr", "nicht", ",", "was", "ich", "wer\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Entkeimt' ich nicht, wie ihr, dem Schooss", "tokens": ["Ent\u00b7keimt'", "ich", "nicht", ",", "wie", "ihr", ",", "dem", "Scho\u00b7oss"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "$,", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der m\u00fctterlichen Erde?", "tokens": ["Der", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Er\u00b7de", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ist nicht mein Halm, so jugendlich,", "tokens": ["Ist", "nicht", "mein", "Halm", ",", "so", "ju\u00b7gend\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schlank emporgeschossen?", "tokens": ["So", "schlank", "em\u00b7por\u00b7ge\u00b7schos\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat meiner Bl\u00fcthen Knospe sich", "tokens": ["Hat", "mei\u00b7ner", "Bl\u00fc\u00b7then", "Knos\u00b7pe", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht dr\u00e4ngend aufgeschlossen?", "tokens": ["Nicht", "dr\u00e4n\u00b7gend", "auf\u00b7ge\u00b7schlos\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Weckt meiner Augen blaues Licht,", "tokens": ["Weckt", "mei\u00b7ner", "Au\u00b7gen", "blau\u00b7es", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Rose meiner Wangen,", "tokens": ["Die", "Ro\u00b7se", "mei\u00b7ner", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Frische meiner Lippen nicht", "tokens": ["Die", "Fri\u00b7sche", "mei\u00b7ner", "Lip\u00b7pen", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der J\u00fcnglinge Verlangen?", "tokens": ["Der", "J\u00fcng\u00b7lin\u00b7ge", "Ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ach klagt um eure Schwester, klagt", "tokens": ["Ach", "klagt", "um", "eu\u00b7re", "Schwes\u00b7ter", ",", "klagt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Rosen und ihr Nelken!", "tokens": ["Ihr", "Ro\u00b7sen", "und", "ihr", "Nel\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie bald, und hin ist meine Pracht,", "tokens": ["Wie", "bald", ",", "und", "hin", "ist", "mei\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KON", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Bl\u00fcthen welken!", "tokens": ["Und", "mei\u00b7ne", "Bl\u00fc\u00b7then", "wel\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Verstreut ist all mein gr\u00fcnes Laub,", "tokens": ["Ver\u00b7streut", "ist", "all", "mein", "gr\u00fc\u00b7nes", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geknickt mein schlanker Stengel,", "tokens": ["Ge\u00b7knickt", "mein", "schlan\u00b7ker", "Sten\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Staub gebettet in den Staub,", "tokens": ["Mein", "Staub", "ge\u00b7bet\u00b7tet", "in", "den", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Geist gereift zum Engel!", "tokens": ["Mein", "Geist", "ge\u00b7reift", "zum", "En\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Der Wandrer, der in meiner Zier,", "tokens": ["Der", "Wand\u00b7rer", ",", "der", "in", "mei\u00b7ner", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meiner Sch\u00f6nheit Schimmer", "tokens": ["In", "mei\u00b7ner", "Sch\u00f6n\u00b7heit", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich schaute, kommt und forscht nach mir,", "tokens": ["Mich", "schau\u00b7te", ",", "kommt", "und", "forscht", "nach", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sieht mich nimmer, nimmer!", "tokens": ["Und", "sieht", "mich", "nim\u00b7mer", ",", "nim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Es kommt der Traute, den ich mir", "tokens": ["Es", "kommt", "der", "Trau\u00b7te", ",", "den", "ich", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erkohren einzig habe. \u2014", "tokens": ["Er\u00b7koh\u00b7ren", "ein\u00b7zig", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach fleuch, Geliebter, fleuch von hier;", "tokens": ["Ach", "fleuch", ",", "Ge\u00b7lieb\u00b7ter", ",", "fleuch", "von", "hier", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "NN", "$,", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein M\u00e4dchen schl\u00e4ft im Grabe.", "tokens": ["Dein", "M\u00e4d\u00b7chen", "schl\u00e4ft", "im", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Ach traure, Theurer, traure nicht!", "tokens": ["Ach", "trau\u00b7re", ",", "Theu\u00b7rer", ",", "trau\u00b7re", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "ADJD", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Grabes Dunkel schwindet,", "tokens": ["Des", "Gra\u00b7bes", "Dun\u00b7kel", "schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und himmlisch und unsterblich Licht", "tokens": ["Und", "himm\u00b7lisch", "und", "uns\u00b7terb\u00b7lich", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00e4nzt dem, der \u00fcberwindet.", "tokens": ["Gl\u00e4nzt", "dem", ",", "der", "\u00fc\u00b7berw\u00b7in\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Triumph! auf Herbstesd\u00e4mmerung", "tokens": ["Tri\u00b7umph", "!", "auf", "Herbs\u00b7tes\u00b7d\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "APPR", "NN"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.2": {"text": "Folgt milder Fr\u00fchlingsschimmer.", "tokens": ["Folgt", "mil\u00b7der", "Fr\u00fch\u00b7lings\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf Trennung folgt Vereinigung,", "tokens": ["Auf", "Tren\u00b7nung", "folgt", "Ver\u00b7ei\u00b7ni\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vereinigung auf immer!", "tokens": ["Ver\u00b7ei\u00b7ni\u00b7gung", "auf", "im\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}