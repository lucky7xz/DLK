{"dta.poem.9496": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "OdE/  \n von lauter Jambischen Versen.  \n Nach der Melodey:  \n Das Gl\u00fcck gantz wanckelm\u00fctig ist etc.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann schon die Sonne scheint einmahl", "tokens": ["Wann", "schon", "die", "Son\u00b7ne", "scheint", "ein\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und durch die wolcken blicket/", "tokens": ["und", "durch", "die", "wol\u00b7cken", "bli\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gra\u00df/ Laub und Thier erquicket/", "tokens": ["Gra\u00df", "/", "Laub", "und", "Thier", "er\u00b7quic\u00b7ket", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Macht lustig Feld und Thal;", "tokens": ["Macht", "lus\u00b7tig", "Feld", "und", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "K\u00f6m\u0303t doch das un gewitter/", "tokens": ["K\u00f6m\u0303t", "doch", "das", "un", "ge\u00b7wit\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "FM", "FM", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und macht die frende bitter/", "tokens": ["und", "macht", "die", "fren\u00b7de", "bit\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Sturmwind sich erhebt/", "tokens": ["Der", "Sturm\u00b7wind", "sich", "er\u00b7hebt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df Meer und Erd\u2019 erbebt/", "tokens": ["Da\u00df", "Meer", "und", "Erd'", "er\u00b7bebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der Vogel zahl", "tokens": ["Der", "Vo\u00b7gel", "zahl"], "token_info": ["word", "word", "word"], "pos": ["ART", "NE", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Schweigt allzumahl/", "tokens": ["Schweigt", "all\u00b7zu\u00b7mahl", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Kein tierelier", "tokens": ["Kein", "tie\u00b7re\u00b7lier"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "fchwingt sich erf\u00fcr/", "tokens": ["fchwingt", "sich", "er\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Der Sonnen-Licht", "tokens": ["Der", "Son\u00b7nen\u00b7Licht"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Kann zeigen nicht", "tokens": ["Kann", "zei\u00b7gen", "nicht"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "VVINF", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Den g\u00fcldnen glantz; das liebliche gesicht", "tokens": ["Den", "g\u00fcld\u00b7nen", "glantz", ";", "das", "lieb\u00b7li\u00b7che", "ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "on das Gl\u00fcck sich zeigt", "tokens": ["on", "das", "Gl\u00fcck", "sich", "zeigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "s einmahl anlachet/", "tokens": ["s", "ein\u00b7mahl", "an\u00b7la\u00b7chet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Lust und freude machet/", "tokens": ["Lust", "und", "freu\u00b7de", "ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn sturm und wetter schweigt/", "tokens": ["Wenn", "sturm", "und", "wet\u00b7ter", "schweigt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bald aber k\u00f6mmt mit hauffen", "tokens": ["Bald", "a\u00b7ber", "k\u00f6mmt", "mit", "hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das ungel\u00fcck gelauffen;", "tokens": ["Das", "un\u00b7ge\u00b7l\u00fcck", "ge\u00b7lauf\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie Schnee und Ei\u00df zergeht/", "tokens": ["Wie", "Schnee", "und", "Ei\u00df", "zer\u00b7geht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Blume nicht besteht/", "tokens": ["Die", "Blu\u00b7me", "nicht", "be\u00b7steht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "So eylt zur\u00fcck", "tokens": ["So", "eylt", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Das schn\u00f6de Gl\u00fcck", "tokens": ["Das", "schn\u00f6\u00b7de", "Gl\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "und weicht mit macht", "tokens": ["und", "weicht", "mit", "macht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "zu Tag und Nacht", "tokens": ["zu", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Es eylt und f", "tokens": ["Es", "eylt", "und", "f"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "So bald es bl\u00fcht/", "tokens": ["So", "bald", "es", "bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Ist nur allein auff fl\u00fcchtigkeit bem\u00fcht.", "tokens": ["Ist", "nur", "al\u00b7lein", "auff", "fl\u00fcch\u00b7tig\u00b7keit", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}