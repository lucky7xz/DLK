{"textgrid.poem.42097": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Don Quichote k\u00e4mpft mit Gott", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da Du mich segnen sollst, was frommt Dir dies,", "tokens": ["Da", "Du", "mich", "seg\u00b7nen", "sollst", ",", "was", "frommt", "Dir", "dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,", "PWS", "VVFIN", "PPER", "PDS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Deine Hand, Gewaltger, mich verstie\u00df,", "tokens": ["Da\u00df", "Dei\u00b7ne", "Hand", ",", "Ge\u00b7walt\u00b7ger", ",", "mich", "ver\u00b7stie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Deine Kraft, Gesegneter, mich lie\u00df,", "tokens": ["Da\u00df", "Dei\u00b7ne", "Kraft", ",", "Ge\u00b7se\u00b7gne\u00b7ter", ",", "mich", "lie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df mich Dein Atem, Z\u00fcrnender, verblie\u00df?", "tokens": ["Da\u00df", "mich", "Dein", "A\u00b7tem", ",", "Z\u00fcr\u00b7nen\u00b7der", ",", "ver\u00b7blie\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun lieg ich irgendwo. Und ich bin nicht mehr", "tokens": ["Nun", "lieg", "ich", "ir\u00b7gend\u00b7wo", ".", "Und", "ich", "bin", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "KON", "PPER", "VAFIN", "PTKNEG", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als Staub im Staube, unerkennbar, sehr", "tokens": ["Als", "Staub", "im", "Stau\u00b7be", ",", "un\u00b7er\u00b7kenn\u00b7bar", ",", "sehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "NN", "APPRART", "NN", "$,", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gering. Und schweift Dein gro\u00dfes Auge her", "tokens": ["Ge\u00b7ring", ".", "Und", "schweift", "Dein", "gro\u00b7\u00dfes", "Au\u00b7ge", "her"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf meine St\u00e4tte, tr\u00fcbt sichs wolkig leer.", "tokens": ["Auf", "mei\u00b7ne", "St\u00e4t\u00b7te", ",", "tr\u00fcbt", "sichs", "wol\u00b7kig", "leer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PIS", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich aber la\u00df Dich nicht. Da Du verneint", "tokens": ["Ich", "a\u00b7ber", "la\u00df", "Dich", "nicht", ".", "Da", "Du", "ver\u00b7neint"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVIMP", "PPER", "PTKNEG", "$.", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir \u00e4ndern Kampf, sieh, Reiner, her: i