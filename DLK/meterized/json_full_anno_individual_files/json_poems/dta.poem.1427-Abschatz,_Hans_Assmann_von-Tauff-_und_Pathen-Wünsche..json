{"dta.poem.1427": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du gr\u00fcssest diese Welt zum Anfang rauher Zeit/", "tokens": ["Du", "gr\u00fcs\u00b7sest", "die\u00b7se", "Welt", "zum", "An\u00b7fang", "rau\u00b7her", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "APPRART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Unschuld Christi Schnee wird izt dein Winter-", "tokens": ["Der", "Un\u00b7schuld", "Chris\u00b7ti", "Schnee", "wird", "izt", "dein", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "NN", "VAFIN", "ADV", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kleid/", "tokens": ["Kleid", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Dich m\u00fcsse durch die Welt ein steter Lentz begleiten/", "tokens": ["Dich", "m\u00fcs\u00b7se", "durch", "die", "Welt", "ein", "ste\u00b7ter", "Lentz", "be\u00b7glei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bi\u00df du den Sommer siehst der k\u00fcnfftgen Ewigkeiten.", "tokens": ["Bi\u00df", "du", "den", "Som\u00b7mer", "siehst", "der", "k\u00fcnfft\u00b7gen", "E\u00b7wig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}