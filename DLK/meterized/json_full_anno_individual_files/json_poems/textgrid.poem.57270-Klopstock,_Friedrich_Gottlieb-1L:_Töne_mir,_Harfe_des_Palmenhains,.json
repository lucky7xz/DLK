{"textgrid.poem.57270": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: T\u00f6ne mir, Harfe des Palmenhains,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00f6ne mir, Harfe des Palmenhains,", "tokens": ["T\u00f6\u00b7ne", "mir", ",", "Har\u00b7fe", "des", "Pal\u00b7men\u00b7hains", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der Lieder Gespielin, die David sang!", "tokens": ["Der", "Lie\u00b7der", "Ge\u00b7spie\u00b7lin", ",", "die", "Da\u00b7vid", "sang", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "NE", "VVFIN", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es erhebt steigender sich Sions Lied,", "tokens": ["Es", "er\u00b7hebt", "stei\u00b7gen\u00b7der", "sich", "Si\u00b7ons", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "NE", "NN", "$,"], "meter": "--++-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wie des Quells, welcher des Hufs Stampfen entscholl.", "tokens": ["Wie", "des", "Quells", ",", "wel\u00b7cher", "des", "Hufs", "Stamp\u00b7fen", "ent\u00b7scholl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-++-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.2": {"line.1": {"text": "H\u00f6her in Wolken, o Palmenhain,", "tokens": ["H\u00f6\u00b7her", "in", "Wol\u00b7ken", ",", "o", "Pal\u00b7men\u00b7hain", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "FM", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Erblickst du das Thal, wie der Lorberwald!", "tokens": ["Er\u00b7blickst", "du", "das", "Thal", ",", "wie", "der", "Lor\u00b7ber\u00b7wald", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und entsenkst Schatten, herab auf den Wald,", "tokens": ["Und", "ent\u00b7senkst", "Schat\u00b7ten", ",", "her\u00b7ab", "auf", "den", "Wald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem Gew\u00f6lk, welches dich deckt, Palme, mit Glanz,", "tokens": ["Dem", "Ge\u00b7w\u00f6lk", ",", "wel\u00b7ches", "dich", "deckt", ",", "Pal\u00b7me", ",", "mit", "Glanz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Tanze, Siona, Triumph einher!", "tokens": ["Tan\u00b7ze", ",", "Si\u00b7o\u00b7na", ",", "Tri\u00b7umph", "ein\u00b7her", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Am Silbergelispel Phiala's trit", "tokens": ["Am", "Sil\u00b7ber\u00b7ge\u00b7lis\u00b7pel", "Phi\u00b7a\u00b7la's", "trit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "VVFIN"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie hervor! schwebet im Tanz! f\u00fchlts, wie du", "tokens": ["Sie", "her\u00b7vor", "!", "schwe\u00b7bet", "im", "Tanz", "!", "f\u00fchlts", ",", "wie", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "PTKVZ", "$.", "VVFIN", "APPRART", "NN", "$.", "VVFIN", "$,", "PWAV", "PPER"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie erhebst, Religion dessen, der ist!", "tokens": ["Sie", "er\u00b7hebst", ",", "Re\u00b7li\u00b7gi\u00b7on", "des\u00b7sen", ",", "der", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "PDS", "$,", "PRELS", "VAFIN", "$."], "meter": "+-++-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.4": {"line.1": {"text": "Seyn wird! und war! Der erbabnen weht", "tokens": ["Seyn", "wird", "!", "und", "war", "!", "Der", "er\u00b7bab\u00b7nen", "weht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "$.", "KON", "VAFIN", "$.", "ART", "ADJA", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sanft Rauschen vom Wipfel der Palme nach.", "tokens": ["Sanft", "Rau\u00b7schen", "vom", "Wip\u00b7fel", "der", "Pal\u00b7me", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "An dem Fall, welchen du t\u00f6nst, reiner Quell", "tokens": ["An", "dem", "Fall", ",", "wel\u00b7chen", "du", "t\u00f6nst", ",", "rei\u00b7ner", "Quell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAT", "PPER", "VVFIN", "$,", "ADJA", "NN"], "meter": "--+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Des Krystals, rufen ihr nach Berge Triumph!", "tokens": ["Des", "Krys\u00b7tals", ",", "ru\u00b7fen", "ihr", "nach", "Ber\u00b7ge", "Tri\u00b7umph", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Feuriger blickt sie! ihr Haupt umkr\u00e4nzt", "tokens": ["Feu\u00b7ri\u00b7ger", "blickt", "sie", "!", "ihr", "Haupt", "um\u00b7kr\u00e4nzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die Rose Sarona, des Blumenthals.", "tokens": ["Die", "Ro\u00b7se", "Sa\u00b7ro\u00b7na", ",", "des", "Blu\u00b7ment\u00b7hals", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Gewand fliesst, wie Gew\u00f6lk, sanft um sie,", "tokens": ["Ihr", "Ge\u00b7wand", "fliesst", ",", "wie", "Ge\u00b7w\u00f6lk", ",", "sanft", "um", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "NN", "$,", "ADJD", "APPR", "PPER", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Wie des Tags Fr\u00fche gef\u00e4rbt, Purpur und Gold.", "tokens": ["Wie", "des", "Tags", "Fr\u00fc\u00b7he", "ge\u00b7f\u00e4rbt", ",", "Pur\u00b7pur", "und", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVPP", "$,", "NN", "KON", "NN", "$."], "meter": "+--+--++--+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Liebevoll schauet, o Sulamith", "tokens": ["Lie\u00b7be\u00b7voll", "schau\u00b7et", ",", "o", "Su\u00b7la\u00b7mith"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "FM", "NE"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Siona, mein Blick dir, und freudig nach!", "tokens": ["Si\u00b7o\u00b7na", ",", "mein", "Blick", "dir", ",", "und", "freu\u00b7dig", "nach", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "PPER", "$,", "KON", "ADJD", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es erf\u00fcllt Wehmuth und Ruh, Wonn' erf\u00fcllt", "tokens": ["Es", "er\u00b7f\u00fcllt", "Weh\u00b7muth", "und", "Ruh", ",", "Wonn'", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "PWAV", "VVPP"], "meter": "--+-+-++-+", "measure": "anapaest.init"}, "line.4": {"text": "Mir das Herz, wenn du dein Lied, himlische, singst.", "tokens": ["Mir", "das", "Herz", ",", "wenn", "du", "dein", "Lied", ",", "him\u00b7li\u00b7sche", ",", "singst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "$,", "ADJA", "$,", "VVFIN", "$."], "meter": "+-+-+-++--+", "measure": "iambic.hexa.chol"}}, "stanza.7": {"line.1": {"text": "H\u00f6rt ihr? Siona begint! schon rauscht", "tokens": ["H\u00f6rt", "ihr", "?", "Si\u00b7o\u00b7na", "be\u00b7gint", "!", "schon", "rauscht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "$.", "NE", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Der heilige Hain von dem Harfenlaut!", "tokens": ["Der", "hei\u00b7li\u00b7ge", "Hain", "von", "dem", "Har\u00b7fen\u00b7laut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Des Krystals Quelle vernimts, horcht, und steht;", "tokens": ["Des", "Krys\u00b7tals", "Quel\u00b7le", "ver\u00b7nimts", ",", "horcht", ",", "und", "steht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Denn es wehn Lispel im Hain rings um sie her.", "tokens": ["Denn", "es", "wehn", "Lis\u00b7pel", "im", "Hain", "rings", "um", "sie", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "APPRART", "NN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+++-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.8": {"line.1": {"text": "Aber itzt st\u00fcrzt sie die Well' herab", "tokens": ["A\u00b7ber", "itzt", "st\u00fcrzt", "sie", "die", "Well'", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mit freudiger Eil! Denn Siona nimt", "tokens": ["Mit", "freu\u00b7di\u00b7ger", "Eil", "!", "Denn", "Si\u00b7o\u00b7na", "nimt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "KON", "NE", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Posaun', h\u00e4lt sie empor, l\u00e4sst sie laut", "tokens": ["Die", "Po\u00b7saun'", ",", "h\u00e4lt", "sie", "em\u00b7por", ",", "l\u00e4sst", "sie", "laut"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "ADJD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Gebirg' hallen! und ruft Donner ins Thal!", "tokens": ["Im", "Ge\u00b7bir\u00b7g'", "hal\u00b7len", "!", "und", "ruft", "Don\u00b7ner", "ins", "Thal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$.", "KON", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.9": {"line.1": {"text": "T\u00f6ne mir, Harfe des Palmenhains,", "tokens": ["T\u00f6\u00b7ne", "mir", ",", "Har\u00b7fe", "des", "Pal\u00b7men\u00b7hains", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der Lieder Gespielin, die David sang!", "tokens": ["Der", "Lie\u00b7der", "Ge\u00b7spie\u00b7lin", ",", "die", "Da\u00b7vid", "sang", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "NE", "VVFIN", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es erhebt steigender sich Sions Lied,", "tokens": ["Es", "er\u00b7hebt", "stei\u00b7gen\u00b7der", "sich", "Si\u00b7ons", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "NE", "NN", "$,"], "meter": "--++-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wie des Quells, welcher des Hufs Stampfen entscholl.", "tokens": ["Wie", "des", "Quells", ",", "wel\u00b7cher", "des", "Hufs", "Stamp\u00b7fen", "ent\u00b7scholl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "wo