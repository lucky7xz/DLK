{"textgrid.poem.65697": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf, r\u00fchret e\u00fcch ihr muntern Saiten,", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, r\u00fchret e\u00fcch ihr muntern Saiten,", "tokens": ["Auf", ",", "r\u00fch\u00b7ret", "e\u00b7\u00fcch", "ihr", "mun\u00b7tern", "Sai\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und flammet meine Geister an,", "tokens": ["Und", "flam\u00b7met", "mei\u00b7ne", "Geis\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Damit ich e\u00fcern Trefflichkeiten,", "tokens": ["Da\u00b7mit", "ich", "e\u00b7\u00fc\u00b7ern", "Treff\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ein w\u00fcrdigs Opfer bringen kan!", "tokens": ["Ein", "w\u00fcr\u00b7digs", "Op\u00b7fer", "brin\u00b7gen", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer ist der nicht in Wollust schwimme,", "tokens": ["Wer", "ist", "der", "nicht", "in", "Wol\u00b7lust", "schwim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn e\u00fcer himmlisches Gestimme", "tokens": ["Wenn", "e\u00b7\u00fc\u00b7er", "himm\u00b7li\u00b7sches", "Ge\u00b7stim\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Durch unsrer Sinnen Tiefen bricht?", "tokens": ["Durch", "uns\u00b7rer", "Sin\u00b7nen", "Tie\u00b7fen", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr spielet schon; Ich bin entz\u00fccket.", "tokens": ["Ihr", "spie\u00b7let", "schon", ";", "Ich", "bin", "ent\u00b7z\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wo werd ich von e\u00fcch hinger\u00fccket?", "tokens": ["Wo", "werd", "ich", "von", "e\u00b7\u00fcch", "hin\u00b7ge\u00b7r\u00fc\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Welch eine Regung f\u00fchl ich nicht!", "tokens": ["Welch", "ei\u00b7ne", "Re\u00b7gung", "f\u00fchl", "ich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So wie die K\u00f6niginn der B\u00fcsche,", "tokens": ["So", "wie", "die", "K\u00f6\u00b7ni\u00b7ginn", "der", "B\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie des Fr\u00fchlings Anmuht f\u00fchlt,", "tokens": ["Wenn", "sie", "des", "Fr\u00fch\u00b7lings", "An\u00b7muht", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Wundervollem Tohngemische", "tokens": ["Mit", "Wun\u00b7der\u00b7vol\u00b7lem", "Tohn\u00b7ge\u00b7mi\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch die erfre\u00fcten L\u00fcfte spielt:", "tokens": ["Durch", "die", "er\u00b7fre\u00fc\u00b7ten", "L\u00fcf\u00b7te", "spielt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So steiget ihr und sinket wieder.", "tokens": ["So", "stei\u00b7get", "ihr", "und", "sin\u00b7ket", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bald lasset ihr e\u00fcch sanfte nider;", "tokens": ["Bald", "las\u00b7set", "ihr", "e\u00b7\u00fcch", "sanf\u00b7te", "ni\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Bald st\u00fcrmet ihr mit Macht herbey.", "tokens": ["Bald", "st\u00fcr\u00b7met", "ihr", "mit", "Macht", "her\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr spielet streng. Ihr spielet sch\u00f6ne", "tokens": ["Ihr", "spie\u00b7let", "streng", ".", "Ihr", "spie\u00b7let", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "PPER", "VVFIN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr mischet e\u00fcre Zaubert\u00f6hne", "tokens": ["Ihr", "mi\u00b7schet", "e\u00b7\u00fc\u00b7re", "Zau\u00b7ber\u00b7t\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Mit tausendfacher Schmeicheley.", "tokens": ["Mit", "tau\u00b7send\u00b7fa\u00b7cher", "Schmei\u00b7che\u00b7ley", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So lernen wir durch Lust und Grausen,", "tokens": ["So", "ler\u00b7nen", "wir", "durch", "Lust", "und", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie kr\u00e4ftig e\u00fcre Z\u00fcge seyn.", "tokens": ["Wie", "kr\u00e4f\u00b7tig", "e\u00b7\u00fc\u00b7re", "Z\u00fc\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bald k\u00f6mmt ein lieblich-sanftes Sausen,", "tokens": ["Bald", "k\u00f6mmt", "ein", "lieb\u00b7lich\u00b7sanf\u00b7tes", "Sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wieget uns in Wollust ein.", "tokens": ["Und", "wie\u00b7get", "uns", "in", "Wol\u00b7lust", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald werden wir von e\u00fcerm Schallen", "tokens": ["Bald", "wer\u00b7den", "wir", "von", "e\u00b7\u00fc\u00b7erm", "Schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mit Furcht und Schrecken \u00fcberfallen;", "tokens": ["Mit", "Furcht", "und", "Schre\u00b7cken", "\u00fc\u00b7berf\u00b7al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bald r\u00fchret ihr uns Geist und Muht;", "tokens": ["Bald", "r\u00fch\u00b7ret", "ihr", "uns", "Geist", "und", "Muht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und bald so f\u00fcgt es e\u00fcer Wille,", "tokens": ["Und", "bald", "so", "f\u00fcgt", "es", "e\u00b7\u00fc\u00b7er", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Da\u00df unter einer holden Stille", "tokens": ["Da\u00df", "un\u00b7ter", "ei\u00b7ner", "hol\u00b7den", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Sturm der Sinnen wieder ruht.", "tokens": ["Der", "Sturm", "der", "Sin\u00b7nen", "wie\u00b7der", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum bleiben e\u00fcre werten Spiele", "tokens": ["Drum", "blei\u00b7ben", "e\u00b7\u00fc\u00b7re", "wer\u00b7ten", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVINF", "VVFIN", "ADJA", "NN"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Das beste Labsal unsrer Brust.", "tokens": ["Das", "bes\u00b7te", "Lab\u00b7sal", "uns\u00b7rer", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wirken in uns ein Gef\u00fchle", "tokens": ["Sie", "wir\u00b7ken", "in", "uns", "ein", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von jenes Paradises Lust.", "tokens": ["Von", "je\u00b7nes", "Pa\u00b7ra\u00b7di\u00b7ses", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ermuntert e\u00fcch, gepriesne Saiten!", "tokens": ["Er\u00b7mun\u00b7tert", "e\u00b7\u00fcch", ",", "ge\u00b7pri\u00b7es\u00b7ne", "Sai\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Verdoppelt e\u00fcre Lieblichkeiten,", "tokens": ["Ver\u00b7dop\u00b7pelt", "e\u00b7\u00fc\u00b7re", "Lieb\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Womit ihr Herz und Sinne zwingt!", "tokens": ["Wo\u00b7mit", "ihr", "Herz", "und", "Sin\u00b7ne", "zwingt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie aber? h\u00f6r ich nicht Climenen", "tokens": ["Wie", "a\u00b7ber", "?", "h\u00f6r", "ich", "nicht", "Cli\u00b7me\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$.", "VVFIN", "PPER", "PTKNEG", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mit ihrer Stimme Wundert\u00f6hnen?", "tokens": ["Mit", "ih\u00b7rer", "Stim\u00b7me", "Wun\u00b7der\u00b7t\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ihr Saiten schweigt! Climene singt.", "tokens": ["Ihr", "Sai\u00b7ten", "schweigt", "!", "Cli\u00b7me\u00b7ne", "singt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "NE", "VVFIN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Auf, r\u00fchret e\u00fcch ihr muntern Saiten,", "tokens": ["Auf", ",", "r\u00fch\u00b7ret", "e\u00b7\u00fcch", "ihr", "mun\u00b7tern", "Sai\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und flammet meine Geister an,", "tokens": ["Und", "flam\u00b7met", "mei\u00b7ne", "Geis\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "