{"dta.poem.23598": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die lateinischen Gl\u00e4ubigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr singt und betet in Latein!", "tokens": ["Ihr", "singt", "und", "be\u00b7tet", "in", "La\u00b7tein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Will Gott kein Gott der Deutschen sein?", "tokens": ["Will", "Gott", "kein", "Gott", "der", "Deut\u00b7schen", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PIAT", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In unsres Feindes Sprache sollen", "tokens": ["In", "uns\u00b7res", "Fein\u00b7des", "Spra\u00b7che", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir Dank und Preis dem H\u00f6chsten zollen?", "tokens": ["Wir", "Dank", "und", "Preis", "dem", "H\u00f6chs\u00b7ten", "zol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist ihm nicht jedes Volk und Reich,", "tokens": ["Ist", "ihm", "nicht", "je\u00b7des", "Volk", "und", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist ihm nicht jede Sprache gleich?", "tokens": ["Ist", "ihm", "nicht", "je\u00b7de", "Spra\u00b7che", "gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr wollt mit fremden, todten T\u00f6nen", "tokens": ["Ihr", "wollt", "mit", "frem\u00b7den", ",", "tod\u00b7ten", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn den Lebendigen vers\u00f6hnen?", "tokens": ["Ihn", "den", "Le\u00b7ben\u00b7di\u00b7gen", "ver\u00b7s\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Zu Gott empor, du deutsches Herz,", "tokens": ["Zu", "Gott", "em\u00b7por", ",", "du", "deut\u00b7sches", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Deutsch bet' und sing' in Freud' und Schmerz!", "tokens": ["Deutsch", "bet'", "und", "sing'", "in", "Freud'", "und", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sprache, die mit dir erschaffen,", "tokens": ["Die", "Spra\u00b7che", ",", "die", "mit", "dir", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ziemt nur vor Gott den Lai'n und Pfaffen.", "tokens": ["Ziemt", "nur", "vor", "Gott", "den", "Lai'n", "und", "Pfaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}