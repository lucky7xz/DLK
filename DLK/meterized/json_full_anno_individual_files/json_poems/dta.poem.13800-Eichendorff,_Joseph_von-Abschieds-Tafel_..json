{"dta.poem.13800": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschieds-Tafel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Dir nun, heitre Sch\u00f6ne,", "tokens": ["Zu", "Dir", "nun", ",", "heit\u00b7re", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wend' ich mich voll Gedanken.", "tokens": ["Wend'", "ich", "mich", "voll", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wie sie zu Dir sich wenden,", "tokens": ["Wie", "sie", "zu", "Dir", "sich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df ich so fr\u00f6hlich sein.", "tokens": ["Mu\u00df", "ich", "so", "fr\u00f6h\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So weit Poeten wohnen,", "tokens": ["So", "weit", "Po\u00b7et\u00b7en", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So weit der W\u00e4lder Kronen,", "tokens": ["So", "weit", "der", "W\u00e4l\u00b7der", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}