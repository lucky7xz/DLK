{"dta.poem.21511": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Den Deutschen in Oesterreich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.85", "af:0.14"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft laut die T\u00f6ne klingen,", "tokens": ["La\u00dft", "laut", "die", "T\u00f6\u00b7ne", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie m\u00e4chtig dr\u00f6hnend Erz,", "tokens": ["Wie", "m\u00e4ch\u00b7tig", "dr\u00f6h\u00b7nend", "Erz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aufschreckend sollen sie dringen", "tokens": ["Auf\u00b7schre\u00b7ckend", "sol\u00b7len", "sie", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VMFIN", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In jedes schwanke Herz;", "tokens": ["In", "je\u00b7des", "schwan\u00b7ke", "Herz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem Schwerte gleich soll\u2019s wettern", "tokens": ["Dem", "Schwer\u00b7te", "gleich", "soll's", "wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Wort gewaltigen Streichs,", "tokens": ["Das", "Wort", "ge\u00b7wal\u00b7ti\u00b7gen", "Streichs", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Das Kampflied soll erschmettern,", "tokens": ["Das", "Kampf\u00b7lied", "soll", "er\u00b7schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Deutschen Oesterreichs!", "tokens": ["Der", "Deut\u00b7schen", "O\u00b7es\u00b7ter\u00b7reichs", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Das war ein hei\u00dfes M\u00fchen,", "tokens": ["Das", "war", "ein", "hei\u00b7\u00dfes", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rastlose deutsche Hand,", "tokens": ["Rast\u00b7lo\u00b7se", "deut\u00b7sche", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Bis du in helles Bl\u00fchen", "tokens": ["Bis", "du", "in", "hel\u00b7les", "Bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gekleidet weit das Land;", "tokens": ["Ge\u00b7klei\u00b7det", "weit", "das", "Land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das war ein eifrig Bauen,", "tokens": ["Das", "war", "ein", "eif\u00b7rig", "Bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Zimmern unverzagt,", "tokens": ["Ein", "Zim\u00b7mern", "un\u00b7ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis herrlich anzuschauen", "tokens": ["Bis", "herr\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJD", "VVIZU"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Bau zur H\u00f6h\u2019 geragt!", "tokens": ["Der", "Bau", "zur", "H\u00f6h'", "ge\u00b7ragt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und sieh\u2019! in deutscher Krone", "tokens": ["Und", "sieh'", "!", "in", "deut\u00b7scher", "Kro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gl\u00e4nzt Austria so hehr;", "tokens": ["Gl\u00e4nzt", "Aust\u00b7ria", "so", "hehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dem deutschen Flei\u00df zum Lohne", "tokens": ["Dem", "deut\u00b7schen", "Flei\u00df", "zum", "Loh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schwillt golden der Saaten Meer;", "tokens": ["Schwillt", "gol\u00b7den", "der", "Saa\u00b7ten", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Durch deutsches Wort verbunden", "tokens": ["Durch", "deut\u00b7sches", "Wort", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schlie\u00dft eng sich Glied an Glied;", "tokens": ["Schlie\u00dft", "eng", "sich", "Glied", "an", "Glied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den sch\u00f6nsten Kranz gewunden", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "Kranz", "ge\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hat rings das deutsche Lied.", "tokens": ["Hat", "rings", "das", "deut\u00b7sche", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und soll das nun zerfallen,", "tokens": ["Und", "soll", "das", "nun", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PDS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was hoch und heilig stand?", "tokens": ["Was", "hoch", "und", "hei\u00b7lig", "stand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und soll dein Wort verhallen,", "tokens": ["Und", "soll", "dein", "Wort", "ver\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Volk, im eignen Land?", "tokens": ["Mein", "Volk", ",", "im", "eig\u00b7nen", "Land", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nein, unersch\u00fcttert setze", "tokens": ["Nein", ",", "un\u00b7er\u00b7sch\u00fct\u00b7tert", "set\u00b7ze"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die volle Kraft darein,", "tokens": ["Die", "vol\u00b7le", "Kraft", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du wahr\u2019st die h\u00f6chsten Sch\u00e4tze,", "tokens": ["Du", "wahr'st", "die", "h\u00f6chs\u00b7ten", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn du bewahr\u2019st, was dein!", "tokens": ["Wenn", "du", "be\u00b7wahr'st", ",", "was", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wie vor tausend Jahren", "tokens": ["Und", "wie", "vor", "tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die V\u00e4ter, k\u00fchn und stark,", "tokens": ["Die", "V\u00e4\u00b7ter", ",", "k\u00fchn", "und", "stark", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor dr\u00e4ngenden Barbaren", "tokens": ["Vor", "dr\u00e4n\u00b7gen\u00b7den", "Bar\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gesch\u00fctzt des Ostens Mark,", "tokens": ["Ge\u00b7sch\u00fctzt", "des", "Os\u00b7tens", "Mark", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So auf demselben Grunde", "tokens": ["So", "auf", "dem\u00b7sel\u00b7ben", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Steh\u2019 heute treu die Hut,", "tokens": ["Steh'", "heu\u00b7te", "treu", "die", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und gebe leuchtend Kunde", "tokens": ["Und", "ge\u00b7be", "leuch\u00b7tend", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vom alten Heldenblut.", "tokens": ["Vom", "al\u00b7ten", "Hel\u00b7den\u00b7blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So h\u00fcte die heilige Flamme", "tokens": ["So", "h\u00fc\u00b7te", "die", "hei\u00b7li\u00b7ge", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Vor jedem Sturm und Sto\u00df", "tokens": ["Vor", "je\u00b7dem", "Sturm", "und", "Sto\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom herrlich gro\u00dfen Stamme", "tokens": ["Vom", "herr\u00b7lich", "gro\u00b7\u00dfen", "Stam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du kr\u00e4ftig edler Spro\u00df!", "tokens": ["Du", "kr\u00e4f\u00b7tig", "ed\u00b7ler", "Spro\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie hei\u00df es dich umstritte,", "tokens": ["Wie", "hei\u00df", "es", "dich", "um\u00b7strit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Steh\u2019 fest und wanke nicht:", "tokens": ["Steh'", "fest", "und", "wan\u00b7ke", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr deutsche Art und Sitte,", "tokens": ["F\u00fcr", "deut\u00b7sche", "Art", "und", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr Freiheit, f\u00fcr das Licht!", "tokens": ["F\u00fcr", "Frei\u00b7heit", ",", "f\u00fcr", "das", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}