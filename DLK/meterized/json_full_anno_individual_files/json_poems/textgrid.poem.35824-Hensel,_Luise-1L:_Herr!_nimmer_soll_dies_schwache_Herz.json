{"textgrid.poem.35824": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr! nimmer soll dies schwache Herz", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr! nimmer soll dies schwache Herz", "tokens": ["Herr", "!", "nim\u00b7mer", "soll", "dies", "schwa\u00b7che", "Herz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VMFIN", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Deiner Huld verzagen;", "tokens": ["An", "Dei\u00b7ner", "Huld", "ver\u00b7za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst es treu durch Freud' und Schmerz", "tokens": ["Du", "wirst", "es", "treu", "durch", "Freud'", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dereinst zur Heimath tragen.", "tokens": ["De\u00b7reinst", "zur", "Hei\u00b7math", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es soll nicht Freude, soll nicht Leid", "tokens": ["Es", "soll", "nicht", "Freu\u00b7de", ",", "soll", "nicht", "Leid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "$,", "VMFIN", "PTKNEG", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Dir, o Gott! mich treiben;", "tokens": ["Von", "Dir", ",", "o", "Gott", "!", "mich", "trei\u00b7