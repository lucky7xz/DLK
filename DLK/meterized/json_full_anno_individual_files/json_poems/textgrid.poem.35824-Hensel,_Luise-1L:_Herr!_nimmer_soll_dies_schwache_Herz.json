}}, "stanza.4": {"line.1": {"text": "Mit ihrer V\u00f6glein Sang und Klang,", "tokens": ["Mit", "ih\u00b7rer", "V\u00f6\u00b7glein", "Sang", "und", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mich so lieblich gr\u00fc\u00dfen:", "tokens": ["Die", "mich", "so", "lieb\u00b7lich", "gr\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dir legt' ich sie aus Lieb' und Dank,", "tokens": ["Dir", "legt'", "ich", "sie", "aus", "Lieb'", "und", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Jesus! gleich zu F\u00fc\u00dfen.", "tokens": ["O", "Je\u00b7sus", "!", "gleich", "zu", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch, Herr! Du hast mich wie die Welt", "tokens": ["Doch", ",", "Herr", "!", "Du", "hast", "mich", "wie", "die", "Welt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$.", "PPER", "VAFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ja selbst aus Nichts bereitet", "tokens": ["Ja", "selbst", "aus", "Nichts", "be\u00b7rei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hast Dein blaues Himmelszelt", "tokens": ["Und", "hast", "Dein", "blau\u00b7es", "Him\u00b7mels\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch \u00fcber mich gebreitet.", "tokens": ["Hoch", "\u00fc\u00b7ber", "mich", "ge\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du hast die Kindheit mein gepflegt", "tokens": ["Du", "hast", "die", "Kind\u00b7heit", "mein", "ge\u00b7pflegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch treuer Eltern Liebe,", "tokens": ["Durch", "treu\u00b7er", "El\u00b7tern", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein Wort mir fr\u00fch in's Herz gelegt,", "tokens": ["Dein", "Wort", "mir", "fr\u00fch", "in's", "Herz", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf da\u00df es Wurzeln triebe.", "tokens": ["Auf", "da\u00df", "es", "Wur\u00b7zeln", "trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du hast den Engel mir verliehn,", "tokens": ["Du", "hast", "den", "En\u00b7gel", "mir", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hin zu Dir mich leitet,", "tokens": ["Der", "hin", "zu", "Dir", "mich", "lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hast, um mich zu Dir zu ziehn,", "tokens": ["Und", "hast", ",", "um", "mich", "zu", "Dir", "zu", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUI", "PRF", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Arme ausgebreitet.", "tokens": ["Die", "Ar\u00b7me", "aus\u00b7ge\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O, guter Herr! was kann ich Dir", "tokens": ["O", ",", "gu\u00b7ter", "Herr", "!", "was", "kann", "ich", "Dir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$.", "PWS", "VMFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr all die Liebe geben?", "tokens": ["F\u00fcr", "all", "die", "Lie\u00b7be", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df mich in Einfalt denn allhier", "tokens": ["La\u00df", "mich", "in", "Ein\u00b7falt", "denn", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Deiner Ehre leben.", "tokens": ["Zu", "Dei\u00b7ner", "Eh\u00b7re", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und la\u00df mich Deine Treu' und Huld", "tokens": ["Und", "la\u00df", "mich", "Dei\u00b7ne", "Treu'", "und", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nimmermehr vergessen,", "tokens": ["Doch", "nim\u00b7mer\u00b7mehr", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df Deine Gnad' und meine Schuld", "tokens": ["La\u00df", "Dei\u00b7ne", "Gnad'", "und", "mei\u00b7ne", "Schuld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Herzen mich ermessen.", "tokens": ["Im", "Her\u00b7zen", "mich", "er\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "La\u00df mich in treuer Liebe Gluth", "tokens": ["La\u00df", "mich", "in", "treu\u00b7er", "Lie\u00b7be", "Gluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nimmermehr erkalten,", "tokens": ["Doch", "nim\u00b7mer\u00b7mehr", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text"