{"dta.poem.18699": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Das Leben ist ein G\u00e4nsespiel:", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "G\u00e4n\u00b7se\u00b7spiel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Je mehr man vorw\u00e4rts gehet,", "tokens": ["Je", "mehr", "man", "vor\u00b7w\u00e4rts", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Je fr\u00fcher kommt man an das Ziel,", "tokens": ["Je", "fr\u00fc\u00b7her", "kommt", "man", "an", "das", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo niemand gerne stehet.", "tokens": ["Wo", "nie\u00b7mand", "ger\u00b7ne", "ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Man sagt die G\u00e4nse w\u00e4ren dumm,", "tokens": ["Man", "sagt", "die", "G\u00e4n\u00b7se", "w\u00e4\u00b7ren", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O! glaubt mir nicht den Leuten:", "tokens": ["O", "!", "glaubt", "mir", "nicht", "den", "Leu\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn eine sieht einmal sich rum", "tokens": ["Denn", "ei\u00b7ne", "sieht", "ein\u00b7mal", "sich", "rum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "ADV", "PRF", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich r\u00fcckw\u00e4rts zu bedeuten.", "tokens": ["Mich", "r\u00fcck\u00b7w\u00e4rts", "zu", "be\u00b7deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ganz anders ist\u2019s in dieser Welt", "tokens": ["Ganz", "an\u00b7ders", "ist's", "in", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo alles vorw\u00e4rts dr\u00fccket,", "tokens": ["Wo", "al\u00b7les", "vor\u00b7w\u00e4rts", "dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn einer stolpert oder f\u00e4llt", "tokens": ["Wenn", "ei\u00b7ner", "stol\u00b7pert", "o\u00b7der", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Keine Seele r\u00fcckw\u00e4rts blicket.", "tokens": ["Kei\u00b7ne", "See\u00b7le", "r\u00fcck\u00b7w\u00e4rts", "bli\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}