{"textgrid.poem.63701": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem Mansardenfenster", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schornsteine, D\u00e4cher weit und breit,", "tokens": ["Schorn\u00b7stei\u00b7ne", ",", "D\u00e4\u00b7cher", "weit", "und", "breit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Trostlose Ziegeleinsamkeit;", "tokens": ["Trost\u00b7lo\u00b7se", "Zie\u00b7ge\u00b7lein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ein Kater, der auf Spatzen jagt,", "tokens": ["Ein", "Ka\u00b7ter", ",", "der", "auf", "Spat\u00b7zen", "jagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein gr\u00fcner Halm \u2013 Gott sei's geklagt.", "tokens": ["Kein", "gr\u00fc\u00b7ner", "Halm", "\u2013", "Gott", "sei's", "ge\u00b7klagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$(", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kein Menschenauge blickt herein,", "tokens": ["Kein", "Men\u00b7schen\u00b7au\u00b7ge", "blickt", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein lampenschimmernd Fensterlein.", "tokens": ["Kein", "lam\u00b7pen\u00b7schim\u00b7mernd", "Fens\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin um jeden Rauch vergn\u00fcgt,", "tokens": ["Ich", "bin", "um", "je\u00b7den", "Rauch", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der kr\u00e4uselnd einem Schlot entfliegt.", "tokens": ["Der", "kr\u00e4u\u00b7selnd", "ei\u00b7nem", "Schlot", "ent\u00b7fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hoch ist's; doch morgen, sprach der Wirt,", "tokens": ["Hoch", "ist's", ";", "doch", "mor\u00b7gen", ",", "sprach", "der", "Wirt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$.", "ADV", "ADV", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Nummer siebzehn reisen wird \u2013 \u2013", "tokens": ["Wenn", "Num\u00b7mer", "sieb\u00b7zehn", "rei\u00b7sen", "wird", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "VVINF", "VVINF", "VAFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sieh! was blitzt vom S\u00fcden her?", "tokens": ["Da", "sieh", "!", "was", "blitzt", "vom", "S\u00fc\u00b7den", "her", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "PWS", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr G\u00f6tter! me