{"textgrid.poem.41834": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1. Vor\u00fcber sind die Feste!", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor\u00fcber sind die Feste wieder,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "sind", "die", "Fes\u00b7te", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die uns gegr\u00fc\u00dft mit Glanz und Licht,", "tokens": ["Die", "uns", "ge\u00b7gr\u00fc\u00dft", "mit", "Glanz", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verstummt die holden Weihnachtslieder,", "tokens": ["Ver\u00b7stummt", "die", "hol\u00b7den", "Weih\u00b7nachts\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "D'raus reinster Liebe Segen spricht.", "tokens": ["D'raus", "reins\u00b7ter", "Lie\u00b7be", "Se\u00b7gen", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es gab daf\u00fcr ein langes Sorgen,", "tokens": ["Es", "gab", "da\u00b7f\u00fcr", "ein", "lan\u00b7ges", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Vorbereiten Tag und Nacht,", "tokens": ["Ein", "Vor\u00b7be\u00b7rei\u00b7ten", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beim Lampenschein ward mancher Morgen", "tokens": ["Beim", "Lam\u00b7pen\u00b7schein", "ward", "man\u00b7cher", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gar arbeitsvoll herangewacht.", "tokens": ["Gar", "ar\u00b7beits\u00b7voll", "her\u00b7an\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Werk der Liebe zu bereiten,", "tokens": ["Das", "Werk", "der", "Lie\u00b7be", "zu", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bem\u00fchte sich so alt und jung,", "tokens": ["Be\u00b7m\u00fch\u00b7te", "sich", "so", "alt", "und", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jedes Herz schien sich zu weiten", "tokens": ["Und", "je\u00b7des", "Herz", "schien", "sich", "zu", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PRF", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Hoffnung und Erinnerung,", "tokens": ["In", "Hoff\u00b7nung", "und", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Liebesfest so ohne Gleichen", "tokens": ["Ein", "Lie\u00b7bes\u00b7fest", "so", "oh\u00b7ne", "Glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im ganzen gro\u00dfen Vaterland,", "tokens": ["Im", "gan\u00b7zen", "gro\u00b7\u00dfen", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Engelsruf und Sternenzeichen", "tokens": ["Wo", "En\u00b7gels\u00b7ruf", "und", "Ster\u00b7nen\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu hoher Botschaft sich verband!", "tokens": ["Zu", "ho\u00b7her", "Bot\u00b7schaft", "sich", "ver\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und Liebe wurde zum Erbarmen:", "tokens": ["Und", "Lie\u00b7be", "wur\u00b7de", "zum", "Er\u00b7bar\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Christbaum aus dem eignem Heim", "tokens": ["Vom", "Christ\u00b7baum", "aus", "dem", "eig\u00b7nem", "Heim"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fiel mancher Strahl auch auf die Armen", "tokens": ["Fiel", "man\u00b7cher", "Strahl", "auch", "auf", "die", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und weckte neuer Hoffnung Keim. \u2013", "tokens": ["Und", "weck\u00b7te", "neu\u00b7er", "Hoff\u00b7nung", "Keim", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die heil'ge Nacht \u2013 die Feiertage", "tokens": ["Die", "heil'\u00b7ge", "Nacht", "\u2013", "die", "Fei\u00b7er\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit aller Weihe, allem Gl\u00fcck,", "tokens": ["Mit", "al\u00b7ler", "Wei\u00b7he", ",", "al\u00b7lem", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PIS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Jahreswechsels ernste Frage \u2013", "tokens": ["Des", "Jah\u00b7res\u00b7wech\u00b7sels", "erns\u00b7te", "Fra\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir blicken jetzt darauf zur\u00fcck.", "tokens": ["Wir", "bli\u00b7cken", "jetzt", "da\u00b7rauf", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Vor\u00fcber wieder sind die Feste", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "wie\u00b7der", "sind", "die", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und uns umf\u00e4ngt die Alltagswelt,", "tokens": ["Und", "uns", "um\u00b7f\u00e4ngt", "die", "All\u00b7tags\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bleibt uns ja davon das Beste:", "tokens": ["Doch", "bleibt", "uns", "ja", "da\u00b7von", "das", "Bes\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie, die am Sterne sich entz\u00fcndet,", "tokens": ["Sie", ",", "die", "am", "Ster\u00b7ne", "sich", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der in der Weihnacht zog vorauf", "tokens": ["Der", "in", "der", "Weih\u00b7nacht", "zog", "vor\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und allen Sehenden verk\u00fcndet:", "tokens": ["Und", "al\u00b7len", "Se\u00b7hen\u00b7den", "ver\u00b7k\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es naht das Heil \u2013 nun wachet auf!", "tokens": ["Es", "naht", "das", "Heil", "\u2013", "nun", "wa\u00b7chet", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun wachet auf zum Liebesglauben,", "tokens": ["Nun", "wa\u00b7chet", "auf", "zum", "Lie\u00b7bes\u00b7glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun dient der neuen Zeit des Lichts \u2013", "tokens": ["Nun", "dient", "der", "neu\u00b7en", "Zeit", "des", "Lichts", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Weihegru\u00df kann niemand rauben,", "tokens": ["Den", "Wei\u00b7he\u00b7gru\u00df", "kann", "nie\u00b7mand", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was ihm nicht dient, zerf\u00e4llt in nichts.", "tokens": ["Was", "ihm", "nicht", "dient", ",", "zer\u00b7f\u00e4llt", "in", "nichts", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "$,", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Den Festen folgt der Arbeit M\u00fchen,", "tokens": ["Den", "Fes\u00b7ten", "folgt", "der", "Ar\u00b7beit", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ihnen freudig ging voran \u2013", "tokens": ["Das", "ih\u00b7nen", "freu\u00b7dig", "ging", "vo\u00b7ran", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn wir im Dienst der Menschheit gl\u00fchen,", "tokens": ["Wenn", "wir", "im", "Dienst", "der", "Menschheit", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sind wir auf rechter Lebensbahn.", "tokens": ["Sind", "wir", "auf", "rech\u00b7ter", "Le\u00b7bens\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Vor\u00fcber sind die Feste wieder,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "sind", "die", "Fes\u00b7te", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die uns gegr\u00fc\u00dft mit Glanz und Licht,", "tokens": ["Die", "uns", "ge\u00b7gr\u00fc\u00dft", "mit", "Glanz", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verstummt die holden Weihnachtslieder,", "tokens": ["Ver\u00b7stummt", "die", "hol\u00b7den", "Weih\u00b7nachts\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "D'raus reinster Liebe Segen spricht.", "tokens": ["D'raus", "reins\u00b7ter", "Lie\u00b7be", "Se\u00b7gen", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "p