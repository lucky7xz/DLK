"}, "line.4": {"text": "Wir blicken jetzt darauf zur\u00fcck.", "tokens": ["Wir", "bli\u00b7cken", "jetzt", "da\u00b7rauf", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Vor\u00fcber wieder sind die Feste", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "wie\u00b7der", "sind", "die", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und uns umf\u00e4ngt die Alltagswelt,", "tokens": ["Und", "uns", "um\u00b7f\u00e4ngt", "die", "All\u00b7tags\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bleibt uns ja davon das Beste:", "tokens": ["Doch", "bleibt", "uns", "ja", "da\u00b7von", "das", "Bes\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Sie, die am Sterne sich entz\u00fcndet,", "tokens": ["Sie", ",", "die", "am", "Ster\u00b7ne", "sich", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der in der Weihnacht zog vorauf", "tokens": ["Der", "in", "der", "Weih\u00b7nacht", "zog", "vor\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und allen Sehenden verk\u00