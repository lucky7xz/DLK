{"dta.poem.8338": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ermunterung des Gem\u00fcths bei  \n einem nebelichten und tr\u00fcben  \n Wetter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf! ermuntert euch ihr Sinnen!", "tokens": ["Auf", "!", "er\u00b7mun\u00b7tert", "euch", "ihr", "Sin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob die tr\u00fcben Himmels-Zinnen", "tokens": ["Ob", "die", "tr\u00fc\u00b7ben", "Him\u00b7mels\u00b7Zin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleich mit Nebel sind umh\u00fcllt:", "tokens": ["Gleich", "mit", "Ne\u00b7bel", "sind", "um\u00b7h\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer will gleich in Kummer weinen,", "tokens": ["Wer", "will", "gleich", "in", "Kum\u00b7mer", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wenn die tr\u00fcben Tag erscheinen", "tokens": ["Wenn", "die", "tr\u00fc\u00b7ben", "Tag", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da die Lufft von Regen quillt:", "tokens": ["Da", "die", "Lufft", "von", "Re\u00b7gen", "quillt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil wir durch Erfahrung wissen,", "tokens": ["Weil", "wir", "durch", "Er\u00b7fah\u00b7rung", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Licht komt nach den Finsternissen.", "tokens": ["Licht", "komt", "nach", "den", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonnenschein und milder Regen,", "tokens": ["Son\u00b7nen\u00b7schein", "und", "mil\u00b7der", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beide bringen uns den Seegen,", "tokens": ["Bei\u00b7de", "brin\u00b7gen", "uns", "den", "See\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus des g\u00fctgen Gebers Hand:", "tokens": ["Aus", "des", "g\u00fct\u00b7gen", "Ge\u00b7bers", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus des Herbstes neblicht Trauffen,", "tokens": ["Aus", "des", "Herbs\u00b7tes", "neb\u00b7licht", "Trauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4chst hernach noch mancher Hauffen", "tokens": ["W\u00e4chst", "her\u00b7nach", "noch", "man\u00b7cher", "Hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Frucht, auf dem besaamten Land:", "tokens": ["Frucht", ",", "auf", "dem", "be\u00b7saam\u00b7ten", "Land", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Darum mu\u00df man in Vergn\u00fcgen,", "tokens": ["Da\u00b7rum", "mu\u00df", "man", "in", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tr\u00fcber Tage Noth besiegen.", "tokens": ["Tr\u00fc\u00b7ber", "Ta\u00b7ge", "Noth", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was ist ein betr\u00fcbtes Leben,", "tokens": ["Was", "ist", "ein", "be\u00b7tr\u00fcb\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das mit Wolken stets umgeben", "tokens": ["Das", "mit", "Wol\u00b7ken", "stets", "um\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das mit Kummer, banger Noth,", "tokens": ["Das", "mit", "Kum\u00b7mer", ",", "ban\u00b7ger", "Noth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie mit einem Dunst umsponnen,", "tokens": ["Wie", "mit", "ei\u00b7nem", "Dunst", "um\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das ohn alles Licht der Sonnen?", "tokens": ["Das", "ohn", "al\u00b7les", "Licht", "der", "Son\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aerger als der bittre Todt,", "tokens": ["A\u00b7er\u00b7ger", "als", "der", "bitt\u00b7re", "Todt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Drum verfliegt ihr tr\u00fcben Schatten,", "tokens": ["Drum", "ver\u00b7fliegt", "ihr", "tr\u00fc\u00b7ben", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich will mich mit Freude gatten.", "tokens": ["Ich", "will", "mich", "mit", "Freu\u00b7de", "gat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Gott mu\u00df man mit frohen Mienen,", "tokens": ["Gott", "mu\u00df", "man", "mit", "fro\u00b7hen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit zufriedner Seele dienen;", "tokens": ["Mit", "zu\u00b7fried\u00b7ner", "See\u00b7le", "die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jhm gef\u00e4llt ohn allen Streit,", "tokens": ["Jhm", "ge\u00b7f\u00e4llt", "ohn", "al\u00b7len", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein solch Herze das nicht zaget,", "tokens": ["Ein", "solch", "Her\u00b7ze", "das", "nicht", "za\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "PDS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das sich nicht mit Grillen plaget.", "tokens": ["Das", "sich", "nicht", "mit", "Gril\u00b7len", "pla\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ruhige Gelassenheit", "tokens": ["Ru\u00b7hi\u00b7ge", "Ge\u00b7las\u00b7sen\u00b7heit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Du solt auch auf tr\u00fcber Erden,", "tokens": ["Du", "solt", "auch", "auf", "tr\u00fc\u00b7ber", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lindern der Natur Beschwerden.", "tokens": ["Lin\u00b7dern", "der", "Na\u00b7tur", "Be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}