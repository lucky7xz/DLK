{"textgrid.poem.64289": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "6. An den Abendstern", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ehmals winktest du mir, F\u00fchrer des schweigenden", "tokens": ["Eh\u00b7mals", "wink\u00b7test", "du", "mir", ",", "F\u00fch\u00b7rer", "des", "schwei\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "ART", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Abends, Freuden herab, kurz, wie sie Sterblichen", "tokens": ["A\u00b7bends", ",", "Freu\u00b7den", "her\u00b7ab", ",", "kurz", ",", "wie", "sie", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "ADV", "$,", "ADJD", "$,", "PWAV", "PPER", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "L\u00e4cheln, farbigen Blasen", "tokens": ["L\u00e4\u00b7cheln", ",", "far\u00b7bi\u00b7gen", "Bla\u00b7sen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "\u00c4hnlich, hauchender Weste Spiel!", "tokens": ["\u00c4hn\u00b7lich", ",", "hau\u00b7chen\u00b7der", "Wes\u00b7te", "Spiel", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Zwar mir waren sie wert, wert, wie dem d\u00fcrstenden", "tokens": ["Zwar", "mir", "wa\u00b7ren", "sie", "wert", ",", "wert", ",", "wie", "dem", "d\u00fcrs\u00b7ten\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "PWAV", "ART", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Weizenhalme der Tau! aber sie schwanden bald!", "tokens": ["Wei\u00b7zen\u00b7hal\u00b7me", "der", "Tau", "!", "a\u00b7ber", "sie", "schwan\u00b7den", "bald", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Selten blicket dein Auge", "tokens": ["Sel\u00b7ten", "bli\u00b7cket", "dein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nun, und tr\u00fcber auf mich herab!", "tokens": ["Nun", ",", "und", "tr\u00fc\u00b7ber", "auf", "mich", "her\u00b7ab", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "ADJD", "APPR", "PPER", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "H\u00fcllen Schleier dich ein? Oder entflie\u00dfen dir", "tokens": ["H\u00fcl\u00b7len", "Schlei\u00b7er", "dich", "ein", "?", "O\u00b7der", "ent\u00b7flie\u00b7\u00dfen", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "PPER", "PTKVZ", "$.", "NE", "VVFIN", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Thr\u00e4nen? Bist du, gleich mir, nagender Traurigkeit", "tokens": ["Thr\u00e4\u00b7nen", "?", "Bist", "du", ",", "gleich", "mir", ",", "na\u00b7gen\u00b7der", "Trau\u00b7rig\u00b7keit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "VAFIN", "PPER", "$,", "ADV", "PPER", "$,", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Raub, ein Erbe des Jammers?", "tokens": ["Raub", ",", "ein", "Er\u00b7be", "des", "Jam\u00b7mers", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Deine strahlenden Br\u00fcder auch?", "tokens": ["Dei\u00b7ne", "strah\u00b7len\u00b7den", "Br\u00fc\u00b7der", "auch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Ist das blaue Gewand leuchtender Sonnen voll,", "tokens": ["Ist", "das", "blau\u00b7e", "Ge\u00b7wand", "leuch\u00b7ten\u00b7der", "Son\u00b7nen", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und mit Monden bes\u00e4't, nur ein Gewebe von", "tokens": ["Und", "mit", "Mon\u00b7den", "be\u00b7s\u00e4't", ",", "nur", "ein", "Ge\u00b7we\u00b7be", "von"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "APPR"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Elend? T\u00f6nen die Sph\u00e4ren", "tokens": ["E\u00b7lend", "?", "T\u00f6\u00b7nen", "die", "Sph\u00e4\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Einen ewigen Klageton?", "tokens": ["Ei\u00b7nen", "e\u00b7wi\u00b7gen", "Kla\u00b7ge\u00b7ton", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-