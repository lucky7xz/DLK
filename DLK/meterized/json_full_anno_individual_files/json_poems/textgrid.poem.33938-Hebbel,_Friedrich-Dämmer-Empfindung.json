{"textgrid.poem.33938": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "D\u00e4mmer-Empfindung", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was treibt mich hier von hinnen?", "tokens": ["Was", "treibt", "mich", "hier", "von", "hin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was lockt mich dort geheimni\u00dfvoll?", "tokens": ["Was", "lockt", "mich", "dort", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ist's, das ich gewinnen,", "tokens": ["Was", "ist's", ",", "das", "ich", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und was, womit ich's kaufen soll?", "tokens": ["Und", "was", ",", "wo\u00b7mit", "ich's", "kau\u00b7fen", "soll", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Trat unsichtbar mein Erbe,", "tokens": ["Trat", "un\u00b7sicht\u00b7bar", "mein", "Er\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Geist, ein lust'ger, schon heran,", "tokens": ["Ein", "Geist", ",", "ein", "lust'\u00b7ger", ",", "schon", "he\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dr\u00e4ngt mich, da\u00df ich sterbe,", "tokens": ["Und", "dr\u00e4ngt", "mich", ",", "da\u00df", "ich", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil er nicht eher leben kann?", "tokens": ["Weil", "er", "nicht", "e\u00b7her", "le\u00b7ben", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und winkt mir aus der Ferne", "tokens": ["Und", "winkt", "mir", "aus", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Traube schon, die mir gereift", "tokens": ["Die", "Trau\u00b7be", "schon", ",", "die", "mir", "ge\u00b7reift"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Auf einem andern Sterne,", "tokens": ["Auf", "ei\u00b7nem", "an\u00b7dern", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und will, da\u00df meine Hand sie streift?", "tokens": ["Und", "will", ",", "da\u00df", "mei\u00b7ne", "Hand", "sie", "streift", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word