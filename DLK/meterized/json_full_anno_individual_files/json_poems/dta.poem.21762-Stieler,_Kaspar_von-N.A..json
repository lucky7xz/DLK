{"dta.poem.21762": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Es lieben die Kazzen/ die Razzen/ die", "tokens": ["Es", "lie\u00b7ben", "die", "Kaz\u00b7zen", "/", "die", "Raz\u00b7zen", "/", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "NN", "$(", "ART"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Raben.", "tokens": ["Ra\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Warum nicht auch die jungen Kna-", "tokens": ["Wa\u00b7rum", "nicht", "auch", "die", "jun\u00b7gen", "Kna"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "ADV", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ben?", "tokens": ["ben", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Wie solte geh\u00f6net denn Filidor sein/", "tokens": ["Wie", "sol\u00b7te", "ge\u00b7h\u00f6\u00b7net", "denn", "Fi\u00b7li\u00b7dor", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "VVPP", "KON", "NE", "VAINF", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "da\u00df er von Lieben was bringt ein?", "tokens": ["da\u00df", "er", "von", "Lie\u00b7ben", "was", "bringt", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "PWS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die W\u00fcrme so kriechen/ die B\u00e4ume/ so", "tokens": ["Die", "W\u00fcr\u00b7me", "so", "krie\u00b7chen", "/", "die", "B\u00e4u\u00b7me", "/", "so"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADV", "VVINF", "$(", "ART", "NN", "$(", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "wachsen", "tokens": ["wach\u00b7sen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "die f\u00fchlen Venus Schwanen-ach-", "tokens": ["die", "f\u00fch\u00b7len", "Ve\u00b7nus", "Schwa\u00b7nen\u00b7ach"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "sen.", "tokens": ["sen."], "token_info": ["abbreviation"], "pos": ["XY"]}, "line.11": {"text": "Wie kommt es denn/ Leute/ da\u00df ihr euch", "tokens": ["Wie", "kommt", "es", "denn", "/", "Leu\u00b7te", "/", "da\u00df", "ihr", "euch"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$(", "NN", "$(", "KOUS", "PPER", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "so stellt", "tokens": ["so", "stellt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "da\u00df euch das Lieben nicht gef\u00e4llt.", "tokens": ["da\u00df", "euch", "das", "Lie\u00b7ben", "nicht", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wie? wollt ihr dem Filidor etwas ver-", "tokens": ["Wie", "?", "wollt", "ihr", "dem", "Fi\u00b7li\u00b7dor", "et\u00b7was", "ver"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "ART", "NN", "ADV", "TRUNC"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "weisen", "tokens": ["wei\u00b7sen"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "dadurch sich manche B\u00fccher preisen/", "tokens": ["da\u00b7durch", "sich", "man\u00b7che", "B\u00fc\u00b7cher", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "mit Sonne/ mit Wonne/ mit Zier/ Lam\u0303", "tokens": ["mit", "Son\u00b7ne", "/", "mit", "Won\u00b7ne", "/", "mit", "Zier", "/", "Lam\u0303"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$(", "APPR", "NN", "$(", "APPR", "NN", "$(", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "erf\u00fcllt", "tokens": ["er\u00b7f\u00fcllt"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "mit T\u00e4ubelein/ mit Liebstes Bild?", "tokens": ["mit", "T\u00e4u\u00b7be\u00b7lein", "/", "mit", "Liebs\u00b7tes", "Bild", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Mein! lasset doch/ Leute/ den Filidor", "tokens": ["Mein", "!", "las\u00b7set", "doch", "/", "Leu\u00b7te", "/", "den", "Fi\u00b7li\u00b7dor"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "$.", "VVFIN", "ADV", "$(", "NN", "$(", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "gehen", "tokens": ["ge\u00b7hen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "er wei\u00df in andern zubestehen.", "tokens": ["er", "wei\u00df", "in", "an\u00b7dern", "zu\u00b7be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "wird F\u00f6bus ihm geben den taumel-trunk", "tokens": ["wird", "F\u00f6\u00b7bus", "ihm", "ge\u00b7ben", "den", "tau\u00b7mel\u00b7trunk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "ein:", "tokens": ["ein", ":"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-", "measure": "single.down"}, "line.25": {"text": "so folgt ein ander Liedelein.", "tokens": ["so", "folgt", "ein", "an\u00b7der", "Lie\u00b7de\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}