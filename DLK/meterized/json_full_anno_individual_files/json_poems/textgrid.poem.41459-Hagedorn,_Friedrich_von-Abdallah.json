{"textgrid.poem.41459": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Abdallah", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abdallah, Hassans Sohn, der vor dem Gro\u00dfvizier,", "tokens": ["Ab\u00b7dal\u00b7lah", ",", "Has\u00b7sans", "Sohn", ",", "der", "vor", "dem", "Gro\u00df\u00b7vi\u00b7zier", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wie vor dem Mahomet, sich bis zur Erde kr\u00fcmmte,", "tokens": ["Wie", "vor", "dem", "Ma\u00b7ho\u00b7met", ",", "sich", "bis", "zur", "Er\u00b7de", "kr\u00fcmm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "PRF", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Fleht um ein reiches Amt, das der dem Seraskier,", "tokens": ["Fleht", "um", "ein", "rei\u00b7ches", "Amt", ",", "das", "der", "dem", "Se\u00b7ras\u00b7kier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Pascha Bajazet, freundvetterlich bestimmte.", "tokens": ["Dem", "Pa\u00b7scha", "Ba\u00b7ja\u00b7zet", ",", "freund\u00b7vet\u00b7ter\u00b7lich", "be\u00b7stimm\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "ADJD", "VVFIN", "$."], "meter": "-+--+-++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ihn h\u00f6rt der Gro\u00dfvizier, und sagt geschwinde: Nein.", "tokens": ["Ihn", "h\u00f6rt", "der", "Gro\u00df\u00b7vi\u00b7zier", ",", "und", "sagt", "ge\u00b7schwin\u00b7de", ":", "Nein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ADJA", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er dankt. Wie? Dein Gesuch wird g\u00e4nzlich abgeschlagen!", "tokens": ["Er", "dankt", ".", "Wie", "?", "Dein", "Ge\u00b7such", "wird", "g\u00e4nz\u00b7lich", "ab\u00b7ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Abdallah kniet, und spricht: Die Huld ist ungemein,", "tokens": ["Ab\u00b7dal\u00b7lah", "kniet", ",", "und", "spricht", ":", "Die", "Huld", "ist", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KON", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Da\u00df ich nicht harren darf, da sie mir's gleich versagen.", "tokens": ["Da\u00df", "ich", "nicht", "har\u00b7ren", "darf", ",", "da", "sie", "mir's", "gleich", "ver\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Abdallah, Hassans Sohn, der vor