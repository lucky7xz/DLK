{"dta.poem.17358": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "208.  Nach der zeit ist keine w\u00fcrckung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch w\u00fcrcke weil du kanst dein Hell und Seeligkeit:", "tokens": ["Mensch", "w\u00fcr\u00b7cke", "weil", "du", "kanst", "dein", "Hell", "und", "See\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOUS", "PPER", "VMFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das w\u00fcrcken h\u00f6ret auf mit endung dieser zeit.", "tokens": ["Das", "w\u00fcr\u00b7cken", "h\u00f6\u00b7ret", "auf", "mit", "en\u00b7dung", "die\u00b7ser", "zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVFIN", "APPR", "APPR", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}