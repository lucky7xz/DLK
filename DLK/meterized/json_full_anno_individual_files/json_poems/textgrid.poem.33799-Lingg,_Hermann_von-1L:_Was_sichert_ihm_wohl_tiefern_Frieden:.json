{"textgrid.poem.33799": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was sichert ihm wohl tiefern Frieden:", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sichert ihm wohl tiefern Frieden:", "tokens": ["Was", "si\u00b7chert", "ihm", "wohl", "tie\u00b7fern", "Frie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sankt Helenens Zypressenlaub?", "tokens": ["Sankt", "He\u00b7le\u00b7nens", "Zyp\u00b7res\u00b7sen\u00b7laub", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Im stolzen Dom der Invaliden", "tokens": ["Im", "stol\u00b7zen", "Dom", "der", "In\u00b7va\u00b7li\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Marmor \u00fcber seinem Staub?", "tokens": ["Der", "Mar\u00b7mor", "\u00fc\u00b7ber", "sei\u00b7nem", "Staub", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob wohl beim Gru\u00dfe des Gesch\u00fctzes", "tokens": ["Ob", "wohl", "beim", "Gru\u00b7\u00dfe", "des", "Ge\u00b7sch\u00fct\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Weltstadt er sich mehr gef\u00e4llt,", "tokens": ["Der", "Welt\u00b7stadt", "er", "sich", "mehr", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Als bei dem Strahl des fl\u00fccht'gen Blitzes,", "tokens": ["Als", "bei", "dem", "Strahl", "des", "fl\u00fccht'\u00b7gen", "Blit\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der fern sein einsam Grab erhellt?", "tokens": ["Der", "fern", "sein", "ein\u00b7sam", "Grab", "er\u00b7hellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAINF", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem Sieger bei Arbela gaben", "tokens": ["Dem", "Sie\u00b7ger", "bei", "Ar\u00b7be\u00b7la", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Seinen kaum ein Grab; ein Raub", "tokens": ["Die", "Sei\u00b7nen", "kaum", "ein", "Grab", ";", "ein", "Raub"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPOSS", "ADV", "ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Flammen, sorglos, unbegraben", "tokens": ["Der", "Flam\u00b7men", ",", "sorg\u00b7los", ",", "un\u00b7be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blieb Hannibals, des Helden Staub.", "tokens": ["Blieb", "Han\u00b7ni\u00b7bals", ",", "des", "Hel\u00b7den", "Staub", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "NN", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Dreifach metallne S\u00e4rge schlie\u00dfen", "tokens": ["Drei\u00b7fach", "me\u00b7tall\u00b7ne", "S\u00e4r\u00b7ge", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Knochen Attila's in sich,", "tokens": ["Die", "Kno\u00b7chen", "At\u00b7tila's", "in", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "PRF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und des Busento Wogen flie\u00dfen", "tokens": ["Und", "des", "Bu\u00b7sen\u00b7to", "Wo\u00b7gen", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NE", "NN", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "\u00dcber dem Grab des Alarich.", "tokens": ["\u00dc\u00b7ber", "dem", "Grab", "des", "A\u00b7la\u00b7rich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Noch einen, Lethe, deiner Schatten,", "tokens": ["Noch", "ei\u00b7nen", ",", "Le\u00b7the", ",", "dei\u00b7ner", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Toten von Sankt Helena,", "tokens": ["Den", "To\u00b7ten", "von", "Sankt", "He\u00b7le\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Kaiser will sein Volk bestatten,", "tokens": ["Den", "Kai\u00b7ser", "will", "sein", "Volk", "be\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dem es seine Gr\u00f6\u00dfe sah.", "tokens": ["In", "dem", "es", "sei\u00b7ne", "Gr\u00f6\u00b7\u00dfe", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es war des Sterbenden Gedanke,", "tokens": ["Es", "war", "des", "Ster\u00b7ben\u00b7den", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Staub einst \u00f6ffne noch den Scho\u00df", "tokens": ["Dem", "Staub", "einst", "\u00f6ff\u00b7ne", "noch", "den", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Land, das ihm zuerst die Schranke", "tokens": ["Das", "Land", ",", "das", "ihm", "zu\u00b7erst", "die", "Schran\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr Siege, Ruhm und Macht erschlo\u00df.", "tokens": ["F\u00fcr", "Sie\u00b7ge", ",", "Ruhm", "und", "Macht", "er\u00b7schlo\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schon dr\u00f6hnet von der Pike Streichen,", "tokens": ["Schon", "dr\u00f6h\u00b7net", "von", "der", "Pi\u00b7ke", "Strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Schaufelschlag die Grabeswand.", "tokens": ["Vom", "Schau\u00b7fel\u00b7schlag", "die", "Gra\u00b7bes\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt ihr's, zermorschte Kriegerleichen,", "tokens": ["H\u00f6rt", "ih\u00b7r's", ",", "zer\u00b7morschte", "Krie\u00b7ger\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Bedeckt von Eis und W\u00fcstensand?", "tokens": ["Be\u00b7deckt", "von", "Eis", "und", "W\u00fcs\u00b7ten\u00b7sand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sp\u00fcren ein geheimes Schwanken", "tokens": ["Es", "sp\u00fc\u00b7ren", "ein", "ge\u00b7hei\u00b7mes", "Schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Throne, wie der V\u00f6lker Rat,", "tokens": ["Die", "Thro\u00b7ne", ",", "wie", "der", "V\u00f6l\u00b7ker", "Rat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da wie mit einem Schlachtgedanken", "tokens": ["Da", "wie", "mit", "ei\u00b7nem", "Schlacht\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Kriegsboot mit der Asche naht. \u2013", "tokens": ["Das", "Kriegs\u00b7boot", "mit", "der", "A\u00b7sche", "naht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Spannt, sprach einst Ziska, meine Decke", "tokens": ["Spannt", ",", "sprach", "einst", "Zis\u00b7ka", ",", "mei\u00b7ne", "De\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf eine Trommel, wenn ich schied;", "tokens": ["Auf", "ei\u00b7ne", "Trom\u00b7mel", ",", "wenn", "ich", "schied", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um da\u00df er tot den Feind noch schrecke,", "tokens": ["Um", "da\u00df", "er", "tot", "den", "Feind", "noch", "schre\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "KOUS", "PPER", "ADJD", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hob man aufs Ro\u00df den toten Cid.", "tokens": ["Hob", "man", "aufs", "Ro\u00df", "den", "to\u00b7ten", "Cid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch nein! sein Schatten mit den Schemen,", "tokens": ["Doch", "nein", "!", "sein", "Schat\u00b7ten", "mit", "den", "Sche\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die einst geherrscht im alten Rom,", "tokens": ["Die", "einst", "ge\u00b7herrscht", "im", "al\u00b7ten", "Rom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sieht von versunknen Diademen", "tokens": ["Sieht", "von", "ver\u00b7sun\u00b7knen", "Di\u00b7a\u00b7de\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Trugbild nur im Lethestrom.", "tokens": ["Das", "Trug\u00b7bild", "nur", "im", "Le\u00b7thes\u00b7trom", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Weht \u00fcber Afrika, Glutl\u00fcfte,", "tokens": ["Weht", "\u00fc\u00b7ber", "Af\u00b7ri\u00b7ka", ",", "Glut\u00b7l\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$,", "NN", "$,"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weht um des alten Atlas Haupt!", "tokens": ["Weht", "um", "des", "al\u00b7ten", "At\u00b7las", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Meer, trag' ihn heim in Frankreichs Gr\u00fcfte,