{"textgrid.poem.57050": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fritz Kunkels Pudel ward, noch ungetauft,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fritz Kunkels Pudel ward, noch ungetauft,", "tokens": ["Fritz", "Kun\u00b7kels", "Pu\u00b7del", "ward", ",", "noch", "un\u00b7ge\u00b7tauft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VAFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "von einem Stiefmilchbruder Korfs gekauft.", "tokens": ["von", "ei\u00b7nem", "Stief\u00b7milch\u00b7bru\u00b7der", "Korfs", "ge\u00b7kauft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es trieb ihn, als er, hilfreich von Natur,", "tokens": ["Es", "trieb", "ihn", ",", "als", "er", ",", "hilf\u00b7reich", "von", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der sogenannten \u00bbLore\u00ab Leid erfuhr,", "tokens": ["der", "so\u00b7ge\u00b7nann\u00b7ten", "\u00bb", "Lo\u00b7re", "\u00ab", "Leid", "er\u00b7fuhr", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "NE", "$(", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "sogleich zu ihr: worauf er, der nicht hie\u00df,", "tokens": ["sog\u00b7leich", "zu", "ihr", ":", "wo\u00b7rauf", "er", ",", "der", "nicht", "hie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "$.", "PWAV", "PPER", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sich ihr zum Troste \u00bbLor", "tokens": ["sich", "ihr", "zum", "Tros\u00b7te", "\u00bb", "Lor"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PRF", "PPOSAT", "APPRART", "NN", "$(", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "den Namen also gleichsam auf sich nehmend \u2013", "tokens": ["den", "Na\u00b7men", "al\u00b7so", "gleich\u00b7sam", "auf", "sich", "neh\u00b7mend", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und alle Welt durch diese Tat besch\u00e4mend!", "tokens": ["und", "al\u00b7le", "Welt", "durch", "die\u00b7se", "Tat", "be\u00b7sch\u00e4\u00b7mend", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Korf selbst vollzog den Taufakt unverweilt.", "tokens": ["Korf", "selbst", "voll\u00b7zog", "den", "Tauf\u00b7akt", "un\u00b7ver\u00b7weilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Vogel aber war fortan geheilt.", "tokens": ["Der", "Vo\u00b7gel", "a\u00b7ber", "war", "for\u00b7tan", "ge\u00b7heilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+