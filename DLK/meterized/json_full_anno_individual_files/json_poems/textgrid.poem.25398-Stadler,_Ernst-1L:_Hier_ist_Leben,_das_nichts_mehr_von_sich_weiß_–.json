{"textgrid.poem.25398": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier ist Leben, das nichts mehr von sich wei\u00df \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ist Leben, das nichts mehr von sich wei\u00df \u2013", "tokens": ["Hier", "ist", "Le\u00b7ben", ",", "das", "nichts", "mehr", "von", "sich", "wei\u00df", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "PRELS", "PIS", "ADV", "APPR", "PRF", "VVFIN", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bewu\u00dftsein tausend Klafter tief ins All gesunken.", "tokens": ["Be\u00b7wu\u00df\u00b7tsein", "tau\u00b7send", "Klaf\u00b7ter", "tief", "ins", "All", "ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier t\u00f6nt durch kahle S\u00e4le der Choral des Nichts.", "tokens": ["Hier", "t\u00f6nt", "durch", "kah\u00b7le", "S\u00e4\u00b7le", "der", "Cho\u00b7ral", "des", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier ist Beschwichtigung, Zuflucht,", "tokens": ["Hier", "ist", "Be\u00b7schwich\u00b7ti\u00b7gung", ",", "Zu\u00b7flucht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Heimkehr, Kinderstube.", "tokens": ["Heim\u00b7kehr", ",", "Kin\u00b7der\u00b7stu\u00b7be", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hier droht nichts Menschliches. Die stieren Augen,", "tokens": ["Hier", "droht", "nichts", "Menschli\u00b7ches", ".", "Die", "stie\u00b7ren", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die verst\u00f6rt und aufgeschreckt im Leeren hangen,", "tokens": ["Die", "ver\u00b7st\u00f6rt", "und", "auf\u00b7ge\u00b7schreckt", "im", "Lee\u00b7ren", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Zittern nur vor Schrecken, denen sie entronnen.", "tokens": ["Zit\u00b7tern", "nur", "vor", "Schre\u00b7cken", ",", "de\u00b7nen", "sie", "ent\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Doch manchen klebt noch Irdisches", "tokens": ["Doch", "man\u00b7chen", "klebt", "noch", "Ir\u00b7di\u00b7sches"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "an unvollkomm'nen Leibern.", "tokens": ["an", "un\u00b7voll\u00b7kom\u00b7m'\u00b7nen", "Lei\u00b7bern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Sie wollen Tag nicht lassen, der entschwindet.", "tokens": ["Sie", "wol\u00b7len", "Tag", "nicht", "las\u00b7sen", ",", "der", "ent\u00b7schwin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "PTKNEG", "VVINF", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sie werfen sich in Kr\u00e4mpfen,", "tokens": ["Sie", "wer\u00b7fen", "sich", "in", "Kr\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "schreien gellend in den B\u00e4dern", "tokens": ["schrei\u00b7en", "gel\u00b7lend", "in", "den", "B\u00e4\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und hocken wimmernd und geschlagen", "tokens": ["Und", "ho\u00b7cken", "wim\u00b7mernd", "und", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "in den Ecken.", "tokens": ["in", "den", "E\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Vielen aber ist Himmel aufgetan.", "tokens": ["Vie\u00b7len", "a\u00b7ber", "ist", "Him\u00b7mel", "auf\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NN", "VVPP", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Sie h\u00f6ren die toten Stimmen aller Dinge", "tokens": ["Sie", "h\u00f6\u00b7ren", "die", "to\u00b7ten", "Stim\u00b7men", "al\u00b7ler", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PIAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "sie umkreisen", "tokens": ["sie", "um\u00b7krei\u00b7sen"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Und die schwebende Musik des Alls.", "tokens": ["Und", "die", "schwe\u00b7ben\u00b7de", "Mu\u00b7sik", "des", "Alls", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+---+-+", "measure": "anapaest.init"}, "line.20": {"text": "Sie reden manchmal fremde Worte,", "tokens": ["Sie", "re\u00b7den", "manch\u00b7mal", "frem\u00b7de", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "die man nicht versteht.", "tokens": ["die", "man", "nicht", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.22": {"text": "Sie l\u00e4cheln still und freundlich so wie Kinder tun.", "tokens": ["Sie", "l\u00e4\u00b7cheln", "still", "und", "freund\u00b7lich", "so", "wie", "Kin\u00b7der", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "ADV", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "In den entr\u00fcckten Augen,", "tokens": ["In", "den", "ent\u00b7r\u00fcck\u00b7ten", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "die ni