{"dta.poem.15783": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Befriedigung alswie im kleinsten Sinngedichte", "tokens": ["Be\u00b7frie\u00b7di\u00b7gung", "als\u00b7wie", "im", "kleins\u00b7ten", "Sinn\u00b7ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist nicht im weitesten Gebiete der Geschichte.", "tokens": ["Ist", "nicht", "im", "wei\u00b7tes\u00b7ten", "Ge\u00b7bie\u00b7te", "der", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn der Geschichte fehlt der Gegenwart Begr\u00e4nzung,", "tokens": ["Denn", "der", "Ge\u00b7schich\u00b7te", "fehlt", "der", "Ge\u00b7gen\u00b7wart", "Be\u00b7gr\u00e4n\u00b7zung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ganze Zukunft ist gefordert als Erg\u00e4nzung.", "tokens": ["Die", "gan\u00b7ze", "Zu\u00b7kunft", "ist", "ge\u00b7for\u00b7dert", "als", "Er\u00b7g\u00e4n\u00b7zung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und im Gedichte nur, wenn es ist rechter Art,", "tokens": 