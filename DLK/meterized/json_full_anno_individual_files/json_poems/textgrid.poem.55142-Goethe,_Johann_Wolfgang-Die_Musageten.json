{"textgrid.poem.55142": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Musageten", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft in tiefen Wintern\u00e4chten", "tokens": ["Oft", "in", "tie\u00b7fen", "Win\u00b7ter\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rief ich an die holden Musen:", "tokens": ["Rief", "ich", "an", "die", "hol\u00b7den", "Mu\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbkeine Morgenr\u00f6te leuchtet,", "tokens": ["\u00bb", "kei\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "leuch\u00b7tet", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es will kein Tag erscheinen;", "tokens": ["Und", "es", "will", "kein", "Tag", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber bringt zur rechten Stunde", "tokens": ["A\u00b7ber", "bringt", "zur", "rech\u00b7ten", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir der Lampe fromm Geleuchte,", "tokens": ["Mir", "der", "Lam\u00b7pe", "fromm", "Ge\u00b7leuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df es statt Auror' und Ph\u00f6bus", "tokens": ["Da\u00df", "es", "statt", "Au\u00b7ror'", "und", "Ph\u00f6\u00b7bus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "KON", "NE"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Meinen stillen Flei\u00df belebe!\u00ab", "tokens": ["Mei\u00b7nen", "stil\u00b7len", "Flei\u00df", "be\u00b7le\u00b7be", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Doch sie lie\u00dfen mich im Schlafe,", "tokens": ["Doch", "sie", "lie\u00b7\u00dfen", "mich", "im", "Schla\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dumpf und unerquicklich, liegen,", "tokens": ["Dumpf", "und", "un\u00b7er\u00b7quick\u00b7lich", ",", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und nach jedem sp\u00e4ten Morgen", "tokens": ["Und", "nach", "je\u00b7dem", "sp\u00e4\u00b7ten", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Folgten ungenutzte Tage.", "tokens": ["Folg\u00b7ten", "un\u00b7ge\u00b7nutz\u00b7te", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sich nun der Fr\u00fchling regte,", "tokens": ["Da", "sich", "nun", "der", "Fr\u00fch\u00b7ling", "reg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt ich zu den Nachtigallen:", "tokens": ["Sagt", "ich", "zu", "den", "Nach\u00b7ti\u00b7gal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbliebe Nachtigallen, schlaget", "tokens": ["\u00bb", "lie\u00b7be", "Nach\u00b7ti\u00b7gal\u00b7len", ",", "schla\u00b7get"], "token_info": ["punct", "word", "word", "punct", "word"], "pos": ["$(", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fch, o fr\u00fch! vor meinem Fenster,", "tokens": ["Fr\u00fch", ",", "o", "fr\u00fch", "!", "vor", "mei\u00b7nem", "Fens\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "FM", "ADJD", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weckt mich aus dem vollen Schlafe,", "tokens": ["Weckt", "mich", "aus", "dem", "vol\u00b7len", "Schla\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der den J\u00fcngling m\u00e4chtig fesselt.\u00ab", "tokens": ["Der", "den", "J\u00fcng\u00b7ling", "m\u00e4ch\u00b7tig", "fes\u00b7selt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch die lieberf\u00fcllten S\u00e4nger", "tokens": ["Doch", "die", "lie\u00b7ber\u00b7f\u00fcll\u00b7ten", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dehnten nachts vor meinem Fenster", "tokens": ["Dehn\u00b7ten", "nachts", "vor", "mei\u00b7nem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ihre s\u00fc\u00dfen Melodien,", "tokens": ["Ih\u00b7re", "s\u00fc\u00b7\u00dfen", "Me\u00b7lo\u00b7dien", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Hielten wach die liebe Seele,", "tokens": ["Hiel\u00b7ten", "wach", "die", "lie\u00b7be", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Regten zartes neues Sehnen", "tokens": ["Reg\u00b7ten", "zar\u00b7tes", "neu\u00b7es", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Aus dem neuger\u00fchrten Busen.", "tokens": ["Aus", "dem", "neu\u00b7ge\u00b7r\u00fchr\u00b7ten", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und so ging die Nacht vor\u00fcber,", "tokens": ["Und", "so", "ging", "die", "Nacht", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und Aurora fand mich schlafen,", "tokens": ["Und", "Au\u00b7ro\u00b7ra", "fand", "mich", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.15": {"text": "Ja, mich weckte kaum die Sonne.", "tokens": ["Ja", ",", "mich", "weck\u00b7te", "kaum", "die", "Son\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Endlich ist es Sommer worden,", "tokens": ["End\u00b7lich", "ist", "es", "Som\u00b7mer", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und beim ersten Morgenschimmer", "tokens": ["Und", "beim", "ers\u00b7ten", "Mor\u00b7gen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reizt mich aus dem holden Schlummer", "tokens": ["Reizt", "mich", "aus", "dem", "hol\u00b7den", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die gesch\u00e4ftig fr\u00fche Fliege.", "tokens": ["Die", "ge\u00b7sch\u00e4f\u00b7tig", "fr\u00fc\u00b7he", "Flie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unbarmherzig kehrt sie wieder,", "tokens": ["Un\u00b7barm\u00b7her\u00b7zig", "kehrt", "sie", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn auch oft der halb Erwachte", "tokens": ["Wenn", "auch", "oft", "der", "halb", "Er\u00b7wach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ungeduldig sie verscheuchet,", "tokens": ["Un\u00b7ge\u00b7dul\u00b7dig", "sie", "ver\u00b7scheu\u00b7chet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lockt die unversch\u00e4mten Schwestern,", "tokens": ["Lockt", "die", "un\u00b7ver\u00b7sch\u00e4m\u00b7ten", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und von meinen Augenlidern", "tokens": ["Und", "von", "mei\u00b7nen", "Au\u00b7gen\u00b7li\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mu\u00df der holde Schlaf entweichen.", "tokens": ["Mu\u00df", "der", "hol\u00b7de", "Schlaf", "ent\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "R\u00fcstig spring ich von dem Lager,", "tokens": ["R\u00fcs\u00b7tig", "spring", "ich", "von", "dem", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Suche die geliebten Musen,", "tokens": ["Su\u00b7che", "die", "ge\u00b7lieb\u00b7ten", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Finde sie im Buchenhaine,", "tokens": ["Fin\u00b7de", "sie", "im", "Bu\u00b7chen\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Mich gef\u00e4llig zu empfangen,", "tokens": ["Mich", "ge\u00b7f\u00e4l\u00b7lig", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und den leidigen Insekten", "tokens": ["Und", "den", "lei\u00b7di\u00b7gen", "In\u00b7sek\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Dank ich manche goldne Stunde.", "tokens": ["Dank", "ich", "man\u00b7che", "gold\u00b7ne", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Seid mir doch, ihr Unbequemen,", "tokens": ["Seid", "mir", "doch", ",", "ihr", "Un\u00b7be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Von dem Dichter hoch gepriesen", "tokens": ["Von", "dem", "Dich\u00b7ter", "hoch", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Als die wahren Musageten.", "tokens": ["Als", "die", "wah\u00b7ren", "Mu\u00b7sa\u00b7ge\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Oft in tiefen Wintern\u00e4chten", "tokens": ["Oft", "in", "tie\u00b7fen", "Win\u00b7ter\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rief ich an die holden Musen:", "tokens": ["Rief", "ich", "an", "die", "hol\u00b7den", "Mu\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbkeine Morgenr\u00f6te leuchtet,", "tokens": ["\u00bb", "kei\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "leuch\u00b7tet", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es will kein Tag erscheinen;", "tokens": ["Und", "es", "will", "kein", "Tag", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber bringt zur rechten Stunde", "tokens": ["A\u00b7ber", "bringt", "zur", "rech\u00b7ten", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir der Lampe fromm Geleuchte,", "tokens": ["Mir", "der", "Lam\u00b7pe", "fromm", "Ge\u00b7leuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra