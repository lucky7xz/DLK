{"textgrid.poem.60641": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man kann den Magen wohl als Bild", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man kann den Magen wohl als Bild", "tokens": ["Man", "kann", "den", "Ma\u00b7gen", "wohl", "als", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des K\u00f6nigtums erblicken:", "tokens": ["Des", "K\u00f6\u00b7nig\u00b7tums", "er\u00b7bli\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist er von einer Not erf\u00fcllt,", "tokens": ["Ist", "er", "von", "ei\u00b7ner", "Not", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird sie den ganzen K\u00f6rper dr\u00fccken.", "tokens": ["Wird", "sie", "den", "gan\u00b7zen", "K\u00f6r\u00b7per", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Einmal geschah es, da\u00df die Glieder grollten,", "tokens": ["Ein\u00b7mal", "ge\u00b7schah", "es", ",", "da\u00df", "die", "Glie\u00b7der", "groll\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADJA", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sie m\u00fc\u00dften alles schaffen f\u00fcr den Magen,", "tokens": ["Sie", "m\u00fc\u00df\u00b7ten", "al\u00b7les", "schaf\u00b7fen", "f\u00fcr", "den", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sie beschlossen, da\u00df sie feiern wollten:", "tokens": ["Und", "sie", "be\u00b7schlos\u00b7sen", ",", "da\u00df", "sie", "fei\u00b7ern", "woll\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Nichtstun w\u00fcrde ihnen auch behagen.", "tokens": ["Das", "Nichts\u00b7tun", "w\u00fcr\u00b7de", "ih\u00b7nen", "auch", "be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u00bber m\u00f6ge\u00ab, sagten sie, \u00bbsich selber doch ern\u00e4hren.", "tokens": ["\u00bb", "er", "m\u00f6\u00b7ge", "\u00ab", ",", "sag\u00b7ten", "sie", ",", "\u00bb", "sich", "sel\u00b7ber", "doch", "er\u00b7n\u00e4h\u00b7ren", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was sollen wir uns ewig f\u00fcr ihn plagen?", "tokens": ["Was", "sol\u00b7len", "wir", "uns", "e\u00b7wig", "f\u00fcr", "ihn", "pla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wir m\u00fchn uns ab \u2013 f\u00fcr wen? f\u00fcr ihn allein!", "tokens": ["Wir", "m\u00fchn", "uns", "ab", "\u2013", "f\u00fcr", "wen", "?", "f\u00fcr", "ihn", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKVZ", "$(", "APPR", "PWS", "$.", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und er will keinen Tag der Ruhe uns gew\u00e4hren,", "tokens": ["Und", "er", "will", "kei\u00b7nen", "Tag", "der", "Ru\u00b7he", "uns", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIAT", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Will immerfort von uns gef\u00fcttert sein.", "tokens": ["Will", "im\u00b7mer\u00b7fort", "von", "uns", "ge\u00b7f\u00fct\u00b7tert", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wir feiern jetzt \u2013 nach seinen eigenen Lehren!\u00ab", "tokens": ["Wir", "fei\u00b7ern", "jetzt", "\u2013", "nach", "sei\u00b7nen", "ei\u00b7ge\u00b7nen", "Leh\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Gesagt, getan. Die H\u00e4nde fa\u00dften nichts mehr an,", "tokens": ["Ge\u00b7sagt", ",", "ge\u00b7tan", ".", "Die", "H\u00e4n\u00b7de", "fa\u00df\u00b7ten", "nichts", "mehr", "an", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$.", "ART", "NN", "VVFIN", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es schlief der Arm, es ruhten Fu\u00df und Bein.", "tokens": ["Es", "schlief", "der", "Arm", ",", "es", "ruh\u00b7ten", "Fu\u00df", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Dem Magen rieten sie, sein eigner Koch zu sein.", "tokens": ["Dem", "Ma\u00b7gen", "rie\u00b7ten", "sie", ",", "sein", "eig\u00b7ner", "Koch", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch schlimmer, als der tolle Streich begann,", "tokens": ["Doch", "schlim\u00b7mer", ",", "als", "der", "tol\u00b7le", "Streich", "be\u00b7gann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "War bald die Folge, die man schwer bereute:", "tokens": ["War", "bald", "die", "Fol\u00b7ge", ",", "die", "man", "schwer", "be\u00b7reu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Des Magens Darben schuf kein neues Blut,", "tokens": ["Des", "Ma\u00b7gens", "Dar\u00b7ben", "schuf", "kein", "neu\u00b7es", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Von Kr\u00e4ften kamen die bet\u00f6rten Leute,", "tokens": ["Von", "Kr\u00e4f\u00b7ten", "ka\u00b7men", "die", "be\u00b7t\u00f6r\u00b7ten", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Jeder verlor, kein einziger gewann.", "tokens": ["Je\u00b7der", "ver\u00b7lor", ",", "kein", "ein\u00b7zi\u00b7ger", "ge\u00b7wann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.19": {"text": "Und so erkannten die Rebellen gut,", "tokens": ["Und", "so", "er\u00b7kann\u00b7ten", "die", "Re\u00b7bel\u00b7len", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Da\u00df er, den sie voll Frevelmut", "tokens": ["Da\u00df", "er", ",", "den", "sie", "voll", "Fre\u00b7vel\u00b7mut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Den schlimmsten M\u00fc\u00dfigg\u00e4nger schalten,", "tokens": ["Den", "schlimms\u00b7ten", "M\u00fc\u00b7\u00dfig\u00b7g\u00e4n\u00b7ger", "schal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Mehr tat als sie, um alles gut in Gang zu halten.", "tokens": ["Mehr", "tat", "als", "sie", ",", "um", "al\u00b7les", "gut", "in", "Gang", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KOUS", "PPER", "$,", "APPR", "PIS", "ADJD", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Genau so, d\u00fcnkt mir, sieht es aus", "tokens": ["Ge\u00b7nau", "so", ",", "d\u00fcnkt", "mir", ",", "sieht", "es", "aus"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit uns und unserm K\u00f6nigshaus.", "tokens": ["Mit", "uns", "und", "un\u00b7serm", "K\u00f6\u00b7nigs\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir alle liefern Arbeit f\u00fcr die Majest\u00e4t,", "tokens": ["Wir", "al\u00b7le", "lie\u00b7fern", "Ar\u00b7beit", "f\u00fcr", "die", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ihrerseits besorgt vom Thron herniedersp\u00e4ht,", "tokens": ["Die", "ih\u00b7rer\u00b7seits", "be\u00b7sorgt", "vom", "Thron", "her\u00b7nie\u00b7der\u00b7sp\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Handwerk und Ackerbau und Handel hebt,", "tokens": ["Hand\u00b7werk", "und", "A\u00b7cker\u00b7bau", "und", "Han\u00b7del", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Amtspersonen \u00fcberall bestellt,", "tokens": ["Die", "Amts\u00b7per\u00b7so\u00b7nen", "\u00fc\u00b7be\u00b7rall", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Heer besoldet, K\u00fcnstler unterh\u00e4lt", "tokens": ["Das", "Heer", "be\u00b7sol\u00b7det", ",", "K\u00fcnst\u00b7ler", "un\u00b7ter\u00b7h\u00e4lt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und stets nach Glanz und Ruhm des Landes strebt.", "tokens": ["Und", "stets", "nach", "Glanz", "und", "Ruhm", "des", "Lan\u00b7des", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wir alle dienen einer Obrigkeit,", "tokens": ["Wir", "al\u00b7le", "die\u00b7nen", "ei\u00b7ner", "Ob\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und sie allein belebt den ganzen Staat.", "tokens": ["Und", "sie", "al\u00b7lein", "be\u00b7lebt", "den", "gan\u00b7zen", "Staat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "So schlichtete Menenius einst den Streit,", "tokens": ["So", "schlich\u00b7te\u00b7te", "Me\u00b7ne\u00b7ni\u00b7us", "einst", "den", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Da sich vom stolzen r\u00f6mischen Senat", "tokens": ["Da", "sich", "vom", "stol\u00b7zen", "r\u00f6\u00b7mi\u00b7schen", "Se\u00b7nat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Emp\u00f6rt getrennt das Proletariat.", "tokens": ["Em\u00b7p\u00f6rt", "ge\u00b7trennt", "das", "Pro\u00b7le\u00b7ta\u00b7ri\u00b7at", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Es grollte, er allein besitze alle Macht,", "tokens": ["Es", "groll\u00b7te", ",", "er", "al\u00b7lein", "be\u00b7sit\u00b7ze", "al\u00b7le", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Verf\u00fcge \u00fcber Gut und Geld und Ehr und W\u00fcrde,", "tokens": ["Ver\u00b7f\u00fc\u00b7ge", "\u00fc\u00b7ber", "Gut", "und", "Geld", "und", "Ehr", "und", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Indessen ihm, dem Volk, nichts sei von aller Pracht,", "tokens": ["In\u00b7des\u00b7sen", "ihm", ",", "dem", "Volk", ",", "nichts", "sei", "von", "al\u00b7ler", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "$,", "PIS", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nur Not und Steuern und des Krieges harte B\u00fcrde.", "tokens": ["Nur", "Not", "und", "Steu\u00b7ern", "und", "des", "Krie\u00b7ges", "har\u00b7te", "B\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sie f\u00fchlten vom Senat sich j\u00e4mmerlich betrogen", "tokens": ["Sie", "f\u00fchl\u00b7ten", "vom", "Se\u00b7nat", "sich", "j\u00e4m\u00b7mer\u00b7lich", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "ADJD", "VVPP"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Und waren schon vereint zum Tor hinausgezogen,", "tokens": ["Und", "wa\u00b7ren", "schon", "ver\u00b7eint", "zum", "Tor", "hin\u00b7aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Um auszuwandern in ein be\u00dfres Land,", "tokens": ["Um", "aus\u00b7zu\u00b7wan\u00b7dern", "in", "ein", "be\u00df\u00b7res", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVIZU", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Als klug Menenius jene Fabel fand,", "tokens": ["Als", "klug", "Me\u00b7ne\u00b7ni\u00b7us", "je\u00b7ne", "Fa\u00b7bel", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NE", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.22": {"text": "Der die Geschichte dann Unsterblichkeit bescherte.", "tokens": ["Der", "die", "Ge\u00b7schich\u00b7te", "dann", "U\u00b7nsterb\u00b7lich\u00b7keit", "be\u00b7scher\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Das Gleichnis von den Gliedern war's, das sie bekehrte", "tokens": ["Das", "Gleich\u00b7nis", "von", "den", "Glie\u00b7dern", "wa\u00b7r's", ",", "das", "sie", "be\u00b7kehr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und fest wie je zuvor mit ihrem Staat verband.", "tokens": ["Und", "fest", "wie", "je", "zu\u00b7vor", "mit", "ih\u00b7rem", "Staat", "ver\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Man kann den Magen wohl als Bild", "tokens": ["Man", "kann", "den", "Ma\u00b7gen", "wohl", "als", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des K\u00f6nigtums erblicken:", "tokens": ["Des", "K\u