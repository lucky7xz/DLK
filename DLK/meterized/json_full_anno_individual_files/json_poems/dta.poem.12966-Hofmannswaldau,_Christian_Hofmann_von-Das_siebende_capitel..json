{"dta.poem.12966": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das siebende capitel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje eitel ist der mensch, wenn er auf menschen bauet,", "tokens": ["Wie", "ei\u00b7tel", "ist", "der", "mensch", ",", "wenn", "er", "auf", "men\u00b7schen", "bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und seine zuversicht auf creaturen setzt.", "tokens": ["Und", "sei\u00b7ne", "zu\u00b7ver\u00b7sicht", "auf", "crea\u00b7tu\u00b7ren", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der ist wahrhafftig arm, dem vor der armuth grauet,", "tokens": ["Der", "ist", "wahr\u00b7haff\u00b7tig", "arm", ",", "dem", "vor", "der", "ar\u00b7muth", "grau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ADJD", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und das, was irrdisch hei\u00dft, vor seinen reichthum sch\u00e4tzt.", "tokens": ["Und", "das", ",", "was", "irr\u00b7disch", "hei\u00dft", ",", "vor", "sei\u00b7nen", "reicht\u00b7hum", "sch\u00e4tzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "ADJD", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum sey nicht so gesinnt, und nimm in reiner liebe", "tokens": ["Drum", "sey", "nicht", "so", "ge\u00b7sinnt", ",", "und", "nimm", "in", "rei\u00b7ner", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,", "KON", "VVIMP", "APPR", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des theuren JEsu dich der armen br\u00fcder an.", "tokens": ["Des", "theu\u00b7ren", "Je\u00b7su", "dich", "der", "ar\u00b7men", "br\u00fc\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verzweifle an dir selbst, und widersteh dem triebe,", "tokens": ["Ver\u00b7zweif\u00b7le", "an", "dir", "selbst", ",", "und", "wi\u00b7der\u00b7steh", "dem", "trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "$,", "KON", "VVFIN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der dir wol viel verspricht, doch nichts gew\u00e4hren kan.", "tokens": ["Der", "dir", "wol", "viel", "ver\u00b7spricht", ",", "doch", "nichts", "ge\u00b7w\u00e4h\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$,", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vertraue lieber GOtt, als deinen eignen wercken:", "tokens": ["Ver\u00b7trau\u00b7e", "lie\u00b7ber", "Gott", ",", "als", "dei\u00b7nen", "eig\u00b7nen", "wer\u00b7cken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wer auf genade baut, der kommt am besten fort.", "tokens": ["Wer", "auf", "ge\u00b7na\u00b7de", "baut", ",", "der", "kommt", "am", "bes\u00b7ten", "fort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "PTKA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wo wir nicht unsern geist in seinem Geiste st\u00e4rcken,", "tokens": ["Wo", "wir", "nicht", "un\u00b7sern", "geist", "in", "sei\u00b7nem", "Geis\u00b7te", "st\u00e4r\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So fahren wir gewi\u00df gar langsam an den port.", "tokens": ["So", "fah\u00b7ren", "wir", "ge\u00b7wi\u00df", "gar", "lang\u00b7sam", "an", "den", "port", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "La\u00df deinen willen sich in GOttes willen sencken,", "tokens": ["La\u00df", "dei\u00b7nen", "wil\u00b7len", "sich", "in", "Got\u00b7tes", "wil\u00b7len", "sen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PRF", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So wird der HErr mit dir, und deinem willen seyn,", "tokens": ["So", "wird", "der", "Herr", "mit", "dir", ",", "und", "dei\u00b7nem", "wil\u00b7len", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "$,", "KON", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hier ist an keine kunst und menschen-krafft zu dencken,", "tokens": ["Hier", "ist", "an", "kei\u00b7ne", "kunst", "und", "men\u00b7schen\u00b7krafft", "zu", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die hoffnung dieser welt trifft niemals richtig ein.", "tokens": ["Die", "hoff\u00b7nung", "die\u00b7ser", "welt", "trifft", "nie\u00b7mals", "rich\u00b7tig", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Drum setze dein vertraun nur auf des H\u00f6chsten gnade,", "tokens": ["Drum", "set\u00b7ze", "dein", "ver\u00b7traun", "nur", "auf", "des", "H\u00f6chs\u00b7ten", "gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJD", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die allen niedrigen empor zu helffen pflegt,", "tokens": ["Die", "al\u00b7len", "nied\u00b7ri\u00b7gen", "em\u00b7por", "zu", "helf\u00b7fen", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Den aber, der von sich die trefflichste parade", "tokens": ["Den", "a\u00b7ber", ",", "der", "von", "sich", "die", "treff\u00b7lichs\u00b7te", "pa\u00b7ra\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "PRELS", "APPR", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und gro\u00dfes wesen macht, st\u00fcrtzt und zu boden schl\u00e4gt.", "tokens": ["Und", "gro\u00b7\u00dfes", "we\u00b7sen", "macht", ",", "st\u00fcrtzt", "und", "zu", "bo\u00b7den", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "R\u00fchm und verla\u00df dich nicht auf deine freund und habe,", "tokens": ["R\u00fchm", "und", "ver\u00b7la\u00df", "dich", "nicht", "auf", "dei\u00b7ne", "freund", "und", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVIMP", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "KON", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auf solchen \u00fcbermuth folgt offt der gr\u00f6ste spott.", "tokens": ["Auf", "sol\u00b7chen", "\u00fc\u00b7ber\u00b7muth", "folgt", "offt", "der", "gr\u00f6s\u00b7te", "spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Den stoltzen Goliath st\u00fcrtzt auch ein kleiner knabe;", "tokens": ["Den", "stolt\u00b7zen", "Go\u00b7li\u00b7ath", "st\u00fcrtzt", "auch", "ein", "klei\u00b7ner", "kna\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Drum wer sich r\u00fchmen will, der r\u00fchme sich in GOtt.", "tokens": ["Drum", "wer", "sich", "r\u00fch\u00b7men", "will", ",", "der", "r\u00fch\u00b7me", "sich", "in", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "PRF", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ja GOtt, der alle ding, und \u00fcber alle dinge", "tokens": ["Ja", "Gott", ",", "der", "al\u00b7le", "ding", ",", "und", "\u00fc\u00b7ber", "al\u00b7le", "din\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "NN", "$,", "PRELS", "PIAT", "NN", "$,", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sich selbst auch geben will. O unbesonnenheit", "tokens": ["Sich", "selbst", "auch", "ge\u00b7ben", "will", ".", "O", "un\u00b7be\u00b7son\u00b7nen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "ADV", "ADV", "VVINF", "VMFIN", "$.", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der rasend-tollen welt! sie achtet GOtt geringe,", "tokens": ["Der", "ra\u00b7sen\u00b7dtol\u00b7len", "welt", "!", "sie", "ach\u00b7tet", "Gott", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und nimmt f\u00fcr alles gut das nichts der eitelkeit.", "tokens": ["Und", "nimmt", "f\u00fcr", "al\u00b7les", "gut", "das", "nichts", "der", "ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "ADJD", "ART", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Bist du itzt sch\u00f6n und frisch, erwarte nur das alter:", "tokens": ["Bist", "du", "itzt", "sch\u00f6n", "und", "frisch", ",", "er\u00b7war\u00b7te", "nur", "das", "al\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,", "VVFIN", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Vielleichte stellt sich auch noch heut ein fieber ein,", "tokens": ["Viel\u00b7leich\u00b7te", "stellt", "sich", "auch", "noch", "heut", "ein", "fie\u00b7ber", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dann d\u00f6rfftest du gewi\u00df in kurtzem ungestalter,", "tokens": ["Dann", "d\u00f6rff\u00b7test", "du", "ge\u00b7wi\u00df", "in", "kurt\u00b7zem", "un\u00b7ge\u00b7stal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und deiner glieder stoltz schon etwas zahmer seyn.", "tokens": ["Und", "dei\u00b7ner", "glie\u00b7der", "stoltz", "schon", "et\u00b7was", "zah\u00b7mer", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Bist du geschickt und klug? ach la\u00df es dich nicht d\u00fcncken.", "tokens": ["Bist", "du", "ge\u00b7schickt", "und", "klug", "?", "ach", "la\u00df", "es", "dich", "nicht", "d\u00fcn\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KON", "ADJD", "$.", "XY", "VVIMP", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Denn wer ihm selbst gef\u00e4llt, gef\u00e4llt dem himmel nicht.", "tokens": ["Denn", "wer", "ihm", "selbst", "ge\u00b7f\u00e4llt", ",", "ge\u00b7f\u00e4llt", "dem", "him\u00b7mel", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Gott, dessen alles ist, l\u00e4st nur die gnade sincken,", "tokens": ["Gott", ",", "des\u00b7sen", "al\u00b7les", "ist", ",", "l\u00e4st", "nur", "die", "gna\u00b7de", "sin\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "PIS", "VAFIN", "$,", "VVFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So ist dein hoher sinn ein leuchter ohne licht.", "tokens": ["So", "ist", "dein", "ho\u00b7her", "sinn", "ein", "leuch\u00b7ter", "oh\u00b7ne", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Drum nimm dir nichts voraus. Wer sich den fr\u00f6mmsten nennet,", "tokens": ["Drum", "nimm", "dir", "nichts", "vo\u00b7raus", ".", "Wer", "sich", "den", "fr\u00f6mms\u00b7ten", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIS", "PTKVZ", "$.", "PWS", "PRF", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und in der heucheley sich selbst bewundern kan,", "tokens": ["Und", "in", "der", "heu\u00b7che\u00b7ley", "sich", "selbst", "be\u00b7wun\u00b7dern", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Den sieht der gro\u00dfe GOtt, der das verborgne kennet,", "tokens": ["Den", "sieht", "der", "gro\u00b7\u00dfe", "Gott", ",", "der", "das", "ver\u00b7borg\u00b7ne", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und nicht betragen wird, stets vor den \u00e4rgsten an.", "tokens": ["Und", "nicht", "be\u00b7tra\u00b7gen", "wird", ",", "stets", "vor", "den", "\u00e4rgs\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVINF", "VAFIN", "$,", "ADV", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Denn GOttes urtheil geht weit tiefer, als der d\u00fcnckel", "tokens": ["Denn", "Got\u00b7tes", "ur\u00b7theil", "geht", "weit", "tie\u00b7fer", ",", "als", "der", "d\u00fcn\u00b7ckel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "NE", "VVFIN", "ADJD", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Der bl\u00f6den sterblichen, die offt der schein betr\u00fcgt.", "tokens": ["Der", "bl\u00f6\u00b7den", "sterb\u00b7li\u00b7chen", ",", "die", "offt", "der", "schein", "be\u00b7tr\u00fcgt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$,", "PRELS", "ADV", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Sein scharffes auge dringt in alle hertzens-winckel,", "tokens": ["Sein", "scharf\u00b7fes", "au\u00b7ge", "dringt", "in", "al\u00b7le", "hert\u00b7zens\u00b7win\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "In welchen vor der welt manch schalck verborgen liegt.", "tokens": ["In", "wel\u00b7chen", "vor", "der", "welt", "manch", "schalck", "ver\u00b7bor\u00b7gen", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "APPR", "ART", "NN", "PIAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Sind deine wercke gleich bey menschen hochgeachtet,", "tokens": ["Sind", "dei\u00b7ne", "wer\u00b7cke", "gleich", "bey", "men\u00b7schen", "hoch\u00b7ge\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "So k\u00f6nnen sie bey GOtt doch wol verworffen seyn.", "tokens": ["So", "k\u00f6n\u00b7nen", "sie", "bey", "Gott", "doch", "wol", "ver\u00b7worf\u00b7fen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wer GOtt den eigen-ruhm zu einem opffer schlachet,", "tokens": ["Wer", "Gott", "den", "ei\u00b7gen\u00b7ruhm", "zu", "ei\u00b7nem", "opf\u00b7fer", "schla\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Den wird die ewigkeit mit ehr und ruhm erfreun.", "tokens": ["Den", "wird", "die", "e\u00b7wig\u00b7keit", "mit", "ehr", "und", "ruhm", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Ist etwas guts an dir; so halt doch andre besser", "tokens": ["Ist", "et\u00b7was", "guts", "an", "dir", ";", "so", "halt", "doch", "and\u00b7re", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "PPER", "$.", "ADV", "VVFIN", "ADV", "PIS", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und heiliger denn dich. Denn ein gebrochner sinn,", "tokens": ["Und", "hei\u00b7li\u00b7ger", "denn", "dich", ".", "Denn", "ein", "ge\u00b7broch\u00b7ner", "sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "KON", "PPER", "$.", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "In dem die demuth herrscht, steigt \u00fcber alle schl\u00f6sser", "tokens": ["In", "dem", "die", "de\u00b7muth", "herrscht", ",", "steigt", "\u00fc\u00b7ber", "al\u00b7le", "schl\u00f6s\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "PIAT", "ADJA"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.52": {"text": "Und allen eigen-ruhm der pharis\u00e4er hin.", "tokens": ["Und", "al\u00b7len", "ei\u00b7gen\u00b7ruhm", "der", "pha\u00b7ri\u00b7s\u00e4\u00b7er", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Wenn du dich in der welt vor den geringsten sch\u00e4tzest,", "tokens": ["Wenn", "du", "dich", "in", "der", "welt", "vor", "den", "ge\u00b7rings\u00b7ten", "sch\u00e4t\u00b7zest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Was schadet dir es? nichts: das aber allzu viel,", "tokens": ["Was", "scha\u00b7det", "dir", "es", "?", "nichts", ":", "das", "a\u00b7ber", "all\u00b7zu", "viel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$.", "PIS", "$.", "PDS", "ADV", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Wenn du dich auch, o mensch! blos \u00fcber einen setzest,", "tokens": ["Wenn", "du", "dich", "auch", ",", "o", "mensch", "!", "blos", "\u00fc\u00b7ber", "ei\u00b7nen", "set\u00b7zest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "$,", "FM", "FM", "$.", "ADV", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Weil GOtt die demuth nur mit friede er\u00f6nen will.", "tokens": ["Weil", "Gott", "die", "de\u00b7muth", "nur", "mit", "frie\u00b7de", "e\u00b7r\u00f6\u00b7nen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.57": {"text": "Wie soltest du nun nicht den stoltzen sinn verdammen,", "tokens": ["Wie", "sol\u00b7test", "du", "nun", "nicht", "den", "stolt\u00b7zen", "sinn", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Der nichts als h\u00f6llen-angst und marter bringen kan.", "tokens": ["Der", "nichts", "als", "h\u00f6l\u00b7len\u00b7angst", "und", "mar\u00b7ter", "brin\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "ADJD", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Ach \u00fcberwinde dich, und z\u00fcnd\u2019 ihn durch die flammen", "tokens": ["Ach", "\u00fc\u00b7berw\u00b7in\u00b7de", "dich", ",", "und", "z\u00fcnd'", "ihn", "durch", "die", "flam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Des Geistes und der reu GOtt als ein opffer an.", "tokens": ["Des", "Geis\u00b7tes", "und", "der", "reu", "Gott", "als", "ein", "opf\u00b7fer", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJD", "NN", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}}}}