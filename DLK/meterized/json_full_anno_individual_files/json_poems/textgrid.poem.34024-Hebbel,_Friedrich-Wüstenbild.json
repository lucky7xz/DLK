{"textgrid.poem.34024": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "W\u00fcstenbild", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber der W\u00fcste schwebt ein Geier und sp\u00e4ht nach der Beute,", "tokens": ["Ue\u00b7ber", "der", "W\u00fcs\u00b7te", "schwebt", "ein", "Gei\u00b7er", "und", "sp\u00e4ht", "nach", "der", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Unten im Sande zieht keuchend ein Wand'rer daher.", "tokens": ["Un\u00b7ten", "im", "San\u00b7de", "zieht", "keu\u00b7chend", "ein", "Wan\u00b7d'\u00b7rer", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ADJD", "ART", "NN", "PAV", "$."], "meter": "+--+-+---+---+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Jener m\u00f6gte verhungern, und nirgends erblickt er ein Leben,", "tokens": ["Je\u00b7ner", "m\u00f6g\u00b7te", "ver\u00b7hun\u00b7gern", ",", "und", "nir\u00b7gends", "er\u00b7blickt", "er", "ein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "VVINF", "$,", "KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Dieser verschmachtet vor Durst, aber ihm rieselt kein Quell.", "tokens": ["Die\u00b7ser", "ver\u00b7schmach\u00b7tet", "vor", "Durst", ",", "a\u00b7ber", "ihm", "rie\u00b7selt", "kein", "Quell", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,", "KON", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Da gewahren sich Beide, und brennen, zusammen zu treffen,", "tokens": ["Da", "ge\u00b7wah\u00b7ren", "sich", "Bei\u00b7de", ",", "und", "bren\u00b7nen", ",", "zu\u00b7sam\u00b7men", "zu", "tref\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIS", "$,", "KON", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Hat doch der Eine sein Fleisch, hat doch der And're sein Blut!", "tokens": ["Hat", "doch", "der", "Ei\u00b7ne", "sein", "Fleisch", ",", "hat", "doch", "der", "An\u00b7d'\u00b7re", "sein", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ART", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ueber der W\u00fcste schwebt ein Geier und sp\u00e4ht nach der Beute,", "tokens": ["Ue\u00b7ber", "der", "W\u00fcs\u00b7te", "schwebt", "ein", "Gei\u00b7er", "und", "sp\u00e4ht", "nach", "der", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Unten im Sande zieht keuchend ein Wand'rer daher.", "tokens": ["Un\u00b7ten", "im", "San\u00b7de", "zieht", "keu\u00b7chend", "ein", "Wan\u00b7d'\u00b7rer", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word