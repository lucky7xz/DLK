{"dta.poem.21631": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Daniel in der L\u00f6wengrube.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wende Dein Antlitz mir zu, o Herr,", "tokens": ["Wen\u00b7de", "Dein", "Ant\u00b7litz", "mir", "zu", ",", "o", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Denn mich d\u00fcrstet nach seinem Lichte!", "tokens": ["Denn", "mich", "d\u00fcrs\u00b7tet", "nach", "sei\u00b7nem", "Lich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Mich umnachtet Entsetzen und Grau\u2019n.", "tokens": ["Mich", "um\u00b7nach\u00b7tet", "Ent\u00b7set\u00b7zen", "und", "Grau'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "NE"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Schick\u2019 Deinen Odem mir zu, o Herr,", "tokens": ["Schick'", "Dei\u00b7nen", "O\u00b7dem", "mir", "zu", ",", "o", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PPER", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Denn mich verlangt nach seinem Wehen,", "tokens": ["Denn", "mich", "ver\u00b7langt", "nach", "sei\u00b7nem", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In mir ringet Leben und Tod!", "tokens": ["In", "mir", "rin\u00b7get", "Le\u00b7ben", "und", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Du h\u00f6rst mein Rufen, ich wei\u00df es!", "tokens": ["Du", "h\u00f6rst", "mein", "Ru\u00b7fen", ",", "ich", "wei\u00df", "es", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ob ich stehe in Wolken des Himmels", "tokens": ["Ob", "ich", "ste\u00b7he", "in", "Wol\u00b7ken", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Weit entr\u00fcckt den Gefilden der Flur,", "tokens": ["Weit", "ent\u00b7r\u00fcckt", "den", "Ge\u00b7fil\u00b7den", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Auf des Gebirges zackigem Haupt \u2014", "tokens": ["Auf", "des", "Ge\u00b7bir\u00b7ges", "za\u00b7cki\u00b7gem", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Oder liege, umg\u00e4hnt von Schl\u00fcnden,", "tokens": ["O\u00b7der", "lie\u00b7ge", ",", "um\u00b7g\u00e4hnt", "von", "Schl\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "In der Erde gr\u00e4\u00dflichem Bauche,", "tokens": ["In", "der", "Er\u00b7de", "gr\u00e4\u00df\u00b7li\u00b7chem", "Bau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Du vernimmst meine Stimme, o Herr!", "tokens": ["Du", "ver\u00b7nimmst", "mei\u00b7ne", "Stim\u00b7me", ",", "o", "Herr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Dein Blick ist auf mir, ich wei\u00df es!", "tokens": ["Dein", "Blick", "ist", "auf", "mir", ",", "ich", "wei\u00df", "es", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Ob Deine Sonne den Himmel durchwandelt,", "tokens": ["Ob", "Dei\u00b7ne", "Son\u00b7ne", "den", "Him\u00b7mel", "durch\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Tr\u00e4nkend die Welten mit Licht,", "tokens": ["Tr\u00e4n\u00b7kend", "die", "Wel\u00b7ten", "mit", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.17": {"text": "Oder Orion die Nacht durchflammet", "tokens": ["O\u00b7der", "O\u00b7rion", "die", "Nacht", "durch\u00b7flam\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Und des Wagens siebenstirnige Pracht, \u2014", "tokens": ["Und", "des", "Wa\u00b7gens", "sie\u00b7bens\u00b7tir\u00b7ni\u00b7ge", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,", "$("], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Du siehst meine N\u00f6the, o Herr! \u2014", "tokens": ["Du", "siehst", "mei\u00b7ne", "N\u00f6\u00b7the", ",", "o", "Herr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.20": {"text": "Schrecken waffnen sich wider mich!", "tokens": ["Schre\u00b7cken", "waff\u00b7nen", "sich", "wi\u00b7der", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.21": {"text": "Br\u00fcllend lechzet nach mir der Tod!", "tokens": ["Br\u00fcl\u00b7lend", "lech\u00b7zet", "nach", "mir", "der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "Schaudernde Aengste, ein eisiger Strom,", "tokens": ["Schau\u00b7dern\u00b7de", "A\u00b7engs\u00b7te", ",", "ein", "ei\u00b7si\u00b7ger", "Strom", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+---+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "W\u00e4lzen sich \u00fcber den sterblichen Leib!", "tokens": ["W\u00e4l\u00b7zen", "sich", "\u00fc\u00b7ber", "den", "sterb\u00b7li\u00b7chen", "Leib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.24": {"text": "Recke die Hand, o Herr und r\u00fchre an meine Seele!", "tokens": ["Re\u00b7cke", "die", "Hand", ",", "o", "Herr", "und", "r\u00fch\u00b7re", "an", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "FM", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-++-+-", "measure": "iambic.septa.invert"}, "line.25": {"text": "Da\u00df sie auf himmlischen Schwingen sich hebe", "tokens": ["Da\u00df", "sie", "auf", "himm\u00b7li\u00b7schen", "Schwin\u00b7gen", "sich", "he\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PRF", "VVFIN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.26": {"text": "Aus des Entsetzens erstarrender Fluth.", "tokens": ["Aus", "des", "Ent\u00b7set\u00b7zens", "er\u00b7star\u00b7ren\u00b7der", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.27": {"text": "Ha\u00df und Verh\u00f6hnung umtoben mein Ohr \u2014", "tokens": ["Ha\u00df", "und", "Ver\u00b7h\u00f6h\u00b7nung", "um\u00b7to\u00b7ben", "mein", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.28": {"text": "Zagend verstummet die Stimme der Hoffnung \u2014", "tokens": ["Za\u00b7gend", "ver\u00b7stum\u00b7met", "die", "Stim\u00b7me", "der", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.29": {"text": "Oeffne den Mund, o Herr und rede zu meiner Seele,", "tokens": ["Oeff\u00b7ne", "den", "Mund", ",", "o", "Herr", "und", "re\u00b7de", "zu", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "FM", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.30": {"text": "Da\u00df sie erwache aus todtem Verstummen!", "tokens": ["Da\u00df", "sie", "er\u00b7wa\u00b7che", "aus", "tod\u00b7tem", "Ver\u00b7stum\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Gedenk\u2019 o meine Seele", "tokens": ["Ge\u00b7denk'", "o", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "FM", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Da\u00df Du entstammst von Gott,", "tokens": ["Da\u00df", "Du", "ent\u00b7stammst", "von", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Sei muthig drum im Unheil", "tokens": ["Sei", "mut\u00b7hig", "drum", "im", "Un\u00b7heil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "PAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Und trage stolz den Spott!", "tokens": ["Und", "tra\u00b7ge", "stolz", "den", "Spott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Streb\u2019 auf, o meine Seele,", "tokens": ["Streb'", "auf", ",", "o", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Sei w\u00fcrdig deines Herrn,", "tokens": ["Sei", "w\u00fcr\u00b7dig", "dei\u00b7nes", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Er harret, da\u00df du kommest", "tokens": ["Er", "har\u00b7ret", ",", "da\u00df", "du", "kom\u00b7mest"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Und er empf\u00e4ngt dich gern!", "tokens": ["Und", "er", "emp\u00b7f\u00e4ngt", "dich", "gern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Soll er herab sich neigen", "tokens": ["Soll", "er", "her\u00b7ab", "sich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "So ringe du empor,", "tokens": ["So", "rin\u00b7ge", "du", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Dann kommt er dir entgegen", "tokens": ["Dann", "kommt", "er", "dir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Und neiget dir sein Ohr;", "tokens": ["Und", "nei\u00b7get", "dir", "sein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Dann sei bereit, o Seele,", "tokens": ["Dann", "sei", "be\u00b7reit", ",", "o", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Dein Gott zieht in dich ein,", "tokens": ["Dein", "Gott", "zieht", "in", "dich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--++", "measure": "iambic.tri.relaxed"}, "line.45": {"text": "Gro\u00df wird und schwer dein Leiden", "tokens": ["Gro\u00df", "wird", "und", "schwer", "dein", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "KON", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Doch du wirst gr\u00f6\u00dfer sein!", "tokens": ["Doch", "du", "wirst", "gr\u00f6\u00b7\u00dfer", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.47": {"text": "Ich halt\u2019 es in H\u00e4nden und lasse es nicht", "tokens": ["Ich", "halt'", "es", "in", "H\u00e4n\u00b7den", "und", "las\u00b7se", "es", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "VVFIN", "PPER", "PTKNEG"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.48": {"text": "Das Band, das, o Herr, mit Dir mich verbindet:", "tokens": ["Das", "Band", ",", "das", ",", "o", "Herr", ",", "mit", "Dir", "mich", "ver\u00b7bin\u00b7det", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "$,", "FM", "NN", "$,", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.49": {"text": "Glauben und br\u00fcnstig Vertrau\u2019n!", "tokens": ["Glau\u00b7ben", "und", "br\u00fcns\u00b7tig", "Ver\u00b7trau'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.50": {"text": "Ich wandle in Nacht, doch am Ziele ist Licht,", "tokens": ["Ich", "wand\u00b7le", "in", "Nacht", ",", "doch", "am", "Zie\u00b7le", "ist", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "ADV", "APPRART", "NN", "VAFIN", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.51": {"text": "Da lodert die Leuchte, die Du mir entz\u00fcndet,", "tokens": ["Da", "lo\u00b7dert", "die", "Leuch\u00b7te", ",", "die", "Du", "mir", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.52": {"text": "Dahin denn, zum Ziel will ich schau\u2019n!", "tokens": ["Da\u00b7hin", "denn", ",", "zum", "Ziel", "will", "ich", "schau'n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "$,", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.53": {"text": "Nicht geh\u00f6rt meine Seele der Erde", "tokens": ["Nicht", "ge\u00b7h\u00f6rt", "mei\u00b7ne", "See\u00b7le", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.54": {"text": "Keine Erdengewalt zerrei\u00dft dieses Band!", "tokens": ["Kei\u00b7ne", "Er\u00b7den\u00b7ge\u00b7walt", "zer\u00b7rei\u00dft", "die\u00b7ses", "Band", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.55": {"text": "Dir, o Herr, geh\u00f6rt meine Seele,", "tokens": ["Dir", ",", "o", "Herr", ",", "ge\u00b7h\u00f6rt", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.56": {"text": "Nichts entrei\u00dft sie aus Deinem Schoo\u00df!", "tokens": ["Nichts", "ent\u00b7rei\u00dft", "sie", "aus", "Dei\u00b7nem", "Schoo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.57": {"text": "Ihr W\u00fcstenl\u00f6wen mit rollendem Schweif,", "tokens": ["Ihr", "W\u00fcs\u00b7ten\u00b7l\u00f6\u00b7wen", "mit", "rol\u00b7len\u00b7dem", "Schweif", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.58": {"text": "Die die Flanken ihr peitscht, die Tatzen spannt,", "tokens": ["Die", "die", "Flan\u00b7ken", "ihr", "peitscht", ",", "die", "Tat\u00b7zen", "spannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.59": {"text": "Ist\u2019s seine Kraft nicht, die in euch tost?", "tokens": ["Ist's", "sei\u00b7ne", "Kraft", "nicht", ",", "die", "in", "euch", "tost", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKNEG", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.60": {"text": "Ihr, deren Rachen wider mich sch\u00e4umt,", "tokens": ["Ihr", ",", "de\u00b7ren", "Ra\u00b7chen", "wi\u00b7der", "mich", "sch\u00e4umt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.61": {"text": "Deren Auge mir gl\u00fcht, deren Stimme mir schallt,", "tokens": ["De\u00b7ren", "Au\u00b7ge", "mir", "gl\u00fcht", ",", "de\u00b7ren", "Stim\u00b7me", "mir", "schallt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "VVFIN", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.62": {"text": "Ist\u2019s nicht sein Donner, der aus euch grollt?", "tokens": ["Ist's", "nicht", "sein", "Don\u00b7ner", ",", "der", "aus", "euch", "grollt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.63": {"text": "Seid ihr nicht Kraft seiner Kraft? Zorn seines Zornes?", "tokens": ["Seid", "ihr", "nicht", "Kraft", "sei\u00b7ner", "Kraft", "?", "Zorn", "sei\u00b7nes", "Zor\u00b7nes", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "PTKNEG", "NN", "PPOSAT", "NN", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.64": {"text": "Beugt euch vor mir, der ich Geist seines Geistes!", "tokens": ["Beugt", "euch", "vor", "mir", ",", "der", "ich", "Geist", "sei\u00b7nes", "Geis\u00b7tes", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "PRELS", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}