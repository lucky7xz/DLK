{"dta.poem.11179": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine liebliche, aber ungestalte sangerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur die stimme, weiter nichts, ist was an dir zierlich ist;", "tokens": ["Nur", "die", "stim\u00b7me", ",", "wei\u00b7ter", "nichts", ",", "ist", "was", "an", "dir", "zier\u00b7lich", "ist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ADV", "PIS", "$,", "VAFIN", "PIS", "APPR", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wer dich h\u00f6rt, der liebet dich; wer dich sieht, der mu\u00df", "tokens": ["Wer", "dich", "h\u00f6rt", ",", "der", "lie\u00b7bet", "dich", ";", "wer", "dich", "sieht", ",", "der", "mu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "$.", "PWS", "PPER", "VVFIN", "$,", "ART", "VMFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "dich hassen:", "tokens": ["dich", "has\u00b7sen", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und wer dich gar wohl betracht, dieser wird die meinung fassen,", "tokens": ["Und", "wer", "dich", "gar", "wohl", "be\u00b7tracht", ",", "die\u00b7ser", "wird", "die", "mei\u00b7nung", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "VVPP", "$,", "PDS", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-++--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Da\u00df du eine nachtigalle, aber nur bey nachte, bist.", "tokens": ["Da\u00df", "du", "ei\u00b7ne", "nach\u00b7ti\u00b7gal\u00b7le", ",", "a\u00b7ber", "nur", "bey", "nach\u00b7te", ",", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "$,", "ADV", "ADV", "APPR", "VVFIN", "$,", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}