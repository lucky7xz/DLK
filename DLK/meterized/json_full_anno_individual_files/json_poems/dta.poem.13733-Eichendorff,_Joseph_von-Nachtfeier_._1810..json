{"dta.poem.13733": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtfeier .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ew'ge W\u00e4chter \u00e4chter Weihe,", "tokens": ["Ew'\u00b7ge", "W\u00e4ch\u00b7ter", "\u00e4ch\u00b7ter", "Wei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als, erhoben vom Verderben", "tokens": ["Als", ",", "er\u00b7ho\u00b7ben", "vom", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "VVPP", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vom Jammer, da die Dreie", "tokens": ["Und", "vom", "Jam\u00b7mer", ",", "da", "die", "Drei\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam traten in das Freie,", "tokens": ["Ein\u00b7sam", "tra\u00b7ten", "in", "das", "Frei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Frei zu leben und zu sterben.", "tokens": ["Frei", "zu", "le\u00b7ben", "und", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}