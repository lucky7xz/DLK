{"dta.poem.19396": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "129.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zur Arbeit ist der Mensch so von Natur bestimmt,", "tokens": ["Zur", "Ar\u00b7beit", "ist", "der", "Mensch", "so", "von", "Na\u00b7tur", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er selbst Arbeit zum Vergn\u00fcgen unternimmt.", "tokens": ["Da\u00df", "er", "selbst", "Ar\u00b7beit", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "un\u00b7ter\u00b7nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was kann am Spiele dich, was an der Jagd dich reizen?", "tokens": ["Was", "kann", "am", "Spie\u00b7le", "dich", ",", "was", "an", "der", "Jagd", "dich", "rei\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPRART", "NN", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach Groschen wirst du nicht und nicht nach Hasen geizen.", "tokens": ["Nach", "Gro\u00b7schen", "wirst", "du", "nicht", "und", "nicht", "nach", "Ha\u00b7sen", "gei\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PTKNEG", "KON", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du n\u00e4hmest sie nicht an, wer sie dir schenken wollte:", "tokens": ["Du", "n\u00e4h\u00b7mest", "sie", "nicht", "an", ",", "wer", "sie", "dir", "schen\u00b7ken", "woll\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "PWS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist der Arbeit Schein, der dich betr\u00fcgen sollte.", "tokens": ["Es", "ist", "der", "Ar\u00b7beit", "Schein", ",", "der", "dich", "be\u00b7tr\u00fc\u00b7gen", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "PRELS", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denn deine Kraft mu\u00df sich stets auf ein Aeu\u00dfres lenken,", "tokens": ["Denn", "dei\u00b7ne", "Kraft", "mu\u00df", "sich", "stets", "auf", "ein", "A\u00b7e\u00b7u\u00df\u00b7res", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und nie beruhigts dich, in Ruh dich zu versenken.", "tokens": ["Und", "nie", "be\u00b7ru\u00b7higts", "dich", ",", "in", "Ruh", "dich", "zu", "ver\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ja selbst die Ruh, die du entathmet sch\u00f6pfst im Nu,", "tokens": ["Ja", "selbst", "die", "Ruh", ",", "die", "du", "en\u00b7tath\u00b7met", "sch\u00f6pfst", "im", "Nu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "VVFIN", "APPRART", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Spornt dich der Th\u00e4tigkeit mit neuem Athem zu.", "tokens": ["Spornt", "dich", "der", "Th\u00e4\u00b7tig\u00b7keit", "mit", "neu\u00b7em", "A\u00b7them", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und willst du auf dir selbst nur ausruhn augenblicklich,", "tokens": ["Und", "willst", "du", "auf", "dir", "selbst", "nur", "aus\u00b7ruhn", "au\u00b7gen\u00b7blick\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PPER", "ADV", "ADV", "VVIZU", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich wirst du selbst dich abarbeiten unerquicklich.", "tokens": ["Gleich", "wirst", "du", "selbst", "dich", "ab\u00b7ar\u00b7bei\u00b7ten", "un\u00b7er\u00b7quick\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wie eine M\u00fchle sich zermalmet und zerr\u00fcttet,", "tokens": ["Wie", "ei\u00b7ne", "M\u00fch\u00b7le", "sich", "zer\u00b7mal\u00b7met", "und", "zer\u00b7r\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn man dem leeren Gang nicht neues Korn aufsch\u00fcttet.", "tokens": ["Wenn", "man", "dem", "lee\u00b7ren", "Gang", "nicht", "neu\u00b7es", "Korn", "auf\u00b7sch\u00fct\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "PTKNEG", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}