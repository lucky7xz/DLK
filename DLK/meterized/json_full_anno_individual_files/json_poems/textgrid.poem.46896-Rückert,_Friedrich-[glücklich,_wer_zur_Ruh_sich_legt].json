{"textgrid.poem.46896": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[gl\u00fccklich, wer zur Ruh sich legt]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich, wer zur Ruh sich legt,", "tokens": ["Gl\u00fcck\u00b7lich", ",", "wer", "zur", "Ruh", "sich", "legt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh sein Herz ein Wunsch bewegt.", "tokens": ["Eh", "sein", "Herz", "ein", "Wunsch", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Welt befriediget", "tokens": ["Denn", "die", "Welt", "be\u00b7frie\u00b7di\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie den Wunsch, den sie erregt.", "tokens": ["Nie", "den", "Wunsch", ",", "den", "sie", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gl\u00fccklich, wer in seiner Brust", "tokens": ["Gl\u00fcck\u00b7lich", ",", "wer", "in", "sei\u00b7ner", "Brust"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Diesen Baum der Lust nicht hegt,", "tokens": ["Die\u00b7sen", "Baum", "der", "Lust", "nicht", "hegt", ","], "token_inf