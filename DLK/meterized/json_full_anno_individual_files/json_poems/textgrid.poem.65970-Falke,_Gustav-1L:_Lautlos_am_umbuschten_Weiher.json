{"textgrid.poem.65970": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lautlos am umbuschten Weiher", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lautlos am umbuschten Weiher", "tokens": ["Laut\u00b7los", "am", "um\u00b7buschten", "Wei\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelt durch das Gras die Nacht,", "tokens": ["Wan\u00b7delt", "durch", "das", "Gras", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter ihr, ein feuchter Schleier,", "tokens": ["Hin\u00b7ter", "ihr", ",", "ein", "feuch\u00b7ter", "Schlei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heben sich die Nebel sacht.", "tokens": ["He\u00b7ben", "sich", "die", "Ne\u00b7bel", "sacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weite, weite stille Strecken", "tokens": ["Wei\u00b7te", ",", "wei\u00b7te", "stil\u00b7le", "Stre\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mag sie wie im Fluge gehn.", "tokens": ["Mag", "sie", "wie", "im", "Flu\u00b7ge", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "