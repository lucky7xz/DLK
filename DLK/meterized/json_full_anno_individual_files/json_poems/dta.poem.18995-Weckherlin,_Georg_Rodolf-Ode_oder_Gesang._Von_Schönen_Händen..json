{"dta.poem.18995": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ode oder Gesang.  \n Von Sch\u00f6nen H\u00e4nden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Dv bist/ O zart schnee-weisse hand/", "tokens": ["Dv", "bist", "/", "O", "zart", "schnee\u00b7weis\u00b7se", "hand", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$(", "NE", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der beste Zeug die lieb zu weben;", "tokens": ["Der", "bes\u00b7te", "Zeug", "die", "lieb", "zu", "we\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist der Trew erw\u00fcnschtes pfand/", "tokens": ["Du", "bist", "der", "Trew", "er\u00b7w\u00fcnschtes", "pfand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Damit man kan vern\u00fcget leben;", "tokens": ["Da\u00b7mit", "man", "kan", "ver\u00b7n\u00fc\u00b7get", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wan auff der Lieb bit oder frag", "tokens": ["Wan", "auff", "der", "Lieb", "bit", "o\u00b7der", "frag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Du wilt zu fridlichem vertrag", "tokens": ["Du", "wilt", "zu", "frid\u00b7li\u00b7chem", "ver\u00b7trag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Stillschweigend deine Zusag geben.", "tokens": ["Still\u00b7schwei\u00b7gend", "dei\u00b7ne", "Zu\u00b7sag", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du kanst/ O hand/ bald den verdru\u00df", "tokens": ["Du", "kanst", "/", "O", "hand", "/", "bald", "den", "ver\u00b7dru\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$(", "NE", "NN", "$(", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald das gefallen verursachen;", "tokens": ["Bald", "das", "ge\u00b7fal\u00b7len", "ver\u00b7ur\u00b7sa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bezeugend zwayer lieb beschlu\u00df", "tokens": ["Be\u00b7zeu\u00b7gend", "zway\u00b7er", "lieb", "be\u00b7schlu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bist du die vrsach da\u00df sie lachen:", "tokens": ["Bist", "du", "die", "vr\u00b7sach", "da\u00df", "sie", "la\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gleichwie wan du der buhler scho\u00df", "tokens": ["Gleich\u00b7wie", "wan", "du", "der", "buh\u00b7ler", "scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Zuruck verstossest hofnung-lo\u00df", "tokens": ["Zu\u00b7ruck", "ver\u00b7stos\u00b7sest", "hof\u00b7nung\u00b7lo\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du sie kanst leichtlich weinen machen.", "tokens": ["Du", "sie", "kanst", "leicht\u00b7lich", "wei\u00b7nen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VMFIN", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schnell/ lauffend/ zittrend/ vnd kunstreich/", "tokens": ["Schnell", "/", "lauf\u00b7fend", "/", "zitt\u00b7rend", "/", "vnd", "kuns\u00b7treich", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$(", "ADJD", "$(", "VVPP", "$(", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kanst du das Instrument ber\u00fchren;", "tokens": ["Kanst", "du", "das", "Inst\u00b7ru\u00b7ment", "be\u00b7r\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd der wolredenheit recht gleich", "tokens": ["Vnd", "der", "wol\u00b7re\u00b7den\u00b7heit", "recht", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "In das geh\u00f6r die hertzen f\u00fchren:", "tokens": ["In", "das", "ge\u00b7h\u00f6r", "die", "hert\u00b7zen", "f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd mit schneeweissem hellen glantz/", "tokens": ["Vnd", "mit", "schnee\u00b7weis\u00b7sem", "hel\u00b7len", "glantz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd deiner fingern leichtem dantz/", "tokens": ["Vnd", "dei\u00b7ner", "fin\u00b7gern", "leich\u00b7tem", "dantz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den thon/ die stim/ die saitten zieren.", "tokens": ["Den", "thon", "/", "die", "stim", "/", "die", "sait\u00b7ten", "zie\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was jmmer vns der Natur gunst", "tokens": ["Was", "jm\u00b7mer", "vns", "der", "Na\u00b7tur", "gunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPER", "ART", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verleyhet/ kanst du bald vergleichen/", "tokens": ["Ver\u00b7ley\u00b7het", "/", "kanst", "du", "bald", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd es mit farbenreicher kunst", "tokens": ["Vnd", "es", "mit", "far\u00b7ben\u00b7rei\u00b7cher", "kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald vber k\u00fcnstlich herau\u00df streichen:", "tokens": ["Bald", "vber", "k\u00fcnst\u00b7lich", "her\u00b7au\u00df", "strei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du kanst mit des verstands gesatz/", "tokens": ["Du", "kanst", "mit", "des", "ver\u00b7stands", "ge\u00b7satz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd der gedancken grossem schatz", "tokens": ["Vnd", "der", "ge\u00b7dan\u00b7cken", "gros\u00b7sem", "schatz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Getrewlich das papier bereichen.", "tokens": ["Ge\u00b7trew\u00b7lich", "das", "pa\u00b7pier", "be\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In meiner hand (O s\u00fcsser lohn!)", "tokens": ["In", "mei\u00b7ner", "hand", "(", "O", "s\u00fcs\u00b7ser", "lohn", "!", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "NE", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wan du/ O sch\u00f6ne hand/ gefangen/", "tokens": ["Wan", "du", "/", "O", "sch\u00f6\u00b7ne", "hand", "/", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "$(", "NE", "ADJA", "NN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gedrucket druckest du mit wohn", "tokens": ["Ge\u00b7dru\u00b7cket", "dru\u00b7ckest", "du", "mit", "wohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd lust mein hertz/ hand vnd verlangen:", "tokens": ["Vnd", "lust", "mein", "hertz", "/", "hand", "vnd", "ver\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$(", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gefangen fanget deine Zucht", "tokens": ["Ge\u00b7fan\u00b7gen", "fan\u00b7get", "dei\u00b7ne", "Zucht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Also/ da\u00df niemahls durch die flucht/", "tokens": ["Al\u00b7so", "/", "da\u00df", "nie\u00b7mahls", "durch", "die", "flucht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ADV", "APPR", "ART", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der dich gefangen/ dir entgangen.", "tokens": ["Der", "dich", "ge\u00b7fan\u00b7gen", "/", "dir", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKVZ", "$(", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gehalten bist du stoltz vnd frey/", "tokens": ["Ge\u00b7hal\u00b7ten", "bist", "du", "stoltz", "vnd", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du vberwindest mich gebunden:", "tokens": ["Du", "vber\u00b7win\u00b7dest", "mich", "ge\u00b7bun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du bist getrew vnd vngetrew", "tokens": ["Du", "bist", "ge\u00b7trew", "vnd", "vn\u00b7ge\u00b7trew"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als triumfierend vberwunden:", "tokens": ["Als", "tri\u00b7um\u00b7fie\u00b7rend", "vber\u00b7wun\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Du pflegest mit vnruh vnd ruh", "tokens": ["Du", "pfle\u00b7gest", "mit", "vn\u00b7ruh", "vnd", "ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Je sanffter vnd je bloser du/", "tokens": ["Je", "sanff\u00b7ter", "vnd", "je", "blo\u00b7ser", "du", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJA", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Je tieffer mein hertz zu verwunden.", "tokens": ["Je", "tief\u00b7fer", "mein", "hertz", "zu", "ver\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "O zartes/ glattes Helfenbein/", "tokens": ["O", "zar\u00b7tes", "/", "glat\u00b7tes", "Hel\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welches die Rosen etwas f\u00e4rben!", "tokens": ["Wel\u00b7ches", "die", "Ro\u00b7sen", "et\u00b7was", "f\u00e4r\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "O von der Morgen r\u00f6htin schein", "tokens": ["O", "von", "der", "Mor\u00b7gen", "r\u00f6h\u00b7tin", "schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gezierter schnee/ schwer zu erwerben!", "tokens": ["Ge\u00b7zier\u00b7ter", "schnee", "/", "schwer", "zu", "er\u00b7wer\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auff deiner lieblichkeit bericht", "tokens": ["Auff", "dei\u00b7ner", "lieb\u00b7lich\u00b7keit", "be\u00b7richt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gib Ich dir hiemit meine pflicht", "tokens": ["Gib", "Ich", "dir", "hie\u00b7mit", "mei\u00b7ne", "pflicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "PAV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bey dir zu leben vnd zu sterben.", "tokens": ["Bey", "dir", "zu", "le\u00b7ben", "vnd", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}