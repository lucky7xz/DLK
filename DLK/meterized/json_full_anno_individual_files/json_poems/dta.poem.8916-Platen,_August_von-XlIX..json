{"dta.poem.8916": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XlIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Denn falscher ist sie, als es Worte malen:", "tokens": ["Denn", "fal\u00b7scher", "ist", "sie", ",", "als", "es", "Wor\u00b7te", "ma\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie sammelt grausam unsern Schmerz in Schalen,", "tokens": ["Sie", "sam\u00b7melt", "grau\u00b7sam", "un\u00b7sern", "Schmerz", "in", "Scha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und reicht zum Trunk sie, wenn wir halb verschmachtet.", "tokens": ["Und", "reicht", "zum", "Trunk", "sie", ",", "wenn", "wir", "halb", "ver\u00b7schmach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir, den als Werkzeug immer sie betrachtet,", "tokens": ["Mir", ",", "den", "als", "Werk\u00b7zeug", "im\u00b7mer", "sie", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "KOUS", "NN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir pre\u00dft Gesang sie aus mit tausend Qualen,", "tokens": ["Mir", "pre\u00dft", "Ge\u00b7sang", "sie", "aus", "mit", "tau\u00b7send", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPER", "APPR", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4\u00dft ihn vielleicht durch ferne Zeiten stralen,", "tokens": ["L\u00e4\u00dft", "ihn", "viel\u00b7leicht", "durch", "fer\u00b7ne", "Zei\u00b7ten", "stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ich aber werd' als Opferthier geschlachtet.", "tokens": ["Ich", "a\u00b7ber", "werd'", "als", "Op\u00b7fer\u00b7thier", "ge\u00b7schlach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O ihr, die ihr beneidetet mein Leben,", "tokens": ["O", "ihr", ",", "die", "ihr", "be\u00b7nei\u00b7de\u00b7tet", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "$,", "PRELS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und meinen gl\u00fccklichen Beruf erhobet,", "tokens": ["Und", "mei\u00b7nen", "gl\u00fcck\u00b7li\u00b7chen", "Be\u00b7ruf", "er\u00b7ho\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie k\u00f6nnt in Irrthum ihr so lange schweben?", "tokens": ["Wie", "k\u00f6nnt", "in", "Irr\u00b7thum", "ihr", "so", "lan\u00b7ge", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "NN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "H\u00e4tt' ich nicht jedes Gift der Welt erprobet,", "tokens": ["H\u00e4tt'", "ich", "nicht", "je\u00b7des", "Gift", "der", "Welt", "er\u00b7pro\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie h\u00e4tt' ich ganz dem Himmel mich ergeben,", "tokens": ["Nie", "h\u00e4tt'", "ich", "ganz", "dem", "Him\u00b7mel", "mich", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nie vollendet was ihr liebt und lobet.", "tokens": ["Und", "nie", "voll\u00b7en\u00b7det", "was", "ihr", "liebt", "und", "lo\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}