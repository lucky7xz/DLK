{"textgrid.poem.59242": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es will die Zeit mit ihrem Schutt verdecken", "tokens": ["Es", "will", "die", "Zeit", "mit", "ih\u00b7rem", "Schutt", "ver\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den hellen Quell, der meiner Brust entsprungen,", "tokens": ["Den", "hel\u00b7len", "Quell", ",", "der", "mei\u00b7ner", "Brust", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umsonst Gebete himmelan geschwungen,", "tokens": ["Um\u00b7sonst", "Ge\u00b7be\u00b7te", "him\u00b7me\u00b7lan", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie m\u00f6gen nicht das Ohr der Gnade wecken.", "tokens": ["Sie", "m\u00f6\u00b7gen", "nicht", "das", "Ohr", "der", "Gna\u00b7de", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So la\u00df die Nacht die grausen Fl\u00fcgel strecken,", "tokens": ["So", "la\u00df", "die", "Nacht", "die", "grau\u00b7sen", "Fl\u00fc\u00b7gel", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur immerzu, mein tapfres Schiff gedrungen!", "tokens": ["Nur", "im\u00b7mer\u00b7zu", ",", "mein", "tapf\u00b7res", "Schiff", "ge\u00b7drun\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer einmal mit den Wogen hat gerungen,", "tokens": ["Wer", "ein\u00b7mal", "mit", "den", "Wo\u00b7gen", "hat", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fchlt sich das Herz gehoben in den Schrecken.", "tokens": ["F\u00fchlt", "sich", "das", "Herz", "ge\u00b7ho\u00b7ben", "in", "den", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schie\u00dft zu, trefft, Pfeile, die durchs Dunkel schwirren!", "tokens": ["Schie\u00dft", "zu", ",", "trefft", ",", "Pfei\u00b7le", ",", "die", "durchs", "Dun\u00b7kel", "schwir\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "$,", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ruhvoll um Klippen \u00fcberm t\u00fcck'schen Grunde", "tokens": ["Ruh\u00b7voll", "um", "Klip\u00b7pen", "\u00fc\u00b7berm", "t\u00fc\u00b7ck'\u00b7schen", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Lenk ich mein Schiff, wohin die Sterne winken.", "tokens": ["Lenk", "ich", "mein", "Schiff", ",", "wo\u00b7hin", "die", "Ster\u00b7ne", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mag dann der Steuermann nach langem Irren,", "tokens": ["Mag", "dann", "der", "Steu\u00b7er\u00b7mann", "nach", "lan\u00b7gem", "Ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rasch ziehend alle Pfeile aus der Wunde,", "tokens": ["Rasch", "zie\u00b7hend", "al\u00b7le", "Pfei\u00b7le", "aus", "der", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tot an der Heimatk\u00fcste niedersinken!", "tokens": ["Tot", "an", "der", "Hei\u00b7mat\u00b7k\u00fcs\u00b7te", "nie\u00b7der\u00b7sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es will die Zeit mit ihrem Schutt verdecken", "tokens": ["Es", "will", "die", "Zeit", "mit", "ih\u00b7rem", "Schutt", "ver\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSA