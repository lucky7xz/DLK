,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich wag mich nicht hinauf.", "tokens": ["Ich", "wag", "mich", "nicht", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wollt Gott, ich f\u00e4nd im Garten", "tokens": ["Wollt", "Gott", ",", "ich", "f\u00e4nd", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drey Rosen auf einem Zweig,", "tokens": ["Drey", "Ro\u00b7sen", "auf", "ei\u00b7nem", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wollte auf sie warten,", "tokens": ["Ich", "woll\u00b7te", "auf", "sie", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Zeichen w\u00e4r's mir gleich;", "tokens": ["Ein", "Zei\u00b7chen", "w\u00e4r's", "mir", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Morgenroth ist weit,", "tokens": ["Das", "Mor\u00b7gen\u00b7roth", "ist", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es streut schon seine Rosen,", "tokens": ["Es", "streut", "schon", "sei\u00b7ne", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Adie meine sch\u00f6ne Maid.", "tokens": ["A\u00b7die", "mei\u00b7ne", "sch\u00f6\u00b7ne", "Maid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}