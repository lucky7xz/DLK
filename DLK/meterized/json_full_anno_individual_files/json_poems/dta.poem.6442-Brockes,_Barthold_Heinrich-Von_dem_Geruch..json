{"dta.poem.6442": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nun aus dem, woraus ein s\u00fcsses Riechen dringet,", "tokens": ["Wenn", "nun", "aus", "dem", ",", "wo\u00b7raus", "ein", "s\u00fcs\u00b7ses", "Rie\u00b7chen", "drin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein lieblicher Geschmack uns mehrentheils entspringet:", "tokens": ["Ein", "lieb\u00b7li\u00b7cher", "Ge\u00b7schmack", "uns", "meh\u00b7ren\u00b7theils", "ent\u00b7sprin\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So stammet solches blos aus GOTTES Ordnung her,", "tokens": ["So", "stam\u00b7met", "sol\u00b7ches", "blos", "aus", "GoT\u00b7TES", "Ord\u00b7nung", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "NE", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der durch dies z\u00e4rtliche Gef\u00fchl es so gefuget,", "tokens": ["Der", "durch", "dies", "z\u00e4rt\u00b7li\u00b7che", "Ge\u00b7f\u00fchl", "es", "so", "ge\u00b7fu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDS", "ADJA", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df jedes Thier dadurch geschickt und f\u00e4hig w\u00e4r,", "tokens": ["Da\u00df", "je\u00b7des", "Thier", "da\u00b7durch", "ge\u00b7schickt", "und", "f\u00e4\u00b7hig", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PAV", "VVPP", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In seiner Kost, die ihn so n\u00e4hret, als vergn\u00fcget,", "tokens": ["In", "sei\u00b7ner", "Kost", ",", "die", "ihn", "so", "n\u00e4h\u00b7ret", ",", "als", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "KOUS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Unterscheid, und was ihm dienlich, zu ergrunden,", "tokens": ["Den", "Un\u00b7ter\u00b7scheid", ",", "und", "was", "ihm", "dien\u00b7lich", ",", "zu", "er\u00b7grun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PWS", "PPER", "ADJD", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auch solche leichter noch zu finden.", "tokens": ["Auch", "sol\u00b7che", "leich\u00b7ter", "noch", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}