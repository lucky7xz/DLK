{"dta.poem.7627": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sch\u00f6pfung Mittelpunkt wenn diese Erde w\u00e4re,", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "Mit\u00b7tel\u00b7punkt", "wenn", "die\u00b7se", "Er\u00b7de", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "PDAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht nebenaus am Rand der Sf\u00e4ren eine Sf\u00e4re,", "tokens": ["Nicht", "ne\u00b7be\u00b7naus", "am", "Rand", "der", "Sf\u00e4\u00b7ren", "ei\u00b7ne", "Sf\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "So h\u00e4tte Menschenwitz ein Recht, beklommener", "tokens": ["So", "h\u00e4t\u00b7te", "Men\u00b7schen\u00b7witz", "ein", "Recht", ",", "be\u00b7klom\u00b7me\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "NN", "ART", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Zu fragen, warum sie nicht sei vollkommener.", "tokens": ["Zu", "fra\u00b7gen", ",", "wa\u00b7rum", "sie", "nicht", "sei", "voll\u00b7kom\u00b7me\u00b7ner", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "PPER", "PTKNEG", "VAFIN", "ADJD", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "So aber hat er nur Ursache, sie zu fragen,", "tokens": ["So", "a\u00b7ber", "hat", "er", "nur", "Ur\u00b7sa\u00b7che", ",", "sie", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wieviel der Winkel kann von heller Mitte sagen.", "tokens": ["Wie\u00b7viel", "der", "Win\u00b7kel", "kann", "von", "hel\u00b7ler", "Mit\u00b7te", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In Mitten steht ein Licht hoch auf dem Tisch im Zimmer,", "tokens": ["In", "Mit\u00b7ten", "steht", "ein", "Licht", "hoch", "auf", "dem", "Tisch", "im", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fcllt den ganzen Raum, doch mit ungleichem Schimmer.", "tokens": ["Und", "f\u00fcllt", "den", "gan\u00b7zen", "Raum", ",", "doch", "mit", "un\u00b7glei\u00b7chem", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein Spiegel wirft den Glanz dem andern Spiegel zu,", "tokens": ["Ein", "Spie\u00b7gel", "wirft", "den", "Glanz", "dem", "an\u00b7dern", "Spie\u00b7gel", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wieder andern, und vorm letzten stehest du.", "tokens": ["Der", "wie\u00b7der", "an\u00b7dern", ",", "und", "vorm", "letz\u00b7ten", "ste\u00b7hest", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "$,", "KON", "APPRART", "ADJA", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du siehst ged\u00e4mpft genug das Licht, da\u00df dichs nicht blende,", "tokens": ["Du", "siehst", "ge\u00b7d\u00e4mpft", "ge\u00b7nug", "das", "Licht", ",", "da\u00df", "dichs", "nicht", "blen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "ADV", "ART", "NN", "$,", "KOUS", "PIS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hell genug, da\u00df dich zum Lichtquell Sehnsucht wende.", "tokens": ["Und", "hell", "ge\u00b7nug", ",", "da\u00df", "dich", "zum", "Licht\u00b7quell", "Sehn\u00b7sucht", "wen\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Im Winkel warte nur geduldig, bis die Augen", "tokens": ["Im", "Win\u00b7kel", "war\u00b7te", "nur", "ge\u00b7dul\u00b7dig", ",", "bis", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir, einzutreten in den Glanz der Mitte, taugen.", "tokens": ["Dir", ",", "ein\u00b7zu\u00b7tre\u00b7ten", "in", "den", "Glanz", "der", "Mit\u00b7te", ",", "tau\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wie schonend Mondlicht sanft um Eulenbl\u00f6dheit flie\u00dft,", "tokens": ["Wie", "scho\u00b7nend", "Mond\u00b7licht", "sanft", "um", "Eu\u00b7len\u00b7bl\u00f6d\u00b7heit", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis sich ein Adlerblick der Sonne k\u00fchn erschlie\u00dft.", "tokens": ["Bis", "sich", "ein", "Ad\u00b7ler\u00b7blick", "der", "Son\u00b7ne", "k\u00fchn", "er\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ein blasser Mond, o Erd', ist deine Mittagssonne,", "tokens": ["Ein", "blas\u00b7ser", "Mond", ",", "o", "Erd'", ",", "ist", "dei\u00b7ne", "Mit\u00b7tags\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "FM", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die nur mit Sehnsucht f\u00fcllt, nicht selbst ist volle Wonne.", "tokens": ["Die", "nur", "mit", "Sehn\u00b7sucht", "f\u00fcllt", ",", "nicht", "selbst", "ist", "vol\u00b7le", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$,", "PTKNEG", "ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Die Sonn' im Wolkenflor webt einen Regenbogen;", "tokens": ["Die", "Sonn'", "im", "Wol\u00b7ken\u00b7flor", "webt", "ei\u00b7nen", "Re\u00b7gen\u00b7bo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie rein ist der Akkord des Farbenspiels gezogen!", "tokens": ["Wie", "rein", "ist", "der", "Ak\u00b7kord", "des", "Far\u00b7ben\u00b7spiels", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Der Bogen aber spielt in einem zweiten dann,", "tokens": ["Der", "Bo\u00b7gen", "a\u00b7ber", "spielt", "in", "ei\u00b7nem", "zwei\u00b7ten", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worin der bunte Schmelz in mattes Grau zerrann.", "tokens": ["Wo\u00b7rin", "der", "bun\u00b7te", "Schmelz", "in", "mat\u00b7tes", "Grau", "zer\u00b7rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Der Regenbogen nicht, vom Regenbogen nur", "tokens": ["Der", "Re\u00b7gen\u00b7bo\u00b7gen", "nicht", ",", "vom", "Re\u00b7gen\u00b7bo\u00b7gen", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "$,", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist du der Nebenglanz, die halberloschne Spur.", "tokens": ["Bist", "du", "der", "Ne\u00b7ben\u00b7glanz", ",", "die", "hal\u00b7ber\u00b7lo\u00b7schne", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "O Mensch, in des Gem\u00fcth sich Lieb' und Hochmuth gatten,", "tokens": ["O", "Mensch", ",", "in", "des", "Ge\u00b7m\u00fcth", "sich", "Lieb'", "und", "Hoch\u00b7muth", "gat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ART", "NN", "PRF", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist zwar Licht vom Licht, doch Schatten nur vom Schatten.", "tokens": ["Du", "bist", "zwar", "Licht", "vom", "Licht", ",", "doch", "Schat\u00b7ten", "nur", "vom", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPRART", "NN", "$,", "KON", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}