{"textgrid.poem.60696": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine Kutsche mit schnaufendem Sechsgespann", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Kutsche mit schnaufendem Sechsgespann", "tokens": ["Ei\u00b7ne", "Kut\u00b7sche", "mit", "schnau\u00b7fen\u00b7dem", "Sechs\u00b7ge\u00b7spann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Fuhr einst eine starke Steigung hinan", "tokens": ["Fuhr", "einst", "ei\u00b7ne", "star\u00b7ke", "Stei\u00b7gung", "hi\u00b7nan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Auf schlechtem Weg in Sonnenglut.", "tokens": ["Auf", "schlech\u00b7tem", "Weg", "in", "Son\u00b7nen\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie stockte. Frauen, M\u00f6nche, Greise stiegen ab.", "tokens": ["Sie", "stock\u00b7te", ".", "Frau\u00b7en", ",", "M\u00f6n\u00b7che", ",", "Grei\u00b7se", "stie\u00b7gen", "ab", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$,", "NN", "$,", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da kommt eine Fliege her und tut,", "tokens": ["Da", "kommt", "ei\u00b7ne", "Flie\u00b7ge", "her", "und", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Als mache ihr Gesumm den Pferden frischen Mut", "tokens": ["Als", "ma\u00b7che", "ihr", "Ge\u00b7summ", "den", "Pfer\u00b7den", "fri\u00b7schen", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und sporne sie zu besserem Trab.", "tokens": ["Und", "spor\u00b7ne", "sie", "zu", "bes\u00b7se\u00b7rem", "Trab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Sie sticht das eine, sticht das andre, ist der Meinung,", "tokens": ["Sie", "sticht", "das", "ei\u00b7ne", ",", "sticht", "das", "and\u00b7re", ",", "ist", "der", "Mei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ART", "$,", "VVFIN", "ART", "ADJA", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df ihre wichtige Erscheinung", "tokens": ["Da\u00df", "ih\u00b7re", "wich\u00b7ti\u00b7ge", "Er\u00b7schei\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Allein die Sache vorw\u00e4rts bringe;", "tokens": ["Al\u00b7lein", "die", "Sa\u00b7che", "vor\u00b7w\u00e4rts", "brin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Setzt auf die Deichsel, auf des Kutschers Nase sich,", "tokens": ["Setzt", "auf", "die", "Deich\u00b7sel", ",", "auf", "des", "Kut\u00b7schers", "Na\u00b7se", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als ob die Reise ohne sie nicht weiterginge.", "tokens": ["Als", "ob", "die", "Rei\u00b7se", "oh\u00b7ne", "sie", "nicht", "wei\u00b7ter\u00b7gin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPR", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So hilft sie hier, hilft dort, ist h\u00f6chst gesch\u00e4ftiglich:", "tokens": ["So", "hilft", "sie", "hier", ",", "hilft", "dort", ",", "ist", "h\u00f6chst", "ge\u00b7sch\u00e4f\u00b7tig\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADV", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein Adjutant, der \u00fcberall Befehl erteilt,", "tokens": ["Ein", "Ad\u00b7ju\u00b7tant", ",", "der", "\u00fc\u00b7be\u00b7rall", "Be\u00b7fehl", "er\u00b7teilt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Damit die Truppe ihren Sieg beeilt;", "tokens": ["Da\u00b7mit", "die", "Trup\u00b7pe", "ih\u00b7ren", "Sieg", "be\u00b7eilt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Beklagt sich, da\u00df sie niemand unterst\u00fctze,", "tokens": ["Be\u00b7klagt", "sich", ",", "da\u00df", "sie", "nie\u00b7mand", "un\u00b7ter\u00b7st\u00fct\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Da\u00df ganz allein auf ihr die ganze Sache sitze,", "tokens": ["Da\u00df", "ganz", "al\u00b7lein", "auf", "ihr", "die", "gan\u00b7ze", "Sa\u00b7che", "sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df einzig sie sich mit den Pferden qu\u00e4le!", "tokens": ["Da\u00df", "ein\u00b7zig", "sie", "sich", "mit", "den", "Pfer\u00b7den", "qu\u00e4\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der M\u00f6nch liest sein Brevier \u2013", "tokens": ["Der", "M\u00f6nch", "liest", "sein", "Bre\u00b7vier", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Da\u00df der sich be\u00dfre Stunden daf\u00fcr w\u00e4hle!", "tokens": ["Da\u00df", "der", "sich", "be\u00df\u00b7re", "Stun\u00b7den", "da\u00b7f\u00fcr", "w\u00e4h\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PRF", "ADJA", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die Frauen machen gar Gesang \u2013", "tokens": ["Die", "Frau\u00b7en", "ma\u00b7chen", "gar", "Ge\u00b7sang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Gewi\u00df war gar nichts wichtiger hier!", "tokens": ["Ge\u00b7wi\u00df", "war", "gar", "nichts", "wich\u00b7ti\u00b7ger", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "ADJD", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Frau Fliege summt die ganze Reih entlang,", "tokens": ["Frau", "Flie\u00b7ge", "summt", "die", "gan\u00b7ze", "Reih", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Mu\u00df jedem ihre Meinung in die Ohren sagen.", "tokens": ["Mu\u00df", "je\u00b7dem", "ih\u00b7re", "Mei\u00b7nung", "in", "die", "Oh\u00b7ren", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nach harter M\u00fch ist oben unser Wagen.", "tokens": ["Nach", "har\u00b7ter", "M\u00fch", "ist", "o\u00b7ben", "un\u00b7ser", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "\u00bbverschnaufen wir!\u00ab sagt nun das Fliegentier;", "tokens": ["\u00bb", "ver\u00b7schnau\u00b7fen", "wir", "!", "\u00ab", "sagt", "nun", "das", "Flie\u00b7gen\u00b7tier", ";"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$.", "$(", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "\u00bbmein Gott, ich plagte mich bis zum Erschlaffen,", "tokens": ["\u00bb", "mein", "Gott", ",", "ich", "plag\u00b7te", "mich", "bis", "zum", "Er\u00b7schlaf\u00b7fen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.28": {"text": "Um unsre Leute hier heraufzuschaffen.", "tokens": ["Um", "uns\u00b7re", "Leu\u00b7te", "hier", "her\u00b7auf\u00b7zu\u00b7schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Also, ihr Herren Pferde, was bezahlt ihr mir?\u00ab", "tokens": ["Al\u00b7so", ",", "ihr", "Her\u00b7ren", "Pfer\u00b7de", ",", "was", "be\u00b7zahlt", "ihr", "mir", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "NN", "$,", "PWS", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich kenne \u00c4hnliches von vielen,", "tokens": ["Ich", "ken\u00b7ne", "\u00c4hn\u00b7li\u00b7ches", "von", "vie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Die immer die Gesch\u00e4ftigen spielen.", "tokens": ["Die", "im\u00b7mer", "die", "Ge\u00b7sch\u00e4f\u00b7ti\u00b7gen", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Sie mischen sich in alle Dinge,", "tokens": ["Sie", "mi\u00b7schen", "sich", "in", "al\u00b7le", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Als ob es ohne sie nicht ginge \u2013", "tokens": ["Als", "ob", "es", "oh\u00b7ne", "sie", "nicht", "gin\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Und sind nur ungelegen \u00fcberall.", "tokens": ["Und", "sind", "nur", "un\u00b7ge\u00b7le\u00b7gen", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Schmei\u00dft sie hinaus mit Knall und Fall!", "tokens": ["Schmei\u00dft", "sie", "hin\u00b7aus", "mit", "Knall", "und", "Fall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APZR", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine Kutsche mit schnaufendem Sechsgespann", "tokens": ["Ei\u00b7ne", "Kut\u00b7sche", "mit", "schnau\u00b7fen\u00b7dem", "Sechs\u00b7ge\u00b7spann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Fuhr einst eine starke Steigung hinan", "tokens": ["Fuhr", "einst", "ei\u00b7ne", "star\u00b7ke", "Stei\u00b7gung", "hi\u00