], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df einzig sie sich mit den Pferden qu\u00e4le!", "tokens": ["Da\u00df", "ein\u00b7zig", "sie", "sich", "mit", "den", "Pfer\u00b7den", "qu\u00e4\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der M\u00f6nch liest sein Brevier \u2013", "tokens": ["Der", "M\u00f6nch", "liest", "sein", "Bre\u00b7vier", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Da\u00df der sich be\u00dfre Stunden daf\u00fcr w\u00e4hle!", "tokens": ["Da\u00df", "der", "sich", "be\u00df\u00b7re", "Stun\u00b7den", "da\u00b7f\u00fcr", "w\u00e4h\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PRF", "ADJA", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die Frauen machen gar Gesang \u2013", "tokens": ["Die", "Frau\u00b7en", "ma\u00b7chen", "gar", "Ge\u00b7sang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Gewi\u00df war gar nichts wichtiger hier!", "tokens": ["Ge\u00b7wi\u00df", "war", "gar", "nichts", "wich\u00b7ti\u00b7ger", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "ADJD", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Frau Fliege summt die ganze Reih entlang,", "tokens": ["Frau", "Flie\u00b7ge", "summt", "die", "gan\u00b7ze", "Reih", "ent\u00b7lang", ","], "tok