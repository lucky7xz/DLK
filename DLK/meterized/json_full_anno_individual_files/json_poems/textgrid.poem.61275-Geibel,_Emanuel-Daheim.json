{"textgrid.poem.61275": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Daheim", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Daheim, daheim! Nach so viel Wandertagen,", "tokens": ["Da\u00b7heim", ",", "da\u00b7heim", "!", "Nach", "so", "viel", "Wan\u00b7der\u00b7ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADV", "$.", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach so viel N\u00e4chten, wo ich sturmverschlagen", "tokens": ["Nach", "so", "viel", "N\u00e4ch\u00b7ten", ",", "wo", "ich", "sturm\u00b7ver\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "$,", "PWAV", "PPER", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Schlaflos im Schiff ersonnen meinen Reim,", "tokens": ["Schlaf\u00b7los", "im", "Schiff", "er\u00b7son\u00b7nen", "mei\u00b7nen", "Reim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach Frost und Glut auf \u00f6den Felsenstiegen,", "tokens": ["Nach", "Frost", "und", "Glut", "auf", "\u00f6\u00b7den", "Fel\u00b7sen\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nach ew'ger Hast - o welche Zauber liegen", "tokens": ["Nach", "ew'\u00b7ger", "Hast", "o", "wel\u00b7che", "Zau\u00b7ber", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "FM", "PWAT", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "In diesem kleinen Wort: Daheim!", "tokens": ["In", "die\u00b7sem", "klei\u00b7nen", "Wort", ":", "Da\u00b7heim", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun knattert im Kamin mit raschem Schimmer", "tokens": ["Nun", "knat\u00b7tert", "im", "Ka\u00b7min", "mit", "ra\u00b7schem", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Flamme schon; mein holzget\u00e4felt Zimmer", "tokens": ["Die", "Flam\u00b7me", "schon", ";", "mein", "holz\u00b7ge\u00b7t\u00e4\u00b7felt", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "PPOSAT", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erd\u00e4mmert rosig. M\u00fc\u00dfig schau ich zu.", "tokens": ["Er\u00b7d\u00e4m\u00b7mert", "ro\u00b7sig", ".", "M\u00fc\u00b7\u00dfig", "schau", "ich", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Armstuhl hier mit den gewundnen F\u00fc\u00dfen,", "tokens": ["Der", "Arm\u00b7stuhl", "hier", "mit", "den", "ge\u00b7wund\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die alten Bilder - alles will mich gr\u00fc\u00dfen", "tokens": ["Die", "al\u00b7ten", "Bil\u00b7der", "al\u00b7les", "will", "mich", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "PIS", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit einem Hauche tiefer Ruh';", "tokens": ["Mit", "ei\u00b7nem", "Hau\u00b7che", "tie\u00b7fer", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die B\u00fccher dort, die mir mit goldner Kunde", "tokens": ["Die", "B\u00fc\u00b7cher", "dort", ",", "die", "mir", "mit", "gold\u00b7ner", "Kun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinwegget\u00e4uscht so manche schwere Stunde,", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7t\u00e4uscht", "so", "man\u00b7che", "schwe\u00b7re", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Hausrat, den die Mutter noch gew\u00e4hlt,", "tokens": ["Der", "Haus\u00b7rat", ",", "den", "die", "Mut\u00b7ter", "noch", "ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Wanduhr selbst, die mit verha\u00dftem Schlage", "tokens": ["Die", "Wan\u00b7duhr", "selbst", ",", "die", "mit", "ver\u00b7ha\u00df\u00b7tem", "Schla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mich oft ins Bett trieb, wenn die sch\u00f6nste Sage", "tokens": ["Mich", "oft", "ins", "Bett", "trieb", ",", "wenn", "die", "sch\u00f6ns\u00b7te", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die blonde Schwester mir erz\u00e4hlt;", "tokens": ["Die", "blon\u00b7de", "Schwes\u00b7ter", "mir", "er\u00b7z\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und hier das Fenster! Ja, das sind die Stra\u00dfen,", "tokens": ["Und", "hier", "das", "Fens\u00b7ter", "!", "Ja", ",", "das", "sind", "die", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$.", "PTKANT", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo wir einst spielten, wo wir abends sa\u00dfen", "tokens": ["Wo", "wir", "einst", "spiel\u00b7ten", ",", "wo", "wir", "a\u00b7bends", "sa\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zur Sommerszeit, vom Lindenduft umwebt;", "tokens": ["Zur", "Som\u00b7mers\u00b7zeit", ",", "vom", "Lin\u00b7den\u00b7duft", "um\u00b7webt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dort stehn die T\u00fcrme, dort aus Stein gebacken", "tokens": ["Dort", "stehn", "die", "T\u00fcr\u00b7me", ",", "dort", "aus", "Stein", "ge\u00b7ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die schwarzen Giebel, hinter deren Zacken", "tokens": ["Die", "schwar\u00b7zen", "Gie\u00b7bel", ",", "hin\u00b7ter", "de\u00b7ren", "Za\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Mond die Silberscheibe hebt.", "tokens": ["Der", "Mond", "die", "Sil\u00b7ber\u00b7schei\u00b7be", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und durch die D\u00e4mmrung flatternd das vertraute", "tokens": ["Und", "durch", "die", "D\u00e4mm\u00b7rung", "flat\u00b7ternd", "das", "ver\u00b7trau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschw\u00e4tz der M\u00e4dchen, die bekannten Laute,", "tokens": ["Ge\u00b7schw\u00e4tz", "der", "M\u00e4d\u00b7chen", ",", "die", "be\u00b7kann\u00b7ten", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach denen sich so oft mein Herz gesehnt,", "tokens": ["Nach", "de\u00b7nen", "sich", "so", "oft", "mein", "Herz", "ge\u00b7sehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ich, indes der Beifall st\u00fcrmisch rauschte,", "tokens": ["Wenn", "ich", ",", "in\u00b7des", "der", "Bei\u00b7fall", "st\u00fcr\u00b7misch", "rauschte", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit halbem Ohr der fremden Weise lauschte,", "tokens": ["Mit", "hal\u00b7bem", "Ohr", "der", "frem\u00b7den", "Wei\u00b7se", "lauschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In einer Loge Samt gelehnt.", "tokens": ["In", "ei\u00b7ner", "Lo\u00b7ge", "Samt", "ge\u00b7lehnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, alles, alles - hell ins Auge schie\u00dfen", "tokens": ["Ach", ",", "al\u00b7les", ",", "al\u00b7les", "hell", "ins", "Au\u00b7ge", "schie\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PIS", "$,", "PIS", "$(", "ADJD", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Tr\u00e4nen mir; sei's drum, sie m\u00f6gen flie\u00dfen!", "tokens": ["Die", "Tr\u00e4\u00b7nen", "mir", ";", "sei's", "drum", ",", "sie", "m\u00f6\u00b7gen", "flie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$.", "VAFIN", "PAV", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Was l\u00e4chelt ihr? - La\u00dft mich, ich bin ein Kind.", "tokens": ["Was", "l\u00e4\u00b7chelt", "ihr", "?", "La\u00dft", "mich", ",", "ich", "bin", "ein", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "$(", "VVIMP", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr aber, nie entflohn aus eurem Ringe,", "tokens": ["Ihr", "a\u00b7ber", ",", "nie", "ent\u00b7flohn", "aus", "eu\u00b7rem", "Rin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr wi\u00dft es nicht, wie lieblich diese Dinge", "tokens": ["Ihr", "wi\u00dft", "es", "nicht", ",", "wie", "lieb\u00b7lich", "die\u00b7se", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ADJD", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nach jahrelangen Fahrten sind.", "tokens": ["Nach", "jah\u00b7re\u00b7lan\u00b7gen", "Fahr\u00b7ten", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr wi\u00dft auch nicht, wie selbst am Starren, Toten", "tokens": ["Ihr", "wi\u00dft", "auch", "nicht", ",", "wie", "selbst", "am", "Star\u00b7ren", ",", "To\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "ADV", "APPRART", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Geiste, der dar\u00fcber einst geboten,", "tokens": ["Vom", "Geis\u00b7te", ",", "der", "da\u00b7r\u00fc\u00b7ber", "einst", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PAV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Schimmer h\u00e4ngen bleibt, ein irres Licht;", "tokens": ["Ein", "Schim\u00b7mer", "h\u00e4n\u00b7gen", "bleibt", ",", "ein", "ir\u00b7res", "Licht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wi\u00dft nicht, wie in Ger\u00e4ten, H\u00e4usern, B\u00e4umen", "tokens": ["Wi\u00dft", "nicht", ",", "wie", "in", "Ge\u00b7r\u00e4\u00b7ten", ",", "H\u00e4u\u00b7sern", ",", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wohnt eine Stimme, die gleichwie aus Tr\u00e4umen", "tokens": ["Wohnt", "ei\u00b7ne", "Stim\u00b7me", ",", "die", "gleich\u00b7wie", "aus", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "KON", "APPR", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Der eignen Jugend zu euch spricht;", "tokens": ["Der", "eig\u00b7nen", "Ju\u00b7gend", "zu", "euch", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Noch wi\u00dft ihr, da\u00df am Born in Waldesmitten,", "tokens": ["Noch", "wi\u00dft", "ihr", ",", "da\u00df", "am", "Born", "in", "Wal\u00b7des\u00b7mit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo ihr mit eurem M\u00e4dchen sonst geschritten,", "tokens": ["Wo", "ihr", "mit", "eu\u00b7rem", "M\u00e4d\u00b7chen", "sonst", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Eichbaum, drein ihr eure Namen schriebt,", "tokens": ["Am", "Eich\u00b7baum", ",", "drein", "ihr", "eu\u00b7re", "Na\u00b7men", "schriebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Euch noch nach Jahren, einsam hingetrauert,", "tokens": ["Euch", "noch", "nach", "Jah\u00b7ren", ",", "ein\u00b7sam", "hin\u00b7ge\u00b7trau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie Rosenduft ein leiser Hauch umschauert", "tokens": ["Wie", "Ro\u00b7sen\u00b7duft", "ein", "lei\u00b7ser", "Hauch", "um\u00b7schau\u00b7ert"], "token_info":