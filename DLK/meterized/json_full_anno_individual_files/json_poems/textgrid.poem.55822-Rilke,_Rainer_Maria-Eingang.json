{"textgrid.poem.55822": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Eingang", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer du auch seist: am Abend tritt hinaus", "tokens": ["Wer", "du", "auch", "seist", ":", "am", "A\u00b7bend", "tritt", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$.", "APPRART", "NN", "VVFIN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "aus deiner Stube, drin du alles wei\u00dft;", "tokens": ["aus", "dei\u00b7ner", "Stu\u00b7be", ",", "drin", "du", "al\u00b7les", "wei\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "als letztes vor der Ferne liegt dein Haus:", "tokens": ["als", "letz\u00b7tes", "vor", "der", "Fer\u00b7ne", "liegt", "dein", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wer du auch seist.", "tokens": ["wer", "du", "auch", "seist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit deinen Augen, welche m\u00fcde kaum", "tokens": ["Mit", "dei\u00b7nen", "Au\u00b7gen", ",", "wel\u00b7che", "m\u00fc\u00b7de", "kaum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "von der verbrauchten Schwelle sich befrein,", "tokens": ["von", "der", "ver\u00b7brauch\u00b7ten", "Schwel\u00b7le", "sich", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "hebst du ganz langsam einen schwarzen Baum", "tokens": ["hebst", "du", "ganz", "lang\u00b7sam", "ei\u00b7nen", "schwar\u00b7zen", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und stellst ihn vor den Himmel: schlank, allein.", "tokens": ["und", "stellst", "ihn", "vor", "den", "Him\u00b7mel", ":", "schlank", ",", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "VVFIN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und hast die Welt gemacht. Und sie ist gro\u00df", "tokens": ["Und", "hast", "die", "Welt", "ge\u00b7macht", ".", "Und", "sie", "ist", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$.", "KON", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "und wie ein Wort, das noch im Schweigen reift.", "tokens": ["und", "wie", "ein", "Wort", ",", "das", "noch", "im", "Schwei\u00b7gen", "reift", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und wie dein Wille ihren Sinn begreift,", "tokens": ["Und", "wie", "dein", "Wil\u00b7le", "ih\u00b7ren", "Sinn", "be\u00b7greift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "lassen sie deine Augen z\u00e4rtlich los...", "tokens": ["las\u00b7sen", "sie", "dei\u00b7ne", "Au\u00b7gen", "z\u00e4rt\u00b7lich", "los", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Wer du auch seist: am Abend tritt hinaus", "tokens": ["Wer", "du", "auch", "seist", ":", "am", "A\u00b7bend", "tritt", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$.", "APPRART", "NN", "VVFIN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "aus deiner Stube, drin du alles wei\u00dft;", "tokens": ["aus", "dei\u00b7ner", "Stu\u00b7be", ",", "drin", "du", "al\u00b7les", "wei\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "als letztes vor der Ferne liegt dein Haus:", "tokens": ["als", "letz\u00b7tes", "vor", "der", "Fer\u00b7ne", "liegt", "dein", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wer du auch seist.", "tokens": ["wer", "du", "auch", "seist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "