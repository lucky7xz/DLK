ct"], "pos": ["PWAV", "NN", "KON", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im k\u00fchlen Schatten dicht verschlung'ner Aeste;", "tokens": ["Im", "k\u00fch\u00b7len", "Schat\u00b7ten", "dicht", "ver\u00b7schlung'\u00b7ner", "A\u00b7es\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich spreche dir von Sturm und Meeresflammen", "tokens": ["Ich", "spre\u00b7che", "dir", "von", "Sturm", "und", "Mee\u00b7res\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schm\u00fccke dich mit Perlen und Korallen,", "tokens": ["Und", "schm\u00fc\u00b7cke", "dich", "mit", "Per\u00b7len", "und", "Ko\u00b7ral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du pfl\u00fcckst mir still der Gold-Orangen beste.", "tokens": ["Du", "pfl\u00fcckst", "mir", "still", "der", "Gold\u00b7Oran\u00b7gen", "bes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Ich hab' als Kind gespielt im fernen Norden,", "tokens": ["Ich", "hab'", "als", "Kind", "ge\u00b7spielt", "im", "fer\u00b7nen", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOUS", "NN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "met