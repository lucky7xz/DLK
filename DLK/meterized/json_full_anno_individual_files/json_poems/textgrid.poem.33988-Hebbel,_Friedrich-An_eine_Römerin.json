{"textgrid.poem.33988": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An eine R\u00f6merin", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' als Kind gespielt im fernen Norden,", "tokens": ["Ich", "hab'", "als", "Kind", "ge\u00b7spielt", "im", "fer\u00b7nen", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOUS", "NN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann bin ich weit und breit herum gekommen", "tokens": ["Dann", "bin", "ich", "weit", "und", "breit", "he\u00b7rum", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "APZR", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und habe schon das dritte Meer durchschwommen,", "tokens": ["Und", "ha\u00b7be", "schon", "das", "drit\u00b7te", "Meer", "durch\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun ruh' ich aus an seinen Bl\u00fcten-Borden.", "tokens": ["Nun", "ruh'", "ich", "aus", "an", "sei\u00b7nen", "Bl\u00fc\u00b7ten\u00b7Bor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dir ist ein schlichtes M\u00e4dchen-Loos geworden,", "tokens": ["Dir", "ist", "ein", "schlich\u00b7tes", "M\u00e4d\u00b7chen\u00b7Loos", "ge\u00b7wor\u00b