{"dta.poem.5970": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und alles durch zu gehn, zu jeder Zeit,", "tokens": ["Und", "al\u00b7les", "durch", "zu", "gehn", ",", "zu", "je\u00b7der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PTKZU", "VVINF", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So hurtig als bereit:", "tokens": ["So", "hur\u00b7tig", "als", "be\u00b7reit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der an sich selber blo\u00df allein", "tokens": ["Der", "an", "sich", "sel\u00b7ber", "blo\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PRF", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bewegung, Flamme, Licht und Schein?", "tokens": ["Be\u00b7we\u00b7gung", ",", "Flam\u00b7me", ",", "Licht", "und", "Schein", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lasst uns dann dies wol \u00fcberlegen:", "tokens": ["Lasst", "uns", "dann", "dies", "wol", "\u00fc\u00b7berl\u00b7e\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die R\u00fcnde von der Sonnen Scheibe", "tokens": ["Die", "R\u00fcn\u00b7de", "von", "der", "Son\u00b7nen", "Schei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Entstehet blo\u00df durch das Bewegen,", "tokens": ["Ent\u00b7ste\u00b7het", "blo\u00df", "durch", "das", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So GOTT beliebt dem Gantzen einzupr\u00e4gen.", "tokens": ["So", "GoTT", "be\u00b7liebt", "dem", "Gant\u00b7zen", "ein\u00b7zu\u00b7pr\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Erinnert euch der kleinen regen Theile,", "tokens": ["E\u00b7rin\u00b7nert", "euch", "der", "klei\u00b7nen", "re\u00b7gen", "Thei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die so subtil, so zart und rein,", "tokens": ["Die", "so", "sub\u00b7til", ",", "so", "zart", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In welchen, eben weil sie klein,", "tokens": ["In", "wel\u00b7chen", ",", "e\u00b7ben", "weil", "sie", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "$,", "ADV", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die rege Krafft von einer steten Eile,", "tokens": ["Die", "re\u00b7ge", "Krafft", "von", "ei\u00b7ner", "ste\u00b7ten", "Ei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und fl\u00fcchtigem Bewegungs-Triebe,", "tokens": ["Und", "fl\u00fcch\u00b7ti\u00b7gem", "Be\u00b7we\u00b7gungs\u00b7Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Am st\u00e4rcksten aufbehalten bliebe.", "tokens": ["Am", "st\u00e4rcks\u00b7ten", "auf\u00b7be\u00b7hal\u00b7ten", "blie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Derselbe Stoff, der allen Raum durchbricht,", "tokens": ["Der\u00b7sel\u00b7be", "Stoff", ",", "der", "al\u00b7len", "Raum", "durch\u00b7bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und der sich unaufh\u00f6rlich reget,", "tokens": ["Und", "der", "sich", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Vom tieffsten Abgrund sich zum Firmament beweget,", "tokens": ["Vom", "tieffs\u00b7ten", "Ab\u00b7grund", "sich", "zum", "Fir\u00b7ma\u00b7ment", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mischt sich mit den, von ihm durchdrungnen C\u00f6rpern, nicht.", "tokens": ["Mischt", "sich", "mit", "den", ",", "von", "ihm", "durch\u00b7drung\u00b7nen", "C\u00f6r\u00b7pern", ",", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "$,", "APPR", "PPER", "ADJA", "NN", "$,", "PTKNEG", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Es bleibt noch eine grosse Menge,", "tokens": ["Es", "bleibt", "noch", "ei\u00b7ne", "gros\u00b7se", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die eine strenge Macht der C\u00f6rper, welche fest,", "tokens": ["Die", "ei\u00b7ne", "stren\u00b7ge", "Macht", "der", "C\u00f6r\u00b7per", ",", "wel\u00b7che", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und der er welchen mu\u00df, in eine Enge,", "tokens": ["Und", "der", "er", "wel\u00b7chen", "mu\u00df", ",", "in", "ei\u00b7ne", "En\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PWAT", "VMFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und folglich in die Form, von einer Kugel presst.", "tokens": ["Und", "folg\u00b7lich", "in", "die", "Form", ",", "von", "ei\u00b7ner", "Ku\u00b7gel", "presst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dadurch entsteht ein\u2019 heisse Fl\u00fcssigkeit,", "tokens": ["Da\u00b7durch", "ent\u00b7steht", "ein'", "heis\u00b7se", "Fl\u00fcs\u00b7sig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Und da dasselbe stets davon zu fliehn bereit,", "tokens": ["Und", "da", "das\u00b7sel\u00b7be", "stets", "da\u00b7von", "zu", "fliehn", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDAT", "ADV", "PAV", "PTKZU", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Jedennoch allezeit,", "tokens": ["Je\u00b7den\u00b7noch", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.26": {"text": "Von jener Himmels-Lufft gedr\u00fccket und gepresst,", "tokens": ["Von", "je\u00b7ner", "Him\u00b7mels\u00b7Lufft", "ge\u00b7dr\u00fc\u00b7cket", "und", "ge\u00b7presst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Als wie ein gl\u00fcender und runder Ofen l\u00e4sst;", "tokens": ["Als", "wie", "ein", "gl\u00fcen\u00b7der", "und", "run\u00b7der", "O\u00b7fen", "l\u00e4sst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "So nimmt sein runder Schein", "tokens": ["So", "nimmt", "sein", "run\u00b7der", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Den Mittel-Punct vom Sonnen-Wirbel ein.", "tokens": ["Den", "Mit\u00b7tel\u00b7Punct", "vom", "Son\u00b7nen\u00b7Wir\u00b7bel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}