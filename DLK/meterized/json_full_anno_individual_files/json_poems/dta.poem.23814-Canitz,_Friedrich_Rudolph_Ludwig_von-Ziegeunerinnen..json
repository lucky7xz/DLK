{"dta.poem.23814": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Ziegeunerinnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.85", "nl:0.14"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Nehmt eure Hertzen wol in acht/", "tokens": ["Nehmt", "eu\u00b7re", "Hert\u00b7zen", "wol", "in", "acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "CARD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr di\u00df lumpen Volck nicht kennet/", "tokens": ["Die", "ihr", "di\u00df", "lum\u00b7pen", "Volck", "nicht", "ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDS", "VVFIN", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das nur auf Mord und Raub durch Land und St\u00e4dte", "tokens": ["Das", "nur", "auf", "Mord", "und", "Raub", "durch", "Land", "und", "St\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "NN", "KON", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie haben viel schon in Gefahr gebracht.", "tokens": ["Sie", "ha\u00b7ben", "viel", "schon", "in", "Ge\u00b7fahr", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie zeigen unser Gl\u00fcck und Ungl\u00fcck richtig an/", "tokens": ["Sie", "zei\u00b7gen", "un\u00b7ser", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "rich\u00b7tig", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dieweil ihr Ja und Nein uns beydes schaffen kan.", "tokens": ["Die\u00b7weil", "ihr", "Ja", "und", "Nein", "uns", "bey\u00b7des", "schaf\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "PTKANT", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}