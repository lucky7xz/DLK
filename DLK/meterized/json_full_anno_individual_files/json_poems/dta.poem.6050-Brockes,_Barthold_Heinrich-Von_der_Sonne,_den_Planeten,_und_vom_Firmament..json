{"dta.poem.6050": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So zeigt sich fr\u00fch das Licht, das sich des Nachts versteckt.", "tokens": ["So", "zeigt", "sich", "fr\u00fch", "das", "Licht", ",", "das", "sich", "des", "Nachts", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ART", "NN", "$,", "PRELS", "PRF", "ART", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf unserm Welt-Theil, der sich reget,", "tokens": ["Auf", "un\u00b7serm", "Welt\u00b7T\u00b7heil", ",", "der", "sich", "re\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Geht alles, folget sich und wird beweget.", "tokens": ["Geht", "al\u00b7les", ",", "fol\u00b7get", "sich", "und", "wird", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "VVFIN", "PRF", "KON", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch die Bewegung nun, ob wir sie gleich nicht sp\u00fcren,", "tokens": ["Durch", "die", "Be\u00b7we\u00b7gung", "nun", ",", "ob", "wir", "sie", "gleich", "nicht", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mu\u00df unsre Helffte dann ihr Licht verlieren;", "tokens": ["Mu\u00df", "uns\u00b7re", "Helff\u00b7te", "dann", "ihr", "Licht", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn sich das Licht der andern Helffte zeigt.", "tokens": ["Wenn", "sich", "das", "Licht", "der", "an\u00b7dern", "Helff\u00b7te", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Man meinet, da\u00df die Sonn uns naht, sich hebt und neigt,", "tokens": ["Man", "mei\u00b7net", ",", "da\u00df", "die", "Sonn", "uns", "naht", ",", "sich", "hebt", "und", "neigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df sie uns fr\u00fch die Pracht des Tages schencket,", "tokens": ["Da\u00df", "sie", "uns", "fr\u00fch", "die", "Pracht", "des", "Ta\u00b7ges", "schen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und uns des Nachts in Schatten sencket.", "tokens": ["Und", "uns", "des", "Nachts", "in", "Schat\u00b7ten", "sen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Erde geht von ihrem Ort,", "tokens": ["Die", "Er\u00b7de", "geht", "von", "ih\u00b7rem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Um einen Grad allt\u00e4glich weiter fort,", "tokens": ["Um", "ei\u00b7nen", "Grad", "all\u00b7t\u00e4g\u00b7lich", "wei\u00b7ter", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wodurch die Sonne, so uns scheinet, als ob sie", "tokens": ["Wo\u00b7durch", "die", "Son\u00b7ne", ",", "so", "uns", "schei\u00b7net", ",", "als", "ob", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Um einem Grad sey fort gereiset", "tokens": ["Um", "ei\u00b7nem", "Grad", "sey", "fort", "ge\u00b7rei\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VAFIN", "PTKVZ", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "In der Eccliptica, uns fr\u00fch", "tokens": ["In", "der", "Ec\u00b7clip\u00b7ti\u00b7ca", ",", "uns", "fr\u00fch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NE", "$,", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In ihrem Stand ein\u2019 Aendrung weiset.", "tokens": ["In", "ih\u00b7rem", "Stand", "ein'", "A\u00b7en\u00b7drung", "wei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Da, wo die Linie, die man", "tokens": ["Da", ",", "wo", "die", "Li\u00b7nie", ",", "die", "man"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "PIS"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Darum so ", "tokens": ["Da\u00b7rum", "so"], "token_info": ["word", "word"], "pos": ["PAV", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Weil sie der Erden Rund in gleiche Theile trennet,", "tokens": ["Weil", "sie", "der", "Er\u00b7den", "Rund", "in", "glei\u00b7che", "Thei\u00b7le", "tren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wo sie die Sonne stets in Gleichheit treffen kan;", "tokens": ["Wo", "sie", "die", "Son\u00b7ne", "stets", "in", "Gleich\u00b7heit", "tref\u00b7fen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sind T\u00e4g und N\u00e4chte gleich. Was aber nach der Lage", "tokens": ["Sind", "T\u00e4g", "und", "N\u00e4ch\u00b7te", "gleich", ".", "Was", "a\u00b7ber", "nach", "der", "La\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "$.", "PWS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nach beyden Angeln weicht,", "tokens": ["Nach", "bey\u00b7den", "An\u00b7geln", "weicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.22": {"text": "Hat Graden-Weis, bald lang\u2019 bald k\u00fcrtzre N\u00e4cht\u2019 und Tage.", "tokens": ["Hat", "Gra\u00b7den\u00b7Weis", ",", "bald", "lang'", "bald", "k\u00fcrtz\u00b7re", "N\u00e4cht'", "und", "Ta\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "ADV", "ADV", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Regel trifft bey S\u00fcd\u2019 und Norden ein:", "tokens": ["Die", "Re\u00b7gel", "trifft", "bey", "S\u00fcd'", "und", "Nor\u00b7den", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "So da\u00df so wol", "tokens": ["So", "da\u00df", "so", "wol"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Jm S\u00fcder-als im Norder-Pol", "tokens": ["Jm", "S\u00fc\u00b7der\u00b7als", "im", "Nor\u00b7der\u00b7Pol"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die V\u00f6lcker, die einander gantz entgegen,", "tokens": ["Die", "V\u00f6l\u00b7cker", ",", "die", "ein\u00b7an\u00b7der", "gantz", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Nach einerley Gesetzen sich bewegen.", "tokens": ["Nach", "ei\u00b7ner\u00b7ley", "Ge\u00b7set\u00b7zen", "sich", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Sechs Monat w\u00e4hrt die Nacht, und sechs des Tages Schein.", "tokens": ["Sechs", "Mo\u00b7nat", "w\u00e4hrt", "die", "Nacht", ",", "und", "sechs", "des", "Ta\u00b7ges", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$,", "KON", "CARD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}