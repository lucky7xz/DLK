{"dta.poem.7980": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3.  Die Nachtschw\u00e4rmer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Zu dem Brunnen, mit den Kr\u00fcgen", "tokens": ["Zu", "dem", "Brun\u00b7nen", ",", "mit", "den", "Kr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6mmt noch sp\u00e4t mein trautes M\u00e4dchen,", "tokens": ["K\u00f6mmt", "noch", "sp\u00e4t", "mein", "trau\u00b7tes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rollt mit raschen, kr\u00e4ft\u2019gen Z\u00fcgen", "tokens": ["Rollt", "mit", "ra\u00b7schen", ",", "kr\u00e4ft'\u00b7gen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "VVINF", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Husch! die Ketten um das R\u00e4dchen;", "tokens": ["Husch", "!", "die", "Ket\u00b7ten", "um", "das", "R\u00e4d\u00b7chen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihr zu helfen, welch Vergn\u00fcgen!", "tokens": ["Ihr", "zu", "hel\u00b7fen", ",", "welch", "Ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja! ich zog mit ganzem Leibe,", "tokens": ["Ja", "!", "ich", "zog", "mit", "gan\u00b7zem", "Lei\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis zersprang des R\u00e4dchens Scheibe.", "tokens": ["Bis", "zer\u00b7sprang", "des", "R\u00e4d\u00b7chens", "Schei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist es nun auch stehn geblieben,", "tokens": ["Ist", "es", "nun", "auch", "stehn", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Haben wir\u2019s doch gut getrieben,", "tokens": ["Ha\u00b7ben", "wir's", "doch", "gut", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sehe Jeder, wie er's treibe!", "tokens": ["Se\u00b7he", "Je\u00b7der", ",", "wie", "er's", "trei\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}