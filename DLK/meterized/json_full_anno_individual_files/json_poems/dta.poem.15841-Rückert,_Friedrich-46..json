{"dta.poem.15841": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sch\u00f6nheit nur zu sehn im Sch\u00f6nen, ist nicht schwer;", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "nur", "zu", "sehn", "im", "Sch\u00f6\u00b7nen", ",", "ist", "nicht", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "APPRART", "NN", "$,", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh' im Unsch\u00f6nen sie, und unsch\u00f6n ists nicht mehr.", "tokens": ["Sieh'", "im", "Un\u00b7sch\u00f6\u00b7nen", "sie", ",", "und", "un\u00b7sch\u00f6n", "ists", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "$,", "KON", "ADJD", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sch\u00f6nheit, Gottes Licht, durchdringt die ganze Welt,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", ",", "Got\u00b7tes", "Licht", ",", "durch\u00b7dringt", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die bl\u00f6den Augen nur den Abglanz vorenth\u00e4lt.", "tokens": ["Die", "bl\u00f6\u00b7den", "Au\u00b7gen", "nur", "den", "Ab\u00b7glanz", "vor\u00b7ent\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du f\u00fchle dich in Gott, und Alles gottvereint,", "tokens": ["Du", "f\u00fch\u00b7le", "dich", "in", "Gott", ",", "und", "Al\u00b7les", "gott\u00b7ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist dir alles sch\u00f6n, was andern anders scheint.", "tokens": ["So", "ist", "dir", "al\u00b7les", "sch\u00f6n", ",", "was", "an\u00b7dern", "an\u00b7ders", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADJD", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}