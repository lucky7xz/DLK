{"dta.poem.6858": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer aber fasset doch, mit welcher Schnelligkeit,", "tokens": ["Wer", "a\u00b7ber", "fas\u00b7set", "doch", ",", "mit", "wel\u00b7cher", "Schnel\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ADV", "$,", "APPR", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von so verschiedener Beschaffenheit,", "tokens": ["Von", "so", "ver\u00b7schie\u00b7de\u00b7ner", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Vorw\u00fcrff, die die Sinnen r\u00fchreu,", "tokens": ["Die", "Vor\u00b7w\u00fcrff", ",", "die", "die", "Sin\u00b7nen", "r\u00fch\u00b7reu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Durch unterschiednen Druck sich im Gehirn formiren,", "tokens": ["Durch", "un\u00b7ter\u00b7schied\u00b7nen", "Druck", "sich", "im", "Ge\u00b7hirn", "for\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In dieser C\u00f6rper Meng\u2019, die ohne Zahl sich h\u00e4ufft?", "tokens": ["In", "die\u00b7ser", "C\u00f6r\u00b7per", "Meng'", ",", "die", "oh\u00b7ne", "Zahl", "sich", "h\u00e4ufft", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NE", "$,", "PRELS", "APPR", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo ist ein Witz, ein Meister, der begreifft,", "tokens": ["Wo", "ist", "ein", "Witz", ",", "ein", "Meis\u00b7ter", ",", "der", "be\u00b7greifft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie sie in einem Puct sich so vereinen k\u00f6unen,", "tokens": ["Wie", "sie", "in", "ei\u00b7nem", "Puct", "sich", "so", "ver\u00b7ei\u00b7nen", "k\u00f6u\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "PRF", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auch in demselben Punct so ordentlich sich trennen?", "tokens": ["Auch", "in", "dem\u00b7sel\u00b7ben", "Punct", "so", "or\u00b7dent\u00b7lich", "sich", "tren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "ADV", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}