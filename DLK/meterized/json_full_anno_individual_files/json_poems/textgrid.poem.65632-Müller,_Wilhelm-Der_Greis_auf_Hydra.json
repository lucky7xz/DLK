{"textgrid.poem.65632": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Greis auf Hydra", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand auf hohem Felsen, tief unter mir die Fluth:", "tokens": ["Ich", "stand", "auf", "ho\u00b7hem", "Fel\u00b7sen", ",", "tief", "un\u00b7ter", "mir", "die", "Fluth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "ADJD", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da schwang sich meine Seele empor in freiem Muth.", "tokens": ["Da", "schwang", "sich", "mei\u00b7ne", "See\u00b7le", "em\u00b7por", "in", "frei\u00b7em", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich lie\u00df die Blicke schweifen weit \u00fcber Land und Meer:", "tokens": ["Ich", "lie\u00df", "die", "Bli\u00b7cke", "schwei\u00b7fen", "weit", "\u00fc\u00b7ber", "Land", "und", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So weit, so weit sie reichen, klirrt keine Kette mehr.", "tokens": ["So", "weit", ",", "so", "weit", "sie", "rei\u00b7chen", ",", "klirrt", "kei\u00b7ne", "Ket\u00b7te", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "So weit, so weit sie reichen, kein halber Mond zu sehn,", "tokens": ["So", "weit", ",", "so", "weit", "sie", "rei\u00b7chen", ",", "kein", "hal\u00b7ber", "Mond", "zu", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Auf Bergen, Th\u00fcrmen, Masten, die heil'gen Kreuze wehn.", "tokens": ["Auf", "Ber\u00b7gen", ",", "Th\u00fcr\u00b7men", ",", "Mas\u00b7ten", ",", "die", "heil'\u00b7gen", "Kreu\u00b7ze", "wehn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "So weit, so weit sie reichen, es hebt sich jede Brust", "tokens": ["So", "weit", ",", "so", "weit", "sie", "rei\u00b7chen", ",", "es", "hebt", "sich", "je\u00b7de", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "PIAT", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Und Alles was uns fesselt, und Alles was uns dr\u00fcckt,", "tokens": ["Und", "Al\u00b7les", "was", "uns", "fes\u00b7selt", ",", "und", "Al\u00b7les", "was", "uns", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "PPER", "ADJD", "$,", "KON", "PIS", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Was Einen nur bek\u00fcmmert, was Einen nur entz\u00fcckt,", "tokens": ["Was", "Ei\u00b7nen", "nur", "be\u00b7k\u00fcm\u00b7mert", ",", "was", "Ei\u00b7nen", "nur", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADV", "VVPP", "$,", "PRELS", "ART", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wir werfen's in das Feuer, wir senken's in die Fluth,", "tokens": ["Wir", "wer\u00b7fen's", "in", "das", "Feu\u00b7er", ",", "wir", "sen\u00b7ken's", "in", "die", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "Die wogt durch alle Herzen in ", "tokens": ["Die", "wogt", "durch", "al\u00b7le", "Her\u00b7zen", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ich sehe Schiffe fahren \u2013 die stolze Woge braust \u2013", "tokens": ["Ich", "se\u00b7he", "Schif\u00b7fe", "fah\u00b7ren", "\u2013", "die", "stol\u00b7ze", "Wo\u00b7ge", "braust", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVINF", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Ist es der Sturm der Freiheit, der in die Segel