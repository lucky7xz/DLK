{"textgrid.poem.33245": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: Urbild des alten Rom! Reliquienschrein", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Urbild des alten Rom! Reliquienschrein", "tokens": ["Ur\u00b7bild", "des", "al\u00b7ten", "Rom", "!", "Re\u00b7li\u00b7qui\u00b7en\u00b7schrein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "NE", "$.", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "F\u00fcr Schaun und hohen Traum, den in die Zeit", "tokens": ["F\u00fcr", "Schaun", "und", "ho\u00b7hen", "Traum", ",", "den", "in", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jahrhunderte von Pracht und Macht gestellt!", "tokens": ["Jahr\u00b7hun\u00b7der\u00b7te", "von", "Pracht", "und", "Macht", "ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun endlich \u2013 endlich \u2013 nach so vielen Tagen", "tokens": ["Nun", "end\u00b7lich", "\u2013", "end\u00b7lich", "\u2013", "nach", "so", "vie\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$(", "ADV", "$(", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von Wanderm\u00fcdigkeit und gierem Durst", "tokens": ["Von", "Wan\u00b7der\u00b7m\u00fc\u00b7dig\u00b7keit", "und", "gie\u00b7rem", "Durst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "(von Durst zum Quell des Wissens, den du birgst)", "tokens": ["(", "von", "Durst", "zum", "Quell", "des", "Wis\u00b7sens", ",", "den", "du", "birgst", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein andrer und dem\u00fctiger kniee ich", "tokens": ["Ein", "an\u00b7drer", "und", "de\u00b7m\u00fc\u00b7ti\u00b7ger", "kni\u00b7ee", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "In deinem Schatten nun und trinke ein", "tokens": ["In", "dei\u00b7nem", "Schat\u00b7ten", "nun", "und", "trin\u00b7ke", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "KON", "VVFIN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dein ragend D\u00fcster, deinen Glanz und Ruhm.", "tokens": ["Dein", "ra\u00b7gend", "D\u00fcs\u00b7ter", ",", "dei\u00b7nen", "Glanz", "und", "Ruhm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Unendlichkeit und \u00d6de! Schwermut, Schweigen!", "tokens": ["Un\u00b7end\u00b7lich\u00b7keit", "und", "\u00d6\u00b7de", "!", "Schwer\u00b7mut", ",", "Schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uralter Zeit Erinnern \u2013 d\u00fcstere Nacht!", "tokens": ["Ur\u00b7al\u00b7ter", "Zeit", "E\u00b7rin\u00b7nern", "\u2013", "d\u00fcs\u00b7te\u00b7re", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$(", "VVFIN", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Ich f\u00fchl euch jetzt \u2013 f\u00fchl eure ganze Wucht \u2013", "tokens": ["Ich", "f\u00fchl", "euch", "jetzt", "\u2013", "f\u00fchl", "eu\u00b7re", "gan\u00b7ze", "Wucht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O Zauber, st\u00e4rker als Jud\u00e4as K\u00f6nig", "tokens": ["O", "Zau\u00b7ber", ",", "st\u00e4r\u00b7ker", "als", "Ju\u00b7d\u00e4\u00b7as", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADJD", "KOKOM", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Voreinst gelehrt im Berg Gethsemane!", "tokens": ["Vor\u00b7e\u00b7inst", "ge\u00b7lehrt", "im", "Berg", "Geth\u00b7se\u00b7ma\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPRART", "NN", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "O Wunder, machtvoller als der Chald\u00e4er", "tokens": ["O", "Wun\u00b7der", ",", "macht\u00b7vol\u00b7ler", "als", "der", "Chal\u00b7d\u00e4\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+--+-+-++-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Jemals verz\u00fcckt aus stillen Sternen zog!", "tokens": ["Je\u00b7mals", "ver\u00b7z\u00fcckt", "aus", "stil\u00b7len", "Ster\u00b7nen", "zog", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Hier, wo ein Held einst st\u00fcrzte, st\u00fcrzt die S\u00e4ule.", "tokens": ["Hier", ",", "wo", "ein", "Held", "einst", "st\u00fcrz\u00b7te", ",", "st\u00fcrzt", "die", "S\u00e4u\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ADV", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier, wo ein goldner toter Adler gl\u00e4nzte,", "tokens": ["Hier", ",", "wo", "ein", "gold\u00b7ner", "to\u00b7ter", "Ad\u00b7ler", "gl\u00e4nz\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00e4lt mittern\u00e4chtig Wacht die Fledermaus.", "tokens": ["H\u00e4lt", "mit\u00b7ter\u00b7n\u00e4ch\u00b7tig", "Wacht", "die", "Fle\u00b7der\u00b7maus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hier, wo der Damen Roms vergoldet Haar", "tokens": ["Hier", ",", "wo", "der", "Da\u00b7men", "Roms", "ver\u00b7gol\u00b7det", "Haar"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "NE", "VVFIN", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Im Winde wehte, wogt nun Ried und Distel.", "tokens": ["Im", "Win\u00b7de", "weh\u00b7te", ",", "wogt", "nun", "Ried", "und", "Dis\u00b7tel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hier, wo auf goldnem Thron der Herrscher lehnte,", "tokens": ["Hier", ",", "wo", "auf", "gold\u00b7nem", "Thron", "der", "Herr\u00b7scher", "lehn\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Schl\u00fcpft geisterhaft aus ihrem Marmorhaus,", "tokens": ["Schl\u00fcpft", "geis\u00b7ter\u00b7haft", "aus", "ih\u00b7rem", "Mar\u00b7mor\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vom Schein des zwiegeh\u00f6rnten Monds beleuchtet,", "tokens": ["Vom", "Schein", "des", "zwie\u00b7ge\u00b7h\u00f6rn\u00b7ten", "Monds", "be\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die flinke Echse schweigend \u00fcber Steine.", "tokens": ["Die", "flin\u00b7ke", "Ech\u00b7se", "schwei\u00b7gend", "\u00fc\u00b7ber", "Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch halt! Die Mauern \u2013 diese Bogeng\u00e4nge,", "tokens": ["Doch", "halt", "!", "Die", "Mau\u00b7ern", "\u2013", "die\u00b7se", "Bo\u00b7gen\u00b7g\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "$(", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hochauf von altem Efeu eingekleidet,", "tokens": ["Hoch\u00b7auf", "von", "al\u00b7tem", "E\u00b7feu", "ein\u00b7ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die schwarzen br\u00f6ckeligen S\u00e4ulensockel", "tokens": ["Die", "schwar\u00b7zen", "br\u00f6\u00b7cke\u00b7li\u00b7gen", "S\u00e4u\u00b7len\u00b7so\u00b7ckel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und d\u00fcstern Sch\u00e4fte, dunklen Kapitelle,", "tokens": ["Und", "d\u00fcs\u00b7tern", "Sch\u00e4f\u00b7te", ",", "dunk\u00b7len", "Ka\u00b7pi\u00b7tel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zerfallenden und fast verbla\u00dften Friese,", "tokens": ["Zer\u00b7fal\u00b7len\u00b7den", "und", "fast", "ver\u00b7bla\u00df\u00b7ten", "Frie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zersprungnen Kranzgeb\u00e4lke \u2013 dieses Wrack \u2013", "tokens": ["Zer\u00b7sprung\u00b7nen", "Kranz\u00b7ge\u00b7b\u00e4l\u00b7ke", "\u2013", "die\u00b7ses", "Wrack", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "All diese Steine \u2013 ach, die grauen Steine \u2013", "tokens": ["All", "die\u00b7se", "Stei\u00b7ne", "\u2013", "ach", ",", "die", "grau\u00b7en", "Stei\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PDAT", "NN", "$(", "XY", "$,", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Sind sie denn alles, was der Zahn der Zeit", "tokens": ["Sind", "sie", "denn", "al\u00b7les", ",", "was", "der", "Zahn", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von all dem Ruhm und ungeheuren Glanz", "tokens": ["Von", "all", "dem", "Ruhm", "und", "un\u00b7ge\u00b7heu\u00b7ren", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "F\u00fcr mich und f\u00fcr das Schicksal \u00fcbriglie\u00df?", "tokens": ["F\u00fcr", "mich", "und", "f\u00fcr", "das", "Schick\u00b7sal", "\u00fcb\u00b7rig\u00b7lie\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "\u00bbnicht alles\u00ab, geben mir die Echos Antwort,", "tokens": ["\u00bb", "nicht", "al\u00b7les", "\u00ab", ",", "ge\u00b7ben", "mir", "die", "E\u00b7chos", "Ant\u00b7wort", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "PIS", "$(", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "\u00bbnicht alles, nein! Prophetische Kl\u00e4nge steigen \u2013", "tokens": ["\u00bb", "nicht", "al\u00b7les", ",", "nein", "!", "Pro\u00b7phe\u00b7ti\u00b7sche", "Kl\u00e4n\u00b7ge", "stei\u00b7gen", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "PIS", "$,", "PTKANT", "$.", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Und laute Kl\u00e4nge \u2013 ewig von uns auf,", "tokens": ["Und", "lau\u00b7te", "Kl\u00e4n\u00b7ge", "\u2013", "e\u00b7wig", "von", "uns", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "$(", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Von allen Tr\u00fcmmern zu den Weisen auf,", "tokens": ["Von", "al\u00b7len", "Tr\u00fcm\u00b7mern", "zu", "den", "Wei\u00b7sen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wie Melodie von Memnon steigt zur Sonne.", "tokens": ["Wie", "Me\u00b7lo\u00b7die", "von", "Mem\u00b7non", "steigt", "zur", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wir leiten alle riesenhaften Geister.", "tokens": ["Wir", "lei\u00b7ten", "al\u00b7le", "rie\u00b7sen\u00b7haf\u00b7ten", "Geis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "In unumschr\u00e4nkter Macht beherrschen wir", "tokens": ["In", "un\u00b7um\u00b7schr\u00e4nk\u00b7ter", "Macht", "be\u00b7herr\u00b7schen", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Mit unserm Schwung die Herzen aller Gro\u00dfen.", "tokens": ["Mit", "un\u00b7serm", "Schwung", "die", "Her\u00b7zen", "al\u00b7ler", "Gro\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wir sind nicht leblos \u2013 wir erblichnen Steine.", "tokens": ["Wir", "sind", "nicht", "leb\u00b7los", "\u2013", "wir", "er\u00b7blich\u00b7nen", "Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$(", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Nicht alle Macht ist hin \u2013 nicht aller Ruhm \u2013", "tokens": ["Nicht", "al\u00b7le", "Macht", "ist", "hin", "\u2013", "nicht", "al\u00b7ler", "Ruhm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VAFIN", "ADV", "$(", "PTKNEG", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Nicht aller Zauber unsres hohen Rufes \u2013", "tokens": ["Nicht", "al\u00b7ler", "Zau\u00b7ber", "uns\u00b7res", "ho\u00b7hen", "Ru\u00b7fes", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Nicht all das Wunder, das uns rund umfa\u00dft \u2013", "tokens": ["Nicht", "all", "das", "Wun\u00b7der", ",", "das", "uns", "rund", "um\u00b7fa\u00dft", "\u2013"], "token_inf