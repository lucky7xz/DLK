ri.plus"}, "line.4": {"text": "Neben seiner noch nicht geschiedenen,", "tokens": ["Ne\u00b7ben", "sei\u00b7ner", "noch", "nicht", "ge\u00b7schie\u00b7de\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+--+-+--", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Zusammen mit lauter zufriedenen", "tokens": ["Zu\u00b7sam\u00b7men", "mit", "lau\u00b7ter", "zu\u00b7frie\u00b7de\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Kindern und Freunden vor einem Kapaun.", "tokens": ["Kin\u00b7dern", "und", "Freun\u00b7den", "vor", "ei\u00b7nem", "Ka\u00b7paun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Und drum rum", "tokens": ["Und", "drum", "rum"], "token_info": ["word", "word", "word"], "pos": ["KON", "PAV", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Bilder und Buddhas sch\u00f6n und stumm,", "tokens": ["Bil\u00b7der", "und", "Budd\u00b7has", "sch\u00f6n", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die er sch\u00e4tzt und uns nennt", "tokens": ["Die", "er", "sch\u00e4tzt", "und", "uns", "nennt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "KON", "PPER", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und deren Seele er kennt.", "tokens": ["Und", "de\u00b7ren", "See\u00b7le", "er", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Als ich im Filmatelier bei ihm war,", "tokens": ["Als", "ich", "im", "Fil\u00b7ma\u00b7te\u00b7lier", "bei", "ihm", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Stand er mit violettem Haar,", "tokens": ["Stand", "er", "mit", "vi\u00b7o\u00b7let\u00b7tem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zwischen phantastischem Alldingsgewirr,", "tokens": ["Zwi\u00b7schen", "phan\u00b7tas\u00b7ti\u00b7schem", "All\u00b7dings\u00b7ge\u00b7wirr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ri\u00df aus dem Tisch ein Bein", "tokens": ["Ri\u00df", "aus", "dem", "Tisch", "ein", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und \u2013 bums klirr \u2013", "tokens": ["Und", "\u2013", "bums", "klirr", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$(", "XY", "XY", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Schlug er damit in ein Fenster hinein.", "tokens": ["Schlug", "er", "da\u00b7mit", "in", "ein", "Fens\u00b7ter", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Das mu\u00dfte so \u2013 so mu\u00dfte es sein.", "tokens": ["Das", "mu\u00df\u00b7te", "so", "\u2013", "so", "mu\u00df\u00b7te", "es", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "$(", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Und dann sp\u00e4t nachts,", "tokens": ["Und", "dann", "sp\u00e4t", "nachts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Da er m\u00fcde m\u00fc\u00dfte sein \u2013 \u2013", "tokens": ["Da", "er", "m\u00fc\u00b7de", "m\u00fc\u00df\u00b7te", "sein", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "PPOSAT", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein! \u2013 \u2013", "tokens": ["Nein", "!", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct", "punct"], "pos": ["PTKANT", "$.", "$(", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Ging er noch weiter,", "tokens": ["Ging", "er", "noch", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Tanzte, trank Wein", "tokens": ["Tanz\u00b7te", ",", "trank", "Wein"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Bis in die helle Stunde", "tokens": ["Bis", "in", "die", "hel\u00b7le", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Weitarmig und heiter,", "tokens": ["Weit\