{"textgrid.poem.55529": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ode an Herrn Professor Zachari\u00e4", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon w\u00e4lzen schnelle R\u00e4der rasselnd sich und tragen", "tokens": ["Schon", "w\u00e4l\u00b7zen", "schnel\u00b7le", "R\u00e4\u00b7der", "ras\u00b7selnd", "sich", "und", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "VVPP", "PRF", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich von dem unbedau'rten Ort,", "tokens": ["Dich", "von", "dem", "un\u00b7be\u00b7dau'r\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und angekettet fest an deinem Wagen", "tokens": ["Und", "an\u00b7ge\u00b7ket\u00b7tet", "fest", "an", "dei\u00b7nem", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Freude mit dir fort.", "tokens": ["Die", "Freu\u00b7de", "mit", "dir", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du bist uns kaum entwichen, und schwerm\u00fctig ziehen", "tokens": ["Du", "bist", "uns", "kaum", "ent\u00b7wi\u00b7chen", ",", "und", "schwer\u00b7m\u00fc\u00b7tig", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus dumpfen H\u00f6hlen (denn dahin", "tokens": ["Aus", "dum\u00b7pfen", "H\u00f6h\u00b7len", "(", "denn", "da\u00b7hin"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Flohn sie bei deiner Ankunft, wie f\u00fcrm Gl\u00fchen", "tokens": ["Flohn", "sie", "bei", "dei\u00b7ner", "An\u00b7kunft", ",", "wie", "f\u00fcrm", "Gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Sonne Nebel fliehn)", "tokens": ["Der", "Son\u00b7ne", "Ne\u00b7bel", "fliehn", ")"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verdru\u00df und Langeweile. Wie die Stymphaliden", "tokens": ["Ver\u00b7dru\u00df", "und", "Lan\u00b7ge\u00b7wei\u00b7le", ".", "Wie", "die", "Stymp\u00b7ha\u00b7li\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$.", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umschw\u00e4rmen sie den Tisch und spr\u00fchn", "tokens": ["Um\u00b7schw\u00e4r\u00b7men", "sie", "den", "Tisch", "und", "spr\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von ihren Fittichen Gift unserm Frieden", "tokens": ["Von", "ih\u00b7ren", "Fit\u00b7ti\u00b7chen", "Gift", "un\u00b7serm", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PPOSAT", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf alle Speisen hin.", "tokens": ["Auf", "al\u00b7le", "Spei\u00b7sen", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wo ist, sie zu verscheuchen, unser g\u00fct'ger Retter,", "tokens": ["Wo", "ist", ",", "sie", "zu", "ver\u00b7scheu\u00b7chen", ",", "un\u00b7ser", "g\u00fct'\u00b7ger", "Ret\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Venus vielgeliebter Sohn,", "tokens": ["Der", "Ve\u00b7nus", "viel\u00b7ge\u00b7lieb\u00b7ter", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Apollos Liebling, Liebling aller G\u00f6tter?", "tokens": ["A\u00b7pol\u00b7los", "Lieb\u00b7ling", ",", "Lieb\u00b7ling", "al\u00b7ler", "G\u00f6t\u00b7ter", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bebt! Er ist uns entflohn.", "tokens": ["Bebt", "!", "Er", "ist", "uns", "ent\u00b7flohn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "O g\u00e4b er mir die St\u00e4rke, seine m\u00e4cht'ge Leier", "tokens": ["O", "g\u00e4b", "er", "mir", "die", "St\u00e4r\u00b7ke", ",", "sei\u00b7ne", "m\u00e4cht'\u00b7ge", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPER", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu schlagen, die Apoll ihm gab;", "tokens": ["Zu", "schla\u00b7gen", ",", "die", "A\u00b7poll", "ihm", "gab", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich r\u00fchrte sie, dann fl\u00f6hn die Ungeheuer", "tokens": ["Ich", "r\u00fchr\u00b7te", "sie", ",", "dann", "fl\u00f6hn", "die", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erschr\u00f6ckt zur H\u00f6ll hinab.", "tokens": ["Er\u00b7schr\u00f6ckt", "zur", "H\u00f6ll", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O leih mir, Sohn der Maja, deiner Ferse Schwingen,", "tokens": ["O", "leih", "mir", ",", "Sohn", "der", "Ma\u00b7ja", ",", "dei\u00b7ner", "Fer\u00b7se", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die du sonst Sterblichen geliehn;", "tokens": ["Die", "du", "sonst", "Sterb\u00b7li\u00b7chen", "ge\u00b7liehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie rei\u00dfen mich aus diesem Elend, bringen", "tokens": ["Sie", "rei\u00b7\u00dfen", "mich", "aus", "die\u00b7sem", "E\u00b7lend", ",", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich nach der Ocker hin.", "tokens": ["Mich", "nach", "der", "O\u00b7cker", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dann folg ich ohnerwartet einstens ihm am Flusse;", "tokens": ["Dann", "folg", "ich", "oh\u00b7ner\u00b7war\u00b7tet", "eins\u00b7tens", "ihm", "am", "Flus\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jedoch so wenig staunet er,", "tokens": ["Je\u00b7doch", "so", "we\u00b7nig", "stau\u00b7net", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ging' ihm, angeheftet seinem Fu\u00dfe,", "tokens": ["Als", "ging'", "ihm", ",", "an\u00b7ge\u00b7hef\u00b7tet", "sei\u00b7nem", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Schatten hinterher.", "tokens": ["Sein", "Schat\u00b7ten", "hin\u00b7ter\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Von ihm dann unzertrennlich w\u00e4rmt den jungen Busen", "tokens": ["Von", "ihm", "dann", "un\u00b7zer\u00b7trenn\u00b7lich", "w\u00e4rmt", "den", "jun\u00b7gen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Glanz, der glorreich ihn umgibt.", "tokens": ["Der", "Glanz", ",", "der", "glor\u00b7reich", "ihn", "um\u00b7gibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er liebet mich, dann lieben mich die Musen,", "tokens": ["Er", "lie\u00b7bet", "mich", ",", "dann", "lie\u00b7ben", "mich", "die", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil mich ihr Liebling liebt.", "tokens": ["Weil", "mich", "ihr", "Lieb\u00b7ling", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Schon w\u00e4lzen schnelle R\u00e4der rasselnd sich und tragen", "tokens": ["Schon", "w\u00e4l\u00b7zen", "schnel\u00b7le", "R\u00e4\u00b7der", "ras\u00b7selnd", "sich", "und", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "VVPP", "PRF", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measur