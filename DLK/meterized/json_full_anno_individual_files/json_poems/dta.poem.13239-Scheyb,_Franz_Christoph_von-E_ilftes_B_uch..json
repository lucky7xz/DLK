{"dta.poem.13239": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eauf auf dann Tugenden! vereiniget die Macht!", "tokens": ["\u201e", "auf", "auf", "dann", "Tu\u00b7gen\u00b7den", "!", "ver\u00b7ei\u00b7ni\u00b7get", "die", "Macht", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "APPR", "ADV", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eerf\u00fcllet was ihr selbst zum Heil des Throns erdacht!", "tokens": ["\u201e", "er\u00b7f\u00fcl\u00b7let", "was", "ihr", "selbst", "zum", "Heil", "des", "Throns", "er\u00b7dacht", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PWS", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ees ligt noch an dem End\u2019. Ergreiffen wir den Degen!", "tokens": ["\u201e", "es", "ligt", "noch", "an", "dem", "End'", ".", "Er\u00b7greif\u00b7fen", "wir", "den", "De\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201egOtt pfleget ihn dem Recht zur Sch\u00fczung beyzulegen.", "tokens": ["\u201e", "gOtt", "pfle\u00b7get", "ihn", "dem", "Recht", "zur", "Sch\u00fc\u00b7zung", "bey\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}