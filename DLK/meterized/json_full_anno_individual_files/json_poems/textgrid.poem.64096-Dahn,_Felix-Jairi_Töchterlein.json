{"textgrid.poem.64096": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Jairi T\u00f6chterlein", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jede Spur war mir vergangen von des Daseins lichten H\u00f6h'n,", "tokens": ["Je\u00b7de", "Spur", "war", "mir", "ver\u00b7gan\u00b7gen", "von", "des", "Da\u00b7seins", "lich\u00b7ten", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und in Todesnacht gefangen lag mein Leben jung und sch\u00f6n.", "tokens": ["Und", "in", "To\u00b7des\u00b7nacht", "ge\u00b7fan\u00b7gen", "lag", "mein", "Le\u00b7ben", "jung", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "VVFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "O wie sah die Seele sehnlich noch dem holden Dasein nach,", "tokens": ["O", "wie", "sah", "die", "See\u00b7le", "sehn\u00b7lich", "noch", "dem", "hol\u00b7den", "Da\u00b7sein", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "ART", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Als, verglimmter Fackel \u00e4hnlich, schmerzlich schwer mein Auge brach!", "tokens": ["Als", ",", "ver\u00b7glimm\u00b7ter", "Fa\u00b7ckel", "\u00e4hn\u00b7lich", ",", "schmerz\u00b7lich", "schwer", "mein", "Au\u00b7ge", "brach", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJA", "NN", "ADJD", "$,", "ADJD", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Diese Welt voll Glanz und Schimmer sollte mir verloren sein,", "tokens": ["Die\u00b7se", "Welt", "voll", "Glanz", "und", "Schim\u00b7mer", "soll\u00b7te", "mir", "ver\u00b7lo\u00b7ren", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "NN", "KON", "NN", "VMFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und dies Auge sollte nimmer Blumen schau'n und Sonnenschein!", "tokens": ["Und", "dies", "Au\u00b7ge", "soll\u00b7te", "nim\u00b7mer", "Blu\u00b7men", "schau'n", "und", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "VMFIN", "ADV", "NN", "VVINF", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Wann der frohe Fr\u00fchlingsreigen die Gespielinnen vereint,", "tokens": ["Wann", "der", "fro\u00b7he", "Fr\u00fch\u00b7lings\u00b7rei\u00b7gen", "die", "Ge\u00b7spie\u00b7lin\u00b7nen", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-++--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.2": {"text": "Sollt' ich ruh'n in kaltem Schweigen, wohin ach! kein Fr\u00fchling scheint!", "tokens": ["Sollt'", "ich", "ruh'n", "in", "kal\u00b7tem", "Schwei\u00b7gen", ",", "wo\u00b7hin", "ach", "!", "kein", "Fr\u00fch\u00b7ling", "scheint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "PWAV", "ITJ", "$.", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Lange lag ich selbstverloren: \u2013 Nacht ringsum \u2013, nur dann und wann,", "tokens": ["Lan\u00b7ge", "lag", "ich", "selbst\u00b7ver\u00b7lo\u00b7ren", ":", "\u2013", "Nacht", "ring\u00b7sum", "\u2013", ",", "nur", "dann", "und", "wann", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$.", "$(", "NN", "ADV", "$(", "$,", "ADV", "ADV", "KON", "PWAV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "N\u00e4her stets, zu meinen Ohren drang's wie dunkle Flut heran", "tokens": ["N\u00e4\u00b7her", "stets", ",", "zu", "mei\u00b7nen", "Oh\u00b7ren", "drang's", "wie", "dunk\u00b7le", "Flut", "he\u00b7ran"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "KOKOM", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Und ich f\u00fchlt' es: wenn die Wogen mich erreichten ganz und gar,", "tokens": ["Und", "ich", "f\u00fchlt'", "es", ":", "wenn", "die", "Wo\u00b7gen", "mich", "er\u00b7reich\u00b7ten", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "KOUS", "ART", "NN", "PPER", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dann w\u00fcrd' ich hinabgezogen in Vernichtung immerdar.", "tokens": ["Dann", "w\u00fcrd'", "ich", "hin\u00b7ab\u00b7ge\u00b7zo\u00b7gen", "in", "Ver\u00b7nich\u00b7tung", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "++--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Da durch all' das dumpfe Rauschen scholl's wie Silberglockenklang,", "tokens": ["Da", "durch", "all'", "das", "dump\u00b7fe", "Rau\u00b7schen", "scholl's", "wie", "Sil\u00b7ber\u00b7glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "ART", "ADJA", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Da\u00df mein Herz zu s\u00fc\u00dfem Lauschen rasch vom Todesschlummer sprang.", "tokens": ["Da\u00df", "mein", "Herz", "zu", "s\u00fc\u00b7\u00dfem", "Lau\u00b7schen", "rasch", "vom", "To\u00b7des\u00b7schlum\u00b7mer", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Neues Leben f\u00fchlt' ich glimmen in des Blutes hei\u00dfem Lauf,", "tokens": ["Neu\u00b7es", "Le\u00b7ben", "f\u00fchlt'", "ich", "glim\u00b7men", "in", "des", "Blu\u00b7tes", "hei\u00b7\u00dfem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und die lieblichste der Stimmen rief mir leise: \u00bbKind, steh, auf!\u00ab", "tokens": ["Und", "die", "lieb\u00b7lichs\u00b7te", "der", "Stim\u00b7men", "rief", "mir", "lei\u00b7se", ":", "\u00bb", "Kind", ",", "steh", ",", "auf", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "ART", "NN", "VVFIN", "PPER", "ADJD", "$.", "$(", "NN", "$,", "VVFIN", "$,", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Da, mit unsichtbaren H\u00e4nden, hob mich's aus dem Sarg empor:", "tokens": ["Da", ",", "mit", "un\u00b7sicht\u00b7ba\u00b7ren", "H\u00e4n\u00b7den", ",", "hob", "mich's", "aus", "dem", "Sarg", "em\u00b7por", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Licht f\u00fchlt' ich mein Auge blenden, wie ich's nie gekannt zuvor.", "tokens": ["Licht", "f\u00fchlt'", "ich", "mein", "Au\u00b7ge", "blen\u00b7den", ",", "wie", "ich's", "nie", "ge\u00b7kannt", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PIS", "ADV", "VVPP", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.10": {"line.1": {"text": "Und ein J\u00fcngling, mild zu schauen, stand vor mir ernst, still und rein,", "tokens": ["Und", "ein", "J\u00fcng\u00b7ling", ",", "mild", "zu", "schau\u00b7en", ",", "stand", "vor", "mir", "ernst", ",", "still", "und", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$,", "VVFIN", "APPR", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und von seinen lichten Brauen sto\u00df ein Glanz wie Sternenschein.", "tokens": ["Und", "von", "sei\u00b7nen", "lich\u00b7ten", "Brau\u00b7en", "sto\u00df", "ein", "Glanz", "wie", "Ster\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "KOKOM", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.11": {"line.1": {"text": "Jesus war's, der \u00bbGalil\u00e4er\u00ab von des Volkes Spott genannt:", "tokens": ["Je\u00b7sus", "wa\u00b7r's", ",", "der", "\u00bb", "Ga\u00b7li\u00b7l\u00e4\u00b7er", "\u00ab", "von", "des", "Vol\u00b7kes", "Spott", "ge\u00b7nannt", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "ART", "$(", "NN", "$(", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Doch ich wei\u00df, dem Himmel n\u00e4her war ich, als er vor mir stand!", "tokens": ["Doch", "ich", "wei\u00df", ",", "dem", "Him\u00b7mel", "n\u00e4\u00b7her", "war", "ich", ",", "als", "er", "vor", "mir", "stand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "ADJD", "VAFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.12": {"line.1": {"text": "Was der Pharis\u00e4er sage, was da zischen Neid und Hohn,", "tokens": ["Was", "der", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", "sa\u00b7ge", ",", "was", "da", "zi\u00b7schen", "Neid", "und", "Hohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ich \u2013 an jedem Herzensschlage f\u00fchl' ich's: Er ist Gottes Sohn!", "tokens": ["Ich", "\u2013", "an", "je\u00b7dem", "Her\u00b7zens\u00b7schla\u00b7ge", "f\u00fchl'", "ich's", ":", "Er", "ist", "Got\u00b7tes", "Sohn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "APPR", "PIAT", "NN", "VVFIN", "PIS", "$.", "PPER", "VAFIN", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.13": {"line.1": {"text": "Tod, nun ist dein Schmerz genommen, gern will ich nun sterben gehn,", "tokens": ["Tod", ",", "nun", "ist", "dein", "Schmerz", "ge\u00b7nom\u00b7men", ",", "gern", "will", "ich", "nun", "ster\u00b7ben", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wei\u00df ich doch, der Tag wird kommen, da ich ihn soll wiedersehn!", "tokens": ["Wei\u00df", "ich", "doch", ",", "der", "Tag", "wird", "kom\u00b7men", ",", "da", "ich", "ihn", "soll", "wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VAFIN", "VVINF", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.14": {"line.1": {"text": "Ja, das Grab ist nur die Pforte, die mich f\u00fchrt zu ihm hinauf:", "tokens": ["Ja", ",", "das", "Grab", "ist", "nur", "die", "Pfor\u00b7te", ",", "die", "mich", "f\u00fchrt", "zu", "ihm", "hin\u00b7auf", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ich vertraue seinem Worte, und er weckt mich wieder auf.", "tokens": ["Ich", "ver\u00b7trau\u00b7e", "sei\u00b7nem", "Wor\u00b7te", ",", "und", "er", "weckt", "mich", "wie\u00b7der", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.15": {"line.1": {"text": "Nicht wie all' die tausend andern, die sein Wort vom Tod entband, \u2013", "tokens": ["Nicht", "wie", "all'", "die", "tau\u00b7send", "an\u00b7dern", ",", "die", "sein", "Wort", "vom", "Tod", "ent\u00b7band", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "KOKOM", "PIS", "ART", "CARD", "ADJA", "$,", "PRELS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Trauter werd' ich mit ihm wandern: denn ich bin ihm wohlbekannt.", "tokens": ["Trau\u00b7ter", "werd'", "ich", "mit", "ihm", "wan\u00b7dern", ":", "denn", "ich", "bin", "ihm", "wohl\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "$.", "KON", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.16": {"line.1": {"text": "Wieder wird durch Nacht und Schweigen dringen dann sein holder Ruf,", "tokens": ["Wie\u00b7der", "wird", "durch", "Nacht", "und", "Schwei\u00b7gen", "drin\u00b7gen", "dann", "sein", "hol\u00b7der", "Ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "KON", "NN", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wieder wird die Macht er zeigen, die mich neu zum Leben schuf.", "tokens": ["Wie\u00b7der", "wird", "die", "Macht", "er", "zei\u00b7gen", ",", "die", "mich", "neu", "zum", "Le\u00b7ben", "schuf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$,", "PRELS", "PRF", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.17": {"line.1": {"text": "L\u00e4chelnd wird er wieder stehen an des offnen Grabes Rand", "tokens": ["L\u00e4\u00b7chelnd", "wird", "er", "wie\u00b7der", "ste\u00b7hen", "an", "des", "off\u00b7nen", "Gra\u00b7bes", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und zu ew'gem Wiedersehen reicht er mir die milde Hand.", "tokens": ["Und", "zu", "ew'\u00b7gem", "Wie\u00b7der\u00b7se\u00b7hen", "reicht", "er", "mir", "die", "mil\u00b7de", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", 