{"dta.poem.7892": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4gerlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Kein\u2019 be\u00dfre Lust in dieser Zeit,", "tokens": ["Kein'", "be\u00df\u00b7re", "Lust", "in", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als durch den Wald zu dringen,", "tokens": ["Als", "durch", "den", "Wald", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo Drossel singt und Habicht schreit,", "tokens": ["Wo", "Dros\u00b7sel", "singt", "und", "Ha\u00b7bicht", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Hirsch\u2019 und Rehe springen.", "tokens": ["Wo", "Hirsch'", "und", "Re\u00b7he", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O s\u00e4\u00df\u2019 mein Lieb im Wipfel gr\u00fcn,", "tokens": ["O", "s\u00e4\u00df'", "mein", "Lieb", "im", "Wip\u00b7fel", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Th\u00e4t wie \u2019ne Drossel schlagen!", "tokens": ["Th\u00e4t", "wie", "'ne", "Dros\u00b7sel", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O spr\u00e4ng\u2019 es, wie ein Reh, dahin,", "tokens": ["O", "spr\u00e4ng'", "es", ",", "wie", "ein", "Reh", ",", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich es k\u00f6nnte jagen!", "tokens": ["Da\u00df", "ich", "es", "k\u00f6nn\u00b7te", "ja\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}