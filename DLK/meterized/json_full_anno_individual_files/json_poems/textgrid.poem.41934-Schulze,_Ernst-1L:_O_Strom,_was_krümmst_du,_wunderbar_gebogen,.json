{"textgrid.poem.41934": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Strom, was kr\u00fcmmst du, wunderbar gebogen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Strom, was kr\u00fcmmst du, wunderbar gebogen,", "tokens": ["O", "Strom", ",", "was", "kr\u00fcmmst", "du", ",", "wun\u00b7der\u00b7bar", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VVFIN", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich rasch dahin durch deinen sch\u00f6nen Strand,", "tokens": ["Dich", "rasch", "da\u00b7hin", "durch", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PAV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zur Seite bald und bald zur\u00fcckgewandt,", "tokens": ["Zur", "Sei\u00b7te", "bald", "und", "bald", "zu\u00b7r\u00fcck\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Ufer jetzt und jetzt durch dich betrogen?", "tokens": ["Vom", "U\u00b7fer", "jetzt", "und", "jetzt", "durch", "dich", "be\u00b7tro\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "KON", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gern gr\u00fc\u00dften wohl noch einmal deine Wogen", "tokens": ["Gern", "gr\u00fc\u00df\u00b7ten", "wohl", "noch", "ein\u00b7mal", "dei\u00b7ne", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ersten Kindheit holdes Vaterland;", "tokens": ["Der", "ers\u00b7ten", "Kind\u00b7heit", "hol\u00b7des", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch willenlos in's enge Thal gebannt,", "tokens": ["Doch", "wil\u00b7len\u00b7los", "in's", "en\u00b7ge", "Thal", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wirst du von st\u00e4rkrer Macht hinabgezogen.", "tokens": ["Wirst", "du", "von", "st\u00e4r\u00b7krer", "Macht", "hin\u00b7ab\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Stets tiefer wird und breiter deine Fluth;", "tokens": ["Stets", "tie\u00b7fer", "wird", "und", "brei\u00b7ter", "dei\u00b7ne", "Fluth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es regt der Mensch auf deinem glatten Spiegel", "tokens": ["Es", "regt", "der", "Mensch", "auf", "dei\u00b7nem", "glat\u00b7ten", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-