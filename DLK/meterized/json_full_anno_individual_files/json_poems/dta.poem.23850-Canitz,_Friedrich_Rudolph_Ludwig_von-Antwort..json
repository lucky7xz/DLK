{"dta.poem.23850": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Antwort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Als jener R\u00f6mer mich zur R\u00f6merin erwehlte/", "tokens": ["Als", "je\u00b7ner", "R\u00f6\u00b7mer", "mich", "zur", "R\u00f6\u00b7me\u00b7rin", "er\u00b7wehl\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den seine Tapferkeit mehr als sein Purpur", "tokens": ["Den", "sei\u00b7ne", "Tap\u00b7fer\u00b7keit", "mehr", "als", "sein", "Pur\u00b7pur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "PIAT", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+--+---+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da dacht ich/ weil mir nichts an Ehr und Freude fehlte/", "tokens": ["Da", "dacht", "ich", "/", "weil", "mir", "nichts", "an", "Ehr", "und", "Freu\u00b7de", "fehl\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PIS", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich w\u00e4re dieses Jahr vollkommen schon begl\u00fcckt.", "tokens": ["Ich", "w\u00e4\u00b7re", "die\u00b7ses", "Jahr", "voll\u00b7kom\u00b7men", "schon", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drum la\u00df ich wie im Traum das angenehme Schreiben/", "tokens": ["Drum", "la\u00df", "ich", "wie", "im", "Traum", "das", "an\u00b7ge\u00b7neh\u00b7me", "Schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KOKOM", "APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch welches mir ein Printz/ den Cron und Zepter ziert/", "tokens": ["Durch", "wel\u00b7ches", "mir", "ein", "Printz", "/", "den", "Cron", "und", "Zep\u00b7ter", "ziert", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus Ernst/ und nicht im Spiel/ um ewig mein zu bleiben/", "tokens": ["Aus", "Ernst", "/", "und", "nicht", "im", "Spiel", "/", "um", "e\u00b7wig", "mein", "zu", "blei\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "KON", "PTKNEG", "APPRART", "NN", "$(", "APPR", "ADJD", "PPOSAT", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zwar von werther Hand/ ward gestern zugef\u00fchrt.", "tokens": ["Und", "zwar", "von", "wert\u00b7her", "Hand", "/", "ward", "ge\u00b7stern", "zu\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$(", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich hab ihn willig auf- und danckbar angenommen/", "tokens": ["Ich", "hab", "ihn", "wil\u00b7lig", "auf", "und", "dan\u00b7ck\u00b7bar", "an\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "TRUNC", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und glaube da\u00df mein Gl\u00fcck nunmehr am h\u00f6chsten ist", "tokens": ["Und", "glau\u00b7be", "da\u00df", "mein", "Gl\u00fcck", "nun\u00b7mehr", "am", "h\u00f6chs\u00b7ten", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "PPOSAT", "NN", "ADV", "APPRART", "ADJA", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was k\u00f6nte sonst aus Rom f\u00fcr mich mehr gutes kom-", "tokens": ["Was", "k\u00f6n\u00b7te", "sonst", "aus", "Rom", "f\u00fcr", "mich", "mehr", "gu\u00b7tes", "kom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "NE", "APPR", "PPER", "PIAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch komme was da wil/ nur nicht der Antichrist.", "tokens": ["Doch", "kom\u00b7me", "was", "da", "wil", "/", "nur", "nicht", "der", "An\u00b7ti\u00b7ch\u00b7rist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "ADV", "VMFIN", "$(", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}