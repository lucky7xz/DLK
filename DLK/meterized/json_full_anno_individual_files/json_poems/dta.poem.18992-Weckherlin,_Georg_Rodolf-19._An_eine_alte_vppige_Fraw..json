{"dta.poem.18992": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "19.  \n  An eine alte vppige Fraw.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Was dienet deine brunst/ dein muhtwill vnd ver-", "tokens": ["Was", "die\u00b7net", "dei\u00b7ne", "brunst", "/", "dein", "muht\u00b7will", "vnd", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$(", "PPOSAT", "ADJD", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "langen?", "tokens": ["lan\u00b7gen", "?"], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Dein Som\u0303er ist dahin/ dein Herbst ist eingebracht/", "tokens": ["Dein", "Som\u0303er", "ist", "da\u00b7hin", "/", "dein", "Herbst", "ist", "ein\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PAV", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dein Winter auff dem hal\u00df: vmbsunst ist diser", "tokens": ["Dein", "Win\u00b7ter", "auff", "dem", "hal\u00df", ":", "vmbsunst", "ist", "di\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$.", "ADV", "VAFIN", "PDS"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "pracht/", "tokens": ["pracht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Nim doch hinweg den busch/ lassz ab von deinem", "tokens": ["Nim", "doch", "hin\u00b7weg", "den", "busch", "/", "lassz", "ab", "von", "dei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "ART", "NN", "$(", "VVIMP", "PTKVZ", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "prangen.", "tokens": ["pran\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Was reich vnd sch\u00f6n vmb dich mag einen geitzhal\u00df", "tokens": ["Was", "reich", "vnd", "sch\u00f6n", "vmb", "dich", "mag", "ei\u00b7nen", "geitz\u00b7hal\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "KON", "ADJD", "APPR", "PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "fangen/", "tokens": ["fan\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wa deines leibs gest\u00e4ud/ wa deiner augen nacht/", "tokens": ["Wa", "dei\u00b7nes", "leibs", "ge\u00b7st\u00e4ud", "/", "wa", "dei\u00b7ner", "au\u00b7gen", "nacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$(", "XY", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wa deines munds saphir/ vnd deines athems", "tokens": ["Wa", "dei\u00b7nes", "munds", "sa\u00b7phir", "/", "vnd", "dei\u00b7nes", "at\u00b7hems"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "$(", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "macht/", "tokens": ["macht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Deiner brust Corduan/ vn\u0303 das gold deiner wange\u0304/", "tokens": ["Dei\u00b7ner", "brust", "Cor\u00b7du\u00b7an", "/", "v\u00f1", "das", "gold", "dei\u00b7ner", "wang\u0113", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$(", "APPR", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+-+--++-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wa dein von bein vnd haut/ flaischloses angesicht", "tokens": ["Wa", "dein", "von", "bein", "vnd", "haut", "/", "flaischlo\u00b7ses", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPOSAT", "APPR", "NE", "KON", "VVFIN", "$(", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht dein Ich wai\u00df nicht was/ vnd ehr so wol", "tokens": ["Nicht", "dein", "Ich", "wai\u00df", "nicht", "was", "/", "vnd", "ehr", "so", "wol"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "PPER", "VVFIN", "PTKNEG", "PWS", "$(", "KON", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "bewahren/", "tokens": ["be\u00b7wah\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Da\u00df seine Lieb gewi\u00df/ wie dein lob/ ein gedicht.", "tokens": ["Da\u00df", "sei\u00b7ne", "Lieb", "ge\u00b7wi\u00df", "/", "wie", "dein", "lob", "/", "ein", "ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$(", "KOKOM", "PPOSAT", "NN", "$(", "ART", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch wan er seine lieb/ dir sich zu offenbahren", "tokens": ["Doch", "wan", "er", "sei\u00b7ne", "lieb", "/", "dir", "sich", "zu", "of\u00b7fen\u00b7bah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PPOSAT", "ADJD", "$(", "PPER", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(meinaydig) schw\u00f6r\u1ebd solt/ So liebet er doch nicht", "tokens": ["(", "mein\u00b7ay\u00b7dig", ")", "schw\u00f6r\u1ebd", "solt", "/", "So", "lie\u00b7bet", "er", "doch", "nicht"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$(", "ADJD", "VMFIN", "$(", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie deines benttels gold das silber deiner haaren.", "tokens": ["Wie", "dei\u00b7nes", "bent\u00b7tels", "gold", "das", "sil\u00b7ber", "dei\u00b7ner", "haa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}