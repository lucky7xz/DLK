{"dta.poem.6570": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um aber besser noch zu fassen, wie die Z\u00fcge", "tokens": ["Um", "a\u00b7ber", "bes\u00b7ser", "noch", "zu", "fas\u00b7sen", ",", "wie", "die", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ADV", "ADJD", "ADV", "PTKZU", "VVINF", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Formiret sind: auf welche Weise sie", "tokens": ["For\u00b7mi\u00b7ret", "sind", ":", "auf", "wel\u00b7che", "Wei\u00b7se", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "$.", "APPR", "PWAT", "NN", "PPER"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Den Augen", "tokens": ["Den", "Au\u00b7gen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Sich einzudr\u00fccken taugen;", "tokens": ["Sich", "ein\u00b7zu\u00b7dr\u00fc\u00b7cken", "tau\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erwege man, da\u00df eine Harmonie,", "tokens": ["Er\u00b7we\u00b7ge", "man", ",", "da\u00df", "ei\u00b7ne", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Recht wie in der Musie, so auch in Farben, liege.", "tokens": ["Recht", "wie", "in", "der", "Mu\u00b7sie", ",", "so", "auch", "in", "Far\u00b7ben", ",", "lie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "ART", "NN", "$,", "ADV", "ADV", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des holden Lichtes helle Strahlen", "tokens": ["Des", "hol\u00b7den", "Lich\u00b7tes", "hel\u00b7le", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Besitzen in sich selbst, und hegen", "tokens": ["Be\u00b7sit\u00b7zen", "in", "sich", "selbst", ",", "und", "he\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PRF", "ADV", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein zitterndes Bewegen;", "tokens": ["Ein", "zit\u00b7tern\u00b7des", "Be\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df durch die Gleich-Maa\u00df und die Zahlen,", "tokens": ["Da\u00df", "durch", "die", "Gleich\u00b7Maa\u00df", "und", "die", "Zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Durch die Vereinigung und durch das Widerstreben", "tokens": ["Durch", "die", "Ver\u00b7ei\u00b7ni\u00b7gung", "und", "durch", "das", "Wi\u00b7der\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der C\u00f6rper, die man sieht", "tokens": ["Der", "C\u00f6r\u00b7per", ",", "die", "man", "sieht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Besondren Unterschied", "tokens": ["Be\u00b7sond\u00b7ren", "Un\u00b7ter\u00b7schied"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Uns deutlich zu erkennen geben.", "tokens": ["Uns", "deut\u00b7lich", "zu", "er\u00b7ken\u00b7nen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Des Mahlers Hand beseelt mit Farben, die fast leben,", "tokens": ["Des", "Mah\u00b7lers", "Hand", "be\u00b7seelt", "mit", "Far\u00b7ben", ",", "die", "fast", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sein sch\u00f6n und k\u00fcnstlich Werck. Der weitber\u00fchmten Meister.", "tokens": ["Sein", "sch\u00f6n", "und", "k\u00fcnst\u00b7lich", "Werck", ".", "Der", "weit\u00b7be\u00b7r\u00fchm\u00b7ten", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "KON", "ADJD", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "In ihrer sch\u00f6nen Kunst tief eingedrungne Geister", "tokens": ["In", "ih\u00b7rer", "sch\u00f6\u00b7nen", "Kunst", "tief", "ein\u00b7ge\u00b7drung\u00b7ne", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Erfanden, nach viel Flei\u00df und M\u00fch,", "tokens": ["Er\u00b7fan\u00b7den", ",", "nach", "viel", "Flei\u00df", "und", "M\u00fch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der Farben holde Harmonie.", "tokens": ["Der", "Far\u00b7ben", "hol\u00b7de", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ja durch der Farben Ton zieht, recht verwunderlich,", "tokens": ["Ja", "durch", "der", "Far\u00b7ben", "Ton", "zieht", ",", "recht", "ver\u00b7wun\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "NN", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die nachgeahmete Natur", "tokens": ["Die", "nach\u00b7geah\u00b7me\u00b7te", "Na\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Fast jedes Aug\u2019 an sich.", "tokens": ["Fast", "je\u00b7des", "Aug'", "an", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}