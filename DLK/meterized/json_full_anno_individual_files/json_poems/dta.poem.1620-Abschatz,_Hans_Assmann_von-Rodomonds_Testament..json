{"dta.poem.1620": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Rodomonds Testament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem das Lebens-Oel in hei\u00dfer Glutt verzehret/", "tokens": ["Nach\u00b7dem", "das", "Le\u00b7bens\u00b7O\u00b7el", "in", "hei\u00b7\u00dfer", "Glutt", "ver\u00b7zeh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mein entmarckter Leib zu leben nicht begehret/", "tokens": ["Und", "mein", "ent\u00b7marck\u00b7ter", "Leib", "zu", "le\u00b7ben", "nicht", "be\u00b7ge\u00b7hret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Fu\u00df ich den lezten Schlu\u00df in wenig Zeilen ein/", "tokens": ["Fu\u00df", "ich", "den", "lez\u00b7ten", "Schlu\u00df", "in", "we\u00b7nig", "Zei\u00b7len", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und soll die j\u00fcngste Bitt an meinen Herren seyn/", "tokens": ["Und", "soll", "die", "j\u00fcngs\u00b7te", "Bitt", "an", "mei\u00b7nen", "Her\u00b7ren", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Um mir/ erzeigter Lieb und treuen Dienste willen/", "tokens": ["Um", "mir", "/", "er\u00b7zeig\u00b7ter", "Lieb", "und", "treu\u00b7en", "Diens\u00b7te", "wil\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$(", "ADJA", "NN", "KON", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was mein halb todter Geist begehret/ zu erf\u00fcllen.", "tokens": ["Was", "mein", "halb", "tod\u00b7ter", "Geist", "be\u00b7ge\u00b7hret", "/", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJD", "ADJA", "NN", "VVFIN", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Der Seele: die bey mir seit ihrer ersten Flucht", "tokens": ["Der", "See\u00b7le", ":", "die", "bey", "mir", "seit", "ih\u00b7rer", "ers\u00b7ten", "Flucht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Die sechsmahl-zehnde Stell und Wohnung ihr gesucht/", "tokens": ["Die", "sechs\u00b7mahl\u00b7zehn\u00b7de", "Stell", "und", "Woh\u00b7nung", "ihr", "ge\u00b7sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die manches Helden Leib vor diesem hat belebet/", "tokens": ["Die", "man\u00b7ches", "Hel\u00b7den", "Leib", "vor", "die\u00b7sem", "hat", "be\u00b7le\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "APPR", "PDAT", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Wechsels-wei\u00df um Thier und Menschen hat geschwebet/", "tokens": ["Die", "Wech\u00b7sels\u00b7wei\u00df", "um", "Thier", "und", "Men\u00b7schen", "hat", "ge\u00b7schwe\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die bald ein munter Pferd/ bald ein gef\u00fcrchtes Schwein/", "tokens": ["Die", "bald", "ein", "mun\u00b7ter", "Pferd", "/", "bald", "ein", "ge\u00b7f\u00fcrch\u00b7tes", "Schwein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$(", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bald ein beherzter B\u00e4r und Lewe m\u00fcssen seyn/", "tokens": ["Bald", "ein", "be\u00b7herz\u00b7ter", "B\u00e4r", "und", "Le\u00b7we", "m\u00fcs\u00b7sen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "NE", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bi\u00df da\u00df sie mit der Zeit in meinen C\u00f6rper kommen;", "tokens": ["Bi\u00df", "da\u00df", "sie", "mit", "der", "Zeit", "in", "mei\u00b7nen", "C\u00f6r\u00b7per", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nun das Verh\u00e4ngnis ihr auch diesen Sitz genommen/", "tokens": ["Nun", "das", "Ver\u00b7h\u00e4ng\u00b7nis", "ihr", "auch", "die\u00b7sen", "Sitz", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "ADV", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nimmt sich kein ander Thier mit meinem Willen an/", "tokens": ["Nimmt", "sich", "kein", "an\u00b7der", "Thier", "mit", "mei\u00b7nem", "Wil\u00b7len", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "ADJD", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als Mavors eignes Kind/ ein kriegerischer Han/", "tokens": ["Als", "Ma\u00b7vors", "eig\u00b7nes", "Kind", "/", "ein", "krie\u00b7ge\u00b7ri\u00b7scher", "Han", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der sich im Streit und Kampff nach meinen Sitten \u00fcbet/", "tokens": ["Der", "sich", "im", "Streit", "und", "Kampff", "nach", "mei\u00b7nen", "Sit\u00b7ten", "\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und muntre Wachsamkeit vor saule Ruhe liebet.", "tokens": ["Und", "mun\u00b7tre", "Wach\u00b7sam\u00b7keit", "vor", "sau\u00b7le", "Ru\u00b7he", "lie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mein treues Hertze schreibt die wahre Danckbarkeit", "tokens": ["Mein", "treu\u00b7es", "Hert\u00b7ze", "schreibt", "die", "wah\u00b7re", "Dan\u00b7ck\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.20": {"text": "Dem werthen Herren zu/ bey dem ich meine Zeit", "tokens": ["Dem", "wert\u00b7hen", "Her\u00b7ren", "zu", "/", "bey", "dem", "ich", "mei\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "$(", "APPR", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Vergn\u00fcgend zugebracht. Das Fell will ich bescheiden/", "tokens": ["Ver\u00b7gn\u00fc\u00b7gend", "zu\u00b7ge\u00b7bracht", ".", "Das", "Fell", "will", "ich", "be\u00b7schei\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$.", "ART", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nach jenes K\u00e4mpffers Art/ die Drummel zu bekleiden/", "tokens": ["Nach", "je\u00b7nes", "K\u00e4mpf\u00b7fers", "Art", "/", "die", "Drum\u00b7mel", "zu", "be\u00b7klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$(", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die wider meine Feind in steten Krieg soll ziehn/", "tokens": ["Die", "wi\u00b7der", "mei\u00b7ne", "Feind", "in", "ste\u00b7ten", "Krieg", "soll", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vor der ein feindlich Heer der Katzen soll entfliehn.", "tokens": ["Vor", "der", "ein", "feind\u00b7lich", "Heer", "der", "Kat\u00b7zen", "soll", "ent\u00b7fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJD", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die weiche Lager-Statt/ des Herren milde Gaben/", "tokens": ["Die", "wei\u00b7che", "La\u00b7ger\u00b7Statt", "/", "des", "Her\u00b7ren", "mil\u00b7de", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Soll Vetter Goliath als Erb und Nachfahr haben/", "tokens": ["Soll", "Vet\u00b7ter", "Go\u00b7li\u00b7ath", "als", "Erb", "und", "Nach\u00b7fahr", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "NE", "KOUS", "NN", "KON", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dem/ der den m\u00fcden Geist der schweren Last entbindt/", "tokens": ["Dem", "/", "der", "den", "m\u00fc\u00b7den", "Geist", "der", "schwe\u00b7ren", "Last", "ent\u00b7bindt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ART", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Bleibt als ein Eigenthum mein lezter Leibes-Wind.", "tokens": ["Bleibt", "als", "ein", "Ei\u00b7gen\u00b7thum", "mein", "lez\u00b7ter", "Lei\u00b7bes\u00b7Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Den hinterlassnen Rest der abgelebten Glieder/", "tokens": ["Den", "hin\u00b7ter\u00b7lass\u00b7nen", "Rest", "der", "ab\u00b7ge\u00b7leb\u00b7ten", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Leg eines Freundes Hand ins Grab fein sanffte nieder/", "tokens": ["Leg", "ei\u00b7nes", "Freun\u00b7des", "Hand", "ins", "Grab", "fein", "sanff\u00b7te", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "APPRART", "NN", "ADJD", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und wo mein treuer Dienst so viel erworben hat/", "tokens": ["Und", "wo", "mein", "treu\u00b7er", "Dienst", "so", "viel", "er\u00b7wor\u00b7ben", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So decke mich manch Myrth- und Oel- und Pappel-Blat.", "tokens": ["So", "de\u00b7cke", "mich", "manch", "Myr\u00b7th", "und", "O\u00b7el", "und", "Pap\u00b7pel\u00b7Blat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "TRUNC", "KON", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "So wird mich Cerberus mit vollem Hals empfahen/", "tokens": ["So", "wird", "mich", "Cer\u00b7be\u00b7rus", "mit", "vol\u00b7lem", "Hals", "em\u00b7pfa\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wenn sich mein Schatten wird zu seiner H\u00fctte nahen/", "tokens": ["Wenn", "sich", "mein", "Schat\u00b7ten", "wird", "zu", "sei\u00b7ner", "H\u00fct\u00b7te", "na\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So werd ich Hecatens Gefert und Diener seyn/", "tokens": ["So", "werd", "ich", "He\u00b7ca\u00b7tens", "Ge\u00b7fert", "und", "Die\u00b7ner", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "VVPP", "KON", "NN", "VAINF", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Und sich der Ewigkeit mein Nahme schreiben ein/", "tokens": ["Und", "sich", "der", "E\u00b7wig\u00b7keit", "mein", "Nah\u00b7me", "schrei\u00b7ben", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "PPOSAT", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Den ich mit eignem Blutt in diese Zeilen \u00e4tze/", "tokens": ["Den", "ich", "mit", "eig\u00b7nem", "Blutt", "in", "die\u00b7se", "Zei\u00b7len", "\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und da\u00df man selbe nicht vor unvollkommen sch\u00e4tze/", "tokens": ["Und", "da\u00df", "man", "sel\u00b7be", "nicht", "vor", "un\u00b7voll\u00b7kom\u00b7men", "sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "PTKNEG", "APPR", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So zeuge/ wer du di\u00df Papir mit Augen siehst/", "tokens": ["So", "zeu\u00b7ge", "/", "wer", "du", "di\u00df", "Pa\u00b7pir", "mit", "Au\u00b7gen", "siehst", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PWS", "PPER", "PDS", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df drauff mein lezter Will und Schlu\u00df verfasset ist.", "tokens": ["Da\u00df", "drauff", "mein", "lez\u00b7ter", "Will", "und", "Schlu\u00df", "ver\u00b7fas\u00b7set", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "PPOSAT", "ADJA", "NN", "KON", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}