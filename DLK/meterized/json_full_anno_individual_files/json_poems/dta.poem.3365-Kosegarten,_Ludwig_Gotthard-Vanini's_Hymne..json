{"dta.poem.3365": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Vanini's Hymne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durchweht von Dessen Odem, der ewig lebt,", "tokens": ["Durch\u00b7weht", "von", "Des\u00b7sen", "O\u00b7dem", ",", "der", "e\u00b7wig", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von dessen Gluth gez\u00fcndet, der nie erlischt,", "tokens": ["Von", "des\u00b7sen", "Gluth", "ge\u00b7z\u00fcn\u00b7det", ",", "der", "nie", "er\u00b7lischt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VVPP", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Entbrennt die Seele, schwingt den Fittig,", "tokens": ["Ent\u00b7brennt", "die", "See\u00b7le", ",", "schwingt", "den", "Fit\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Steiget in nimmererflogne H\u00f6hen,", "tokens": ["Stei\u00b7get", "in", "nim\u00b7me\u00b7rer\u00b7flog\u00b7ne", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Und strebet m\u00fchsam aufw\u00e4rts zum Throne Dess,", "tokens": ["Und", "stre\u00b7bet", "m\u00fch\u00b7sam", "auf\u00b7w\u00e4rts", "zum", "Thro\u00b7ne", "Dess", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den keine Zunge nannte, kein Hymnus sang,", "tokens": ["Den", "kei\u00b7ne", "Zun\u00b7ge", "nann\u00b7te", ",", "kein", "Hym\u00b7nus", "sang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den keine Schranke gr\u00e4nzt noch enget,", "tokens": ["Den", "kei\u00b7ne", "Schran\u00b7ke", "gr\u00e4nzt", "noch", "en\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht des Beginns, noch des Endens Schranke.", "tokens": ["Nicht", "des", "Be\u00b7ginns", ",", "noch", "des", "En\u00b7dens", "Schran\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "ADV", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Er ist der Wesen Urgrund, und ist ihr Ziel,", "tokens": ["Er", "ist", "der", "We\u00b7sen", "Ur\u00b7grund", ",", "und", "ist", "ihr", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "KON", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sein eigner ewger Urgrund, sein eignes Ziel,", "tokens": ["Sein", "eig\u00b7ner", "ew\u00b7ger", "Ur\u00b7grund", ",", "sein", "eig\u00b7nes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Beginnt, begr\u00e4nzt, beschr\u00e4nkt sich selber,", "tokens": ["Be\u00b7ginnt", ",", "be\u00b7gr\u00e4nzt", ",", "be\u00b7schr\u00e4nkt", "sich", "sel\u00b7ber", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gr\u00e4nzenlos zwar, und beginn- und endlos.", "tokens": ["Gr\u00e4n\u00b7zen\u00b7los", "zwar", ",", "und", "be\u00b7ginn", "und", "end\u00b7los", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KON", "TRUNC", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ganz, ungetheilt, untheilbar, und unverr\u00fcckt,", "tokens": ["Ganz", ",", "un\u00b7ge\u00b7theilt", ",", "un\u00b7theil\u00b7bar", ",", "und", "un\u00b7ver\u00b7r\u00fcckt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,", "ADJD", "$,", "KON", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erf\u00fcllt sein Wesen jeglichen Atomus", "tokens": ["Er\u00b7f\u00fcllt", "sein", "We\u00b7sen", "jeg\u00b7li\u00b7chen", "A\u00b7to\u00b7mus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PIAT", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Des ungemessnen Raums und jeden", "tokens": ["Des", "un\u00b7ge\u00b7mess\u00b7nen", "Raums", "und", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stiebenden Tropfen des Zeitenstromes.", "tokens": ["Stie\u00b7ben\u00b7den", "Trop\u00b7fen", "des", "Zei\u00b7ten\u00b7stro\u00b7mes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Ihn decken hohe Tempelgew\u00f6lbe nicht.", "tokens": ["Ihn", "de\u00b7cken", "ho\u00b7he", "Tem\u00b7pel\u00b7ge\u00b7w\u00f6l\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihn fassen nicht die Himmel, die Erden nicht.", "tokens": ["Ihn", "fas\u00b7sen", "nicht", "die", "Him\u00b7mel", ",", "die", "Er\u00b7den", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Frey, unumh\u00fcllet, ungefesselt", "tokens": ["Frey", ",", "u\u00b7num\u00b7h\u00fcl\u00b7let", ",", "un\u00b7ge\u00b7fes\u00b7selt"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Waltet und herrscht er im grossen Alle.", "tokens": ["Wal\u00b7tet", "und", "herrscht", "er", "im", "gros\u00b7sen", "Al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Sein Will' ist That. Wer steuert dem M\u00e4chtigen?", "tokens": ["Sein", "Will'", "ist", "That", ".", "Wer", "steu\u00b7ert", "dem", "M\u00e4ch\u00b7ti\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$.", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wer hemmt den Unr\u00fcckrufbaren? Gross ist er", "tokens": ["Wer", "hemmt", "den", "Un\u00b7r\u00fcck\u00b7ruf\u00b7ba\u00b7ren", "?", "Gross", "ist", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "NN", "VAFIN", "PPER"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gut; nicht mit der Messkunst Gr\u00f6ssen,", "tokens": ["Und", "gut", ";", "nicht", "mit", "der", "Mess\u00b7kunst", "Gr\u00f6s\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht mit der G\u00fcte der Sittenlehren.", "tokens": ["Nicht", "mit", "der", "G\u00fc\u00b7te", "der", "Sit\u00b7ten\u00b7leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Stracks, flugs, im Hui geschiehet, was er gebeut.", "tokens": ["Stracks", ",", "flugs", ",", "im", "Hui", "ge\u00b7schie\u00b7het", ",", "was", "er", "ge\u00b7beut", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "$,", "APPRART", "NN", "VVPP", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Weltall schlief des eisernen Nichtseyns Schlaf.", "tokens": ["Das", "Wel\u00b7tall", "schlief", "des", "ei\u00b7ser\u00b7nen", "Nich\u00b7tseyns", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er rief: Erwache! Schnellerwachend", "tokens": ["Er", "rief", ":", "Er\u00b7wa\u00b7che", "!", "Schnel\u00b7ler\u00b7wa\u00b7chend"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "$.", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rafft' es sich auf, und erstaunt' und kniete.", "tokens": ["Rafft'", "es", "sich", "auf", ",", "und", "er\u00b7staunt'", "und", "knie\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Sein alldurchdringend Auge durchschaut das All,", "tokens": ["Sein", "all\u00b7durch\u00b7drin\u00b7gend", "Au\u00b7ge", "durch\u00b7schaut", "das", "All", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und hegt und tr\u00e4gt, bewahret und w\u00e4rmet es.", "tokens": ["Und", "hegt", "und", "tr\u00e4gt", ",", "be\u00b7wah\u00b7ret", "und", "w\u00e4r\u00b7met", "es", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Allm\u00e4chtig herrscht sein Wink, allm\u00e4chtig", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "herrscht", "sein", "Wink", ",", "all\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Waltet des Schrecklichen hohe Brane.", "tokens": ["Wal\u00b7tet", "des", "Schreck\u00b7li\u00b7chen", "ho\u00b7he", "Bra\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Dich fleh' ich, Guter! l\u00e4chel' auf mich herab!", "tokens": ["Dich", "fleh'", "ich", ",", "Gu\u00b7ter", "!", "l\u00e4\u00b7chel'", "auf", "mich", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "+--+------+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mit Demantketten binde mich fest an dich!", "tokens": ["Mit", "De\u00b7mant\u00b7ket\u00b7ten", "bin\u00b7de", "mich", "fest", "an", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-----+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bey dir, bey dir ist volle Gn\u00fcge,", "tokens": ["Bey", "dir", ",", "bey", "dir", "ist", "vol\u00b7le", "Gn\u00fc\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einzig bey dir, und bey keinem andern!", "tokens": ["Ein\u00b7zig", "bey", "dir", ",", "und", "bey", "kei\u00b7nem", "an\u00b7dern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,", "KON", "APPR", "PIAT", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Wohl dem, der dich ergreifet, an dich sich h\u00e4ngt,", "tokens": ["Wohl", "dem", ",", "der", "dich", "er\u00b7grei\u00b7fet", ",", "an", "dich", "sich", "h\u00e4ngt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "An dich sich innig schmieget, dich fest umflicht!", "tokens": ["An", "dich", "sich", "in\u00b7nig", "schmie\u00b7get", ",", "dich", "fest", "um\u00b7flicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADJD", "VVFIN", "$,", "PRF", "ADJD", "VVPP", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dich habend, Vater, hat er alles,", "tokens": ["Dich", "ha\u00b7bend", ",", "Va\u00b7ter", ",", "hat", "er", "al\u00b7les", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "NN", "$,", "VAFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Alles, was s\u00e4ttigt, und was beseligt.", "tokens": ["Al\u00b7les", ",", "was", "s\u00e4t\u00b7tigt", ",", "und", "was", "be\u00b7se\u00b7ligt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "VVFIN", "$,", "KON", "PWS", "VVPP", "$."], "meter": "+--+--+---", "measure": "dactylic.tri.plus"}}, "stanza.11": {"line.1": {"text": "Du, du entzeuchst dich keinem, der dein bedarf.", "tokens": ["Du", ",", "du", "ent\u00b7zeuchst", "dich", "kei\u00b7nem", ",", "der", "dein", "be\u00b7darf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VVFIN", "PPER", "PIS", "$,", "PRELS", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Freywillig schenkst du jeglichem jegliches.", "tokens": ["Frey\u00b7wil\u00b7lig", "schenkst", "du", "jeg\u00b7li\u00b7chem", "jeg\u00b7li\u00b7ches", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIAT", "ADJA", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dich selbst, der war, und ist, und seyn wird,", "tokens": ["Dich", "selbst", ",", "der", "war", ",", "und", "ist", ",", "und", "seyn", "wird", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "VAFIN", "$,", "KON", "VAFIN", "$,", "KON", "VAINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ewiger, schenkst du dem frommen Fleher!", "tokens": ["E\u00b7wi\u00b7ger", ",", "schenkst", "du", "dem", "from\u00b7men", "Fle\u00b7her", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}}, "stanza.12": {"line.1": {"text": "Du bist dem M\u00fcherliegenden Nerv' und Mark,", "tokens": ["Du", "bist", "dem", "M\u00fc\u00b7her\u00b7lie\u00b7gen\u00b7den", "Ner\u00b7v'", "und", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und bist dem Klippenscheiternden Bucht und Port,", "tokens": ["Und", "bist", "dem", "Klip\u00b7pen\u00b7schei\u00b7tern\u00b7den", "Bucht", "und", "Port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "KON", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und bist der durstgeborstnen Lippe", "tokens": ["Und", "bist", "der", "durst\u00b7ge\u00b7borst\u00b7nen", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lechzender Wanderer Quellenk\u00fchle.", "tokens": ["Lech\u00b7zen\u00b7der", "Wan\u00b7de\u00b7rer", "Quel\u00b7len\u00b7k\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Du bist der Arbeitseligen s\u00fcsse Ruh,", "tokens": ["Du", "bist", "der", "Ar\u00b7beit\u00b7se\u00b7li\u00b7gen", "s\u00fcs\u00b7se", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bist unsren Busen Frieden und Freudigkeit,", "tokens": ["Bist", "un\u00b7sren", "Bu\u00b7sen", "Frie\u00b7den", "und", "Freu\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bist jeder Sch\u00f6nheit Urgebilde,", "tokens": ["Bist", "je\u00b7der", "Sch\u00f6n\u00b7heit", "Ur\u00b7ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jeglicher Treflichkeit ewge Urform,", "tokens": ["Jeg\u00b7li\u00b7cher", "Tref\u00b7lich\u00b7keit", "ew\u00b7ge", "Ur\u00b7form", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Bist Zahl und Maass, und Zirkel und Harmonie,", "tokens": ["Bist", "Zahl", "und", "Maass", ",", "und", "Zir\u00b7kel", "und", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Pracht und Ordnung, Hoheit und Majest\u00e4t,", "tokens": ["Und", "Pracht", "und", "Ord\u00b7nung", ",", "Ho\u00b7heit", "und", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bist unsre Wonne, unsre Wollust,", "tokens": ["Bist", "uns\u00b7re", "Won\u00b7ne", ",", "uns\u00b7re", "Wol\u00b7lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unsre Ambrosia, unser Nektar.", "tokens": ["Uns\u00b7re", "A\u00b7mbro\u00b7sia", ",", "un\u00b7ser", "Nek\u00b7tar", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "+----+--+", "measure": "iambic.tri.chol"}}, "stanza.15": {"line.1": {"text": "O du, der Wahrheit Richtscheid, des Rechtes Norm,", "tokens": ["O", "du", ",", "der", "Wahr\u00b7heit", "Richt\u00b7scheid", ",", "des", "Rech\u00b7tes", "Norm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Guten Bleyschnur, heiliges Urgesetz,", "tokens": ["Des", "Gu\u00b7ten", "Bley\u00b7schnur", ",", "hei\u00b7li\u00b7ges", "Ur\u00b7ge\u00b7setz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du unsre Hoffnung, unsre Weisheit,", "tokens": ["Du", "uns\u00b7re", "Hoff\u00b7nung", ",", "uns\u00b7re", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leuchtende Fackel des irren Geistes,", "tokens": ["Leuch\u00b7ten\u00b7de", "Fa\u00b7ckel", "des", "ir\u00b7ren", "Geis\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.16": {"line.1": {"text": "Glanz, Lichtstrahl, W\u00fcrde, Hoheit, wie sing' ich dich!", "tokens": ["Glanz", ",", "Licht\u00b7strahl", ",", "W\u00fcr\u00b7de", ",", "Ho\u00b7heit", ",", "wie", "sing'", "ich", "dich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VAFIN", "$,", "NN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Licht, Liebe, Leben, Labsal, wie feyr' ich dich!", "tokens": ["Licht", ",", "Lie\u00b7be", ",", "Le\u00b7ben", ",", "Lab\u00b7sal", ",", "wie", "fey\u00b7r'", "ich", "dich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Summen Summe! All des Allen!", "tokens": ["Der", "Sum\u00b7men", "Sum\u00b7me", "!", "All", "des", "Al\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einziger, Ewiger, Gr\u00f6sster, Bester!", "tokens": ["Ein\u00b7zi\u00b7ger", ",", "E\u00b7wi\u00b7ger", ",", "Gr\u00f6s\u00b7ster", ",", "Bes\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "$,", "NN", "$,", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}