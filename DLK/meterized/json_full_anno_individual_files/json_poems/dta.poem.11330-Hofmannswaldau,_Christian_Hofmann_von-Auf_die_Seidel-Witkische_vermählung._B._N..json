{"dta.poem.11330": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Seidel-Witkische verm\u00e4hlung.  \n  B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund nach der alten zeit! so mag ich dich wohl nennen,", "tokens": ["Freund", "nach", "der", "al\u00b7ten", "zeit", "!", "so", "mag", "ich", "dich", "wohl", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "ADV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wie gl\u00fccklich ist der mann, der so, wie du, gedenckt;", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "ist", "der", "mann", ",", "der", "so", ",", "wie", "du", ",", "ge\u00b7denckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "$,", "PWAV", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der andre k\u00fcnsteln l\u00e4st und nach dem gl\u00fccke rennen,", "tokens": ["Der", "and\u00b7re", "k\u00fcns\u00b7teln", "l\u00e4st", "und", "nach", "dem", "gl\u00fc\u00b7cke", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADJD", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich aber und sein hertz in GOttes rath gesenckt,", "tokens": ["Sich", "a\u00b7ber", "und", "sein", "hertz", "in", "Got\u00b7tes", "rath", "ge\u00b7senckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KON", "PPOSAT", "NN", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nimmt, was der himmel giebt, danckt, wenn er was empfangen,", "tokens": ["Nimmt", ",", "was", "der", "him\u00b7mel", "giebt", ",", "danckt", ",", "wenn", "er", "was", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und willig andern g\u00f6unt, was sie vor ihm erlangen.", "tokens": ["Und", "wil\u00b7lig", "an\u00b7dern", "g\u00f6unt", ",", "was", "sie", "vor", "ihm", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "ADJD", "$,", "PRELS", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So war es eher dem, als noch die pest der zeit,", "tokens": ["So", "war", "es", "e\u00b7her", "dem", ",", "als", "noch", "die", "pest", "der", "zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "$,", "KOUS", "ADV", "ART", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ehrsucht und der geitz, in ihrer kindheit waren.", "tokens": ["Die", "ehr\u00b7sucht", "und", "der", "geitz", ",", "in", "ih\u00b7rer", "kind\u00b7heit", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Heut ist es umgekehrt: Man trotzt die ewigkeit,", "tokens": ["Heut", "ist", "es", "um\u00b7ge\u00b7kehrt", ":", "Man", "trotzt", "die", "e\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und will zu seiner ruh nicht gehen sonder fahren.", "tokens": ["Und", "will", "zu", "sei\u00b7ner", "ruh", "nicht", "ge\u00b7hen", "son\u00b7der", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gott tr\u00e4gt uns langsam fort; Wir legen fl\u00fcgel an:", "tokens": ["Gott", "tr\u00e4gt", "uns", "lang\u00b7sam", "fort", ";", "Wir", "le\u00b7gen", "fl\u00fc\u00b7gel", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er sucht, was n\u00fctzlich ist; Wir, was uns st\u00fcrtzen kan.", "tokens": ["Er", "sucht", ",", "was", "n\u00fctz\u00b7lich", "ist", ";", "Wir", ",", "was", "uns", "st\u00fcrt\u00b7zen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$.", "PPER", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ha! denckt ein weltling offt: Es kan mir nicht mehr fehlen,", "tokens": ["Ha", "!", "denckt", "ein", "welt\u00b7ling", "offt", ":", "Es", "kan", "mir", "nicht", "mehr", "feh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ART", "NN", "ADV", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Heut tret ich in den hof, denn in die kammer ein.", "tokens": ["Heut", "tret", "ich", "in", "den", "hof", ",", "denn", "in", "die", "kam\u00b7mer", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, spricht ein ander thor: Ich mag mich nicht verm\u00e4hlen,", "tokens": ["Und", ",", "spricht", "ein", "an\u00b7der", "thor", ":", "Ich", "mag", "mich", "nicht", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es m\u00fcste denn ein kind mit viertzig tausend seyn.", "tokens": ["Es", "m\u00fcs\u00b7te", "denn", "ein", "kind", "mit", "viert\u00b7zig", "tau\u00b7send", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "APPR", "CARD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er spricht es; Doch die zeit zwingt beyde zu gestehen,", "tokens": ["Er", "spricht", "es", ";", "Doch", "die", "zeit", "zwingt", "bey\u00b7de", "zu", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "KON", "ART", "NN", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df man nur ohne GOtt, nicht wider GOtt, kan gehen.", "tokens": ["Da\u00df", "man", "nur", "oh\u00b7ne", "Gott", ",", "nicht", "wi\u00b7der", "Gott", ",", "kan", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie sehr ist doch dein hertz von dieser art entfernt,", "tokens": ["Wie", "sehr", "ist", "doch", "dein", "hertz", "von", "die\u00b7ser", "art", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Seide! den man nie sieht in die ferne sorgen.", "tokens": ["Mein", "Sei\u00b7de", "!", "den", "man", "nie", "sieht", "in", "die", "fer\u00b7ne", "sor\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "PIS", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hast von jugend auf GOtt recht vertrau\u2019n gelernt;", "tokens": ["Du", "hast", "von", "ju\u00b7gend", "auf", "Gott", "recht", "ver\u00b7trau'n", "ge\u00b7lernt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "NN", "ADJD", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie dir der abend ist, so ist dir auch der morgen.", "tokens": ["Wie", "dir", "der", "a\u00b7bend", "ist", ",", "so", "ist", "dir", "auch", "der", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJD", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "ART", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du hast dir zwar die welt niemahls zu leicht gemacht;", "tokens": ["Du", "hast", "dir", "zwar", "die", "welt", "nie\u00b7mahls", "zu", "leicht", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Doch hast du niemahls auch so weit voraus gedacht.", "tokens": ["Doch", "hast", "du", "nie\u00b7mahls", "auch", "so", "weit", "vo\u00b7raus", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die heyrath, die du thatst, geschah\u2019 nach GOttes willen;", "tokens": ["Die", "hey\u00b7rath", ",", "die", "du", "thatst", ",", "ge\u00b7scha\u00b7h'", "nach", "Got\u00b7tes", "wil\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADJD", "APPR", "NN", "NN", "$."], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nicht aber nach der kunst der \u00fcberklugen welt.", "tokens": ["Nicht", "a\u00b7ber", "nach", "der", "kunst", "der", "\u00fc\u00b7berk\u00b7lu\u00b7gen", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sah\u2019 dich nicht vorher mit geld den kasten f\u00fcllen:", "tokens": ["Man", "sah'", "dich", "nicht", "vor\u00b7her", "mit", "geld", "den", "kas\u00b7ten", "f\u00fcl\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du dachtst an keinen dienst, doch war er schon bestellt,", "tokens": ["Du", "dachtst", "an", "kei\u00b7nen", "dienst", ",", "doch", "war", "er", "schon", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein was vielen fehlt, die ohne sorgen leben,", "tokens": ["Al\u00b7lein", "was", "vie\u00b7len", "fehlt", ",", "die", "oh\u00b7ne", "sor\u00b7gen", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PIS", "VVFIN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "War, da\u00df du keinen tag ohn arbeit hingegeben.", "tokens": ["War", ",", "da\u00df", "du", "kei\u00b7nen", "tag", "ohn", "ar\u00b7beit", "hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drum ist dir auch dein thun bi\u00df diesen tag gegl\u00fcckt:", "tokens": ["Drum", "ist", "dir", "auch", "dein", "thun", "bi\u00df", "die\u00b7sen", "tag", "ge\u00b7gl\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PPOSAT", "VVINF", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hast, was ihrer viel umsonst zu fr\u00fch begehren.", "tokens": ["Du", "hast", ",", "was", "ih\u00b7rer", "viel", "um\u00b7sonst", "zu", "fr\u00fch", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPOSAT", "ADV", "ADV", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer deine freunde kennt, und deinen tisch anblickt,", "tokens": ["Wer", "dei\u00b7ne", "freun\u00b7de", "kennt", ",", "und", "dei\u00b7nen", "tisch", "an\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der denckt, sie m\u00fcsten dich das erste jahr verzehren.", "tokens": ["Der", "denckt", ",", "sie", "m\u00fcs\u00b7ten", "dich", "das", "ers\u00b7te", "jahr", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VMFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es leben viel von dir, und gleichwohl sieht man nicht,", "tokens": ["Es", "le\u00b7ben", "viel", "von", "dir", ",", "und", "gleich\u00b7wohl", "sieht", "man", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "KON", "ADV", "VVFIN", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ihnen h\u00fclff und rath, dir jemahls brod gebricht.", "tokens": ["Da\u00df", "ih\u00b7nen", "h\u00fclff", "und", "rath", ",", "dir", "je\u00b7mahls", "brod", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "NN", "$,", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wie du mit dir gethan, thust du mit deinem kinde.", "tokens": ["Wie", "du", "mit", "dir", "ge\u00b7than", ",", "thust", "du", "mit", "dei\u00b7nem", "kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVPP", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du giebst es mit bedacht und ohne kummer hin.", "tokens": ["Du", "giebst", "es", "mit", "be\u00b7dacht", "und", "oh\u00b7ne", "kum\u00b7mer", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "VVPP", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob sich dein schwieger-sohn auch in die nahrung finde!", "tokens": ["Ob", "sich", "dein", "schwie\u00b7ger\u00b7sohn", "auch", "in", "die", "nah\u00b7rung", "fin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob er bemittelt sey? das kommt dir nicht in sinn.", "tokens": ["Ob", "er", "be\u00b7mit\u00b7telt", "sey", "?", "das", "kommt", "dir", "nicht", "in", "sinn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$.", "PDS", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du siehest auf sein hertz, und l\u00e4st den H\u00f6chsten rathen.", "tokens": ["Du", "sie\u00b7hest", "auf", "sein", "hertz", ",", "und", "l\u00e4st", "den", "H\u00f6chs\u00b7ten", "ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der GOtt, der menschen schafft, der schafft auch wohl dueaten.", "tokens": ["Der", "Gott", ",", "der", "men\u00b7schen", "schafft", ",", "der", "schafft", "auch", "wohl", "du\u00b7ea\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ach! d\u00e4chte jeder so, wie gl\u00fccklich w\u00e4ren wir!", "tokens": ["Ach", "!", "d\u00e4ch\u00b7te", "je\u00b7der", "so", ",", "wie", "gl\u00fcck\u00b7lich", "w\u00e4\u00b7ren", "wir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PIS", "ADV", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man w\u00fcrde nicht so viel bey ehen unruh fehen.", "tokens": ["Man", "w\u00fcr\u00b7de", "nicht", "so", "viel", "bey", "e\u00b7hen", "un\u00b7ruh", "fe\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PTKNEG", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier wirfft ein w\u00fcster mann der frau die m\u00e4ngel f\u00fcr;", "tokens": ["Hier", "wirfft", "ein", "w\u00fcs\u00b7ter", "mann", "der", "frau", "die", "m\u00e4n\u00b7gel", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Warum? Dieweil ihm nicht an gelde gnug geschehen:", "tokens": ["Wa\u00b7rum", "?", "Die\u00b7weil", "ihm", "nicht", "an", "gel\u00b7de", "gnug", "ge\u00b7sche\u00b7hen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPER", "PTKNEG", "APPR", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dort pocht ein stoltzes weib auf ihren klumpen gold,", "tokens": ["Dort", "pocht", "ein", "stolt\u00b7zes", "weib", "auf", "ih\u00b7ren", "klum\u00b7pen", "gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und billich; weil sie doch uns jeden ku\u00df verzollt.", "tokens": ["Und", "bil\u00b7lich", ";", "weil", "sie", "doch", "uns", "je\u00b7den", "ku\u00df", "ver\u00b7zollt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "KOUS", "PPER", "ADV", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dein kind bringt keinen schatz, als gottesfurcht und liebe,", "tokens": ["Dein", "kind", "bringt", "kei\u00b7nen", "schatz", ",", "als", "got\u00b7tes\u00b7furcht", "und", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,", "KOUS", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sucht kein gegen-gut, als redlichkeit und treu.", "tokens": ["Und", "sucht", "kein", "ge\u00b7gen\u00b7gut", ",", "als", "red\u00b7lich\u00b7keit", "und", "treu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,", "KOUS", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer mercket nicht allhier des weisen himmels triebe?", "tokens": ["Wer", "mer\u00b7cket", "nicht", "all\u00b7hier", "des", "wei\u00b7sen", "him\u00b7mels", "trie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer sieht nicht, da\u00df ihr hertz dem deinen \u00e4hnlich sey?", "tokens": ["Wer", "sieht", "nicht", ",", "da\u00df", "ihr", "hertz", "dem", "dei\u00b7nen", "\u00e4hn\u00b7lich", "sey", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "ART", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun, ist ihr hertz dir gleich, so ist auch leicht zu gl\u00e4uben,", "tokens": ["Nun", ",", "ist", "ihr", "hertz", "dir", "gleich", ",", "so", "ist", "auch", "leicht", "zu", "gl\u00e4u\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "$,", "ADV", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df sie, gleichwie dein hau\u00df, stets wird gesegnet bleiben.", "tokens": ["Da\u00df", "sie", ",", "gleich\u00b7wie", "dein", "hau\u00df", ",", "stets", "wird", "ge\u00b7seg\u00b7net", "blei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KON", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "VVPP", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Ich seh der trauung hier zwar nur von ferne zu:", "tokens": ["Ich", "seh", "der", "trau\u00b7ung", "hier", "zwar", "nur", "von", "fer\u00b7ne", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "ADV", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch d\u00fcnckt mich, da\u00df mein geist schon als im spiegel schauet,", "tokens": ["Doch", "d\u00fcnckt", "mich", ",", "da\u00df", "mein", "geist", "schon", "als", "im", "spie\u00b7gel", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADV", "KOKOM", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie dieses neue paar ihm ewig-s\u00fcsse ruh,", "tokens": ["Wie", "die\u00b7ses", "neu\u00b7e", "paar", "ihm", "e\u00b7wig\u00b7s\u00fcs\u00b7se", "ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir neue freud und lust, uns ruhm und ehre bauet.", "tokens": ["Dir", "neu\u00b7e", "freud", "und", "lust", ",", "uns", "ruhm", "und", "eh\u00b7re", "bau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "VVFIN", "KON", "VVFIN", "$,", "PPER", "NN", "KON", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Seidel! wilst du mehr? Ich wei\u00df, du bist zu klug.", "tokens": ["Mein", "Sei\u00b7del", "!", "wilst", "du", "mehr", "?", "Ich", "wei\u00df", ",", "du", "bist", "zu", "klug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VMFIN", 