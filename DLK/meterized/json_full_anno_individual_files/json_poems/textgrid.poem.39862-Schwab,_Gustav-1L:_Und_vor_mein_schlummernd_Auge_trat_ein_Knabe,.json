{"textgrid.poem.39862": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und vor mein schlummernd Auge trat ein Knabe,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und vor mein schlummernd Auge trat ein Knabe,", "tokens": ["Und", "vor", "mein", "schlum\u00b7mernd", "Au\u00b7ge", "trat", "ein", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Leichtsinn'gen Schritts, gleichg\u00fcltigen Gesichts;", "tokens": ["Leicht\u00b7sinn'\u00b7gen", "Schritts", ",", "gleich\u00b7g\u00fcl\u00b7ti\u00b7gen", "Ge\u00b7sichts", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJA", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "In seinem Blick, um seinen Mund war nichts;", "tokens": ["In", "sei\u00b7nem", "Blick", ",", "um", "sei\u00b7nen", "Mund", "war", "nichts", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUI", "PPOSAT", "NN", "VAFIN", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der deutete hinaus mit seinem Stabe", "tokens": ["Der", "deu\u00b7te\u00b7te", "hin\u00b7aus", "mit", "sei\u00b7nem", "Sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APZR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und sprach: \u00bbKomm, sieh', was ich bereitet habe!\u00ab", "tokens": ["Und", "sprach", ":", "\u00bb", "Komm", ",", "sieh'", ",", "was", "ich", "be\u00b7rei\u00b7tet", "ha\u00b7be", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "VVFIN", "$,", "VVFIN", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich drauf: \u00bbDu bist kein Bote mir des Lichts;", "tokens": ["Ich", "drauf", ":", "\u00bb", "Du", "bist", "kein", "Bo\u00b7te", "mir", "des", "Lichts", ";"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$.", "$(", "PPER", "VAFIN", "PIAT", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein eitler Gang, dein leeres Auge spricht's;", "tokens": ["Dein", "eit\u00b7ler", "Gang", ",", "dein", "lee\u00b7res", "Au\u00b7ge", "spricht's", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von dir erwart' ich keine Sehergabe.", "tokens": ["Von", "dir", "er\u00b7wart'", "ich", "kei\u00b7ne", "Se\u00b7her\u00b7ga\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer bist du?\u00ab \u2013 Da bescheiden sprach der Kleine:", "tokens": ["Wer", "bist", "du", "?", "\u00ab", "\u2013", "Da", "be\u00b7schei\u00b7den", "sprach", "der", "Klei\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "$(", "$(", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbich bin der ", "tokens": ["\u00bb", "ich", "bin", "der"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Bin ich der letzte nicht von Gottes Engeln.", "tokens": ["Bin", "ich", "der", "letz\u00b7te", "nicht", "von", "Got\u00b7tes", "En\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "PTKNEG", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Komm, folge mir! ich zeige frei von M\u00e4ngeln,", "tokens": ["Komm", ",", "fol\u00b7ge", "mir", "!", "ich", "zei\u00b7ge", "frei", "von", "M\u00e4n\u00b7geln", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Seelensch\u00f6nheit dir die S\u00fc\u00dfe, Reine!\u00ab", "tokens": ["In", "See\u00b7len\u00b7sch\u00f6n\u00b7heit", "dir", "die", "S\u00fc\u00b7\u00dfe", ",", "Rei\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er ging, ein Zephyr kr\u00e4uselte sein Haar.", "tokens": ["Er", "ging", ",", "ein", "Ze\u00b7phyr", "kr\u00e4u\u00b7sel\u00b7te", "sein", "Haar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und vor mein schlummernd Auge trat ein Knabe,", "tokens": ["Und", "vor", "mei