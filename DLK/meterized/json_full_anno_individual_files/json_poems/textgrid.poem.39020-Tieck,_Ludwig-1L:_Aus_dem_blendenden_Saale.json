{"textgrid.poem.39020": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus dem blendenden Saale", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem blendenden Saale", "tokens": ["Aus", "dem", "blen\u00b7den\u00b7den", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Tret' ich in die Dunkelheit der Nacht,", "tokens": ["Tret'", "ich", "in", "die", "Dun\u00b7kel\u00b7heit", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Froh doch erm\u00fcdet den Weg nach Hause suchend.", "tokens": ["Froh", "doch", "er\u00b7m\u00fc\u00b7det", "den", "Weg", "nach", "Hau\u00b7se", "su\u00b7chend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Auch Lust und Thorheit \u00fcbers\u00e4ttigen,", "tokens": ["Auch", "Lust", "und", "Thor\u00b7heit", "\u00fc\u00b7bers\u00b7\u00e4t\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und die Seele w\u00fcnscht die ernste Ruhe wieder.", "tokens": ["Und", "die", "See\u00b7le", "w\u00fcnscht", "die", "erns\u00b7te", "Ru\u00b7he", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Da taumelt ein Alter", "tokens": ["Da", "tau\u00b7melt", "ein", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Stra\u00dfe hinab,", "tokens": ["Die", "Stra\u00b7\u00dfe", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Von einem Knaben geleitet.", "tokens": ["Von", "ei\u00b7nem", "Kna\u00b7ben", "ge\u00b7lei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Alte murrt und klagt,", "tokens": ["Der", "Al\u00b7te", "murrt", "und", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und z\u00fcrnt, so scheint es, mit sich und der Welt;", "tokens": ["Und", "z\u00fcrnt", ",", "so", "scheint", "es", ",", "mit", "sich", "und", "der", "Welt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "APPR", "PRF", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch im bekannten Refrain,", "tokens": ["Doch", "im", "be\u00b7kann\u00b7ten", "Re\u00b7frain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Der t\u00e4glich das Ohr bet\u00e4ubt,", "tokens": ["Der", "t\u00e4g\u00b7lich", "das", "Ohr", "be\u00b7t\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Singt der halbberauschte Junge:", "tokens": ["Singt", "der", "halb\u00b7be\u00b7rauschte", "Jun\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Sei ruhig, mein V\u00e4terchen;", "tokens": ["Sei", "ru\u00b7hig", ",", "mein", "V\u00e4\u00b7ter\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Was thut das Haarbeutelchen,", "tokens": ["Was", "thut", "das", "Haar\u00b7beu\u00b7tel\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das wir heut', und gestern und einige", "tokens": ["Das", "wir", "heut'", ",", "und", "ge\u00b7stern", "und", "ei\u00b7ni\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "$,", "KON", "ADJD", "KON", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Male mehr uns getrunken in Lustigkeit?", "tokens": ["Ma\u00b7le", "mehr", "uns", "ge\u00b7trun\u00b7ken", "in", "Lus\u00b7tig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Vor\u00fcber ist nun die Zeit des Fr\u00f6hlichseyns,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist", "nun", "die", "Zeit", "des", "Fr\u00f6h\u00b7lich\u00b7seyns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Schon morgen fr\u00fch sitzen wir S\u00fcnderchen", "tokens": ["Schon", "mor\u00b7gen", "fr\u00fch", "sit\u00b7zen", "wir", "S\u00fcn\u00b7der\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "PPER", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und streuen uns Asche auf die H\u00e4upterchen.", "tokens": ["Und", "streu\u00b7en", "uns", "A\u00b7sche", "auf", "die", "H\u00e4up\u00b7ter\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Glaube mir, der Herr der Welt, wie das Sternenheer,", "tokens": ["Glau\u00b7be", "mir", ",", "der", "Herr", "der", "Welt", ",", "wie", "das", "Ster\u00b7nen\u00b7heer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NN", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "So schuf er auch neben der Fr\u00f6mmigkeit", "tokens": ["So", "schuf", "er", "auch", "ne\u00b7ben", "der", "Fr\u00f6m\u00b7mig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Lust an der Lust und das Carneval:", "tokens": ["Die", "Lust", "an", "der", "Lust", "und", "das", "Car\u00b7ne\u00b7val", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Uebel nimmt es gewi\u00df nicht der Gn\u00e4dige,", "tokens": ["Ue\u00b7bel", "nimmt", "es", "ge\u00b7wi\u00df", "nicht", "der", "Gn\u00e4\u00b7di\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Wenn er dich heut' zum Beschlu\u00df also wackeln sieht,", "tokens": ["Wenn", "er", "dich", "heut'", "zum", "Be\u00b7schlu\u00df", "al\u00b7so", "wa\u00b7ckeln", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn wirklich wir trieben es m\u00e4\u00dfiglich", "tokens": ["Denn", "wirk\u00b7lich", "wir", "trie\u00b7ben", "es", "m\u00e4\u00b7\u00dfig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Und tranken und l\u00e4rmten nicht allzuviel.", "tokens": ["Und", "tran\u00b7ken", "und", "l\u00e4rm\u00b7ten", "nicht", "all\u00b7zu\u00b7viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PTKNEG", "PIAT", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Doch wird dir zu schwer dein sanft Herzelein,", "tokens": ["Doch", "wird", "dir", "zu", "schwer", "dein", "sanft", "Her\u00b7ze\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKA", "ADJD", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Gehst du ja nun hin zu dem Beichtiger,", "tokens": ["Gehst", "du", "ja", "nun", "hin", "zu", "dem", "Beich\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Der dich, o du Guter, von S\u00fcnden dann reiniget.", "tokens": ["Der", "dich", ",", "o", "du", "Gu\u00b7ter", ",", "von", "S\u00fcn\u00b7den", "dann", "rei\u00b7ni\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "FM", "PPER", "NN", "$,", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Drum fr\u00f6hlich noch jetzt bis zur Schlafenszeit,", "tokens": ["Drum", "fr\u00f6h\u00b7lich", "noch", "jetzt", "bis", "zur", "Schla\u00b7fens\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "ADV", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Schlafe dann, V\u00e4terchen, festiglich,", "tokens": ["Schla\u00b7fe", "dann", ",", "V\u00e4\u00b7ter\u00b7chen", ",", "fes\u00b7tig\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$,", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Auf dann erwache zur Heiterkeit,", "tokens": ["Auf", "dann", "er\u00b7wa\u00b7che", "zur", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Bu\u00dfe thu, lebe dann tugendlich. \u2013", "tokens": ["Bu\u00b7\u00dfe", "thu", ",", "le\u00b7be", "dann", "tu\u00b7gend\u00b7lich", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Immer noch murrte der Alte,", "tokens": ["Im\u00b7mer", "noch", "murr\u00b7te", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Und l\u00e4chelnd folgt' ich dem Paare,", "tokens": ["Und", "l\u00e4\u00b7chelnd", "folgt'", "ich", "dem", "Paa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weil es mir ein frommer Hymnus schien,", "tokens": ["Weil", "es", "mir", "ein", "from\u00b7mer", "Hym\u00b7nus", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der von des Sohnes Lippen, den Vater zu tr\u00f6sten,", "tokens": ["Der", "von", "des", "Soh\u00b7nes", "Lip\u00b7pen", ",", "den", "Va\u00b7ter", "zu", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "---+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Durch die Nacht erklang.", "tokens": ["Durch", "die", "Nacht", "er\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": 