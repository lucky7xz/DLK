": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Sei ruhig, mein V\u00e4terchen;", "tokens": ["Sei", "ru\u00b7hig", ",", "mein", "V\u00e4\u00b7ter\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Was thut das Haarbeutelchen,", "tokens": ["Was", "thut", "das", "Haar\u00b7beu\u00b7tel\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das wir heut', und gestern und einige", "tokens": ["Das", "wir", "heut'", ",", "und", "ge\u00b7stern", "und", "ei\u00b7ni\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "$,", "KON", "ADJD", "KON", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Male mehr uns getrunken in Lustigkeit?", "tokens": ["Ma\u00b7le", "mehr", "uns", "ge\u00b7trun\u00b7ken", "in", "Lus\u00b7tig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Vor\u00fcber ist nun die Zeit des Fr\u00f6hlichseyns,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist", "nun", "die", "Zeit", "des", "Fr\u00f6h\u00b7lich\u00b7seyns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Schon morgen fr\u00fch sitzen wir S\u00fcnderchen", "tokens": ["Schon", "mor\u00b7gen", "fr\u00fch", "sit\u00b7zen", "wir", "S\u00fcn\u00b7der\