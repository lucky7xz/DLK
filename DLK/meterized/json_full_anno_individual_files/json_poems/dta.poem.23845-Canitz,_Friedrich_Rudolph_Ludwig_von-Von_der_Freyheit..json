{"dta.poem.23845": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Von der Freyheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe meinen Leib als ein Gewand verschleissen/", "tokens": ["Ich", "se\u00b7he", "mei\u00b7nen", "Leib", "als", "ein", "Ge\u00b7wand", "ver\u00b7schleis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KOKOM", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was aber in mir wohnt/ und Seele wird geheissen/", "tokens": ["Was", "a\u00b7ber", "in", "mir", "wohnt", "/", "und", "See\u00b7le", "wird", "ge\u00b7heis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPER", "VVFIN", "$(", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Empfindet einen Trieb/ der nach der Freyheit strebt;", "tokens": ["Emp\u00b7fin\u00b7det", "ei\u00b7nen", "Trieb", "/", "der", "nach", "der", "Frey\u00b7heit", "strebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch eh\u2019 ich sie erlangt/ hab\u2019 ich fast ausgelebt.", "tokens": ["Doch", "eh'", "ich", "sie", "er\u00b7langt", "/", "hab'", "ich", "fast", "aus\u00b7ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVPP", "$(", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich habe solchen Wunsch vielleicht bey mir gesp\u00fchret/", "tokens": ["Ich", "ha\u00b7be", "sol\u00b7chen", "Wunsch", "viel\u00b7leicht", "bey", "mir", "ge\u00b7sp\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So bald mein erstes Blut und Othem sich ger\u00fchret/", "tokens": ["So", "bald", "mein", "ers\u00b7tes", "Blut", "und", "O\u00b7them", "sich", "ge\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "KON", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer wei\u00df wie oft ich schon/ ich unvollkommne Frucht/", "tokens": ["Wer", "wei\u00df", "wie", "oft", "ich", "schon", "/", "ich", "un\u00b7voll\u00b7komm\u00b7ne", "Frucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOKOM", "ADV", "PPER", "ADV", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Fortgang zur Geburth mit Ungest\u00fcm gesucht?", "tokens": ["Den", "Fort\u00b7gang", "zur", "Ge\u00b7burth", "mit", "Un\u00b7ge\u00b7st\u00fcm", "ge\u00b7sucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ob nicht mein freyer Geist/ sich mit den bittern Z\u00e4hren/", "tokens": ["Ob", "nicht", "mein", "frey\u00b7er", "Geist", "/", "sich", "mit", "den", "bit\u00b7tern", "Z\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "ADJA", "NN", "$(", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hernachmahls f\u00fcr den Zwang der Windeln wollen weh", "tokens": ["Her\u00b7nach\u00b7mahls", "f\u00fcr", "den", "Zwang", "der", "Win\u00b7deln", "wol\u00b7len", "weh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und ob nicht dazumahl mein unvergn\u00fcgter Mund/", "tokens": ["Und", "ob", "nicht", "da\u00b7zu\u00b7mahl", "mein", "un\u00b7ver\u00b7gn\u00fcg\u00b7ter", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKNEG", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn ihm der Ammen-Brust nicht bald zu Dienste stund/", "tokens": ["Wenn", "ihm", "der", "Am\u00b7men\u00b7Brust", "nicht", "bald", "zu", "Diens\u00b7te", "stund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein gleiches Klage-Lied aus Ungedult gesungen/", "tokens": ["Ein", "glei\u00b7ches", "Kla\u00b7ge\u00b7Lied", "aus", "Un\u00b7ge\u00b7dult", "ge\u00b7sun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als mir bey reiff\u2019rer Zeit der Kummer ", "tokens": ["Als", "mir", "bey", "reiff'\u00b7rer", "Zeit", "der", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Das wei\u00df ich: da ich erst wie zu mir selber kam/", "tokens": ["Das", "wei\u00df", "ich", ":", "da", "ich", "erst", "wie", "zu", "mir", "sel\u00b7ber", "kam", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "KOUS", "PPER", "ADV", "KOKOM", "APPR", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und mich des Lehrers Flei\u00df in strenger Aufsicht nahm/", "tokens": ["Und", "mich", "des", "Leh\u00b7rers", "Flei\u00df", "in", "stren\u00b7ger", "Auf\u00b7sicht", "nahm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da\u00df ich mich aus Verdru\u00df gekr\u00fcmmet und gewunden/", "tokens": ["Da\u00df", "ich", "mich", "aus", "Ver\u00b7dru\u00df", "ge\u00b7kr\u00fcm\u00b7met", "und", "ge\u00b7wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVPP", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So offt als der Tyrann/ zu den gesetzten Stunden/", "tokens": ["So", "offt", "als", "der", "Ty\u00b7rann", "/", "zu", "den", "ge\u00b7setz\u00b7ten", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "ART", "NN", "$(", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Durch ein verha\u00dftes Wort/ mich in dem Spiel gest\u00f6hrt/", "tokens": ["Durch", "ein", "ver\u00b7ha\u00df\u00b7tes", "Wort", "/", "mich", "in", "dem", "Spiel", "ge\u00b7st\u00f6hrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und eh\u2019ich Teutsch gekont/ was R\u00f6misches gelehrt.", "tokens": ["Und", "eh'\u00b7ich", "Teutsch", "ge\u00b7kont", "/", "was", "R\u00f6\u00b7mi\u00b7sches", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVPP", "$(", "PWS", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Doch m\u00f6cht ich nur itzund der Kindheit Lust erfahren!", "tokens": ["Doch", "m\u00f6cht", "ich", "nur", "it\u00b7zund", "der", "Kind\u00b7heit", "Lust", "er\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Unmuht nimmt nicht ab/ er w\u00e4chset mit den Jahren;", "tokens": ["Der", "Un\u00b7muht", "nimmt", "nicht", "ab", "/", "er", "w\u00e4ch\u00b7set", "mit", "den", "Jah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Was n\u00fctzet der Verstand/ als da\u00df er mit Bedacht", "tokens": ["Was", "n\u00fct\u00b7zet", "der", "Ver\u00b7stand", "/", "als", "da\u00df", "er", "mit", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$(", "KOKOM", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Freyheit sch\u00e4tzen lernt/ die Ketten schwerer macht?", "tokens": ["Die", "Frey\u00b7heit", "sch\u00e4t\u00b7zen", "lernt", "/", "die", "Ket\u00b7ten", "schwe\u00b7rer", "macht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$(", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein Baum wars/ nur ein Baum/ dran solche Fr\u00fcchte", "tokens": ["Ein", "Baum", "wars", "/", "nur", "ein", "Baum", "/", "dran", "sol\u00b7che", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$(", "ADV", "ART", "NN", "$(", "PAV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die dort der erste Mensch solt\u2019 unbetastet lassen;", "tokens": ["Die", "dort", "der", "ers\u00b7te", "Mensch", "solt'", "un\u00b7be\u00b7tas\u00b7tet", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Uns aber ist noch mehr zu halten auferlegt/", "tokens": ["Uns", "a\u00b7ber", "ist", "noch", "mehr", "zu", "hal\u00b7ten", "auf\u00b7er\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil nun ein gantzer Wald so viel verbottnes tr\u00e4gt.", "tokens": ["Weil", "nun", "ein", "gant\u00b7zer", "Wald", "so", "viel", "ver\u00b7bott\u00b7nes", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADV", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wir h\u00f6ren \u00fcberall Verf\u00fchrungs-Schlangen pfeiffen;", "tokens": ["Wir", "h\u00f6\u00b7ren", "\u00fc\u00b7be\u00b7rall", "Ver\u00b7f\u00fch\u00b7rungs\u00b7Schlan\u00b7gen", "pfeif\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wir wollen hier und da nach fremden Aepffeln greiffen;", "tokens": ["Wir", "wol\u00b7len", "hier", "und", "da", "nach", "frem\u00b7den", "A\u00b7e\u00b7pf\u00b7feln", "greif\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "KON", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.31": {"text": "Wie w\u00e4ssert uns der Mund! die Hand wird ausgestreckt;", "tokens": ["Wie", "w\u00e4s\u00b7sert", "uns", "der", "Mund", "!", "die", "Hand", "wird", "aus\u00b7ge\u00b7streckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Jedoch des Him\u0303els-Schlu\u00df/ der uns mit Flam\u0303en schreckt/", "tokens": ["Je\u00b7doch", "des", "Him\u0303els\u00b7Schlu\u00df", "/", "der", "uns", "mit", "Flam\u0303en", "schreckt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.33": {"text": "Der hei\u00dft so wol die Lust/ indem wir wachen/ z\u00e4umen/", "tokens": ["Der", "hei\u00dft", "so", "wol", "die", "Lust", "/", "in\u00b7dem", "wir", "wa\u00b7chen", "/", "z\u00e4u\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ART", "NN", "$(", "KOUS", "PPER", "VVINF", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Als selber in dem Schlaaf nach dem Gesetze tr\u00e4umen.", "tokens": ["Als", "sel\u00b7ber", "in", "dem", "Schlaaf", "nach", "dem", "Ge\u00b7set\u00b7ze", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wol dem/ der seinen Sin\u0303 und Fleisch darnach bequemt!", "tokens": ["Wol", "dem", "/", "der", "sei\u00b7nen", "Si\u00f1", "und", "Fleisch", "dar\u00b7nach", "be\u00b7quemt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$(", "ART", "PPOSAT", "NN", "KON", "NN", "PAV", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.36": {"text": "Denn wer zu offenbahr und gar zu ungez\u00e4hmt", "tokens": ["Denn", "wer", "zu", "of\u00b7fen\u00b7bahr", "und", "gar", "zu", "un\u00b7ge\u00b7z\u00e4hmt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PTKA", "ADJD", "KON", "ADV", "PTKA", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "In der Begierden Schlamm gewohnet ist zu w\u00fchlen/", "tokens": ["In", "der", "Be\u00b7gier\u00b7den", "Schlamm", "ge\u00b7woh\u00b7net", "ist", "zu", "w\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wird meistens in der Welt auch schon die Rache f\u00fchlen;", "tokens": ["Wird", "meis\u00b7tens", "in", "der", "Welt", "auch", "schon", "die", "Ra\u00b7che", "f\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Folgt ihm gleich Schwerdt und Mord nicht auf dem Fus-", "tokens": ["Folgt", "ihm", "gleich", "Schwerdt", "und", "Mord", "nicht", "auf", "dem", "Fus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "NN", "KON", "NN", "PTKNEG", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "So w\u00e4hrts doch kurtze Frist/ bi\u00df da\u00df in dem Gemach/", "tokens": ["So", "w\u00e4hrts", "doch", "kurt\u00b7ze", "Frist", "/", "bi\u00df", "da\u00df", "in", "dem", "Ge\u00b7mach", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJA", "NN", "$(", "APPR", "KOUS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.41": {"text": "Das man zu Sommers-Zeit/ so wie im Winter heitzet/", "tokens": ["Das", "man", "zu", "Som\u00b7mer\u00b7sZeit", "/", "so", "wie", "im", "Win\u00b7ter", "heit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "NN", "$(", "ADV", "KOKOM", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ihm ein verschwiegner Artzt den alten Adam beitzet;", "tokens": ["Ihm", "ein", "ver\u00b7schwieg\u00b7ner", "Artzt", "den", "al\u00b7ten", "A\u00b7dam", "beit\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.43": {"text": "Da wird sein G\u00f6tter-Brodt und Nectar-s\u00fcsses Na\u00df/", "tokens": ["Da", "wird", "sein", "G\u00f6t\u00b7ter\u00b7Brodt", "und", "Nec\u00b7ta\u00b7rs\u00fcs\u00b7ses", "Na\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ein Zwieback und ein Tranck von lauem Sassafra\u00df.", "tokens": ["Ein", "Zwie\u00b7back", "und", "ein", "Tranck", "von", "lau\u00b7em", "Sas\u00b7sa\u00b7fra\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "So ists: was unserm Fleisch am heftigsten behaget/", "tokens": ["So", "ists", ":", "was", "un\u00b7serm", "Fleisch", "am", "hef\u00b7tigs\u00b7ten", "be\u