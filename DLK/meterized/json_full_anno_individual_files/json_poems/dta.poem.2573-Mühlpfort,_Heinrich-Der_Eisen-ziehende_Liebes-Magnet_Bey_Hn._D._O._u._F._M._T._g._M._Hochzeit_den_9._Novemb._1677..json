{"dta.poem.2573": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Eisen-ziehende Liebes-Magnet  \n  Bey Hn. D. O. u. F. M. T. g. M. Hochzeit  \n den 9. Novemb. 1677.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was macht man auf der Welt das allen doch gefalle?", "tokens": ["Was", "macht", "man", "auf", "der", "Welt", "das", "al\u00b7len", "doch", "ge\u00b7fal\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "APPR", "ART", "NN", "ART", "PIAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel Lieben ist nicht gut", "tokens": ["Viel", "Lie\u00b7ben", "ist", "nicht", "gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "VAFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wer es gar nicht thut", "tokens": ["Und", "wer", "es", "gar", "nicht", "thut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "PTKNEG", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der spielet mit der Zeit als wie mit einem Balle;", "tokens": ["Der", "spie\u00b7let", "mit", "der", "Zeit", "als", "wie", "mit", "ei\u00b7nem", "Bal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "KOUS", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wenn die besten Jahre hin/", "tokens": ["Und", "wenn", "die", "bes\u00b7ten", "Jah\u00b7re", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So sieht er/ nichts ist sein Gewin.", "tokens": ["So", "sieht", "er", "/", "nichts", "ist", "sein", "Ge\u00b7win", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PIS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was ist der Mensch ihm selbst? ein \u00f6de Trauer-W\u00fcste/", "tokens": ["Was", "ist", "der", "Mensch", "ihm", "selbst", "?", "ein", "\u00f6\u00b7de", "Trau\u00b7e\u00b7rW\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "PPER", "ADV", "$.", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Ampel ohne Licht/", "tokens": ["Ein", "Am\u00b7pel", "oh\u00b7ne", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ein Feld/ dem Gra\u00df gebricht/", "tokens": ["Ein", "Feld", "/", "dem", "Gra\u00df", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ein Garten ohne Frucht/ ein schwartzes Schau-Ger\u00fcste/", "tokens": ["Ein", "Gar\u00b7ten", "oh\u00b7ne", "Frucht", "/", "ein", "schwart\u00b7zes", "Schau\u00b7Ge\u00b7r\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wo die Personen alle schreyn:", "tokens": ["Wo", "die", "Per\u00b7so\u00b7nen", "al\u00b7le", "schreyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Weh! wer in Einsamkeit mu\u00df seyn.", "tokens": ["Weh", "!", "wer", "in", "Ein\u00b7sam\u00b7keit", "mu\u00df", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "APPR", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Herr Nachbar/ er hat auch bi\u00dfher genung erfahren/", "tokens": ["Herr", "Nach\u00b7bar", "/", "er", "hat", "auch", "bi\u00df\u00b7her", "ge\u00b7nung", "er\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "PPER", "VAFIN", "ADV", "ADV", "ADV", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Wie nah\u2019 der Wittber-Stand", "tokens": ["Wie", "nah'", "der", "Witt\u00b7ber\u00b7Stand"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Dem Kummer sey verwand/", "tokens": ["Dem", "Kum\u00b7mer", "sey", "ver\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Und da\u00df/ wer Handlung treibt/ und f\u00fchret Gut und Wahren/", "tokens": ["Und", "da\u00df", "/", "wer", "Hand\u00b7lung", "treibt", "/", "und", "f\u00fch\u00b7ret", "Gut", "und", "Wah\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$(", "PWS", "NN", "VVFIN", "$(", "KON", "VVFIN", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sol es in vollem Lauffe gehn/", "tokens": ["Sol", "es", "in", "vol\u00b7lem", "Lauf\u00b7fe", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Alleine nicht kan wol bestehn.", "tokens": ["Al\u00b7lei\u00b7ne", "nicht", "kan", "wol", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Es mag ein ander nun von grossen Wundern sagen:", "tokens": ["Es", "mag", "ein", "an\u00b7der", "nun", "von", "gros\u00b7sen", "Wun\u00b7dern", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJD", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Di\u00df ist der Lauff der Welt/", "tokens": ["Di\u00df", "ist", "der", "Lauff", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Die nichts vor sch\u00e4tzbar h\u00e4lt/", "tokens": ["Die", "nichts", "vor", "sch\u00e4tz\u00b7bar", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ADJD", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Was nicht mit grosser Pracht und Ruhm wird f\u00fcrgetragen.", "tokens": ["Was", "nicht", "mit", "gros\u00b7ser", "Pracht", "und", "Ruhm", "wird", "f\u00fcr\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "ADJA", "NN", "KON", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich wei\u00df/ sein redlich dentscher Sinn", "tokens": ["Ich", "wei\u00df", "/", "sein", "red\u00b7lich", "dent\u00b7scher", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wirft solche Eytelkeiten hin.", "tokens": ["Wirft", "sol\u00b7che", "Ey\u00b7tel\u00b7kei\u00b7ten", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ich wil in seinem Kram bey seinem Eisen finden", "tokens": ["Ich", "wil", "in", "sei\u00b7nem", "Kram", "bey", "sei\u00b7nem", "Ei\u00b7sen", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wofern es mir erlaubt", "tokens": ["Wo\u00b7fern", "es", "mir", "er\u00b7laubt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Und er nur selber glaubt/", "tokens": ["Und", "er", "nur", "sel\u00b7ber", "glaubt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Was ihn/ ", "tokens": ["Was", "ihn", "/"], "token_info": ["word", "word", "punct"], "pos": ["PWS", "PPER", "$("], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Und was mit Zunder-reicher Brunst", "tokens": ["Und", "was", "mit", "Zun\u00b7der\u00b7rei\u00b7cher", "Brunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Jhn hat bewegt zur Gegen-Gunst.", "tokens": ["Jhn", "hat", "be\u00b7wegt", "zur", "Ge\u00b7gen\u00b7Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Vor war sein Kram zwar voll und \u00fcberall gespicket", "tokens": ["Vor", "war", "sein", "Kram", "zwar", "voll", "und", "\u00fc\u00b7be\u00b7rall", "ge\u00b7spi\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Man sah\u2019 der Kunden Schaar", "tokens": ["Man", "sah'", "der", "Kun\u00b7den", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Fast alle Stunden dar/", "tokens": ["Fast", "al\u00b7le", "Stun\u00b7den", "dar", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Und seine Handlung lief nach Hertzens Wunsch begl\u00fccket/", "tokens": ["Und", "sei\u00b7ne", "Hand\u00b7lung", "lief", "nach", "Hert\u00b7zens", "Wunsch", "be\u00b7gl\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er selbst nahm alles wol in acht", "tokens": ["Er", "selbst", "nahm", "al\u00b7les", "wol", "in", "acht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PIS", "ADV", "APPR", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und wie\u00df sich embsig Tag und Nacht.", "tokens": ["Und", "wie\u00df", "sich", "emb\u00b7sig", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Allein was w\u00fcrcken nicht die Schickungen von Oben?", "tokens": ["Al\u00b7lein", "was", "w\u00fcr\u00b7cken", "nicht", "die", "Schi\u00b7ckun\u00b7gen", "von", "O\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Hand/ so alles schafft", "tokens": ["Die", "Hand", "/", "so", "al\u00b7les", "schafft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "PIS", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Durch ihre Macht und Krafft", "tokens": ["Durch", "ih\u00b7re", "Macht", "und", "Krafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.40": {"text": "Braucht bey uns Sterblichen die gr\u00f6sten Wunder-Proben;", "tokens": ["Braucht", "bey", "uns", "Sterb\u00b7li\u00b7chen", "die", "gr\u00f6s\u00b7ten", "Wun\u00b7der\u00b7Pro\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Indem ", "tokens": ["In\u00b7dem"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.42": {"text": "Fehlt in dem Kram noch ein ", "tokens": ["Fehlt", "in", "dem", "Kram", "noch", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ART"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.43": {"text": "Es kommet ", "tokens": ["Es", "kom\u00b7met"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.44": {"text": "Sie wil den Urbar bauhn/", "tokens": ["Sie", "wil", "den", "Ur\u00b7bar", "bauhn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Pflegt sich da umbzuschaun/", "tokens": ["Pflegt", "sich", "da", "um\u00b7bzu\u00b7schaun", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.46": {"text": "Alsbald so reget sich der Wahren gantzer Hauffen/", "tokens": ["Als\u00b7bald", "so", "re\u00b7get", "sich", "der", "Wah\u00b7ren", "gant\u00b7zer", "Hauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Man sieht aus Eisen/ Bley und Stahl", "tokens": ["Man", "sieht", "aus", "Ei\u00b7sen", "/", "Bley", "und", "Stahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Auffahren einen Liebes-Strahl.", "tokens": ["Auf\u00b7fah\u00b7ren", "ei\u00b7nen", "Lie\u00b7bes\u00b7Strahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Es eilt das Pflugschaar nach/ und wil sie gar begleiten/", "tokens": ["Es", "eilt", "das", "Pflug\u00b7schaar", "nach", "/", "und", "wil", "sie", "gar", "be\u00b7glei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "$(", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Der Ege-Zincken schwitzt", "tokens": ["Der", "E\u00b7ge\u00b7Zin\u00b7cken", "schwitzt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Von heisser Gluth erhitzt/", "tokens": ["Von", "heis\u00b7ser", "Gluth", "er\u00b7hitzt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.52": {"text": "Die Bohrer fahren aus/ und stellen sich zur Seiten/", "tokens": ["Die", "Boh\u00b7rer", "fah\u00b7ren", "aus", "/", "und", "stel\u00b7len", "sich", "zur", "Sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "KON", "VVFIN", "PRF", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Ja was der gantze Kram nur f\u00fchrt/", "tokens": ["Ja", "was", "der", "gant\u00b7ze", "Kram", "nur", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PRELS", "ART", "ADJA", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Das wird durch diesen Blitz ber\u00fchrt.", "tokens": ["Das", "wird", "durch", "die\