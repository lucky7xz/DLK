{"textgrid.poem.63591": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich dieser Tr\u00e4nen sch\u00e4men? Ew'ge M\u00e4chte,", "tokens": ["Mich", "die\u00b7ser", "Tr\u00e4\u00b7nen", "sch\u00e4\u00b7men", "?", "Ew'\u00b7ge", "M\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "VVINF", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was gabt ihr Tr\u00e4nen uns, wenn solches Leid", "tokens": ["Was", "gabt", "ihr", "Tr\u00e4\u00b7nen", "uns", ",", "wenn", "sol\u00b7ches", "Leid"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "PPER", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Menschenauge nicht zum Tauen br\u00e4chte!", "tokens": ["Ein", "Men\u00b7schen\u00b7au\u00b7ge", "nicht", "zum", "Tau\u00b7en", "br\u00e4ch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn einer hingeht aus der Zeitlichkeit,", "tokens": ["Wenn", "ei\u00b7ner", "hin\u00b7geht", "aus", "der", "Zeit\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der sich am Gl\u00fcck gesonnt, des reifes Leben", "tokens": ["Der", "sich", "am", "Gl\u00fcck", "ge\u00b7sonnt", ",", "des", "rei\u00b7fes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "NN", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In reichen Garben prangte weit und breit,", "tokens": ["In", "rei\u00b7chen", "Gar\u00b7ben", "prang\u00b7te", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Um solchen d\u00fcrft ihr Klage nicht erheben.", "tokens": ["Um", "sol\u00b7chen", "d\u00fcrft", "ihr", "Kla\u00b7ge", "nicht", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "An ihm ward milde das Gesetz vollstreckt,", "tokens": ["An", "ihm", "ward", "mil\u00b7de", "das", "Ge\u00b7setz", "voll\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dem alles Erdendasein untergeben.", "tokens": ["Dem", "al\u00b7les", "Er\u00b7den\u00b7da\u00b7sein", "un\u00b7ter\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und wenn ein m\u00fcdes Haupt der H\u00fcgel deckt,", "tokens": ["Und", "wenn", "ein", "m\u00fc\u00b7des", "Haupt", "der", "H\u00fc\u00b7gel", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das keinen Lohn der Lebensm\u00fch' gesehen", "tokens": ["Das", "kei\u00b7nen", "Lohn", "der", "Le\u00b7bens\u00b7m\u00fch'", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und fragte: ward ich nur zur Qual geweckt?", "tokens": ["Und", "frag\u00b7te", ":", "ward", "ich", "nur", "zur", "Qual", "ge\u00b7weckt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "An dessen Grabe m\u00f6gt ihr klaglos stehen.", "tokens": ["An", "des\u00b7sen", "Gra\u00b7be", "m\u00f6gt", "ihr", "klag\u00b7los", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df er gelebt, war eurer Tr\u00e4nen wert;", "tokens": ["Da\u00df", "er", "ge\u00b7lebt", ",", "war", "eu\u00b7rer", "Tr\u00e4\u00b7nen", "wert", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Nun darf er ausruhn. Ihm ist wohl geschehen.", "tokens": ["Nun", "darf", "er", "aus\u00b7ruhn", ".", "Ihm", "ist", "wohl", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Doch hier! \u2013 ein Kind, mit keiner Schuld beschwert,", "tokens": ["Doch", "hier", "!", "\u2013", "ein", "Kind", ",", "mit", "kei\u00b7ner", "Schuld", "be\u00b7schwert", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "$(", "ART", "NN", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die Blumenseele jedem Lufthauch offen,", "tokens": ["Die", "Blu\u00b7men\u00b7see\u00b7le", "je\u00b7dem", "Luft\u00b7hauch", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Vom Schimmer reinen Morgentaus verkl\u00e4rt;", "tokens": ["Vom", "Schim\u00b7mer", "rei\u00b7nen", "Mor\u00b7gen\u00b7taus", "ver\u00b7kl\u00e4rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Sein ganzes Sein ein sch\u00f6nverk\u00fcndet Hoffen,", "tokens": ["Sein", "gan\u00b7zes", "Sein", "ein", "sch\u00f6n\u00b7ver\u00b7k\u00fcn\u00b7det", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Ein Feiertagsgedanke der Natur,", "tokens": ["Ein", "Fei\u00b7er\u00b7tags\u00b7ge\u00b7dan\u00b7ke", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Die es gebildet aus den zartsten Stoffen,", "tokens": ["Die", "es", "ge\u00b7bil\u00b7det", "aus", "den", "zarts\u00b7ten", "Stof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und doch von ihr vernichtet, spielend nur,", "tokens": ["Und", "doch", "von", "ihr", "ver\u00b7nich\u00b7tet", ",", "spie\u00b7lend", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVPP", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Als ob sie nur am Schaffen sich erfreute,", "tokens": ["Als", "ob", "sie", "nur", "am", "Schaf\u00b7fen", "sich", "er\u00b7freu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Nicht am Erhalten ihrer Kreatur; \u2013", "tokens": ["Nicht", "am", "Er\u00b7hal\u00b7ten", "ih\u00b7rer", "Kre\u00b7a\u00b7tur", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und nun den s\u00fc\u00dfen Leib dem Schmerz zur Beute,", "tokens": ["Und", "nun", "den", "s\u00fc\u00b7\u00dfen", "Leib", "dem", "Schmerz", "zur", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die Seele sehn in Todes\u00e4ngsten ringen,", "tokens": ["Die", "See\u00b7le", "sehn", "in", "To\u00b7de\u00b7s\u00e4ngs\u00b7ten", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die einer M\u00fccke wehzutun sich scheute,", "tokens": ["Die", "ei\u00b7ner", "M\u00fc\u00b7cke", "weh\u00b7zu\u00b7tun", "sich", "scheu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und w\u00e4hrend blasse \u00c4rmchen uns umschlingen,", "tokens": ["Und", "w\u00e4h\u00b7rend", "blas\u00b7se", "\u00c4rm\u00b7chen", "uns", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Veratmen sehn ein liebstes Lebensgl\u00fcck", "tokens": ["Ver\u00b7at\u00b7men", "sehn", "ein", "liebs\u00b7tes", "Le\u00b7bens\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "In jammervoll hilflosem H\u00e4nderingen \u2013 \u2013", "tokens": ["In", "jam\u00b7mer\u00b7voll", "hilf\u00b7lo\u00b7sem", "H\u00e4n\u00b7de\u00b7rin\u00b7gen", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Wer da den Strom der Z\u00e4hren h\u00e4lt zur\u00fcck,", "tokens": ["Wer", "da", "den", "Strom", "der", "Z\u00e4h\u00b7ren", "h\u00e4lt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Ward nicht ges\u00e4ugt von einem Erdenweibe,", "tokens": ["Ward", "nicht", "ge\u00b7s\u00e4ugt", "von", "ei\u00b7nem", "Er\u00b7den\u0