{"textgrid.poem.46947": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alte Fritz sa\u00df drunten in den N\u00e4chten,", "tokens": ["Der", "al\u00b7te", "Fritz", "sa\u00df", "drun\u00b7ten", "in", "den", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf einem Thron aus Thatenglanz gewoben,", "tokens": ["Auf", "ei\u00b7nem", "Thron", "aus", "Tha\u00b7ten\u00b7glanz", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dachte, weil den Busen Seufzer hoben,", "tokens": ["Und", "dach\u00b7te", ",", "weil", "den", "Bu\u00b7sen", "Seuf\u00b7zer", "ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An sein einst freies Volk, das ward zu Knechten.", "tokens": ["An", "sein", "einst", "frei\u00b7es", "Volk", ",", "das", "ward", "zu", "Knech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "$,", "PDS", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da kam, so lange von des Schicksals M\u00e4chten", "tokens": ["Da", "kam", ",", "so", "lan\u00b7ge", "von", "des", "Schick\u00b7sals", "M\u00e4ch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im ird'schen Stand des Lebens aufgehoben,", "tokens": ["Im", "ird'\u00b7schen", "Stand", "des", "Le\u00b7bens", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sein alter Bruder kam jetzt her von droben,", "tokens": ["Sein", "al\u00b7ter", "Bru\u00b7der", "kam", "jetzt", "her", "von", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den sah er und hub an: \u00bbWill Preu\u00dfen fechten?\u00ab", "tokens": ["Den", "sah", "er", "und", "hub", "an", ":", "\u00bb", "Will", "Preu\u00b7\u00dfen", "fech\u00b7ten", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "KON", "VVFIN", "PTKVZ", "$.", "$(", "VMFIN", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der aber sprach mit Siegesglanz im Blicke:", "tokens": ["Der", "a\u00b7ber", "sprach", "mit", "Sie\u00b7ges\u00b7glanz", "im", "Bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "\u00bbich komme dir als Bote, da\u00df erschienen", "tokens": ["\u00bb", "ich", "kom\u00b7me", "dir", "als", "Bo\u00b7te", ",", "da\u00df", "er\u00b7schie\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "KOUS", "NN", "$,", "KOUS", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nun ist die Stunde, wo es bricht die Stricke.\u00ab", "tokens": ["Nun", "ist", "die", "Stun\u00b7de", ",", "wo", "es", "bricht", "die", "Stri\u00b7cke", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da sprang der alte K\u00f6nig auf mit Mienen,", "tokens": ["Da", "sprang", "der", "al\u00b7te", "K\u00f6\u00b7nig", "auf", "mit", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Als ob er selbst zu neuem Kampf sich schicke,", "tokens": ["Als", "ob", "er", "selbst", "zu", "neu\u00b7em", "Kampf", "sich", "schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und sprach: \u00bbJetzt will ich wieder sein mit ihnen.\u00ab", "tokens": ["Und", "sprach", ":", "\u00bb", "Jetzt", "will", "ich", "wie\u00b7der", "sein", "mit", "ih\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanz