{"textgrid.poem.57685": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Wiesengras", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Wiesengras ist lang und weich,", "tokens": ["Das", "Wie\u00b7sen\u00b7gras", "ist", "lang", "und", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne flammt und gl\u00fcht,", "tokens": ["Die", "Son\u00b7ne", "flammt", "und", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um rote Disteln zittert die Luft,", "tokens": ["Um", "ro\u00b7te", "Dis\u00b7teln", "zit\u00b7tert", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die ganze Wiese bl\u00fcht.", "tokens": ["Die", "gan\u00b7ze", "Wie\u00b7se", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie Wachen, stark und scharf bewehrt,", "tokens": ["Wie", "Wa\u00b7chen", ",", "stark", "und", "scharf", "be\u00b7wehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Disteln uns umbl\u00fchn,", "tokens": ["Die", "Dis\u00b7teln", "uns", "um\u00b7bl\u00fchn", ","], "token_info": ["word", "word