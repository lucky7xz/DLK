{"dta.poem.8450": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An ein Veilchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Birg die Thr\u00e4nen der Wehmut, bis mein Liebchen", "tokens": ["Birg", "die", "Thr\u00e4\u00b7nen", "der", "Weh\u00b7mut", ",", "bis", "mein", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Diese Quelle besucht! Entpfl\u00fcckt sie l\u00e4chelnd", "tokens": ["Die\u00b7se", "Quel\u00b7le", "be\u00b7sucht", "!", "Ent\u00b7pfl\u00fcckt", "sie", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "VVPP", "$.", "VVFIN", "PPER", "ADJD"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dich dem Rasen, die Brust mit dir zu schm\u00fccken;", "tokens": ["Dich", "dem", "Ra\u00b7sen", ",", "die", "Brust", "mit", "dir", "zu", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "O dann schmiege dich an ihr Herz, und sag' ihr,", "tokens": ["O", "dann", "schmie\u00b7ge", "dich", "an", "ihr", "Herz", ",", "und", "sag'", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Dass die Tropfen in deinem blauen Kelche", "tokens": ["Dass", "die", "Trop\u00b7fen", "in", "dei\u00b7nem", "blau\u00b7en", "Kel\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Aus der Seele des treusten J\u00fcnglings flossen,", "tokens": ["Aus", "der", "See\u00b7le", "des", "treus\u00b7ten", "J\u00fcng\u00b7lings", "flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Der sein Leben verweinet, und den Tod w\u00fcnscht!", "tokens": ["Der", "sein", "Le\u00b7ben", "ver\u00b7wei\u00b7net", ",", "und", "den", "Tod", "w\u00fcnscht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}