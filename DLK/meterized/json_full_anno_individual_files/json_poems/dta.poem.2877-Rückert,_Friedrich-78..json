{"dta.poem.2877": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir ein weises Wort zu denken und zu schreiben", "tokens": ["Wenn", "dir", "ein", "wei\u00b7ses", "Wort", "zu", "den\u00b7ken", "und", "zu", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich darbot heute, la\u00df es nicht bis morgen bleiben.", "tokens": ["Sich", "dar\u00b7bot", "heu\u00b7te", ",", "la\u00df", "es", "nicht", "bis", "mor\u00b7gen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "$,", "VVIMP", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Noch minder aber wenn Gelegenheit zu thun", "tokens": ["Noch", "min\u00b7der", "a\u00b7ber", "wenn", "Ge\u00b7le\u00b7gen\u00b7heit", "zu", "thun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "KOUS", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hast ein gutes Werk, la\u00df es auf morgen ruhn.", "tokens": ["Du", "hast", "ein", "gu\u00b7tes", "Werk", ",", "la\u00df", "es", "auf", "mor\u00b7gen", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "VVIMP", "PPER", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein unterdr\u00fccktes Wort kommt wieder neugeboren,", "tokens": ["Ein", "un\u00b7ter\u00b7dr\u00fcck\u00b7tes", "Wort", "kommt", "wie\u00b7der", "neu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die unterlassne That doch ist und bleibt verloren.", "tokens": ["Die", "un\u00b7ter\u00b7lass\u00b7ne", "That", "doch", "ist", "und", "bleibt", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "KON", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und geht verloren auch ein Wort, so ists nicht viel;", "tokens": ["Und", "geht", "ver\u00b7lo\u00b7ren", "auch", "ein", "Wort", ",", "so", "ists", "nicht", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "ADV", "ART", "NN", "$,", "ADV", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn nur die That ist Ernst, und der Gedank' ein Spiel.", "tokens": ["Denn", "nur", "die", "That", "ist", "Ernst", ",", "und", "der", "Ge\u00b7dank'", "ein", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "NE", "$,", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du aber, wenn dir Zeit und Ort und Kraft nicht bleiben", "tokens": ["Du", "a\u00b7ber", ",", "wenn", "dir", "Zeit", "und", "Ort", "und", "Kraft", "nicht", "blei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "NN", "KON", "NN", "KON", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Ernst zu thun, magst du mit Ernst dein Spielwerk treiben.", "tokens": ["Den", "Ernst", "zu", "thun", ",", "magst", "du", "mit", "Ernst", "dein", "Spiel\u00b7werk", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VMFIN", "PPER", "APPR", "NE", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}