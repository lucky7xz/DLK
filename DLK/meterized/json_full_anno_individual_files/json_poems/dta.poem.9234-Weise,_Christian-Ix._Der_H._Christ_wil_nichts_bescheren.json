{"dta.poem.9234": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n Der H. Christ wil nichts bescheren", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt freuen sich die kinder", "tokens": ["Jetzt", "freu\u00b7en", "sich", "die", "kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auff ihren heilgen christ/", "tokens": ["Auff", "ih\u00b7ren", "heil\u00b7gen", "christ", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und suchen ihn geschwinder/", "tokens": ["Und", "su\u00b7chen", "ihn", "ge\u00b7schwin\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als er vorhanden ist/", "tokens": ["Als", "er", "vor\u00b7han\u00b7den", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich aber darff nicht fr\u00f6lich seyn/", "tokens": ["Ich", "a\u00b7ber", "darff", "nicht", "fr\u00f6\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein heilger christ der saget nein.", "tokens": ["Mein", "heil\u00b7ger", "christ", "der", "sa\u00b7get", "nein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "VVFIN", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ich hab in allen dingen", "tokens": ["Ich", "hab", "in", "al\u00b7len", "din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergebne zuversicht/", "tokens": ["Ver\u00b7geb\u00b7ne", "zu\u00b7ver\u00b7sicht", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein beten und mein singen", "tokens": ["Mein", "be\u00b7ten", "und", "mein", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hilfft im geringsten nicht/", "tokens": ["Hilfft", "im", "ge\u00b7rings\u00b7ten", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Je mehr ich nur wil fr\u00f6mmer seyn.", "tokens": ["Je", "mehr", "ich", "nur", "wil", "fr\u00f6m\u00b7mer", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Je mehr und \u00f6ffters sagt er nein.", "tokens": ["Je", "mehr", "und", "\u00f6ff\u00b7ters", "sagt", "er", "nein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "VVFIN", "PPER", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Sanct Niclaus will nicht bitten/", "tokens": ["Sanct", "Nicl\u00b7aus", "will", "nicht", "bit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als wie er sonst gethan/", "tokens": ["Als", "wie", "er", "sonst", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sanct Peter hilfts versch\u00fctten/", "tokens": ["Sanct", "Pe\u00b7ter", "hilfts", "ver\u00b7sch\u00fct\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo er nur wei\u00df und kan/", "tokens": ["Wo", "er", "nur", "wei\u00df", "und", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "KON", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und also kans nicht anders seyn/", "tokens": ["Und", "al\u00b7so", "kans", "nicht", "an\u00b7ders", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PTKNEG", "ADV", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein H. christ der saget nein.", "tokens": ["Mein", "H.", "christ", "der", "sa\u00b7get", "nein", "."], "token_info": ["word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "NE", "ART", "VVFIN", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "4. Kein mensch wil b\u00fcrge werden", "tokens": ["Kein", "mensch", "wil", "b\u00fcr\u00b7ge", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "NN", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor mich verlassnes kind/", "tokens": ["Vor", "mich", "ver\u00b7lass\u00b7nes", "kind", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil niemand die beschwerden", "tokens": ["Weil", "nie\u00b7mand", "die", "be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sehr als ich empfind/", "tokens": ["So", "sehr", "als", "ich", "emp\u00b7find", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sanet Ruppert zwickt mich an das bein/", "tokens": ["Sa\u00b7net", "Rup\u00b7pert", "zwickt", "mich", "an", "das", "bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Der H. christ der saget nein.", "tokens": ["Der", "H.", "christ", "der", "sa\u00b7get", "nein", "."], "token_info": ["word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "ART", "VVFIN", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "5. Die leute/ so ihn kennen/", "tokens": ["Die", "leu\u00b7te", "/", "so", "ihn", "ken\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die wissen/ wer er ist/", "tokens": ["Die", "wis\u00b7sen", "/", "wer", "er", "ist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "$(", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df ihn nicht zu nennen/", "tokens": ["Ich", "wei\u00df", "ihn", "nicht", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als nur den H. christ/", "tokens": ["Als", "nur", "den", "H.", "christ", "/"], "token_info": ["word", "word", "word", "abbreviation", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NE", "NE", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich hoff/ und denck auff ihn allein/", "tokens": ["Ich", "hoff", "/", "und", "denck", "auff", "ihn", "al\u00b7lein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVIMP", "APPR", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und gleichwohl sagt er immer nein.", "tokens": ["Und", "gleich\u00b7wohl", "sagt", "er", "im\u00b7mer", "nein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Ach la\u00df dich doch behandeln/", "tokens": ["Ach", "la\u00df", "dich", "doch", "be\u00b7han\u00b7deln", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du lieber H. christ/", "tokens": ["Du", "lie\u00b7ber", "H.", "christ", "/"], "token_info": ["word", "word", "abbreviation", "word", "punct"], "pos": ["PPER", "ADV", "NE", "NE", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du kanst dich leicht verwandeln/", "tokens": ["Du", "kanst", "dich", "leicht", "ver\u00b7wan\u00b7deln", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil du barmhertzig bist/", "tokens": ["Weil", "du", "barm\u00b7hert\u00b7zig", "bist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach stelle dich geneigter ein/", "tokens": ["Ach", "stel\u00b7le", "dich", "ge\u00b7neig\u00b7ter", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PRF", "APPR", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sage nur nicht allzeit nein", "tokens": ["Und", "sa\u00b7ge", "nur", "nicht", "all\u00b7zeit", "nein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADV", "PTKANT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Ich wil nicht viel begehren/", "tokens": ["Ich", "wil", "nicht", "viel", "be\u00b7geh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein wille soll geschehn/", "tokens": ["Dein", "wil\u00b7le", "soll", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df du mir nur bescheren/", "tokens": ["La\u00df", "du", "mir", "nur", "be\u00b7sche\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich wil es gerne sehn/", "tokens": ["Ich", "wil", "es", "ger\u00b7ne", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und solt es nur ein r\u00f6\u00dfgen seyn/", "tokens": ["Und", "solt", "es", "nur", "ein", "r\u00f6\u00df\u00b7gen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur sage nicht best\u00e4ndig nein.", "tokens": ["Nur", "sa\u00b7ge", "nicht", "be\u00b7st\u00e4n\u00b7dig", "nein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADJD", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Inzwischen sol ich leiden/", "tokens": ["I\u00b7nzwi\u00b7schen", "sol", "ich", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Was du mir unrecht thust/", "tokens": ["Was", "du", "mir", "un\u00b7recht", "thust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll ich die gaben meyden", "tokens": ["Soll", "ich", "die", "ga\u00b7ben", "mey\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alle kinder-lust/", "tokens": ["Und", "al\u00b7le", "kin\u00b7der\u00b7lust", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So geb ich mich gedultig drein/", "tokens": ["So", "geb", "ich", "mich", "ge\u00b7dul\u00b7tig", "drein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sag jetzo doch nicht ewig nein.", "tokens": ["Sag", "jet\u00b7zo", "doch", "nicht", "e\u00b7wig", "nein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PTKNEG", "ADJD", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}