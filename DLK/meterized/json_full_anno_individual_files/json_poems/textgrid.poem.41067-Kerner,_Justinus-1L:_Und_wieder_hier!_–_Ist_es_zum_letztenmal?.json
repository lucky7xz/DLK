{"textgrid.poem.41067": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und wieder hier! \u2013 Ist es zum letztenmal?", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder hier! \u2013 Ist es zum letztenmal?", "tokens": ["Und", "wie\u00b7der", "hier", "!", "\u2013", "Ist", "es", "zum", "letz\u00b7ten\u00b7mal", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$.", "$(", "VAFIN", "PPER", "APPRART", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der kranken Augen bald erloschner Strahl", "tokens": ["Der", "kran\u00b7ken", "Au\u00b7gen", "bald", "er\u00b7lo\u00b7schner", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sieht kaum noch deinen blauen Himmel blinken,", "tokens": ["Sieht", "kaum", "noch", "dei\u00b7nen", "blau\u00b7en", "Him\u00b7mel", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O du mir einst so licht gewesnes Tal.", "tokens": ["O", "du", "mir", "einst", "so", "licht", "ge\u00b7wes\u00b7nes", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "ADV", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So la\u00df nur, um zu stillen meine Qual,", "tokens": ["So", "la\u00df", "nur", ",", "um", "zu", "stil\u00b7len", "mei\u00b7ne", "Qual", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich in die Waldnacht deiner Berge sinken!", "tokens": ["Mich", "in", "die", "Wald\u00b7nacht", "dei\u00b7ner", "Ber\u00b7ge", "sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Als ich mich so von Haus und Menschen stahl", "tokens": ["Als", "ich", "mich", "so", "von", "Haus", "und", "Men\u00b7schen", "stahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hin, wo aus deines frommen Klosters Hallen", "tokens": ["Hin", ",", "wo", "aus", "dei\u00b7nes", "from\u00b7men", "Klos\u00b7ters", "Hal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ges\u00e4nge auf zur nahen Waldnacht schallen,", "tokens": ["Ge\u00b7s\u00e4n\u00b7ge", "auf", "zur", "na\u00b7hen", "Wald\u00b7nacht", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit meinem Kummer hier allein zu wallen,", "tokens": ["Mit", "mei\u00b7nem", "Kum\u00b7mer", "hier", "al\u00b7lein", "zu", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Geschah's, da\u00df pl\u00f6tzlich hell mein Auge sah.", "tokens": ["Ge\u00b7scha\u00b7h's", ",", "da\u00df", "pl\u00f6tz\u00b7lich", "hell", "mein", "Au\u00b7ge", "sah", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADJD", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und sieh! ein lichtes Wesen stand mir nah,", "tokens": ["Und", "sieh", "!", "ein", "lich\u00b7tes", "We\u00b7sen", "stand", "mir", "nah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das sprach, wie einer Harfe leis Verhallen:", "tokens": ["Das", "sprach", ",", "wie", "ei\u00b7ner", "Har\u00b7fe", "leis", "Ver\u00b7hal\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbich bin, la\u00df dir mein tr\u00f6stend Wort gefallen,", "tokens": ["\u00bb", "ich", "bin", ",", "la\u00df", "dir", "mein", "tr\u00f6s\u00b7tend", "Wort", "ge\u00b7fal\u00b7len", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$,", "VVIMP", "PPER", "PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Des Berges Heilige \u2013 C\u00e4cilia.", "tokens": ["Des", "Ber\u00b7ges", "Hei\u00b7li\u00b7ge", "\u2013", "C\u00e4\u00b7ci\u00b7lia", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$(", "NE", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Kleinm\u00fctiger! verzweifle l\u00e4nger nicht!", "tokens": ["Klein\u00b7m\u00fc\u00b7ti\u00b7ger", "!", "ver\u00b7zweif\u00b7le", "l\u00e4n\u00b7ger", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Geht dir das \u00e4u\u00dfere Auge auch verloren,", "tokens": ["Geht", "dir", "das", "\u00e4u\u00b7\u00dfe\u00b7re", "Au\u00b7ge", "auch", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der S\u00e4nger ist zum innern Schaun geboren \u2013", "tokens": ["Der", "S\u00e4n\u00b7ger", "ist", "zum", "in\u00b7nern", "Schaun", "ge\u00b7bo\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Kl\u00e4nge der Natur \u2013 auch sie sind Licht.", "tokens": ["Die", "Kl\u00e4n\u00b7ge", "der", "Na\u00b7tur", "\u2013", "auch", "sie", "sind", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ADV", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Quellen rauschen und die V\u00f6gel singen,", "tokens": ["Die", "Quel\u00b7len", "rau\u00b7schen", "und", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ton aus Metallen und aus Steinen bricht,", "tokens": ["Ton", "aus", "Me\u00b7tal\u00b7len", "und", "aus", "Stei\u00b7nen", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "In Farb' und Bildern wird dein Geist ihn bringen!\u00ab", "tokens": ["In", "Fa\u00b7rb'", "und", "Bil\u00b7dern", "wird", "dein", "Geist", "ihn", "brin\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.5": {"line.1": {"text": "Die Heilige verschwand; melodisch Klingen", "tokens": ["Die", "Hei\u00b7li\u00b7ge", "ver\u00b7schwand", ";", "me\u00b7lo\u00b7disch", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "$.", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von B\u00e4umen, Quellen, V\u00f6geln hob mit Macht", "tokens": ["Von", "B\u00e4u\u00b7men", ",", "Quel\u00b7len", ",", "V\u00f6\u00b7geln", "hob", "mit", "Macht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Herze mir, ich stieg zu Tal, das lacht", "tokens": ["Das", "Her\u00b7ze", "mir", ",", "ich", "stieg", "zu", "Tal", ",", "das", "lacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "APPR", "NN", "$,", "PDS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie ehmals freundlich mir im Sonnenstrahl.", "tokens": ["Wie", "eh\u00b7mals", "freund\u00b7lich", "mir", "im", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ja, la\u00df auf deine Matten sanft mich sinken,", "tokens": ["Ja", ",", "la\u00df", "auf", "dei\u00b7ne", "Mat\u00b7ten", "sanft", "mich", "sin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVIMP", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einatmen tief, aus m\u00e4chtigem Pokal", "tokens": ["Ei\u00b7nat\u00b7men", "tief", ",", "aus", "m\u00e4ch\u00b7ti\u00b7gem", "Po\u00b7kal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Morgen- und von Abendgold, mein Tal!", "tokens": ["Von", "Mor\u00b7gen", "und", "von", "A\u00b7bend\u00b7gold", ",", "mein", "Tal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "APPR", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Heilborn deiner milden Luft zu trinken.", "tokens": ["Den", "Heil\u00b7born", "dei\u00b7ner", "mil\u00b7den", "Luft", "zu", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und wieder hier! \u2013 Ist es zum letztenmal?", "tokens": ["Und", "wie\