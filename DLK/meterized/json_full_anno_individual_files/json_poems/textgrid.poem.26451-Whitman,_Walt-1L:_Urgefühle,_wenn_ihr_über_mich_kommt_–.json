{"textgrid.poem.26451": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Urgef\u00fchle, wenn ihr \u00fcber mich kommt \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Urgef\u00fchle, wenn ihr \u00fcber mich kommt \u2013", "tokens": ["Ur\u00b7ge\u00b7f\u00fch\u00b7le", ",", "wenn", "ihr", "\u00fc\u00b7ber", "mich", "kommt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "O, jetzt seid ihr da!", "tokens": ["O", ",", "jetzt", "seid", "ihr", "da", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fcllt mich mit schaffender Sch\u00f6nheit,", "tokens": ["F\u00fcllt", "mich", "mit", "schaf\u00b7fen\u00b7der", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Mit der Lawine meiner Leidenschaften,", "tokens": ["Mit", "der", "La\u00b7wi\u00b7ne", "mei\u00b7ner", "Lei\u00b7den\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit l\u00fcsternem Leben, grob und geil!", "tokens": ["Mit", "l\u00fcs\u00b7ter\u00b7nem", "Le\u00b7ben", ",", "grob", "und", "geil", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "NN", "$."], "meter": "-+--+-+-+", 