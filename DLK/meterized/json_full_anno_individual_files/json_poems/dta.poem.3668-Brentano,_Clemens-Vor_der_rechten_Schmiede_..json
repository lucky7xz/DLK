{"dta.poem.3668": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Vor der rechten Schmiede .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beschlag, beschlag's R\u00f6\u00dfle,               ", "tokens": ["Be\u00b7schlag", ",", "be\u00b7schlag's", "R\u00f6\u00df\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zu Ulm steht ein Schl\u00f6\u00dfle,", "tokens": ["Zu", "Ulm", "steht", "ein", "Schl\u00f6\u00df\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Steht ein Schmiedle nah dabei,", "tokens": ["Steht", "ein", "Schmied\u00b7le", "nah", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmiedle adschlag mirs R\u00f6\u00dfle gleich,", "tokens": ["Schmied\u00b7le", "adsc\u00b7hlag", "mirs", "R\u00f6\u00df\u00b7le", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "NE", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Hab ich N\u00e4gele zu tief rein g'schlage,", "tokens": ["Hab", "ich", "N\u00e4\u00b7ge\u00b7le", "zu", "tief", "rein", "g'\u00b7schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "PTKA", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.6": {"text": "Mu\u00df ichs wieder rausser grabe.", "tokens": ["Mu\u00df", "ichs", "wie\u00b7der", "raus\u00b7ser", "gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}