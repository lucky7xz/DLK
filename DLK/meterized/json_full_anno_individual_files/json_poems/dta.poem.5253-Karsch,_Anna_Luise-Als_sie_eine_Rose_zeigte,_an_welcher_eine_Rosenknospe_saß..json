{"dta.poem.5253": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Als sie eine Rose zeigte, an welcher  \n eine Rosenknospe sa\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Die Rosenschwester ist noch klein,", "tokens": ["Die", "Ro\u00b7sen\u00b7schwes\u00b7ter", "ist", "noch", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie ist aufgebl\u00fcht, wird diese nicht mehr seyn.", "tokens": ["Wenn", "sie", "ist", "auf\u00b7ge\u00b7bl\u00fcht", ",", "wird", "die\u00b7se", "nicht", "mehr", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "$,", "VAFIN", "PDS", "PTKNEG", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}