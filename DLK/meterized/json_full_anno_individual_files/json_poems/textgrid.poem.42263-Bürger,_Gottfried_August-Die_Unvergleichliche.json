{"textgrid.poem.42263": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Die Unvergleichliche", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch Ideal aus Engelsphantasie", "tokens": ["Welch", "I\u00b7deal", "aus", "En\u00b7gel\u00b7sphan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat der Natur als Muster vorgeschwebet,", "tokens": ["Hat", "der", "Na\u00b7tur", "als", "Mus\u00b7ter", "vor\u00b7ge\u00b7schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als sie die H\u00fcll' um einen Geist gewebet,", "tokens": ["Als", "sie", "die", "H\u00fcll'", "um", "ei\u00b7nen", "Geist", "ge\u00b7we\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den sie herab vom dritten Himmel lieh?", "tokens": ["Den", "sie", "her\u00b7ab", "vom", "drit\u00b7ten", "Him\u00b7mel", "lieh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O G\u00f6tterwerk! Mit welcher Harmonie", "tokens": ["O", "G\u00f6t\u00b7ter\u00b7werk", "!", "Mit", "wel\u00b7cher", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier Geist in Leib und Leib in Geist verschwebet!", "tokens": ["Hier", "Geist", "in", "Leib", "und", "Leib", "in", "Geist", "ver\u00b7schwe\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An Allem, was hienieden Sch\u00f6nes lebet,", "tokens": ["An", "Al\u00b7lem", ",", "was", "hien\u00b7ie\u00b7den", "Sch\u00f6\u00b7nes", "le\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vernahm mein Sinn so reinen Einklang nie.", "tokens": ["Ver\u00b7nahm", "mein", "Sinn", "so", "rei\u00b7nen", "Ein\u00b7klang", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der, welchem noch der Adel ihrer Mienen,", "tokens": ["Der", ",", "wel\u00b7chem", "noch", "der", "A\u00b7del", "ih\u00b7rer", "Mie\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Himmel nie in ihrem Aug' erschienen,", "tokens": ["Der", "Him\u00b7mel", "nie", "in", "ih\u00b7rem", "Aug'", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entweiht vielleicht mein hohes Lied durch