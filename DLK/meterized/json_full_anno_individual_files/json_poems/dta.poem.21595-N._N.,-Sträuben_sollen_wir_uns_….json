{"dta.poem.21595": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Str\u00e4uben sollen wir uns \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Str\u00e4uben sollen wir uns wider das Eisenjoch,", "tokens": ["Str\u00e4u\u00b7ben", "sol\u00b7len", "wir", "uns", "wi\u00b7der", "das", "Ei\u00b7sen\u00b7joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dem der Gewohnheit Schmutz W\u00fcrde des Alters lieh;", "tokens": ["Dem", "der", "Ge\u00b7wohn\u00b7heit", "Schmutz", "W\u00fcr\u00b7de", "des", "Al\u00b7ters", "lieh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Wen das steigende Licht gr\u00fc\u00dft,", "tokens": ["Wen", "das", "stei\u00b7gen\u00b7de", "Licht", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nicht sehn\u2019 er die Nacht zur\u00fcck!", "tokens": ["Nicht", "sehn'", "er", "die", "Nacht", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Feigheit knechtet die Zeit, beuget der Nacken Kraft;", "tokens": ["Feig\u00b7heit", "knech\u00b7tet", "die", "Zeit", ",", "beu\u00b7get", "der", "Na\u00b7cken", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Wenige wagen nur frei zu gestehen, was", "tokens": ["We\u00b7ni\u00b7ge", "wa\u00b7gen", "nur", "frei", "zu", "ge\u00b7ste\u00b7hen", ",", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$,", "PWS"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "L\u00e4ngst ihr k\u00fchnerer Blick sah,", "tokens": ["L\u00e4ngst", "ihr", "k\u00fch\u00b7ne\u00b7rer", "Blick", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "L\u00e4ngst ihnen im Busen lebt.", "tokens": ["L\u00e4ngst", "ih\u00b7nen", "im", "Bu\u00b7sen", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Weit noch seltener sind aus der Berufnen Schaar,", "tokens": ["Weit", "noch", "sel\u00b7te\u00b7ner", "sind", "aus", "der", "Be\u00b7ruf\u00b7nen", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die, der Lebendigkeit th\u00e4tigen Daseins Freund,", "tokens": ["Die", ",", "der", "Le\u00b7ben\u00b7dig\u00b7keit", "th\u00e4\u00b7ti\u00b7gen", "Da\u00b7seins", "Freund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In die Speichen des Rades", "tokens": ["In", "die", "Spei\u00b7chen", "des", "Ra\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Eingreifen mit fester Hand,", "tokens": ["Ein\u00b7grei\u00b7fen", "mit", "fes\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Heilig gelten der Zeit Rechte des Alters nur:", "tokens": ["Hei\u00b7lig", "gel\u00b7ten", "der", "Zeit", "Rech\u00b7te", "des", "Al\u00b7ters", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "NN", "ART", "NN", "ADV", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Weil es bestand vordem, ist es bestehenswerth!", "tokens": ["Weil", "es", "be\u00b7stand", "vor\u00b7dem", ",", "ist", "es", "be\u00b7ste\u00b7hens\u00b7werth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Heilig gelten der Zeit nicht", "tokens": ["Hei\u00b7lig", "gel\u00b7ten", "der", "Zeit", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "PTKNEG"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Treupflichten des eignen Sinns. \u2014", "tokens": ["Treu\u00b7pf\u00b7lich\u00b7ten", "des", "eig\u00b7nen", "Sinns", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Str\u00e4uben sollen wir uns wider das Eisenjoch,", "tokens": ["Str\u00e4u\u00b7ben", "sol\u00b7len", "wir", "uns", "wi\u00b7der", "das", "Ei\u00b7sen\u00b7joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dem der Gewohnheit Schmutz W\u00fcrde des Alters lieh;", "tokens": ["Dem", "der", "Ge\u00b7wohn\u00b7heit", "Schmutz", "W\u00fcr\u00b7de", "des", "Al\u00b7ters", "lieh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Wen das steigende Licht gr\u00fc\u00dft,", "tokens": ["Wen", "das", "stei\u00b7gen\u00b7de", "Licht", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nicht sehn\u2019 er die Nacht zur\u00fcck!", "tokens": ["Nicht", "sehn'", "er", "die", "Nacht", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}