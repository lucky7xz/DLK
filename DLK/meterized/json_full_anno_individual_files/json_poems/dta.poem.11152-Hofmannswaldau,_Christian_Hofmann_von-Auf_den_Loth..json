{"dta.poem.11152": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Loth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwey engel haben dich aus einem brand' erl\u00f6st,", "tokens": ["Zwey", "en\u00b7gel", "ha\u00b7ben", "dich", "aus", "ei\u00b7nem", "brand'", "er\u00b7l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwey weiber haben dir den andern eingefl\u00f6st.", "tokens": ["Zwey", "wei\u00b7ber", "ha\u00b7ben", "dir", "den", "an\u00b7dern", "ein\u00b7ge\u00b7fl\u00f6st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus was die flucht dich bracht, drein st\u00fcrtzt dich die begier;", "tokens": ["Aus", "was", "die", "flucht", "dich", "bracht", ",", "drein", "st\u00fcrtzt", "dich", "die", "be\u00b7gier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "VVFIN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Sodom l\u00e4st du dort, das andre findst du hier.", "tokens": ["Ein", "So\u00b7dom", "l\u00e4st", "du", "dort", ",", "das", "and\u00b7re", "findst", "du", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PIS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}