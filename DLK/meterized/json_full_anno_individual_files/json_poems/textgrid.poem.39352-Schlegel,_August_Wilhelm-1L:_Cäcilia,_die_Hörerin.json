{"textgrid.poem.39352": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: C\u00e4cilia, die H\u00f6rerin", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "C\u00e4cilia, die H\u00f6rerin", "tokens": ["C\u00e4\u00b7ci\u00b7lia", ",", "die", "H\u00f6\u00b7re\u00b7rin"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der hohen Himmels Harmonieen,", "tokens": ["Der", "ho\u00b7hen", "Him\u00b7mels", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hat ihren Namen selbst bedeutsam dir verliehen.", "tokens": ["Hat", "ih\u00b7ren", "Na\u00b7men", "selbst", "be\u00b7deut\u00b7sam", "dir", "ver\u00b7lie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Mutter ward dir sichre F\u00fchrerin", "tokens": ["Die", "Mut\u00b7ter", "ward", "dir", "sich\u00b7re", "F\u00fch\u00b7re\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im vielverschlung'nen Labyrinth der T\u00f6ne;", "tokens": ["Im", "viel\u00b7ver\u00b7schlung'\u00b7nen", "La\u00b7by\u00b7rinth", "der", "T\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {