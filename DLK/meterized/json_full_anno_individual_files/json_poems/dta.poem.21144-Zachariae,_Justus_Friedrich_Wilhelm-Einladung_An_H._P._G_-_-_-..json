{"dta.poem.21144": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Einladung  \n An H. P. G - - -.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund, unser Leben ist kurz, der Thoren aber sind\nviel,", "tokens": ["Freund", ",", "un\u00b7ser", "Le\u00b7ben", "ist", "kurz", ",", "der", "Tho\u00b7ren", "a\u00b7ber", "sind", "viel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADV", "VAFIN", "ADV", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die uns die theuren Stunden entziehn.", "tokens": ["Die", "uns", "die", "theu\u00b7ren", "Stun\u00b7den", "ent\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sey geitzig, Freund, auf die Zeit, die uns die Freund-", "tokens": ["Sey", "geit\u00b7zig", ",", "Freund", ",", "auf", "die", "Zeit", ",", "die", "uns", "die", "Freun\u00b7d"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "NN", "$,", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ART", "TRUNC"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Es sey uns jede Stunde, wie Gold.", "tokens": ["Es", "sey", "uns", "je\u00b7de", "Stun\u00b7de", ",", "wie", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Schon lange gr\u00fcnt uns nicht mehr der abgestorbene", "tokens": ["Schon", "lan\u00b7ge", "gr\u00fcnt", "uns", "nicht", "mehr", "der", "ab\u00b7ge\u00b7stor\u00b7be\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der in den s\u00fcssen Schatten uns rief;", "tokens": ["Der", "in", "den", "s\u00fcs\u00b7sen", "Schat\u00b7ten", "uns", "rief", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Schon lange singt uns nicht mehr der Vogel Z\u00e4rtlich-", "tokens": ["Schon", "lan\u00b7ge", "singt", "uns", "nicht", "mehr", "der", "Vo\u00b7gel", "Z\u00e4rt\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NE", "TRUNC"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und w\u00fcste St\u00fcrme brausen daher.", "tokens": ["Und", "w\u00fcs\u00b7te", "St\u00fcr\u00b7me", "brau\u00b7sen", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Der Schenktisch l\u00e4chelt zwar auch in Strephons pr\u00e4ch-", "tokens": ["Der", "Schenk\u00b7tisch", "l\u00e4\u00b7chelt", "zwar", "auch", "in", "Stre\u00b7phons", "pr\u00e4ch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "NE", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus heitern Caravinen dir zu;", "tokens": ["Aus", "hei\u00b7tern", "Ca\u00b7ra\u00b7vi\u00b7nen", "dir", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Doch, Freund, der pr\u00e4chtige Saal herberget l\u00fcgenden", "tokens": ["Doch", ",", "Freund", ",", "der", "pr\u00e4ch\u00b7ti\u00b7ge", "Saal", "her\u00b7ber\u00b7get", "l\u00fc\u00b7gen\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und einen Narren, schlimm, wie sein Wein.", "tokens": ["Und", "ei\u00b7nen", "Nar\u00b7ren", ",", "schlimm", ",", "wie", "sein", "Wein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Nein, G \u2014, eile zu mir! wie froh erwartet dich schon", "tokens": ["Nein", ",", "G", ",", "ei\u00b7le", "zu", "mir", "!", "wie", "froh", "er\u00b7war\u00b7tet", "dich", "schon"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NE", "$(", "$,", "VVFIN", "APPR", "PPER", "$.", "PWAV", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Das Weinglas, und mein treues Clavier!", "tokens": ["Das", "Wein\u00b7glas", ",", "und", "mein", "treu\u00b7es", "Cla\u00b7vier", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein ungeschwefelter Wein, und von der Lieb ein Ge-", "tokens": ["Ein", "un\u00b7ge\u00b7schwe\u00b7fel\u00b7ter", "Wein", ",", "und", "von", "der", "Lieb", "ein", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Geht allen Festen der K\u00f6nige vor.", "tokens": ["Geht", "al\u00b7len", "Fes\u00b7ten", "der", "K\u00f6\u00b7ni\u00b7ge", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wer wei\u00df, wie lange das Gl\u00fcck uns hier beysammen", "tokens": ["Wer", "wei\u00df", ",", "wie", "lan\u00b7ge", "das", "Gl\u00fcck", "uns", "hier", "bey\u00b7sam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "PPER", "ADV", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da es uns immer grausam getrennt.", "tokens": ["Da", "es", "uns", "im\u00b7mer", "grau\u00b7sam", "ge\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es hat vielleicht uns vereint, um noch grausamer zu", "tokens": ["Es", "hat", "viel\u00b7leicht", "uns", "ver\u00b7eint", ",", "um", "noch", "grau\u00b7sa\u00b7mer", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "VVPP", "$,", "KOUI", "ADV", "ADJD", "PTKZU"], "meter": "-+-+--+-+++-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Wenn es uns wieder schrecklicher trennt.", "tokens": ["Wenn", "es", "uns", "wie\u00b7der", "schreck\u00b7li\u00b7cher", "trennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Freund, wo ist G \u2014 hin. Er ward uns wieder ge-", "tokens": ["Freund", ",", "wo", "ist", "G", "hin", ".", "Er", "ward", "uns", "wie\u00b7der", "ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VAFIN", "NE", "$(", "PTKVZ", "$.", "PPER", "VAFIN", "PPER", "ADV", "TRUNC"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nun bringt kein Wunsch ihn wieder zur\u00fcck.", "tokens": ["Nun", "bringt", "kein", "Wunsch", "ihn", "wie\u00b7der", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Es flie\u00dft ein trauriger Bach tief in das einsame Thal;", "tokens": ["Es", "flie\u00dft", "ein", "trau\u00b7ri\u00b7ger", "Bach", "tief", "in", "das", "ein\u00b7sa\u00b7me", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Allein er flie\u00dft nicht wieder zur\u00fcck.", "tokens": ["Al\u00b7lein", "er", "flie\u00dft", "nicht", "wie\u00b7der", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "O Freund, komm eilig zu mir, und scherz den Unmuth", "tokens": ["O", "Freund", ",", "komm", "ei\u00b7lig", "zu", "mir", ",", "und", "scherz", "den", "Un\u00b7muth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "ADJD", "APPR", "PPER", "$,", "KON", "ADJD", "ART", "NN"], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Der unsre tr\u00fcben Stirnen umw\u00f6lkt!", "tokens": ["Der", "uns\u00b7re", "tr\u00fc\u00b7ben", "Stir\u00b7nen", "um\u00b7w\u00f6lkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Es fliehe Schwermuth und Gram, wenn das hellt\u00f6-", "tokens": ["Es", "flie\u00b7he", "Schwer\u00b7muth", "und", "Gram", ",", "wenn", "das", "hell\u00b7t\u00f6"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "KOUS", "ART", "TRUNC"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf unsrer Freunde Wohlseyn erklingt!", "tokens": ["Auf", "uns\u00b7rer", "Freun\u00b7de", "Wohl\u00b7seyn", "er\u00b7klingt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}