{"textgrid.poem.62674": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine gewisse artige Liebes-Begebenheit", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6ne ", "tokens": ["Die", "sch\u00f6\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und seufzte von Herzen nach ihren Galan:", "tokens": ["Und", "seufz\u00b7te", "von", "Her\u00b7zen", "nach", "ih\u00b7ren", "Ga\u00b7lan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Inzwischen kam ", "tokens": ["I\u00b7nzwi\u00b7schen", "kam"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und sagte: Mein Kindgen! wohlan! ich erscheine,", "tokens": ["Und", "sag\u00b7te", ":", "Mein", "Kind\u00b7gen", "!", "wo\u00b7hlan", "!", "ich", "er\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "NN", "$.", "ADV", "$.", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Damit sich mein Herze mit deinem vereine.", "tokens": ["Da\u00b7mit", "sich", "mein", "Her\u00b7ze", "mit", "dei\u00b7nem", "ver\u00b7ei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "VVFIN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie nennten die Treue den irrigen Wahn,", "tokens": ["Sie", "nenn\u