{"textgrid.poem.33890": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11. Nachklang", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, zauberische Huldgestalt,", "tokens": ["Ach", ",", "zau\u00b7be\u00b7ri\u00b7sche", "Huld\u00b7ge\u00b7stalt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die nie vergessen l\u00e4\u00dft!", "tokens": ["Die", "nie", "ver\u00b7ges\u00b7sen", "l\u00e4\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du h\u00e4ltst mit ewiger Gewalt", "tokens": ["Du", "h\u00e4ltst", "mit", "e\u00b7wi\u00b7ger", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich noch im Tode fest!", "tokens": ["Mich", "noch", "im", "To\u00b7de", "fest", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du spielst, ein sanftes Abendroth,", "tokens": ["Du", "spielst", ",", "ein", "sanf\u00b7tes", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In meine Brust hinein,", "tokens": ["In", "mei\u00b7ne", "Brust", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und bist du allenthalben todt,", "tokens": ["Und", "bist", "du", "al\u00b7len\u00b7thal\u00b7ben", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dort wirst du's nimmer sein.", "tokens": ["Dort", "wirst", "du's", "nim\u00b7mer", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, zauberische Huldgestalt,", "tokens": ["Ach", ",", "zau\u00b7be\u00b7ri\u00b7sche", "Huld\u00b7ge\u00b7stalt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die nie vergessen l\u00e4\u00dft!", "tokens": ["Die", "nie", "ver\u00b7ges\u00b7sen", "l\u00e4\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du h\u00e4ltst mit ewiger Gewalt", "tokens": ["Du", "h\u00e4ltst", "mit", "e\u00b7wi\u00b7ger", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich noch im Tode fest!", "tokens": ["Mich", "noch", "im", "To\u00b7de", "fest", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du spielst, ein sanftes Abendroth,", "tokens": ["Du", "spielst", ",", "ein", "sanf\u00b7tes", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "