{"dta.poem.20353": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Als sie bey tr\u00fcbem sturm-wetter ihre  \n w\u00e4sche bleichete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm sanffter westen wind/ und brich den kalten norden/", "tokens": ["Komm", "sanff\u00b7ter", "wes\u00b7ten", "wind", "/", "und", "brich", "den", "kal\u00b7ten", "nor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "NE", "$(", "KON", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der heute mit gewalt durch alle g\u00e4rten streicht/", "tokens": ["Der", "heu\u00b7te", "mit", "ge\u00b7walt", "durch", "al\u00b7le", "g\u00e4r\u00b7ten", "streicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und macht/ da\u00df Solimen/ da sie die w\u00e4sche bleicht/", "tokens": ["Und", "macht", "/", "da\u00df", "So\u00b7li\u00b7men", "/", "da", "sie", "die", "w\u00e4\u00b7sche", "bleicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "NN", "$(", "KOUS", "PPER", "ART", "ADJA", "VVFIN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die strahlen ihrer lust sind zu gew\u00f6lcken worden.", "tokens": ["Die", "strah\u00b7len", "ih\u00b7rer", "lust", "sind", "zu", "ge\u00b7w\u00f6l\u00b7cken", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VAFIN", "PTKZU", "VVINF", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vertreibe diesen feind durch ein gelindes wehen/", "tokens": ["Ver\u00b7trei\u00b7be", "die\u00b7sen", "feind", "durch", "ein", "ge\u00b7lin\u00b7des", "we\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Beut alle Zephyren zu deinen diensten auff/", "tokens": ["Beut", "al\u00b7le", "Ze\u00b7phy\u00b7ren", "zu", "dei\u00b7nen", "diens\u00b7ten", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "APPR", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und hemme seiner macht den vorgesetzten lauff/", "tokens": ["Und", "hem\u00b7me", "sei\u00b7ner", "macht", "den", "vor\u00b7ge\u00b7setz\u00b7ten", "lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der selber der natur will ihren weg verdrehen.", "tokens": ["Der", "sel\u00b7ber", "der", "na\u00b7tur", "will", "ih\u00b7ren", "weg", "ver\u00b7dre\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VMFIN", "PPOSAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die erde lieget schon der Solime zun f\u00fcssen/", "tokens": ["Die", "er\u00b7de", "lie\u00b7get", "schon", "der", "So\u00b7li\u00b7me", "zun", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und beut ihr klee und gra\u00df zum gr\u00fcnen bleich-platz an;", "tokens": ["Und", "beut", "ihr", "klee", "und", "gra\u00df", "zum", "gr\u00fc\u00b7nen", "bleich\u00b7platz", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie thut ihr selbst gewalt/ und schm\u00fccket wie sie kan", "tokens": ["Sie", "thut", "ihr", "selbst", "ge\u00b7walt", "/", "und", "schm\u00fc\u00b7cket", "wie", "sie", "kan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$(", "KON", "VVFIN", "KOKOM", "PPER", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ihr buntes sommer-kleid mit tulpen und narcissen.", "tokens": ["Ihr", "bun\u00b7tes", "som\u00b7mer\u00b7kleid", "mit", "tul\u00b7pen", "und", "nar\u00b7cis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was s\u00e4umstu sonne nun mit deinen heissen blicken?", "tokens": ["Was", "s\u00e4ums\u00b7tu", "son\u00b7ne", "nun", "mit", "dei\u00b7nen", "heis\u00b7sen", "bli\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zerrei\u00df der wolcken flor/ bestrahle deine braut/", "tokens": ["Zer\u00b7rei\u00df", "der", "wol\u00b7cken", "flor", "/", "be\u00b7strah\u00b7le", "dei\u00b7ne", "braut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$(", "VVFIN", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die dir der himmel selbst von anfang anvertraut/", "tokens": ["Die", "dir", "der", "him\u00b7mel", "selbst", "von", "an\u00b7fang", "an\u00b7ver\u00b7traut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und zeig ihr l\u00e4nger nicht den so verha\u00dften r\u00fccken.", "tokens": ["Und", "zeig", "ihr", "l\u00e4n\u00b7ger", "nicht", "den", "so", "ver\u00b7ha\u00df\u00b7ten", "r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKNEG", "ART", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie hofft/ wie Solime/ mit sehnlichem verlangen", "tokens": ["Sie", "hofft", "/", "wie", "So\u00b7li\u00b7me", "/", "mit", "sehn\u00b7li\u00b7chem", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "NE", "$(", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Auff dich und deinen schein/ der beyden noch gebricht/", "tokens": ["Auff", "dich", "und", "dei\u00b7nen", "schein", "/", "der", "bey\u00b7den", "noch", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$(", "ART", "PIAT", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Drum \u00f6ffne/ wie du pflegst/ dein s\u00fcsses angesicht/", "tokens": ["Drum", "\u00f6ff\u00b7ne", "/", "wie", "du", "pflegst", "/", "dein", "s\u00fcs\u00b7ses", "an\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$(", "PWAV", "PPER", "VVFIN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bevor der abend k\u00f6mmt mit seiner nacht gegangen.", "tokens": ["Be\u00b7vor", "der", "a\u00b7bend", "k\u00f6mmt", "mit", "sei\u00b7ner", "nacht", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Denn tritt der mond hervor/ der k\u00f6nig aller sterne/", "tokens": ["Denn", "tritt", "der", "mond", "her\u00b7vor", "/", "der", "k\u00f6\u00b7nig", "al\u00b7ler", "ster\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$(", "ART", "NN", "PIAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der werffe seinen schein/ aus der gew\u00f6lbten h\u00f6h/", "tokens": ["Der", "werf\u00b7fe", "sei\u00b7nen", "schein", "/", "aus", "der", "ge\u00b7w\u00f6lb\u00b7ten", "h\u00f6h", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$(", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Auff den gebreiten zeug der sch\u00f6nen Solime/", "tokens": ["Auff", "den", "ge\u00b7brei\u00b7ten", "zeug", "der", "sch\u00f6\u00b7nen", "So\u00b7li\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+--", "measure": "iambic.penta.invert"}, "line.24": {"text": "Und schaue/ da\u00df kein dieb ihr was davon entferne.", "tokens": ["Und", "schau\u00b7e", "/", "da\u00df", "kein", "dieb", "ihr", "was", "da\u00b7von", "ent\u00b7fer\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PIAT", "NN", "PPOSAT", "PWS", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Aurora lasse nichts als nasse perlen thauen!", "tokens": ["Au\u00b7ro\u00b7ra", "las\u00b7se", "nichts", "als", "nas\u00b7se", "per\u00b7len", "thau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "KOKOM", "VVFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die thr\u00e4nen die sie sonst verliehrt um ihren freund/", "tokens": ["Die", "thr\u00e4\u00b7nen", "die", "sie", "sonst", "ver\u00b7liehrt", "um", "ih\u00b7ren", "freund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den sie/ wie Solime den bruder/ itzt beweint/", "tokens": ["Den", "sie", "/", "wie", "So\u00b7li\u00b7me", "den", "bru\u00b7der", "/", "itzt", "be\u00b7weint", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "KOKOM", "NN", "ART", "NN", "$(", "ADV", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.28": {"text": "Die m\u00fcsse man/ erwacht/ auff allen t\u00fcchern schauen.", "tokens": ["Die", "m\u00fcs\u00b7se", "man", "/", "er\u00b7wacht", "/", "auff", "al\u00b7len", "t\u00fc\u00b7chern", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "$(", "VVPP", "$(", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wind/ erde/ sonn und mond/ Aurora/ h\u00f6rt mein flehen!", "tokens": ["Wind", "/", "er\u00b7de", "/", "sonn", "und", "mond", "/", "Au\u00b7ro\u00b7ra", "/", "h\u00f6rt", "mein", "fle\u00b7hen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "XY", "KON", "NN", "$(", "NE", "$(", "VVFIN", "PPOSAT", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.30": {"text": "Nehmt euch der Solime und ihres zeuges an.", "tokens": ["Nehmt", "euch", "der", "So\u00b7li\u00b7me", "und", "ih\u00b7res", "zeu\u00b7ges", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "PPOSAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sch\u00fctz\u2019t und bewachet es/ so gut ein ieder kan/", "tokens": ["Sch\u00fctz't", "und", "be\u00b7wa\u00b7chet", "es", "/", "so", "gut", "ein", "ie\u00b7der", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "$(", "ADV", "ADJD", "ART", "PIS", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sonst ist es mehr um mich/ als ihre lust/ geschehen.", "tokens": ["Sonst", "ist", "es", "mehr", "um", "mich", "/", "als", "ih\u00b7re", "lust", "/", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$(", "KOKOM", "PPOSAT", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}