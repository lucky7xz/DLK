{"dta.poem.2890": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer aus dem Hause geht bei fr\u00fcher Morgenhelle", "tokens": ["Wer", "aus", "dem", "Hau\u00b7se", "geht", "bei", "fr\u00fc\u00b7her", "Mor\u00b7gen\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu wichtigem Gesch\u00e4ft, und st\u00f6\u00dft sich an die Schwelle,", "tokens": ["Zu", "wich\u00b7ti\u00b7gem", "Ge\u00b7sch\u00e4ft", ",", "und", "st\u00f6\u00dft", "sich", "an", "die", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verachten soll er nicht die Warnung, sondern lenken", "tokens": ["Ver\u00b7ach\u00b7ten", "soll", "er", "nicht", "die", "War\u00b7nung", ",", "son\u00b7dern", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur\u00fcck, um noch einmal den Ausgang zu bedenken.", "tokens": ["Zu\u00b7r\u00fcck", ",", "um", "noch", "ein\u00b7mal", "den", "Aus\u00b7gang", "zu", "be\u00b7den\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "KOUI", "ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn du hast recht bedacht, schlag das Bedenken nieder,", "tokens": ["Wenn", "du", "hast", "recht", "be\u00b7dacht", ",", "schlag", "das", "Be\u00b7den\u00b7ken", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADJD", "VVPP", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geh aus und sto\u00dfe dich an keinen Ansto\u00df wieder.", "tokens": ["Geh", "aus", "und", "sto\u00b7\u00dfe", "dich", "an", "kei\u00b7nen", "An\u00b7sto\u00df", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "PRF", "APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur dazu sind gesandt den Menschen \u00fcble Zeichen,", "tokens": ["Nur", "da\u00b7zu", "sind", "ge\u00b7sandt", "den", "Men\u00b7schen", "\u00fcb\u00b7le", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VAFIN", "VVPP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie davor zur\u00fcck von \u00fcblen Pfaden weichen;", "tokens": ["Da\u00df", "sie", "da\u00b7vor", "zu\u00b7r\u00fcck", "von", "\u00fcb\u00b7len", "Pfa\u00b7den", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "PTKVZ", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und g\u00fcnstige dazu, da\u00df sie den guten Mut", "tokens": ["Und", "g\u00fcns\u00b7ti\u00b7ge", "da\u00b7zu", ",", "da\u00df", "sie", "den", "gu\u00b7ten", "Mut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "PAV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir st\u00e4rken, wenn der Weg, auf dem du gehst, ist gut.", "tokens": ["Dir", "st\u00e4r\u00b7ken", ",", "wenn", "der", "Weg", ",", "auf", "dem", "du", "gehst", ",", "ist", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mit Vogelfluge winkt und mahnt mit Vogelstimmen", "tokens": ["Mit", "Vo\u00b7gel\u00b7flu\u00b7ge", "winkt", "und", "mahnt", "mit", "Vo\u00b7gel\u00b7stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Selbst die Natur dich an zum Guten, ab vom Schlimmen.", "tokens": ["Selbst", "die", "Na\u00b7tur", "dich", "an", "zum", "Gu\u00b7ten", ",", "ab", "vom", "Schlim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "APPR", "APPRART", "NN", "$,", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}