{"dta.poem.15875": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein zierliches Besteck, das drei Glimmstengel fa\u00dft,", "tokens": ["Ein", "zier\u00b7li\u00b7ches", "Be\u00b7steck", ",", "das", "drei", "Glimms\u00b7ten\u00b7gel", "fa\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Tasche vor der Brust ist es wie angepa\u00dft.", "tokens": ["Der", "Ta\u00b7sche", "vor", "der", "Brust", "ist", "es", "wie", "an\u00b7ge\u00b7pa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "PPER", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie wunderbar es doch sich treffen mu\u00df! Drei Engel,", "tokens": ["Wie", "wun\u00b7der\u00b7bar", "es", "doch", "sich", "tref\u00b7fen", "mu\u00df", "!", "Drei", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "PRF", "VVINF", "VMFIN", "$.", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drei M\u00e4dchen haben es gestickt f\u00fcr drei Glimmstengel.", "tokens": ["Drei", "M\u00e4d\u00b7chen", "ha\u00b7ben", "es", "ge\u00b7stickt", "f\u00fcr", "drei", "Glimms\u00b7ten\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVPP", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Das Dritt' ist, da\u00df ich drei auch grade t\u00e4glich brauche,", "tokens": ["Das", "Dritt'", "ist", ",", "da\u00df", "ich", "drei", "auch", "gra\u00b7de", "t\u00e4g\u00b7lich", "brau\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "CARD", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jeder Stickerin zu Ehren einen rauche.", "tokens": ["Und", "je\u00b7der", "Sti\u00b7cke\u00b7rin", "zu", "Eh\u00b7ren", "ei\u00b7nen", "rau\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Weihrauch zwar, der nicht f\u00fcr Frauen recht sich schickt,", "tokens": ["Ein", "Weih\u00b7rauch", "zwar", ",", "der", "nicht", "f\u00fcr", "Frau\u00b7en", "recht", "sich", "schickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PTKNEG", "APPR", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch haben sie mirs ja zu Anderm nicht gestickt.", "tokens": ["Doch", "ha\u00b7ben", "sie", "mirs", "ja", "zu", "An\u00b7derm", "nicht", "ge\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NE", "ADV", "APPR", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}