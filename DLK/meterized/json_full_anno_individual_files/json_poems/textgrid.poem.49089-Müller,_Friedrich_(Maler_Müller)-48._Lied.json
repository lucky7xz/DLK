{"textgrid.poem.49089": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "48. Lied", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein holdes Bild schwebt immer mir vor Augen,", "tokens": ["Dein", "hol\u00b7des", "Bild", "schwebt", "im\u00b7mer", "mir", "vor", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Thal und W\u00e4lder folgt's mir nach;", "tokens": ["Durch", "Thal", "und", "W\u00e4l\u00b7der", "folgt's", "mir", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald schlummernd auf der Au, umscherzt von Zephirs Hauchen", "tokens": ["Bald", "schlum\u00b7mernd", "auf", "der", "Au", ",", "um\u00b7scherzt", "von", "Ze\u00b7phirs", "Hau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Liebesg\u00f6ttern, jung und wach.", "tokens": ["Und", "Lie\u00b7bes\u00b7g\u00f6t\u00b7tern", ",", "jung", "und", "wach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald, wie am Wasserfall du unter Bl\u00fctenb\u00fcschen", "tokens": ["Bald", ",", "wie", "am", "Was\u00b7ser\u00b7fall", "du", "un\u00b7ter", "Bl\u00fc\u00b7ten\u00b7b\u00fc\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPRART", "NN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf frischem Moos die Glieder reckst;", "tokens": ["Auf", "fri\u00b7schem", "Moos", "die", "Glie\u00b7der", "reckst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nun den linden West, dich sanfter zu erfrischen,", "tokens": ["Und", "nun", "den", "lin\u00b7den", "West", ",", "dich", "sanf\u00b7ter", "zu", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Rosenknospen zu dir neckst.", "tokens": ["Von", "Ro\u00b7sen\u00b7knos\u00b7pen", "zu", "dir", "neckst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vom Myrtenstrauch, um den die Liebesg\u00f6tter gaukeln,", "tokens": ["Vom", "Myr\u00b7ten\u00b7strauch", ",", "um", "den", "die", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "gau\u00b7keln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUI", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwingt Amor sich auf deinen Schoo\u00df;", "tokens": ["Schwingt", "A\u00b7mor", "sich", "auf", "dei\u00b7nen", "Schoo\u00df", ";"], "token_info": ["word", "word