{"dta.poem.18311": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "In memoriam!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Weh, man hat dich l\u00e4ngst begraben,", "tokens": ["Weh", ",", "man", "hat", "dich", "l\u00e4ngst", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn nur noch an Soll und Haben", "tokens": ["Denn", "nur", "noch", "an", "Soll", "und", "Ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "NE", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denkt die Menschheit dieser Zeit!", "tokens": ["Denkt", "die", "Menschheit", "die\u00b7ser", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ihre R\u00e4der w\u00fchlen Schaum,", "tokens": ["Ih\u00b7re", "R\u00e4\u00b7der", "w\u00fch\u00b7len", "Schaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Funken spr\u00fchen ihre Essen;", "tokens": ["Fun\u00b7ken", "spr\u00fc\u00b7hen", "ih\u00b7re", "Es\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, und l\u00e4ngst hat sie vergessen", "tokens": ["Ach", ",", "und", "l\u00e4ngst", "hat", "sie", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer Jugend goldnen Traum!", "tokens": ["Ih\u00b7rer", "Ju\u00b7gend", "gold\u00b7nen", "Traum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ausgebrannt ist jede Brust,", "tokens": ["Aus\u00b7ge\u00b7brannt", "ist", "je\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Alt\u00e4re stehn verlassen,", "tokens": ["Die", "Al\u00b7t\u00e4\u00b7re", "stehn", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Horch, und drau\u00dfen auf den Gassen", "tokens": ["Horch", ",", "und", "drau\u00b7\u00dfen", "auf", "den", "Gas\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Predigt die entmenschte Lust!", "tokens": ["Pre\u00b7digt", "die", "ent\u00b7menschte", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Um das Haupt des Helicon", "tokens": ["Um", "das", "Haupt", "des", "He\u00b7li\u00b7con"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwirren tausend irre Fragen", "tokens": ["Schwir\u00b7ren", "tau\u00b7send", "ir\u00b7re", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Zeitgeist h\u00f6rt man klagen", "tokens": ["Und", "den", "Zeit\u00b7geist", "h\u00f6rt", "man", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An den Wassern von Babylon!", "tokens": ["An", "den", "Was\u00b7sern", "von", "Ba\u00b7by\u00b7lon", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}