{"textgrid.poem.33781": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Nordische Sommernacht", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nordische Sommernacht leuchtet im Schnee,", "tokens": ["Nor\u00b7di\u00b7sche", "Som\u00b7mer\u00b7nacht", "leuch\u00b7tet", "im", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-++--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Leuchtet im flutenden Hertasee.", "tokens": ["Leuch\u00b7tet", "im", "flu\u00b7ten\u00b7den", "Her\u00b7ta\u00b7see", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Rosige D\u00e4mmerung, ruhiger Schein", "tokens": ["Ro\u00b7si\u00b7ge", "D\u00e4m\u00b7me\u00b7rung", ",", "ru\u00b7hi\u00b7ger", "Schein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Tropft in die Wellen wie funkelnder Wein.", "tokens": ["Tropft", "in", "die", "Wel\u00b7len", "wie", "fun\u00b7keln\u00b7der", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwischen ergl\u00fchenden Buchen am Strand", "tokens": ["Zwi\u00b7schen", "er\u00b7gl\u00fc\u00b7hen\u00b7den", "Bu\u00b7chen", "am", "Strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Schimmert das heilige Inselland,", "tokens": ["Schim\u00b7mert", "das", "hei\u00b7li\u00b7ge", "In\u00b7sel\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Schimmern die Steine des Opferaltars", "tokens": ["Schim\u00b7mern", "die", "Stei\u00b7ne", "des", "Op\u00b7fer\u00b7al\u00b7tars"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Herta's, der segnenden G\u00f6ttin des Jahrs.", "tokens": ["Her\u00b7ta's", ",", "der", "seg\u00b7nen\u00b7den", "G\u00f6t\u00b7tin", "des", "Jahrs", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Sommersonnwendnacht, o heilige Zeit,", "tokens": ["Som\u00b7mer\u00b7sonn\u00b7wend\u00b7nacht", ",", "o", "hei\u00b7li\u00b7ge", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Opfer schon stehn dir am Ufer bereit.", "tokens": ["Op\u00b7fer", "schon", "stehn", "dir", "am", "U\u00b7fer", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Rosse, schneewei\u00dfe vom edelsten Blut,", "tokens": ["Ros\u00b7se", ",", "schnee\u00b7wei\u00b7\u00dfe", "vom", "e\u00b7dels\u00b7ten", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Sch\u00fctteln die M\u00e4hnen voll Todesmut,", "tokens": ["Sch\u00fct\u00b7teln", "die", "M\u00e4h\u00b7nen", "voll", "To\u00b7des\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Schlagen die Hufe und schnauben empor,", "tokens": ["Schla\u00b7gen", "die", "Hu\u00b7fe", "und", "schnau\u00b7ben", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Opferdampf steigt aus den N\u00fcstern hervor.", "tokens": ["Op\u00b7fer\u00b7dampf", "steigt", "aus", "den", "N\u00fcs\u00b7tern", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-++--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Diener der G\u00f6ttin, unfreie Geburt,", "tokens": ["Die\u00b7ner", "der", "G\u00f6t\u00b7tin", ",", "un\u00b7frei\u00b7e", "Ge\u00b7burt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "F\u00fchren die Schimmel am silbernen Gurt.", "tokens": ["F\u00fch\u00b7ren", "die", "Schim\u00b7mel", "am", "sil\u00b7ber\u00b7nen", "Gurt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "Sie auch, der G\u00f6ttin zum Opfer geweiht,", "tokens": ["Sie", "auch", ",", "der", "G\u00f6t\u00b7tin", "zum", "Op\u00b7fer", "ge\u00b7weiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Stehen entwaffnet zum Tode bereit.", "tokens": ["Ste\u00b7hen", "ent\u00b7waff\u00b7net", "zum", "To\u00b7de", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "Einer von ihnen, ein Greis, erhebt", "tokens": ["Ei\u00b7ner", "von", "ih\u00b7nen", ",", "ein", "Greis", ",", "er\u00b7hebt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PIS", "APPR", "PPER", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Freudig sein Haupt, sein Mund erbebt.", "tokens": ["Freu\u00b7dig", "sein", "Haupt", ",", "sein", "Mund", "er\u00b7bebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "\u00bbschwinde, mein Leben, wie Abendrot!", "tokens": ["\u00bb", "schwin\u00b7de", ",", "mein", "Le\u00b7ben", ",", "wie", "A\u00b7ben\u00b7drot", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "$,", "PPOSAT", "NN", "$,", "PWAV", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Kurz wie die Sommernacht, kurz ist der Tod.", "tokens": ["Kurz", "wie", "die", "Som\u00b7mer\u00b7nacht", ",", "kurz", "ist", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Kaum da\u00df im Dunkel mein Aug' erlischt,", "tokens": ["Kaum", "da\u00df", "im", "Dun\u00b7kel", "mein", "Aug'", "er\u00b7lischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPRART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Werd' ich vom ewigen Morgen erfrischt.\u00ab", "tokens": ["Werd'", "ich", "vom", "e\u00b7wi\u00b7gen", "Mor\u00b7gen", "er\u00b7frischt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Sprach's; da erhebt sich ein J\u00fcngling und spricht:", "tokens": ["Sprach's", ";", "da", "er\u00b7hebt", "sich", "ein", "J\u00fcng\u00b7ling", "und", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PRF", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "\u00bbgreis, deine Worte vers\u00f6hnen mich nicht.", "tokens": ["\u00bb", "greis", ",", "dei\u00b7ne", "Wor\u00b7te", "ver\u00b7s\u00f6h\u00b7nen", "mich", "nicht", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.14": {"line.1": {"text": "Kurz, wie der Wintertag, arm und kahl", "tokens": ["Kurz", ",", "wie", "der", "Win\u00b7ter\u00b7tag", ",", "arm", "und", "kahl"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Deucht mich des Lebens so fl\u00fcchtiger Strahl.", "tokens": ["Deucht", "mich", "des", "Le\u00b7bens", "so", "fl\u00fcch\u00b7ti\u00b7ger", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.15": {"line.1": {"text": "Fiel' ich, ja fiel' ich ein Held in der Schlacht,", "tokens": ["Fiel'", "ich", ",", "ja", "fiel'", "ich", "ein", "Held", "in", "der", "Schlacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Hell wie die Flamme der nordischen Nacht,", "tokens": ["Hell", "wie", "die", "Flam\u00b7me", "der", "nor\u00b7di\u00b7schen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.16": {"line.1": {"text": "Dann, ja dann h\u00e4tt' ich gelebt und mit Recht;", "tokens": ["Dann", ",", "ja", "dann", "h\u00e4tt'", "ich", "ge\u00b7lebt", "und", "mit", "Recht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "KON", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weh mir! nun sterb' ich ein elender Knecht!\u00ab", "tokens": ["Weh", "mir", "!", "nun", "sterb'", "ich", "ein", "e\u00b7len\u00b7der", "Knecht", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.17": {"line.1": {"text": "Sprach's, da erschienen vom rauchenden Herd", "tokens": ["Sprach's", ",", "da", "er\u00b7schie\u00b7nen", "vom", "rau\u00b7chen\u00b7den", "Herd"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Priester der G\u00f6ttin mit blinkendem Schwert.", "tokens": ["Pries\u00b7ter", "der", "G\u00f6t\u00b7tin", "mit", "blin\u00b7ken\u00b7dem", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.18": {"line.1": {"text": "Von der Gefangenen Nacken und Fu\u00df", "tokens": ["Von", "der", "Ge\u00b7fan\u00b7ge\u00b7nen", "Na\u00b7cken", "und", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sprudelt zur Erde der s\u00fchnende Gru\u00df.", "tokens": ["Spru\u00b7delt", "zur", "Er\u00b7de", "der", "s\u00fch\u00b7nen\u00b7de", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.19": {"line.1": {"text": "Auf und hinaus in die Fluten der Tro\u00df!", "tokens": ["Auf", "und", "hin\u00b7aus", "in", "die", "Flu\u00b7ten", "der", "Tro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Blutige M\u00e4nner auf blutigem Ro\u00df", "tokens": ["Blu\u00b7ti\u00b7ge", "M\u00e4n\u00b7ner", "auf", "blu\u00b7ti\u00b7gem", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.20": {"line.1": {"text": "Schwimmen die sch\u00e4umenden Wogen entlang;", "tokens": ["Schwim\u00b7men", "die", "sch\u00e4u\u00b7men\u00b7den", "Wo\u00b7gen", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPO", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Furchtbar erschallt der Druiden Gesang.", "tokens": ["Furcht\u00b7bar", "er\u00b7schallt", "der", "Dru\u00b7i\u00b7den", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.21": {"line.1": {"text": "Brechende Blicke zum letztenmal", "tokens": ["Bre\u00b7chen\u00b7de", "Bli\u00b7cke", "zum", "letz\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Gr\u00fc\u00dfen den scheidenden Sonnenstrahl.", "tokens": ["Gr\u00fc\u00b7\u00dfen", "den", "schei\u00b7den\u00b7den", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.22": {"line.1": {"text": "Mutiges Wiehern zum letztenmal", "tokens": ["Mu\u00b7ti\u00b7ges", "Wie\u00b7hern", "zum", "letz\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Schmettert im hallenden Hertatal.", "tokens": ["Schmet\u00b7tert", "im", "hal\u00b7len\u00b7den", "Her\u00b7ta\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.23": {"line.1": {"text": "Tiefer und tiefer versinken sie bald,", "tokens": ["Tie\u00b7fer", "und", "tie\u00b7fer", "ver\u00b7sin\u00b7ken", "sie", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Dunkler und dunkler wird Ufer und Wald.", "tokens": ["Dunk\u00b7ler", "und", "dunk\u00b7ler", "wird", "U\u00b7fer", "und", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJA", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.24": {"line.1": {"text": "Dunkler und stiller wird Ufer und Well',", "tokens": ["Dunk\u00b7ler", "und", "stil\u00b7ler", "wird", "U\u00b7fer", "und", "Well'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJD", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Aber im Osten schon r\u00f6tet sich's hell.", "tokens": ["A\u00b7ber", "im", "Os\u00b7ten", "schon", "r\u00f6\u00b7tet", "sich's", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VVFIN", "PIS", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.25": {"line.1": {"text": "\u00d6stlich ergl\u00fcht es, der Morgen erwacht \u2013", "tokens": ["\u00d6st\u00b7lich", "er\u00b7gl\u00fcht", "es", ",", "der", "Mor\u00b7gen", "er\u00b7wacht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN"