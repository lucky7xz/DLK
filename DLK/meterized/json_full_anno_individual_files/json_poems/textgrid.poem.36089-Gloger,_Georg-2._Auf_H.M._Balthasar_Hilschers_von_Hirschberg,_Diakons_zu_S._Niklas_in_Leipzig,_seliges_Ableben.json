{"textgrid.poem.36089": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2. Auf H.M. Balthasar Hilschers von Hirschberg, Diakons zu S. Niklas in Leipzig, seliges Ableben", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trawer', trawer', Gotteshaus,", "tokens": ["Tra\u00b7wer'", ",", "tra\u00b7wer'", ",", "Got\u00b7tes\u00b7haus", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Ruhmes Ruhm ist aus.", "tokens": ["Dei\u00b7nes", "Ruh\u00b7mes", "Ruhm", "ist", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er ist schon \u00fcber hin. Da hilft kein Helfen nicht.", "tokens": ["Er", "ist", "schon", "\u00fc\u00b7ber", "hin", ".", "Da", "hilft", "kein", "Hel\u00b7fen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PTKVZ", "$.", "ADV", "VVFIN", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der wol beredte Mund, die sch\u00f6n begabte Seele", "tokens": ["Der", "wol", "be\u00b7red\u00b7te", "Mund", ",", "die", "sch\u00f6n", "be\u00b7gab\u00b7te", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist aus dem Kerker los, in dem sie sich entbricht", "tokens": ["Ist", "aus", "dem", "Ker\u00b7ker", "los", ",", "in", "dem", "sie", "sich", "ent\u00b7bricht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der dicken Nebelluft vnd finstern Leibesh\u00f6le.", "tokens": ["Der", "di\u00b7cken", "Ne\u00b7bel\u00b7luft", "vnd", "fins\u00b7tern", "Lei\u00b7bes\u00b7h\u00f6\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er vnd sein erleuchter Sinn", "tokens": ["Er", "vnd", "sein", "er\u00b7leuch\u00b7ter", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Sind auf eine Stunde hin.", "tokens": ["Sind", "auf", "ei\u00b7ne", "Stun\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "So geht es mit vns zu; wenn kaum ein Mundvoll Geist", "tokens": ["So", "geht", "es", "mit", "vns", "zu", ";", "wenn", "kaum", "ein", "Mund\u00b7voll", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$.", "KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vns durch den Mund entf\u00e4hrt, so ist die Seel' entleibet.", "tokens": ["Vns", "durch", "den", "Mund", "ent\u00b7f\u00e4hrt", ",", "so", "ist", "die", "Seel'", "ent\u00b7lei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$,", "ADV", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das, was so sch\u00f6ne war, ein H\u00e4uflein Asche hei\u00dft,", "tokens": ["Das", ",", "was", "so", "sch\u00f6\u00b7ne", "war", ",", "ein", "H\u00e4uf\u00b7lein", "A\u00b7sche", "hei\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "ADJA", "VAFIN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die vns von Hand, von Bein, vnd allem vberbleibet.", "tokens": ["Die", "vns", "von", "Hand", ",", "von", "Bein", ",", "vnd", "al\u00b7lem", "vberb\u00b7lei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "$,", "APPR", "NN", "$,", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Denke, denke wie wir stehn,", "tokens": ["Den\u00b7ke", ",", "den\u00b7ke", "wie", "wir", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Weil wir eh als Schnee zergehn.", "tokens": ["Weil", "wir", "eh", "als", "Schnee", "zer\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "KOUS", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Wer hette doch gedacht vor etlich Gestern noch,", "tokens": ["Wer", "het\u00b7te", "doch", "ge\u00b7dacht", "vor", "et\u00b7lich", "Ge\u00b7stern", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "VVPP", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Da\u00df der, des Leichen wir nun heute folgen m\u00fc\u00dfen,", "tokens": ["Da\u00df", "der", ",", "des", "Lei\u00b7chen", "wir", "nun", "heu\u00b7te", "fol\u00b7gen", "m\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ART", "NN", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Solt eine Leiche sein, noch l\u00e4uft die Rechnung hoch,", "tokens": ["Solt", "ei\u00b7ne", "Lei\u00b7che", "sein", ",", "noch", "l\u00e4uft", "die", "Rech\u00b7nung", "hoch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VAINF", "$,", "ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die wir aus Vnbedacht sehr weit zu setzen wi\u00dfen.", "tokens": ["Die", "wir", "aus", "Vn\u00b7be\u00b7dacht", "sehr", "weit", "zu", "set\u00b7zen", "wi\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Aber irre dich ia nicht", "tokens": ["A\u00b7ber", "ir\u00b7re", "dich", "i\u00b7a", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Da\u00df dirs nicht an Zeit gebricht.", "tokens": ["Da\u00df", "dirs", "nicht", "an", "Zeit", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Die Erndte deiner Zeit wird eh zur Garbe bracht,", "tokens": ["Die", "Ernd\u00b7te", "dei\u00b7ner", "Zeit", "wird", "eh", "zur", "Gar\u00b7be", "bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "KOUS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Eh sie verscho\u00dft, verk\u00f6rnt, vnd v\u00f6llig kan verreiffen.", "tokens": ["Eh", "sie", "ver\u00b7scho\u00dft", ",", "ver\u00b7k\u00f6rnt", ",", "vnd", "v\u00f6l\u00b7lig", "kan", "ver\u00b7reif\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVPP", "$,", "KON", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ja eh noch mancher selbst hett' an den Tod gedacht", "tokens": ["Ja", "eh", "noch", "man\u00b7cher", "selbst", "hett'", "an", "den", "Tod", "ge\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "KOUS", "ADV", "PIS", "ADV", "VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sieht man den Sensenman schon nach dem Halme greifen.", "tokens": ["Sieht", "man", "den", "Sen\u00b7sen\u00b7man", "schon", "nach", "dem", "Hal\u00b7me", "grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Eh der Meyschein kan vergehn,", "tokens": ["Eh", "der", "Meysc\u00b7hein", "kan", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Kan die Garb' in Mandeln stehn,", "tokens": ["Kan", "die", "Ga\u00b7rb'", "in", "Man\u00b7deln", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wie das genung beglaubt die bla\u00dfe Leiche hier.", "tokens": ["Wie", "das", "ge\u00b7nung", "be\u00b7glaubt", "die", "bla\u00b7\u00dfe", "Lei\u00b7che", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Aus der vorgestern noch der Athem nicht gewichen,", "tokens": ["Aus", "der", "vor\u00b7ges\u00b7tern", "noch", "der", "A\u00b7them", "nicht", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der heute kehret doch die F\u00fc\u00dfe nach der Th\u00fcr,", "tokens": ["Der", "heu\u00b7te", "keh\u00b7ret", "doch", "die", "F\u00fc\u00b7\u00dfe", "nach", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mit bleichem Todtenblass' vmb Hand vnd Mund bestrichen.", "tokens": ["Mit", "blei\u00b7chem", "Tod\u00b7ten\u00b7blass'", "vmb", "Hand", "vnd", "Mund", "be\u00b7stri\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Niemand hette das gedacht", "tokens": ["Nie\u00b7mand", "het\u00b7te", "das", "ge\u00b7dacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Vnd das Ziel so kurz gemacht.", "tokens": ["Vnd", "das", "Ziel", "so", "kurz", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Wo kurz auch hei\u00dfen kan, wenn man von schwerer Last", "tokens": ["Wo", "kurz", "auch", "hei\u00b7\u00dfen", "kan", ",", "wenn", "man", "von", "schwe\u00b7rer", "Last"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "VVINF", "VMFIN", "$,", "KOUS", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Vnd schwerer Hofsarbeit fein bald sich ab kan fr\u00f6hnen,", "tokens": ["Vnd", "schwe\u00b7rer", "Hof\u00b7sar\u00b7beit", "fein", "bald", "sich", "ab", "kan", "fr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "ADV", "PRF", "PTKVZ", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Der k\u00fchlen Schatten hat zu seiner sanften Rast,", "tokens": ["Der", "k\u00fch\u00b7len", "Schat\u00b7ten", "hat", "zu", "sei\u00b7ner", "sanf\u00b7ten", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wornach wir armen Leut vns billich sollten sehnen,", "tokens": ["Wor\u00b7nach", "wir", "ar\u00b7men", "Leut", "vns", "bil\u00b7lich", "soll\u00b7ten", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Weil man hier viel M\u00fch vnd Noth", "tokens": ["Weil", "man", "hier", "viel", "M\u00fch", "vnd", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Hat vmbs liebe Trawerbrot.", "tokens": ["Hat", "vmbs", "lie\u00b7be", "Tra\u00b7wer\u00b7brot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Nun wol! er ist zu Rand' vnd sitzt in stiller Ruh,", "tokens": ["Nun", "wol", "!", "er", "ist", "zu", "Rand'", "vnd", "sitzt", "in", "stil\u00b7ler", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PPER", "VAFIN", "APPR", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Tritt alle Sterblichkeit getrost zu seinen F\u00fc\u00dfen,", "tokens": ["Tritt", "al\u00b7le", "Sterb\u00b7lich\u00b7keit", "ge\u00b7trost", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sieht hinter sich mit Lust dem rauhen Strome zu,", "tokens": ["Sieht", "hin\u00b7ter", "sich", "mit", "Lust", "dem", "rau\u00b7hen", "Stro\u00b7me", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Durch den er nunmehr sich hat ritterlich geri\u00dfen.", "tokens": ["Durch", "den", "er", "nun\u00b7mehr", "sich", "hat", "rit\u00b7ter\u00b7lich", "ge\u00b7ri\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PRF", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wol! wer hier auch gleichfalls kan", "tokens": ["Wol", "!", "wer", "hier", "auch", "gleich\u00b7falls", "kan"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PWS", "ADV", "ADV", "ADV", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Vngestr\u00e4ndet l\u00e4nden an.", "tokens": ["Vn\u00b7ge\u00b7str\u00e4n\u00b7det", "l\u00e4n\u00b7den", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Die Seinen l\u00e4\u00dft er zwar in tiefer Trawer Noth,", "tokens": ["Die", "Sei\u00b7nen", "l\u00e4\u00dft", "er", "zwar", "in", "tie\u00b7fer", "Tra\u00b7wer", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alex