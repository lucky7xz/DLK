{"dta.poem.13263": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich wei\u00df, versezte sie, wer es gewesen sey;", "tokens": ["\u201e", "ich", "wei\u00df", ",", "ver\u00b7sez\u00b7te", "sie", ",", "wer", "es", "ge\u00b7we\u00b7sen", "sey", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAPP", "VAFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u201edem ist die ganze Burg und aller Zutritt frey.", "tokens": ["\u201e", "dem", "ist", "die", "gan\u00b7ze", "Burg", "und", "al\u00b7ler", "Zu\u00b7tritt", "frey", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "KON", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "395\u201dIch offenbare nicht, was mich dazu verbinde;", "tokens": ["\"", "Ich", "of\u00b7fen\u00b7ba\u00b7re", "nicht", ",", "was", "mich", "da\u00b7zu", "ver\u00b7bin\u00b7de", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "PRF", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201egenug: der ists, bey dem ich Trost und Zuflucht finde.", "tokens": ["\u201e", "ge\u00b7nug", ":", "der", "ists", ",", "bey", "dem", "ich", "Trost", "und", "Zu\u00b7flucht", "fin\u00b7de", "."], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "ART", "NE", "$,", "APPR", "PRELS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "\u201eso viel ich Rath und Hilff von euch allhier geborgt,", "tokens": ["\u201e", "so", "viel", "ich", "Rath", "und", "Hilff", "von", "euch", "all\u00b7hier", "ge\u00b7borgt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PPER", "NN", "KON", "NN", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eso viel hat er f\u00fcr mich durch Bitten dort gesorgt.", "tokens": ["\u201e", "so", "viel", "hat", "er", "f\u00fcr", "mich", "durch", "Bit\u00b7ten", "dort", "ge\u00b7sorgt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VAFIN", "PPER", "APPR", "PPER", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edem kann ich nicht genug Verehrungen erstatten;", "tokens": ["\u201e", "dem", "kann", "ich", "nicht", "ge\u00b7nug", "Ver\u00b7eh\u00b7run\u00b7gen", "er\u00b7stat\u00b7ten", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VMFIN", "PPER", "PTKNEG", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "400\u201dMein Seegen, Heil und Wohl seynd seine Wunderthaten.", "tokens": ["\"", "Mein", "See\u00b7gen", ",", "Heil", "und", "Wohl", "seynd", "sei\u00b7ne", "Wun\u00b7der\u00b7tha\u00b7ten", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "NN", "KON", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}