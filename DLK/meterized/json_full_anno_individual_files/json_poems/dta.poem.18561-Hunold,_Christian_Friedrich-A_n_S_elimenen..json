{"dta.poem.18561": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n  S elimenen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Er z\u00fcrne nicht darob/ Annehmlichste der Zeit!", "tokens": ["Er", "z\u00fcr\u00b7ne", "nicht", "da\u00b7rob", "/", "An\u00b7nehm\u00b7lichs\u00b7te", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PAV", "$(", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df mein getreuer Sinn sich stets an dir erg\u00f6tzet.", "tokens": ["Da\u00df", "mein", "ge\u00b7treu\u00b7er", "Sinn", "sich", "stets", "an", "dir", "er\u00b7g\u00f6t\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und deiner sch\u00f6nen Pracht ein ewig Denckmahl setzet", "tokens": ["Und", "dei\u00b7ner", "sch\u00f6\u00b7nen", "Pracht", "ein", "e\u00b7wig", "Denck\u00b7mahl", "set\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Unschuld labet sich an deiner Lieblichkeit", "tokens": ["Die", "Un\u00b7schuld", "la\u00b7bet", "sich", "an", "dei\u00b7ner", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn da der Mund sich nicht darff selbst gel\u00fccklich wissen/", "tokens": ["Denn", "da", "der", "Mund", "sich", "nicht", "darff", "selbst", "ge\u00b7l\u00fcck\u00b7lich", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PRF", "PTKNEG", "VMFIN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So g\u00f6nne da\u00df ich dich im Geiste m\u00f6ge k\u00fcssen.", "tokens": ["So", "g\u00f6n\u00b7ne", "da\u00df", "ich", "dich", "im", "Geis\u00b7te", "m\u00f6\u00b7ge", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "PPER", "PRF", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein unbefleckter Ku\u00df ist auch bey G\u00f6ttern rein;", "tokens": ["Ein", "un\u00b7be\u00b7fleck\u00b7ter", "Ku\u00df", "ist", "auch", "bey", "G\u00f6t\u00b7tern", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du weist/ das meinen Trieb die Keuschheit selbst erreget/", "tokens": ["Du", "weist", "/", "das", "mei\u00b7nen", "Trieb", "die", "Keuschheit", "selbst", "er\u00b7re\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und da\u00df die reinste Gluth mich dir zum F\u00fcssen leget/", "tokens": ["Und", "da\u00df", "die", "reins\u00b7te", "Gluth", "mich", "dir", "zum", "F\u00fcs\u00b7sen", "le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dennoch will dein Hertz von Stahl und Eisen seyn:", "tokens": ["Und", "den\u00b7noch", "will", "dein", "Hertz", "von", "Stahl", "und", "Ei\u00b7sen", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach dencke nur/ was selbst dem Himmel mu\u00df belieben/", "tokens": ["Ach", "den\u00b7cke", "nur", "/", "was", "selbst", "dem", "Him\u00b7mel", "mu\u00df", "be\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "$(", "PWS", "ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird einem Engel au ch zur Regel vorgeschrieben.", "tokens": ["Wird", "ei\u00b7nem", "En\u00b7gel", "au", "ch", "zur", "Re\u00b7gel", "vor\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NE", "NE", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich hatte nicht so bald das unverhoffte Gl\u00fcck/", "tokens": ["Ich", "hat\u00b7te", "nicht", "so", "bald", "das", "un\u00b7ver\u00b7hoff\u00b7te", "Gl\u00fcck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich unvergleichliche zum ersten Mahl zu sehen/", "tokens": ["Dich", "un\u00b7ver\u00b7gleich\u00b7li\u00b7che", "zum", "ers\u00b7ten", "Mahl", "zu", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So muste gleich das Hertz bey sich erfreut gestehen/", "tokens": ["So", "mus\u00b7te", "gleich", "das", "Hertz", "bey", "sich", "er\u00b7freut", "ge\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "APPR", "PRF", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier zeiget die Natur ihr rechtes Meister-St\u00fcck:", "tokens": ["Hier", "zei\u00b7get", "die", "Na\u00b7tur", "ihr", "rech\u00b7tes", "Meis\u00b7ter\u00b7St\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn wer bewundert nicht die seltne Sch\u00f6nheits gaben/", "tokens": ["Denn", "wer", "be\u00b7wun\u00b7dert", "nicht", "die", "selt\u00b7ne", "Sch\u00f6n\u00b7heits", "ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn sich die Tugenden damit verschwistert haben.", "tokens": ["Wenn", "sich", "die", "Tu\u00b7gen\u00b7den", "da\u00b7mit", "ver\u00b7schwis\u00b7tert", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "PAV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Man nennet dich mit Recht die Blume dieser Stadt", "tokens": ["Man", "nen\u00b7net", "dich", "mit", "Recht", "die", "Blu\u00b7me", "die\u00b7ser", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf welche sich der Schnee der Lilien geleget/", "tokens": ["Auf", "wel\u00b7che", "sich", "der", "Schnee", "der", "Li\u00b7li\u00b7en", "ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wo die Rose selbst den Purpur eingepr\u00e4get/", "tokens": ["Und", "wo", "die", "Ro\u00b7se", "selbst", "den", "Pur\u00b7pur", "ein\u00b7ge\u00b7pr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Venus sich allein zur Lust gepflantzet hat:", "tokens": ["Die", "Ve\u00b7nus", "sich", "al\u00b7lein", "zur", "Lust", "ge\u00b7pflant\u00b7zet", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Blumen bricht man sonst/ doch diese mu\u00df ich meiden/", "tokens": ["Die", "Blu\u00b7men", "bricht", "man", "sonst", "/", "doch", "die\u00b7se", "mu\u00df", "ich", "mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "$(", "KON", "PDS", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Auge soll sich nur an ihrer Sch\u00f6nheit weiden.", "tokens": ["Mein", "Au\u00b7ge", "soll", "sich", "nur", "an", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Denn das Verh\u00e4ngni\u00df will/ da\u00df mich die sch\u00f6ne Gluth", "tokens": ["Denn", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "will", "/", "da\u00df", "mich", "die", "sch\u00f6\u00b7ne", "Gluth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "$(", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch nicht vergn\u00fcgen soll und in die Augen steigen/", "tokens": ["Noch", "nicht", "ver\u00b7gn\u00fc\u00b7gen", "soll", "und", "in", "die", "Au\u00b7gen", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVINF", "VMFIN", "KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie w\u00fcrde sich sonst bald in tausend Flammen zeigen/", "tokens": ["Sie", "w\u00fcr\u00b7de", "sich", "sonst", "bald", "in", "tau\u00b7send", "Flam\u00b7men", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "ADV", "APPR", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn nur ein Strahl von dir entz\u00fcndet Geist und Blut:", "tokens": ["Denn", "nur", "ein", "Strahl", "von", "dir", "ent\u00b7z\u00fcn\u00b7det", "Geist", "und", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wen nun so lange Zeit zwey sch\u00f6ne Sonnen brennen/", "tokens": ["Wen", "nun", "so", "lan\u00b7ge", "Zeit", "zwey", "sch\u00f6\u00b7ne", "Son\u00b7nen", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJA", "NN", "CARD", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird sich gewi\u00dflich nicht von Feuer frey bekennen.", "tokens": ["Wird", "sich", "ge\u00b7wi\u00df\u00b7lich", "nicht", "von", "Feu\u00b7er", "frey", "be\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "PTKNEG", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du weist/ das meine Brust von keinen Marmor ist/", "tokens": ["Du", "weist", "/", "das", "mei\u00b7ne", "Brust", "von", "kei\u00b7nen", "Mar\u00b7mor", "ist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PDS", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Obgleich dein strenger Sinn sich Diamanten glechet;", "tokens": ["Ob\u00b7gleich", "dein", "stren\u00b7ger", "Sinn", "sich", "Di\u00b7a\u00b7man\u00b7ten", "gle\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jedoch ein harter Stein wird endlich auch erweichet/", "tokens": ["Je\u00b7doch", "ein", "har\u00b7ter", "Stein", "wird", "end\u00b7lich", "auch", "er\u00b7wei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer wei\u00df/ was vor ein Gl\u00fcck mir meine Qvaal vers\u00fcst.", "tokens": ["Wer", "wei\u00df", "/", "was", "vor", "ein", "Gl\u00fcck", "mir", "mei\u00b7ne", "Qvaal", "ver\u00b7s\u00fcst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "PWS", "APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Geist ist doch vergn\u00fcgt/ ob er die Fesseln tr\u00e4get/", "tokens": ["Mein", "Geist", "ist", "doch", "ver\u00b7gn\u00fcgt", "/", "ob", "er", "die", "Fes\u00b7seln", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil die Galanteste sie ihm hat angeleget.", "tokens": ["Weil", "die", "Ga\u00b7lan\u00b7tes\u00b7te", "sie", "ihm", "hat", "an\u00b7ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Erlaube mir demnach vollkomne Meisterin/", "tokens": ["Er\u00b7lau\u00b7be", "mir", "dem\u00b7nach", "voll\u00b7kom\u00b7ne", "Meis\u00b7te\u00b7rin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df die getreue Brust dir einen Tempel bauet/", "tokens": ["Da\u00df", "die", "ge\u00b7treu\u00b7e", "Brust", "dir", "ei\u00b7nen", "Tem\u00b7pel", "bau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo man dein sch\u00f6nes Bild allzeit verewigt schauet/", "tokens": ["Wo", "man", "dein", "sch\u00f6\u00b7nes", "Bild", "all\u00b7zeit", "ve\u00b7re\u00b7wigt", "schau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "ADJA", "NN", "ADV", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Hertze leget sich zu einen Opffer hin.", "tokens": ["Das", "Hert\u00b7ze", "le\u00b7get", "sich", "zu", "ei\u00b7nen", "Opf\u00b7fer", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verachte nicht/ mein Kind die allerreinsten Flammen/", "