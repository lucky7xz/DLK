], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ART", "PPER", "PTKA", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil mir der Himmel selbst von diesen Troste spricht:", "tokens": ["Weil", "mir", "der", "Him\u00b7mel", "selbst", "von", "die\u00b7sen", "Tros\u00b7te", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es mu\u00df die gantze Welt gerecht und billig nennen;", "tokens": ["Es", "mu\u00df", "die", "gant\u00b7ze", "Welt", "ge\u00b7recht", "und", "bil\u00b7lig", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer andre brennt/ der soll zur Straffe wieder brennen.", "tokens": ["Wer", "and\u00b7re", "brennt", "/", "der", "soll", "zur", "Straf\u00b7fe", "wie\u00b7der", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$(", "ART", "VMFIN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}