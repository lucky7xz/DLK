{"textgrid.poem.48860": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "85. \u00dcber seiner Freundin Pr\u00e4sent. Er redet sein Herze an", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Herze mu\u00df ja noch, mein Herz', an dich gedenken.", "tokens": ["Dein", "Her\u00b7ze", "mu\u00df", "ja", "noch", ",", "mein", "Her\u00b7z'", ",", "an", "dich", "ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie hat dich noch in ihr, vergisset deiner nie.", "tokens": ["Sie", "hat", "dich", "noch", "in", "ihr", ",", "ver\u00b7gis\u00b7set", "dei\u00b7ner", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$,", "VVFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schau doch, di\u00df ist ihr Pfand. Wilst du nicht glauben? Wie?", "tokens": ["Schau", "doch", ",", "di\u00df", "ist", "ihr", "Pfand", ".", "Wilst", "du", "nicht", "glau\u00b7ben", "?", "Wie", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$.", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "PWAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was sind die Sachen denn, die Tr\u00e4ume, die dich kr\u00e4nken?", "tokens": ["Was", "sind", "die", "Sa\u00b7chen", "denn", ",", "die", 