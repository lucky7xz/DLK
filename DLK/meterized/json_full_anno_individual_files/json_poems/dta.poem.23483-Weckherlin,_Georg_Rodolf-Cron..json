{"dta.poem.23483": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cron.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Damit steif bleib seiner Tuge\u0303te\u0303 bund/", "tokens": ["Da\u00b7mit", "steif", "bleib", "sei\u00b7ner", "Tug\u1ebdt\u1ebd", "bund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VVFIN", "PPOSAT", "NN", "ADJD", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Der seinen leib vnd sehl erquicket/", "tokens": ["Der", "sei\u00b7nen", "leib", "vnd", "sehl", "er\u00b7quic\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KON", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So ehret Er mit der that vnd dem", "tokens": ["So", "eh\u00b7ret", "Er", "mit", "der", "that", "vnd", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "VVFIN", "KON", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mund", "tokens": ["mund"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Die/ welche Raht vnd That begl\u00fccket.", "tokens": ["Die", "/", "wel\u00b7che", "Raht", "vnd", "That", "be\u00b7gl\u00fc\u00b7cket", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die N\u00fcchterkeit (die bey Hof in dem bann/", "tokens": ["Die", "N\u00fcch\u00b7ter\u00b7keit", "(", "die", "bey", "Hof", "in", "dem", "bann", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "NE", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die schier kein F\u00fcrst/ kein Reicher/ kein", "tokens": ["Die", "schier", "kein", "F\u00fcrst", "/", "kein", "Rei\u00b7cher", "/", "kein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "PIAT", "NN", "$(", "PIAT", "NN", "$(", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hofman", "tokens": ["Hof\u00b7man"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Mehr will weder sehen noch h\u00f6ren/", "tokens": ["Mehr", "will", "we\u00b7der", "se\u00b7hen", "noch", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "KON", "VVFIN", "ADV", "VVINF", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Vnd darumb in Jhm mehr zuehren)", "tokens": ["Vnd", "da\u00b7rumb", "in", "Jhm", "mehr", "zu\u00b7eh\u00b7ren", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APPR", "PPER", "ADV", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Die macht das sein koch Fr\u00fch-auf-sein", "tokens": ["Die", "macht", "das", "sein", "koch", "Fr\u00fch\u00b7auf\u00b7sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Seine mahlzeit nach gefallen beraitet;", "tokens": ["Sei\u00b7ne", "mahl\u00b7zeit", "nach", "ge\u00b7fal\u00b7len", "be\u00b7rai\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Vnd das der Reichtumb falscher schein", "tokens": ["Vnd", "das", "der", "Reich\u00b7tumb", "fal\u00b7scher", "schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "(der Gailheit nest) Jhr banier nicht au\u00df-", "tokens": ["(", "der", "Gail\u00b7heit", "nest", ")", "Ihr", "ba\u00b7nier", "nicht", "au\u00df"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "$(", "PPER", "ADV", "PTKNEG", "TRUNC"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Ja Sie verhindert den Wollust/", "tokens": ["Ja", "Sie", "ver\u00b7hin\u00b7dert", "den", "Wol\u00b7lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Das Er niemal die Tugent vnderdrucket/", "tokens": ["Das", "Er", "nie\u00b7mal", "die", "Tu\u00b7gent", "vn\u00b7der\u00b7dru\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Vnd sie macht/ das die Tugent schmucket", "tokens": ["Vnd", "sie", "macht", "/", "das", "die", "Tu\u00b7gent", "schmu\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$(", "PRELS", "ART", "NN", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Sein volck mit lieb/ vnd mit lob seine brust.", "tokens": ["Sein", "volck", "mit", "lieb", "/", "vnd", "mit", "lob", "sei\u00b7ne", "brust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJD", "$(", "KON", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}