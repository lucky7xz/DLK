{"textgrid.poem.43891": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Treuer Sinn,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Treuer Sinn,", "tokens": ["Treu\u00b7er", "Sinn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wirf den falschen Kummer hin!", "tokens": ["Wirf", "den", "fal\u00b7schen", "Kum\u00b7mer", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Las den Zweifel der Gedancken", "tokens": ["Las", "den", "Zwei\u00b7fel", "der", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht mit meiner Liebe zancken,", "tokens": ["Nicht", "mit", "mei\u00b7ner", "Lie\u00b7be", "zan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da Capo.", "tokens": ["Da", "Ca\u00b7po", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Da ich l\u00e4ngst dein Opfer bin.", "tokens": ["Da", "ich", "l\u00e4ngst", "dein", "Op\u00b7fer", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fcck und Zeit", "tokens": ["Gl\u00fcck", "und", "Zeit"], "token_info": ["word", "wor