{"textgrid.poem.47721": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[erwach', erwache, Fischer]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bberwach', erwache, Fischer,", "tokens": ["\u00bb", "er\u00b7wach'", ",", "er\u00b7wa\u00b7che", ",", "Fi\u00b7scher", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$,", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und leihe mir dein Fahrzeug!", "tokens": ["Und", "lei\u00b7he", "mir", "dein", "Fahr\u00b7zeug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn angewandelt hat mich", "tokens": ["Denn", "an\u00b7ge\u00b7wan\u00b7delt", "hat", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ein m\u00e4chtiges Gel\u00fcsten,", "tokens": ["Ein", "m\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7l\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu schaukeln auf der See.", "tokens": ["Zu", "schau\u00b7keln", "auf", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Spange hier von Silber,", "tokens": ["Die", "Span\u00b7ge", "hier", "von", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Zierde meines Busens,", "tokens": ["Die", "Zier\u00b7de", "mei\u00b7nes", "Bu\u00b7sens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In deine H\u00e4nde leg' ich;", "tokens": ["In", "dei\u00b7ne", "H\u00e4n\u00b7de", "leg'", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Den Fingerreif von Golde", "tokens": ["Den", "Fin\u00b7ger\u00b7reif", "von", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nicht allzu werth eracht' ich,", "tokens": ["Nicht", "all\u00b7zu", "werth", "er\u00b7acht'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Zu bieten und zu schenken,", "tokens": ["Zu", "bie\u00b7ten", "und", "zu", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Woferne du gehorchest;", "tokens": ["Wo\u00b7fer\u00b7ne", "du", "ge\u00b7hor\u00b7chest", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Drum s\u00e4ume nicht, ersteh'!\u00ab \u2013", "tokens": ["Drum", "s\u00e4u\u00b7me", "nicht", ",", "er\u00b7steh'", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "$,", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Begierig horcht der Fischer,", "tokens": ["Be\u00b7gie\u00b7rig", "horcht", "der", "Fi\u00b7scher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der d\u00fcrftige, der alte,", "tokens": ["Der", "d\u00fcrf\u00b7ti\u00b7ge", ",", "der", "al\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hebt sich auf, er tappet,", "tokens": ["Er", "hebt", "sich", "auf", ",", "er", "tap\u00b7pet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entz\u00fcndet eine Lampe,", "tokens": ["Ent\u00b7z\u00fcn\u00b7det", "ei\u00b7ne", "Lam\u00b7pe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kommt, leuchtet und betrachtet;", "tokens": ["Kommt", ",", "leuch\u00b7tet", "und", "be\u00b7trach\u00b7tet", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das edle Gut, es lacht ihn", "tokens": ["Das", "ed\u00b7le", "Gut", ",", "es", "lacht", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit hellen Augen an.", "tokens": ["Mit", "hel\u00b7len", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sofort die blanke Spange,", "tokens": ["So\u00b7fort", "die", "blan\u00b7ke", "Span\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die von der Brust gel\u00f6s'te,", "tokens": ["Die", "von", "der", "Brust", "ge\u00b7l\u00f6s'\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Den Fingerreif von Golde,", "tokens": ["Den", "Fin\u00b7ger\u00b7reif", "von", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Den von der Hand gestreiften,", "tokens": ["Den", "von", "der", "Hand", "ge\u00b7streif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ergreifet der Vergn\u00fcgte,", "tokens": ["Er\u00b7grei\u00b7fet", "der", "Ver\u00b7gn\u00fcg\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und spendet ihr den Kahn.", "tokens": ["Und", "spen\u00b7det", "ihr", "den", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Sie steigt hinein, sie rudert", "tokens": ["Sie", "steigt", "hin\u00b7ein", ",", "sie", "ru\u00b7dert"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Nach einer Stell' im Meere,", "tokens": ["Nach", "ei\u00b7ner", "Stell'", "im", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Wo neulich in die Wogen", "tokens": ["Wo", "neu\u00b7lich", "in", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Ein Knabe fischend einsank,", "tokens": ["Ein", "Kna\u00b7be", "fi\u00b7schend", "ein\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "Dorthin die schwanke lenkt sie,", "tokens": ["Dor\u00b7thin", "die", "schwan\u00b7ke", "lenkt", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Die schauerliche Bahn.", "tokens": ["Die", "schau\u00b7er\u00b7li\u00b7che", "Bahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "\u00bbnun lebe wohl, o Vater,", "tokens": ["\u00bb", "nun", "le\u00b7be", "wohl", ",", "o", "Va\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "$,", "FM", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.21": {"text": "Nun lebe wohl, o Mutter,", "tokens": ["Nun", "le\u00b7be", "wohl", ",", "o", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Nun Schwestern und Gebr\u00fcder", "tokens": ["Nun", "Schwes\u00b7tern", "und", "Ge\u00b7br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Und alle, die mir gut!", "tokens": ["Und", "al\u00b7le", ",", "die", "mir", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Ade, Ade auf ewig,", "tokens": ["A\u00b7de", ",", "A\u00b7de", "auf", "e\u00b7wig", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ADJD", "$,"], "meter": "+-+----", "measure": "unknown.measure.di"}, "line.25": {"text": "Mond, Sternenlicht und Sonne,", "tokens": ["Mond", ",", "Ster\u00b7nen\u00b7licht", "und", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Nie mehr zu schau'n gedenk' ich", "tokens": ["Nie", "mehr", "zu", "schau'n", "ge\u00b7denk'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "In eure sch\u00f6ne Gluth.", "tokens": ["In", "eu\u00b7re", "sch\u00f6\u00b7ne", "Gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Denn jener hier Gesunkne,", "tokens": ["Denn", "je\u00b7ner", "hier", "Ge\u00b7sunk\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Her winket er bei Tage,", "tokens": ["Her", "win\u00b7ket", "er", "bei", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Her winket er im Dunkeln", "tokens": ["Her", "win\u00b7ket", "er", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Die ihm verlobte Seele,", "tokens": ["Die", "ihm", "ver\u00b7lob\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Und sie gehorcht mit Freude,", "tokens": ["Und", "sie", "ge\u00b7horcht", "mit", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Die nur zu ihm gebettet", "tokens": ["Die", "nur", "zu", "ihm", "ge\u00b7bet\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Von ihrem Leide ruht.", "tokens": ["Von", "ih\u00b7rem", "Lei\u00b7de", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "In deine feuchten Arme", "tokens": ["In", "dei\u00b7ne", "feuch\u00b7ten", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Empfange mich, Geliebter!\u00ab", "tokens": ["Emp\u00b7fan\u00b7ge", "mich", ",", "Ge\u00b7lieb\u00b7ter", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Sie ruft es in die Tiefe,", "tokens": ["Sie", "ruft", "es", "in", "die", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Sie st\u00fcrzet in die Fluth.", "tokens": ["Sie", "st\u00fcr\u00b7zet", "in", "die", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bberwach', erwache, Fischer,", "tokens": ["\u00bb", "er\u00b7wach'", ",", "er\u00b7wa\u00b7che", ",", "Fi\u00b7scher", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$,", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und leihe mir dein Fahrzeug!", "tokens": ["Und", "lei\u00b7he", "mir", "dein", "Fahr\u00b7zeug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn angewandelt hat mich", "tokens": ["Denn", "an\u00b7ge\u00b7wan\u00b7delt", "hat", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ein m\u00e4chtiges Gel\u00fcsten,", "tokens": ["Ein", "m\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7l\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu schaukeln auf der See.", "tokens": ["Zu", "schau\u00b7keln", "auf", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Spange hier von Silber,", "tokens": ["Die", "Span\u00b7ge", "hier", "von", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Zierde meines Busens,", "tokens": ["Die", "Zier\u00b7de", "mei\u00b7nes", "Bu\u00b7sens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In deine H\u00e4nde leg' ich;", "tokens": ["In", "dei\u00b7ne", "H\u00e4n\u00b7de", "leg'", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Den Fingerreif von Golde", "tokens": ["Den", "Fin\u00b7ger\u00b7reif", "von", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nicht allzu werth eracht' ich,", "tokens": ["Nicht", "all\u00b7zu", "werth", "er\u00b7acht'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Zu bieten und zu schenken,", "tokens": ["Zu", "bie\u00b7ten", "und", "zu", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Woferne du gehorchest;", "tokens": ["Wo\u00b7fer\u00b7ne", "du", "ge\u00b7hor\u00b7chest", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Drum s\u00e4ume nicht, ersteh'!\u00ab \u2013", "tokens": ["Drum", "s\u00e4u\u00b7me", "nicht", ",", "er\u00b7steh'", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "$,", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Begierig horcht der Fischer,", "tokens": ["Be\u00b7gie\u00b7rig", "horcht", "der", "Fi\u00b7scher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der d\u00fcrftige, der alte,", "tokens": ["Der", "d\u00fcrf\u00b7ti\u00b7ge", ",", "der", "al\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hebt sich auf, er tappet,", "tokens": ["Er", "hebt", "sich", "auf", ",", "er", "tap\u00b7pet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entz\u00fcndet eine Lampe,", "tokens": ["Ent\u00b7z\u00fcn\u00b7det", "ei\u00b7ne", "Lam\u00b7pe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kommt, leuchtet und betrachtet;", "tokens": ["Kommt", ",", "leuch\u00b7tet", "und", "be\u00b7trach\u00b7tet", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das edle Gut, es lacht ihn", "tokens": ["Das", "ed\u00b7le", "Gut", ",", "es", "lacht", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit hellen Augen an.", "tokens": ["Mit", "hel\u00b7len", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sofort die blanke Spange,", "tokens": ["So\u00b7fort", "die", "blan\u00b7ke", "Span\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die von der Brust gel\u00f6s'te,", "tokens": ["Die", "von", "der", "Brust", "ge\u00b7l\u00f6s'\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Den Fingerreif von Golde,", "tokens": ["Den", "Fin\u00b7ger\u00b7reif", "von", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Den von der Hand gestreiften,", "tokens": ["Den", "von", "der", "Hand", "ge\u00b7streif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ergreifet der Vergn\u00fcgte,", "tokens": ["Er\u00b7grei\u00b7fet", "der", "Ver\u00b7gn\u00fcg\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und spendet ihr den Kahn.", "tokens": ["Und", "spen\u00b7det", "ihr", "den", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Sie steigt hinein, sie rudert", "tokens": ["Sie", "steigt", "hin\u00b7ein", ",", "sie", "ru\u00b7dert"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Nach einer Stell' im Meere,", "tokens": ["Nach", "ei\u00b7ner", "Stell'", "im", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Wo neulich in die Wogen", "tokens": ["Wo", "neu\u00b7lich", "in", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Ein Knabe fischend einsank,", "tokens": ["Ein", "Kna\u00b7be", "fi\u00b7schend", "ein\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "Dorthin die schwanke lenkt sie,", "tokens": ["Dor\u00b7thin", "die", "schwan\u00b7ke", "lenkt", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Die schauerliche Bahn.", "tokens": ["Die", "schau\u00b7er\u00b7li\u00b7che", "Bahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "\u00bbnun lebe wohl, o Vater,", "tokens": ["\u00bb", "nun", "le\u00b7be", "wohl", ",", "o", "Va\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "$,", "FM", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.21": {"text": "Nun lebe wohl, o Mutter,", "tokens": ["Nun", "le\u00b7be", "wohl", ",", "o", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Nun Schwestern und Gebr\u00fcder", "tokens": ["Nun", "Schwes\u00b7tern", "und", "Ge\u00b7br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Und alle, die mir gut!", "tokens": ["Und", "al\u00b7le", ",", "die", "mir", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Ade, Ade auf ewig,", "tokens": ["A\u00b7de", ",", "A\u00b7de", "auf", "e\u00b7wig", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ADJD", "$,"], "meter": "+-+----", "measure": "unknown.measure.di"}, "line.25": {"text": "Mond, Sternenlicht und Sonne,", "tokens": ["Mond", ",", "Ster\u00b7nen\u00b7licht", "und", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Nie mehr zu schau'n gedenk' ich", "tokens": ["Nie", "mehr", "zu", "schau'n", "ge\u00b7denk'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "In eure sch\u00f6ne Gluth.", "tokens": ["In", "eu\u00b7re", "sch\u00f6\u00b7ne", "Gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Denn jener hier Gesunkne,", "tokens": ["Denn", "je\u00b7ner", "hier", "Ge\u00b7sunk\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Her winket er bei Tage,", "tokens": ["Her", "win\u00b7ket", "er", "bei", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Her winket er im Dunkeln", "tokens": ["Her", "win\u00b7ket", "er", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Die ihm verlobte Seele,", "tokens": ["Die", "ihm", "ver\u00b7lob\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Und sie gehorcht mit Freude,", "tokens": ["Und", "sie", "ge\u00b7horcht", "mit", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Die nur zu ihm gebettet", "tokens": ["Die", "nur", "zu", "ihm", "ge\u00b7bet\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Von ihrem Leide ruht.", "tokens": ["Von", "ih\u00b7rem", "Lei\u00b7de", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "In deine feuchten Arme", "tokens": ["In", "dei\u00b7ne", "feuch\u00b7ten", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Empfange mich, Geliebter!\u00ab", "tokens": ["Emp\u00b7fan\u00b7ge", "mich", ",", "Ge\u00b7lieb\u00b7ter", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Sie ruft es in die Tiefe,", "tokens": ["Sie", "ruft", "es", "in", "die", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Sie st\u00fcrzet in die Fluth.", "tokens": ["Sie", "st\u00fcr\u00b7zet", "in", "die", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}