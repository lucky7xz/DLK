{"textgrid.poem.60671": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Berg, der sich in Wehen f\u00fchlte,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Berg, der sich in Wehen f\u00fchlte,", "tokens": ["Ein", "Berg", ",", "der", "sich", "in", "We\u00b7hen", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Solch einen Schrei aus seinen Tiefen w\u00fchlte,", "tokens": ["Solch", "ei\u00b7nen", "Schrei", "aus", "sei\u00b7nen", "Tie\u00b7fen", "w\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df jeder, der den L\u00e4rm vernahm", "tokens": ["Da\u00df", "je\u00b7der", ",", "der", "den", "L\u00e4rm", "ver\u00b7nahm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schnell herbeigelaufen kam,", "tokens": ["Und", "schnell", "her\u00b7bei\u00b7ge\u00b7lau\u00b7fen", "kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht anders meinte, als ein Unget\u00fcm", "tokens": ["Nicht", "an\u00b7ders", "mein\u00b7te", ",", "als", "ein", "Un\u00b7ge\u00b7t\u00fcm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "$,", "KOUS", "ART", "NN