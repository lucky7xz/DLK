{"dta.poem.15803": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vorm Spiegel auf dem Tisch im K\u00f6rbchen standen Fr\u00fcchte,", "tokens": ["Vorm", "Spie\u00b7gel", "auf", "dem", "Tisch", "im", "K\u00f6rb\u00b7chen", "stan\u00b7den", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die spiegelten sich ab im Spiegel bei dem Lichte.", "tokens": ["Die", "spie\u00b7gel\u00b7ten", "sich", "ab", "im", "Spie\u00b7gel", "bei", "dem", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKVZ", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie standen zweimal da, einmal auf ihrem Tische,", "tokens": ["Sie", "stan\u00b7den", "zwei\u00b7mal", "da", ",", "ein\u00b7mal", "auf", "ih\u00b7rem", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das andremal im Glas, und mit zwiefacher Frische.", "tokens": ["Das", "and\u00b7re\u00b7mal", "im", "Glas", ",", "und", "mit", "zwie\u00b7fac\u00b7her", "Fri\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPRART", "NN", "$,", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}}, "stanza.3": {"line.1": {"text": "Der Vater sprach zum Kind: Wenn du hier w\u00e4hlen solltest,", "tokens": ["Der", "Va\u00b7ter", "sprach", "zum", "Kind", ":", "Wenn", "du", "hier", "w\u00e4h\u00b7len", "soll\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$.", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von beiden K\u00f6rbchen, sprich, welcheins du lieber wolltest?", "tokens": ["Von", "bei\u00b7den", "K\u00f6rb\u00b7chen", ",", "sprich", ",", "welc\u00b7heins", "du", "lie\u00b7ber", "woll\u00b7test", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJD", "$,", "PRELS", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das liebe Kind sprach unbedenklich: Das da drinnen!", "tokens": ["Das", "lie\u00b7be", "Kind", "sprach", "un\u00b7be\u00b7denk\u00b7lich", ":", "Das", "da", "drin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$.", "PDS", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Vater aber nahm das andere von hinnen,", "tokens": ["Der", "Va\u00b7ter", "a\u00b7ber", "nahm", "das", "an\u00b7de\u00b7re", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und sprach zum Kinde: Nimm dir nun das K\u00f6rbchen dort!", "tokens": ["Und", "sprach", "zum", "Kin\u00b7de", ":", "Nimm", "dir", "nun", "das", "K\u00f6rb\u00b7chen", "dort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$.", "VVIMP", "PPER", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verwundert aber riefs: O Vater, es ist fort.", "tokens": ["Ver\u00b7wun\u00b7dert", "a\u00b7ber", "riefs", ":", "O", "Va\u00b7ter", ",", "es", "ist", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "$.", "NE", "NN", "$,", "PPER", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Vater sprach: Und wei\u00dft du auch, wo's hingekommen?", "tokens": ["Der", "Va\u00b7ter", "sprach", ":", "Und", "wei\u00dft", "du", "auch", ",", "wo's", "hin\u00b7ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "VVFIN", "PPER", "ADV", "$,", "PWAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist verschwunden, weil ich dieses weggenommen.", "tokens": ["Es", "ist", "ver\u00b7schwun\u00b7den", ",", "weil", "ich", "die\u00b7ses", "weg\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "O da\u00df doch, liebes Kind, nie, weil gering dir gilt", "tokens": ["O", "da\u00df", "doch", ",", "lie\u00b7bes", "Kind", ",", "nie", ",", "weil", "ge\u00b7ring", "dir", "gilt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ADV", "$,", "ADJA", "NN", "$,", "ADV", "$,", "KOUS", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wirklichkeit, du greifst nach einem Spiegelbild!", "tokens": ["Die", "Wirk\u00b7lich\u00b7keit", ",", "du", "greifst", "nach", "ei\u00b7nem", "Spie\u00b7gel\u00b7bild", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}