{"textgrid.poem.66671": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die weichen Winde streicheln mich vom Garten,", "tokens": ["Die", "wei\u00b7chen", "Win\u00b7de", "strei\u00b7cheln", "mich", "vom", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herz ward fr\u00f6hlich aus Melancholie;", "tokens": ["Mein", "Herz", "ward", "fr\u00f6h\u00b7lich", "aus", "Me\u00b7lan\u00b7cho\u00b7lie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mag nun das Schicksal mischen seine Karten,", "tokens": ["Mag", "nun", "das", "Schick\u00b7sal", "mi\u00b7schen", "sei\u00b7ne", "Kar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich fand die Ruh, nach der ich lange schrie.", "tokens": ["Ich", "fand", "die", "Ruh", ",", "nach", "der", "ich", "lan\u00b7ge", "schrie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mitunter nur spukt's auf aus l\u00e4ngstverscharrten", "tokens": ["Mi\u00b7tun\u00b7ter", "nur", "spukt's", "auf", "aus", "l\u00e4ngst\u00b7ver\u00b7scharr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "APPR", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gebeinen meiner Unrast, schwanke Knie", "tokens": ["Ge\u00b7bei\u00b7nen", "mei\u00b7ner", "Un\u00b7rast", ",", "schwan\u00b7ke", "Knie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gehn schlotternd um, des Gl\u00fcckes Fetzen flattern,", "tokens": ["Gehn", "schlot\u00b7ternd", "um", ",", "des", "Gl\u00fc\u00b7ckes", "Fet\u00b7zen", "flat\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Aus Gr\u00e4bern rascheln toter Reue Nattern ...", "tokens": ["Aus", "Gr\u00e4\u00b7bern", "ra\u00b7scheln", "to\u00b7ter", "Reu\u00b7e", "Nat\u00b7tern", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die nicht mehr Nattern sind. Nur ausged\u00f6rrte,", "tokens": ["Die", "nicht", "mehr", "Nat\u00b7tern", "sind", ".", "Nur", "aus\u00b7ge\u00b7d\u00f6rr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PIAT", "NN", "VAFIN", "$.", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Phantastisch aufgeblasne Schlangenbrut,", "tokens": ["Phan\u00b7tas\u00b7tisch", "auf\u00b7ge\u00b7blas\u00b7ne", "Schlan\u00b7gen\u00b7brut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die b\u00f6se Laune mir vors Auge zerrte,", "tokens": ["Die", "b\u00f6\u00b7se", "Lau\u00b7ne", "mir", "vors", "Au\u00b7ge", "zerr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zuwidersinn und qu\u00e4lerische Wut,", "tokens": ["Zu\u00b7wi\u00b7der\u00b7sinn", "und", "qu\u00e4\u00b7le\u00b7ri\u00b7sche", "Wut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ich den Ausblick mir ins Licht versperrte", "tokens": ["Wenn", "ich", "den", "Aus\u00b7blick", "mir", "ins", "Licht", "ver\u00b7sperr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus grauem, gr\u00fcblerischem Untermut \u2013", "tokens": ["Aus", "grau\u00b7em", ",", "gr\u00fcb\u00b7ler\u00b7i\u00b7schem", "Un\u00b7ter\u00b7mut", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So regt sich wohl zuweilen halb in Tr\u00e4umen", "tokens": ["So", "regt", "sich", "wohl", "zu\u00b7wei\u00b7len", "halb", "in", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wahnwitzige Hast, zu z\u00fcgeln nicht, zu z\u00e4umen.", "tokens": ["Wahn\u00b7wit\u00b7zi\u00b7ge", "Hast", ",", "zu", "z\u00fc\u00b7geln", "nicht", ",", "zu", "z\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PTKZU", "VVINF", "PTKNEG", "$,", "PTKZU", "VVINF", "$."], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Ein Reiter war ich auf dem schnellsten Rosse,", "tokens": ["Ein", "Rei\u00b7ter", "war", "ich", "auf", "dem", "schnells\u00b7ten", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kopf dicht an Hals, hussa! Die M\u00fctze flog,", "tokens": ["Kopf", "dicht", "an", "Hals", ",", "hus\u00b7sa", "!", "Die", "M\u00fct\u00b7ze", "flog", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "$,", "ITJ", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Kot des Hufs bespritzt, weit in die Gosse,", "tokens": ["Vom", "Kot", "des", "Hufs", "be\u00b7spritzt", ",", "weit", "in", "die", "Gos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indes des Pferdes N\u00fcster Stickluft sog.", "tokens": ["In\u00b7des", "des", "Pfer\u00b7des", "N\u00fcs\u00b7ter", "Stick\u00b7luft", "so\u00b7g."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "XY"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Der j\u00e4he Wirbelsturm ward mein Genosse,", "tokens": ["Der", "j\u00e4\u00b7he", "Wir\u00b7bels\u00b7turm", "ward", "mein", "Ge\u00b7nos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Birke, Pappelbaum zur Erde bog \u2013", "tokens": ["Der", "Bir\u00b7ke", ",", "Pap\u00b7pel\u00b7baum", "zur", "Er\u00b7de", "bog", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So ging es rasend \u00fcber Stock und Steine,", "tokens": ["So", "ging", "es", "ra\u00b7send", "\u00fc\u00b7ber", "Stock", "und", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Himmel droht' in schwefelgelbem Scheine.", "tokens": ["Der", "Him\u00b7mel", "droht'", "in", "schwe\u00b7fel\u00b7gel\u00b7bem", "Schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Pardauz! Dies Wort blieb eingebrannt der Lippe,", "tokens": ["Par\u00b7dauz", "!", "Dies", "Wort", "blieb", "ein\u00b7ge\u00b7brannt", "der", "Lip\u00b7pe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch war's sch\u00f6n: der Schwung von Fall zu Fall.", "tokens": ["Und", "doch", "wa\u00b7r's", "sch\u00f6n", ":", "der", "Schwung", "von", "Fall", "zu", "Fall", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "$.", "ART", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Niemals beneid' ich die normale Sippe,", "tokens": ["Nie\u00b7mals", "be\u00b7neid'", "ich", "die", "nor\u00b7ma\u00b7le", "Sip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die zahme Kracken vorf\u00fchrt aus dem Stall.", "tokens": ["Die", "zah\u00b7me", "Kra\u00b7cken", "vor\u00b7f\u00fchrt", "aus", "dem", "Stall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Spottlust der Passanten war mir schnippe,", "tokens": ["Die", "Spott\u00b7lust", "der", "Pas\u00b7san\u00b7ten", "war", "mir", "schnip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu k\u00f6stlich schien der wilde Intervall", "tokens": ["Zu", "k\u00f6st\u00b7lich", "schien", "der", "wil\u00b7de", "In\u00b7ter\u00b7vall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von Sturz zu Sturz: und brach man ein paar Knochen,", "tokens": ["Von", "Sturz", "zu", "Sturz", ":", "und", "brach", "man", "ein", "paar", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$.", "KON", "VVFIN", "PIS", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So kam man doch ins Leben nicht gekrochen.", "tokens": ["So", "kam", "man", "doch", "ins", "Le\u00b7ben", "nicht", "ge\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPRART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Hundetrott wird mir wohl kaum gelingen,", "tokens": ["Der", "Hun\u00b7de\u00b7trott", "wird", "mir", "wohl", "kaum", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jag' ich auch just nicht mehr so toll drauf los.", "tokens": ["Jag'", "ich", "auch", "just", "nicht", "mehr", "so", "toll", "drauf", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "ADV", "ADJD", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Solang noch Kraft in meiner Seele Schwingen,", "tokens": ["So\u00b7lang", "noch", "Kraft", "in", "mei\u00b7ner", "See\u00b7le", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geht's munter weiter: Welt, wie bist du gro\u00df!", "tokens": ["Geht's", "mun\u00b7ter", "wei\u00b7ter", ":", "Welt", ",", "wie", "bist", "du", "gro\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$.", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den selbstgez\u00e4hmten Hengst soll keiner dingen,", "tokens": ["Den", "selbst\u00b7ge\u00b7z\u00e4hm\u00b7ten", "Hengst", "soll", "kei\u00b7ner", "din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df er ihn spannt zu Peitschenhieb und -sto\u00df", "tokens": ["Da\u00df", "er", "ihn", "spannt", "zu", "Peit\u00b7schen\u00b7hieb", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u2013 Schon der Gedanke liegt mir schwer im Magen \u2013", "tokens": ["\u2013", "Schon", "der", "Ge\u00b7dan\u00b7ke", "liegt", "mir", "schwer", "im", "Ma\u00b7gen", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vor seinen traurigen Furagewagen ...", "tokens": ["Vor", "sei\u00b7nen", "trau\u00b7ri\u00b7gen", "Fu\u00b7ra\u00b7ge\u00b7wa\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Vom Garten streicheln mich die weichen Winde,", "tokens": ["Vom", "Gar\u00b7ten", "strei\u00b7cheln", "mich", "die", "wei\u00b7chen", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Schwermut ward mein Herz ein fr\u00f6hlich Ding,", "tokens": ["Aus", "Schwer\u00b7mut", "ward", "mein", "Herz", "ein", "fr\u00f6h\u00b7lich", "Ding", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier ist mein Weib, bei der die Ruh ich finde,", "tokens": ["Hier", "ist", "mein", "Weib", ",", "bei", "der", "die", "Ruh", "ich", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "ART", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach der umsonst ich auf die Suche ging.", "tokens": ["Nach", "der", "um\u00b7sonst", "ich", "auf", "die", "Su\u00b7che", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hier ist der Baum, dran ich mein R\u00f6\u00dflein binde,", "tokens": ["Hier", "ist", "der", "Baum", ",", "dran", "ich", "mein", "R\u00f6\u00df\u00b7lein", "bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hier ist der Ast, dran ich das Zaumzeug hing \u2013", "tokens": ["Hier", "ist", "der", "Ast", ",", "dran", "ich", "das", "Zaum\u00b7zeug", "hing", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PAV", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text"