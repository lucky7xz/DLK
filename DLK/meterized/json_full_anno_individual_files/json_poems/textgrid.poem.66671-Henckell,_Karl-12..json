"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indes des Pferdes N\u00fcster Stickluft sog.", "tokens": ["In\u00b7des", "des", "Pfer\u00b7des", "N\u00fcs\u00b7ter", "Stick\u00b7luft", "so\u00b7g."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "XY"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Der j\u00e4he Wirbelsturm ward mein Genosse,", "tokens": ["Der", "j\u00e4\u00b7he", "Wir\u00b7bels\u00b7turm", "ward", "mein", "Ge\u00b7nos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Birke, Pappelbaum zur Erde bog \u2013", "tokens": ["Der", "Bir\u00b7ke", ",", "Pap\u00b7pel\u00b7baum", "zur", "Er\u00b7de", "bog", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So ging es rasend \u00fcber Stock und Steine,", "tokens": ["So", "ging", "es", "ra\u00b7send", "\u00fc\u00b7ber", "Stock", "und", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Himmel droht' in schwefelgelbem Scheine.", "tokens": ["Der", "Him\u00b7mel", "droht'", "in", "schwe\u00b7fel\u00b7gel\u00b7bem", "Schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Pardauz! Dies Wort blieb eingebrannt der Lippe,", "tokens": ["Par\u00b7dauz", "!", "Die