{"dta.poem.7595": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, unter wei\u00dflicher Wolken zerstreutem V\u00f6lkchen", "tokens": ["Sieh", ",", "un\u00b7ter", "wei\u00df\u00b7li\u00b7cher", "Wol\u00b7ken", "zer\u00b7streu\u00b7tem", "V\u00f6lk\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Mond am Himmel schwebt als kleinstes wei\u00dfes W\u00f6lkchen.", "tokens": ["Der", "Mond", "am", "Him\u00b7mel", "schwebt", "als", "kleins\u00b7tes", "wei\u00b7\u00dfes", "W\u00f6lk\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "KOKOM", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ganz wie die andern bleich, an Gr\u00f6\u00dfe keinem gleich,", "tokens": ["Ganz", "wie", "die", "an\u00b7dern", "bleich", ",", "an", "Gr\u00f6\u00b7\u00dfe", "kei\u00b7nem", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "ADJD", "$,", "APPR", "NN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unscheinbar in des Tags ihm fremden Sonnenreich.", "tokens": ["Un\u00b7schein\u00b7bar", "in", "des", "Tags", "ihm", "frem\u00b7den", "Son\u00b7nen\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch, tritt er an mit Macht das Regiment der Nacht,", "tokens": ["Doch", ",", "tritt", "er", "an", "mit", "Macht", "das", "Re\u00b7gi\u00b7ment", "der", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "APPR", "APPR", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh, wie sein Licht zunicht den Stolz der Wolken macht!", "tokens": ["Sieh", ",", "wie", "sein", "Licht", "zu\u00b7nicht", "den", "Stolz", "der", "Wol\u00b7ken", "macht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie betteln nun um Glanz, und ehrerbietig ganz", "tokens": ["Sie", "bet\u00b7teln", "nun", "um", "Glanz", ",", "und", "ehr\u00b7er\u00b7bie\u00b7tig", "ganz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,", "KON", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von ferne schlingen sie um seinen Thron den Kranz.", "tokens": ["Von", "fer\u00b7ne", "schlin\u00b7gen", "sie", "um", "sei\u00b7nen", "Thron", "den", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So, der sich unterm Chor Unedler erst verlor,", "tokens": ["So", ",", "der", "sich", "un\u00b7term", "Chor", "U\u00b7ned\u00b7ler", "erst", "ver\u00b7lor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PRF", "APPRART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann seine Zeit kommt, tritt des Edlen Glanz hervor.", "tokens": ["Wann", "sei\u00b7ne", "Zeit", "kommt", ",", "tritt", "des", "Ed\u00b7len", "Glanz", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}