{"textgrid.poem.48736": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "23. An Herrn Otto Br\u00fcggemann, als er zu Reval wieder gesund worden war", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was das erz\u00fcrnte Meer nur neulich Willens war,", "tokens": ["Was", "das", "er\u00b7z\u00fcrn\u00b7te", "Meer", "nur", "neu\u00b7lich", "Wil\u00b7lens", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ADV", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das hatte gestern noch die Krankheit in dem Sinne.", "tokens": ["das", "hat\u00b7te", "ge\u00b7stern", "noch", "die", "Krank\u00b7heit", "in", "dem", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie beide waren eins, da\u00df euch die Plutoninne", "tokens": ["Sie", "bei\u00b7de", "wa\u00b7ren", "eins", ",", "da\u00df", "euch", "die", "Plu\u00b7to\u00b7nin\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VAFIN", "PIS", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das schwache Leben setzt in t\u00f6tliche Gefahr.", "tokens": ["das", "schwa\u00b7che", "Le\u00b7ben", "setzt", "in", "t\u00f6t\u00b7li\u00b7che", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine