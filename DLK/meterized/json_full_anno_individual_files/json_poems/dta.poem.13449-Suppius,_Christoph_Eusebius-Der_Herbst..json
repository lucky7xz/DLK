{"dta.poem.13449": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der Herbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Die Natur scheint itzo zu veralten,", "tokens": ["Die", "Na\u00b7tur", "scheint", "it\u00b7zo", "zu", "ver\u00b7al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jhr Gesicht zeigt runzelichte Falten,", "tokens": ["Ihr", "Ge\u00b7sicht", "zeigt", "run\u00b7ze\u00b7lich\u00b7te", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und es fliehen die Annehmlichkeiten,", "tokens": ["Und", "es", "flie\u00b7hen", "die", "An\u00b7nehm\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Vor des Winters kaltem N\u00e4herschreiten.", "tokens": ["Vor", "des", "Win\u00b7ters", "kal\u00b7tem", "N\u00e4\u00b7her\u00b7schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "J\u00fcngstens sah ich sie beym bunten Lenzen", "tokens": ["J\u00fcng\u00b7stens", "sah", "ich", "sie", "beym", "bun\u00b7ten", "Len\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00f6chstverliebt im g\u00fcldnen Brautschmuck gl\u00e4nzen,", "tokens": ["H\u00f6chst\u00b7ver\u00b7liebt", "im", "g\u00fcld\u00b7nen", "Brautsc\u00b7hmuck", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und sie schien mit reizungsvollen Blicken", "tokens": ["Und", "sie", "schien", "mit", "rei\u00b7zungs\u00b7vol\u00b7len", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Selbst die Schaar der Nymphen zu entz\u00fccken.", "tokens": ["Selbst", "die", "Schaar", "der", "Nym\u00b7phen", "zu", "ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber ach! ihr mattes Wesen zeiget,", "tokens": ["A\u00b7ber", "ach", "!", "ihr", "mat\u00b7tes", "We\u00b7sen", "zei\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df sie nun dem Grabe n\u00e4her steiget,", "tokens": ["Da\u00df", "sie", "nun", "dem", "Gra\u00b7be", "n\u00e4\u00b7her", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und ihr kahler Scheitel l\u00e4sset sehen,", "tokens": ["Und", "ihr", "kah\u00b7ler", "Schei\u00b7tel", "l\u00e4s\u00b7set", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df mirs wird nach Weiberweise gehen.", "tokens": ["Da\u00df", "mirs", "wird", "nach", "Wei\u00b7ber\u00b7wei\u00b7se", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Doch bevor ich zu den kalten Stufen", "tokens": ["Doch", "be\u00b7vor", "ich", "zu", "den", "kal\u00b7ten", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einer Winternacht werd abgerufen,", "tokens": ["Ei\u00b7ner", "Win\u00b7ter\u00b7nacht", "werd", "ab\u00b7ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "So will ich mich sonder alles H\u00e4rmen", "tokens": ["So", "will", "ich", "mich", "son\u00b7der", "al\u00b7les", "H\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PIAT", "NN"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Erstlich noch an Wein und Liebe w\u00e4rmen,", "tokens": ["Erst\u00b7lich", "noch", "an", "Wein", "und", "Lie\u00b7be", "w\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Werd ich mich dadurch erhitzet f\u00fchlen,", "tokens": ["Werd", "ich", "mich", "da\u00b7durch", "er\u00b7hit\u00b7zet", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PAV", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ey! wie will ich auf der Leyer spielen,", "tokens": ["Ey", "!", "wie", "will", "ich", "auf", "der", "Le\u00b7yer", "spie\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Phillis wird der Innhalt meiner Lieder,", "tokens": ["Phil\u00b7lis", "wird", "der", "Inn\u00b7halt", "mei\u00b7ner", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Holla! fort! es leben meine Br\u00fcder.", "tokens": ["Hol\u00b7la", "!", "fort", "!", "es", "le\u00b7ben", "mei\u00b7ne", "Br\u00fc\u00b7der", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Freundschaft! komm, und mische dich im Reihen,", "tokens": ["Freund\u00b7schaft", "!", "komm", ",", "und", "mi\u00b7sche", "dich", "im", "Rei\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$,", "KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dir will ich ein neues Eden weihen,", "tokens": ["Dir", "will", "ich", "ein", "neu\u00b7es", "E\u00b7den", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Unschuld soll uns mit Vergn\u00fcgen", "tokens": ["Und", "die", "Un\u00b7schuld", "soll", "uns", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In den Schlaf zu s\u00fcssen Tr\u00e4umen wiegen.", "tokens": ["In", "den", "Schlaf", "zu", "s\u00fcs\u00b7sen", "Tr\u00e4u\u00b7men", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wird uns denn der Winter n\u00e4her dringen,", "tokens": ["Wird", "uns", "denn", "der", "Win\u00b7ter", "n\u00e4\u00b7her", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da will ich erst h\u00f6chstzufrieden singen;", "tokens": ["Da", "will", "ich", "erst", "h\u00f6chst\u00b7zu\u00b7frie\u00b7den", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJA", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Top! es leben Phillis, Wein und Tugend,", "tokens": ["Top", "!", "es", "le\u00b7ben", "Phil\u00b7lis", ",", "Wein", "und", "Tu\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und der Freundschaft ewigmuntre Jugend.", "tokens": ["Und", "der", "Freund\u00b7schaft", "e\u00b7wig\u00b7munt\u00b7re", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}