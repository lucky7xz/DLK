{"textgrid.poem.38001": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Familiengem\u00e4lde", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An allem Ort und Ende,", "tokens": ["An", "al\u00b7lem", "Ort", "und", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Soll der gesegnet sein,", "tokens": ["Soll", "der", "ge\u00b7seg\u00b7net", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "VVPP", "VAINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Den Arbeit seiner H\u00e4nde,", "tokens": ["Den", "Ar\u00b7beit", "sei\u00b7ner", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ern\u00e4hret still und fein,", "tokens": ["Er\u00b7n\u00e4h\u00b7ret", "still", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gott will ihm dazu geben,", "tokens": ["Gott", "will", "ihm", "da\u00b7zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Ehfrau tugendreich,", "tokens": ["Ein", "Eh\u00b7frau", "tu\u00b7gen\u00b7dreich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die einr fruchtbaren Weinreben", "tokens": ["Die", "einr", "frucht\