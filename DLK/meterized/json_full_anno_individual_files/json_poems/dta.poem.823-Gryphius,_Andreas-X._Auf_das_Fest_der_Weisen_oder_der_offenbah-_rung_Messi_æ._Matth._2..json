{"dta.poem.823": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "X.  Auf das Fest der Weisen/ oder der offenbah-  \n rung Messi \u00e6.  Matth. 2.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Wo sol ich dich mein Licht/ mein h\u00f6chster K\u00f6nig finden/ ", "tokens": ["Wo", "sol", "ich", "dich", "mein", "Licht", "/", "mein", "h\u00f6chs\u00b7ter", "K\u00f6\u00b7nig", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn auch dein eigen Volck nichts von dir h\u00e4lt noch", "tokens": ["Wenn", "auch", "dein", "ei\u00b7gen", "Volck", "nichts", "von", "dir", "h\u00e4lt", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJD", "NN", "PIS", "APPR", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wei\u00df? ", "tokens": ["wei\u00df", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Was hilfft mich suchen viel? was nutzt der fragen flei\u00df", "tokens": ["Was", "hilfft", "mich", "su\u00b7chen", "viel", "?", "was", "nutzt", "der", "fra\u00b7gen", "flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "VVFIN", "ADV", "$.", "PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn mich verblendet hat die Nacht der tr\u00fcben S\u00fcnden?", "tokens": ["Wenn", "mich", "ver\u00b7blen\u00b7det", "hat", "die", "Nacht", "der", "tr\u00fc\u00b7ben", "S\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch! wenn du nur nicht hei\u00dft den hellen Stern ver-", "tokens": ["Doch", "!", "wenn", "du", "nur", "nicht", "hei\u00dft", "den", "hel\u00b7len", "Stern", "ver"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$.", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schwinden/ ", "tokens": ["schwin\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Den deine Lieb aufsteckt/ soll meine Seelen rei\u00df", "tokens": ["Den", "dei\u00b7ne", "Lieb", "auf\u00b7steckt", "/", "soll", "mei\u00b7ne", "See\u00b7len", "rei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$(", "VMFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Starck fort vnd f\u00fcr sich gehn/ bi\u00df ich dich Zions Prei\u00df", "tokens": ["Starck", "fort", "vnd", "f\u00fcr", "sich", "gehn", "/", "bi\u00df", "ich", "dich", "Zi\u00b7ons", "Prei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "APPR", "PRF", "VVINF", "$(", "KOUS", "PPER", "PRF", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vnd sch\u00f6nste Sarons Blum/ m\u00f6g vmb mein Hertze winden.", "tokens": ["Vnd", "sch\u00f6ns\u00b7te", "Sa\u00b7rons", "Blum", "/", "m\u00f6g", "vmb", "mein", "Hert\u00b7ze", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NE", "NE", "$(", "VMFIN", "APPR", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn wil ich dir f\u00fcr Goldt mein F\u00fcrst nicht falsche trew/", "tokens": ["Denn", "wil", "ich", "dir", "f\u00fcr", "Goldt", "mein", "F\u00fcrst", "nicht", "fal\u00b7sche", "trew", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "APPR", "NE", "PPOSAT", "NN", "PTKNEG", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr Weyrauch/ Andachts ", "tokens": ["F\u00fcr", "Wey\u00b7rauch", "/", "An\u00b7dachts"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$(", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mein Priester/ der du Todt vnd S\u00fcnde tilgest/ geben.", "tokens": ["Mein", "Pries\u00b7ter", "/", "der", "du", "Todt", "vnd", "S\u00fcn\u00b7de", "til\u00b7gest", "/", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "NN", "KON", "NN", "VVFIN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du l\u00e4utere dis Gold in Liebe/ la\u00df in pein", "tokens": ["Du", "l\u00e4u\u00b7te\u00b7re", "dis", "Gold", "in", "Lie\u00b7be", "/", "la\u00df", "in", "pein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "NN", "APPR", "NN", "$(", "VVIMP", "APPR", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr Weyrauch die Gedult/ ja mich dein R\u00e4uchwerck seyn-", "tokens": ["F\u00fcr", "Wey\u00b7rauch", "die", "Ge\u00b7dult", "/", "ja", "mich", "dein", "R\u00e4uch\u00b7werck", "seyn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$(", "ADV", "PPER", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd wenn der C\u00f6rper faull\u2019t so hei\u00df die Seele leben.", "tokens": ["Vnd", "wenn", "der", "C\u00f6r\u00b7per", "faull't", "so", "hei\u00df", "die", "See\u00b7le", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "ADV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}