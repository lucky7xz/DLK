{"dta.poem.13599": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Terzett .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch kehrt ihr beide", "tokens": ["Doch", "kehrt", "ihr", "bei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Im letzten Strahl", "tokens": ["Im", "letz\u00b7ten", "Strahl"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Sonne wieder", "tokens": ["Der", "Son\u00b7ne", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Zur\u00fcck ins Thal,", "tokens": ["Zu\u00b7r\u00fcck", "ins", "Thal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Schaut mir so freudig", "tokens": ["Schaut", "mir", "so", "freu\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "In's Angesicht:", "tokens": ["In's", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da wei\u00df ich's pl\u00f6tzlich \u2014", "tokens": ["Da", "wei\u00df", "ich's", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Doch sag' ich's nicht.", "tokens": ["Doch", "sag'", "ich's", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}