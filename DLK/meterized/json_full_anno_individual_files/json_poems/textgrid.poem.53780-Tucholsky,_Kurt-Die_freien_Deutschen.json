{"textgrid.poem.53780": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die freien Deutschen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Papst abends durch seine Gem\u00e4cher geht,", "tokens": ["Wenn", "der", "Papst", "a\u00b7bends", "durch", "sei\u00b7ne", "Ge\u00b7m\u00e4\u00b7cher", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "leise, vorsichtig wandelnd, es ist schon sp\u00e4t,", "tokens": ["lei\u00b7se", ",", "vor\u00b7sich\u00b7tig", "wan\u00b7delnd", ",", "es", "ist", "schon", "sp\u00e4t", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVPP", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "bleibt er am B\u00fccherbord ein bi\u00dfchen stehn,", "tokens": ["bleibt", "er", "am", "B\u00fc\u00b7cher\u00b7bord", "ein", "bi\u00df\u00b7chen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "l\u00e4\u00dft den Blick \u00fcber mattschimmernde Titel gehn . . .", "tokens": ["l\u00e4\u00dft", "den", "Blick", "\u00fc\u00b7ber", "matt\u00b7schim\u00b7mern\u00b7de", "Ti\u00b7tel", "gehn", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$.", "$.", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.5": {"text": "Herders Werke \u2013 ist da zu lesen . . .", "tokens": ["Her\u00b7ders", "Wer\u00b7ke", "\u2013", "ist", "da", "zu", "le\u00b7sen", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "NN", "$(", "VAFIN", "ADV", "PTKZU", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbah \u2013 Deutschland \u2013\u00ab denkt er, \u00bbein gutes Land.", "tokens": ["\u00bb", "ah", "\u2013", "Deutschland", "\u2013", "\u00ab", "denkt", "er", ",", "\u00bb", "ein", "gu\u00b7tes", "Land", "."], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$(", "NE", "$(", "$(", "VVFIN", "PPER", "$,", "$(", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Das ist uns sicher. Das haben wir fest in der Hand.", "tokens": ["Das", "ist", "uns", "si\u00b7cher", ".", "Das", "ha\u00b7ben", "wir", "fest", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "$.", "PDS", "VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da ist nichts zu f\u00fcrchten . . . \u00dcbrigens ist das sein Gl\u00fcck \u2013!\u00ab", "tokens": ["Da", "ist", "nichts", "zu", "f\u00fcrch\u00b7ten", ".", ".", ".", "\u00dcb\u00b7ri\u00b7gens", "ist", "das", "sein", "Gl\u00fcck", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKZU", "VVINF", "$.", "$.", "$.", "ADV", "VAFIN", "ART", "PPOSAT", "NN", "$(", "$.", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und dann geht er ein St\u00fcck", "tokens": ["Und", "dann", "geht", "er", "ein", "St\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.10": {"text": "und zieht sich g\u00e4nzlich in seine Gem\u00e4cher zur\u00fcck.", "tokens": ["und", "zieht", "sich", "g\u00e4nz\u00b7lich", "in", "sei\u00b7ne", "Ge\u00b7m\u00e4\u00b7cher", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn ein Bankdirektor am Adriatischen Meer", "tokens": ["Wenn", "ein", "Bank\u00b7di\u00b7rek\u00b7tor", "am", "Ad\u00b7ri\u00b7a\u00b7ti\u00b7schen", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "badet \u2013 frischen Wind bringt die Luft von S\u00fcden her,", "tokens": ["ba\u00b7det", "\u2013", "fri\u00b7schen", "Wind", "bringt", "die", "Luft", "von", "S\u00fc\u00b7den", "her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "die Wellen glitzern . . . drau\u00dfen treibt ein Boot . . .", "tokens": ["die", "Wel\u00b7len", "glit\u00b7zern", ".", ".", ".", "drau\u00b7\u00dfen", "treibt", "ein", "Boot", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$.", "$.", "ADV", "VVFIN", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Bankmann frottiert sich mit seinem Bademantel, der ist wei\u00df und rot . . .", "tokens": ["Der", "Bank\u00b7mann", "frot\u00b7tiert", "sich", "mit", "sei\u00b7nem", "Ba\u00b7de\u00b7man\u00b7tel", ",", "der", "ist", "wei\u00df", "und", "rot", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,", "PRELS", "VAFIN", "ADJD", "KON", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "\u00bb\u00fcbrigens\u00ab, sagt er zu seinem Schwager, der neben ihm sitzt", "tokens": ["\u00bb", "\u00fcb\u00b7ri\u00b7gens", "\u00ab", ",", "sagt", "er", "zu", "sei\u00b7nem", "Schwa\u00b7ger", ",", "der", "ne\u00b7ben", "ihm", "sitzt"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "und dumpfbr\u00fctend schwitzt,", "tokens": ["und", "dumpf\u00b7br\u00fc\u00b7tend", "schwitzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.7": {"text": "\u00bbdiesmal bin ich direkt ruhig auf Urlaub gefahren.", "tokens": ["\u00bb", "dies\u00b7mal", "bin", "ich", "di\u00b7rekt", "ru\u00b7hig", "auf", "Ur\u00b7laub", "ge\u00b7fah\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADJD", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "+-+---++-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Alles ist still. Im Reichstag liegen se sich in den Haaren.", "tokens": ["Al\u00b7les", "ist", "still", ".", "Im", "Reichs\u00b7tag", "lie\u00b7gen", "se", "sich", "in", "den", "Haa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "$.", "APPRART", "NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.9": {"text": "La\u00df se liegen. Kein Bolschewismus. Kein Experiment.", "tokens": ["La\u00df", "se", "lie\u00b7gen", ".", "Kein", "Bol\u00b7sche\u00b7wis\u00b7mus", ".", "Kein", "Ex\u00b7pe\u00b7ri\u00b7ment", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVFIN", "$.", "PIAT", "NN", "$.", "PIAT", "NN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Unberufen . . . Bei so einem Pr\u00e4sident \u2013!\u00ab", "tokens": ["Un\u00b7be\u00b7ru\u00b7fen", ".", ".", ".", "Bei", "so", "ei\u00b7nem", "Pr\u00e4\u00b7si\u00b7dent", "\u2013", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$.", "$.", "$.", "APPR", "ADV", "ART", "NN", "$(", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Und der Schwager schwitzt und hockt kalbsd\u00e4mlich da.", "tokens": ["Und", "der", "Schwa\u00b7ger", "schwitzt", "und", "hockt", "kalbs\u00b7d\u00e4m\u00b7lich", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.12": {"text": "Sch\u00f6ner Sonnenfriede liegt \u00fcber der Adria.", "tokens": ["Sch\u00f6\u00b7ner", "Son\u00b7nen\u00b7frie\u00b7de", "liegt", "\u00fc\u00b7ber", "der", "Ad\u00b7ria", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NE", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Gutsbesitzer. Milit\u00e4rs. Stahlhelmkommis. Richter. Polizei.", "tokens": ["Guts\u00b7be\u00b7sit\u00b7zer", ".", "Mi\u00b7li\u00b7t\u00e4rs", ".", "Stahl\u00b7helm\u00b7kom\u00b7mis", ".", "Rich\u00b7ter", ".", "Po\u00b7li\u00b7zei", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NE", "$.", "NN", "$.", "NN", "$."], "meter": "+-+----+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Eine himmlische Ruhe und Gewi\u00dfheit ist in ihnen.", "tokens": ["Ei\u00b7ne", "himm\u00b7li\u00b7sche", "Ru\u00b7he", "und", "Ge\u00b7wi\u00df\u00b7heit", "ist", "in", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Die Revolution ist endg\u00fcltig vorbei.", "tokens": ["Die", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "ist", "end\u00b7g\u00fcl\u00b7tig", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Aber im ganzen Lande \u2013 das h\u00e4tte ich beinah vergessen \u2013", "tokens": ["A\u00b7ber", "im", "gan\u00b7zen", "Lan\u00b7de", "\u2013", "das", "h\u00e4t\u00b7te", "ich", "bei\u00b7nah", "ver\u00b7ges\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$(", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "klappen sich auf die gewaltigsten Schnurrbartfressen:", "tokens": ["klap\u00b7pen", "sich", "auf", "die", "ge\u00b7wal\u00b7tigs\u00b7ten", "Schnurr\u00b7bart\u00b7fres\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.3": {"text": "\u00bbein freies Deutschland! Anschlu\u00df an \u00d6sterreich!", "tokens": ["\u00bb", "ein", "frei\u00b7es", "Deutschland", "!", "An\u00b7schlu\u00df", "an", "\u00d6s\u00b7ter\u00b7reich", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "NN", "APPR", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Frei von dem welschen Joch! Frei wolln wir sein! Aber gleich \u2013!\u00ab", "tokens": ["Frei", "von", "dem", "wel\u00b7schen", "Joch", "!", "Frei", "wolln", "wir", "sein", "!", "A\u00b7ber", "gleich", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "NN", "VMFIN", "PPER", "VAINF", "$.", "KON", "ADV", "$(", "$.", "$("], "meter": "+--+-++-+-+-+", "measure": "iambic.septa.invert"}, "line.5": {"text": "Innerlich stramm stehn. Versklavt von tausend Gewalten.", "tokens": ["In\u00b7ner\u00b7lich", "stramm", "stehn", ".", "Ver\u00b7sklavt", "von", "tau\u00b7send", "Ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$.", "NN", "APPR", "CARD", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Im \u00fcbrigen: \u00bbWeg mit Wersalch!\u00ab", "tokens":