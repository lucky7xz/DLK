{"textgrid.poem.60248": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "An Ihro K\u00f6nigliche Hoheit", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Prinze\u00dfin! die so lieblich bl\u00fchet,", "tokens": ["Prin\u00b7ze\u00b7\u00dfin", "!", "die", "so", "lieb\u00b7lich", "bl\u00fc\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "So freundlich ist wie sch\u00f6ner May \u2013", "tokens": ["So", "freund\u00b7lich", "ist", "wie", "sch\u00f6\u00b7ner", "May", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "KOKOM", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des n\u00e4chsten Jahres Fr\u00fchling siehet", "tokens": ["Des", "n\u00e4chs\u00b7ten", "Jah\u00b7res", "Fr\u00fch\u00b7ling", "sie\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich wandeln, wenn der Morgen neu", "tokens": ["Dich", "wan\u00b7deln", ",", "wenn", "der", "Mor\u00b7gen", "neu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Tannenhaynes S\u00e4nger weckte,", "tokens": ["Des", "Tan\u00b7nen\u00b7hay\u00b7nes", "S\u00e4n\u00b7ger", "weck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wie mit Diamanten-Glanz", "tokens": ["Und", "wie", "mit", "Di\u00b7a\u00b7man\u00b7ten\u00b7Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Tau das Ufergras bedeckte,", "tokens": ["Der", "Tau", "das", "U\u00b7fer\u00b7gras", "be\u00b7deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da wandelst Du im Blumenkranz;", "tokens": ["Da", "wan\u00b7delst", "Du", "im", "Blu\u00b7men\u00b7kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und jede Dryas wird sich neigen,", "tokens": ["Und", "je\u00b7de", "Dryas", "wird", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und jedes Wiesenbl\u00fcmchen Dir", "tokens": ["Und", "je\u00b7des", "Wie\u00b7sen\u00b7bl\u00fcm\u00b7chen", "Dir"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Dem\u00fcthig sich zu F\u00fc\u00dfen beugen \u2013", "tokens": ["De\u00b7m\u00fct\u00b7hig", "sich", "zu", "F\u00fc\u00b7\u00dfen", "beu\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und: \u00bbFlora, Flora wandelt hier!\u00ab", "tokens": ["Und", ":", "\u00bb", "Flo\u00b7ra", ",", "Flo\u00b7ra", "wan\u00b7delt", "hier", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$.", "$(", "NN", "$,", "NN", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wird Nachtigall und Lerche singen.", "tokens": ["Wird", "Nach\u00b7ti\u00b7gall", "und", "Ler\u00b7che", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Wandersmann verwundert sich", "tokens": ["Der", "Wan\u00b7ders\u00b7mann", "ver\u00b7wun\u00b7dert", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Bei allen diesen Wunderdingen;", "tokens": ["Bei", "al\u00b7len", "die\u00b7sen", "Wun\u00b7der\u00b7din\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Horcht die Ges\u00e4nge, siehet Dich,", "tokens": ["Horcht", "die", "Ge\u00b7s\u00e4n\u00b7ge", ",", "sie\u00b7het", "Dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Und sieht das Landhaus k\u00f6niglich", "tokens": ["Und", "sieht", "das", "Land\u00b7haus", "k\u00f6\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Emporgestiegen in dem Hayne", "tokens": ["Em\u00b7por\u00b7ge\u00b7stie\u00b7gen", "in", "dem", "Hay\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Auf Ferdinands Befehl so sch\u00f6n!", "tokens": ["Auf", "Fer\u00b7di\u00b7nands", "Be\u00b7fehl", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "An Seinem Arme sieht Er Deine", "tokens": ["An", "Sei\u00b7nem", "Ar\u00b7me", "sieht", "Er", "Dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Durchlauchte Theure Mutter gehn,", "tokens": ["Durch\u00b7lauch\u00b7te", "Theu\u00b7re", "Mut\u00b7ter", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und w\u00fcnschet Heil dem Hohen Paare,", "tokens": ["Und", "w\u00fcn\u00b7schet", "Heil", "dem", "Ho\u00b7hen", "Paa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Dem Du gegeben worden bist", "tokens": ["Dem", "Du", "ge\u00b7ge\u00b7ben", "wor\u00b7den", "bist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVPP", "VAPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Von einer die durch Jahre", "tokens": ["Von", "ei\u00b7ner", "die", "durch", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Nicht kalt geworden ist.", "tokens": ["Nicht", "kalt", "ge\u00b7wor\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aber wehe den Dryaden", "tokens": ["A\u00b7ber", "we\u00b7he", "den", "Dry\u00b7a\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Friedrichsfelder Flur,", "tokens": ["Auf", "der", "Fried\u00b7richs\u00b7fel\u00b7der", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die umsonst Euch zu sich laden,", "tokens": ["Die", "um\u00b7sonst", "Euch", "zu", "sich", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! sie leben k\u00fcnftig nur", "tokens": ["Ach", "!", "sie", "le\u00b7ben", "k\u00fcnf\u00b7tig", "nur"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ADJD", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre Traurigkeit zu fl\u00fcstern.", "tokens": ["Ih\u00b7re", "Trau\u00b7rig\u00b7keit", "zu", "fl\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer sie sieht, und sonst sie sah,", "tokens": ["Wer", "sie", "sieht", ",", "und", "sonst", "sie", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KON", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Siehts an ihren Reizverd\u00fcstern:", "tokens": ["Siehts", "an", "ih\u00b7ren", "Reiz\u00b7ver\u00b7d\u00fcs\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ferdinand sey nicht mehr da!", "tokens": ["Fer\u00b7di\u00b7nand", "sey", "nicht", "mehr", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Stolz wird Ihn das Spreegestade", "tokens": ["Stolz", "wird", "Ihn", "das", "Spree\u00b7ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In der Sommerwohnung sehn;", "tokens": ["In", "der", "Som\u00b7mer\u00b7woh\u00b7nung", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und der Schwan wird Dich um Gnade", "tokens": ["Und", "der", "Schwan", "wird", "Dich", "um", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "APPR", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.12": {"text": "Deines holden Blickes flehn \u2013", "tokens": ["Dei\u00b7nes", "hol\u00b7den", "Bli\u00b7ckes", "flehn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Prinze\u00dfin! die so lieblich bl\u00fchet,", "tokens": ["Prin\u00b7ze\u00b7\u00dfin", "!", "die", "so", "lieb\u00b7lich", "bl\u00fc\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "So freundlich ist wie sch\u00f6ner May