{"textgrid.poem.58232": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "270. Arznei wider den Vorwitz", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Menschen Auge sucht und liebt bald dies, bald jenes,", "tokens": ["Des", "Men\u00b7schen", "Au\u00b7ge", "sucht", "und", "liebt", "bald", "dies", ",", "bald", "je\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "KON", "VVFIN", "ADV", "PDS", "$,", "ADV", "PDS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald hier 'was Neues ist, bald schaut er da 'was Sch\u00f6nes,", "tokens": ["Bald", "hier", "'was", "Neu\u00b7es", "ist", ",", "bald", "schaut", "er", "da", "'was", "Sch\u00f6\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er spielt mit Puppenzeug, weil er nichts Bess'res hat.", "tokens": ["Er", "spielt", "mit", "Pup\u00b7pen\u00b7zeug", ",", "weil", "er", "nichts", "Bess'\u00b7res", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "PIS", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach Armer, h\u00e4ttest du Gott selbst in dir gefunden,", "tokens": ["Ach", "Ar\u00b7mer", ",", "h\u00e4t\u00b7test", "du", "Gott", "selbst", "in", "dir", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VAFIN", "PPER", "NN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wei\u00df, dies sch\u00f6nste Gut hielt' Herz und Sinn gebunden,", "tokens": ["Ich", "wei\u00df", ",", "dies", "sch\u00f6ns\u00b7te", "Gut", "hielt'", "Herz", "und", "Sinn", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein Vorwitz w\u00e4r' gestillt, dein l\u00fcsternd Auge satt!", "tokens": ["Dein", "Vor\u00b7witz", "w\u00e4r'", "ge\u00b7stillt", ",", "dein", "l\u00fcs\u00b7ternd", "Au\u00b7ge", "satt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPOSAT", "ADJD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Menschen Auge sucht und liebt bald dies, bald jenes,", "tokens": ["Des", "Men\u00b7schen", "Au\u00b7ge", "sucht", "und", "liebt", "bald", "dies", ",", "bald", "je\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "KON", "VVFIN", "ADV", "PDS", "$,", "ADV", "PDS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald hier 'was Neues ist, bald schaut er da 'was Sch\u00f6nes,", "tokens": ["Bald", "hier", "'was", "Neu\u00b7es", "ist", ",", "bald", "schaut", "er", "da", "'was", "Sch\u00f6\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "