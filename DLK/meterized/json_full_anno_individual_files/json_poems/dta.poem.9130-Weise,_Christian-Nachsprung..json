{"dta.poem.9130": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Nachsprung.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Lustig ihr m\u00e4dgen die hochzeit ist aus/", "tokens": ["Lus\u00b7tig", "ihr", "m\u00e4d\u00b7gen", "die", "hoch\u00b7zeit", "ist", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "ART", "NN", "VAFIN", "APPR", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Wandert mit euren bedienten hinau\u00df/", "tokens": ["Wan\u00b7dert", "mit", "eu\u00b7ren", "be\u00b7dien\u00b7ten", "hin\u00b7au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "APZR", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Lasset euch aber beyleibe nicht hertzen/", "tokens": ["Las\u00b7set", "euch", "a\u00b7ber", "bey\u00b7lei\u00b7be", "nicht", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gehet fein leise/ die mutter die wacht/", "tokens": ["Ge\u00b7het", "fein", "lei\u00b7se", "/", "die", "mut\u00b7ter", "die", "wacht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "$(", "ART", "NN", "ART", "VVFIN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Lasset die kerlen einandermahl schertzen/", "tokens": ["Las\u00b7set", "die", "ker\u00b7len", "ein\u00b7an\u00b7der\u00b7mahl", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "H\u00e4tten sie vormahls sich lustig gemacht.", "tokens": ["H\u00e4t\u00b7ten", "sie", "vor\u00b7mahls", "sich", "lus\u00b7tig", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "ADJD", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Gehet geschwinder ihr kindergen ihr/", "tokens": ["Ge\u00b7het", "ge\u00b7schwin\u00b7der", "ihr", "kin\u00b7der\u00b7gen", "ihr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "PPOSAT", "$("], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Leget euch nieder und schlaffet daf\u00fcr/", "tokens": ["Le\u00b7get", "euch", "nie\u00b7der", "und", "schlaf\u00b7fet", "da\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PAV", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Sehet die armen verliebeten schafe/", "tokens": ["Se\u00b7het", "die", "ar\u00b7men", "ver\u00b7lie\u00b7be\u00b7ten", "scha\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "VVFIN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Sind sie nicht truncken/ sie stehen ja kaum/", "tokens": ["Sind", "sie", "nicht", "trun\u00b7cken", "/", "sie", "ste\u00b7hen", "ja", "kaum", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$(", "PPER", "VVFIN", "ADV", "ADV", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Springet inzwischen und tantzet im schlafe", "tokens": ["Sprin\u00b7get", "in\u00b7zwi\u00b7schen", "und", "tant\u00b7zet", "im", "schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "APPRART", "VVFIN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Morgen erzehlet den lustigen traum.", "tokens": ["Mor\u00b7gen", "er\u00b7zeh\u00b7let", "den", "lus\u00b7ti\u00b7gen", "traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Gehet und leget euch immer zur ruh/", "tokens": ["Ge\u00b7het", "und", "le\u00b7get", "euch", "im\u00b7mer", "zur", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rt ihr noch lange den st\u00e4ndigen zu?", "tokens": ["H\u00f6rt", "ihr", "noch", "lan\u00b7ge", "den", "st\u00e4n\u00b7di\u00b7gen", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Sehet die mutter die legt sich ans fenster", "tokens": ["Se\u00b7het", "die", "mut\u00b7ter", "die", "legt", "sich", "ans", "fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Nehmet euch besser im finstern in acht:", "tokens": ["Neh\u00b7met", "euch", "bes\u00b7ser", "im", "fins\u00b7tern", "in", "acht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "VVFIN", "APPR", "CARD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "W\u00fcnschet ihr niedlichen gassengespenster", "tokens": ["W\u00fcn\u00b7schet", "ihr", "nied\u00b7li\u00b7chen", "gas\u00b7sen\u00b7ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Allerseits eine geruhige nacht.", "tokens": ["Al\u00b7ler\u00b7seits", "ei\u00b7ne", "ge\u00b7ru\u00b7hi\u00b7ge", "nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}