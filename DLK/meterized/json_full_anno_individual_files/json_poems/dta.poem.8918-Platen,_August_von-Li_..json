{"dta.poem.8918": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Li .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Gebeut mir stets, da\u00df ich das H\u00f6chste preise:", "tokens": ["Ge\u00b7beut", "mir", "stets", ",", "da\u00df", "ich", "das", "H\u00f6chs\u00b7te", "prei\u00b7se", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "D'rum r\u00fchmt' ich K\u00fcnstler, F\u00fcrsten, Frau'n und Weise,", "tokens": ["D'\u00b7rum", "r\u00fchmt'", "ich", "K\u00fcnst\u00b7ler", ",", "F\u00fcrs\u00b7ten", ",", "Frau'n", "und", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Dem Zuge folgend eines gro\u00dfen Hanges.", "tokens": ["Dem", "Zu\u00b7ge", "fol\u00b7gend", "ei\u00b7nes", "gro\u00b7\u00dfen", "Han\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dich nenn' ich nun die Seele dieses Dranges,", "tokens": ["Dich", "nenn'", "ich", "nun", "die", "See\u00b7le", "die\u00b7ses", "Dran\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den sonn'gen Gipfel meiner Lebensreise,", "tokens": ["Den", "sonn'\u00b7gen", "Gip\u00b7fel", "mei\u00b7ner", "Le\u00b7bens\u00b7rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Mittelpunkt, um den ich lobend kreise,", "tokens": ["Den", "Mit\u00b7tel\u00b7punkt", ",", "um", "den", "ich", "lo\u00b7bend", "krei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bestrickt vom Schwindel des Planetenganges.", "tokens": ["Be\u00b7strickt", "vom", "Schwin\u00b7del", "des", "Pla\u00b7ne\u00b7ten\u00b7gan\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch wenn vor Liebe deine Worte beben,", "tokens": ["Doch", "wenn", "vor", "Lie\u00b7be", "dei\u00b7ne", "Wor\u00b7te", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O so verleihst du, Freund! mir mehr in diesen,", "tokens": ["O", "so", "ver\u00b7leihst", "du", ",", "Freund", "!", "mir", "mehr", "in", "die\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "$,", "NN", "$.", "PPER", "ADV", "APPR", "PDAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als meiner Kunst beschieden ist zu geben.", "tokens": ["Als", "mei\u00b7ner", "Kunst", "be\u00b7schie\u00b7den", "ist", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zwar hat auch dir die Welt sich hold erwiesen;", "tokens": ["Zwar", "hat", "auch", "dir", "die", "Welt", "sich", "hold", "er\u00b7wie\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ART", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn sch\u00f6ner stirbt ein Solcher, den im Leben", "tokens": ["Denn", "sch\u00f6\u00b7ner", "stirbt", "ein", "Sol\u00b7cher", ",", "den", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein unverg\u00e4nglicher Gesang gepriesen.", "tokens": ["Ein", "un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7cher", "Ge\u00b7sang", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}