{"dta.poem.16013": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wievieles Wasser flie\u00dft in einem Strom zusammen,", "tokens": ["Wie\u00b7vie\u00b7les", "Was\u00b7ser", "flie\u00dft", "in", "ei\u00b7nem", "Strom", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Holz wievielerlei geht auf in gleichen Flammen!", "tokens": ["Und", "Holz", "wie\u00b7vie\u00b7ler\u00b7lei", "geht", "auf", "in", "glei\u00b7chen", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer z\u00e4hlt die Geister, die in einem Geist verschwammen?", "tokens": ["Wer", "z\u00e4hlt", "die", "Geis\u00b7ter", ",", "die", "in", "ei\u00b7nem", "Geist", "ver\u00b7schwam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Riesenkindlein saugt sich gro\u00df an vielen Ammen.", "tokens": ["Das", "Rie\u00b7sen\u00b7kin\u00b7dlein", "saugt", "sich", "gro\u00df", "an", "vie\u00b7len", "Am\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aus welchem Welttheil die und jene Blumen stammen,", "tokens": ["Aus", "wel\u00b7chem", "Welt\u00b7theil", "die", "und", "je\u00b7ne", "Blu\u00b7men", "stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "KON", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einem Garten stehn sie alle sch\u00f6n beisammen.", "tokens": ["In", "ei\u00b7nem", "Gar\u00b7ten", "stehn", "sie", "al\u00b7le", "sch\u00f6n", "bei\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}