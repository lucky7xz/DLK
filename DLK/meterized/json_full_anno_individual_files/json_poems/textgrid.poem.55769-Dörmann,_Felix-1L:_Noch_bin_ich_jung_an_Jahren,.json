{"textgrid.poem.55769": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch bin ich jung an Jahren,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch bin ich jung an Jahren,", "tokens": ["Noch", "bin", "ich", "jung", "an", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch gl\u00e4nzt mein Scheitel braun,", "tokens": ["Noch", "gl\u00e4nzt", "mein", "Schei\u00b7tel", "braun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch w\u00e4rest Du im Klaren,", "tokens": ["Doch", "w\u00e4\u00b7rest", "Du", "im", "Kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was ich erlebt, erfahren,", "tokens": ["Was", "ich", "er\u00b7lebt", ",", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dir w\u00fcrde sicher grau'n.", "tokens": ["Dir", "w\u00fcr\u00b7de", "si\u00b7cher", "grau'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VAFIN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Du w\u00fcrdest nimmer meinen,", "tokens": ["Du", "w\u00fcr\u00b7dest", "nim\u00b7mer", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie heut' so herzlos k\u00fchl,", "tokens": ["Wie", "heut'", "so", "herz\u00b7los", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df alt ich wollte scheinen, \u2013", "tokens": ["Da\u00df", "alt", "ich", "woll\u00b7te", "schei\u00b7nen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VMFIN", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Du w\u00fcrdest mich beweinen,", "tokens": ["Du", "w\u00fcr\u00b7dest", "mich", "be\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nicht h\u00f6hnen mein Gef\u00fchl.", "tokens": ["Nicht", "h\u00f6h\u00b7nen", "mein", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Noch bin ich jung an Jahren,", "tokens": ["Noch", "bin", "ich", "jung", "an", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch gl\u00e4nzt mein Scheitel braun,", "tokens": ["Noch", "gl\u00e4nzt", "mein", "Schei\u00b7tel", "braun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch w\u00e4rest Du im Klaren,", "tokens": ["Doch", "w\u00e4\u00b7rest", "Du", "im", "Kla\u00b7ren", ","], "token_info": ["