"du", "den\u00b7kest", ",", "da\u00df", "im", "Raum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Bl\u00fcht um mich dein Liebestraum,", "tokens": ["Bl\u00fcht", "um", "mich", "dein", "Lie\u00b7best\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du denkest, da\u00df aufs neue", "tokens": ["Wenn", "du", "den\u00b7kest", ",", "da\u00df", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "APPRART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich durch dich der Welt mich freue;", "tokens": ["Ich", "durch", "dich", "der", "Welt", "mich", "freu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "O so wirst du auch dich scheun,", "tokens": ["O", "so", "wirst", "du", "auch", "dich", "scheun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Anders als dich mein zu freun;", "tokens": ["An\u00b7ders", "als", "dich", "mein", "zu", "freun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heiter unter Bl\u00fctenb\u00e4umen", "tokens": ["Hei\u00b7ter", "un\u00b7ter", "Bl\u00fc\u00b7ten\u00