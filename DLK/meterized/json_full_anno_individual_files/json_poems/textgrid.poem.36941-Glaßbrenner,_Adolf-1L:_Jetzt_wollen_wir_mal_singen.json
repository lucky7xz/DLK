{"textgrid.poem.36941": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jetzt wollen wir mal singen", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt wollen wir mal singen", "tokens": ["Jetzt", "wol\u00b7len", "wir", "mal", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das nagelneue Lied,", "tokens": ["Das", "na\u00b7gel\u00b7neu\u00b7e", "Lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von einem schmucken Junker, \u2013", "tokens": ["Von", "ei\u00b7nem", "schmu\u00b7cken", "Jun\u00b7ker", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ei, ei, vom schmucken Junker!", "tokens": ["Ei", ",", "ei", ",", "vom", "schmu\u00b7cken", "Jun\u00b7ker", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der sich in Zwei verliebt.", "tokens": ["Der", "sich", "in", "Zwei", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "CARD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Eine war die Freiheit,", "tokens": ["Die", "Ei\u00b7ne", "war", "die", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Zweit' die H\u00f6ferei;", "tokens": ["Die", "Zweit'", "die", "H\u00f6\u00b7fe\u00b7rei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Freiheit lie\u00df er fahren, \u2013", "tokens": ["Die", "Frei\u00b7heit", "lie\u00df", "er", "fah\u00b7ren", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ei, ei, die lie\u00df er fahren!", "tokens": ["Ei", ",", "ei", ",", "die", "lie\u00df", "er", "fah\u00b7ren", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "PRELS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und schwur der H\u00f6ferei.", "tokens": ["Und", "schwur", "der", "H\u00f6\u00b7fe\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die H\u00f6ferei fr\u00e4gt leise:", "tokens": ["Die", "H\u00f6\u00b7fe\u00b7rei", "fr\u00e4gt", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Warst du und bleibst mir treu?", "tokens": ["Warst", "du", "und", "bleibst", "mir", "treu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbder Schwarze soll mich holen, \u2013", "tokens": ["\u00bb", "der", "Schwar\u00b7ze", "soll", "mich", "ho\u00b7len", ",", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PRF", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ei, ei, der soll ihn holen!", "tokens": ["Ei", ",", "ei", ",", "der", "soll", "ihn", "ho\u00b7len", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "PRELS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich war und bleib' dir treu!\u00ab", "tokens": ["Ich", "war", "und", "bleib'", "dir", "treu", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jetzt kriegt er Orden, Degen", "tokens": ["Jetzt", "kriegt", "er", "Or\u00b7den", ",", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und gl\u00e4nzendes Gewand;", "tokens": ["Und", "gl\u00e4n\u00b7zen\u00b7des", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es dauert kaum drei Tage, \u2013", "tokens": ["Es", "dau\u00b7ert", "kaum", "drei", "Ta\u00b7ge", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ei, ei, kaum dreier Tage!", "tokens": ["Ei", ",", "ei", ",", "kaum", "drei\u00b7er", "Ta\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da reicht sie ihm die Hand.", "tokens": ["Da", "reicht", "sie", "ihm", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch an dem Hochzeitsfeste,", "tokens": ["Doch", "an", "dem", "Hoch\u00b7zeits\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Just bei der Tafellust,", "tokens": ["Just", "bei", "der", "Ta\u00b7fel\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam der schwarze Satan, \u2013", "tokens": ["Da", "kam", "der", "schwar\u00b7ze", "Sa\u00b7tan", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Ei, ei, der schwarze Satan!", "tokens": ["Ei", ",", "ei", ",", "der", "schwar\u00b7ze", "Sa\u00b7tan", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und packt ihn bei der Brust.", "tokens": ["Und", "packt", "ihn", "bei", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbden Eid hast du gebrochen,", "tokens": ["\u00bb", "den", "Eid", "hast", "du", "ge\u00b7bro\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verlassen deine Braut!", "tokens": ["Ver\u00b7las\u00b7sen", "dei\u00b7ne", "Braut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Freiheit bist verlobet, \u2013", "tokens": ["Der", "Frei\u00b7heit", "bist", "ver\u00b7lo\u00b7bet", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ei, ei, der bist verlobet!", "tokens": ["Ei", ",", "ei", ",", "der", "bist", "ver\u00b7lo\u00b7bet", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der H\u00f6ferei getraut.\u00ab", "tokens": ["Der", "H\u00f6\u00b7fe\u00b7rei", "ge\u00b7traut", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So nahm er ihn beim Kragen", "tokens": ["So", "nahm", "er", "ihn", "beim", "Kra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und fuhr mit ihm hinaus;", "tokens": ["Und", "fuhr", "mit", "ihm", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wittwe aber lachte, \u2013", "tokens": ["Die", "Witt\u00b7we", "a\u00b7ber", "lach\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ei, ei, die Wittwe lachte!", "tokens": ["Ei", ",", "ei", ",", "die", "Witt\u00b7we", "lach\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die stummen G\u00e4ste aus.", "tokens": ["Die", "stum\u00b7men", "G\u00e4s\u00b7te", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und ist mein Mann zur H\u00f6llen,", "tokens": ["Und", "ist", "mein", "Mann", "zur", "H\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab' ich doch Gold und Pracht!", "tokens": ["Hab'", "ich", "doch", "Gold", "und", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mehr als hundert Buhlen, \u2013", "tokens": ["Und", "mehr", "als", "hun\u00b7dert", "Buh\u00b7len", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "KOKOM", "CARD", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ei, ei, als hundert Buhlen!", "tokens": ["Ei", ",", "ei", ",", "als", "hun\u00b7dert", "Buh\u00b7len", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "KOUS", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bei Tage und bei Nacht!", "tokens": ["Bei", "Ta\u00b7ge", "und", "bei", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Jetzt wollen wir mal singen", "tokens": ["Jetzt", "wol\u00b7len", "wir", "mal", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das nagelneue Lied,", "tokens": ["Das", "na\u00b7gel\u00b7neu\u00b7e", "Lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von einem schmucken Junker, \u2013", "tokens": ["Von", "ei\u00b7nem", "schmu\u00b7cken", "Jun\u00b7ker", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ei, ei, vom schmucken Junker!", "tokens": ["Ei", ",", "ei", ",", "vom", "schmu\u00b7cken", "Jun\u00b7ker", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der sich in Zwei verliebt.", "tokens": ["Der", "sich", "in", "Zwei", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "CARD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Eine war die Freiheit,", "tokens": ["Die", "Ei\u00b7ne", "war", "die", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Zweit' die H\u00f6fer