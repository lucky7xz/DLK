{"textgrid.poem.67197": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie selig ists, zu sehn, wenn Stunden wieder tagen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie selig ists, zu sehn, wenn Stunden wieder tagen,", "tokens": ["Wie", "se\u00b7lig", "ists", ",", "zu", "sehn", ",", "wenn", "Stun\u00b7den", "wie\u00b7der", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF", "$,", "KOUS", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sich vergn\u00fcgt der Mensch umsieht in den Gefilden,", "tokens": ["Wo", "sich", "ver\u00b7gn\u00fcgt", "der", "Mensch", "um\u00b7sieht", "in", "den", "Ge\u00b7fil\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVFIN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn Menschen sich um das Befinden fragen,", "tokens": ["Wenn", "Men\u00b7schen", "sich", "um", "das", "Be\u00b7fin\u00b7den", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Menschen sich zum frohen Leben bilden.", "tokens": ["Wenn", "Men\u00b7schen", "sich", "zum", "fro\u00b7hen", "Le\u00b7ben", "bil\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie sich der Himmel w\u00f6lbt, und auseinander dehnet,", "tokens": ["Wie", "sich", "der", "Him\u00b7mel", "w\u00f6lbt", ",", "und", "aus\u00b7ein\u00b7an\u00b7der", "deh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$,", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist die Freude dann an Ebnen und im Freien,", "tokens": ["So", "ist", "die", "Freu\u00b7de", "dann", "an", "Eb\u00b7nen", "und", "im", "Frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn sich das Herz nach neuem Leben sehnet,", "tokens": ["Wenn", "sich", "das", "Herz", "nach", "neu\u00b7em", "Le\u00b7ben", "seh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die V\u00f6gel singen, zum Gesange schreien.", "tokens": ["Die", "V\u00f6\u00b7gel", "sin\u00b7gen", ",", "zum", "Ge\u00b7san\u00b7ge", "schrei\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Mensch, der oft sein Inneres gefraget,", "tokens": ["Der", "Mensch", ",", "der", "oft", "sein", "In\u00b7ne\u00b7res", "ge\u00b7fra\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Spricht von dem Leben dann, aus dem die Rede gehet,", "tokens": ["Spricht", "von", "dem", "Le\u00b7ben", "dann", ",", "aus", "dem", "die", "Re\u00b7de", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn nicht der Gram an einer Seele naget,", "tokens": ["Wenn", "nicht", "der", "Gram", "an", "ei\u00b7ner", "See\u00b7le", "na\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und froh der Mann vor seinen G\u00fctern stehet.", "tokens": ["Und", "froh", "der", "Mann", "vor", "sei\u00b7nen", "G\u00fc\u00b7tern", "ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn eine Wohnung prangt, in hoher Luft gebauet,", "tokens": ["Wenn", "ei\u00b7ne", "Woh\u00b7nung", "prangt", ",", "in", "ho\u00b7her", "Luft", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hat der Mensch das Feld ger\u00e4umiger und Wege", "tokens": ["So", "hat", "der", "Mensch", "das", "Feld", "ge\u00b7r\u00e4u\u00b7mi\u00b7ger", "und", "We\u00b7ge"], "token_info": ["word", "word"