{"textgrid.poem.39082": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Wo sind die Lerchen hingeflogen", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo sind die Lerchen hingeflogen,", "tokens": ["Wo", "sind", "die", "Ler\u00b7chen", "hin\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die sonst den jungen Tag begr\u00fc\u00dft?", "tokens": ["die", "sonst", "den", "jun\u00b7gen", "Tag", "be\u00b7gr\u00fc\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hoch schweben sie am Himmelsbogen,", "tokens": ["Hoch", "schwe\u00b7ben", "sie", "am", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "von Morgenl\u00fcftchen wach gek\u00fc\u00dft:", "tokens": ["von", "Mor\u00b7gen\u00b7l\u00fcft\u00b7chen", "wach", "ge\u00b7k\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es flo\u00df ein Regen s\u00fc\u00dfer Lieder", "tokens": ["Es", "flo\u00df", "ein", "Re\u00b7gen", "s\u00fc\u00b7\u00dfer", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "herab auf die begl\u00fcckte Welt,", "tokens": ["her\u00b7ab", "auf", "die", "be\u00b7gl\u00fcck\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und alle Herzen t\u00f6nten wieder,", "tokens": ["und", "al\u00b7le", "Her\u00b7zen", "t\u00f6n\u00b7ten", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "und jedes f\u00fchlte sich ein Held.", "tokens": ["und", "je\u00b7des", "f\u00fchl\u00b7te", "sich", "ein", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jetzt schweigt die Flur! \u2013 Lautlose Schw\u00fcle", "tokens": ["Jetzt", "schweigt", "die", "Flur", "!", "\u2013", "Laut\u00b7lo\u00b7se", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "liegt ausgegossen weit und breit,", "tokens": ["liegt", "aus\u00b7ge\u00b7gos\u00b7sen", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVIZU", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Willk\u00fcr ruht auf seidnem Pf\u00fchle", "tokens": ["die", "Will\u00b7k\u00fcr", "ruht", "auf", "seid\u00b7nem", "Pf\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und freut sich ihrer Sicherheit:", "tokens": ["und", "freut", "sich", "ih\u00b7rer", "Si\u00b7cher\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als h\u00e4tte mit den freien Kehlen", "tokens": ["Als", "h\u00e4t\u00b7te", "mit", "den", "frei\u00b7en", "Keh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "sie auch die Herzen stumm gemacht!", "tokens": ["sie", "auch", "die", "Her\u00b7zen", "stumm", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als schwiegen zitternd alle Seelen,", "tokens": ["Als", "schwie\u00b7gen", "zit\u00b7ternd", "al\u00b7le", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "weil sie die Lippen \u00fcberwacht!", "tokens": ["weil", "sie", "die", "Lip\u00b7pen", "\u00fc\u00b7ber\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich aber sah die Wolken steigen,", "tokens": ["Ich", "a\u00b7ber", "sah", "die", "Wol\u00b7ken", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und Blitze zucken um den Turm \u2013", "tokens": ["und", "Blit\u00b7ze", "zu\u00b7cken", "um", "den", "Turm", "\u2013"], "token_info": ["word", "word", "word", "word", "wor