{"dta.poem.20387": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An den Hn. Land-Rath von Wulffen/  \n \u00fcber die gl\u00fcckliche verm\u00e4hlung mit dem  \n \u00e4ltesten Fr\u00e4ulein von Dan-  \n ckelmann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was vor in Griechenland am Paris ist geschehn/", "tokens": ["Was", "vor", "in", "Grie\u00b7chen\u00b7land", "am", "Pa\u00b7ris", "ist", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "APPR", "NE", "APPRART", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das k\u00f6nnen wir nunmehr bey deiner heyrath schauen;", "tokens": ["Das", "k\u00f6n\u00b7nen", "wir", "nun\u00b7mehr", "bey", "dei\u00b7ner", "hey\u00b7rath", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann Juno heisset dich nach reichen Nymphen sehn/", "tokens": ["Dann", "Ju\u00b7no", "heis\u00b7set", "dich", "nach", "rei\u00b7chen", "Nym\u00b7phen", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Venus locket dir mit hundert sch\u00f6nen frauen.", "tokens": ["Und", "Ve\u00b7nus", "lo\u00b7cket", "dir", "mit", "hun\u00b7dert", "sch\u00f6\u00b7nen", "frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "CARD", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Pallas aber tr\u00e4gt dir eine tochter an/", "tokens": ["Die", "Pal\u00b7las", "a\u00b7ber", "tr\u00e4gt", "dir", "ei\u00b7ne", "toch\u00b7ter", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die sie durch ihren sohn/ von Danckelmann/ gebohren.", "tokens": ["Die", "sie", "durch", "ih\u00b7ren", "sohn", "/", "von", "Dan\u00b7ckel\u00b7mann", "/", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "$(", "APPR", "NE", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was thust du werthester? du fliehst der ersten wahn/", "tokens": ["Was", "thust", "du", "wert\u00b7hes\u00b7ter", "?", "du", "fliehst", "der", "ers\u00b7ten", "wahn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und hast die dritte dir zur liebsten au\u00dferkohren.", "tokens": ["Und", "hast", "die", "drit\u00b7te", "dir", "zur", "liebs\u00b7ten", "au\u00b7\u00dfer\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach h\u00e4tte Paris so/ wie du anitzt/ gedacht/", "tokens": ["Ach", "h\u00e4t\u00b7te", "Pa\u00b7ris", "so", "/", "wie", "du", "a\u00b7nitzt", "/", "ge\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "VAFIN", "NE", "ADV", "$(", "PWAV", "PPER", "VVFIN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So w\u00e4re Troja nicht in feur und blut vergangen!", "tokens": ["So", "w\u00e4\u00b7re", "Tro\u00b7ja", "nicht", "in", "feur", "und", "blut", "ver\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PTKNEG", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dann dein Exempel lehrt/ da\u00df/ wer nach tugend tracht/", "tokens": ["Dann", "dein", "Ex\u00b7em\u00b7pel", "lehrt", "/", "da\u00df", "/", "wer", "nach", "tu\u00b7gend", "tracht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$(", "KOUS", "$(", "PWS", "APPR", "NN", "VVFIN", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Auch stand und sch\u00f6nheit kan mit seiner braut erlangen.", "tokens": ["Auch", "stand", "und", "sch\u00f6n\u00b7heit", "kan", "mit", "sei\u00b7ner", "braut", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "NN", "VMFIN", "APPR", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}