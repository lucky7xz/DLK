{"dta.poem.7460": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XvII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die uns als wilde, rohe Zweifler hassen,", "tokens": ["Die", "uns", "als", "wil\u00b7de", ",", "ro\u00b7he", "Zweif\u00b7ler", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "ADJA", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und drob manch derben Fluch uns schon gespendet,", "tokens": ["Und", "drob", "manch", "der\u00b7ben", "Fluch", "uns", "schon", "ge\u00b7spen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die frommen Leute \u2014 wie sind sie verblendet;", "tokens": ["Die", "from\u00b7men", "Leu\u00b7te", "wie", "sind", "sie", "ver\u00b7blen\u00b7det", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KOKOM", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Zieht erst der Fr\u00fchling jubelnd durch die Stra\u00dfen,", "tokens": ["Zieht", "erst", "der", "Fr\u00fch\u00b7ling", "ju\u00b7belnd", "durch", "die", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie wird des Herzens eitler Trotz gewendet,", "tokens": ["Wie", "wird", "des", "Her\u00b7zens", "eit\u00b7ler", "Trotz", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sich's mit jedem Strauch nach oben wendet", "tokens": ["Da\u00df", "sich's", "mit", "je\u00b7dem", "Strauch", "nach", "o\u00b7ben", "wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "NN", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein St\u00fcck des sch\u00f6nen Himmels zu erfassen!", "tokens": ["Ein", "St\u00fcck", "des", "sch\u00f6\u00b7nen", "Him\u00b7mels", "zu", "er\u00b7fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja, naht des Jahres F\u00fcrst mit seinem Hof,", "tokens": ["Ja", ",", "naht", "des", "Jah\u00b7res", "F\u00fcrst", "mit", "sei\u00b7nem", "Hof", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und jauchzt der Lenz auf Bergen und in Kl\u00fcften,", "tokens": ["Und", "jauchzt", "der", "Lenz", "auf", "Ber\u00b7gen", "und", "in", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo klagend kaum der Nebel niedertrof \u2014", "tokens": ["Wo", "kla\u00b7gend", "kaum", "der", "Ne\u00b7bel", "nie\u00b7der\u00b7trof"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Schlief' auch sein Glaube dann in Todesgr\u00fcften,", "tokens": ["Schlie\u00b7f'", "auch", "sein", "Glau\u00b7be", "dann", "in", "To\u00b7des\u00b7gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Der ew'ge Faust, der stolze Philosoph,", "tokens": ["Der", "ew'\u00b7ge", "Faust", ",", "der", "stol\u00b7ze", "Phi\u00b7lo\u00b7soph", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er hascht ihn wieder aus den blauen L\u00fcften.", "tokens": ["Er", "hascht", "ihn", "wie\u00b7der", "aus", "den", "blau\u00b7en", "L\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}