{"textgrid.poem.48438": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "9. Auf ihres Wirtes seinen Namenstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fehlte kaum ein Haar, so h\u00e4ttet ihr verreiset", "tokens": ["Es", "fehl\u00b7te", "kaum", "ein", "Haar", ",", "so", "h\u00e4t\u00b7tet", "ihr", "ver\u00b7rei\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den lieben sch\u00f6nen Tag, der euch auf Freude weiset", "tokens": ["den", "lie\u00b7ben", "sch\u00f6\u00b7nen", "Tag", ",", "der", "euch", "auf", "Freu\u00b7de", "wei\u00b7set"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und uns hei\u00dft fr\u00f6hlich sein. Doch zweifelt nicht daran,", "tokens": ["und", "uns", "hei\u00dft", "fr\u00f6h\u00b7lich", "sein", ".", "Doch", "zwei\u00b7felt", "nicht", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VAINF", "$.", "KON", "VVFIN", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wir h\u00e4tten euch den Dienst, die Ehr' auch angetan,", "tokens": ["wir", "h\u00e4t\u00b7ten", "euch", "den", "Dienst", ",", "die", "Ehr'", "auch", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wenn ihr schon w\u00e4ret nicht bei uns allhier gewesen!", "tokens": ["wenn", "ihr", "schon", "w\u00e4\u00b7ret", "nicht", "bei", "uns", "all\u00b7hier", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "PTKNEG", "APPR", "PPER", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So nehmt nun dieses Band! F\u00fcr euch ist di\u00df zu lesen,", "tokens": ["So", "nehmt", "nun", "die\u00b7ses", "Band", "!", "F\u00fcr", "euch", "ist", "di\u00df", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDAT", "NN", "$.", "APPR", "PPER", "VAFIN", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "f\u00fcr uns ist euer Trank und was ihr uns setzt f\u00fcr.", "tokens": ["f\u00fcr", "uns", "ist", "eu\u00b7er", "Trank", "und", "was", "ihr", "uns", "setzt", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "KON", "PWS", "PPER", "PPER", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ie besser tut ihr uns, ie wenger ihr uns Bier", "tokens": ["Ie", "bes\u00b7ser", "tut", "ihr", "uns", ",", "ie", "wen\u00b7ger", "ihr", "uns", "Bier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "VVFIN", "PPER", "PPER", "$,", "ADV", "PIS", "PPER", "PRF", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "auf heute, traget auf. Bei klarem Sonnenscheine", "tokens": ["auf", "heu\u00b7te", ",", "tra\u00b7get", "auf", ".", "Bei", "kla\u00b7rem", "Son\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "$,", "VVFIN", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "schmeckt rein und besser nichts, als was man trinkt vom Rheine", "tokens": ["schmeckt", "rein", "und", "bes\u00b7ser", "nichts", ",", "als", "was", "man", "trinkt", "vom", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "PIS", "$,", "KOUS", "PRELS", "PIS", "VVFIN", "APPRART", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und was uns Spanien schickt. Doch denket di\u00df hierbei:", "tokens": ["und", "was", "uns", "Spa\u00b7ni\u00b7en", "schickt", ".", "Doch", "den\u00b7ket", "di\u00df", "hier\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NE", "VVFIN", "$.", "KON", "VVFIN", "PDS", "ADV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "viel kosten darf es nicht! Das Fordern steht uns frei,", "tokens": ["viel", "kos\u00b7ten", "darf", "es", "nicht", "!", "Das", "For\u00b7dern", "steht", "uns", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "PTKNEG", "$.", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "das Haben habet ihr. L\u00f6st aber euch aufs Beste!", "tokens": ["das", "Ha\u00b7ben", "ha\u00b7bet", "ihr", ".", 