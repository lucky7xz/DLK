{"dta.poem.21619": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Abschied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Und als die schw\u00fcle Nacht den Schleier hob,", "tokens": ["Und", "als", "die", "schw\u00fc\u00b7le", "Nacht", "den", "Schlei\u00b7er", "hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da lie\u00df von mir die t\u00f6dtliche Maenade.", "tokens": ["Da", "lie\u00df", "von", "mir", "die", "t\u00f6dt\u00b7li\u00b7che", "Mae\u00b7na\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie sah mich an, ein Graunbild ohne Gnade;", "tokens": ["Sie", "sah", "mich", "an", ",", "ein", "Graun\u00b7bild", "oh\u00b7ne", "Gna\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Blut ward Eis, der Rausch der Lust zerstob.", "tokens": ["Mein", "Blut", "ward", "Eis", ",", "der", "Rausch", "der", "Lust", "zer\u00b7stob", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und in die Brust, d\u2019ran ihre Lippe lag,", "tokens": ["Und", "in", "die", "Brust", ",", "d'\u00b7ran", "ih\u00b7re", "Lip\u00b7pe", "lag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Eingrub der Schmerz die grimmen Pantherz\u00e4hne.", "tokens": ["Ein\u00b7grub", "der", "Schmerz", "die", "grim\u00b7men", "Pan\u00b7ther\u00b7z\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dumpf sank ich hin, das Auge ohne Thr\u00e4ne \u2014", "tokens": ["Dumpf", "sank", "ich", "hin", ",", "das", "Au\u00b7ge", "oh\u00b7ne", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In\u2019s Leben aber rief der graue Tag. \u2014", "tokens": ["In's", "Le\u00b7ben", "a\u00b7ber", "rief", "der", "grau\u00b7e", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}