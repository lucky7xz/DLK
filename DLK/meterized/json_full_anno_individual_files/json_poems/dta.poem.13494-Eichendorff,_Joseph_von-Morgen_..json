{"dta.poem.13494": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Morgen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Fliegt der erste Morgenstrahl", "tokens": ["Fliegt", "der", "ers\u00b7te", "Mor\u00b7gen\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch das stille Nebelthal,", "tokens": ["Durch", "das", "stil\u00b7le", "Ne\u00b7belt\u00b7hal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rauscht erwachend Wald und H\u00fcgel:", "tokens": ["Rauscht", "er\u00b7wa\u00b7chend", "Wald", "und", "H\u00fc\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer da fliegen kann, nimmt Fl\u00fcgel!", "tokens": ["Wer", "da", "flie\u00b7gen", "kann", ",", "nimmt", "Fl\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und sein H\u00fctlein in die Luft", "tokens": ["Und", "sein", "H\u00fct\u00b7lein", "in", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirft der Mensch vor Lust und ruft:", "tokens": ["Wirft", "der", "Mensch", "vor", "Lust", "und", "ruft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat Gesang doch auch noch Schwingen,", "tokens": ["Hat", "Ge\u00b7sang", "doch", "auch", "noch", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADV", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun so will ich fr\u00f6hlich singen!", "tokens": ["Nun", "so", "will", "ich", "fr\u00f6h\u00b7lich", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hinaus, o Mensch, weit in die Welt,", "tokens": ["Hin\u00b7aus", ",", "o", "Mensch", ",", "weit", "in", "die", "Welt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bangt dir das Herz in krankem Muth", "tokens": ["Bangt", "dir", "das", "Herz", "in", "kran\u00b7kem", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts ist so tr\u00fcb in Nacht gestellt,", "tokens": ["Nichts", "ist", "so", "tr\u00fcb", "in", "Nacht", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Morgen leicht macht's wieder gut.", "tokens": ["Der", "Mor\u00b7gen", "leicht", "macht's", "wie\u00b7der", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}