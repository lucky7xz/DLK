{"textgrid.poem.35529": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun wogt einher in m\u00e4cht'gem Geroll,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wogt einher in m\u00e4cht'gem Geroll,", "tokens": ["Nun", "wogt", "ein\u00b7her", "in", "m\u00e4cht'\u00b7gem", "Ge\u00b7roll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Den wir gehegt, den alten Groll,", "tokens": ["Den", "wir", "ge\u00b7hegt", ",", "den", "al\u00b7ten", "Groll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir lie\u00dfen ihn bei den Toten!", "tokens": ["Wir", "lie\u00b7\u00dfen", "ihn", "bei", "den", "To\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wir lie\u00dfen dahinten in W\u00fcstenei'n", "tokens": ["Wir", "lie\u00b7\u00dfen", "da\u00b7hin\u00b7ten", "in", "W\u00fcs\u00b7te\u00b7nei'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der Verzweiflung Dornengeranke \u2013", "tokens": ["Der", "Ver\u00b7zwei\u00b7flung", "Dor\u00b7nen\u00b7ge\u00b7ran\u00b7ke", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ins Leben f\u00fchrte uns siegreich ein", "tokens": ["Ins", "Le\u00b7ben", "f\u00fchr\u00b7te", "uns", "sieg\u00b7reich", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "ART"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die uns zerdr\u00fcckt, zerwalzt und zerst\u00fcckt,", "tokens": ["Die", "uns", "zer\u00b7dr\u00fcckt", ",", "zer\u00b7walzt", "und", "zer\u00b7st\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die D\u00e4monen verloren die Bannkraft!", "tokens": ["Die", "D\u00e4\u00b7mo\u00b7nen", "ver\u00b7lo\u00b7ren", "die", "Bann\u00b7kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wir haben uns zu der ", "tokens": ["Wir", "ha\u00b7ben", "uns", "zu", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und uns durchpocht nun die Mannkraft!", "tokens": ["Und", "uns", "durch\u00b7pocht", "nun", "die", "Mann\u00b7kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sansara dahinten nebelumkreist \u2013", "tokens": ["San\u00b7sa\u00b7ra", "da\u00b7hin\u00b7ten", "ne\u00b7bel\u00b7um\u00b7kreist", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$("], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Wir rangen uns zu den H\u00f6hen!", "tokens": ["Wir", "ran\u00b7gen", "uns", "zu", "den", "H\u00f6\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Lebendig ward uns der neue Geist,", "tokens": ["Le\u00b7ben\u00b7dig", "ward", "uns", "der", "neu\u00b7e", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-----+-+", "measure": "dactylic.init"}, "line.8": {"text": "Der da schafft in der Zeiten Wehen! ...", "tokens": ["Der", "da", "schafft", "in", "der", "Zei\u00b7ten", "We\u00b7hen", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Der da schafft in der Zeiten D\u00e4mmerungsschwall", "tokens": ["Der", "da", "schafft", "in", "der", "Zei\u00b7ten", "D\u00e4m\u00b7me\u00b7rungs\u00b7schwall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und ringt zur Fr\u00fchlingsentfaltung!", "tokens": ["Und", "ringt", "zur", "Fr\u00fch\u00b7lings\u00b7ent\u00b7fal\u00b7tung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ob seine Botschaft noch vielen Gelall \u2013", "tokens": ["Ob", "sei\u00b7ne", "Bot\u00b7schaft", "noch", "vie\u00b7len", "Ge\u00b7lall", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er w\u00e4chst und ersteht zur Gestaltung!", "tokens": ["Er", "w\u00e4chst", "und", "er\u00b7steht", "zur", "Ge\u00b7stal\u00b7tung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und immer t\u00f6nender wird sein Wort", "tokens": ["Und", "im\u00b7mer", "t\u00f6\u00b7nen\u00b7der", "wird", "sein", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und br\u00fcnstiger wirbt er um Herzen \u2013", "tokens": ["Und", "br\u00fcns\u00b7ti\u00b7ger", "wirbt", "er", "um", "Her\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Dann rei\u00dft er uns alle im Sturme fort", "tokens": ["Dann", "rei\u00dft", "er", "uns", "al\u00b7le", "im", "Stur\u00b7me", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PIS", "APPRART", "NN", "PTKVZ"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und begr\u00e4bt unsere letzten Schmerzen!", "tokens": ["Und", "be\u00b7gr\u00e4bt", "un\u00b7se\u00b7re", "letz\u00b7ten", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Es atmet sich k\u00f6stlich das Leben!", "tokens": ["Es", "at\u00b7met", "sich", "k\u00f6st\u00b7lich", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es hat sich ", "tokens": ["Es", "hat", "sich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und will sich dem Fr\u00fchling ergeben!", "tokens": ["Und", "will", "sich", "dem", "Fr\u00fch\u00b7ling", "er\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Vergangenheit tr\u00e4nenged\u00fcngte Saat", "tokens": ["Der", "Ver\u00b7gan\u00b7gen\u00b7heit", "tr\u00e4\u00b7nen\u00b7ge\u00b7d\u00fcng\u00b7te", "Saat"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Spro\u00dft auf zu fruchtschweren Halmen,", "tokens": ["Spro\u00dft", "auf", "zu", "frucht\u00b7schwe\u00b7ren", "Hal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und alles Wachsen eint sich zur Tat,", "tokens": ["Und", "al\u00b7les", "Wach\u00b7sen", "eint", "sich", "zur", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Drin sich die Zweifel zermalmen!", "tokens": ["Drin", "sich", "die", "Zwei\u00b7fel", "zer\u00b7mal\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Wir zweifelten \u2013 ja! Wir kosteten wohl", "tokens": ["Wir", "zwei\u00b7fel\u00b7ten", "\u2013", "ja", "!", "Wir", "kos\u00b7te\u00b7ten", "wohl"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PTKANT", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Zwiestreits bitterste Wunden!", "tokens": ["Des", "Zwies\u00b7treits", "bit\u00b7ters\u00b7te", "Wun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir opferten wohl Idol um Idol \u2013", "tokens": ["Wir", "op\u00b7fer\u00b7ten", "wohl", "I\u00b7dol", "um", "I\u00b7dol", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR", "NE", "$("], "meter": "-+---++-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und hatten doch nimmer gefunden,", "tokens": ["Und", "hat\u00b7ten", "doch", "nim\u00b7mer", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Was uns aus unserer Enge erl\u00f6st \u2013", "tokens": ["Was", "uns", "aus", "un\u00b7se\u00b7rer", "En\u00b7ge", "er\u00b7l\u00f6st", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Aus der unzul\u00e4nglichen Kleinheit,", "tokens": ["Aus", "der", "un\u00b7zu\u00b7l\u00e4ng\u00b7li\u00b7chen", "Klein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Drin sich der Dinge Wesen entbl\u00f6\u00dft", "tokens": ["Drin", "sich", "der", "Din\u00b7ge", "We\u00b7sen", "ent\u00b7bl\u00f6\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Zu grenzenloser Gemeinheit!", "tokens": ["Zu", "gren\u00b7zen\u00b7lo\u00b7ser", "Ge\u00b7mein\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wir lagen im Grunde und st\u00f6hnten auf \u2013", "tokens": ["Wir", "la\u00b7gen", "im", "Grun\u00b7de", "und", "st\u00f6hn\u00b7ten", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "APPR", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Kaum rafften wir uns zum Rufen!", "tokens": ["Kaum", "raff\u00b7ten", "wir", "uns", "zum", "Ru\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und dr\u00fcber stampfte des Alltags Lauf,", "tokens": ["Und", "dr\u00fc\u00b7ber", "stampf\u00b7te", "des", "All\u00b7tags", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zertrat uns mit erzenen Hufen!", "tokens": ["Zer\u00b7trat", "uns", "mit", "er\u00b7ze\u00b7nen", "Hu\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und keiner \u2013 und keiner, der uns die Hand", "tokens": ["Und", "kei\u00b7ner", "\u2013", "und", "kei\u00b7ner", ",", "der", "uns", "die", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "$(", "KON", "PIS", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Gereicht \u2013 die blutenden Wunden", "tokens": ["Ge\u00b7reicht", "\u2013", "die", "blu\u00b7ten\u00b7den", "Wun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bewahrt vor herzenzermergelndem Brand \u2013", "tokens": ["Be\u00b7wahrt", "vor", "her\u00b7zen\u00b7zer\u00b7mer\u00b7geln\u00b7dem", "Brand", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der uns zur ", "tokens": ["Der", "uns", "zur"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Da endlich \u2013 bei Gott! \u2013 wie vom Wahnsinn geweckt,", "tokens": ["Da", "end\u00b7lich", "\u2013", "bei", "Gott", "!", "\u2013", "wie", "vom", "Wahn\u00b7sinn", "ge\u00b7weckt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "APPR", "NN", "$.", "$(", "KOKOM", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Als ziemten uns Siegerlose! \u2013", "tokens": ["Als", "ziem\u00b7ten", "uns", "Sie\u00b7ger\u00b7lo\u00b7se", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Haben wir uns emporgereckt", "tokens": ["Ha\u00b7ben", "wir", "uns", "em\u00b7por\u00b7ge\u00b7reckt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Da unten umkroch uns ein giftig Gerank,", "tokens": ["Da", "un\u00b7ten", "um\u00b7kroch", "uns", "ein", "gif\u00b7tig", "Ge\u00b7rank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Das brannte sich ein wie Nesseln,", "tokens": ["Das", "brann\u00b7te", "sich", "ein", "wie", "Nes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Umd\u00fcnstete uns wie mit Pestgestank,", "tokens": ["Um\u00b7d\u00fcns\u00b7te\u00b7te", "uns", "wie", "mit", "Pest\u00b7ge\u00b7stank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Beschlich uns mit h\u00e4renen Fesseln! ...", "tokens": ["Be\u00b7schlich", "uns", "mit", "h\u00e4\u00b7re\u00b7nen", "Fes\u00b7seln", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PPER", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Nun warfen wir von uns das Dornenkleid", "tokens": ["Nun", "war\u00b7fen", "w