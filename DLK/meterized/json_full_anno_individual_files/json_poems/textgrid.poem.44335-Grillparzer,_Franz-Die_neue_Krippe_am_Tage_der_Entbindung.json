{"textgrid.poem.44335": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Die neue Krippe am Tage der Entbindung", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gloria in excelsis! \u00d6ffnet Aug und Ohren,", "tokens": ["Glo\u00b7ria", "in", "ex\u00b7cel\u00b7sis", "!", "\u00d6ff\u00b7net", "Aug", "und", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Vorzeit Wunder haben sich erneut,", "tokens": ["Der", "Vor\u00b7zeit", "Wun\u00b7der", "ha\u00b7ben", "sich", "er\u00b7neut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seht, eine Jungfrau hat ein Kind geboren,", "tokens": ["Seht", ",", "ei\u00b7ne", "Jung\u00b7frau", "hat", "ein", "Kind", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das laut genug \u2013 doch r\u00fcckw\u00e4rts \u2013 prophezeit!", "tokens": ["Das", "laut", "ge\u00b7nug", "\u2013", "doch", "r\u00fcck\u00b7w\u00e4rts", "\u2013", "pro\u00b7phe\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADJD", "ADV", "$(", "ADV", "ADV", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Jungfrau Eltern stehn mit gesenkten Ohren,", "tokens": ["Der", "Jung\u00b7frau", "El\u00b7tern", "stehn", "mit", "ge\u00b7senk\u00b7ten", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gleich den W\u00e4chtern der Krippe zu Christus Zeit,", "tokens": ["Gleich", "den", "W\u00e4ch\u00b7tern", "der", "Krip\u00b7pe", "zu", "Chris\u00b7tus", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der Br\u00e4utigam lehnt dort in Unschuld verloren", "tokens": ["Der", "Br\u00e4u\u00b7ti\u00b7gam", "lehnt", "dort", "in", "Un\u00b7schuld", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ADV", "APPR", "NN", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und ist auf ein Wunder zu schw\u00f6ren bereit.", "tokens": ["Und", "ist", "auf", "ein", "Wun\u00b7der", "zu", "schw\u00f6\u00b7ren", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Hart an dem N\u00e4hrvater und Quasi-Gatten", "tokens": ["Hart", "an", "dem", "N\u00e4hr\u00b7va\u00b7ter", "und", "Qua\u00b7si\u00b7Gat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Sitzt die selige Jungfrau in holdem Ermatten", "tokens": ["Sitzt", "die", "se\u00b7li\u00b7ge", "Jung\u00b7frau", "in", "hol\u00b7dem", "Er\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und denkt vielleicht eben \u2013 ans \u00dcberschatten!", "tokens": ["Und", "denkt", "viel\u00b7leicht", "e\u00b7ben", "\u2013", "ans", "\u00dc\u00b7bersc\u00b7hat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$(", "APPRART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Der Basen ungl\u00e4ubiges Volk st\u00fcrzt herein", "tokens": ["Der", "Ba\u00b7sen", "un\u00b7gl\u00e4u\u00b7bi\u00b7ges", "Volk", "st\u00fcrzt", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und kreischt sein Ave! \u2013 doch scheinet ihr Schrei'n", "tokens": ["Und", "kreischt", "sein", "A\u00b7ve", "!", "\u2013", "doch", "schei\u00b7net", "ihr", "Schrei'n"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "$(", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So Mutter als Frucht nicht zu benedein.", "tokens": ["So", "Mut\u00b7ter", "als", "Frucht", "nicht", "zu", "be\u00b7ne\u00b7dein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "PTKNEG", "PTKZU", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Gloria in excelsis! \u00d6ffnet Aug und Ohren,", "tokens": ["Glo\u00b7ria", "in", "ex\u00b7cel\u00b7sis", "!", "\u00d6ff\u00b7net", "Aug", "und", "Oh\u00b7ren", ","], "token_inf