{"textgrid.poem.39884": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Br\u00fcgg' um Thor und Mauer", "tokens": ["Zu", "Br\u00fcg\u00b7g'", "um", "Thor", "und", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da schweigt der Tag wie die stille Nacht,", "tokens": ["Da", "schweigt", "der", "Tag", "wie", "die", "stil\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da hat so finstre Trauer", "tokens": ["Da", "hat", "so", "finst\u00b7re", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der lange, blutige Krieg gebracht.", "tokens": ["Der", "lan\u00b7ge", ",", "blu\u00b7ti\u00b7ge", "Krieg", "ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Viel Ritter sind gesunken", "tokens": ["Viel", "Rit\u00b7ter", "sind", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In der Berge Schlucht, in den kalten Schnee,", "tokens": ["In", "der", "Ber\u00b7ge", "Schlucht", ",", "in", "den", "kal\u00b7ten", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Viel Rosse haben getrunken", "tokens": ["Viel", "Ros\u00b7se", "ha\u00b7ben", "ge\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von der k\u00fchlen Flut im tiefsten See.", "tokens": ["Von", "der", "k\u00fch\u00b7len", "Flut", "im", "tiefs\u00b7ten", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Es ritt durch Tag und N\u00e4chte", "tokens": ["Es", "ritt", "durch", "Tag", "und", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Herzog auf seiner ersten Flucht,", "tokens": ["Der", "Her\u00b7zog", "auf", "sei\u00b7ner", "ers\u00b7ten", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Dann hub er die wunde Rechte", "tokens": ["Dann", "hub", "er", "die", "wun\u00b7de", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und pr\u00fcft' auf's neue des Schwertes Wucht.", "tokens": ["Und", "pr\u00fcft'", "auf's", "neu\u00b7e", "des", "Schwer\u00b7tes", "Wucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und ist auf's neue gezogen", "tokens": ["Und", "ist", "auf's", "neu\u00b7e", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hinaus, zu r\u00e4chen des Heeres Schmach,", "tokens": ["Hin\u00b7aus", ",", "zu", "r\u00e4\u00b7chen", "des", "Hee\u00b7res", "Schmach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und kommt kein Bote geflogen?", "tokens": ["Und", "kommt", "kein", "Bo\u00b7te", "ge\u00b7flo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und sagt das Volk nicht die Kunde nach?", "tokens": ["Und", "sagt", "das", "Volk", "nicht", "die", "Kun\u00b7de", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und kehrt sich nicht die Trauer", "tokens": ["Und", "kehrt", "sich", "nicht", "die", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Siegesruf und Freudengelag?", "tokens": ["In", "Sie\u00b7ges\u00b7ruf", "und", "Freu\u00b7den\u00b7ge\u00b7lag", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der W\u00e4chter von der Mauer", "tokens": ["Der", "W\u00e4ch\u00b7ter", "von", "der", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er sp\u00e4het hinaus den langen Tag.", "tokens": ["Er", "sp\u00e4\u00b7het", "hin\u00b7aus", "den", "lan\u00b7gen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Da pocht zur Abendstunde", "tokens": ["Da", "pocht", "zur", "A\u00b7bends\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zuletzt an's Thor ein kranker Knecht;", "tokens": ["Zu\u00b7letzt", "an's", "Thor", "ein", "kran\u00b7ker", "Knecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schleicht sein Fu\u00df, von der Wunde,", "tokens": ["Es", "schleicht", "sein", "Fu\u00df", ",", "von", "der", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von der Flucht in's ferne Land geschw\u00e4cht.", "tokens": ["Von", "der", "Flucht", "in's", "fer\u00b7ne", "Land", "ge\u00b7schw\u00e4cht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Die Lumpen so ihn decken,", "tokens": ["Die", "Lum\u00b7pen", "so", "ihn", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Verk\u00fcnden Jammer und eitel Not,", "tokens": ["Ver\u00b7k\u00fcn\u00b7den", "Jam\u00b7mer", "und", "ei\u00b7tel", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Glieder l\u00e4hmt der Schrecken,", "tokens": ["Die", "Glie\u00b7der", "l\u00e4hmt", "der", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Antlitz wohnt der blasse Tod.", "tokens": ["Im", "Ant\u00b7litz", "wohnt", "der", "blas\u00b7se", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbo Bote, voll des Leides!\u00ab", "tokens": ["\u00bb", "o", "Bo\u00b7te", ",", "voll", "des", "Lei\u00b7des", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$,", "ADJD", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der W\u00e4chter von der Zinne schalt,", "tokens": ["Der", "W\u00e4ch\u00b7ter", "von", "der", "Zin\u00b7ne", "schalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdas Heer verga\u00df des Eides,", "tokens": ["\u00bb", "das", "Heer", "ver\u00b7ga\u00df", "des", "Ei\u00b7des", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fluch \u00fcber deine Jammergestalt!\u00ab", "tokens": ["Fluch", "\u00fc\u00b7ber", "dei\u00b7ne", "Jam\u00b7mer\u00b7ge\u00b7stalt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "\u00bbmein Amt war nicht zu schlagen,\u00ab", "tokens": ["\u00bb", "mein", "Amt", "war", "nicht", "zu", "schla\u00b7gen", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach drauf der Mann mit Herzeleid,", "tokens": ["Sprach", "drauf", "der", "Mann", "mit", "Her\u00b7ze\u00b7leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdoch kann ich zeugen und sagen,", "tokens": ["\u00bb", "doch", "kann", "ich", "zeu\u00b7gen", "und", "sa\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie liegen Alle, getreu dem Eid.\u00ab", "tokens": ["Sie", "lie\u00b7gen", "Al\u00b7le", ",", "ge\u00b7treu", "dem", "Eid", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "ADJD", "ART", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbso sprich, die vierzig Tausend", "tokens": ["\u00bb", "so", "sprich", ",", "die", "vier\u00b7zig", "Tau\u00b7send"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "ADJD", "$,", "PRELS", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie m\u00e4hte alle der wilde Sturm?\u00ab", "tokens": ["Sie", "m\u00e4h\u00b7te", "al\u00b7le", "der", "wil\u00b7de", "Sturm", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbja nieder warf er sie brausend,", "tokens": ["\u00bb", "ja", "nie\u00b7der", "warf", "er", "sie", "brau\u00b7send", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKVZ", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vor des Schweizers Speer und vor Nancy's Thurm.\u00ab", "tokens": ["Vor", "des", "Schwei\u00b7zers", "Speer", "und", "vor", "Nan\u00b7cy's", "Thurm", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "APPR", "NE", "NN", "$.", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbweh mir! so mu\u00dft' erbleichen", "tokens": ["\u00bb", "weh", "mir", "!", "so", "mu\u00dft'", "er\u00b7blei\u00b7chen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "$.", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Purpurrock des k\u00fchnsten Herrn?\u00ab", "tokens": ["Der", "