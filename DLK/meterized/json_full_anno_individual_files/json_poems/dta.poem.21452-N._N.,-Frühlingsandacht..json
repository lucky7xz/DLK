{"dta.poem.21452": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsandacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Des Fr\u00fchlings St\u00fcrme durchbrausen das Land \u2026", "tokens": ["Des", "Fr\u00fch\u00b7lings", "St\u00fcr\u00b7me", "durch\u00b7brau\u00b7sen", "das", "Land", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Meine Seele durchlodert der Sehnsucht Brand.", "tokens": ["Mei\u00b7ne", "See\u00b7le", "durch\u00b7lo\u00b7dert", "der", "Sehn\u00b7sucht", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Es treibt mich hinaus in der Einsamkeit Dom,", "tokens": ["Es", "treibt", "mich", "hin\u00b7aus", "in", "der", "Ein\u00b7sam\u00b7keit", "Dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APZR", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich k\u00fchle die Gluth in der Winde Strom.", "tokens": ["Ich", "k\u00fch\u00b7le", "die", "Gluth", "in", "der", "Win\u00b7de", "Strom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Versunken liegt die Erinnerung weit:", "tokens": ["Ver\u00b7sun\u00b7ken", "liegt", "die", "E\u00b7rin\u00b7ne\u00b7rung", "weit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mich gr\u00fc\u00dft die Sonne der Ewigkeit.", "tokens": ["Mich", "gr\u00fc\u00dft", "die", "Son\u00b7ne", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die B\u00e4che rauschen mir liebend zu,", "tokens": ["Die", "B\u00e4\u00b7che", "rau\u00b7schen", "mir", "lie\u00b7bend", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die V\u00f6gel singen: sei gl\u00fccklich auch du.", "tokens": ["Die", "V\u00f6\u00b7gel", "sin\u00b7gen", ":", "sei", "gl\u00fcck\u00b7lich", "auch", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "VAFIN", "ADJD", "ADV", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die B\u00e4ume neigen sich zum Willkomm\u2019,", "tokens": ["Die", "B\u00e4u\u00b7me", "nei\u00b7gen", "sich", "zum", "Will\u00b7komm'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.10": {"text": "S\u00fc\u00dfe Andacht erf\u00fcllt mich: Ich bin fromm.", "tokens": ["S\u00fc\u00b7\u00dfe", "An\u00b7dacht", "er\u00b7f\u00fcllt", "mich", ":", "Ich", "bin", "fromm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.11": {"text": "Tiefheiliger Schauer mich durchweht:", "tokens": ["Tief\u00b7hei\u00b7li\u00b7ger", "Schau\u00b7er", "mich", "durch\u00b7weht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Es weiht mich der Sch\u00f6pfung Majest\u00e4t.", "tokens": ["Es", "weiht", "mich", "der", "Sch\u00f6p\u00b7fung", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Ich sauge den Odem der Gottheit ein,", "tokens": ["Ich", "sau\u00b7ge", "den", "O\u00b7dem", "der", "Got\u00b7theit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Eins bin ich mit dem allewigen Sein.", "tokens": ["Eins", "bin", "ich", "mit", "dem", "al\u00b7le\u00b7wi\u00b7gen", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}