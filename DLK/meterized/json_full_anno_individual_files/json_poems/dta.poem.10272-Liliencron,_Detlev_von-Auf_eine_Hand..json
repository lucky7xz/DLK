{"dta.poem.10272": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine Hand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Die Hand, die zitternd in der meinen lag             ", "tokens": ["Die", "Hand", ",", "die", "zit\u00b7ternd", "in", "der", "mei\u00b7nen", "lag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Maientag, als weit die Amseln sangen,", "tokens": ["Am", "Mai\u00b7en\u00b7tag", ",", "als", "weit", "die", "Am\u00b7seln", "san\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die heimlich mir, ein unbewu\u00dft Verlangen,", "tokens": ["Die", "heim\u00b7lich", "mir", ",", "ein", "un\u00b7be\u00b7wu\u00dft", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Garten einst die frische Rose brach.", "tokens": ["Im", "Gar\u00b7ten", "einst", "die", "fri\u00b7sche", "Ro\u00b7se", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die mir, wenn staubbedeckt der hei\u00dfe Tag", "tokens": ["Die", "mir", ",", "wenn", "staub\u00b7be\u00b7deckt", "der", "hei\u00b7\u00dfe", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "$,", "KOUS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Mannespflicht und Arbeit war gegangen,", "tokens": ["In", "Man\u00b7nes\u00b7pflicht", "und", "Ar\u00b7beit", "war", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am wei\u00dfen Arme blitzen G\u00fcldenspangen,", "tokens": ["Am", "wei\u00b7\u00dfen", "Ar\u00b7me", "blit\u00b7zen", "G\u00fcl\u00b7den\u00b7span\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den k\u00fchlen Trunk kredenzte im Gemach.", "tokens": ["Den", "k\u00fch\u00b7len", "Trunk", "kre\u00b7denz\u00b7te", "im", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die liebestill manch Hinderni\u00df entr\u00fcckte", "tokens": ["Die", "lie\u00b7be\u00b7still", "manch", "Hin\u00b7der\u00b7ni\u00df", "ent\u00b7r\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und breite Sorgenstr\u00f6me \u00fcberbr\u00fcckte,", "tokens": ["Und", "brei\u00b7te", "Sor\u00b7gen\u00b7str\u00f6\u00b7me", "\u00fc\u00b7berb\u00b7r\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die treue Hand, die sch\u00f6ne, anmutreiche.", "tokens": ["Die", "treu\u00b7e", "Hand", ",", "die", "sch\u00f6\u00b7ne", ",", "an\u00b7mu\u00b7trei\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O la\u00df sie ruhen einst auf meinem Herzen,", "tokens": ["O", "la\u00df", "sie", "ru\u00b7hen", "einst", "auf", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich verlasse dieses Land der Schmerzen,", "tokens": ["Wenn", "ich", "ver\u00b7las\u00b7se", "die\u00b7ses", "Land", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich gesegnet bin, wenn ich erbleiche.", "tokens": ["Da\u00df", "ich", "ge\u00b7seg\u00b7net", "bin", ",", "wenn", "ich", "er\u00b7blei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}