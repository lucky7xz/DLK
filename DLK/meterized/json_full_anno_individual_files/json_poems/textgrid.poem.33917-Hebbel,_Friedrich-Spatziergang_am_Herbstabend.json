{"textgrid.poem.33917": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Spatziergang am Herbstabend", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich Abends einsam gehe", "tokens": ["Wenn", "ich", "A\u00b7bends", "ein\u00b7sam", "ge\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Bl\u00e4tter fallen sehe,", "tokens": ["Und", "die", "Bl\u00e4t\u00b7ter", "fal\u00b7len", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Finsternisse nieder wallen,", "tokens": ["Fins\u00b7ter\u00b7nis\u00b7se", "nie\u00b7der", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ferne, fromme Glocken hallen:", "tokens": ["Fer\u00b7ne", ",", "from\u00b7me", "Glo\u00b7cken", "hal\u00b7len", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, wie viele sanfte Bilder,", "tokens": ["Ach", ",", "wie", "vie\u00b7le", "sanf\u00b7te", "Bil\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer inniger und milder,", "tokens": ["Im\u00b7mer", "in\u00b7ni\u00b7ger", "und", "mil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Schatten l\u00e4ngst vergangner Zeiten,", "tokens": ["Schat\u00b7ten", "l\u00e4ngst", "ver\u00b7gang\u00b7ner", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seh' ich dann vor\u00fcber gleiten.", "tokens": ["Seh'", "ich", "dann", "vor\u00b7\u00fc\u00b7ber", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was ich in den fernsten Stunden,", "tokens": ["Was", "ich", "in", "den", "ferns\u00b7ten", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Oft nur halb bewu\u00dft, empfunden,", "tokens": ["Oft", "nur", "halb", "be\u00b7wu\u00dft", ",", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADJD", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00e4mmert auf in Seel' und Sinnen,", "tokens": ["D\u00e4m\u00b7mert", "auf", "in", "Seel'", "und", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich noch einmal zu umspinnen.", "tokens": ["Mich", "noch", "ein\u00b7mal", "zu", "um\u00b7spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "sta