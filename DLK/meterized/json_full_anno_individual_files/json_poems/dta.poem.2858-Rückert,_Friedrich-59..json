{"dta.poem.2858": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In was du bildend dich wirst ganzer Seele tauchen,", "tokens": ["In", "was", "du", "bil\u00b7dend", "dich", "wirst", "gan\u00b7zer", "See\u00b7le", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "PPER", "VAFIN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das kannst du fetzenweis am wenigsten verbrauchen.", "tokens": ["Das", "kannst", "du", "fet\u00b7zen\u00b7weis", "am", "we\u00b7nigs\u00b7ten", "ver\u00b7brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "APPRART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Was im Vor\u00fcbergehn den Geist ber\u00fchrt und streift,", "tokens": ["Was", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7gehn", "den", "Geist", "be\u00b7r\u00fchrt", "und", "streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ists wovon zum Schmuck er dis und das ergreift.", "tokens": ["Das", "ists", "wo\u00b7von", "zum", "Schmuck", "er", "dis", "und", "das", "er\u00b7greift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PWAV", "APPRART", "NN", "PPER", "PDS", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht wo du Einzelnes aufz\u00e4hlst das du gewannst,", "tokens": ["Nicht", "wo", "du", "Ein\u00b7zel\u00b7nes", "auf\u00b7z\u00e4hlst", "das", "du", "ge\u00b7wannst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWAV", "PPER", "PIS", "VVFIN", "ART", "PPER", "VVPP", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das meiste lernst du da, wo du's nicht z\u00e4hlen kannst.", "tokens": ["Das", "meis\u00b7te", "lernst", "du", "da", ",", "wo", "du's", "nicht", "z\u00e4h\u00b7len", "kannst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PIS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}