{"textgrid.poem.59806": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich Gl\u00fccklichster der Gl\u00fccklichen! Derweil die", "tokens": ["Ich", "Gl\u00fcck\u00b7lichs\u00b7ter", "der", "Gl\u00fcck\u00b7li\u00b7chen", "!", "Der\u00b7weil", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "$.", "KOUS", "ART"], "meter": "-++--+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Welt sich um sich selbst in Dummheit dreht", "tokens": ["Welt", "sich", "um", "sich", "selbst", "in", "Dumm\u00b7heit", "dreht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "PRF", "ADV", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und jeglicher auf seine Art dem Heil,", "tokens": ["Und", "jeg\u00b7li\u00b7cher", "auf", "sei\u00b7ne", "Art", "dem", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das offenbar liegt, aus dem Wege geht;", "tokens": ["Das", "of\u00b7fen\u00b7bar", "liegt", ",", "aus", "dem", "We\u00b7ge", "geht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Derweil der M\u00f6nch den eignen Leib kasteit", "tokens": ["Der\u00b7weil", "der", "M\u00f6nch", "den", "eig\u00b7nen", "Leib", "kas\u00b7teit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und w\u00e4hnt, da\u00df ihn der Himmel einst entsch\u00e4digt", "tokens": ["Und", "w\u00e4hnt", ",", "da\u00df", "ihn", "der", "Him\u00b7mel", "einst", "ent\u00b7sch\u00e4\u00b7digt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcr die auf Erden wundgeriebnen Knie \u2013", "tokens": ["F\u00fcr", "die", "auf", "Er\u00b7den", "wund\u00b7ge\u00b7rieb\u00b7nen", "Knie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Derweil der Pfaff vom Jenseits prophezeit,", "tokens": ["Der\u00b7weil", "der", "Pfaff", "vom", "Jen\u00b7seits", "pro\u00b7phe\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "APPRART", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "In frommer Wut den Leuten Dinge predigt,", "tokens": ["In", "from\u00b7mer", "Wut", "den", "Leu\u00b7ten", "Din\u00b7ge", "pre\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Von denen er so wenig wei\u00df wie sie:", "tokens": ["Von", "de\u00b7nen", "er", "so", "we\u00b7nig", "wei\u00df", "wie", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PIS", "VVFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Knie ic