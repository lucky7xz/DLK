{"textgrid.poem.52355": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "5. De Buren bi Regenweder", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbbrrr!\u00ab seggt de Schult Kampehl, \u00bbwat is't f\u00f6r Weder!\u00ab,", "tokens": ["\u00bb", "brrr", "!", "\u00ab", "seggt", "de", "Schult", "Kam\u00b7pehl", ",", "\u00bb", "wat", "is't", "f\u00f6r", "We\u00b7der", "!", "\u00ab", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "VVFIN", "NE", "NE", "NN", "$,", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "As hei herin tredd in den Kraug,", "tokens": ["As", "hei", "he\u00b7rin", "tredd", "in", "den", "Kraug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "APPR", "ART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "\u00bbdat is f\u00f6r'n Hund nich gaud genaug!", "tokens": ["\u00bb", "dat", "is", "f\u00f6r'n", "Hund", "nich", "gaud", "ge\u00b7naug", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Na, s\u00fcnd ji denn nu hir, ein jeder?\u00ab", "tokens": ["Na", ",", "s\u00fcnd", "ji", "denn", "nu", "hir", ",", "ein", "je\u00b7der", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "ART", "PIS", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbick gl\u00f6w binah. Blot Jochen \u00d6lbarg fehlt.\u00ab", "tokens": ["\u00bb", "ick", "gl\u00f6w", "bi\u00b7nah", ".", "Blot", "Jo\u00b7chen", "\u00d6l\u00b7barg", "fehlt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "$.", "NN", "NE", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbwo ", "tokens": ["\u00bb", "wo"], "token_info": ["punct", "word"], "pos": ["$(", "PWAV"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Dat is 'ne olle Nuss' sin Lewsdag west", "tokens": ["Dat", "is", "'", "ne", "ol\u00b7le", "Nuss", "'", "sin", "Lews\u00b7dag", "west"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM", "FM", "$(", "FM", "FM", "FM", "$(", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Un bliwwt ok so! Wo ", "tokens": ["Un", "bliwwt", "ok", "so", "!", "Wo"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "ADV", "ADV", "$.", "PWAV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Na, lat't em r\u00fcm nah 'n Deuwel lopen! \u2013", "tokens": ["Na", ",", "lat't", "em", "r\u00fcm", "nah", "'n", "Deu\u00b7wel", "lo\u00b7pen", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "FM.la", "FM.la", "FM.la", "ADJD", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "H\u00fcrt, Kinnings, ick heww jug tauhopen", "tokens": ["H\u00fcrt", ",", "Kin\u00b7nings", ",", "ick", "heww", "jug", "tau\u00b7ho\u00b7pen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mal kamen laten, dat w' uns mal beraden,", "tokens": ["Mal", "ka\u00b7men", "la\u00b7ten", ",", "dat", "w'", "uns", "mal", "be\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVFIN", "$,", "PAV", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Wat bi so'n Weder is tau dauhn.", "tokens": ["Wat", "bi", "so'n", "We\u00b7der", "is", "tau", "dauhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "De Regen will sick gor nich stau'n", "tokens": ["De", "Re\u00b7gen", "will", "sick", "gor", "nich", "stau'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VMFIN", "PRF", "NE", "PTKNEG", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Un deiht uns grugelichen Schaden.", "tokens": ["Un", "deiht", "uns", "gru\u00b7ge\u00b7li\u00b7chen", "Scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Wi m\u00f6t dortau wat dauhn inde\u00df.", "tokens": ["Wi", "m\u00f6t", "dor\u00b7tau", "wat", "dauhn", "in\u00b7de\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.16": {"text": "Uns' Rogg is rin, doch all uns' Weit", "tokens": ["Un\u00b7s'", "Rogg", "is", "rin", ",", "doch", "all", "un\u00b7s'", "Weit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADV", "PIAT", "PPOSAT", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.17": {"text": "Un Hawern noch in Hocken steiht,", "tokens": ["Un", "Ha\u00b7wern", "noch", "in", "Ho\u00b7cken", "steiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dat ward jo alltausam tau Me\u00df.\u00ab", "tokens": ["Dat", "ward", "jo", "all\u00b7tau\u00b7sam", "tau", "Me\u00df", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "\u00bbdor hest du recht!\u00ab seggt Jochen Br\u00fcmmer,", "tokens": ["\u00bb", "dor", "hest", "du", "recht", "!", "\u00ab", "seggt", "Jo\u00b7chen", "Br\u00fcm\u00b7mer", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJD", "$.", "$(", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "\u00bbdat ward tau Me\u00df, dat s\u00e4d ick \u00fcmmer.\u00ab", "tokens": ["\u00bb", "dat", "ward", "tau", "Me\u00df", ",", "dat", "s\u00e4d", "ick", "\u00fcm\u00b7mer", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "VAFIN", "CARD", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "\u00bbja\u00ab, seggt oll Bolt, \u00bbwenn't nu nich rinne k\u00fcmmt,", "tokens": ["\u00bb", "ja", "\u00ab", ",", "seggt", "oll", "Bolt", ",", "\u00bb", "wenn't", "nu", "nich", "rin\u00b7ne", "k\u00fcmmt", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "ADJD", "NE", "$,", "$(", "VVFIN", "ADV", "PTKNEG", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Denn ward't tau Me\u00df, dat is bestimmt.\u00ab", "tokens": ["Denn", "ward't", "tau", "Me\u00df", ",", "dat", "is", "be\u00b7stimmt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "$,", "FM.nl", "FM.nl", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "\u00bbde Sak\u00ab, seggt nu de Schult, \u00bblett mi nich rauhn,", "tokens": ["\u00bb", "de", "Sak", "\u00ab", ",", "seggt", "nu", "de", "Schult", ",", "\u00bb", "lett", "mi", "nich", "rauhn", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$(", "$,", "VVFIN", "ADV", "NE", "NE", "$,", "$(", "VVIMP", "NE", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Dor m\u00f6t wi doch wat gegen dauhn,", "tokens": ["Dor", "m\u00f6t", "wi", "doch", "wat", "ge\u00b7gen", "dauhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Dat regent jo in einen furt.", "tokens": ["Dat", "re\u00b7gent", "jo", "in", "ei\u00b7nen", "furt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.26": {"text": "Du, Vadder Vo\u00df, red du doch mal en Wurd!\u00ab", "tokens": ["Du", ",", "Vad\u00b7der", "Vo\u00df", ",", "red", "du", "doch", "mal", "en", "Wurd", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "NE", "NE", "$,", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "\u00bbje\u00ab, seggt oll Vo\u00df, \u00bbdat is so, as dat is!", "tokens": ["\u00bb", "je", "\u00ab", ",", "seggt", "oll", "Vo\u00df", ",", "\u00bb", "dat", "is", "so", ",", "as", "dat", "is", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "ADJD", "NE", "$,", "$(", "ART", "FM", "ADV", "$,", "FM", "FM", "FM", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.28": {"text": "Dat't regen deiht, dat is nu mal gewi\u00df.\u00ab", "tokens": ["Dat't", "re\u00b7gen", "deiht", ",", "dat", "is", "nu", "mal", "ge\u00b7wi\u00df", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "VVFIN", "$,", "FM.nl", "FM.nl", "ADV", "ADV", "ADV", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.29": {"text": "\u00bbja\u00ab, seggt oll Funk, \u00bbun regen deiht't.", "tokens": ["\u00bb", "ja", "\u00ab", ",", "seggt", "oll", "Funk", ",", "\u00bb", "un", "re\u00b7gen", "deiht'", "t."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "abbreviation"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "ADJD", "NN", "$,", "$(", "FM", "ADJA", "PPOSAT", "NN"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.30": {"text": "Wo dat noch dick an'n Hewen steiht!\u00ab", "tokens": ["Wo", "dat", "noch", "dick", "an'n", "He\u00b7wen", "steiht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "ADV", "ADJD", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "\u00bbje\u00ab, seggt de Schult, \u00bbwat makt wi denn dorbi?", "tokens": ["\u00bb", "je", "\u00ab", ",", "seggt", "de", "Schult", ",", "\u00bb", "wat", "makt", "wi", "denn", "dor\u00b7bi", "?"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "NE", "NE", "$,", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Du, Vadder Krull, red du mal fri.\u00ab", "tokens": ["Du", ",", "Vad\u00b7der", "Krull", ",", "red", "du", "mal", "fri", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "NE", "NE", "$,", "VVFIN", "PPER", "ADV", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "\u00bbwer weit't?\u00ab seggt Krull. \u2013 \u00bbWer weit't\u00ab, seggt Bolt.", "tokens": ["\u00bb", "wer", "weit't", "?", "\u00ab", "seggt", "Krull", ".", "\u2013", "\u00bb", "Wer", "weit't", "\u00ab", ",", "seggt", "Bolt", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "$.", "$(", "VVFIN", "NE", "$.", "$(", "$(", "PWS", "VVFIN", "$(", "$,", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "\u00bbun dorbi is't entfamten kolt", "tokens": ["\u00bb", "un", "dor\u00b7bi", "is't", "ent\u00b7fam\u00b7ten", "kolt"], "token_info": ["punct", "word", "word", "word", "word", "word"