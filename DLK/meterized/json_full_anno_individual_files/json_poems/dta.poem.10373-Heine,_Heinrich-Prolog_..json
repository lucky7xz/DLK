{"dta.poem.10373": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Prolog .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Mit hohlen, schneewei\u00dfen Wangen;", "tokens": ["Mit", "hoh\u00b7len", ",", "schnee\u00b7wei\u00b7\u00dfen", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er schwankte und schlenderte schlotternd herum,", "tokens": ["Er", "schwank\u00b7te", "und", "schlen\u00b7der\u00b7te", "schlot\u00b7ternd", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "In dumpfen Tr\u00e4umen befangen.", "tokens": ["In", "dum\u00b7pfen", "Tr\u00e4u\u00b7men", "be\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er war so h\u00f6lzern, so t\u00e4ppisch, so links,", "tokens": ["Er", "war", "so", "h\u00f6l\u00b7zern", ",", "so", "t\u00e4p\u00b7pisch", ",", "so", "links", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$,", "ADV", "ADJD", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Bl\u00fcmlein und M\u00e4gdlein, die kicherten rings,", "tokens": ["Die", "Bl\u00fcm\u00b7lein", "und", "M\u00e4gd\u00b7lein", ",", "die", "ki\u00b7cher\u00b7ten", "rings", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PRELS", "VVFIN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wenn er stolpernd vorbeigegangen.", "tokens": ["Wenn", "er", "stol\u00b7pernd", "vor\u00b7bei\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Oft sa\u00df er im finstersten Winkel zu Haus;", "tokens": ["Oft", "sa\u00df", "er", "im", "fins\u00b7ters\u00b7ten", "Win\u00b7kel", "zu", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er hatt' sich vor Menschen verkrochen.", "tokens": ["Er", "hatt'", "sich", "vor", "Men\u00b7schen", "ver\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da streckte er sehnend die Arme aus,", "tokens": ["Da", "streck\u00b7te", "er", "seh\u00b7nend", "die", "Ar\u00b7me", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Doch hat er kein W\u00f6rtlein gesprochen.", "tokens": ["Doch", "hat", "er", "kein", "W\u00f6rt\u00b7lein", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kam aber die Mitternachtstunde heran,", "tokens": ["Kam", "a\u00b7ber", "die", "Mit\u00b7ter\u00b7nachts\u00b7tun\u00b7de", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ein seltsames Singen und Klingen begann.", "tokens": ["Ein", "selt\u00b7sa\u00b7mes", "Sin\u00b7gen", "und", "Klin\u00b7gen", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "An die Th\u00fcre da h\u00f6rt er es pochen.", "tokens": ["An", "die", "Th\u00fc\u00b7re", "da", "h\u00f6rt", "er", "es", "po\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Da kommt seine Liebste geschlichen herein,", "tokens": ["Da", "kommt", "sei\u00b7ne", "Liebs\u00b7te", "ge\u00b7schli\u00b7chen", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "VVPP", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im rauschenden Wellenschaumkleide.", "tokens": ["Im", "rau\u00b7schen\u00b7den", "Wel\u00b7len\u00b7schaum\u00b7klei\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie bl\u00fcht und gl\u00fcht, wie ein R\u00f6selein,", "tokens": ["Sie", "bl\u00fcht", "und", "gl\u00fcht", ",", "wie", "ein", "R\u00f6\u00b7se\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr Schleier ist eitel Geschmeide.", "tokens": ["Ihr", "Schlei\u00b7er", "ist", "ei\u00b7tel", "Ge\u00b7schmei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Goldlocken umspielen die schlanke Gestalt,", "tokens": ["Gold\u00b7lo\u00b7cken", "um\u00b7spie\u00b7len", "die", "schlan\u00b7ke", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die Aeugelein gr\u00fc\u00dfen mit s\u00fc\u00dfer Gewalt \u2014", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "gr\u00fc\u00b7\u00dfen", "mit", "s\u00fc\u00b7\u00dfer", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "In die Arme sinken sich beide.", "tokens": ["In", "die", "Ar\u00b7me", "sin\u00b7ken", "sich", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "PIS", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Ritter umschlingt sie mit Liebesmacht,", "tokens": ["Der", "Rit\u00b7ter", "um\u00b7schlingt", "sie", "mit", "Lie\u00b7bes\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der H\u00f6lzerne steht jetzt in Feuer,", "tokens": ["Der", "H\u00f6l\u00b7zer\u00b7ne", "steht", "jetzt", "in", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Blasse err\u00f6thet, der Tr\u00e4umer erwacht,", "tokens": ["Der", "Blas\u00b7se", "er\u00b7r\u00f6t\u00b7het", ",", "der", "Tr\u00e4u\u00b7mer", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Bl\u00f6de wird freier und freier.", "tokens": ["Der", "Bl\u00f6\u00b7de", "wird", "frei\u00b7er", "und", "frei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sie aber, sie hat ihn gar schalkhaft geneckt,", "tokens": ["Sie", "a\u00b7ber", ",", "sie", "hat", "ihn", "gar", "schalk\u00b7haft", "ge\u00b7neckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie hat ihm ganz leise den Kopf bedeckt", "tokens": ["Sie", "hat", "ihm", "ganz", "lei\u00b7se", "den", "Kopf", "be\u00b7deckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "ART", "NN", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Mit dem wei\u00dfen, demantenen Schleier.", "tokens": ["Mit", "dem", "wei\u00b7\u00dfen", ",", "de\u00b7man\u00b7te\u00b7nen", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "In einen kristallenen Wasserpalast", "tokens": ["In", "ei\u00b7nen", "kris\u00b7tal\u00b7le\u00b7nen", "Was\u00b7ser\u00b7pa\u00b7last"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ist pl\u00f6tzlich gezaubert der Ritter.", "tokens": ["Ist", "pl\u00f6tz\u00b7lich", "ge\u00b7zau\u00b7bert", "der", "Rit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er staunt, und die Augen erblinden ihm fast,", "tokens": ["Er", "staunt", ",", "und", "die", "Au\u00b7gen", "er\u00b7blin\u00b7den", "ihm", "fast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Vor alle dem Glanz und Geflitter.", "tokens": ["Vor", "al\u00b7le", "dem", "Glanz", "und", "Ge\u00b7flit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Doch h\u00e4lt ihn die Nixe umarmet gar traut,", "tokens": ["Doch", "h\u00e4lt", "ihn", "die", "Ni\u00b7xe", "um\u00b7ar\u00b7met", "gar", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der Ritter ist Br\u00e4ut'gam, die Nixe ist Braut,", "tokens": ["Der", "Rit\u00b7ter", "ist", "Br\u00e4ut'\u00b7gam", ",", "die", "Ni\u00b7xe", "ist", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "$,", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Ihre Jungfraun spielen die Zither.", "tokens": ["Ih\u00b7re", "Jung\u00b7fraun", "spie\u00b7len", "die", "Zi\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Sie spielen und singen; es tanzen herein", "tokens": ["Sie", "spie\u00b7len", "und", "sin\u00b7gen", ";", "es", "tan\u00b7zen", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "PPER", "VVFIN", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Viel winzige M\u00e4dchen und B\u00fcbchen.", "tokens": ["Viel", "win\u00b7zi\u00b7ge", "M\u00e4d\u00b7chen", "und", "B\u00fcb\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Ritter, der will sich zu Tode freu'n,", "tokens": ["Der", "Rit\u00b7ter", ",", "der", "will", "sich", "zu", "To\u00b7de", "freu'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und fester umschlingt er sein Liebchen \u2014", "tokens": ["Und", "fes\u00b7ter", "um\u00b7schlingt", "er", "sein", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Da l\u00f6schen auf einmal die Lichter aus,", "tokens": ["Da", "l\u00f6\u00b7schen", "auf", "ein\u00b7mal", "die", "Lich\u00b7ter", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Ritter sitzt wieder ganz einsam zu Haus,", "tokens": ["Der", "Rit\u00b7ter", "sitzt", "wie\u00b7der", "ganz", "ein\u00b7sam", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-----+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "In dem dustern Poetenst\u00fcbchen.", "tokens": ["In", "dem", "dus\u00b7tern", "Poe\u00b7ten\u00b7st\u00fcb\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}