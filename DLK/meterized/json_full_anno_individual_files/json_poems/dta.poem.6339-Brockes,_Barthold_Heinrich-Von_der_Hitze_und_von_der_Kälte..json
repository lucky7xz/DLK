{"dta.poem.6339": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Gluht schickt einen hellen Schein", "tokens": ["Die", "Gluht", "schickt", "ei\u00b7nen", "hel\u00b7len", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Besch\u00e4fftigt in die Lufft hinein,", "tokens": ["Be\u00b7sch\u00e4ff\u00b7tigt", "in", "die", "Lufft", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sich der Stoff, so brennt, in grosser Menge reget,", "tokens": ["Wenn", "sich", "der", "Stoff", ",", "so", "brennt", ",", "in", "gros\u00b7ser", "Men\u00b7ge", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "$,", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mit Geschwindigkeit beweget.", "tokens": ["Und", "mit", "Ge\u00b7schwin\u00b7dig\u00b7keit", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}