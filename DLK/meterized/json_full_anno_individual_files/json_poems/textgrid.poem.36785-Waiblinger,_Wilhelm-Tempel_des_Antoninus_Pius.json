{"textgrid.poem.36785": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Tempel des Antoninus Pius", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles in unserer Zeit ist arch\u00e4ologisch geworden,", "tokens": ["Al\u00b7les", "in", "un\u00b7se\u00b7rer", "Zeit", "ist", "ar\u00b7ch\u00e4\u00b7o\u00b7lo\u00b7gisch", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "VAPP", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und das Alterthum gilt mehr als im Alterthum einst.", "tokens": ["Und", "das", "Al\u00b7ter\u00b7thum", "gilt", "mehr", "als", "im", "Al\u00b7ter\u00b7thum", "einst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIS", "KOKOM", "APPRART", "NN", "ADV", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Vetturine sind nun von klassischem Schwindel ergriffen:", "tokens": ["Vet\u00b7tu\u00b7ri\u00b7ne", "sind", "nun", "von", "klas\u00b7si\u00b7schem", "Schwin\u00b7del", "er\u00b7grif\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Alsbald, wie sie dein Thor, ewige Roma passirt;", "tokens": ["Als\u00b7bald", ",", "wie", "sie", "dein", "Thor", ",", "e\u00b7wi\u00b7ge", "Ro\u00b7ma", "pas\u00b7sirt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPOSAT", "NN", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-++---+-+", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Rasch den Corso hinab, mit Wagen, Gep\u00e4ck und mit Rossen", "tokens": ["Rasch", "den", "Cor\u00b7so", "hin\u00b7ab", ",", "mit", "Wa\u00b7gen", ",", "Ge\u00b7p\u00e4ck", "und", "mit", "Ros\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "APPR", "NN", "$,", "NN", "KON", "APPR", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Geht's in den Tempel sogleich, in die Dogana hinein.", "tokens": ["Geht's", "in", "den", "Tem\u00b7pel", "sog\u00b7leich", ",", "in", "die", "Do\u00b7ga\u00b7na", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADV", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}}, "stanza.2": {"line.1": {"text": "Alles in unserer Zeit ist arch\u00e4ologisch geworden,", "tokens": ["Al\u00b7les", "in", "un\u00b7se\u00b7rer", "Zeit", "ist", "ar\u00b7ch\u00e4\u00b7o\u00b7lo\u00b7gisch", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "VAPP", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und das Alterthum gilt mehr als im Alterthum einst.", "tokens": ["Und", "das", "Al\u00b7ter\u00b7thum", "gilt", "mehr", "als", "im", "Al\u00b7ter\u00b7thum", "einst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIS", "KOKOM", "APPRART", "NN", "ADV", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Vetturine sind nun von klassischem Schwindel ergriffen:", "tokens": ["Vet\u00b7tu\u00b7ri\u00b7ne", "sind", "nun"