{"dta.poem.7668": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dich nicht gutes Geld noch gutes Wort verdrie\u00dfen,", "tokens": ["La\u00df", "dich", "nicht", "gu\u00b7tes", "Geld", "noch", "gu\u00b7tes", "Wort", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADJA", "NN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn es um Fried' und Ruh den Handel gilt zu schlie\u00dfen.", "tokens": ["Wenn", "es", "um", "Fried'", "und", "Ruh", "den", "Han\u00b7del", "gilt", "zu", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht ist das beste Geld, das beste Wort zu gut,", "tokens": ["Nicht", "ist", "das", "bes\u00b7te", "Geld", ",", "das", "bes\u00b7te", "Wort", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel besser noch ist Fried', und Ruh, die noth dir thut.", "tokens": ["Viel", "bes\u00b7ser", "noch", "ist", "Fried'", ",", "und", "Ruh", ",", "die", "noth", "dir", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VAFIN", "NE", "$,", "KON", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du gabest Geld und Wort sonst unn\u00fctz manches aus;", "tokens": ["Du", "ga\u00b7best", "Geld", "und", "Wort", "sonst", "un\u00b7n\u00fctz", "man\u00b7ches", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADV", "ADJD", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verwende sie nunmehr zum Nutzen in dein Haus.", "tokens": ["Ver\u00b7wen\u00b7de", "sie", "nun\u00b7mehr", "zum", "Nut\u00b7zen", "in", "dein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gib was du hast, gib was du geben kannst mit Ehren,", "tokens": ["Gib", "was", "du", "hast", ",", "gib", "was", "du", "ge\u00b7ben", "kannst", "mit", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PWS", "PPER", "VAFIN", "$,", "VVIMP", "PWS", "PPER", "VVINF", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Gro\u00dfmuth, so da\u00df sie dir's nicht zur Feigheit kehren", "tokens": ["Aus", "Gro\u00df\u00b7muth", ",", "so", "da\u00df", "sie", "dir's", "nicht", "zur", "Feig\u00b7heit", "keh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "ADV", "KOUS", "PPER", "PIS", "PTKNEG", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}