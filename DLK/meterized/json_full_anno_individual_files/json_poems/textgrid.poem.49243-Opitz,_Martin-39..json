{"textgrid.poem.49243": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6ne Venus gieng mit ihrem kleinen Sohne", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ve\u00b7nus", "gieng", "mit", "ih\u00b7rem", "klei\u00b7nen", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dreien Gratien zur Juno g\u00fcldnem Throne,", "tokens": ["Und", "drei\u00b7en", "Gra\u00b7ti\u00b7en", "zur", "Ju\u00b7no", "g\u00fcld\u00b7nem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als sie den Jupiter gleich nicht zu Hause fand,", "tokens": ["Als", "sie", "den", "Ju\u00b7pi\u00b7ter", "gleich", "nicht", "zu", "Hau\u00b7se", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil er verreiset war ins schwarze Mohrenland.", "tokens": ["Weil", "er", "ver\u00b7rei\u00b7set", "war", "ins", "schwar\u00b7ze", "Moh\u00b7ren\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das k\u00f6stlich Himmelbrod lag auf der g\u00fcldnen Schale,", 