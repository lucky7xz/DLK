{"dta.poem.19402": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "135.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Thiere von dem Tod w\u00fc\u00dften soviel wie ihr,", "tokens": ["Wenn", "Thie\u00b7re", "von", "dem", "Tod", "w\u00fc\u00df\u00b7ten", "so\u00b7viel", "wie", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "PIS", "KOKOM", "PPER", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Zur Speise w\u00fcrd' euch auf der Welt kein fettes Thier.", "tokens": ["Zur", "Spei\u00b7se", "w\u00fcrd'", "euch", "auf", "der", "Welt", "kein", "fet\u00b7tes", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So sprach einst der Profet. Weil sie vom Tod nichts wissen,", "tokens": ["So", "sprach", "einst", "der", "Pro\u00b7fet", ".", "Weil", "sie", "vom", "Tod", "nichts", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$.", "KOUS", "PPER", "APPRART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum werden Thiere fett, und ihnen schmeckt der Bissen.", "tokens": ["Drum", "wer\u00b7den", "Thie\u00b7re", "fett", ",", "und", "ih\u00b7nen", "schmeckt", "der", "Bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und fett nur werden auch gleich Thieren mit Behagen", "tokens": ["Und", "fett", "nur", "wer\u00b7den", "auch", "gleich", "Thie\u00b7ren", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "ADV", "ADV", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Menschen, die den Tod sich aus dem Sinne schlagen.", "tokens": ["Die", "Men\u00b7schen", ",", "die", "den", "Tod", "sich", "aus", "dem", "Sin\u00b7ne", "schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}