{"dta.poem.21158": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das schlafende M\u00e4dchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die G\u00f6ttin s\u00fc\u00dfer Freuden,", "tokens": ["Die", "G\u00f6t\u00b7tin", "s\u00fc\u00b7\u00dfer", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Nacht, stieg aus dem Meer,", "tokens": ["Die", "Nacht", ",", "stieg", "aus", "dem", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sanfter Liebe Leiden", "tokens": ["Und", "sanf\u00b7ter", "Lie\u00b7be", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sang keine Fl\u00f6te mehr;", "tokens": ["Sang", "kei\u00b7ne", "Fl\u00f6\u00b7te", "mehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Mond mit blassem Scheine", "tokens": ["Der", "Mond", "mit", "blas\u00b7sem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Versilberte die stillen Haine.", "tokens": ["Ver\u00b7sil\u00b7ber\u00b7te", "die", "stil\u00b7len", "Hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da f\u00fchrte mich die Liebe", "tokens": ["Da", "f\u00fchr\u00b7te", "mich", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu meinem M\u00e4dchen hin.", "tokens": ["Zu", "mei\u00b7nem", "M\u00e4d\u00b7chen", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich fand ihr Aug oft tr\u00fcbe", "tokens": ["Ich", "fand", "ihr", "Aug", "oft", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Lieb und Eigensinn;", "tokens": ["Aus", "Lieb", "und", "Ei\u00b7gen\u00b7sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und niemals durft ichs wagen,", "tokens": ["Und", "nie\u00b7mals", "durft", "ichs", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jhr was von K\u00fcssen vorzusagen.", "tokens": ["Ihr", "was", "von", "K\u00fcs\u00b7sen", "vor\u00b7zu\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nachl\u00e4\u00dfig hingelehnet,", "tokens": ["Nach\u00b7l\u00e4\u00b7\u00dfig", "hin\u00b7ge\u00b7leh\u00b7net", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schlief sie ietzt am Clavier.", "tokens": ["Schlief", "sie", "ietzt", "am", "Cla\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zur Ehrfurcht sters gew\u00f6hnet,", "tokens": ["Zur", "Ehr\u00b7furcht", "sters", "ge\u00b7w\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Naht ich mich nicht zu ihr;", "tokens": ["Naht", "ich", "mich", "nicht", "zu", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "PTKNEG", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch weckten ihre Wangen", "tokens": ["Doch", "weck\u00b7ten", "ih\u00b7re", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein ganzes z\u00e4rtliches Verlangen.", "tokens": ["Mein", "gan\u00b7zes", "z\u00e4rt\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn Weste sich liebkosen,", "tokens": ["Wenn", "Wes\u00b7te", "sich", "lieb\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Lacht so nicht ihr Gesicht;", "tokens": ["Lacht", "so", "nicht", "ihr", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und so sch\u00f6n schl\u00e4ft auf Rosen", "tokens": ["Und", "so", "sch\u00f6n", "schl\u00e4ft", "auf", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die Blumeng\u00f6ttin nicht.", "tokens": ["Die", "Blu\u00b7men\u00b7g\u00f6t\u00b7tin", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In ihren sanften Mienen", "tokens": ["In", "ih\u00b7ren", "sanf\u00b7ten", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War nie der Himmel mehr erschienen.", "tokens": ["War", "nie", "der", "Him\u00b7mel", "mehr", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kanst du sie ietzt nicht k\u00fcssen,", "tokens": ["Kanst", "du", "sie", "ietzt", "nicht", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So k\u00fcssest du sie nie!", "tokens": ["So", "k\u00fcs\u00b7sest", "du", "sie", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wollt ich mich entschliessen \u2014", "tokens": ["So", "wollt", "ich", "mich", "ent\u00b7schlies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach! da erwachte sie!", "tokens": ["Ach", "!", "da", "er\u00b7wach\u00b7te", "sie", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nichts konte mehr mich strafen!", "tokens": ["Nichts", "kon\u00b7te", "mehr", "mich", "stra\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie wird so sch\u00f6n nicht wieder schlafen!", "tokens": ["Sie", "wird", "so", "sch\u00f6n", "nicht", "wie\u00b7der", "schla\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}