{"dta.poem.831": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.  Auf den Sontag de\u00df guten Seemans/  \n oder  Sexagesim\u00e6.  Luc. 8.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6re nichts/ wenn du mich heissest h\u00f6ren! ", "tokens": ["Ich", "h\u00f6\u00b7re", "nichts", "/", "wenn", "du", "mich", "heis\u00b7sest", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$(", "KOUS", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein werther Saam bringt leider wenig Frucht", "tokens": ["Dein", "wert\u00b7her", "Saam", "bringt", "lei\u00b7der", "we\u00b7nig", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In mir! Ach HERR/ der H\u00f6llen Vogel sucht", "tokens": ["In", "mir", "!", "Ach", "HeRR", "/", "der", "H\u00f6l\u00b7len", "Vo\u00b7gel", "sucht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$.", "ITJ", "NN", "$(", "ART", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Embsig dein wortt vnd mein Hertz zu versehren", "tokens": ["Emb\u00b7sig", "dein", "wortt", "vnd", "mein", "Hertz", "zu", "ver\u00b7seh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn sich die Bl\u00fct in meinem Geist wil mehren", "tokens": ["Wenn", "sich", "die", "Bl\u00fct", "in", "mei\u00b7nem", "Geist", "wil", "meh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kr\u00e4nckt mich die Hitz vnd (was ich offt verflucht)", "tokens": ["Kr\u00e4nckt", "mich", "die", "Hitz", "vnd", "(", "was", "ich", "offt", "ver\u00b7flucht", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "$(", "PWS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Sorgen Angst/ (Ach! scharffe dornen zucht!)", "tokens": ["Der", "Sor\u00b7gen", "Angst", "/", "(", "Ach", "!", "scharf\u00b7fe", "dor\u00b7nen", "zucht", "!", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$(", "$(", "ITJ", "$.", "VVFIN", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erst\u00f6ckt in mir schier alle gute Lehren.", "tokens": ["Er\u00b7st\u00f6ckt", "in", "mir", "schier", "al\u00b7le", "gu\u00b7te", "Leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schrecke die V\u00f6gel HErr die mich berauben", "tokens": ["Schre\u00b7cke", "die", "V\u00f6\u00b7gel", "Herr", "die", "mich", "be\u00b7rau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "ART", "PPER", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "La\u00df mich auch in der versuchung dir glauben", "tokens": ["La\u00df", "mich", "auch", "in", "der", "ver\u00b7su\u00b7chung", "dir", "glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Vnd rei\u00df die Disteln aus die gantz mein Hertz vmbgeben:", "tokens": ["Vnd", "rei\u00df", "die", "Dis\u00b7teln", "aus", "die", "gantz", "mein", "Hertz", "vmb\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "La\u00df mich durch Regen der Gnaden erguicken/", "tokens": ["La\u00df", "mich", "durch", "Re\u00b7gen", "der", "Gna\u00b7den", "er\u00b7gui\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ART", "NN", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schicke Gedult wenn das Creutze wil dr\u00fccken", "tokens": ["Schi\u00b7cke", "Ge\u00b7dult", "wenn", "das", "Creut\u00b7ze", "wil", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KOUS", "ART", "NN", "VMFIN", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Da\u00df an der Dornen statt dein Wortt m\u00f6g in mir leben.", "tokens": ["Da\u00df", "an", "der", "Dor\u00b7nen", "statt", "dein", "Wortt", "m\u00f6g", "in", "mir", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}