{"textgrid.poem.65920": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Geliebte", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebte, nimm uns hin mit schweren Schw\u00e4chen!", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "nimm", "uns", "hin", "mit", "schwe\u00b7ren", "Schw\u00e4\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin blo\u00df Mensch aus eigner Leidenschaft:", "tokens": ["Ich", "bin", "blo\u00df", "Mensch", "aus", "eig\u00b7ner", "Lei\u00b7den\u00b7schaft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf deinen Flechten goldet mir geborgne Kraft,", "tokens": ["Auf", "dei\u00b7nen", "Flech\u00b7ten", "gol\u00b7det", "mir", "ge\u00b7borg\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als schl\u00fcrft ich Glut aus hold besonnten B\u00e4chen.", "tokens": ["Als", "schl\u00fcrft", "ich", "Glut", "aus", "hold", "be\u00b7sonn\u00b7ten", "B\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit freien Augen gl\u00fcckt dir das Versprechen", "tokens": ["Mit", "frei\u00b7e