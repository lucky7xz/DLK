{"textgrid.poem.52778": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nimmer verrathen die G\u00f6tter den Liebling, ob er den Lorbeer", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimmer verrathen die G\u00f6tter den Liebling, ob er den Lorbeer", "tokens": ["Nim\u00b7mer", "ver\u00b7ra\u00b7then", "die", "G\u00f6t\u00b7ter", "den", "Lieb\u00b7ling", ",", "ob", "er", "den", "Lor\u00b7beer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+--+--+--+-++-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Dreimal uns\u00e4glichen Spotts unter die F\u00fc\u00dfe sich tritt.", "tokens": ["Drei\u00b7mal", "un\u00b7s\u00e4g\u00b7li\u00b7chen", "Spotts", "un\u00b7ter", "die", "F\u00fc\u00b7\u00dfe", "sich", "tritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+---++--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "G\u00f6the umarmte im Geist den Homer und den S\u00e4nger von Lidos,", "tokens": ["G\u00f6\u00b7the", "um\u00b7arm\u00b7te", "im", "Geist", "den", "Ho\u00b7mer", "und", "den", "S\u00e4n\u00b7ger", "von", "Li\u00b7dos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ART", "NE", "KON", "ART", "NN", "APPR", "NE", "$,"], "meter": "+--+--+-+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Aber Sophrosyne rieth ihm von Euripides ab;", "tokens": ["A\u00b7ber", "Sop\u00b7hro\u00b7sy\u00b7ne", "rieth", "ihm", "von", "Eu\u00b7ri\u00b7pi\u00b7des", "a