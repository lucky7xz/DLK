{"dta.poem.11664": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Alles mit Maas.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Mancherlei sind der Gaben, die g\u00fctige G\u00f6tter den Menschen", "tokens": ["Man\u00b7cher\u00b7lei", "sind", "der", "Ga\u00b7ben", ",", "die", "g\u00fc\u00b7ti\u00b7ge", "G\u00f6t\u00b7ter", "den", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Zum Genusse verliehn und f\u00fcr die t\u00e4gliche Nothdurft.", "tokens": ["Zum", "Ge\u00b7nus\u00b7se", "ver\u00b7liehn", "und", "f\u00fcr", "die", "t\u00e4g\u00b7li\u00b7che", "Noth\u00b7durft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber vor jeglichem Ding begehr' ich gebratenen Schweinsfu\u00df.", "tokens": ["A\u00b7ber", "vor", "jeg\u00b7li\u00b7chem", "Ding", "be\u00b7gehr'", "ich", "ge\u00b7bra\u00b7te\u00b7nen", "Schweins\u00b7fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--++", "measure": "dactylic.di.plus"}, "line.4": {"text": "Meine Frau Wirthin, die merkt's, nun hab' ich alle Tag'", "tokens": ["Mei\u00b7ne", "Frau", "Wirt\u00b7hin", ",", "die", "merkt's", ",", "nun", "hab'", "ich", "al\u00b7le", "Tag'"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "$,", "PRELS", "PIS", "$,", "ADV", "VAFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Oefters ahnt' mir im Geist: jezt ist kein einziger", "tokens": ["Oef\u00b7ters", "ahnt'", "mir", "im", "Geist", ":", "jezt", "ist", "kein", "ein\u00b7zi\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$.", "ADV", "VAFIN", "PIAT", "ADJA"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "In der Stadt mehr zu finden: Was krieg' ich zu Mit-", "tokens": ["In", "der", "Stadt", "mehr", "zu", "fin\u00b7den", ":", "Was", "krieg'", "ich", "zu", "Mit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$.", "PWS", "VVFIN", "PPER", "APPR", "TRUNC"], "meter": "--+--+--++-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Spr\u00e4che der K\u00f6nig nun gleich zu seinem Koch: Schaff'", "tokens": ["Spr\u00e4\u00b7che", "der", "K\u00f6\u00b7nig", "nun", "gleich", "zu", "sei\u00b7nem", "Koch", ":", "Schaff'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NE", "$.", "NN"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Gnade der Himmel dem Mann! denn nirgend mehr wan-", "tokens": ["Gna\u00b7de", "der", "Him\u00b7mel", "dem", "Mann", "!", "denn", "nir\u00b7gend", "mehr", "wan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "$.", "KON", "ADV", "ADV", "TRUNC"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Und ich sagte zur Wirthin zulezt: Nun la\u00dft mir die", "tokens": ["Und", "ich", "sag\u00b7te", "zur", "Wirt\u00b7hin", "zu\u00b7lezt", ":", "Nun", "la\u00dft", "mir", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ADV", "$.", "ADV", "VVFIN", "PPER", "ART"], "meter": "--+--++-+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Denn er schmeckt mir nicht mehr wie sonst, der br\u00e4un-", "tokens": ["Denn", "er", "schmeckt", "mir", "nicht", "mehr", "wie", "sonst", ",", "der", "br\u00e4un"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "KOKOM", "ADV", "$,", "ART", "TRUNC"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Aber sie denkt, aus Zartgef\u00fchl nur verb\u00e4t' ich die Schweins-", "tokens": ["A\u00b7ber", "sie", "denkt", ",", "aus", "Zart\u00b7ge\u00b7f\u00fchl", "nur", "ver\u00b7b\u00e4t'", "ich", "die", "Schweins"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "NN", "ADV", "VVFIN", "PPER", "ART", "TRUNC"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "L\u00e4chelnd bringet sie mir auch heute gebratenen Schweins-", "tokens": ["L\u00e4\u00b7chelnd", "brin\u00b7get", "sie", "mir", "auch", "heu\u00b7te", "ge\u00b7bra\u00b7te\u00b7nen", "Schweins"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ADJA", "TRUNC"], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Ei so hole der Teufel auf ewig die h\u00f6llischen Schweinsf\u00fc\u00df'!", "tokens": ["Ei", "so", "ho\u00b7le", "der", "Teu\u00b7fel", "auf", "e\u00b7wig", "die", "h\u00f6l\u00b7li\u00b7schen", "Schweins\u00b7f\u00fc\u00df'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}}}}}