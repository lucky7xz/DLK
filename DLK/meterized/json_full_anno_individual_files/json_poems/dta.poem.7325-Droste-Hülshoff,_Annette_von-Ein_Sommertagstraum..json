{"dta.poem.7325": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Ein Sommertagstraum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Im tiefen West der Schwaden grollte,", "tokens": ["Im", "tie\u00b7fen", "West", "der", "Schwa\u00b7den", "groll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es stand die Luft, ein siedend Meer,", "tokens": ["Es", "stand", "die", "Luft", ",", "ein", "sie\u00b7dend", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An meines Fensters Vorhang rollte", "tokens": ["An", "mei\u00b7nes", "Fens\u00b7ters", "Vor\u00b7hang", "roll\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonnenkugel, gl\u00fch und schwer,", "tokens": ["Die", "Son\u00b7nen\u00b7ku\u00b7gel", ",", "gl\u00fch", "und", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie ein Kranker, lang gestreckt,", "tokens": ["Und", "wie", "ein", "Kran\u00b7ker", ",", "lang", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lag ich auf gr\u00fcnen Sophakissen,", "tokens": ["Lag", "ich", "auf", "gr\u00fc\u00b7nen", "Sop\u00b7ha\u00b7kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Haupt von w\u00fcstem Schmerz zerrissen,", "tokens": ["Das", "Haupt", "von", "w\u00fcs\u00b7tem", "Schmerz", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Stirne fieberhaft gefleckt.", "tokens": ["Die", "Stir\u00b7ne", "fie\u00b7ber\u00b7haft", "ge\u00b7fleckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Um mich Geschenke, die man heute", "tokens": ["Um", "mich", "Ge\u00b7schen\u00b7ke", ",", "die", "man", "heu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPER", "NN", "$,", "PRELS", "PIS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu meinem Wiegenfest gesandt,", "tokens": ["Zu", "mei\u00b7nem", "Wie\u00b7gen\u00b7fest", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denare, Schriften, Meeres Beute,", "tokens": ["De\u00b7na\u00b7re", ",", "Schrif\u00b7ten", ",", "Mee\u00b7res", "Beu\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hab' mich schn\u00f6de abgewandt;", "tokens": ["Ich", "hab'", "mich", "schn\u00f6\u00b7de", "ab\u00b7ge\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Tode matt und schlafberaubt", "tokens": ["Zum", "To\u00b7de", "matt", "und", "schlaf\u00b7be\u00b7raubt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Studirt ich der Gardine Bauschen,", "tokens": ["Stu\u00b7dirt", "ich", "der", "Gar\u00b7di\u00b7ne", "Bau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und horchte auf des Blutes Rauschen", "tokens": ["Und", "horch\u00b7te", "auf", "des", "Blu\u00b7tes", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Klingeln im bet\u00e4ubten Haupt.", "tokens": ["Und", "Klin\u00b7geln", "im", "be\u00b7t\u00e4ub\u00b7ten", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zuweilen dehnte sich ein Murren", "tokens": ["Zu\u00b7wei\u00b7len", "dehn\u00b7te", "sich", "ein", "Mur\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Horizont entlang, es schlich", "tokens": ["Den", "Ho\u00b7ri\u00b7zont", "ent\u00b7lang", ",", "es", "schlich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Haag' ein Rieseln und ein Surren,", "tokens": ["Am", "Haag'", "ein", "Rie\u00b7seln", "und", "ein", "Sur\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie flatternder Libelle Strich;", "tokens": ["Wie", "flat\u00b7tern\u00b7der", "Li\u00b7bel\u00b7le", "Strich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Bet\u00e4ubend zog Resedaduft", "tokens": ["Be\u00b7t\u00e4u\u00b7bend", "zog", "Re\u00b7se\u00b7da\u00b7duft"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "VVFIN", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Durch des Balkones offne Th\u00fcren,", "tokens": ["Durch", "des", "Bal\u00b7ko\u00b7nes", "off\u00b7ne", "Th\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "In jeder Nerve war zu sp\u00fcren", "tokens": ["In", "je\u00b7der", "Ner\u00b7ve", "war", "zu", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die schwefelnde Gewitterluft.", "tokens": ["Die", "schwe\u00b7feln\u00b7de", "Ge\u00b7wit\u00b7ter\u00b7luft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Da pl\u00f6tzlich schien sich aufzurichten", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "schien", "sich", "auf\u00b7zu\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PRF", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Fensterrahm ein Schattenwall,", "tokens": ["Am", "Fens\u00b7ter\u00b7rahm", "ein", "Schat\u00b7ten\u00b7wall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und m\u00e4hlig schob die dunklen Schichten", "tokens": ["Und", "m\u00e4h\u00b7lig", "schob", "die", "dunk\u00b7len", "Schich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er n\u00e4her an den gl\u00fchen Ball.", "tokens": ["Er", "n\u00e4\u00b7her", "an", "den", "gl\u00fc\u00b7hen", "Ball", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch der Gardine Spalten zog", "tokens": ["Durch", "der", "Gar\u00b7di\u00b7ne", "Spal\u00b7ten", "zog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Ein frischer Hauch, ich schlo\u00df die Augen,", "tokens": ["Ein", "fri\u00b7scher", "Hauch", ",", "ich", "schlo\u00df", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Um tiefer, tiefer einzusaugen,", "tokens": ["Um", "tie\u00b7fer", ",", "tie\u00b7fer", "ein\u00b7zu\u00b7sau\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was leise spielend mich umflog.", "tokens": ["Was", "lei\u00b7se", "spie\u00b7lend", "mich", "um\u00b7flog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Genau vernahm ich noch das Rucken", "tokens": ["Ge\u00b7nau", "ver\u00b7nahm", "ich", "noch", "das", "Ru\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des flatternden Papiers, das Licht", "tokens": ["Des", "flat\u00b7tern\u00b7den", "Pa\u00b7piers", ",", "das", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Stufe sah ich schmerzend zucken;", "tokens": ["Der", "Stu\u00b7fe", "sah", "ich", "schmer\u00b7zend", "zu\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob ich entschlief? mich d\u00fcnkt es nicht.", "tokens": ["Ob", "ich", "ent\u00b7schlief", "?", "mich", "d\u00fcnkt", "es", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch schneller schien am Autograph", "tokens": ["Doch", "schnel\u00b7ler", "schien", "am", "Au\u00b7to\u00b7graph"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das d\u00fcrre J\u00fcngelchen zu wehen,", "tokens": ["Das", "d\u00fcr\u00b7re", "J\u00fcn\u00b7gel\u00b7chen", "zu", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein glitzernd Aug' der Stein zu drehen,", "tokens": ["Ein", "glit\u00b7zernd", "Aug'", "der", "Stein", "zu", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Muschel dehnte sich im Schlaf.", "tokens": ["Die", "Mu\u00b7schel", "dehn\u00b7te", "sich", "im", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und, n\u00e4cht'ger M\u00fccke zu vergleichen,", "tokens": ["Und", ",", "n\u00e4cht'\u00b7ger", "M\u00fc\u00b7cke", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ums\u00e4uselte mich halber Klang,", "tokens": ["Um\u00b7s\u00e4u\u00b7sel\u00b7te", "mich", "hal\u00b7ber", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Teppich schien es sacht zu streichen,", "tokens": ["Am", "Tep\u00b7pich", "schien", "es", "sacht", "zu", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lief des Polsters Saum entlang,", "tokens": ["Und", "lief", "des", "Pols\u00b7ters", "Saum", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie wenn im zitternden Papier", "tokens": ["Wie", "wenn", "im", "zit\u00b7tern\u00b7den", "Pa\u00b7pier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Fliege zarte F\u00fc\u00dfchen irren;", "tokens": ["Der", "Flie\u00b7ge", "zar\u00b7te", "F\u00fc\u00df\u00b7chen", "ir\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und heller feiner aus dem Schwirren", "tokens": ["Und", "hel\u00b7ler", "fei\u00b7ner", "aus", "dem", "Schwir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Drang es wie Wortes Hauch zu mir:", "tokens": ["Drang", "es", "wie", "Wor\u00b7tes", "Hauch", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "NN", "APPR", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}