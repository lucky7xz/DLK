{"textgrid.poem.32771": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Die Zufriedenheit", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was frag' ich viel nach Geld und Gut,", "tokens": ["Was", "frag'", "ich", "viel", "nach", "Geld", "und", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich zufrieden bin!", "tokens": ["Wenn", "ich", "zu\u00b7frie\u00b7den", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Giebt Gott mir nur gesundes Blut,", "tokens": ["Giebt", "Gott", "mir", "nur", "ge\u00b7sun\u00b7des", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hab' ich frohen Sinn,", "tokens": ["So", "hab'", "ich", "fro\u00b7hen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sing' aus dankbarem Gem\u00fct", "tokens": ["Und", "sing'", "aus", "dank\u00b7ba\u00b7rem", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Morgen- und mein Abendlied.", "tokens": ["Mein", "Mor\u00b7gen", "und", "mein", "A\u00b7ben\u00b7dlied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So mancher schwimmt im \u00dcberflu\u00df,", "tokens": ["So", "man\u00b7cher", "schwimmt", "im", "\u00dc\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat Haus und Hof und Geld;", "tokens": ["Hat", "Haus", "und", "Hof", "und", "Geld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ist doch immer voll Verdru\u00df,", "tokens": ["Und", "ist", "doch", "im\u00b7mer", "voll", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und freut sich nicht der Welt.", "tokens": ["Und", "freut", "sich", "nicht", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Je mehr er hat, je mehr er will;", "tokens": ["Je", "mehr", "er", "hat", ",", "je", "mehr", "er", "will", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VAFIN", "$,", "ADV", "ADV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nie schweigen seine Klagen still.", "tokens": ["Nie", "schwei\u00b7gen", "sei\u00b7ne", "Kla\u00b7gen", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da hei\u00dft die Welt ein Jammerthal,", "tokens": ["Da", "hei\u00dft", "die", "Welt", "ein", "Jam\u00b7mer\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und deucht mir doch so sch\u00f6n;", "tokens": ["Und", "deucht", "mir", "doch", "so", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat Freuden ohne Ma\u00df und Zahl,", "tokens": ["Hat", "Freu\u00b7den", "oh\u00b7ne", "Ma\u00df", "und", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4\u00dft keinen leer ausgehn.", "tokens": ["L\u00e4\u00dft", "kei\u00b7nen", "leer", "aus\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das K\u00e4ferlein und V\u00f6gelein", "tokens": ["Das", "K\u00e4\u00b7fer\u00b7lein", "und", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darf sich ja auch des Maien freun.", "tokens": ["Darf", "sich", "ja", "auch", "des", "Mai\u00b7en", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und uns zuliebe schm\u00fccken ja", "tokens": ["Und", "uns", "zu\u00b7lie\u00b7be", "schm\u00fc\u00b7cken", "ja"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Wiese, Berg und Wald;", "tokens": ["Sich", "Wie\u00b7se", ",", "Berg", "und", "Wald", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und V\u00f6gel singen fern und nah,", "tokens": ["Und", "V\u00f6\u00b7gel", "sin\u00b7gen", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df alles wiederhallt. \u2013", "tokens": ["Da\u00df", "al\u00b7les", "wie\u00b7der\u00b7hallt", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bei'r Arbeit singt die Lerch' uns zu,", "tokens": ["Bei'r", "Ar\u00b7beit", "singt", "die", "Ler\u00b7ch'", "uns", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Nachtigall bei'r s\u00fc\u00dfen Ruh'.", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "bei'r", "s\u00fc\u00b7\u00dfen", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn die goldne Sonn' aufgeht,", "tokens": ["Und", "wenn", "die", "gold\u00b7ne", "Sonn'", "auf\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und golden wird die Welt,", "tokens": ["Und", "gol\u00b7den", "wird", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alles in der Bl\u00fcte steht,", "tokens": ["Und", "al\u00b7les", "in", "der", "Bl\u00fc\u00b7te", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00c4hren tr\u00e4gt das Feld:", "tokens": ["Und", "\u00c4h\u00b7ren", "tr\u00e4gt", "das", "Feld", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann denk' ich, alle diese Pracht", "tokens": ["Dann", "denk'", "ich", ",", "al\u00b7le", "die\u00b7se", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PIS", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat Gott zu meiner Lust gemacht.", "tokens": ["Hat", "Gott", "zu", "mei\u00b7ner", "Lust", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann preis' ich Gott, und lobe Gott,", "tokens": ["Dann", "preis'", "ich", "Gott", ",", "und", "lo\u00b7be", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schweb' in hohem Mut;", "tokens": ["Und", "schweb'", "in", "ho\u00b7hem", "Mut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und denk', es ist ein lieber Gott,", "tokens": ["Und", "denk'", ",", "es", "ist", "ein", "lie\u00b7ber", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meint's mit Menschen gut;", "tokens": ["Und", "meint's", "mit", "Men\u00b7schen", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum will ich immer dankbar sein,", "tokens": ["Drum", "will", "ich", "im\u00b7mer", "dank\u00b7bar", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mich ob seiner G\u00fcte freun!", "tokens": ["Und", "mich", "ob", "sei\u00b7ner", "G\u00fc\u00b7te", "freun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was frag' ich viel nach Geld und Gut,", "tokens": ["Was", "frag'", "ich", "viel", "nach", "Geld", "und", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich zufrieden bin!", "tokens": ["Wenn", "ich", "zu\u00b7frie\u00b7den", "bin", "!"], "token_