{"dta.poem.13145": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edie Sach ist zimlich schwer; jedoch es wird gelingen;", "tokens": ["\u201e", "die", "Sach", "ist", "zim\u00b7lich", "schwer", ";", "je\u00b7doch", "es", "wird", "ge\u00b7lin\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADV", "ADJD", "$.", "ADV", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eein Trapp kann nicht gerad sich nach den Wolcken schwingen.", "tokens": ["\u201e", "ein", "Trapp", "kann", "nicht", "ge\u00b7rad", "sich", "nach", "den", "Wol\u00b7cken", "schwin\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PTKNEG", "ADV", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "205\u201dEr hupft, pr\u00fcft einen Schritt, er hebt sich, wagt den Sprung;", "tokens": ["\"", "Er", "hupft", ",", "pr\u00fcft", "ei\u00b7nen", "Schritt", ",", "er", "hebt", "sich", ",", "wagt", "den", "Sprung", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edurch seiner Fl\u00fcgel Kraft bringt er sich in den Schwung;", "tokens": ["\u201e", "durch", "sei\u00b7ner", "Fl\u00fc\u00b7gel", "Kraft", "bringt", "er", "sich", "in", "den", "Schwung", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "\u201eer rudert, flattert, bebt und schl\u00e4gt mit dem Gefieder;", "tokens": ["\u201e", "er", "ru\u00b7dert", ",", "flat\u00b7tert", ",", "bebt", "und", "schl\u00e4gt", "mit", "dem", "Ge\u00b7fie\u00b7der", ";"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ehier dringt er in die H\u00f6h, und dort l\u00e4\u00dft er sich nieder;", "tokens": ["\u201e", "hier", "dringt", "er", "in", "die", "H\u00f6h", ",", "und", "dort", "l\u00e4\u00dft", "er", "sich", "nie\u00b7der", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ebi\u00df allgemach der Grund ihm von den Klauen geht,", "tokens": ["\u201e", "bi\u00df", "all\u00b7ge\u00b7mach", "der", "Grund", "ihm", "von", "den", "Klau\u00b7en", "geht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "210\u201dUnd er sich in die Luft durch krumme Weege dreht;", "tokens": ["\"", "Und", "er", "sich", "in", "die", "Luft", "durch", "krum\u00b7me", "Wee\u00b7ge", "dreht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "PRF", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201edurch seiner Federn Hilff der Winde Wogen theilet,", "tokens": ["\u201e", "durch", "sei\u00b7ner", "Fe\u00b7dern", "Hilff", "der", "Win\u00b7de", "Wo\u00b7gen", "thei\u00b7let", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eund so mit M\u00fch und Macht nach Berg und H\u00fcgeln eilet.", "tokens": ["\u201e", "und", "so", "mit", "M\u00fch", "und", "Macht", "nach", "Berg", "und", "H\u00fc\u00b7geln", "ei\u00b7let", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "APPR", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}