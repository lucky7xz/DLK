{"dta.poem.23364": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Stiftungslied  \n  der adelichen Ressource zu Kuhschnappel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nie soll es doch ihnen gelingen,", "tokens": ["Nie", "soll", "es", "doch", "ih\u00b7nen", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wir halten vom Ziele sie fern:", "tokens": ["Wir", "hal\u00b7ten", "vom", "Zie\u00b7le", "sie", "fern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie bleiben das Lumpengesindel,", "tokens": ["Sie", "blei\u00b7ben", "das", "Lum\u00b7pen\u00b7ge\u00b7sin\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wir bleiben die gn\u00e4digen Herrn.", "tokens": ["Wir", "blei\u00b7ben", "die", "gn\u00e4\u00b7di\u00b7gen", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Und haben wir Manches verloren,", "tokens": ["Und", "ha\u00b7ben", "wir", "Man\u00b7ches", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So kehret auch Manches zur\u00fcck;", "tokens": ["So", "keh\u00b7ret", "auch", "Man\u00b7ches", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Stets gehet die Zeit noch im Kreise,", "tokens": ["Stets", "ge\u00b7het", "die", "Zeit", "noch", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie bringet zur\u00fcck uns das Gl\u00fcck.", "tokens": ["Sie", "brin\u00b7get", "zu\u00b7r\u00fcck", "uns", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "PPER", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Hervor mit den alten Gesetzen,", "tokens": ["Her\u00b7vor", "mit", "den", "al\u00b7ten", "Ge\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und weg mit der Constitution!", "tokens": ["Und", "weg", "mit", "der", "Con\u00b7sti\u00b7tu\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da kommen die besseren Zeiten", "tokens": ["Da", "kom\u00b7men", "die", "bes\u00b7se\u00b7ren", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von selber f\u00fcr uns und den Thron.", "tokens": ["Von", "sel\u00b7ber", "f\u00fcr", "uns", "und", "den", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PPER", "KON", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Drum lasset uns hoffen und harren,", "tokens": ["Drum", "las\u00b7set", "uns", "hof\u00b7fen", "und", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Weil Adel und Tugend nicht stirbt,", "tokens": ["Weil", "A\u00b7del", "und", "Tu\u00b7gend", "nicht", "stirbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da\u00df endlich der Adel Europas", "tokens": ["Da\u00df", "end\u00b7lich", "der", "A\u00b7del", "Eu\u00b7ro\u00b7pas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein Recht auch noch wieder erwirbt.", "tokens": ["Sein", "Recht", "auch", "noch", "wie\u00b7der", "er\u00b7wirbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}