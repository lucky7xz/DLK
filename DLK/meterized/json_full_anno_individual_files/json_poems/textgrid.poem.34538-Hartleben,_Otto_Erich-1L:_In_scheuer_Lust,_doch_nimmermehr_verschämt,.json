{"textgrid.poem.34538": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: In scheuer Lust, doch nimmermehr versch\u00e4mt,", "genre": "verse", "period": "N.A.", "pub_year": 1895, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In scheuer Lust, doch nimmermehr versch\u00e4mt,", "tokens": ["In", "scheu\u00b7er", "Lust", ",", "doch", "nim\u00b7mer\u00b7mehr", "ver\u00b7sch\u00e4mt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hobst du die runden, weissen Arme auf", "tokens": ["hobst", "du", "die", "run\u00b7den", ",", "weis\u00b7sen", "Ar\u00b7me", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und dehntest sie empor und suchtest blinzelnd", "tokens": ["und", "dehn\u00b7test", "sie", "em\u00b7por", "und", "such\u00b7test", "blin\u00b7zelnd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "dein Bild im Spiegel.", "tokens": ["dein", "Bild", "im", "Spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich aber stand entfesselt hinter dir", "tokens": ["Ich", "a\u00b7ber", "stand", "ent\u00b7fes\u00b7selt", "hin\u00b7ter", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und sah in deinen vollen, blanken Schultern", "tokens": ["und", "sah", "in", "dei\u00b7nen", "vol\u00b7len", ",", "blan\u00b7ken", "Schul\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die beiden Gr\u00fcbchen.", "tokens": ["die", "bei\u00b7den", "Gr\u00fcb\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Da beugt ich mich auf diesen Nacken nieder", "tokens": ["Da", "beugt", "ich", "mich", "auf", "die\u00b7sen", "Na\u00b7cken", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zum Kuss ...", "tokens": ["zum", "Kuss", "..."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Es ward mir klar, wie du den G\u00f6ttern still", "tokens": ["Es", "ward", "mir", "klar", ",", "wie", "du", "den", "G\u00f6t\u00b7tern", "still"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "PWAV", "PPER", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "vertraut, gar innig wohl befreundet bist.", "tokens": ["ver\u00b7traut", ",", "gar", "in\u00b7nig", "wohl", "be\u00b7freun\u00b7det", "bist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn sie dir nahen, tupfen sie dir leise", "tokens": ["Wenn", "sie", "dir", "na\u00b7hen", ",", "tup\u00b7fen", "sie", "dir", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJA", "$,", "VVFIN", "PPER", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit leichtem Finger auf dies schwellende Rund \u2013", "tokens": ["mit", "leich\u00b7tem", "Fin\u00b7ger", "auf", "dies", "schwel\u00b7len\u00b7de", "Rund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PDS", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "und also lieblich, Menschensinn verwirrend,", "tokens": ["und", "al\u00b7so", "lieb\u00b7lich", ",", "Men\u00b7schen\u00b7sinn", "ver\u00b7wir\u00b7rend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "blieb ihres Grusses Spur in deinem Fleisch.", "tokens": ["blieb", "ih\u00b7res", "Grus\u00b7ses", "Spur", "in", "dei\u00b7nem", "Fleisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "In scheuer Lust, doch nimmermehr versch\u00e4mt,", "tokens": ["In", "scheu\u00b7er", "Lust", ",", "doch", "nim\u00b7mer\u00b7mehr", "ver\u00b7sch\u00e4mt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hobst du die runden, weissen Arme auf", "tokens": ["hobst", "du", "die", "run\u00b7den", ",", "weis\u00b