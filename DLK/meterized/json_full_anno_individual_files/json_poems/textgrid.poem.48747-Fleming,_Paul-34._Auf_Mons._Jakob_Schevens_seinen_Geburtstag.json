{"textgrid.poem.48747": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "34. Auf Mons. Jakob Schevens seinen Geburtstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So l\u00f6se dich denn nun mit deiner kalten Schalen,", "tokens": ["So", "l\u00f6\u00b7se", "dich", "denn", "nun", "mit", "dei\u00b7ner", "kal\u00b7ten", "Scha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die wolschmeckt nach Kaneel und reich an Zucker ist,", "tokens": ["die", "wol\u00b7schmeckt", "nach", "Ka\u00b7ne\u00b7el", "und", "reich", "an", "Zu\u00b7cker", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "KON", "ADJD", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "nachdem du deinen Tag in voller Freude siehst,", "tokens": ["nach\u00b7dem", "du", "dei\u00b7nen", "Tag", "in", "vol\u00b7ler", "Freu\u00b7de", "siehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den dir Matutens Zier und Ph\u00f6bus sch\u00f6ne Stralen", "tokens": ["den", "dir", "Ma\u00b7tu\u00b7tens", "Zier", "und", "Ph\u00f6\u00b7bus", "sch\u00f6\u00b7ne", "Stra\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "NE", "NN", "KON", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "bei abgehellter Luft mit lichten Farben malen.", "tokens": ["bei", "ab\u00b7ge\u00b7hell\u00b7ter", "Luft", "mit", "lich\u00b7ten", "Far\u00b7ben", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Heut ist es mehr als recht, da\u00df du dich um uns m\u00fchst,", "tokens": ["Heut", "ist", "es", "mehr", "als", "recht", ",", "da\u00df", "du", "dich", "um", "uns", "m\u00fchst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "KOKOM", "ADJD", "$,", "KOUS", "PPER", "PRF", "APPR", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "verordnest das Konfect, den Lautertrank angl\u00fchst.", "tokens": ["ver\u00b7ord\u00b7nest", "das", "Kon\u00b7fect", ",", "den", "Lau\u00b7ter\u00b7trank", "an\u00b7gl\u00fchst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer so geehret wird, der mu\u00df es so bezahlen.", "tokens": ["Wer", "so", "ge\u00b7eh\u00b7ret", "wird", ",", "der", "mu\u00df", "es", "so", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$,", "ART", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Koch, der ist dein Freund, der Kellner kennt dich wol,", "tokens": ["Der", "Koch", ",", "der", "ist", "dein", "Freund", ",", "der", "Kell\u00b7ner", "kennt", "dich", "wol", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "es ist dir um ein Wort, so steht die Tafel voll;", "tokens": ["es", "ist", "dir", "um", "ein", "Wort", ",", "so", "steht", "die", "Ta\u00b7fel", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und so kanst du dich recht den lieben Tag durch freuen.", "tokens": ["und", "so", "kanst", "du", "dich", "recht", "den", "lie\u00b7ben", "Tag", "durch", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "ADJD", "ART", "ADJA", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wolan, versieh dich mein und r\u00e4ume meine Stat.", "tokens": ["Wo\u00b7lan", ",", "ver\u00b7sieh", "dich", "mein", "und", "r\u00e4u\u00b7me", "mei\u00b7ne", "Stat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "PPOSAT", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich will der dritte sein bei deinen treuen Zweien.", "tokens": ["Ich", "will", "der", "drit\u00b7te", "sein", "bei", "dei\u00b7nen", "treu\u00b7en", "Zwei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "