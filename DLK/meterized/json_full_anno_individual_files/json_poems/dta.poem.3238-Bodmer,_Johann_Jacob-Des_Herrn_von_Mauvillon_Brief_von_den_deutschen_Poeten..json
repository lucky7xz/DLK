{"dta.poem.3238": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Des Herrn von Mauvillon Brief  \n von den deutschen Poeten.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198490", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df sich die Bluhmen auch zusammen br\u00fcnstig freyhn,", "tokens": ["Da\u00df", "sich", "die", "Bluh\u00b7men", "auch", "zu\u00b7sam\u00b7men", "br\u00fcns\u00b7tig", "freyhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und durch des Zephyrs Gunst in Stand der Ehe treten,", "tokens": ["Und", "durch", "des", "Ze\u00b7phyrs", "Gunst", "in", "Stand", "der", "E\u00b7he", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}