{"textgrid.poem.60587": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch Kleist ist hin, la\u00dft weit herum erschallen,", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch Kleist ist hin, la\u00dft weit herum erschallen,", "tokens": ["Auch", "Kleist", "ist", "hin", ",", "la\u00dft", "weit", "he\u00b7rum", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "$,", "VVFIN", "ADJD", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Musen an den Oderstrand;", "tokens": ["Ihr", "Mu\u00b7sen", "an", "den", "O\u00b7der\u00b7strand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Edler ist im Streit gefallen", "tokens": ["Ein", "Ed\u00b7ler", "ist", "im", "Streit", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Streit f\u00fcrs Vaterland.", "tokens": ["Im", "Streit", "f\u00fcrs", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sein Heldenblut flo\u00df auf die g\u00fcldne Leyer", "tokens": ["Sein", "Hel\u00b7den\u00b7blut", "flo\u00df", "auf", "die", "g\u00fcld\u00b7ne", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die sonst in seiner Hand erklang,", "tokens": ["Die", "sonst", "in", "sei\u00b7ner", "Hand", "er\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In die mit kriegerischen Feuer", "tokens": ["In", "die", "mit", "krie\u00b7ge\u00b7ri\u00b7schen", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er von der Tugend sang.", "tokens": ["Er", "von", "der", "Tu\u00b7gend", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kleist ist nicht mehr! La\u00dft weit herum erschallen,", "tokens": ["Kleist", "ist", "nicht", "mehr", "!", "La\u00dft", "weit", "he\u00b7rum", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ADV", "$.", "VVIMP", "ADJD", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Musen, durch die bange Welt,", "tokens": ["Ihr", "Mu\u00b7sen", ",", "durch", "die", "ban\u00b7ge", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Musen Liebling ist gefallen,", "tokens": ["Der", "Mu\u00b7sen", "Lieb\u00b7ling", "ist", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Menschenfreund und Held!", "tokens": ["Ein", "Men\u00b7schen\u00b7freund", "und", "Held", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Freundschaft Schmerz, die mit best\u00e4ubten Haaren", "tokens": ["Der", "Freund\u00b7schaft", "Schmerz", ",", "die", "mit", "be\u00b7st\u00e4ub\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stumm \u00fcber seiner Urne weint,", "tokens": ["Stumm", "\u00fc\u00b7ber", "sei\u00b7ner", "Ur\u00b7ne", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "R\u00fchrt auch die Feinde, selbst Barbaren,", "tokens": ["R\u00fchrt", "auch", "die", "Fein\u00b7de", ",", "selbst", "Bar\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beklagen einen Feind.", "tokens": ["Be\u00b7kla\u00b7gen", "ei\u00b7nen", "Feind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch ewig Lob erwartet grosse Seelen:", "tokens": ["Doch", "e\u00b7wig", "Lob", "er\u00b7war\u00b7tet", "gros\u00b7se", "See\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die, zur Unsterblichkeit ernannt,", "tokens": ["Die", ",", "zur", "U\u00b7nsterb\u00b7lich\u00b7keit", "er\u00b7nannt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Den sch\u00f6nen Tod der Helden wehlen,", "tokens": ["Den", "sch\u00f6\u00b7nen", "Tod", "der", "Hel\u00b7den", "weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Tod f\u00fcrs Vaterland.", "tokens": ["Den", "Tod", "f\u00fcrs", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie fliehn empor, und werden aufgenommen,", "tokens": ["Sie", "fliehn", "em\u00b7por", ",", "und", "wer\u00b7den", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In H\u00fctten der Gl\u00fcckseeligkeit,", "tokens": ["In", "H\u00fct\u00b7ten", "der", "Gl\u00fcck\u00b7see\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Gustav Adolf hingekommen", "tokens": ["Wo", "Gus\u00b7tav", "A\u00b7dolf", "hin\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "NE", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wunder ieder Zeit.", "tokens": ["Das", "Wun\u00b7der", "ie\u00b7der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dort ist auch Kleist! hinweg mit feigem Grame,", "tokens": ["Dort", "ist", "auch", "Kleist", "!", "hin\u00b7weg", "mit", "fei\u00b7gem", "Gra\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$.", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hoch unter Sternen geht der Held", "tokens": ["Hoch", "un\u00b7ter", "Ster\u00b7nen", "geht", "der", "Held"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Graf Schwerin: ein grosser Name,", "tokens": ["Und", "Graf", "Schwe\u00b7rin", ":", "ein", "gros\u00b7ser", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Keith und Winterfeld.", "tokens": ["Mit", "Keith", "und", "Win\u00b7ter\u00b7feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Auf Friedrich sehn die Helden Friedrichs nieder,", "tokens": ["Auf", "Fried\u00b7rich", "sehn", "die", "Hel\u00b7den", "Fried\u00b7richs", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewundernd, mit besorgtem Blick,", "tokens": ["Be\u00b7wun\u00b7dernd", ",", "mit", "be\u00b7sorg\u00b7tem", "Blick", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und flehn f\u00fcr Ihn, und ihre Br\u00fcder,", "tokens": ["Und", "flehn", "f\u00fcr", "Ihn", ",", "und", "ih\u00b7re", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um Leben und um Gl\u00fcck;", "tokens": ["Um", "Le\u00b7ben", "und", "um", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie flehn zu Gott, um Frieden f\u00fcr die Erde,", "tokens": ["Sie", "flehn", "zu", "Gott", ",", "um", "Frie\u00b7den", "f\u00fcr", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KOUI", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Damit in Ketten ewger Nacht,", "tokens": ["Da\u00b7mit", "in", "Ket\u00b7ten", "ew\u00b7ger", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Furie gefesselt werde,", "tokens": ["Die", "Fu\u00b7rie", "ge\u00b7fes\u00b7selt", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Deutschland w\u00fcste macht;", "tokens": ["Die", "Deutschland", "w\u00fcs\u00b7te", "macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.10": {"line.1": {"text": "Und bis ihr einst der, dem die Himmel dienen,", "tokens": ["Und", "bis", "ihr", "einst", "der", ",", "dem", "die", "Him\u00b7mel", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Gott des Donners widersteht,", "tokens": ["Der", "Gott", "des", "Don\u00b7ners", "wi\u00b7der\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch unter brennenden Ruinen,", "tokens": ["Noch", "un\u00b7ter", "bren\u00b7nen\u00b7den", "Ru\u00b7i\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcber Leichen geht.", "tokens": ["Und", "\u00fc\u00b7ber", "Lei\u00b7chen", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Auch Kleist ist hin, la\u00dft weit herum erschallen,", "tokens": ["Auch", "Kleist", "ist", "hin", ",", "la\u00dft", "weit", "he\u00b7rum", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "$,", "VVFIN", "ADJD", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Musen an den Oderstrand;", "tokens": ["Ihr", "Mu\u00b7sen", "an", "den", "O\u00b7der\u00b7strand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "A