{"dta.poem.14959": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Von Engeln und von Bengeln.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Fl\u00fcgeldecken zersprangen,", "tokens": ["Die", "Fl\u00fc\u00b7gel\u00b7de\u00b7cken", "zer\u00b7spran\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weit, morgensch\u00f6n strahlt' die Welt,", "tokens": ["Weit", ",", "mor\u00b7gen\u00b7sch\u00f6n", "strahlt'", "die", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und \u00fcber's Gr\u00fcn sie sich schwangen", "tokens": ["Und", "\u00fc\u00b7ber's", "Gr\u00fcn", "sie", "sich", "schwan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPER", "PRF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis an das Himmelszelt.", "tokens": ["Bis", "an", "das", "Him\u00b7mels\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}