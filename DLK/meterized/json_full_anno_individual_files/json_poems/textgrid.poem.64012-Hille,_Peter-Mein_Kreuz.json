en_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Aasfliegen strotzen so im Schillerpanzer \u2013", "tokens": ["Aas\u00b7flie\u00b7gen", "strot\u00b7zen", "so", "im", "Schil\u00b7ler\u00b7pan\u00b7zer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.26": {"text": "Mir einen Wisch mit Lauge.", "tokens": ["Mir", "ei\u00b7nen", "Wisch", "mit", "Lau\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Von Doktor So und so.", "tokens": ["Von", "Dok\u00b7tor", "So", "und", "so", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": " und J\u00fcngerfrauen,", "tokens": ["und", "J\u00fcn\u00b7ger\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Die stehn gar mildiglich verwundert, unverwandt", "tokens": ["Die", "stehn", "gar", "mil\u00b7dig\u00b7lich", "ver\u00b7wun\u00b7dert", ",", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "VVFIN", "$,", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Zu mir empor zu schauen.", "tokens": ["Zu", "mir", "em\u00b7por", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Dann ruft der Topf sie", "tokens": ["Dann", "ruft", "der", "Topf", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.32": {"text": "\u00bbleben Sie recht wohl, Herr Hille!\u00ab", "tokens": ["\u00bb", "le\u00b7ben", "Sie", "recht", "wohl", ",", "Herr", "Hil\u00b7le", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ADV", "$,", "NN", "NE", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}