{"dta.poem.14681": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4ger und J\u00e4gerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' ich ein muntres Hirschlein schlank,", "tokens": ["W\u00e4r'", "ich", "ein", "mun\u00b7tres", "Hirsc\u00b7hlein", "schlank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt' ich im gr\u00fcnen Walde geh'n,", "tokens": ["Wollt'", "ich", "im", "gr\u00fc\u00b7nen", "Wal\u00b7de", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Spazieren geh'n bei H\u00f6rnerklang,", "tokens": ["Spa\u00b7zie\u00b7ren", "geh'n", "bei", "H\u00f6r\u00b7ner\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach meinem Liebsten mich umseh'n.", "tokens": ["Nach", "mei\u00b7nem", "Liebs\u00b7ten", "mich", "um\u00b7seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}