{"dta.poem.946": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "W ALLER   I M   S CHNEE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Noch zwingt mich treue &#252;ber dir zu wachen               ", "tokens": ["Noch", "zwingt", "mich", "treu\u00b7e", "&#252;", "ber", "dir", "zu", "wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$(", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und deines duldens sch\u00f6nheit dass ich weile               ", "tokens": ["Und", "dei\u00b7nes", "dul\u00b7dens", "sch\u00f6n\u00b7heit", "dass", "ich", "wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein heilig streben ist mich traurig machen               ", "tokens": ["Mein", "hei\u00b7lig", "stre\u00b7ben", "ist", "mich", "trau\u00b7rig", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "VVFIN", "VAFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Damit ich wahrer deine trauer teile             ", "tokens": ["Da\u00b7mit", "ich", "wah\u00b7rer", "dei\u00b7ne", "trau\u00b7er", "tei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nie wird ein warmer anruf mich empfangen &#183;               ", "tokens": ["Nie", "wird", "ein", "war\u00b7mer", "an\u00b7ruf", "mich", "emp\u00b7fan\u00b7gen", "&#183;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis in die sp&#228;ten stunden unsres bundes               ", "tokens": ["Bis", "in", "die", "sp", "&#228;", "ten", "stun\u00b7den", "uns\u00b7res", "bun\u00b7des"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "FM", "FM", "FM", "FM", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Muss ich erkennen mit ergebnem bangen               ", "tokens": ["Muss", "ich", "er\u00b7ken\u00b7nen", "mit", "er\u00b7geb\u00b7nem", "ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das herbe schicksal winterlichen fundes.             ", "tokens": ["Das", "her\u00b7be", "schick\u00b7sal", "win\u00b7ter\u00b7li\u00b7chen", "fun\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}