n", "weis\u00b7sen", "Dampf", "ent\u00b7str\u00f6\u00b7mend", "fuhr", "der", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vorbei und f\u00fchrte mich dahin im Wagen.", "tokens": ["Vor\u00b7bei", "und", "f\u00fchr\u00b7te", "mich", "da\u00b7hin", "im", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "VVFIN", "PPER", "PAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ohn' Abenteuer ging der Wintertag,", "tokens": ["Ohn'", "A\u00b7bent\u00b7eu\u00b7er", "ging", "der", "Win\u00b7ter\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Freuden gab der Sommer mir nicht eine,", "tokens": ["Und", "Freu\u00b7den", "gab", "der", "Som\u00b7mer", "mir", "nicht", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich, der sie liebe, wie's auch kommen mag,", "tokens": ["Ich", ",", "der", "sie", "lie\u00b7be", ",", "wie's", "auch", "kom\u00b7men", "mag", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im L\u00e4rm des Tages wie im Mondenscheine.", "tokens": ["Im", "L\u00e4rm", "des", "Ta\u00b7ges", "wie", "im", "Mon\u00b7den\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ohn' Abenteuer ging der Wintertag,", "tokens": ["Ohn'", "A\u00b7bent\u00b7eu\u00b7er", "ging", "der", "Win\u00b7ter\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Im Sommer war bei mir der Gram alleine.", "tokens": ["Im", "Som\u00b7mer", "war", "bei", "mir", "der", "Gram", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "APPR", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Undankbar menschlich Herz, erinnre dich,", "tokens": ["Un\u00b7dank\u00b7bar", "menschlich", "Herz", ",", "e\u00b7rinn\u00b7re", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Du neugeschaffner Gentleman, der fl\u00fcchtet,", "tokens": ["Du", "neu\u00b7ge\u00b7schaff\u00b7ner", "Gent\u00b7le\u00b7man", ",", "der", "fl\u00fcch\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erinnre dich! Hier hat der Glaube dich", "tokens": ["E\u00b7rinn\u00b7re", "dich", "!", "Hier", "hat", "der", "Glau\u00b7be", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VAFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Fern von dem Schmutz der St\u00