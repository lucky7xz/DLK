{"textgrid.poem.49478": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[von neuem schaute triumphierend ich]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von neuem schaute triumphierend ich", "tokens": ["Von", "neu\u00b7em", "schau\u00b7te", "tri\u00b7um\u00b7phie\u00b7rend", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "ADJD", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Stadt, die einst mich hielt in dunklen Schauern", "tokens": ["Die", "Stadt", ",", "die", "einst", "mich", "hielt", "in", "dunk\u00b7len", "Schau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Ungl\u00fcck dr\u00fcckte tief zu Boden mich,", "tokens": ["Mein", "Un\u00b7gl\u00fcck", "dr\u00fcck\u00b7te", "tief", "zu", "Bo\u00b7den", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer z\u00e4hlt die Seufzer, wer ermass mein Trauern?", "tokens": ["Wer", "z\u00e4hlt", "die", "Seuf\u00b7zer", ",", "wer", "er\u00b7mass", "mein", "Trau\u00b7ern", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von neuem schaute triumphierend ich", "tokens": ["Von", "neu\u00b7em", "schau\u00b7te", "tri\u00b7um\u00b7phie\u00b7rend", "ich"], "token_