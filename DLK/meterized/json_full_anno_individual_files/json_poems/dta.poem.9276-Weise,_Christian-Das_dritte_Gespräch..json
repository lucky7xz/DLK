{"dta.poem.9276": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Wjr w\u00fcnschen alle reich zu werden/", "tokens": ["Wjr", "w\u00fcn\u00b7schen", "al\u00b7le", "reich", "zu", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wissen nicht was reichthum ist/", "tokens": ["Und", "wis\u00b7sen", "nicht", "was", "reicht\u00b7hum", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "FM", "FM", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df man bey solchen koth der erden/", "tokens": ["Da\u00df", "man", "bey", "sol\u00b7chen", "koth", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des himmels gar zu leicht vergist/", "tokens": ["Des", "him\u00b7mels", "gar", "zu", "leicht", "ver\u00b7gist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKA", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und an der sch\u00f6nsten perlen stat/", "tokens": ["Und", "an", "der", "sch\u00f6ns\u00b7ten", "per\u00b7len", "stat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur spreu und staub in h\u00e4nden hat.", "tokens": ["Nur", "spreu", "und", "staub", "in", "h\u00e4n\u00b7den", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "APPR", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. GOtt hat uns ja so viel versprochen", "tokens": ["Gott", "hat", "uns", "ja", "so", "viel", "ver\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als woll\u2019er unser Vater seyn.", "tokens": ["Als", "woll'\u00b7er", "un\u00b7ser", "Va\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der segen bleibt auch ungebrochen/", "tokens": ["Der", "se\u00b7gen", "bleibt", "auch", "un\u00b7ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und stellt sich alle stunden ein.", "tokens": ["Und", "stellt", "sich", "al\u00b7le", "stun\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PIAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Derhalben ach aus was f\u00fcr noth", "tokens": ["Der\u00b7hal\u00b7ben", "ach", "aus", "was", "f\u00fcr", "noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "APPR", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bck\u00fcmmern wir uns um das brod.", "tokens": ["Bck\u00fcm\u00b7mern", "wir", "uns", "um", "das", "brod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Je mehr wir geld im kasten haben/", "tokens": ["Je", "mehr", "wir", "geld", "im", "kas\u00b7ten", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "APPRART", "ADJA", "VAFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Je mehr begehren wir darzu;", "tokens": ["Je", "mehr", "be\u00b7geh\u00b7ren", "wir", "dar\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und also st\u00f6ren uns die gaben", "tokens": ["Und", "al\u00b7so", "st\u00f6\u00b7ren", "uns", "die", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur an der s\u00fcssen lebens-ruh:", "tokens": ["Nur", "an", "der", "s\u00fcs\u00b7sen", "le\u00b7bens\u00b7ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sonst der aller\u00e4rmste mann", "tokens": ["Da", "sonst", "der", "al\u00b7le\u00b7r\u00e4rms\u00b7te", "mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gar sanfft und sicher schlaffen kan.", "tokens": ["Gar", "sanfft", "und", "si\u00b7cher", "schlaf\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Und \u00fcber di\u00df/ die armen leute", "tokens": ["Und", "\u00fc\u00b7ber", "di\u00df", "/", "die", "ar\u00b7men", "leu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "PDS", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind auff den abend gleich so satt/", "tokens": ["Sind", "auff", "den", "a\u00b7bend", "gleich", "so", "satt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da dennoch mancher kaum auff heute", "tokens": ["Da", "den\u00b7noch", "man\u00b7cher", "kaum", "auff", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "ADV", "APPR", "ADV"], "meter": "-+-+-+++-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Den proviant zu finden hat.", "tokens": ["Den", "pro\u00b7vi\u00b7ant", "zu", "fin\u00b7den", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum liegt es nicht an geld und gut", "tokens": ["Drum", "liegt", "es", "nicht", "an", "geld", "und", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df uns das leben sanffte thut.", "tokens": ["Da\u00df", "uns", "das", "le\u00b7ben", "sanff\u00b7te", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVINF", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Die reichen k\u00f6nnen mehr nicht essen/", "tokens": ["Die", "rei\u00b7chen", "k\u00f6n\u00b7nen", "mehr", "nicht", "es\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als was in ihren magen geht:", "tokens": ["Als", "was", "in", "ih\u00b7ren", "ma\u00b7gen", "geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der ist schon richtig abgemessen/", "tokens": ["Der", "ist", "schon", "rich\u00b7tig", "ab\u00b7ge\u00b7mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn gleich der tiseh voll speisen steht/", "tokens": ["Wenn", "gleich", "der", "ti\u00b7seh", "voll", "spei\u00b7sen", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "VVINF", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Der appetit wird nur verf\u00fchrt/", "tokens": ["Der", "ap\u00b7pe\u00b7tit", "wird", "nur", "ver\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und manche sch\u00fcssel nicht ber\u00fchrt.", "tokens": ["Und", "man\u00b7che", "sch\u00fcs\u00b7sel", "nicht", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Die meisten wollen ihren kindern", "tokens": ["Die", "meis\u00b7ten", "wol\u00b7len", "ih\u00b7ren", "kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch ihren geitz beh\u00fclfflich seyn/", "tokens": ["Durch", "ih\u00b7ren", "geitz", "be\u00b7h\u00fclf\u00b7flich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch diese lassen sich verhindern/", "tokens": ["Doch", "die\u00b7se", "las\u00b7sen", "sich", "ver\u00b7hin\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bilden sich viel sachen ein/", "tokens": ["Und", "bil\u00b7den", "sich", "viel", "sa\u00b7chen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "VVFIN", "ART", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Da\u00df man bey solchem \u00fcbermuth", "tokens": ["Da\u00df", "man", "bey", "sol\u00b7chem", "\u00fc\u00b7ber\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein geld erwirbt und viel verthut.", "tokens": ["Kein", "geld", "er\u00b7wirbt", "und", "viel", "ver\u00b7thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Ach sind die kinder unsers gleichen/", "tokens": ["Ach", "sind", "die", "kin\u00b7der", "un\u00b7sers", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So m\u00f6gen sie dasselbe thun/", "tokens": ["So", "m\u00f6\u00b7gen", "sie", "das\u00b7sel\u00b7be", "thun", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDAT", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wodurch wir unser gut erreichen.", "tokens": ["Wo\u00b7durch", "wir", "un\u00b7ser", "gut", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man darff nicht auff dem polster ruhn/", "tokens": ["Man", "darff", "nicht", "auff", "dem", "pols\u00b7ter", "ruhn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wer die faulheit auch begehrt/", "tokens": ["Und", "wer", "die", "faul\u00b7heit", "auch", "be\u00b7gehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der ist kaum eines dreyers werth.", "tokens": ["Der", "ist", "kaum", "ei\u00b7nes", "drey\u00b7ers", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Es bleibt darbey/ das ist das beste:", "tokens": ["Es", "bleibt", "dar\u00b7bey", "/", "das", "ist", "das", "bes\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$(", "PDS", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gott lasse mich vergn\u00fcgsam seyn.", "tokens": ["Gott", "las\u00b7se", "mich", "ver\u00b7gn\u00fcg\u00b7sam", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Je mehr ich mich auff erden m\u00e4ste/", "tokens": ["Je", "mehr", "ich", "mich", "auff", "er\u00b7den", "m\u00e4s\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "APPR", "NN", "ADJA", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Je schwerer geh\u2019 ich himmel ein.", "tokens": ["Je", "schwe\u00b7rer", "geh'", "ich", "him\u00b7mel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn ich sterbe/ k\u00f6mmt das geld", "tokens": ["Und", "wenn", "ich", "ster\u00b7be", "/", "k\u00f6mmt", "das", "geld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$(", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht neben mir in jene welt.", "tokens": ["Nicht", "ne\u00b7ben", "mir", "in", "je\u00b7ne", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}