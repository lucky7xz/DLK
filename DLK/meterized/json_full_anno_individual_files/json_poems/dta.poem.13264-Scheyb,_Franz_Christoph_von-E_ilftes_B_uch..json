{"dta.poem.13264": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas aber diesen Flug, den Adler-Flug belangt,", "tokens": ["\u201e", "was", "a\u00b7ber", "die\u00b7sen", "Flug", ",", "den", "Ad\u00b7ler\u00b7Flug", "be\u00b7langt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "PDAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eso wei\u00df ich nicht was man daraus zu ziehn verlangt;", "tokens": ["\u201e", "so", "wei\u00df", "ich", "nicht", "was", "man", "da\u00b7raus", "zu", "ziehn", "ver\u00b7langt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKNEG", "PWS", "PIS", "PAV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eer wohnt ja bey dem Thron seit etlich hundert Jahren;", "tokens": ["\u201e", "er", "wohnt", "ja", "bey", "dem", "Thron", "seit", "et\u00b7lich", "hun\u00b7dert", "Jah\u00b7ren", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "APPR", "ADJD", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eob ihm gleich manches Mahl die Wetter schrecklich waren.", "tokens": ["\u201e", "ob", "ihm", "gleich", "man\u00b7ches", "Mahl", "die", "Wet\u00b7ter", "schreck\u00b7lich", "wa\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "PIAT", "NN", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "405\u201dJezt aber kam der Feind so nah, da\u00df er mit Zwang", "tokens": ["\"", "Jezt", "a\u00b7ber", "kam", "der", "Feind", "so", "nah", ",", "da\u00df", "er", "mit", "Zwang"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ejhn von der Vater-Stadt, von seinem Nest verdrang.", "tokens": ["\u201e", "jhn", "von", "der", "Va\u00b7ter\u00b7Stadt", ",", "von", "sei\u00b7nem", "Nest", "ver\u00b7drang", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201egewalt und Uebermacht nicht vorgesehner Waffen", "tokens": ["\u201e", "ge\u00b7walt", "und", "Ue\u00b7ber\u00b7macht", "nicht", "vor\u00b7ge\u00b7seh\u00b7ner", "Waf\u00b7fen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "KON", "NN", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ekann F\u00fcrsten von dem Thron, warum nicht Adler? schaffen.", "tokens": ["\u201e", "kann", "F\u00fcrs\u00b7ten", "von", "dem", "Thron", ",", "wa\u00b7rum", "nicht", "Ad\u00b7ler", "?", "schaf\u00b7fen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VMFIN", "NN", "APPR", "ART", "NN", "$,", "PWAV", "PTKNEG", "NN",