{"textgrid.poem.61381": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ich dich lie\u00df, du wundersch\u00f6nes Weib,", "tokens": ["Da", "ich", "dich", "lie\u00df", ",", "du", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom dumpfen Stundenschlag hinweggetrieben,", "tokens": ["Vom", "dum\u00b7pfen", "Stun\u00b7den\u00b7schlag", "hin\u00b7weg\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da schied von dir der staubgeborne Leib,", "tokens": ["Da", "schied", "von", "dir", "der", "staub\u00b7ge\u00b7bor\u00b7ne", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch ist die Seel' in deiner Haft geblieben.", "tokens": ["Doch", "ist", "die", "Seel'", "in", "dei\u00b7ner", "Haft", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Sinnen, Sehnen, die Gedanken all", "tokens": ["Mein", "Sin\u00b7nen", ",", "Seh\u00b7nen", ",", "die", "Ge\u00b7dan\u00b7ken", "all"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "ART", "NN", "PIAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Umflattern dich, verspottend Schlo\u00df und Riegel,", "tokens": ["Um\u00b7flat\u00b7tern", "dich", ",", "ver\u00b7spot\u00b7tend", "Schlo\u00df", "und", "Rie\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVPP", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja, selbst der Gaukler Traum ward dein Vasall,", "tokens": ["Ja", ",", "selbst", "der", "Gauk\u00b7ler", "Traum", "ward", "dein", "Va\u00b7sall", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein Bild allein noch zeigt sein Wunderspiegel.", "tokens": ["Dein", "Bild", "al\u00b7lein", "noch", "zeigt", "sein", "Wun\u00b7der\u00b7spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So bin ich dein bei Tag, so bleib' ich dein,", "tokens": ["So", "bin", "ich", "dein", "bei", "Tag", ",", "so", "bleib'", "ich", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "APPR", "NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Nacht und Schlaf auf meinen Wimpern liegen;", "tokens": ["Wenn", "Nacht", "und", "Schlaf", "auf", "mei\u00b7nen", "Wim\u00b7pern", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word"