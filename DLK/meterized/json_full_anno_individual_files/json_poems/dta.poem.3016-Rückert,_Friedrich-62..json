{"dta.poem.3016": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du r\u00fcstest dich umsonst mit allgemeinen S\u00e4tzen,", "tokens": ["Du", "r\u00fcs\u00b7test", "dich", "um\u00b7sonst", "mit", "all\u00b7ge\u00b7mei\u00b7nen", "S\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um ein Besondres draus mir folgernd anzuschw\u00e4tzen.", "tokens": ["Um", "ein", "Be\u00b7sond\u00b7res", "draus", "mir", "fol\u00b7gernd", "an\u00b7zu\u00b7schw\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PAV", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dir gieng der Vordersatz nur als unschuldig hin,", "tokens": ["Dir", "gieng", "der", "Vor\u00b7der\u00b7satz", "nur", "als", "un\u00b7schul\u00b7dig", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "KOUS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ich die T\u00fccke die sich barg nicht sah darin.", "tokens": ["Weil", "ich", "die", "T\u00fc\u00b7cke", "die", "sich", "barg", "nicht", "sah", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "PRF", "VVFIN", "PTKNEG", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun ziehst du Waffen vor aus seinem holen Bauch,", "tokens": ["Nun", "ziehst", "du", "Waf\u00b7fen", "vor", "aus", "sei\u00b7nem", "ho\u00b7len", "Bauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und brauchst sie gegen mich, ein schlechter Kriegsgebrauch.", "tokens": ["Und", "brauchst", "sie", "ge\u00b7gen", "mich", ",", "ein", "schlech\u00b7ter", "Kriegs\u00b7ge\u00b7brauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch hilft dir darum nicht dein Leeres voller T\u00fccke;", "tokens": ["Doch", "hilft", "dir", "da\u00b7rum", "nicht", "dein", "Lee\u00b7res", "vol\u00b7ler", "T\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "PTKNEG", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Frieden, den ich schlo\u00df, nehm' ich mit Fug zur\u00fccke.", "tokens": ["Den", "Frie\u00b7den", ",", "den", "ich", "schlo\u00df", ",", "nehm'", "ich", "mit", "Fug", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich schlage nur zur\u00fcck die wirkliche Gefahr,", "tokens": ["Ich", "schla\u00b7ge", "nur", "zu\u00b7r\u00fcck", "die", "wirk\u00b7li\u00b7che", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und frage gar nicht nach dem Grund der sie gebar.", "tokens": ["Und", "fra\u00b7ge", "gar", "nicht", "nach", "dem", "Grund", "der", "sie", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "APPR", "ART", "NN", "ART", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Beweisen k\u00f6nntest du, ich m\u00fc\u00dft' es dir erlauben,", "tokens": ["Be\u00b7wei\u00b7sen", "k\u00f6nn\u00b7test", "du", ",", "ich", "m\u00fc\u00dft'", "es", "dir", "er\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Tag sei Nacht; allein was zwingt mich es zu glauben?", "tokens": ["Der", "Tag", "sei", "Nacht", ";", "al\u00b7lein", "was", "zwingt", "mich", "es", "zu", "glau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$.", "ADV", "PWS", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Du folgerst aus dem Grund die Wahrheit deines Fundes,", "tokens": ["Du", "fol\u00b7gerst", "aus", "dem", "Grund", "die", "Wahr\u00b7heit", "dei\u00b7nes", "Fun\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ich aus deinem Fund die Falschheit nur des Grundes.", "tokens": ["Doch", "ich", "aus", "dei\u00b7nem", "Fund", "die", "Falschheit", "nur", "des", "Grun\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}