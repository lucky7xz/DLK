{"dta.poem.11131": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein unartiges frauenzimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Caspiam bekommt, braucht keinen bienenstock,", "tokens": ["Wer", "Cas\u00b7pi\u00b7am", "be\u00b7kommt", ",", "braucht", "kei\u00b7nen", "bie\u00b7nen\u00b7stock", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er braucht kein hackebrett und keine r\u00fcbehacke:", "tokens": ["Er", "braucht", "kein", "hac\u00b7ke\u00b7brett", "und", "kei\u00b7ne", "r\u00fc\u00b7be\u00b7ha\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "PIAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn sie am fr\u00f6mmsten ist, so sieht sie wie ein bock;", "tokens": ["Wenn", "sie", "am", "fr\u00f6mms\u00b7ten", "ist", ",", "so", "sieht", "sie", "wie", "ein", "bock", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn sie am sch\u00f6nsten ist, so gleicht sie meinem sacke.", "tokens": ["Wenn", "sie", "am", "sch\u00f6ns\u00b7ten", "ist", ",", "so", "gleicht", "sie", "mei\u00b7nem", "sa\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}