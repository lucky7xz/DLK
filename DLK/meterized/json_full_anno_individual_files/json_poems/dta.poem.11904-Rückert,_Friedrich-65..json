{"dta.poem.11904": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Alles mag in Gott, in Allem Gott betrachten,", "tokens": ["Wer", "Al\u00b7les", "mag", "in", "Gott", ",", "in", "Al\u00b7lem", "Gott", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "APPR", "NN", "$,", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat keinen Grund ein Ding gro\u00df oder klein zu achten.", "tokens": ["Hat", "kei\u00b7nen", "Grund", "ein", "Ding", "gro\u00df", "o\u00b7der", "klein", "zu", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie sollte scheinen ihm ein Allergr\u00f6stes gro\u00df,", "tokens": ["Wie", "soll\u00b7te", "schei\u00b7nen", "ihm", "ein", "Al\u00b7ler\u00b7gr\u00f6s\u00b7tes", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "VVFIN", "PPER", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da es ein Kleinstes ist vorm Einziggro\u00dfen blo\u00df?", "tokens": ["Da", "es", "ein", "Kleins\u00b7tes", "ist", "vorm", "Ein\u00b7zig\u00b7gro\u00b7\u00dfen", "blo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VAFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie d\u00fcrfte gelten ihm das Allerkleinste klein,", "tokens": ["Wie", "d\u00fcrf\u00b7te", "gel\u00b7ten", "ihm", "das", "Al\u00b7ler\u00b7kleins\u00b7te", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "VVINF", "PPER", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da mit dem Gr\u00f6sten es hat Gottes Geist gemein?", "tokens": ["Da", "mit", "dem", "Gr\u00f6s\u00b7ten", "es", "hat", "Got\u00b7tes", "Geist", "ge\u00b7mein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VAFIN", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nach deiner Einsicht nur erhebest du zumeist", "tokens": ["Nach", "dei\u00b7ner", "Ein\u00b7sicht", "nur", "er\u00b7he\u00b7best", "du", "zu\u00b7meist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das, was am klarsten dir abspiegelt Gottes Geist.", "tokens": ["Das", ",", "was", "am", "klars\u00b7ten", "dir", "ab\u00b7spie\u00b7gelt", "Got\u00b7tes", "Geist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPRART", "ADJA", "PPER", "VVFIN", "NN", "NN", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Je h\u00f6her aber selbst wird deine Einsicht steigen,", "tokens": ["Je", "h\u00f6\u00b7her", "a\u00b7ber", "selbst", "wird", "dei\u00b7ne", "Ein\u00b7sicht", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADV", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je klarer wird der Geist in Allem dir sich zeigen.", "tokens": ["Je", "kla\u00b7rer", "wird", "der", "Geist", "in", "Al\u00b7lem", "dir", "sich", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "APPR", "PIS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Des B\u00f6sen Schein ists, was des Guten Glanz verh\u00e4lt;", "tokens": ["Des", "B\u00f6\u00b7sen", "Schein", "ists", ",", "was", "des", "Gu\u00b7ten", "Glanz", "ver\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zerst\u00f6r das B\u00f6s' in dir, so siehst du gut die Welt.", "tokens": ["Zer\u00b7st\u00f6r", "das", "B\u00f6s'", "in", "dir", ",", "so", "siehst", "du", "gut", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}