{"dta.poem.21710": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Agathon an  Telxione Thelxione .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zauber, welcher neunmal mich umwunden,", "tokens": ["Zau\u00b7ber", ",", "wel\u00b7cher", "neun\u00b7mal", "mich", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Talisman, der meine Kr\u00e4fte band,", "tokens": ["Ta\u00b7lis\u00b7man", ",", "der", "mei\u00b7ne", "Kr\u00e4f\u00b7te", "band", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Welches D\u00e4mons Hauch' bist du zerschwunden,", "tokens": ["Wel\u00b7ches", "D\u00e4\u00b7mons", "Hauch'", "bist", "du", "zer\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bist gesprengt von welches Heros Hand?", "tokens": ["Bist", "ge\u00b7sprengt", "von", "wel\u00b7ches", "He\u00b7ros", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "PWAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Weggeblasen ist der Kerkerbrodem,", "tokens": ["Weg\u00b7ge\u00b7bla\u00b7sen", "ist", "der", "Ker\u00b7ker\u00b7bro\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Welcher schw\u00fcl und \u00e4ngstend auf mir lag", "tokens": ["Wel\u00b7cher", "schw\u00fcl", "und", "\u00e4ngs\u00b7tend", "auf", "mir", "lag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Lebensluft umweht mich, Lebensodem;", "tokens": ["Le\u00b7bens\u00b7luft", "um\u00b7weht", "mich", ",", "Le\u00b7ben\u00b7so\u00b7dem", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Golden gl\u00e4nzt mir der entflorte Tag.", "tokens": ["Gol\u00b7den", "gl\u00e4nzt", "mir", "der", "ent\u00b7flor\u00b7te", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wo sind nun die t\u00e4uschenden Gebilde,", "tokens": ["Wo", "sind", "nun", "die", "t\u00e4u\u00b7schen\u00b7den", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wo die Gaukel meiner Fantasie?", "tokens": ["Wo", "die", "Gau\u00b7kel", "mei\u00b7ner", "Fan\u00b7ta\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo die F\u00fcll' und Frisch' und Huld und Milde,", "tokens": ["Wo", "die", "F\u00fcll'", "und", "Frisch'", "und", "Huld", "und", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Falsche, die des Dichters Wahn dir lieh?", "tokens": ["Fal\u00b7sche", ",", "die", "des", "Dich\u00b7ters", "Wahn", "dir", "lieh", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wo des hohen Ideales Z\u00fcge,", "tokens": ["Wo", "des", "ho\u00b7hen", "I\u00b7dea\u00b7les", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Das sein Rausch in dir verwirklicht sah?", "tokens": ["Das", "sein", "Rausch", "in", "dir", "ver\u00b7wirk\u00b7licht", "sah", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Mit dem Rausche schwand des Rausches L\u00fcge,", "tokens": ["Mit", "dem", "Rau\u00b7sche", "schwand", "des", "Rau\u00b7sches", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und entzaubert stehst du vor mir da.", "tokens": ["Und", "ent\u00b7zau\u00b7bert", "stehst", "du", "vor", "mir", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wie? dem Geist Praxiteles entrungen", "tokens": ["Wie", "?", "dem", "Geist", "Pra\u00b7xi\u00b7te\u00b7les", "ent\u00b7run\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "ART", "NN", "NE", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00e4tte sich s\u00fcssschmerzend diese Frau?", "tokens": ["H\u00e4t\u00b7te", "sich", "s\u00fcss\u00b7schmer\u00b7zend", "die\u00b7se", "Frau", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aus dem Meissel Polyklet's entsprungen", "tokens": ["Aus", "dem", "Meis\u00b7sel", "Po\u00b7ly\u00b7klet's", "ent\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4r' unstr\u00e4flich dieser Gliederbau?", "tokens": ["W\u00e4r'", "uns\u00b7tr\u00e4f\u00b7lich", "die\u00b7ser", "Glie\u00b7der\u00b7bau", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Diese Formen trotzten jedem Tadel?", "tokens": ["Die\u00b7se", "For\u00b7men", "trotz\u00b7ten", "je\u00b7dem", "Ta\u00b7del", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dieses Auge sonder Ruh und Glanz,", "tokens": ["Die\u00b7ses", "Au\u00b7ge", "son\u00b7der", "Ruh", "und", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Diese Stirne sonder Sinn und Adel", "tokens": ["Die\u00b7se", "Stir\u00b7ne", "son\u00b7der", "Sinn", "und", "A\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "K\u00e4mpften mit Niobens um den Kranz?", "tokens": ["K\u00e4mpf\u00b7ten", "mit", "Ni\u00b7o\u00b7bens", "um", "den", "Kranz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Dieses, w\u00e4hnt' ich, sey der Wuchs Dionens?", "tokens": ["Die\u00b7ses", ",", "w\u00e4hnt'", "ich", ",", "sey", "der", "Wuchs", "Di\u00b7o\u00b7nens", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dies der Flug, den Atalanta flog?", "tokens": ["Dies", "der", "Flug", ",", "den", "A\u00b7tal\u00b7an\u00b7ta", "flog", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "$,", "ART", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dies der Marmorbusen Hermionens,", "tokens": ["Dies", "der", "Mar\u00b7mor\u00b7bu\u00b7sen", "Her\u00b7mi\u00b7o\u00b7nens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Draus Orest Heroen-Frohheit sog?", "tokens": ["Draus", "O\u00b7rest", "He\u00b7ro\u00b7en\u00b7Froh\u00b7heit", "sog", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dies der Honigreiche Mund Athenens,", "tokens": ["Dies", "der", "Ho\u00b7ni\u00b7grei\u00b7che", "Mund", "A\u00b7the\u00b7nens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dem Verst\u00e4ndigkeit und G\u00fct' entquoll?", "tokens": ["Dem", "Ver\u00b7st\u00e4n\u00b7dig\u00b7keit", "und", "G\u00fct'", "ent\u00b7quoll", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Dies die Tinten Anadyomenens,", "tokens": ["Dies", "die", "Tin\u00b7ten", "A\u00b7nad\u00b7yo\u00b7me\u00b7nens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NE", "$,"], "meter": "--+--+---", "measure": "anapaest.di.plus"}, "line.8": {"text": "Als sie blendend dem Gesch\u00e4um' entschwoll?", "tokens": ["Als", "sie", "blen\u00b7dend", "dem", "Ge\u00b7sch\u00e4um'", "ent\u00b7schwoll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Also l\u00e4chelten die Charitinnen", "tokens": ["Al\u00b7so", "l\u00e4\u00b7chel\u00b7ten", "die", "Cha\u00b7ri\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jedem Horcher durch das Ohr ins Herz?", "tokens": ["Je\u00b7dem", "Hor\u00b7cher", "durch", "das", "Ohr", "ins", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Also wechselten die Pierinnen", "tokens": ["Al\u00b7so", "wech\u00b7sel\u00b7ten", "die", "Pie\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Spielend frohen Ernst und weisen Scherz?", "tokens": ["Spie\u00b7lend", "fro\u00b7hen", "Ernst", "und", "wei\u00b7sen", "Scherz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Aus des Aethers reinstem Duft gewoben", "tokens": ["Aus", "des", "A\u00b7e\u00b7thers", "reins\u00b7tem", "Duft", "ge\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "W\u00e4re diese Seele, dieser Leib? \u2014", "tokens": ["W\u00e4\u00b7re", "die\u00b7se", "See\u00b7le", ",", "die\u00b7ser", "Leib", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$,", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Nein, die G\u00f6ttinn ist in Dunst zerstoben,", "tokens": ["Nein", ",", "die", "G\u00f6t\u00b7tinn", "ist", "in", "Dunst", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und geblieben ist ein sterblich Weib.", "tokens": ["Und", "ge\u00b7blie\u00b7ben", "ist", "ein", "sterb\u00b7lich", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Dejanirens Lieb', Ismenens G\u00fcte,", "tokens": ["De\u00b7ja\u00b7ni\u00b7rens", "Lieb'", ",", "Is\u00b7me\u00b7nens", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NE", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Iphigeniens himmelklarer Sinn,", "tokens": ["Ip\u00b7hi\u00b7ge\u00b7ni\u00b7ens", "him\u00b7mel\u00b7kla\u00b7rer", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jede Unschuld, jede Sch\u00f6ne bl\u00fchte,", "tokens": ["Je\u00b7de", "Un\u00b7schuld", ",", "je\u00b7de", "Sch\u00f6\u00b7ne", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie ich w\u00e4hnt', in dieser Heuchlerinn!", "tokens": ["Wie", "ich", "w\u00e4hnt'", ",", "in", "die\u00b7ser", "Heuch\u00b7le\u00b7rinn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jeder T\u00fccke, w\u00e4hnt' ich, jedes Zwanges", "tokens": ["Je\u00b7der", "T\u00fc\u00b7cke", ",", "w\u00e4hnt'", "ich", ",", "je\u00b7des", "Zwan\u00b7ges"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "VVFIN", "PPER", "$,", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sey sie ledig; blank und frank und frey", "tokens": ["Sey", "sie", "le\u00b7dig", ";", "blank", "und", "frank", "und", "frey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "PTKVZ", "KON", "ADJD", "KON", "ADJD"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Sey nicht Daphne werther des Gesanges", "tokens": ["Sey", "nicht", "Daph\u00b7ne", "wert\u00b7her", "des", "Ge\u00b7san\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "NN", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Als es diese meines Hymnus sey.", "tokens": ["Als", "es", "die\u00b7se", "mei\u00b7nes", "Hym\u00b7nus", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wie der K\u00fcnstler an dem Ideale", "tokens": ["Wie", "der", "K\u00fcnst\u00b7ler", "an", "dem", "I\u00b7dea\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seines Geistes h\u00e4ngt mit s\u00fcssem Hang,", "tokens": ["Sei\u00b7nes", "Geis\u00b7tes", "h\u00e4ngt", "mit", "s\u00fcs\u00b7sem", "Hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie aus Hebens nektarvoller Schale", "tokens": ["Wie", "aus", "He\u00b7bens", "nek\u00b7tar\u00b7vol\u00b7ler", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der Alcide die Verg\u00f6tt'rung trank;", "tokens": ["Der", "Al\u00b7ci\u00b7de", "die", "Ver\u00b7g\u00f6tt'\u00b7rung", "trank", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Also hing an ih