{"dta.poem.16070": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "124.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Anmuth, Freundlichkeit, Gef\u00e4lligkeit und Milde", "tokens": ["Wer", "An\u00b7muth", ",", "Freund\u00b7lich\u00b7keit", ",", "Ge\u00b7f\u00e4l\u00b7lig\u00b7keit", "und", "Mil\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht braucht in seinem Haus, doch drau\u00dfen f\u00fchrt im Schilde,", "tokens": ["Nicht", "braucht", "in", "sei\u00b7nem", "Haus", ",", "doch", "drau\u00b7\u00dfen", "f\u00fchrt", "im", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit diesen Tugenden ist er nicht reich bedacht,", "tokens": ["Mit", "die\u00b7sen", "Tu\u00b7gen\u00b7den", "ist", "er", "nicht", "reich", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil er zum Feierkleid und Festtagschmuck sie macht.", "tokens": ["Weil", "er", "zum", "Fei\u00b7er\u00b7kleid", "und", "Fest\u00b7tagsc\u00b7hmuck", "sie", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er sucht nur vor der Welt mit seinem Flitterputze", "tokens": ["Er", "sucht", "nur", "vor", "der", "Welt", "mit", "sei\u00b7nem", "Flit\u00b7ter\u00b7put\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu gl\u00e4nzen, und daheim geht er in seinem Schmutze.", "tokens": ["Zu", "gl\u00e4n\u00b7zen", ",", "und", "da\u00b7heim", "geht", "er", "in", "sei\u00b7nem", "Schmut\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}