{"textgrid.poem.67037": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Stella", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gute Stella! w\u00e4hnest du mich begl\u00fcckt,", "tokens": ["Du", "gu\u00b7te", "Stel\u00b7la", "!", "w\u00e4h\u00b7nest", "du", "mich", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wann ich im Tale still und verlassen, und", "tokens": ["Wann", "ich", "im", "Ta\u00b7le", "still", "und", "ver\u00b7las\u00b7sen", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "PTKVZ", "KON", "VVINF", "$,", "KON"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von dir vergessen wandle, wann in", "tokens": ["Von", "dir", "ver\u00b7ges\u00b7sen", "wand\u00b7le", ",", "wann", "in"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "VVFIN", "$,", "PWAV", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fl\u00fcchtigen Freuden dein Leben hinh\u00fcpft?", "tokens": ["Fl\u00fcch\u00b7ti\u00b7gen", "Freu\u00b7den", "dein", "Le\u00b7ben", "hin\u00b7h\u00fcpft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Schon oft, wenn meine Br\u00fcder, die Gl\u00fcckliche,", "tokens": ["Schon", "oft", ",", "wenn", "mei\u00b7ne", "Br\u00fc\u00b7der", ",", "die", "Gl\u00fcck\u00b7li\u00b7che", ",