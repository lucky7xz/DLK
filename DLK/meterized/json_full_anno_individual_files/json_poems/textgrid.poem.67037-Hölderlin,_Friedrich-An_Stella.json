"word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Empfindungen, der Freuden, recht zu", "tokens": ["Emp\u00b7fin\u00b7dun\u00b7gen", ",", "der", "Freu\u00b7den", ",", "recht", "zu"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "$,", "ADJD", "PTKZU"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Handeln, so viele, der gute Sch\u00f6pfer:", "tokens": ["Han\u00b7deln", ",", "so", "vie\u00b7le", ",", "der", "gu\u00b7te", "Sch\u00f6p\u00b7fer", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PIS", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Doch gibt es W\u00fcnsche, denen der Sp\u00f6tter h\u00f6hnt \u2013", "tokens": ["Doch", "gibt", "es", "W\u00fcn\u00b7sche", ",", "de\u00b7nen", "der", "Sp\u00f6t\u00b7ter", "h\u00f6hnt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Stella! du nicht! h\u00f6hne dem Armen nicht! \u2013", "tokens": ["O", "Stel\u00b7la", "!", "du", "nicht", "!", "h\u00f6h\u00b7ne", "dem", "Ar\u00b7men", "nicht", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$.", "PPER", "PTKNEG", "$.", "VVFIN", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gibt unerf\u00fcllte W\u00fcnsche \u2013 \u2013 Tugend,", "tokens": ["Gibt", "un\u00b7er\u00b7f\u00fcll\u00b7te", "W\u00fcn\u00b7sche", "\u2013", "\u2013", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hehre Gef\u00e4hrtin! du kennst die W\u00fcnsche.", "tokens": ["Heh\u00b7re", "Ge\u00b7f\u00e4hr\u00b7tin", "!", "du", "kennst", "die", "W\u00fcn\u00b7sche", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Ach la\u00df mich weinen! \u2013 nein! ich will heiter sein!", "tokens": ["Ach", "la\u00df", "mich", "wei\u00b7nen", "!", "\u2013", "nein", "!", "ich", "will", "hei\u00b7ter", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "VVINF", "$.", "$(", "PTKANT", "$.", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist ja nimmer gew\u00fcnscht wird, wo", "tokens": ["Ist", "ja", "nim\u00b7mer", "ge\u00b7w\u00fcnscht", "wird", ",", "wo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "VAFIN", "$,", "PWAV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Der Sterbliche sein Schicksal preiset, \u2013", "tokens": ["Der", "Sterb\u00b7li\u00b7che", "sein", "Schick\u00b7sal", "prei\u00b7set", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort ist es, wo ich dich wiedersehe.", "tokens": ["Dort", "ist", "es", ",", "wo", "ich", "dich", "wie\u00b7der\u00b7se\u00b7he", "."], "token_info": [