 "PPER", "ADV", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach langem Sehnen, endlich erl\u00f6st zu sein,", "tokens": ["Nach", "lan\u00b7gem", "Seh\u00b7nen", ",", "end\u00b7lich", "er\u00b7l\u00f6st", "zu", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und s\u00e4he dich als Pilger nimmer,", "tokens": ["Und", "s\u00e4\u00b7he", "dich", "als", "Pil\u00b7ger", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stella! so seh ich dich jenseits wieder.", "tokens": ["Stel\u00b7la", "!", "so", "seh", "ich", "dich", "jen\u00b7seits", "wie\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}