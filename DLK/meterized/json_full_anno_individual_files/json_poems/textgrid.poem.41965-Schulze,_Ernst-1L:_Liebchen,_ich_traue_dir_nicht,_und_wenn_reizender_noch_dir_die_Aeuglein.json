{"textgrid.poem.41965": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebchen, ich traue dir nicht, und wenn reizender noch dir die Aeuglein", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebchen, ich traue dir nicht, und wenn reizender noch dir die Aeuglein", "tokens": ["Lieb\u00b7chen", ",", "ich", "trau\u00b7e", "dir", "nicht", ",", "und", "wenn", "rei\u00b7zen\u00b7der", "noch", "dir", "die", "A\u00b7e\u00b7u\u00b7glein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "KOUS", "ADJD", "ADV", "PPER", "ART", "NN"], "meter": "+--+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Blickten, die nur dein Geist lenket und nimmer dein Herz;", "tokens": ["Blick\u00b7ten", ",", "die", "nur", "dein", "Geist", "len\u00b7ket", "und", "nim\u00b7mer", "dein", "Herz", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "L\u00e4cheltest du viel freundlicher auch, zwar ist es unm\u00f6glich,", "tokens": ["L\u00e4\u00b7chel\u00b7test", "du", "viel", "freund\u00b7li\u00b7cher", "auch", ",", "zwar", "ist", "es", "un\u00b7m\u00f6g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und dein schmeichelndes Wort w\u00e4r' es auch doppelt so s\u00fc\u00df.", "tokens": ["Und", "dein", "schmei\u00b7cheln\u00b7des", "Wort", "w\u00e4r'", "es", "auch", "dop\u00b7pelt", "so", "s\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADJD", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Oft schon riefst du zur\u00fcck den Entfliehenden; reuig gehorcht' er,", "tokens": ["Oft", "schon", "riefst", "du", "zu\u00b7r\u00fcck", "den", "Ent\u00b7flie\u00b7hen\u00b7den", ";", "reu\u00b7ig", "ge\u00b7horcht'", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "ADJD", "VVFIN", "PPER", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Und schnell, wenn du gebotst, linderte Zorn sich und Schmerz.", "tokens": ["Und", "schnell", ",", "wenn", "du", "ge\u00b7botst", ",", "lin\u00b7der\u00b7te", "Zorn", "sich", "und", "Schmerz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADJA", "NN", "PRF", "KON", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Dein nur war der Triumph und mein schuldlose Bestrafung,", "tokens": ["Dein", "nur", "war", "der", "Tri\u00b7umph", "und", "mein", "schuld\u00b7lo\u00b7se", "Be\u00b7stra\u00b7fung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VAFIN", "ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und ich b\u00fc\u00dfte, wo du fl\u00fcchtiges Wesen gefehlt.", "tokens": ["Und", "ich", "b\u00fc\u00df\u00b7te", ",", "wo", "du", "fl\u00fcch\u00b7ti\u00b7ges", "We\u00b7sen", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Doch bald panzerte wieder mit t\u00f6dtlichem Frost sich die Brust dir;", "tokens": ["Doch", "bald", "pan\u00b7zer\u00b7te", "wie\u00b7der", "mit", "t\u00f6dt\u00b7li\u00b7chem", "Frost", "sich", "die", "Brust", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN", "PRF", "ART", "NN", "PPER", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Schmeichler schaltest du den, der dir ge\u00f6ffnet das Herz,", "tokens": ["Schmeich\u00b7ler", "schal\u00b7test", "du", "den", ",", "der", "dir", "ge\u00b7\u00f6ff\u00b7net", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "$,", "PRELS", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Z\u00fcrntest dem k\u00fchneren Freund und verlachtest stolz den Verzagten:", "tokens": ["Z\u00fcrn\u00b7test", "dem", "k\u00fch\u00b7ne\u00b7ren", "Freund", "und", "ver\u00b7lach\u00b7test", "stolz", "den", "Ver\u00b7zag\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Nur wer muthig entfloh, zwang dich mit eigener List.", "tokens": ["Nur", "wer", "mut\u00b7hig", "ent\u00b7floh", ",", "zwang", "dich", "mit", "ei\u00b7ge\u00b7ner", "List", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADJD", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "So viel h\u00fcpfende Funken entspr\u00fchn dem lodernden Heerd nicht,", "tokens": ["So", "viel", "h\u00fcp\u00b7fen\u00b7de", "Fun\u00b7ken", "ent\u00b7spr\u00fchn", "dem", "lo\u00b7dern\u00b7den", "Heerd", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "So viel Wellen umhegt nicht das unendliche Meer,", "tokens": ["So", "viel", "Wel\u00b7len", "um\u00b7hegt", "nicht", "das", "un\u00b7end\u00b7li\u00b7che", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Als feindselige Launen sich dir im Busen bek\u00e4mpfen;", "tokens": ["Als", "feind\u00b7se\u00b7li\u00b7ge", "Lau\u00b7nen", "sich", "dir", "im", "Bu\u00b7sen", "be\u00b7k\u00e4mp\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PRF", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-++-+--+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Sklavin bist du dir selbst; Liebchen, err\u00f6thest du nicht?", "tokens": ["Skla\u00b7vin", "bist", "du", "dir", "selbst", ";", "Lieb\u00b7chen", ",", "er\u00b7r\u00f6\u00b7thest", "du", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "ADV", "$.", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.17": {"text": "Nur wer fest die begonnene Bahn fortwandelt, beherrscht sich,", "tokens": ["Nur", "wer", "fest", "die", "be\u00b7gon\u00b7ne\u00b7ne", "Bahn", "fort\u00b7wan\u00b7delt", ",", "be\u00b7herrscht", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,", "VVFIN", "PRF", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Treue, wie Ketten von Gold fesselt und schm\u00fcckt sie zugleich.", "tokens": ["Treu\u00b7e", ",", "wie", "Ket\u00b7ten", "von", "Gold", "fes\u00b7selt", "und", "schm\u00fcckt", "sie", "zu\u00b7gleich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "APPR", "NN", "ADJD", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Wie? du l\u00e4chelst mich an und spottest keck der Vermahnung?", "tokens": ["Wie", "?", "du", "l\u00e4\u00b7chelst", "mich", "an", "und", "spot\u00b7test", "keck", "der", "Ver\u00b7mah\u00b7nung", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-++--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Siehe dich vor, fast schon w\u00fcnsch' ich dein Sch\u00fcler zu seyn.", "tokens": ["Sie\u00b7he", "dich", "vor", ",", "fast", "schon", "w\u00fcn\u00b7sch'", "ich", "dein", "Sch\u00fc\u00b7ler", "zu", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.21": {"text": "Winke mir nur, ich folge dir gern, und, bist du auch treulos,", "tokens": ["Win\u00b7ke", "mir", "nur", ",", "ich", "fol\u00b7ge", "dir", "gern", ",", "und", ",", "bist", "du", "auch", "treu\u00b7los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "$,", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Ist uns die Wahrheit fern, immer doch reizet der Schein.", "tokens": ["Ist", "uns", "die", "Wahr\u00b7heit", "fern", ",", "im\u00b7mer", "doch", "rei\u00b7zet", "der", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.23": {"text": "Schmeichle mir nur und k\u00fcsse mich oft; nie soll es mir weh thun,", "tokens": ["Schmeich\u00b7le", "mir", "nur", "und", "k\u00fcs\u00b7se", "mich", "oft", ";", "nie", "soll", "es", "mir", "weh", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "$.", "ADV", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+-+--+-+-++-", "measure": "iambic.septa.invert"}, "line.24": {"text": "Da\u00df nicht Liebe, da\u00df nur Laune mich gl\u00fccklicher macht.", "tokens": ["Da\u00df", "nicht", "Lie\u00b7be", ",", "da\u00df", "nur", "Lau\u00b7ne", "mich", "gl\u00fcck\u00b7li\u00b7cher", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "NN", "$,", "KOUS", "ADV", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Rasch den Genu\u00df des entfliehenden Jetzt fortraffen ist Weisheit,", "tokens": ["Rasch", "den", "Ge\u00b7nu\u00df", "des", "ent\u00b7flie\u00b7hen\u00b7den", "Jetzt", "for\u00b7traf\u00b7fen", "ist", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "ADV", "VVPP", "VAFIN", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Und die Erinnrung bleibt immer ein dauerndes Gut.", "tokens": ["Und", "die", "E\u00b7rinn\u00b7rung", "bleibt", "im\u00b7mer", "ein", "dau\u00b7ern\u00b7des", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "---+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Doch wenn ich still hinsink' in den Rausch gluthvoller Umarmung,", "tokens": ["Doch", "wenn", "ich", "still", "hin\u00b7sink'", "in", "den", "Rausch", "gluth\u00b7vol\u00b7ler", "Um\u00b7ar\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Wenn hochklopfend das Herz jegliche Fessel zersprengt,", "tokens": ["Wenn", "hoch\u00b7klop\u00b7fend", "das", "Herz", "jeg\u00b7li\u00b7che", "Fes\u00b7sel", "zer\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+---+---+--+", "measure": "iambic.tetra.chol"}, "line.29": {"text": "Wenn dein ewiger Ku\u00df auf dem Flug der gewaltigen Sehnsucht", "tokens": ["Wenn", "dein", "e\u00b7wi\u00b7ger", "Ku\u00df", "auf", "dem", "Flug", "der", "ge\u00b7wal\u00b7ti\u00b7gen", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.30": {"text": "Hoch den erbebenden Geist hebt in den Himmel der Lust,", "tokens": ["Hoch", "den", "er\u00b7be\u00b7ben\u00b7den", "Geist", "hebt", "in", "den", "Him\u00b7mel", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.31": {"text": "Kann ich auch dann im beth\u00f6renden Wahn, o kann ich auch dann noch", "tokens": ["Kann", "ich", "auch", "dann", "im", "be\u00b7th\u00f6\u00b7ren\u00b7den", "Wahn", ",", "o", "kann", "ich", "auch", "dann", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,", "FM", "VMFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.32": {"text": "Rufen das frostige Wort: Liebchen, ich traue dir nicht!", "tokens": ["Ru\u00b7fen", "das", "fros\u00b7ti\u00b7ge", "Wort", ":", "Lieb\u00b7chen", ",", "ich", "trau\u00b7e", "dir", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Liebchen, ich traue dir nicht, und wenn reizender noch dir die Aeuglein", "tokens": ["Lieb\u00b7chen", ",", "ich", "trau\u00b7e", "dir", "nicht", ",", "und", "wenn", "rei\u00b7zen\u00b7der", "noch", "dir", "die", "A\u00b7e\u00b7u\u00b7glein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "KOUS", "ADJD", "ADV", "PPER", "ART", "NN"], "meter": "+--+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Blickten, die nur dein Geist lenket und nimmer dein Herz;", "tokens": ["Blick\u00b7ten", ",", "die", "nur", "dein", "Geist", "len\u00b7ket", "und", "nim\u00b7mer", "dein", "Herz", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "L\u00e4cheltest du viel freundlicher auch, zwar ist es unm\u00f6glich,", "tokens": ["L\u00e4\u00b7chel\u00b7test", "du", "viel", "freund\u00b7li\u00b7cher", "auch", ",", "zwar", "ist", "es", "un\u00b7m\u00f6g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und dein schmeichelndes Wort w\u00e4r' es auch doppelt so s\u00fc\u00df.", "tokens": ["Und", "dein", "schmei\u00b7cheln\u00b7des", "Wort", "w\u00e4r'", "es", "auch", "dop\u00b7pelt", "so", "s\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADJD", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Oft schon riefst du zur\u00fcck den Entfliehenden; reuig gehorcht' er,", "tokens": ["Oft", "schon", "riefst", "du", "zu\u00b7r\u00fcck", "den", "Ent\u00b7flie\u00b7hen\u00b7den", ";", "reu\u00b7ig", "ge\u00b7horcht'", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "ADJD", "VVFIN", "PPER", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Und schnell, wenn du gebotst, linderte Zorn sich und Schmerz.", "tokens": ["Und", "schnell", ",", "wenn", "du", "ge\u00b7botst", ",", "lin\u00b7der\u00b7te", "Zorn", "sich", "und", "Schmerz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADJA", "NN", "PRF", "KON", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Dein nur war der Triumph und mein schuldlose Bestrafung,", "tokens": ["Dein", "nur", "war", "der", "Tri\u00b7umph", "und", "mein", "schuld\u00b7lo\u00b7se", "Be\u00b7stra\u00b7fung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "VAFIN", "ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und ich b\u00fc\u00dfte, wo du fl\u00fcchtiges Wesen gefehlt.", "tokens": ["Und", "ich", "b\u00fc\u00df\u00b7te", ",", "wo", "du", "fl\u00fcch\u00b7ti\u00b7ges", "We\u00b7sen", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Doch bald panzerte wieder mit t\u00f6dtlichem Frost sich die Brust dir;", "tokens": ["Doch", "bald", "pan\u00b7zer\u00b7te", "wie\u00b7der", "mit", "t\u00f6dt\u00b7li\u00b7chem", "Frost", "sich", "die", "Brust", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN", "PRF", "ART", "NN", "PPER", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Schmeichler schaltest du den, der dir ge\u00f6ffnet das Herz,", "tokens": ["Schmeich\u00b7ler", "schal\u00b7test", "du", "den", ",", "der", "dir", "ge\u00b7\u00f6ff\u00b7net", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "$,", "PRELS", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Z\u00fcrntest dem k\u00fchneren Freund und verlachtest stolz den Verzagten:", "tokens": ["Z\u00fcrn\u00b7test", "dem", "k\u00fch\u00b7ne\u00b7ren", "Freund", "und", "ver\u00b7lach\u00b7test", "stolz", "den", "Ver\u00b7zag\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Nur wer muthig entfloh, zwang dich mit eigener List.", "tokens": ["Nur", "wer", "mut\u00b7hig", "ent\u00b7floh", ",", "zwang", "dich", "mit", "ei\u00b7ge\u00b7ner", "List", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADJD", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "So viel h\u00fcpfende Funken entspr\u00fchn dem lodernden Heerd nicht,", "tokens": ["So", "viel", "h\u00fcp\u00b7fen\u00b7de", "Fun\u00b7ken", "ent\u00b7spr\u00fchn", "dem", "lo\u00b7dern\u00b7den", "Heerd", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "So viel Wellen umhegt nicht das unendliche Meer,", "tokens": ["So", "viel", "Wel\u00b7len", "um\u00b7hegt", "nicht", "das", "un\u00b7end\u00b7li\u00b7che", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Als feindselige Launen sich dir im Busen bek\u00e4mpfen;", "tokens": ["Als", "feind\u00b7se\u00b7li\u00b7ge", "Lau\u00b7nen", "sich", "dir", "im", "Bu\u00b7sen", "be\u00b7k\u00e4mp\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PRF", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-++-+--+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Sklavin bist du dir selbst; Liebchen, err\u00f6thest du nicht?", "tokens": ["Skla\u00b7vin", "bist", "du", "dir", "selbst", ";", "Lieb\u00b7chen", ",", "er\u00b7r\u00f6\u00b7thest", "du", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "ADV", "$.", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.17": {"text": "Nur wer fest die begonnene Bahn fortwandelt, beherrscht sich,", "tokens": ["Nur", "wer", "fest", "die", "be\u00b7gon\u00b7ne\u00b7ne", "Bahn", "fort\u00b7wan\u00b7delt", ",", "be\u00b7herrscht", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,", "VVFIN", "PRF", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Treue, wie Ketten von Gold fesselt und schm\u00fcckt sie zugleich.", "tokens": ["Treu\u00b7e", ",", "wie", "Ket\u00b7ten", "von", "Gold", "fes\u00b7selt", "und", "schm\u00fcckt", "sie", "zu\u00b7gleich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "APPR", "NN", "ADJD", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Wie? du l\u00e4chelst mich an und spottest keck der Vermahnung?", "tokens": ["Wie", "?", "du", "l\u00e4\u00b7chelst", "mich", "an", "und", "spot\u00b7test", "keck", "der", "Ver\u00b7mah\u00b7nung", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-++--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Siehe dich vor, fast schon w\u00fcnsch' ich dein Sch\u00fcler zu seyn.", "tokens": ["Sie\u00b7he", "dich", "vor", ",", "fast", "schon", "w\u00fcn\u00b7sch'", "ich", "dein", "Sch\u00fc\u00b7ler", "zu", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.21": {"text": "Winke mir nur, ich folge dir gern, und, bist du auch treulos,", "tokens": ["Win\u00b7ke", "mir", "nur", ",", "ich", "fol\u00b7ge", "dir", "gern", ",", "und", ",", "bist", "du", "auch", "treu\u00b7los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "$,", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Ist uns die Wahrheit fern, immer doch reizet der Schein.", "tokens": ["Ist", "uns", "die", "Wahr\u00b7heit", "fern", ",", "im\u00b7mer", "doch", "rei\u00b7zet", "der", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.23": {"text": "Schmeichle mir nur und k\u00fcsse mich oft; nie soll es mir weh thun,", "tokens": ["Schmeich\u00b7le", "mir", "nur", "und", "k\u00fcs\u00b7se", "mich", "oft", ";", "nie", "soll", "es", "mir", "weh", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ADV", "$.", "ADV", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+-+--+-+-++-", "measure": "iambic.septa.invert"}, "line.24": {"text": "Da\u00df nicht Liebe, da\u00df nur Laune mich gl\u00fccklicher macht.", "tokens": ["Da\u00df", "nicht", "Lie\u00b7be", ",", "da\u00df", "nur", "Lau\u00b7ne", "mich", "gl\u00fcck\u00b7li\u00b7cher", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "NN", "$,", "KOUS", "ADV", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Rasch den Genu\u00df des entfliehenden Jetzt fortraffen ist Weisheit,", "tokens": ["Rasch", "den", "Ge\u00b7nu\u00df", "des", "ent\u00b7flie\u00b7hen\u00b7den", "Jetzt", "for\u00b7traf\u00b7fen", "ist", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "ADV", "VVPP", "VAFIN", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Und die Erinnrung bleibt immer ein dauerndes Gut.", "tokens": ["Und", "die", "E\u00b7rinn\u00b7rung", "bleibt", "im\u00b7mer", "ein", "dau\u00b7ern\u00b7des", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "---+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Doch wenn ich still hinsink' in den Rausch gluthvoller Umarmung,", "tokens": ["Doch", "wenn", "ich", "still", "hin\u00b7sink'", "in", "den", "Rausch", "gluth\u00b7vol\u00b7ler", "Um\u00b7ar\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Wenn hochklopfend das Herz jegliche Fessel zersprengt,", "tokens": ["Wenn", "hoch\u00b7klop\u00b7fend", "das", "Herz", "jeg\u00b7li\u00b7che", "Fes\u00b7sel", "zer\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+---+---+--+", "measure": "iambic.tetra.chol"}, "line.29": {"text": "Wenn dein ewiger Ku\u00df auf dem Flug der gewaltigen Sehnsucht", "tokens": ["Wenn", "dein", "e\u00b7wi\u00b7ger", "Ku\u00df", "auf", "dem", "Flug", "der", "ge\u00b7wal\u00b7ti\u00b7gen", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.30": {"text": "Hoch den erbebenden Geist hebt in den Himmel der Lust,", "tokens": ["Hoch", "den", "er\u00b7be\u00b7ben\u00b7den", "Geist", "hebt", "in", "den", "Him\u00b7mel", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.31": {"text": "Kann ich auch dann im beth\u00f6renden Wahn, o kann ich auch dann noch", "tokens": ["Kann", "ich", "auch", "dann", "im", "be\u00b7th\u00f6\u00b7ren\u00b7den", "Wahn", ",", "o", "kann", "ich", "auch", "dann", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,", "FM", "VMFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.32": {"text": "Rufen das frostige Wort: Liebchen, ich traue dir nicht!", "tokens": ["Ru\u00b7fen", "das", "fros\u00b7ti\u00b7ge", "Wort", ":", "Lieb\u00b7chen", ",", "ich", "trau\u00b7e", "dir", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}