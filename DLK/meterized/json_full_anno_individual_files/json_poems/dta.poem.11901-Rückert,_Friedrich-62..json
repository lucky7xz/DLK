{"dta.poem.11901": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich gut in guter Zeit gehts Dummen wie Gescheiten,", "tokens": ["Gleich", "gut", "in", "gu\u00b7ter", "Zeit", "gehts", "Dum\u00b7men", "wie", "Ge\u00b7schei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weit besser diesen doch, wann kommen b\u00f6se Zeiten.", "tokens": ["Weit", "bes\u00b7ser", "die\u00b7sen", "doch", ",", "wann", "kom\u00b7men", "b\u00f6\u00b7se", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PDAT", "ADV", "$,", "PWAV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Solang' im T\u00fcmpfel frisch das Wasser war, da schl\u00fcpfte", "tokens": ["So\u00b7lang'", "im", "T\u00fcmp\u00b7fel", "frisch", "das", "Was\u00b7ser", "war", ",", "da", "schl\u00fcpf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPRART", "NN", "ADJD", "ART", "NN", "VAFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So froh darin der Fisch, alswie der Frosch drin h\u00fcpfte.", "tokens": ["So", "froh", "da\u00b7rin", "der", "Fisch", ",", "als\u00b7wie", "der", "Frosch", "drin", "h\u00fcpf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "ART", "NN", "$,", "KOKOM", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch als verr\u00e4therisch in Sommerglut erlosch", "tokens": ["Doch", "als", "ver\u00b7r\u00e4\u00b7the\u00b7risch", "in", "Som\u00b7mer\u00b7glut", "er\u00b7losch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Flut, kam um der Fisch, und weiter sprang der Frosch.", "tokens": ["Die", "Flut", ",", "kam", "um", "der", "Fisch", ",", "und", "wei\u00b7ter", "sprang", "der", "Frosch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drum klage nicht ein Mann, wenn Nahrungsquellen schwinden,", "tokens": ["Drum", "kla\u00b7ge", "nicht", "ein", "Mann", ",", "wenn", "Nah\u00b7rungs\u00b7quel\u00b7len", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "ART", "NN", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der leicht wo anders kann ein Unterkommen finden.", "tokens": ["Der", "leicht", "wo", "an\u00b7ders", "kann", "ein", "Un\u00b7ter\u00b7kom\u00b7men", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PWAV", "ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}