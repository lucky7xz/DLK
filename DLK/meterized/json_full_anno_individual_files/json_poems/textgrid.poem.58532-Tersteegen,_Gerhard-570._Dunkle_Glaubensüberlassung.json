{"textgrid.poem.58532": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "570. Dunkle Glaubens\u00fcberlassung", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt h\u00e4lt mein Freund sich auf im Kabinett inwendig,", "tokens": ["Jetzt", "h\u00e4lt", "mein", "Freund", "sich", "auf", "im", "Ka\u00b7bi\u00b7nett", "in\u00b7wen\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "APPR", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich nicht zu sehn und nicht zu wissen tracht';", "tokens": ["Ich", "nicht", "zu", "sehn", "und", "nicht", "zu", "wis\u00b7sen", "tracht'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PTKZU", "VVINF", "KON", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Sehn macht mich zum Sehn unt\u00fcchtig und elendig,", "tokens": ["Mein", "Sehn", "macht", "mich", "zum", "Sehn", "un\u00b7t\u00fcch\u00b7tig", "und", "e\u00b7len\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich kann nicht gehn hinein, ich mu\u00df am T\u00fcrlein wachen,", "tokens": ["Ich", "kann", "nicht", "gehn", "hin\u00b7ein", ",", "ich", "mu\u00df", "am", "T\u00fcr\u00b7lein", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "PTKVZ", "$,", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit seinem Tun vergn\u00fcgt, la\u00df ich ihn immer machen.", "tokens": ["Mit", "sei\u00b7nem", "Tun", "ver\u00b7gn\u00fcgt", ",", "la\u00df", "ich", "ihn", "im\u00b7mer", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "VVIMP", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch in mir bin ich arm und blo\u00df,", "tokens": ["Doch", "in", "mir", "bin", "ich", "arm", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PPER", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die D\u00fcrr- und Dunkelheit ist gro\u00df,", "tokens": ["Die", "D\u00fcrr", "und", "Dun\u00b7kel\u00b7heit", "ist", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich soll dennoch nicht weinen und nicht klagen,", "tokens": ["Ich", "soll", "den\u00b7noch", "nicht", "wei\u00b7nen", "und", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nicht sehen um, nicht f\u00fcrchten und nicht fragen:", "tokens": ["Nicht", "se\u00b7hen", "um", ",", "nicht", "f\u00fcrch\u00b7ten", "und", "nicht", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "PTKVZ", "$,", "PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "\u00bbwo bin ich hier, Ist dies der rechte Pfad?\u00ab", "tokens": ["\u00bb", "wo", "bin", "ich", "hier", ",", "Ist", "dies", "der", "rech\u00b7te", "Pfad", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "ADV", "$,", "VAFIN", "PDS", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ich leb' so hin auf Gottes Gnad',", "tokens": ["Ich", "leb'", "so", "hin", "auf", "Got\u00b7tes", "Gnad'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Das Ruder ist nicht mehr in meiner Hand,", "tokens": ["Das", "Ru\u00b7der", "ist", "nicht", "mehr", "in", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Gott wei\u00df, wo noch mein Schifflein findet Land;", "tokens": ["Gott", "wei\u00df", ",", "wo", "noch", "mein", "Schif\u00b7flein", "fin\u00b7det", "Land", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ich bin zufrieden doch in dieser meiner Pein,", "tokens": ["Ich", "bin", "zu\u00b7frie\u00b7den", "doch", "in", "die\u00b7ser", "mei\u00b7ner", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "APPR", "PDAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die \u00dcberlassung mu\u00df jetzt ohne Schranken sein.", "tokens": ["Die", "\u00dc\u00b7berl\u00b7as\u00b7sung", "mu\u00df", "jetzt", "oh\u00b7ne", "Schran\u00b7ken", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jetzt h\u00e4lt mein Freund sich auf im Kabinett inwendig,", "tokens": ["Jetzt", "h\u00e4lt", "mein", "Freund", "sich", "auf", "im", "Ka\u00b7bi\u00b7nett", "in\u00b7wen\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "APPR", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich nicht zu sehn und nicht zu wissen tracht';", "tokens": ["Ich", "nicht", "zu", "sehn", "und", "nicht", "zu", "wis\u00b7sen", "tracht'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PTKZU", "VVINF", "KON", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Sehn macht mich zum Sehn unt\u00fcchtig und elendig,", "tokens": ["Mein", "Sehn", "macht", "mich", "zum", "Sehn", "un\u00b7t\u00fcch\u00b7tig", "und", "e\u00b7len\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich kann nicht gehn hinein, ich mu\u00df am T\u00fcrlein wachen,", "tokens": ["