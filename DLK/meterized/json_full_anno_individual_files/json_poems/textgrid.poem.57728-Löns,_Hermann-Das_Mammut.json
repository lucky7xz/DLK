{"textgrid.poem.57728": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das Mammut", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief unten im Moore das Mammut bl\u00e4st,", "tokens": ["Tief", "un\u00b7ten", "im", "Moo\u00b7re", "das", "Mam\u00b7mut", "bl\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Man h\u00f6rt es hoch oben auf der Geest;", "tokens": ["Man", "h\u00f6rt", "es", "hoch", "o\u00b7ben", "auf", "der", "Geest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aus allen Lagern rund um das Moor", "tokens": ["Aus", "al\u00b7len", "La\u00b7gern", "rund", "um", "das", "Moor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Eilen bewaffnete M\u00e4nner hervor,", "tokens": ["Ei\u00b7len", "be\u00b7waff\u00b7ne\u00b7te", "M\u00e4n\u00b7ner", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Gefolgt von der mageren Meute;", "tokens": ["Ge\u00b7folgt", "von", "der", "ma\u00b7ge\u00b7ren", "Meu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Jagdtag, Fleischtag ist heute.", "tokens": ["Jagd\u00b7tag", ",", "Fleischtag", "ist", "heu\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die Kinder spielen das sch\u00f6ne Spiel", "tokens": ["Die", "Kin\u00b7der", "spie\u00b7len", "das", "sch\u00f6\u00b7ne", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom Mammut, das in die Grube fiel;", "tokens": ["Vom", "Mam\u00b7mut", ",", "das", "in", "die", "Gru\u00b7be", "fiel", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das dickste M\u00e4dchen mu\u00df Mammut sein,", "tokens": ["Das", "dicks\u00b7te", "M\u00e4d\u00b7chen", "mu\u00df", "Mam\u00b7mut", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Erdloch sitzen und lauthals schrei'n,", "tokens": ["Im", "Erd\u00b7loch", "sit\u00b7zen", "und", "laut\u00b7hals", "schrei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die ganz kleinen Kinder sind Meute;", "tokens": ["Die", "ganz", "klei\u00b7nen", "Kin\u00b7der", "sind", "Meu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Jagdtag, Spieltag ist heute.", "tokens": ["Jagd\u00b7tag", ",", "Spiel\u00b7tag", "ist", "heu\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Der Zaubersmann geht wichtig einher:", "tokens": ["Der", "Zau\u00b7bers\u00b7mann", "geht", "wich\u00b7tig", "ein\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Jaja, jaja, wenn ich nicht w\u00e4r'!", "tokens": ["Ja\u00b7ja", ",", "ja\u00b7ja", ",", "wenn", "ich", "nicht", "w\u00e4r'", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "KOUS", "PPER", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich machte gestern den gro\u00dfen Wind,", "tokens": ["Ich", "mach\u00b7te", "ge\u00b7stern", "den", "gro\u00b7\u00dfen", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da wurde das Untier dumm und blind,", "tokens": ["Da", "wur\u00b7de", "das", "Un\u00b7tier", "dumm", "und", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nun denket auch meiner, ihr Leute;", "tokens": ["Nun", "den\u00b7ket", "auch", "mei\u00b7ner", ",", "ihr", "Leu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Jagdtag, Zahltag ist heute.", "tokens": ["Jagd\u00b7tag", ",", "Zahl\u00b7tag", "ist", "heu\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "Das Mammut trompetet in Angst und Not,", "tokens": ["Das", "Mam\u00b7mut", "trom\u00b7pe\u00b7tet", "in", "Angst", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Von allen Seiten naht sich der Tod;", "tokens": ["Von", "al\u00b7len", "Sei\u00b7ten", "naht", "sich", "der", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Es zischt der Pfeil, es saust der Speer,", "tokens": ["Es", "zischt", "der", "Pfeil", ",", "es", "saust", "der", "Speer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Qu\u00e4lgeister werden immer noch mehr,", "tokens": ["Der", "Qu\u00e4l\u00b7geis\u00b7ter", "wer\u00b7den", "im\u00b7mer", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wie Ameisen bei ihrer Beute;", "tokens": ["Wie", "A\u00b7mei\u00b7sen", "bei", "ih\u00b7rer", "Beu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Jagdtag, Schlachttag ist heute.", "tokens": ["Jagd\u00b7tag", ",", "Schlacht\u00b7tag", "ist", "heu\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Der H\u00e4uptling teilt richtig und br\u00fcderlich,", "tokens": ["Der", "H\u00e4upt\u00b7ling", "teilt", "rich\u00b7tig", "und", "br\u00fc\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Die M\u00fcrbebraten beh\u00e4lt er f\u00fcr sich;", "tokens": ["Die", "M\u00fcr\u00b7be\u00b7bra\u00b7ten", "be\u00b7h\u00e4lt", "er", "f\u00fcr", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PRF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein jeder sackt seinen Anteil ein,", "tokens": ["Ein", "je\u00b7der", "sackt", "sei\u00b7nen", "An\u00b7teil", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des Zauberers St\u00fcck, das ist grade nicht klein,", "tokens": ["Des", "Zau\u00b7be\u00b7rers", "St\u00fcck", ",", "das", "ist", "gra\u00b7de", "nicht", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PDS", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Am Gerippe balgt sich die Meute;", "tokens": ["Am", "Ge\u00b7rip\u00b7pe", "balgt", "sich", "die", "Meu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Jagdtag, Bei\u00dftag ist heute.", "tokens": ["Jagd\u00b7tag", ",", "Bei\u00df\u00b7tag", "ist", "heu\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Was kriecht denn da aus dem Busche heran?", "tokens": ["Was", "kriecht", "denn", "da", "aus", "dem", "Bu\u00b7sche", "he\u00b7ran", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom Nachbarstamme ist es ein Mann.", "tokens": ["Vom", "Nach\u00b7bar\u00b7stam\u00b7me", "ist", "es", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und noch einer kommt und immer noch mehr,", "tokens": ["Und", "noch", "ei\u00b7ner", "kommt", "und", "im\u00b7mer", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "VVFIN", "KON", "ADV", "ADV", "ADV", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Des Mammuts Todesschrei lockte sie her.", "tokens": ["Des", "Mam\u00b7muts", "To\u00b7des\u00b7schrei", "lock\u00b7te", "sie", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Hand von dem Wildbret, ihr Leute!", "tokens": ["Hand", "von", "dem", "Wild\u00b7bret", ",", "ihr", "Leu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Jagdtag, Rauftag ist heute.", "tokens": ["Jagd\u00b7tag", ",", "Rauf\u00b7tag", "ist", "heu\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Wir gruben das Loch und wir hetzten's hinein.", "tokens": ["Wir", "gru\u00b7ben", "das", "Loch", "und", "wir", "hetz\u00b7ten's", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PPER", "NE", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aber wir schlugen's tot und unser soll's sein.", "tokens": ["A\u00b7ber", "wir", "schlu\u00b7gen's", "tot", "und", "un\u00b7ser", "soll's", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "PPOSAT", "PIS", "VAINF", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Es funkeln die Augen bluthungrige Lust,", "tokens": ["Es", "fun\u00b7keln", "die", "Au\u00b7gen", "blut\u00b7hung\u00b7ri\u00b7ge", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ein Krieger f\u00e4llt um, einen Speer in der Brust.", "tokens": ["Ein", "Krie\u00b7ger", "f\u00e4llt", "um", ",", "ei\u00b7nen", "Speer", "in", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Hilfe, zu Hilfe ihr Leute!", "tokens": ["Hil\u00b7fe", ",", "zu", "Hil\u00b7fe", "ihr", "Leu\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Jagdtag, Bluttag ist heute.", "tokens": ["Jagd\u00b7tag", ",", "Blut\u00b7tag", "ist", "heu\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.8": {"line.1": {"text": "Ein Kriegsgeschrei hier, ein Kriegsgeschrei dort,", "tokens": ["Ein", "Kriegs\u00b7ge\u00b7schrei", "hier", ",", "ein", "Kriegs\u00b7ge\u00b7schrei", "dort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die M\u00e4nner werfen die Fleischballen fort;", "tokens": ["Die", "M\u00e4n\u00b7ner", "wer\u00b7fen", "die", "Fleischbal\u00b7len", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es knirscht der Speer, das Beil das kracht,", "tokens": ["Es", "knirscht", "der", "Speer", ",", "das", "Beil", "das", "kracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Zauberer schnell sich von dannen macht,", "tokens": ["Der", "Zau\u00b7be\u00b7rer", "schnell", "sich", "von", "dan\u00b7nen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PRF", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Erwischt noch manch St\u00fcck von der Beute;", "tokens": ["Er\u00b7wischt", "noch", "manch", "St\u00fcck", "von", "der", "Beu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Jagdtag, Rafftag ist heute.", "tokens": ["Jagd\u00b7tag", ",", "Raff\u00b7tag", "ist", "heu\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.9": {"line.1": {"text": "Die Sonne hinter der Geest versinkt,", "tokens": ["Die", "Son\u00b7ne", "hin\u00b7ter", "der", "Geest", "ver\u00b7sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom Lager die Totenklage erklingt;", "tokens": ["Vom", "La\u00b7ger", "die", "To\u00b7ten\u00b7kla\u00b7ge", "er\u00b7klingt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Moore liegt ein vergessener Mann,", "tokens": ["Im", "Moo\u00b7re", "liegt", "ein", "ver\u00b7ges\u00b7se\u00b7ner", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-