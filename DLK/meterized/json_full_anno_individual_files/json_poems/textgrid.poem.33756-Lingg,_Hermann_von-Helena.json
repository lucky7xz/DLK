{"textgrid.poem.33756": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Helena", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Menelaos' goldnem Saale", "tokens": ["In", "Me\u00b7ne\u00b7la\u00b7os'", "gold\u00b7nem", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sa\u00df Nestor's Sohn und Telemach.", "tokens": ["Sa\u00df", "Ne\u00b7stor's", "Sohn", "und", "Te\u00b7le\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie freuten sich mit ihm beim Mahle,", "tokens": ["Sie", "freu\u00b7ten", "sich", "mit", "ihm", "beim", "Mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch als er von Odysseus sprach,", "tokens": ["Doch", "als", "er", "von", "O\u00b7dys\u00b7seus", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Barg in des Mantels Purpurh\u00fclle", "tokens": ["Barg", "in", "des", "Man\u00b7tels", "Pur\u00b7pur\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der J\u00fcngling rasch sein Angesicht", "tokens": ["Der", "J\u00fcng\u00b7ling", "rasch", "sein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und seiner Tr\u00e4nen dunkle F\u00fclle \u2013", "tokens": ["Und", "sei\u00b7ner", "Tr\u00e4\u00b7nen", "dunk\u00b7le", "F\u00fcl\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur Helenen entging es nicht.", "tokens": ["Nur", "He\u00b7le\u00b7nen", "ent\u00b7ging", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Sie kam gleich Artemis geschritten", "tokens": ["Sie", "kam", "gleich", "Ar\u00b7te\u00b7mis", "ge\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NE", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom duftenden Gemach hervor,", "tokens": ["Vom", "duf\u00b7ten\u00b7den", "Ge\u00b7mach", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr stellte an der Tafel Mitten", "tokens": ["Ihr", "stell\u00b7te", "an", "der", "Ta\u00b7fel", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Stuhl der Dienerinnen Chor,", "tokens": ["Den", "Stuhl", "der", "Die\u00b7ne\u00b7rin\u00b7nen", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Teppich brachten sie, den weichen,", "tokens": ["Den", "Tep\u00b7pich", "brach\u00b7ten", "sie", ",", "den", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und eilten, ihrer K\u00f6nigin", "tokens": ["Und", "eil\u00b7ten", ",", "ih\u00b7rer", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Korb von Silber darzureichen,", "tokens": ["Den", "Korb", "von", "Sil\u00b7ber", "dar\u00b7zu\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Spindel und das Garn darin.", "tokens": ["Die", "Spin\u00b7del", "und", "das", "Garn", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und so zu Menelaos wandte", "tokens": ["Und", "so", "zu", "Me\u00b7ne\u00b7laos", "wand\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NE", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Gattin sich von ihrem Thron:", "tokens": ["Die", "Gat\u00b7tin", "sich", "von", "ih\u00b7rem", "Thron", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich den Gast dort recht erkannte,", "tokens": ["Wenn", "ich", "den", "Gast", "dort", "recht", "er\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ist er des Odysseus Sohn.", "tokens": ["So", "ist", "er", "des", "O\u00b7dys\u00b7seus", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er sieht \u2013 ich mu\u00dft' ihn l\u00e4ngst betrachten \u2013", "tokens": ["Er", "sieht", "\u2013", "ich", "mu\u00dft'", "ihn", "l\u00e4ngst", "be\u00b7trach\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sprach Menelaos, ganz ihm gleich,", "tokens": ["Sprach", "Me\u00b7ne\u00b7laos", ",", "ganz", "ihm", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADV", "PPER", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und als des Helden wir gedachten,", "tokens": ["Und", "als", "des", "Hel\u00b7den", "wir", "ge\u00b7dach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ward auch sein Herz von Tr\u00e4nen weich.", "tokens": ["Ward", "auch", "sein", "Herz", "von", "Tr\u00e4\u00b7nen", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es ist so! rief der Nestoride.", "tokens": ["Es", "ist", "so", "!", "rief", "der", "Nes\u00b7to\u00b7ri\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem bei der Herzenssaite Ton", "tokens": ["Dem", "bei", "der", "Her\u00b7zens\u00b7sai\u00b7te", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Tr\u00e4ne bebt' am Augenlide,", "tokens": ["Die", "Tr\u00e4\u00b7ne", "bebt'", "am", "Au\u00b7gen\u00b7li\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist es, des Odysseus Sohn,", "tokens": ["Er", "ist", "es", ",", "des", "O\u00b7dys\u00b7seus", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des vielerfahrnen, reich an Ehren,", "tokens": ["Des", "vie\u00b7ler\u00b7fahr\u00b7nen", ",", "reich", "an", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem ach, noch fern in wilder Flut", "tokens": ["Dem", "ach", ",", "noch", "fern", "in", "wil\u00b7der", "Flut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "XY", "$,", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Heimkehr Tag die G\u00f6tter wehren,", "tokens": ["Der", "Heim\u00b7kehr", "Tag", "die", "G\u00f6t\u00b7ter", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der schon vielleicht im Meere ruht!", "tokens": ["Der", "schon", "viel\u00b7leicht", "im", "Mee\u00b7re", "ruht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O welche Stunde, reich gesegnet,", "tokens": ["O", "wel\u00b7che", "Stun\u00b7de", ",", "reich", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rief Menelaos, bringst du mit,", "tokens": ["Rief", "Me\u00b7ne\u00b7laos", ",", "bringst", "du", "mit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da mir des Mannes Sohn begegnet,", "tokens": ["Da", "mir", "des", "Man\u00b7nes", "Sohn", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der viel f\u00fcr mich erlitt und stritt!", "tokens": ["Der", "viel", "f\u00fcr", "mich", "er\u00b7litt", "und", "stritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie w\u00e4r' er selbst erst mir willkommen!", "tokens": ["Wie", "w\u00e4r'", "er", "selbst", "erst", "mir", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich r\u00e4umte eine Stadt ihm ein,", "tokens": ["Ich", "r\u00e4um\u00b7te", "ei\u00b7ne", "Stadt", "ihm", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So sollt' er bei mir aufgenommen", "tokens": ["So", "sollt'", "er", "bei", "mir", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bis an sein Lebensende sein.", "tokens": ["Bis", "an", "sein", "Le\u00b7ben\u00b7sen\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er sprach es, und in Aller Herzen", "tokens": ["Er", "sprach", "es", ",", "und", "in", "Al\u00b7ler", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drang K\u00fcmmernis und tiefer Gram,", "tokens": ["Drang", "K\u00fcm\u00b7mer\u00b7nis", "und", "tie\u00b7fer", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ein Erinnern aller Schmerzen", "tokens": ["Da\u00df", "ein", "E\u00b7rin\u00b7nern", "al\u00b7ler", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die gro\u00dfen Seelen \u00fcberkam.", "tokens": ["Die", "gro\u00b7\u00dfen", "See\u00b7len", "\u00fc\u00b7ber\u00b7kam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch Helena stand auf und mischte", "tokens": ["Doch", "He\u00b7le\u00b7na", "stand", "auf", "und", "mischte"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Zaubermittel in den Wein,", "tokens": ["Ein", "Zau\u00b7ber\u00b7mit\u00b7tel", "in", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das vom Ged\u00e4chtnis weg verwischte", "tokens": ["Das", "vom", "Ge\u00b7d\u00e4cht\u00b7nis", "weg", "ver\u00b7wischte"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPRART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jedweden Kummer, jede Pein.", "tokens": ["Jed\u00b7we\u00b7den", "Kum\u00b7mer", ",", "je\u00b7de", "Pein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Und alle Ha\u00df- und Zorngedanken,", "tokens": ["Und", "al\u00b7le", "Ha\u00df", "und", "Zorn\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Ungl\u00fccks Macht, der Feinde Hohn", "tokens": ["Des", "Un\u00b7gl\u00fccks", "Macht", ",", "der", "Fein\u00b7de", "Hohn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verga\u00dfen, die den Zauber tranken,", "tokens": ["Ver\u00b7ga\u00b7\u00dfen", ",", "die", "den", "Zau\u00b7ber", "tran\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Helena trank nicht davon.", "tokens": ["Nur", "He\u00b7le\u00b7na", "trank", "nicht", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Blick sah nach des Tores Schwelle,", "tokens": ["Ihr", "Blick", "sah", "nach", "des", "To\u00b7res", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie starrte traumhaft vor sich aus;", "tokens": ["Sie", "starr\u00b7te", "traum\u00b7haft", "vor", "sich", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr war, als leuchte Fackelhelle,", "tokens": ["Ihr", "war", ",", "als", "leuch\u00b7te", "Fa\u00b7ckel\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein sch\u00f6ner J\u00fcngling trat ins Haus.", "tokens": ["Ein", "sch\u00f6\u00b7ner", "J\u00fcng\u00b7ling", "tr