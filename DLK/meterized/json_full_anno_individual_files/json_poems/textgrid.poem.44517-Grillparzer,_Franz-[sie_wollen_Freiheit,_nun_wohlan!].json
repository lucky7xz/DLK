{"textgrid.poem.44517": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[sie wollen Freiheit, nun wohlan!]", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie wollen Freiheit, nun wohlan!", "tokens": ["Sie", "wol\u00b7len", "Frei\u00b7heit", ",", "nun", "wo\u00b7hlan", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebt ihnen eine Eisenbahn,", "tokens": ["Gebt", "ih\u00b7nen", "ei\u00b7ne", "Ei\u00b7sen\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da m\u00f6gen sie denn frei verkehren,", "tokens": ["Da", "m\u00f6\u00b7gen", "sie", "denn", "frei", "ver\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schacher wird sie dienen lehren.", "tokens": ["Der", "Scha\u00b7cher", "wird", "sie", "die\u00b7nen", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie brauchen gl\u00e4ubig einen Gott,", "tokens": ["Sie", "brau\u00b7chen", "gl\u00e4u\u00b7big", "ei\u00b7nen", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Hegel hat des nimmer Spott,", "tokens": ["Herr", "He\u00b7gel", "hat", "des", "nim\u00b7mer", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verdaun sie erst ein Subjekt-Objekt,", "tokens": ["Ver\u00b7daun", "sie", "erst", "ein", "Sub\u00b7jek\u00b7tOb\u00b7jekt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hat nie noch ein Glaube sich weiter erstreckt.", "tokens": ["Hat", "nie", "noch", "ein", "Glau\u00b7be", "sich", "wei\u00b7ter", "er\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Und d\u00fcrstet sie nach Poesie,", "tokens": ["Und", "d\u00fcrs\u00b7tet", "sie", "nach", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Prosa ist verlegen nie,", "tokens": ["Die", "Pro\u00b7sa", "ist", "ver\u00b7le\u00b7gen", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Novelle und Tendenzgedicht", "tokens": ["No\u00b7vel\u00b7le", "und", "Ten\u00b7denz\u00b7ge\u00b7dicht"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ist Poesie und ists auch nicht.", "tokens": ["Ist", "Poe\u00b7sie", "und", "ists", "auch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFI