{"dta.poem.2780": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ejusdem. Desiderium VIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach k\u00f6nte sich mein Haupt in eine Fluth verkehren/", "tokens": ["Ach", "k\u00f6n\u00b7te", "sich", "mein", "Haupt", "in", "ei\u00b7ne", "Fluth", "ver\u00b7keh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es m\u00fcsten Tropffen da wo jetzund Haare stehn/", "tokens": ["Es", "m\u00fcs\u00b7ten", "Tropf\u00b7fen", "da", "wo", "je\u00b7tzund", "Haa\u00b7re", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "ADV", "PWAV", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Stirne w\u00e4r ein Feld durchschwemmt mit heissen Z\u00e4h-", "tokens": ["Die", "Stir\u00b7ne", "w\u00e4r", "ein", "Feld", "durch\u00b7schwemmt", "mit", "heis\u00b7sen", "Z\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVFIN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie m\u00f6chten ungehemmt auf ihren Ufern gehn.", "tokens": ["Sie", "m\u00f6ch\u00b7ten", "un\u00b7ge\u00b7hemmt", "auf", "ih\u00b7ren", "U\u00b7fern", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df die zwey Augen nicht zerrinnen in zwo Quellen/", "tokens": ["Da\u00df", "die", "zwey", "Au\u00b7gen", "nicht", "zer\u00b7rin\u00b7nen", "in", "zwo", "Quel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "CARD", "NN", "PTKNEG", "VVINF", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es wird ein eintzig Strom aus beyden Fl\u00fcssen seyn/", "tokens": ["Es", "wird", "ein", "eint\u00b7zig", "Strom", "aus", "bey\u00b7den", "Fl\u00fcs\u00b7sen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "APPR", "PIAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und br\u00e4chte ", "tokens": ["Und", "br\u00e4ch\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "So nehme meine Fluth den gr\u00f6sten Raum doch ein.", "tokens": ["So", "neh\u00b7me", "mei\u00b7ne", "Fluth", "den", "gr\u00f6s\u00b7ten", "Raum", "doch", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ob dort Andromache schon an dem Felsen thr\u00e4net/", "tokens": ["Ob", "dort", "And\u00b7ro\u00b7ma\u00b7che", "schon", "an", "dem", "Fel\u00b7sen", "thr\u00e4\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "So gleicht ihr gantzes Na\u00df doch meinen Z\u00e4hren nicht/", "tokens": ["So", "gleicht", "ihr", "gant\u00b7zes", "Na\u00df", "doch", "mei\u00b7nen", "Z\u00e4h\u00b7ren", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ob David schwemmt sein Bett und sich unendlich sehnet/", "tokens": ["Ob", "Da\u00b7vid", "schwemmt", "sein", "Bett", "und", "sich", "un\u00b7end\u00b7lich", "seh\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "PPOSAT", "NN", "KON", "PRF", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So h\u00e4lt der Thr\u00e4nen Macht den meinen kaums Gewicht.", "tokens": ["So", "h\u00e4lt", "der", "Thr\u00e4\u00b7nen", "Macht", "den", "mei\u00b7nen", "kaums", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ART", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es mag die Magdalen des HErren F\u00fcsse baden/", "tokens": ["Es", "mag", "die", "Mag\u00b7da\u00b7len", "des", "Her\u00b7ren", "F\u00fcs\u00b7se", "ba\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "In wahrer Hertzens-Reu und ungef\u00e4rbter Bu\u00df/", "tokens": ["In", "wah\u00b7rer", "Hert\u00b7zens\u00b7Reu", "und", "un\u00b7ge\u00b7f\u00e4rb\u00b7ter", "Bu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und Petrus welcher sich mit Meineyd hat beladen/", "tokens": ["Und", "Pet\u00b7rus", "wel\u00b7cher", "sich", "mit", "Mei\u00b7neyd", "hat", "be\u00b7la\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "PRELS", "PRF", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gekr\u00e4nckt von Furcht und Angst zerschmeltzt in einen Flu\u00df.", "tokens": ["Ge\u00b7kr\u00e4nckt", "von", "Furcht", "und", "Angst", "zer\u00b7schmeltzt", "in", "ei\u00b7nen", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So ist mirs nicht genug Ich will des Nilus Gusse/", "tokens": ["So", "ist", "mirs", "nicht", "ge\u00b7nug", "Ich", "will", "des", "Ni\u00b7lus", "Gus\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PTKNEG", "ADV", "PPER", "VMFIN", "ART", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wenn Siebenstr\u00f6hung er der Jsis Aecker netzt/", "tokens": ["Wenn", "Sie\u00b7ben\u00b7str\u00f6\u00b7hung", "er", "der", "Jsis", "A\u00b7e\u00b7cker", "netzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ART", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und dann/ den Wassermann wann er die nassen F\u00fcsse/", "tokens": ["Und", "dann", "/", "den", "Was\u00b7ser\u00b7mann", "wann", "er", "die", "nas\u00b7sen", "F\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ART", "NN", "PWAV", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ins triebe Winter-Jahr mit vollen Kr\u00fcgen setzt.", "tokens": ["Ins", "trie\u00b7be", "Win\u00b7ter\u00b7Jahr", "mit", "vol\u00b7len", "Kr\u00fc\u00b7gen", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie wenn ein Wolckenbruch vom blauen Himmels-Bogen/", "tokens": ["Wie", "wenn", "ein", "Wol\u00b7cken\u00b7bruch", "vom", "blau\u00b7en", "Him\u00b7mels\u00b7Bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mit schwartzem Ungest\u00fcmm erschrecklich sich ergeust/", "tokens": ["Mit", "schwart\u00b7zem", "Un\u00b7ge\u00b7st\u00fcmm", "er\u00b7schreck\u00b7lich", "sich", "er\u00b7geust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Stadt/ D\u00f6rffer/ Feld und Wald die stehn in Wasserwogen/", "tokens": ["Stadt", "/", "D\u00f6rf\u00b7fer", "/", "Feld", "und", "Wald", "die", "stehn", "in", "Was\u00b7ser\u00b7wo\u00b7gen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "NN", "KON", "NN", "ART", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und alles \u00fcberdeckt wohin die Fluth sich rei\u00dft.", "tokens": ["Und", "al\u00b7les", "\u00fc\u00b7berd\u00b7eckt", "wo\u00b7hin", "die", "Fluth", "sich", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So w\u00fcnsch ich da\u00df sich auch der Augen Brunn erhebe/", "tokens": ["So", "w\u00fcnsch", "ich", "da\u00df", "sich", "auch", "der", "Au\u00b7gen", "Brunn", "er\u00b7he\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "PRF", "ADV", "ART", "NN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und da\u00df mein kranckes Haupt sey ein weite See/", "tokens": ["Und", "da\u00df", "mein", "kran\u00b7ckes", "Haupt", "sey", "ein", "wei\u00b7te", "See", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Da\u00df stat der Augen ich zwey B\u00e4che von mir gebe/", "tokens": ["Da\u00df", "stat", "der", "Au\u00b7gen", "ich", "zwey", "B\u00e4\u00b7che", "von", "mir", "ge\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "CARD", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df von den Wangen mir das Wasser niemals geh\u2019", "tokens": ["Da\u00df", "von", "den", "Wan\u00b7gen", "mir", "das", "Was\u00b7ser", "nie\u00b7mals", "geh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Damit ich nimmermehr dieselben trucknen m\u00f6chte/", "tokens": ["Da\u00b7mit", "ich", "nim\u00b7mer\u00b7mehr", "die\u00b7sel\u00b7ben", "truck\u00b7nen", "m\u00f6ch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDAT", "NN", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df auch das Auge selbst in eigner Fluth erstickt/", "tokens": ["Da\u00df", "auch", "das", "Au\u00b7ge", "selbst", "in", "eig\u00b7ner", "Fluth", "er\u00b7stickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und da\u00df die Thr\u00e4ne mir den letzten Tropffen br\u00e4chte/", "tokens": ["Und", "da\u00df", "die", "Thr\u00e4\u00b7ne", "mir", "den", "letz\u00b7ten", "Tropf\u00b7fen", "br\u00e4ch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der mit dem Weinen auch zugleich mein Leyd ausdr\u00fcckt.", "tokens": ["Der", "mit", "dem", "Wei\u00b7nen", "auch", "zu\u00b7gleich", "mein", "Leyd", "aus\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Jhr blaues Wasser-Volck ihr Meer-Einwohnerinnen/", "tokens": ["Ihr", "blau\u00b7es", "Was\u00b7ser\u00b7Volck", "ihr", "Meer\u00b7Ein\u00b7woh\u00b7ne\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Seyd tausendmal begl\u00fcckt die ihr verwandelt seyd/", "tokens": ["Seyd", "tau\u00b7send\u00b7mal", "be\u00b7gl\u00fcckt", "die", "ihr", "ver\u00b7wan\u00b7delt", "seyd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "VVPP", "PRELS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Jhr Glieder die ihr m\u00fcst nunmehr als Brunnen rinnen/", "tokens": ["Ihr", "Glie\u00b7der", "die", "ihr", "m\u00fcst", "nun\u00b7mehr", "als", "Brun\u00b7nen", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPER", "VMFIN", "ADV", "KOUS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und vormals Jungfern war\u2019t voll holder Lieblichkeit.", "tokens": ["Und", "vor\u00b7mals", "Jung\u00b7fern", "wa\u00b7r't", "voll", "hol\u00b7der", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "Da\u00df meine Armen nicht verwandeln sich in B\u00e4che/", "tokens": ["Da\u00df", "mei\u00b7ne", "Ar\u00b7men", "nicht", "ver\u00b7wan\u00b7deln", "sich", "in", "B\u00e4\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und graue Wellen gehn auff dem bemoosten Haar/", "tokens": ["Und", "grau\u00b7e", "Wel\u00b7len", "gehn", "auff", "dem", "be\u00b7moos\u00b7ten", "Haar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ich w\u00fcnsch ein Brunn zu seyn/ je mehr ich dieses spreche/", "tokens": ["Ich", "w\u00fcnsch", "ein", "Brunn", "zu", "seyn", "/", "je", "mehr", "ich", "die\u00b7ses", "spre\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VAINF", "$(", "ADV", "ADV", "PPER", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Je mehr wird mirs versagt und bin verlassen gar.", "tokens": ["Je", "mehr", "wird", "mirs", "ver\u00b7sagt", "und", "bin", "ver\u00b7las\u00b7sen", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NE", "VVPP", "KON", "VAFIN", "VVPP", "ADV", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.41": {"text": "Ach k\u00f6nt ich ", "tokens": ["Ach", "k\u00f6nt", "ich"], "token_info": ["word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.42": {"text": "Umb seine ", "tokens": ["Umb", "sei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUI", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.43": {"text": "Und m\u00fcste meine Fluth wie ", "tokens": ["Und", "m\u00fcs\u00b7te", "mei\u00b7ne", "Fluth", "wie"], "token_info": ["word", "word", "word", "word", "word"], "pos":