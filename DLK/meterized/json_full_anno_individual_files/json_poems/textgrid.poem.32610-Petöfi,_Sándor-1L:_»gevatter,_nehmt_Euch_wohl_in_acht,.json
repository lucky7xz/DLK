{"textgrid.poem.32610": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbgevatter, nehmt Euch wohl in acht,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgevatter, nehmt Euch wohl in acht,", "tokens": ["\u00bb", "ge\u00b7vat\u00b7ter", ",", "nehmt", "Euch", "wohl", "in", "acht", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "PPER", "ADV", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Euer Weibchen seid bedacht,", "tokens": ["Auf", "Eu\u00b7er", "Weib\u00b7chen", "seid", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Frau ist jung, die Frau ist sch\u00f6n,", "tokens": ["Die", "Frau", "ist", "jung", ",", "die", "Frau", "ist", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Ungl\u00fcck ist gar bald geschehn!\u00ab", "tokens": ["Ein", "Un\u00b7gl\u00fcck", "ist", "gar", "bald", "ge\u00b7schehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbei, guter Freund, was h\u00f6r' ich gar?", "tokens": ["\u00bb", "ei", ",", "gu\u00b7ter", "Freund", ",", "was", "h\u00f6r'", "ich", "gar", "?"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das w\u00e4r' ja traurig, wenn es wahr,", "tokens": ["Das", "w\u00e4r'", "ja", "trau\u00b7rig", ",", "wenn", "es", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wer da bl\u00e4st in fremden Brei,", "tokens": ["Doch", "wer", "da", "bl\u00e4st", "in", "frem\u00b7den", "Brei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verbr\u00fcht sich leicht den Mund dabei!\u00ab", "tokens": ["Ver\u00b7br\u00fcht", "sich", "leicht", "den", "Mund", "da\u00b7bei", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "PAV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbha, \u00fcber solch verbotnen Schmaus", "tokens": ["\u00bb", "ha", ",", "\u00fc\u00b7ber", "solch", "ver\u00b7bot\u00b7nen", "Schmaus"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist meine Alte l\u00e4ngst hinaus!\u00ab", "tokens": ["Ist", "mei\u00b7ne", "Al\u00b7te", "l\u00e4ngst", "hin\u00b7aus", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbsalz leckt auch gern die alte Gei\u00df,", "tokens": ["\u00bb", "salz", "leckt", "auch", "gern", "die", "al\u00b7te", "Gei\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr sag' ich nicht, um keinen Preis!\u00ab", "tokens": ["Mehr", "sag'", "ich", "nicht", ",", "um", "kei\u00b7nen", "Preis", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "$,", "KOUI", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Nachbar oft noch wiederkehrt,", "tokens": ["Der", "Nach\u00b7bar", "oft", "noch", "wie\u00b7der\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Damit dem Freund er Vorsicht lehrt,", "tokens": ["Da\u00b7mit", "dem", "Freund", "er", "Vor\u00b7sicht", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und stets ermahnt er ihn aufs neu',", "tokens": ["Und", "stets", "er\u00b7mahnt", "er", "ihn", "aufs", "neu'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie jung und sch\u00f6n das Weibchen sei.", "tokens": ["Wie", "jung", "und", "sch\u00f6n", "das", "Weib\u00b7chen", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch pl\u00f6tzlich \u2013 was war denn geschehn? \u2013", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "\u2013", "was", "war", "denn", "ge\u00b7schehn", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "$(", "PWS", "VAFIN", "ADV", "VVPP", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Lie\u00df sich der Alte nicht mehr sehn,", "tokens": ["Lie\u00df", "sich", "der", "Al\u00b7te", "nicht", "mehr", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis jener hinzugehn beschlo\u00df,", "tokens": ["Bis", "je\u00b7ner", "hin\u00b7zu\u00b7gehn", "be\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um zu erfahren, was denn los?", "tokens": ["Um", "zu", "er\u00b7fah\u00b7ren", ",", "was", "denn", "los", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und unterwegs bereitet er", "tokens": ["Und", "un\u00b7ter\u00b7wegs", "be\u00b7rei\u00b7tet", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich schon die richt'ge Antwort her", "tokens": ["Sich", "schon", "die", "richt'\u00b7ge", "Ant\u00b7wort", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "ART", "ADJA", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf die gewohnte Litanei:", "tokens": ["Auf", "die", "ge\u00b7wohn\u00b7te", "Li\u00b7ta\u00b7nei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sch\u00f6n und jung sein Weibchen sei.", "tokens": ["Wie", "sch\u00f6n", "und", "jung", "sein", "Weib\u00b7chen", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch diesmal \u2013 h\u00e4lt der Nachbar fein", "tokens": ["Doch", "dies\u00b7mal", "\u2013", "h\u00e4lt", "der", "Nach\u00b7bar", "fein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den guten Rat f\u00fcr sich allein,", "tokens": ["Den", "gu\u00b7ten", "Rat", "f\u00fcr", "sich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sitzt am Herd voll Traurigkeit,", "tokens": ["Er", "sitzt", "am", "Herd", "voll", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wort erkl\u00e4rt sein schweres Leid:", "tokens": ["Ein", "Wort", "er\u00b7kl\u00e4rt", "sein", "schwe\u00b7res", "Leid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbwahr ist, was Ihr, Gott sei's geklagt!", "tokens": ["\u00bb", "wahr", "ist", ",", "was", "Ihr", ",", "Gott", "sei's", "ge\u00b7klagt", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "$,", "PWS", "PPER", "$,", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir von der alten Gei\u00df gesagt! ...", "tokens": ["Mir", "von", "der", "al\u00b7ten", "Gei\u00df", "ge\u00b7sagt", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Just schreit's! ... Dort in der Wiege liegt's!\u00ab ...", "tokens": ["Just", "schreit's", "!", "...", "Dort", "in", "der", "Wie\u00b7ge", "liegt's", "!", "\u00ab", "..."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "$.", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "... Was kann der Alte tun: er wiegt's!", "tokens": ["...", "Was", "kann", "der", "Al\u00b7te", "tun", ":", "er", "wiegt's", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "ART", "NN", "VVINF", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbgevatter, nehmt Euch wohl in acht,", "tokens": ["\u00bb", "ge\u00b7vat\u00b7ter", ",", "nehmt", "Euch", "wohl", "in", "acht", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "PPER", "ADV", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Euer Weibchen sei