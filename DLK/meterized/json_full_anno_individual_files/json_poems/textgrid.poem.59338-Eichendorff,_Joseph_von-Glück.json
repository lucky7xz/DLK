{"textgrid.poem.59338": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie jauchzt meine Seele", "tokens": ["Wie", "jauchzt", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und singet in sich!", "tokens": ["Und", "sin\u00b7get", "in", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kaum, da\u00df ich's verhehle", "tokens": ["Kaum", ",", "da\u00df", "ich's", "ver\u00b7heh\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PIS", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So gl\u00fccklich bin ich.", "tokens": ["So", "gl\u00fcck\u00b7lich", "bin", "ich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Rings Menschen sich drehen", "tokens": ["Rings", "Men\u00b7schen", "sich", "dre\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und sprechen gescheut,", "tokens": ["Und", "spre\u00b7chen", "ge\u00b7scheut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich kann nichts verstehen,", "tokens": ["Ich", "kann", "nichts", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So fr\u00f6hlich zerstreut. \u2013", "tokens": ["So", "fr\u00f6h\u00b7lich", "zer\u00b7streut", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Zu eng wird das Zimmer,", "tokens": ["Zu", "eng", "wird", "das", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wie gl\u00e4nzet das Feld,", "tokens": ["Wie", "gl\u00e4n\u00b7zet", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die T\u00e4ler voll Schimmer,", "tokens": ["Die", "T\u00e4\u00b7ler", "voll", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Weit herrlich die Welt!", "tokens": ["Weit", "herr\u00b7lich", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Gepre\u00dft bricht die Freude", "tokens": ["Ge\u00b7pre\u00dft", "bricht", "die", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Durch Riegel und Schlo\u00df,", "tokens": ["Durch", "Rie\u00b7gel", "und", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Fort \u00fcber die Heide!", "tokens": ["Fort", "\u00fc\u00b7ber", "die", "Hei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ach, h\u00e4tt ich ein Ro\u00df! \u2013", "tokens": ["Ach", ",", "h\u00e4tt", "ich", "ein", "Ro\u00df", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und frag ich und sinn ich,", "tokens": ["Und", "frag", "ich", "und", "sinn", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "KON", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wie so mir geschehn?: \u2013", "tokens": ["Wie", "so", "mir", "ge\u00b7schehn", "?", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVPP", "$.", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mein Liebchen herzinnig,", "tokens": ["Mein", "Lieb\u00b7chen", "her\u00b7zin\u00b7nig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das soll ich heut sehn!", "tokens": ["Das", "soll", "ich", "heut", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Wie jauchzt meine Seele", "tokens": ["Wie", "jauchzt", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "meas