{"textgrid.poem.24069": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Groteske", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah im Traume eine Abendr\u00f6te,", "tokens": ["Ich", "sah", "im", "Trau\u00b7me", "ei\u00b7ne", "A\u00b7bend\u00b7r\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die war wie wellendes, dampfendes Blut,", "tokens": ["Die", "war", "wie", "wel\u00b7len\u00b7des", ",", "damp\u00b7fen\u00b7des", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KOKOM", "PIS", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tief dunkel.", "tokens": ["Tief", "dun\u00b7kel", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Faul, breit, quoll sie molkig,", "tokens": ["Faul", ",", "breit", ",", "quoll", "sie", "mol\u00b7kig", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--++", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schwappend am leeren Horizonte lang gedehnt.", "tokens": ["Schwap\u00b7pend", "am", "lee\u00b7ren", "Ho\u00b7ri\u00b7zon\u00b7te", "lang", "ge\u00b7dehnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Schwer lag sie: leuchtender Schlamm.", "tokens": ["Schwer", "lag", "sie", ":", "leuch\u00b7ten\u00b7der", "Schlamm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "War das die Sonne, die da hinten sank?", "tokens": ["War", "das", "die", "Son\u00b7ne", ",", "die", "da", "hin\u00b7ten", "sank", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mir schien, und ich glaubt es im Traum,", "tokens": ["Mir", "schien", ",", "und", "ich", "glaubt", "es", "im", "Traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Glaubt es mit krampfendem Lachen: ein himmlischer Riese,", "tokens": ["Glaubt", "es", "mit", "kramp\u00b7fen\u00b7dem", "La\u00b7chen", ":", "ein", "himm\u00b7li\u00b7scher", "Rie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "Irgend einer der Wandler da oben,", "tokens": ["Ir\u00b7gend", "ei\u00b7ner", "der", "Wand\u00b7ler", "da", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die sich Wolkenfetzen um die Schenkel schlagen,", "tokens": ["Die", "sich", "Wol\u00b7ken\u00b7fet\u00b7zen", "um", "die", "Schen\u00