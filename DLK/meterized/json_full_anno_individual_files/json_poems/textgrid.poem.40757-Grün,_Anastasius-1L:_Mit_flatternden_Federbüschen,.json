{"textgrid.poem.40757": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit flatternden Federb\u00fcschen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit flatternden Federb\u00fcschen,", "tokens": ["Mit", "flat\u00b7tern\u00b7den", "Fe\u00b7der\u00b7b\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit schmetterndem H\u00f6rnerklang", "tokens": ["Mit", "schmet\u00b7tern\u00b7dem", "H\u00f6r\u00b7ner\u00b7klang"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ziehn J\u00e4ger, die schmucken frischen", "tokens": ["Ziehn", "J\u00e4\u00b7ger", ",", "die", "schmu\u00b7cken", "fri\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gesellen, das Dorf entlang.", "tokens": ["Ge\u00b7sel\u00b7len", ",", "das", "Dorf", "ent\u00b7lang", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie ziehn an des Landes Grenzen,", "tokens": ["Sie", "ziehn", "an", "des", "Lan\u00b7des", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vorposten zu treuer Wacht,", "tokens": ["Vor\u00b7pos\u00b7ten", "zu", "treu\u00b7er", "Wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Waffen funkeln und gl\u00e4nzen,", "tokens": ["Die", "Waf\u00b7fen", "fun\u00b7keln", "und", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Taktschritt dr\u00f6hnt mit Macht.", "tokens": ["Der", "Takt\u00b7schritt", "dr\u00f6hnt", "mit", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Weib sitzt an der Schwelle,", "tokens": ["Ein", "Weib", "sitzt", "an", "der", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Kn\u00e4blein an der Brust,", "tokens": ["Ihr", "Kn\u00e4\u00b7blein", "an", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem leuchten die Aeuglein so helle,", "tokens": ["Dem", "leuch\u00b7ten", "die", "A\u00b7e\u00b7u\u00b7glein", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das klatscht in die H\u00e4nde vor Lust.", "tokens": ["Das", "klatscht", "in", "die", "H\u00e4n\u00b7de", "vor", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "\u00bbgeduld, du Schelm, du kleiner,", "tokens": ["\u00bb", "ge\u00b7duld", ",", "du", "Schelm", ",", "du", "klei\u00b7ner", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PPER", "NN", "$,", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Jahre verrinnen schnell,", "tokens": ["Die", "Jah\u00b7re", "ver\u00b7rin\u00b7nen", "schnell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann wirst auch du wohl so Einer,", "tokens": ["Dann", "wirst", "auch", "du", "wohl", "so", "Ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADV", "ADV", "PIS", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Solch schmucker frischer Gesell!\u00ab", "tokens": ["Solch", "schmu\u00b7cker", "fri\u00b7scher", "Ge\u00b7sell", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Die Tritte, die Kl\u00e4nge allm\u00e4hlich", "tokens": ["Die", "Trit\u00b7te", ",", "die", "Kl\u00e4n\u00b7ge", "all\u00b7m\u00e4h\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Verhallen am Waldessaum;", "tokens": ["Ver\u00b7hal\u00b7len", "am", "Wal\u00b7des\u00b7saum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Mutter, stolz und selig,", "tokens": ["Die", "Mut\u00b7ter", ",", "stolz", "und", "se\u00b7lig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tr\u00e4umt sch\u00f6nen Zukunftstraum:", "tokens": ["Tr\u00e4umt", "sch\u00f6\u00b7nen", "Zu\u00b7kunft\u00b7straum", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbo Kind, geboren in Schmerzen,", "tokens": ["\u00bb", "o", "Kind", ",", "ge\u00b7bo\u00b7ren", "in", "Schmer\u00b7zen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So hilflos noch und zart,", "tokens": ["So", "hil\u00b7flos", "noch", "und", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erstarke am Mutterherzen", "tokens": ["Er\u00b7star\u00b7ke", "am", "Mut\u00b7ter\u00b7her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu rechter Mannesart!", "tokens": ["Zu", "rech\u00b7ter", "Man\u00b7ne\u00b7sart", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O bl\u00fche, du holde Bl\u00fcthe,", "tokens": ["O", "bl\u00fc\u00b7he", ",", "du", "hol\u00b7de", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "O wachse, frei von Harm;", "tokens": ["O", "wach\u00b7se", ",", "frei", "von", "Harm", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich schirme, bewache, beh\u00fcte", "tokens": ["Dich", "schir\u00b7me", ",", "be\u00b7wa\u00b7che", ",", "be\u00b7h\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "$,", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mein Aug', mein Herz, mein Arm!", "tokens": ["Mein", "Aug'", ",", "mein", "Herz", ",", "mein", "Arm", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch wie viel M\u00fchn und Gefahren", "tokens": ["Doch", "wie", "viel", "M\u00fchn", "und", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIAT", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Noch bis ans ferne Ziel!", "tokens": ["Noch", "bis", "ans", "fer\u00b7ne", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von sorgenschweren Jahren,", "tokens": ["Von", "sor\u00b7gen\u00b7schwe\u00b7ren", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchwachten N\u00e4chten wie viel!", "tokens": ["Durch\u00b7wach\u00b7ten", "N\u00e4ch\u00b7ten", "wie", "viel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KOKOM", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Mit Wonne den eigenen Schlummer", "tokens": ["Mit", "Won\u00b7ne", "den", "ei\u00b7ge\u00b7nen", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Leg' ich dem deinen zu;", "tokens": ["Leg'", "ich", "dem", "dei\u00b7nen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "PPOSAT", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein sei die Angst und der Kummer,", "tokens": ["Mein", "sei", "die", "Angst", "und", "der", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dein sei die Lust und die Ruh'!", "tokens": ["Dein", "sei", "die", "Lust", "und", "die", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Ja, ganz vergessen meiner,", "tokens": ["Ja", ",", "ganz", "ver\u00b7ges\u00b7sen", "mei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVPP", "PPOSAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In dir nur leb' ich allein;", "tokens": ["In", "dir", "nur", "leb'", "ich", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann wirst du wohl auch so Einer,", "tokens": ["Dann", "wirst", "du", "wohl", "auch", "so", "Ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "PIS", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein Stolz, mein Stab einst sein.\u00ab \u2013", "tokens": ["Mein", "Stolz", ",", "mein", "Stab", "einst", "sein", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADV", "VAINF", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Horch, w\u00fcster Schall durchzittert", "tokens": ["Horch", ",", "w\u00fcs\u00b7ter", "Schall", "durch\u00b7zit\u00b7tert"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der jungen Mutter Traum;", "tokens": ["Der", "jun\u00b7gen", "Mut\u00b7ter", "Traum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es hat gar schlimm gewittert", "tokens": ["Es", "hat", "gar", "schlimm", "ge\u00b7wit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am fernen Waldessaum.", "tokens": ["Am", "fer\u00b7nen", "Wal\u00b7des\u00b7saum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die Bahre von Tannen\u00e4sten", "tokens": ["Die", "Bah\u00b7re", "von", "Tan\u00b7ne\u00b7n\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Jetzt tragen Krieger vorbei,", "tokens": ["Jetzt", "tra\u00b7gen", "Krie\u00b7ger", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie bringen der Tapfern Besten", "tokens": ["Sie", "brin\u00b7gen", "der", "Tap\u00b7fern", "Bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Getroffen vom Todesblei.", "tokens": ["Ge\u00b7trof\u00b7fen", "vom", "To\u00b7des\u00b7blei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Vom blinden Erz zerrissen", "tokens": ["Vom", "blin\u00b7den", "Erz", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein edler Lebensdocht,", "tokens": ["Ein", "ed\u00b7ler", "Le\u00b7bens\u00b7docht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An dem gleich treubefli\u00dfen", "tokens": ["An", "dem", "gleich", "treu\u00b7be\u00b7fli\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einst Mutterliebe flocht!", "tokens": ["Einst", "Mut\u00b7ter\u00b7lie\u00b7be", "flocht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ach, all die M\u00fchn und Sorgen,", "tokens": ["Ach", ",", "all", "die", "M\u00fchn", "und", "Sor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Jahre kummerbewegt,", "tokens": ["Die", "Jah\u00b7re", "kum\u00b7mer\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Auf da\u00df man so Einen morgen", "tokens": ["Auf", "da\u00df", "man", "so", "Ei\u00b7nen", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PIS", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ans Mutterherz dir legt!", "tokens": ["Ans", "Mut\u00b7ter\u00b7herz", "dir", "legt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Mit flatternden Federb\u00fcschen,", "tokens": ["Mit", "flat\u00b7tern\u00b7den", "Fe\u00b7der\u00b7b\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit schmetterndem H\u00f6rnerklang", "tokens": ["Mit", "schmet\u00b7tern\u00b7dem", "H\u00f6r\u00b7ner\u00b7klang"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ziehn J\u00e4ger, die schmucken frischen", "tokens": ["Ziehn", "J\u00e4\u00b7ger", ",", "die", "schmu\u00b7cken", "fri\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gesellen, das Dorf entlang.", "tokens": ["Ge\u00b7sel\u00b7len", ",", "das", "Dorf", "ent\u00b7lang", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.16": {"line.1": {"text": "Sie ziehn an des Landes Grenzen,", "tokens": ["Sie", "ziehn", "an", "des", "Lan\u00b7des", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vorposten zu treuer Wacht,", "tokens": ["Vor\u00b7pos\u00b7ten", "zu", "treu\u00b7er", "Wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Waffen funkeln und gl\u00e4nzen,", "tokens": ["Die", "Waf\u00b7fen", "fun\u00b7keln", "und", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Taktschritt dr\u00f6hnt mit Macht.", "tokens": ["Der", "Takt\u00b7schritt", "dr\u00f6hnt", "mit", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Ein Weib sitzt an der Schwelle,", "tokens": ["Ein", "Weib", "sitzt", "an", "der", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Kn\u00e4blein an der Brust,", "tokens": ["Ihr", "Kn\u00e4\u00b7blein", "an", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": 