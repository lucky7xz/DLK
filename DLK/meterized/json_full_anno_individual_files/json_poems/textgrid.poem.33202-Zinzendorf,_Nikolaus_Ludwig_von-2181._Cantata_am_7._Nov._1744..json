{"textgrid.poem.33202": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2181. Cantata am 7. Nov. 1744.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nigen sollen deine Pfleger, und ihre F\u00fcrstinnen deine S\u00e4ug-ammen seyn.", "tokens": ["K\u00f6\u00b7ni\u00b7gen", "sol\u00b7len", "dei\u00b7ne", "Pfle\u00b7ger", ",", "und", "ih\u00b7re", "F\u00fcrs\u00b7tin\u00b7nen", "dei\u00b7ne", "S\u00e4ug\u00b7am\u00b7men", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+--+-+-+--+--+-+-+--+", "measure": "iambic.octa.plus.invert"}}, "stanza.2": {"line.1": {"text": "Von der Creuz Theocratie ist zu observiren, da\u00df die Obrigkeit und sie immer harmoniren.", "tokens": ["Von", "der", "Creuz", "Theo\u00b7cra\u00b7tie", "ist", "zu", "ob\u00b7ser\u00b7vi\u00b7ren", ",", "da\u00df", "die", "Ob\u00b7rig\u00b7keit", "und", "sie", "im\u00b7mer", "har\u00b7mo\u00b7ni\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VAFIN", "PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "KON", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+--+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "An diesem unserm gnaden-orte fehlts der verheissung an keinem worte.", "tokens": ["An", "die\u00b7sem", "un\u00b7serm", "gna\u00b7den\u00b7or\u00b7te", "fehlts", "der", "ver\u00b7heis\u00b7sung", "an", "kei\u00b7nem", "wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos"