{"textgrid.poem.58108": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "146. Wo man mit verkehrt, da wird man mit geehrt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weltumgang stecket an. Mach dich ihr nicht gemein,", "tokens": ["Wel\u00b7tum\u00b7gang", "ste\u00b7cket", "an", ".", "Mach", "dich", "ihr", "nicht", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$.", "VVFIN", "PPER", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sonst wirst du unvermerkt, o Seel', ein Weltkind sein;", "tokens": ["Sonst", "wirst", "du", "un\u00b7ver\u00b7merkt", ",", "o", "Seel'", ",", "ein", "Welt\u00b7kind", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "FM", "NN", "$,", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur lebe stets mit Gott gemein auf dieser Erden,", "tokens": ["Nur", "le\u00b7be", "stets", "mit", "Gott", "ge\u00b7mein", "auf", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wirst du g\u00f6ttlich auch und leicht vollkommen werden!", "tokens": ["So", "wirst", "du", "g\u00f6tt\u00b7lich", "auch", "und", "leicht", "voll\u00b7kom\u00b7men", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN",