{"textgrid.poem.25412": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welt reichte nur vom kleinen Garten,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welt reichte nur vom kleinen Garten,", "tokens": ["Welt", "reich\u00b7te", "nur", "vom", "klei\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "drin die Dahlien bl\u00fchten, bis zur Zelle", "tokens": ["drin", "die", "Da\u00b7hli\u00b7en", "bl\u00fch\u00b7ten", ",", "bis", "zur", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und durch die G\u00e4nge nach dem Hof", "tokens": ["Und", "durch", "die", "G\u00e4n\u00b7ge", "nach", "dem", "Hof"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und fr\u00fch und Abends zur Kapelle.", "tokens": ["und", "fr\u00fch", "und", "A\u00b7bends", "zur", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aber unter mir war Ebene, ins Gr\u00fcn versenkt,", "tokens": ["A\u00b7ber", "un\u00b7ter", "mir", "war", "E\u00b7be\u00b7ne", ",", "ins", "Gr\u00fcn", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+---+-+", "measure": "unknown.measure.hexa"}, "line.6": {"text": "mit vielen Kirchen und wei\u00df bl\u00fchenden Obstb\u00e4umen,", "tokens": ["mit", "vie\u00b7len", "Kir\u00b7chen", "und", "wei\u00df", "bl\u00fc\u00b7hen\u00b7den", "Obst\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Hingedr\u00e4ngten D\u00f6rfern, weit ins Land ger\u00fcckt,", "tokens": ["Hin\u00b7ge\u00b7dr\u00e4ng\u00b7ten", "D\u00f6r\u00b7fern", ",", "weit", "ins", "Land", "ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "bis \u00fcbern Rhein, wo wieder blaue Berge sie ums\u00e4umen.", "tokens": ["bis", "\u00fc\u00b7bern", "Rhein", ",", "wo", "wie\u00b7der", "blau\u00b7e", "Ber\u00b7ge", "sie", "um\u00b7s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NE", "$,", "PWAV", "ADV", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "An ganz stillen Nachmittagen meinte man", "tokens": ["An", "ganz", "stil\u00b7len", "Nach\u00b7mit\u00b7ta\u00b7gen", "mein\u00b7te", "man"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "PIS"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "die Stimmen von den Stra\u00dfen heraufwehen zu h\u00f6ren,", "tokens": ["die", "Stim\u00b7men", "von", "den", "Stra\u00b7\u00dfen", "her\u00b7auf\u00b7we\u00b7hen", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "und Abends kam Gel\u00e4ute,", "tokens": ["und", "A\u00b7bends", "kam", "Ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Das hoch den blau ziehenden Rauch der Kamine \u00fcberflog", "tokens": ["Das", "hoch", "den", "blau", "zie\u00b7hen\u00b7den", "Rauch", "der", "Ka\u00b7mi\u00b7ne", "\u00fc\u00b7berf\u00b7log"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "ART", "ADJD", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+---+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "und mich in meinem Nachsinnen erfreute.", "tokens": ["und", "mich", "in", "mei\u00b7nem", "Nach\u00b7sin\u00b7nen", "er\u00b7freu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn dann die Nacht herabsank", "tokens": ["Wenn", "dann", "die", "Nacht", "her\u00b7ab\u00b7sank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und \u00fcber meinem Fenster die Sterne erglommen,", "tokens": ["und", "\u00fc\u00b7ber", "mei\u00b7nem", "Fens\u00b7ter", "die", "Ster\u00b7ne", "er\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "War eine fremde Welt aus B\u00fcchern", "tokens": ["War", "ei\u00b7ne", "frem\u00b7de", "Welt", "aus", "B\u00fc\u00b7chern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "auf mich hergesenkt und hat mich hingenommen.", "tokens": ["auf", "mich", "her\u00b7ge\u00b7senkt", "und", "hat", "mich", "hin\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "KON", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Ich las von Torheit dieser Welt, Bedr\u00e4ngnis, Sp\u00e4\u00dfen,", "tokens": ["Ich", "las", "von", "Tor\u00b7heit", "die\u00b7ser", "Welt", ",", "Be\u00b7dr\u00e4ng\u00b7nis", ",", "Sp\u00e4\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PDAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Trug und Leiden,", "tokens": ["Trug", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Fromme Heiligengeschichten, grausenvoll und lieblich,", "tokens": ["From\u00b7me", "Hei\u00b7li\u00b7gen\u00b7ge\u00b7schich\u00b7ten", ",", "grau\u00b7sen\u00b7voll", "und", "lieb\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "und die alte Weisheit der Heiden.", "tokens": ["und", "die", "al\u00b7te", "Weis\u00b7heit", "der", "Hei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Sinnen und Suchen vieler Menschenseelen", "tokens": ["Sin\u00b7nen", "und", "Su\u00b7chen", "vie\u00b7ler", "Men\u00b7schen\u00b7see\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PIAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "war vor meine Augen hingestellt,", "tokens": ["war", "vor", "mei\u00b7ne", "Au\u00b7gen", "hin\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Und Wunder der Sch\u00f6pfung und Leben, das ich liebte,", "tokens": ["Und", "Wun\u00b7der", "der", "Sch\u00f6p\u00b7fung", "und", "Le\u00b7ben", ",", "das", "ich", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "KON", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "und die Herrlichkeit der Welt.", "tokens": ["und", "die", "Herr\u00b7lich\u00b7keit", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich beschlo\u00df, all das Krause,", "tokens": ["Und", "ich", "be\u00b7schlo\u00df", ",", "all", "das", "Krau\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PIAT", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "das ich seit so viel Jahren", "tokens": ["das", "ich", "seit", "so", "viel", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "APPR", "ADV", "PIAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aus B\u00fcchern und Wald und Menschenherzen", "tokens": ["Aus", "B\u00fc\u00b7chern", "und", "Wald", "und", "Men\u00b7schen\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und einsamen Stunden erfahren,", "tokens": ["und", "ein\u00b7sa\u00b7men", "Stun\u00b7den", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Alles Gute,", "tokens": ["Al\u00b7les", "Gu\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "das ich in diesem Erdenleben empfangen,", "tokens": ["das", "ich", "in", "die\u00b7sem", "Er\u00b7den\u00b7le\u00b7ben", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Treu und k\u00fcnstlich in Bild und Schrift", "tokens": ["Treu", "und", "k\u00fcnst\u00b7lich", "in", "Bild", "und", "Schrift"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "zu bewahren und einzufangen.", "tokens": ["zu", "be\u00b7wah\u00b7ren", "und", "ein\u00b7zu\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "VVIZU", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Sp\u00e4ter, wenn die Augen schw\u00e4cher w\u00fcrden,", "tokens": ["Sp\u00e4\u00b7ter", ",", "wenn", "die", "Au\u00b7gen", "schw\u00e4\u00b7cher", "w\u00fcr\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "in den alten Tagen,", "tokens": ["in", "den", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "W\u00fcrd ich in meiner Zelle sitzen", "tokens": ["W\u00fcrd", "ich", "in", "mei\u00b7ner", "Zel\u00b7le", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "und \u00fcbers Elsa\u00df hinblicken", "tokens": ["und", "\u00fc\u00b7bers", "El\u00b7sa\u00df", "hin\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "und mein Buch aufschlagen,", "tokens": ["und", "mein", "Buch", "auf\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.14": {"text": "Und meiner Seele spr\u00e4ngen", "tokens": ["Und", "mei\u00b7ner", "See\u00b7le", "spr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "wie am Heiligenquell im Wald", "tokens": ["wie", "am", "Hei\u00b7li\u00b7gen\u00b7quell", "im", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "A