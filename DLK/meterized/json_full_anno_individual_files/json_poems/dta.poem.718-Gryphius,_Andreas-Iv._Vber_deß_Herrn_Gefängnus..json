{"dta.poem.718": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  Vber de\u00df Herrn Gef\u00e4ngnus.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Wje in dem Garten sind dem Teufel eingegangen ", "tokens": ["Wie", "in", "dem", "Gar\u00b7ten", "sind", "dem", "Teu\u00b7fel", "ein\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seine J\u00e4gergarn\u2019 vnd harter Ketten macht/", "tokens": ["In", "sei\u00b7ne", "J\u00e4\u00b7ger\u00b7garn'", "vnd", "har\u00b7ter", "Ket\u00b7ten", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die jhre missethat erbeigen auff vns bracht;", "tokens": ["Die", "jhre", "mis\u00b7se\u00b7that", "er\u00b7bei\u00b7gen", "auff", "vns", "bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "So wird die vnschuld selbst im Garten auffgefangen.", "tokens": ["So", "wird", "die", "vn\u00b7schuld", "selbst", "im", "Gar\u00b7ten", "auff\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die freyheit f\u00e4lt in strick/ durch list der grimmen Schlangen.", "tokens": ["Die", "frey\u00b7heit", "f\u00e4lt", "in", "strick", "/", "durch", "list", "der", "grim\u00b7men", "Schlan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$(", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die hand durch welcher krafft/ dz werck der welt erkracht/ ", "tokens": ["Die", "hand", "durch", "wel\u00b7cher", "krafft", "/", "dz", "werck", "der", "welt", "er\u00b7kracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PWAT", "NN", "$(", "KOUS", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der hellen Gottheit glantz wird in der schwartzen nacht ", "tokens": ["Der", "hel\u00b7len", "Got\u00b7theit", "glantz", "wird", "in", "der", "schwart\u00b7zen", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In fessell eingelegt vns Freyheit zu erlangen.", "tokens": ["In", "fes\u00b7sell", "ein\u00b7ge\u00b7legt", "vns", "Frey\u00b7heit", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVPP", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig wird ein Knecht/ der tollen Knechte schar", "tokens": ["Der", "K\u00f6\u00b7nig", "wird", "ein", "Knecht", "/", "der", "tol\u00b7len", "Knech\u00b7te", "schar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schlegt auff den Erben zu. Er gibt sich selber dar/", "tokens": ["Schlegt", "auff", "den", "Er\u00b7ben", "zu", ".", "Er", "gibt", "sich", "sel\u00b7ber", "dar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "PRF", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit er was nicht frey au\u00df band\u2019 vnd K\u00e4rcker reisse.", "tokens": ["Da\u00b7mit", "er", "was", "nicht", "frey", "au\u00df", "band'", "vnd", "K\u00e4r\u00b7cker", "reis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PTKNEG", "ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hilff! der du durch den dinst das Dinsthau\u00df vmbgekehrt/", "tokens": ["Hilff", "!", "der", "du", "durch", "den", "dinst", "das", "Dinst\u00b7hau\u00df", "vmb\u00b7ge\u00b7kehrt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der du in danden hast dem starcken auch gewehrt: ", "tokens": ["Der", "du", "in", "dan\u00b7den", "hast", "dem", "star\u00b7cken", "auch", "ge\u00b7wehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "VAFIN", "ART", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das ich von S\u00fcnden frey/ mich deines dinst\u2019s befleisse.", "tokens": ["Das", "ich", "von", "S\u00fcn\u00b7den", "frey", "/", "mich", "dei\u00b7nes", "dinst's", "be\u00b7fleis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "ADJD", "$(", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}