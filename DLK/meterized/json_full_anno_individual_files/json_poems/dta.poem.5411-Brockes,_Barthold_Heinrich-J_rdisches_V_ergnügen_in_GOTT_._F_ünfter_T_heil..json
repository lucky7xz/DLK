{"dta.poem.5411": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, wie f\u00e4ngt man es denn an, von diesem Ungl\u00fcck zu", "tokens": ["Al\u00b7lein", ",", "wie", "f\u00e4ngt", "man", "es", "denn", "an", ",", "von", "die\u00b7sem", "Un\u00b7gl\u00fcck", "zu"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "VVFIN", "PIS", "PPER", "ADV", "PTKVZ", "$,", "APPR", "PDAT", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "genesen,", "tokens": ["ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Das alles Ungl\u00fccks Urqvell ist? Es macht uns die Gewohn-", "tokens": ["Das", "al\u00b7les", "Un\u00b7gl\u00fccks", "Urq\u00b7vell", "ist", "?", "Es", "macht", "uns", "die", "Ge\u00b7wohn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "NN", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "heit blind", "tokens": ["heit", "blind"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJD"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Und taub und f\u00fchl-los. Unser Geist, als der von einem", "tokens": ["Und", "taub", "und", "f\u00fchl\u00b7los", ".", "Un\u00b7ser", "Geist", ",", "als", "der", "von", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "$.", "PPOSAT", "NN", "$,", "KOUS", "ART", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "regen Wesen,", "tokens": ["re\u00b7gen", "We\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Kann gantz unm\u00f6glich m\u00fc\u00dfig seyn. Die feurigen Begierden", "tokens": ["Kann", "gantz", "un\u00b7m\u00f6g\u00b7lich", "m\u00fc\u00b7\u00dfig", "seyn", ".", "Die", "feu\u00b7ri\u00b7gen", "Be\u00b7gier\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJD", "ADJD", "VAINF", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "sind", "tokens": ["sind"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Dadurch bey uns gleich wilden Pferden, die nimmer stille", "tokens": ["Da\u00b7durch", "bey", "uns", "gleich", "wil\u00b7den", "Pfer\u00b7den", ",", "die", "nim\u00b7mer", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "APPR", "PPER", "ADV", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+--++-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "stehen k\u00f6nnen,", "tokens": ["ste\u00b7hen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Den Ort, woselbst sie sind, nicht achten und stets nach", "tokens": ["Den", "Ort", ",", "wo\u00b7selbst", "sie", "sind", ",", "nicht", "ach\u00b7ten", "und", "stets", "nach"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VAFIN", "$,", "PTKNEG", "VVINF", "KON", "ADV", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "fernem Ziele rennen;", "tokens": ["fer\u00b7nem", "Zie\u00b7le", "ren\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Ja durch die allersch\u00f6nsten Wiesen, ohn\u2019 alles, was aus", "tokens": ["Ja", "durch", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Wie\u00b7sen", ",", "ohn'", "al\u00b7les", ",", "was", "aus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "PIS", "$,", "PRELS", "APPR"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "ihnen sch\u00f6n,", "tokens": ["ih\u00b7nen", "sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Ergetz- und n\u00fctzlich zu geniessen, zu sehen, immer weiter", "tokens": ["Er\u00b7getz", "und", "n\u00fctz\u00b7lich", "zu", "ge\u00b7nies\u00b7sen", ",", "zu", "se\u00b7hen", ",", "im\u00b7mer", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["TRUNC", "KON", "ADJD", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "ADV", "ADV"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "gehn.", "tokens": ["gehn", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Wir schieben den Genu\u00df von allem, was wir besitzen, im-", "tokens": ["Wir", "schie\u00b7ben", "den", "Ge\u00b7nu\u00df", "von", "al\u00b7lem", ",", "was", "wir", "be\u00b7sit\u00b7zen", ",", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "TRUNC"], "meter": "-+---+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "mer auf,", "tokens": ["mer", "auf", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Und gleichen Geitzigen, die scharren in ihrem gantzen Le-", "tokens": ["Und", "glei\u00b7chen", "Geit\u00b7zi\u00b7gen", ",", "die", "schar\u00b7ren", "in", "ih\u00b7rem", "gant\u00b7zen", "Le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "bens-Lauf,", "tokens": ["bens\u00b7Lauf", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Bi\u00df an den Tod, um sich so dann an ihren eingeschlossnen", "tokens": ["Bi\u00df", "an", "den", "Tod", ",", "um", "sich", "so", "dann", "an", "ih\u00b7ren", "ein\u00b7ge\u00b7schloss\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "KOUI", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.22": {"text": "Sch\u00e4tzen", "tokens": ["Sch\u00e4t\u00b7zen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Den Rest des Lebens zu ergetzen.", "tokens": ["Den", "Rest", "des", "Le\u00b7bens", "zu", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Wann wir bey diesem Zustand nun die Fl\u00fcchtigkeit der", "tokens": ["Wann", "wir", "bey", "die\u00b7sem", "Zu\u00b7stand", "nun", "die", "Fl\u00fcch\u00b7tig\u00b7keit", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PDAT", "NN", "ADV", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zeit betrachten,", "tokens": ["Zeit", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.26": {"text": "Und, bey derselben schnellen Flucht, auf unsre kurtze Dauer", "tokens": ["Und", ",", "bey", "der\u00b7sel\u00b7ben", "schnel\u00b7len", "Flucht", ",", "auf", "uns\u00b7re", "kurt\u00b7ze", "Dau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "PDAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.27": {"text": "achten,", "tokens": ["ach\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "Erwegen, was wir einst geschrieben: Wir scheinen fast", "tokens": ["Er\u00b7we\u00b7gen", ",", "was", "wir", "einst", "ge\u00b7schrie\u00b7ben", ":", "Wir", "schei\u00b7nen", "fast"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "in unserm Leben", "tokens": ["in", "un\u00b7serm", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Mit einem stetem Nichts umgeben;", "tokens": ["Mit", "ei\u00b7nem", "ste\u00b7tem", "Nichts", "um\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}