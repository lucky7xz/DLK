{"textgrid.poem.57902": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das waren Tage, gl\u00fchend und hei\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das waren Tage, gl\u00fchend und hei\u00df,", "tokens": ["Das", "wa\u00b7ren", "Ta\u00b7ge", ",", "gl\u00fc\u00b7hend", "und", "hei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bei sengender Sonne Prallen,", "tokens": ["Bei", "sen\u00b7gen\u00b7der", "Son\u00b7ne", "Pral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da sind wir in der hei\u00dfen Stadt", "tokens": ["Da", "sind", "wir", "in", "der", "hei\u00b7\u00dfen", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beinahe vom Stengel gefallen.", "tokens": ["Bei\u00b7na\u00b7he", "vom", "Sten\u00b7gel", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Doch ihr Gutes hat die Hitze auch,", "tokens": ["Doch", "ihr", "Gu\u00b7tes", "hat", "die", "Hit\u00b7ze", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da drau\u00dfen auf dem Lande,", "tokens": ["Da", "drau\u00b7\u00dfen", "auf", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da schafft der Bauer die Ernte heim", "tokens": ["Da", "schafft", "der", "Bau\u00b7er", "die", "Ern\u00b7te", "heim"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bei sengendem Sonnenbrande.", "tokens": ["Bei", "sen\u00b7gen\u00b7dem", "Son\u00b7nen\u00b7bran\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch in der Stadt, da ist sie meist", "tokens": ["Doch", "in", "der", "Stadt", ",", "da", "ist", "sie", "meist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von zweifelhaftem Werte,", "tokens": ["Von", "zwei\u00b7fel\u00b7haf\u00b7tem", "Wer\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was sie da zeitigt, ach, sehr oft,", "tokens": ["Was", "sie", "da", "zei\u00b7tigt", ",", "ach", ",", "sehr", "oft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "ITJ", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ist es das Verkehrte.", "tokens": ["Da", "ist", "es", "das", "Ver\u00b7kehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "