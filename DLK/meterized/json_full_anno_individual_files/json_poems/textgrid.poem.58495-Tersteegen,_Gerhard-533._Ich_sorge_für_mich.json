{"textgrid.poem.58495": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "533. Ich sorge f\u00fcr mich", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was geht mich's an, wie's der, wie's jener macht,", "tokens": ["Was", "geht", "mich's", "an", ",", "wie's", "der", ",", "wie's", "je\u00b7ner", "macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "PTKVZ", "$,", "KOUS", "ART", "$,", "KOUS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wie man sich auch gegen mich betr\u00e4gt?", "tokens": ["Und", "wie", "man", "sich", "auch", "ge\u00b7gen", "mich", "be\u00b7tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mach' ich's nur recht, bleib' ich nur auf der Wacht,", "tokens": ["Mach'", "ich's", "nur", "recht", ",", "bleib'", "ich", "nur", "auf", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJD", "$,", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Werd' ich nur klein, werd' ich nur rein gefegt,", "tokens": ["Werd'", "ich", "nur", "klein", ",", "werd'", "ich", "nur", "rein", "ge\u00b7fegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann scheure jeder, was er kann!", "tokens": ["Dann", "scheu\u00b7re", "je\u00b7der", ",", "was", "er", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott schenkt's, Gott lenkt's: ich lieb' den Mann.", "tokens": ["Gott", "schenkt's", ",", "Gott", "lenkt's", ":", "ich", "lieb'", "den", "Mann", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "NE", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was geht mich's an, wie's der, wie's jener macht,", "tokens": ["Was", "geht", "mich's", "an", ",", "wie's", "der", ",", "wie's", "je\u00b7ner", "macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "PTKVZ", "$,", "KOUS", "ART", "$,", "KOUS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wie man sich auch gegen mich betr\u00e4gt?", "tokens": ["Und", "wie", "man", "sich", "auch", "ge\u00b7gen", "mich", "be\u00b7tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mach' ich's nur recht, bleib' ich nur auf der Wacht,", "tokens": ["Mach'", "ich's", "nur", "recht", ",", "bleib'", "ich", "nur", "auf", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJD", "$,", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "mea