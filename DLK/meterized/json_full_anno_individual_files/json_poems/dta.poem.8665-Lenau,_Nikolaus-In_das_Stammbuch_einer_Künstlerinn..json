{"dta.poem.8665": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "In das Stammbuch einer K\u00fcnstlerinn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Nach langem Wege durch die Sommerschw\u00fcle", "tokens": ["Nach", "lan\u00b7gem", "We\u00b7ge", "durch", "die", "Som\u00b7mer\u00b7schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rauscht' uns ein Wald entgegen seinen Gru\u00df;", "tokens": ["Rauscht'", "uns", "ein", "Wald", "ent\u00b7ge\u00b7gen", "sei\u00b7nen", "Gru\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Uns \u00fcbergo\u00df die Luft mit s\u00fc\u00dfer K\u00fchle,", "tokens": ["Uns", "\u00fc\u00b7ber\u00b7go\u00df", "die", "Luft", "mit", "s\u00fc\u00b7\u00dfer", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Bl\u00e4tternacht mit ihrem Labeku\u00df.", "tokens": ["Die", "Bl\u00e4t\u00b7ter\u00b7nacht", "mit", "ih\u00b7rem", "La\u00b7be\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wie wir aus den hei\u00dfen, hellen Triften,", "tokens": ["Und", "wie", "wir", "aus", "den", "hei\u00b7\u00dfen", ",", "hel\u00b7len", "Trif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo m\u00fchend sich der Mensch dem Leben weiht,", "tokens": ["Wo", "m\u00fc\u00b7hend", "sich", "der", "Mensch", "dem", "Le\u00b7ben", "weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In's Waldgeheimni\u00df weiter uns vertieften,", "tokens": ["In's", "Wald\u00b7ge\u00b7heim\u00b7ni\u00df", "wei\u00b7ter", "uns", "ver\u00b7tief\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und in den Schatten Gottes: Einsamkeit; \u2014", "tokens": ["Und", "in", "den", "Schat\u00b7ten", "Got\u00b7tes", ":", "Ein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$.", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So flohen deine heiteren Gespr\u00e4che", "tokens": ["So", "flo\u00b7hen", "dei\u00b7ne", "hei\u00b7te\u00b7ren", "Ge\u00b7spr\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Fort von des ", "tokens": ["Fort", "von", "des"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.11": {"text": "Waldein, und wanden sich als klare B\u00e4che", "tokens": ["Wal\u00b7dein", ",", "und", "wan\u00b7den", "sich", "als", "kla\u00b7re", "B\u00e4\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "VVFIN", "PRF", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Durch's Labyrinth der Kunst mit leisem Klang;", "tokens": ["Durch's", "La\u00b7by\u00b7rinth", "der", "Kunst", "mit", "lei\u00b7sem", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Auf ihren Wellen bebten die Gestalten", "tokens": ["Auf", "ih\u00b7ren", "Wel\u00b7len", "beb\u00b7ten", "die", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Von all' den Blumen, die ihr Lauf ber\u00fchrt;", "tokens": ["Von", "all'", "den", "Blu\u00b7men", ",", "die", "ihr", "Lauf", "be\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ich aber sah, nachh\u00e4ngend ihrem Walten,", "tokens": ["Ich", "a\u00b7ber", "sah", ",", "nach\u00b7h\u00e4n\u00b7gend", "ih\u00b7rem", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die froherstaunte Seele mir entf\u00fchrt.", "tokens": ["Die", "fro\u00b7her\u00b7staun\u00b7te", "See\u00b7le", "mir", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}