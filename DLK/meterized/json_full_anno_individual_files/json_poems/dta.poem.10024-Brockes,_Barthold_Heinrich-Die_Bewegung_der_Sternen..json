{"dta.poem.10024": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Bewegung der Sternen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich j\u00fcngst, zur Abend-Zeit im dunckeln", "tokens": ["In\u00b7dem", "ich", "j\u00fcngst", ",", "zur", "A\u00b7ben\u00b7dZeit", "im", "dun\u00b7ckeln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPRART", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der flammenden Gestirn\u2019 ergetzlich helles funckeln,", "tokens": ["Der", "flam\u00b7men\u00b7den", "Ge\u00b7stirn'", "er\u00b7getz\u00b7lich", "hel\u00b7les", "fun\u00b7ckeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Womit das tieffe Blau des Firmaments sich", "tokens": ["Wo\u00b7mit", "das", "tief\u00b7fe", "Blau", "des", "Fir\u00b7ma\u00b7ments", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit inniglich ger\u00fchrter Seel\u2019, erblickte;", "tokens": ["Mit", "in\u00b7nig\u00b7lich", "ge\u00b7r\u00fchr\u00b7ter", "Seel'", ",", "er\u00b7blick\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erf\u00fcllete mein reges Blut", "tokens": ["Er\u00b7f\u00fcl\u00b7le\u00b7te", "mein", "re\u00b7ges", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Die\u00df Himmels-Feur mit einer Himmels-Glut.", "tokens": ["Die\u00df", "Him\u00b7mels\u00b7Feur", "mit", "ei\u00b7ner", "Him\u00b7mels\u00b7Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich dachte nicht allein", "tokens": ["Ich", "dach\u00b7te", "nicht", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Derselben leuchten, strahlen, brennen", "tokens": ["Der\u00b7sel\u00b7ben", "leuch\u00b7ten", ",", "strah\u00b7len", ",", "bren\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mit froher Anmuth nach; es drang der rege Schein", "tokens": ["Mit", "fro\u00b7her", "An\u00b7muth", "nach", ";", "es", "drang", "der", "re\u00b7ge", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und dieser grossen C\u00f6rper rennen,", "tokens": ["Und", "die\u00b7ser", "gros\u00b7sen", "C\u00f6r\u00b7per", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Jhr feurigs unaufh\u00f6rlichs regen,", "tokens": ["Ihr", "feu\u00b7rigs", "un\u00b7auf\u00b7h\u00f6r\u00b7lichs", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Jhr unbegreifflich schnell bewegen,", "tokens": ["Ihr", "un\u00b7be\u00b7greif\u00b7flich", "schnell", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Mit welchem sie nie stille stehn,", "tokens": ["Mit", "wel\u00b7chem", "sie", "nie", "stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Seit dem sie GOTT erschuff, in meine Seel\u2019 hinein.", "tokens": ["Seit", "dem", "sie", "GoTT", "er\u00b7schuff", ",", "in", "mei\u00b7ne", "Seel'", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NE", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich stellte gantz erstaunet mir,", "tokens": ["Ich", "stell\u00b7te", "gantz", "er\u00b7stau\u00b7net", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Voll Schrecken halb, halb voll Vergn\u00fcgen,", "tokens": ["Voll", "Schre\u00b7cken", "halb", ",", "halb", "voll", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADJD", "$,", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Des Himmels rege C\u00f6rper f\u00fcr.", "tokens": ["Des", "Him\u00b7mels", "re\u00b7ge", "C\u00f6r\u00b7per", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Jhr so entsetzlich schnelles fliegen:", "tokens": ["Ihr", "so", "ent\u00b7setz\u00b7lich", "schnel\u00b7les", "flie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und dachte: sollt ein Mensch nur eine Viertel-Stunde", "tokens": ["Und", "dach\u00b7te", ":", "sollt", "ein", "Mensch", "nur", "ei\u00b7ne", "Vier\u00b7tel\u00b7Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "VMFIN", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In diesem Grentzen-losen Meere", "tokens": ["In", "die\u00b7sem", "Grent\u00b7zen\u00b7lo\u00b7sen", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Der Tieffen, die unendlich, sehn", "tokens": ["Der", "Tief\u00b7fen", ",", "die", "un\u00b7end\u00b7lich", ",", "sehn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$,", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wie Flammen-reich der Himmels-C\u00f6rper Heere", "tokens": ["Wie", "Flam\u00b7men\u00b7reich", "der", "Him\u00b7mels\u00b7C\u00f6\u00b7rper", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Daselbst so schrecklich schnell hell durch einander gehn;", "tokens": ["Da\u00b7selbst", "so", "schreck\u00b7lich", "schnell", "hell", "durch", "ein\u00b7an\u00b7der", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADJD", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Wie alles sonder Ruh und doch in Ordnung schwebe,", "tokens": ["Wie", "al\u00b7les", "son\u00b7der", "Ruh", "und", "doch", "in", "Ord\u00b7nung", "schwe\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie so viel Welte sich in solcher Eile lencken,", "tokens": ["Wie", "so", "viel", "Wel\u00b7te", "sich", "in", "sol\u00b7cher", "Ei\u00b7le", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und wirbelnd durch einander schwencken;", "tokens": ["Und", "wir\u00b7belnd", "durch", "ein\u00b7an\u00b7der", "schwen\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Unm\u00f6glich k\u00f6nnt er anders dencken,", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "k\u00f6nnt", "er", "an\u00b7ders", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Als da\u00df der gantze Himmel lebe.", "tokens": ["Als", "da\u00df", "der", "gant\u00b7ze", "Him\u00b7mel", "le\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Fast halb entz\u00fcckt durch die verhimmelnde Gedancken", "tokens": ["Fast", "halb", "ent\u00b7z\u00fcckt", "durch", "die", "ver\u00b7him\u00b7meln\u00b7de", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Zieht gleichsam sich mein Geist aus seines C\u00f6rpers Schran-", "tokens": ["Zieht", "gleich\u00b7sam", "sich", "mein", "Geist", "aus", "sei\u00b7nes", "C\u00f6r\u00b7pers", "Schran"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wagt es, sich mit allem dencken", "tokens": ["Und", "wagt", "es", ",", "sich", "mit", "al\u00b7lem", "den\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRF", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ins tieffen Himmels tieffste Tieffe,", "tokens": ["Ins", "tief\u00b7fen", "Him\u00b7mels", "tieffs\u00b7te", "Tief\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So tieff ihm m\u00f6glich, einzusencken.", "tokens": ["So", "tieff", "ihm", "m\u00f6g\u00b7lich", ",", "ein\u00b7zu\u00b7sen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt bin ich da. Mein GOtt! welch eine Schaar", "tokens": ["Jetzt", "bin", "ich", "da", ".", "Mein", "Gott", "!", "welch", "ei\u00b7ne", "Schaar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "PPOSAT", "NN", "$.", "PWAT", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von leuchtenden Planeten, welche sich", "tokens": ["Von", "leuch\u00b7ten\u00b7den", "Pla\u00b7ne\u00b7ten", ",", "wel\u00b7che", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PRF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "So lieblich hell, als schnell und f\u00fcrchterlich,", "tokens": ["So", "lieb\u00b7lich", "hell", ",", "als", "schnell", "und", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "KOUS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bewegen, drehn, und ohn verweilen", "tokens": ["Be\u00b7we\u00b7gen", ",", "drehn", ",", "und", "ohn", "ver\u00b7wei\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "CARD", "$,", "KON", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Blitze durch einander eilen,", "tokens": ["Wie", "Blit\u00b7ze", "durch", "ein\u00b7an\u00b7der", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird mein erstaunter Geist gewahr!", "tokens": ["Wird", "mein", "er\u00b7staun\u00b7ter", "Geist", "ge\u00b7wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Welch ein entsetzlich grosses Gantz,", "tokens": ["Welch", "ein", "ent\u00b7setz\u00b7lich", "gros\u00b7ses", "Gantz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gef\u00fcgt von Strahlen, Licht und Glantz,", "tokens": ["Ge\u00b7f\u00fcgt", "von", "Strah\u00b7len", ",", "Licht", "und", "Glantz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Stellt meiner Seelen Blick sich dar!", "tokens": ["Stellt", "mei\u00b7ner", "See\u00b7len", "Blick", "sich", "dar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O GOtt! wie wird, bey solcher regen Glut,", "tokens": ["O", "Gott", "!", "wie", "wird", ",", "bey", "sol\u00b7cher", "re\u00b7gen", "Glut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VAFIN", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir doch zu Muht!", "tokens": ["Mir", "doch", "zu", "Muht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ob diesen herrlichen Jdeen,", "tokens": ["Ob", "die\u00b7sen", "herr\u00b7li\u00b7chen", "Jdeen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die mir hiebey in meiner Seel\u2019 entstehen,", "tokens": ["Die", "mir", "hie\u00b7bey", "in", "mei\u00b7ner", "Seel'", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und die ich selbst nicht fassen kann,", "tokens": ["Und", "die", "ich", "selbst", "nicht", "fas\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tritt mich ein Seelen-Schwindel an.", "tokens": ["Tritt", "mich", "ein", "See\u00b7len\u00b7Schwin\u00b7del", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Geist, sammt aller Krafft, wird gleichsam hier ver-", "tokens": ["Der", "Geist", ",", "sammt", "al\u00b7ler", "Krafft", ",", "wird", "gleich\u00b7sam", "hier", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PIAT", "NN", "$,", "VAFIN", "ADJD", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mein Dencken wird mit \u00fcmgeschwungen.", "tokens": ["Mein", "Den\u00b7cken", "wird", "mit", "\u00fcm\u00b7ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}