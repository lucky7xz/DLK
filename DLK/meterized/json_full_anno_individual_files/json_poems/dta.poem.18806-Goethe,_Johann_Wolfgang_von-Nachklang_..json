{"dta.poem.18806": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Nachklang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Es klingt so pr\u00e4chtig, wenn der Dichter", "tokens": ["Es", "klingt", "so", "pr\u00e4ch\u00b7tig", ",", "wenn", "der", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sonne bald, dem Kaiser sich vergleicht;", "tokens": ["Der", "Son\u00b7ne", "bald", ",", "dem", "Kai\u00b7ser", "sich", "ver\u00b7gleicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch er verbirgt die traurigen Gesichter,", "tokens": ["Doch", "er", "ver\u00b7birgt", "die", "trau\u00b7ri\u00b7gen", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn er in d\u00fcstren N\u00e4chten schleicht.", "tokens": ["Wenn", "er", "in", "d\u00fcst\u00b7ren", "N\u00e4ch\u00b7ten", "schleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von Wolken streifenhaft befangen", "tokens": ["Von", "Wol\u00b7ken", "strei\u00b7fen\u00b7haft", "be\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Versank zu Nacht des Himmels reinstes Blau;", "tokens": ["Ver\u00b7sank", "zu", "Nacht", "des", "Him\u00b7mels", "reins\u00b7tes", "Blau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vermagert bleich sind meine Wangen", "tokens": ["Ver\u00b7ma\u00b7gert", "bleich", "sind", "mei\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Herzensthr\u00e4nen grau.", "tokens": ["Und", "mei\u00b7ne", "Her\u00b7zenst\u00b7hr\u00e4\u00b7nen", "grau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Lass mich nicht so der Nacht dem Schmerze,", "tokens": ["Lass", "mich", "nicht", "so", "der", "Nacht", "dem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du allerliebstes, du mein Mondgesicht!", "tokens": ["Du", "al\u00b7ler\u00b7liebs\u00b7tes", ",", "du", "mein", "Mond\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O du mein Phosphor, meine Kerze,", "tokens": ["O", "du", "mein", "Phos\u00b7phor", ",", "mei\u00b7ne", "Ker\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du meine Sonne, du mein Licht.", "tokens": ["Du", "mei\u00b7ne", "Son\u00b7ne", ",", "du", "mein", "Licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}