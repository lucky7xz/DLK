{"dta.poem.11039": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An meinem Geburtstage.  \n Beym Eintritt in das sieben und funfzigste Jahr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gottlob! heut sey ich abermal den Tag, woran ich diese\nWelt", "tokens": ["Gott\u00b7lob", "!", "heut", "sey", "ich", "a\u00b7ber\u00b7mal", "den", "Tag", ",", "wo\u00b7ran", "ich", "die\u00b7se", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Zum allerersten mal gesehn, gesund vergn\u00fcgt und fr\u00f6lich wieder.", "tokens": ["Zum", "al\u00b7le\u00b7rers\u00b7ten", "mal", "ge\u00b7sehn", ",", "ge\u00b7sund", "ver\u00b7gn\u00fcgt", "und", "fr\u00f6\u00b7lich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "VVPP", "$,", "ADJD", "VVPP", "KON", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Ich stim\u0303e dir demnach, o Herr, durch den, was worden, sich erh\u00e4lt,", "tokens": ["Ich", "stim\u0303e", "dir", "dem\u00b7nach", ",", "o", "Herr", ",", "durch", "den", ",", "was", "wor\u00b7den", ",", "sich", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "$,", "FM", "NN", "$,", "APPR", "ART", "$,", "PRELS", "VAPP", "$,", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "F\u00fcr die\u00df dein gn\u00e4diges Erhalten, Dank, Ehre, Ruhm-und Lo-", "tokens": ["F\u00fcr", "die\u00df", "dein", "gn\u00e4\u00b7di\u00b7ges", "Er\u00b7hal\u00b7ten", ",", "Dank", ",", "Eh\u00b7re", ",", "Ruhm\u00b7\u00b7und", "Lo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PDS", "PPOSAT", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Mit recht ger\u00fchrter Seelen, an. Wem anders, Herr, als dir allein,", "tokens": ["Mit", "recht", "ge\u00b7r\u00fchr\u00b7ter", "See\u00b7len", ",", "an", ".", "Wem", "an\u00b7ders", ",", "Herr", ",", "als", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "PTKVZ", "$.", "PWS", "ADV", "$,", "NN", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Kann ich f\u00fcr so viel Gnad und Huld, so sich in dieser Huld vereinen,", "tokens": ["Kann", "ich", "f\u00fcr", "so", "viel", "Gnad", "und", "Huld", ",", "so", "sich", "in", "die\u00b7ser", "Huld", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADV", "PIAT", "NN", "KON", "NN", "$,", "ADV", "PRF", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.7": {"text": "In welchen so viel gutes steckt, so wohl f\u00fcr mich, als f\u00fcr die", "tokens": ["In", "wel\u00b7chen", "so", "viel", "gu\u00b7tes", "steckt", ",", "so", "wohl", "f\u00fcr", "mich", ",", "als", "f\u00fcr", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADV", "PIAT", "ADJA", "VVFIN", "$,", "ADV", "ADV", "APPR", "PPER", "$,", "KOUS", "APPR", "ART"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "In Demuth-voller Lust und Ehrfurcht, verpflichtet und er-", "tokens": ["In", "De\u00b7muth\u00b7vol\u00b7ler", "Lust", "und", "Ehr\u00b7furcht", ",", "ver\u00b7pflich\u00b7tet", "und", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$,", "VVPP", "KON", "TRUNC"], "meter": "---+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ach la\u00df mich dir gef\u00e4llig danken! Es sind, von meinen Lebens-", "tokens": ["Ach", "la\u00df", "mich", "dir", "ge\u00b7f\u00e4l\u00b7lig", "dan\u00b7ken", "!", "Es", "sind", ",", "von", "mei\u00b7nen", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "PPER", "PPER", "ADJD", "VVINF", "$.", "PPER", "VAFIN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Nun sechs und funfzig Jahre schon, als wie ein Strom, dahin ge-", "tokens": ["Nun", "sechs", "und", "funf\u00b7zig", "Jah\u00b7re", "schon", ",", "als", "wie", "ein", "Strom", ",", "da\u00b7hin", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "CARD", "KON", "CARD", "NN", "ADV", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "PAV", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Ich tret heut in ein neues ein. Wer diese lange Zeit erwegt,", "tokens": ["Ich", "tret", "heut", "in", "ein", "neu\u00b7es", "ein", ".", "Wer", "die\u00b7se", "lan\u00b7ge", "Zeit", "er\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "PTKVZ", "$.", "PWS", "PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.12": {"text": "Aus wie viel N\u00e4chten, wie viel Tagen, aus wieviel Stunden", "tokens": ["Aus", "wie", "viel", "N\u00e4ch\u00b7ten", ",", "wie", "viel", "Ta\u00b7gen", ",", "aus", "wie\u00b7viel", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "KOKOM", "PIAT", "NN", "$,", "PWAV", "PIAT", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "+--+--++-+--+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Solch eine lange Daur besteht, und, wie er schuldig, \u00fcberlegt,", "tokens": ["Solch", "ei\u00b7ne", "lan\u00b7ge", "Daur", "be\u00b7steht", ",", "und", ",", "wie", "er", "schul\u00b7dig", ",", "\u00fc\u00b7ber\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "$,", "PWAV", "PPER", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.14": {"text": "Wie viel in einer jeglichen, die alle gl\u00fccklich sind verschwunden,", "tokens": ["Wie", "viel", "in", "ei\u00b7ner", "jeg\u00b7li\u00b7chen", ",", "die", "al\u00b7le", "gl\u00fcck\u00b7lich", "sind", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "PIAT", "$,", "PRELS", "PIS", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.15": {"text": "Mir Ungl\u00fcck \u00fcberkommen k\u00f6nnen; ja wenn er noch dabey", "tokens": ["Mir", "Un\u00b7gl\u00fcck", "\u00fc\u00b7ber\u00b7kom\u00b7men", "k\u00f6n\u00b7nen", ";", "ja", "wenn", "er", "noch", "da\u00b7bey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVINF", "VMINF", "$.", "ADV", "KOUS", "PPER", "ADV", "PAV"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Wie sich, mit dieser gro\u00dfen Zahl, noch  eine gr\u00f6\u00dfere verbunden,", "tokens": ["Wie", "sich", ",", "mit", "die\u00b7ser", "gro\u00b7\u00dfen", "Zahl", ",", "noch", "ei\u00b7ne", "gr\u00f6\u00b7\u00dfe\u00b7re", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "$,", "APPR", "PDAT", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.17": {"text": "Da Gott, in jeglichem der Meinen, mir ein vermehrtes Gut", "tokens": ["Da", "Gott", ",", "in", "jeg\u00b7li\u00b7chem", "der", "Mei\u00b7nen", ",", "mir", "ein", "ver\u00b7mehr\u00b7tes", "Gut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "APPR", "PIAT", "ART", "NN", "$,", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Und ihre, so wie meine Zeit, zugleich begl\u00fcckt verfliessen lassen,", "tokens": ["Und", "ih\u00b7re", ",", "so", "wie", "mei\u00b7ne", "Zeit", ",", "zu\u00b7gleich", "be\u00b7gl\u00fcckt", "ver\u00b7flies\u00b7sen", "las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "$,", "ADV", "KOKOM", "PPOSAT", "NN", "$,", "ADV", "VVPP", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.19": {"text": "Und, statt unz\u00e4hlig m\u00f6glichen Verdrusses, nichts auf uns gelenkt", "tokens": ["Und", ",", "statt", "un\u00b7z\u00e4h\u00b7lig", "m\u00f6g\u00b7li\u00b7chen", "Ver\u00b7drus\u00b7ses", ",", "nichts", "auf", "uns", "ge\u00b7lenkt"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUI", "ADJD", "ADJA", "NN", "$,", "PIS", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.20": {"text": "Als lauter Wohlfahrt, Heil und Segen; der wird, statt alle", "tokens": ["Als", "lau\u00b7ter", "Wohl\u00b7fahrt", ",", "Heil", "und", "Se\u00b7gen", ";", "der", "wird", ",", "statt", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "$,", "NN", "KON", "NN", "$.", "ART", "VAFIN", "$,", "KOUI", "PIAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Von ihrer Menge recht bedecket, in fr\u00f6licher Verwirrung stehn;", "tokens": ["Von", "ih\u00b7rer", "Men\u00b7ge", "recht", "be\u00b7de\u00b7cket", ",", "in", "fr\u00f6\u00b7li\u00b7cher", "Ver\u00b7wir\u00b7rung", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.22": {"text": "Er wird sich selbst, durch ihre Meng, in ihrer gro\u00dfen Quell", "tokens": ["Er", "wird", "sich", "selbst", ",", "durch", "ih\u00b7re", "Meng", ",", "in", "ih\u00b7rer", "gro\u00b7\u00dfen", "Quell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.23": {"text": "Sein armes Nichts und sein Verdienst, das nichts verdienet,", "tokens": ["Sein", "ar\u00b7mes", "Nichts", "und", "sein", "Ver\u00b7dienst", ",", "das", "nichts", "ver\u00b7die\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und nichts als G\u00fcte, nichts als Gnade, der m\u00e4chtig weisen", "tokens": ["Und", "nichts", "als", "G\u00fc\u00b7te", ",", "nichts", "als", "Gna\u00b7de", ",", "der", "m\u00e4ch\u00b7tig", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "NN", "$,", "PIS", "KOKOM", "NN", "$,", "PRELS", "ADJD", "VVINF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Die\u00df mein Erkennen deiner Gr\u00f6\u00dfe, und meines Nichts, weil", "tokens": ["Die\u00df", "mein", "Er\u00b7ken\u00b7nen", "dei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", ",", "und", "mei\u00b7nes", "Nichts", ",", "weil"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PDS", "PPOSAT", "NN", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$,", "KOUS"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "F\u00fcr so viel ungez\u00e4hlten Guts, nichts anders weis, nichts an-", "tokens": ["F\u00fcr", "so", "viel", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "Guts", ",", "nichts", "an\u00b7ders", "weis", ",", "nichts", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN", "$,", "PIS", "ADV", "PTKVZ", "$,", "PIS", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.27": {"text": "La\u00df dir, \u00f2 Vater aller Ding! um deiner ewgen Lieb allein,", "tokens": ["La\u00df", "dir", ",", "\u00f2", "Va\u00b7ter", "al\u00b7ler", "Ding", "!", "um", "dei\u00b7ner", "ew\u00b7gen", "Lieb", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJA", "NN", "PIAT", "NN", "$.", "APPR", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.28": {"text": "Der du die ewge Liebe bist, doch ein gef\u00e4lligs Opfer seyn!", "tokens": ["Der", "du", "die", "ew\u00b7ge", "Lie\u00b7be", "bist", ",", "doch", "ein", "ge\u00b7f\u00e4l\u00b7ligs", "Op\u00b7fer", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VAFIN", "$,", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.29": {"text": "Soll ich, nach deinem weisen Rath, an Jahren hier noch", "tokens": ["Soll", "ich", ",", "nach", "dei\u00b7nem", "wei\u00b7sen", "Rath", ",", "an", "Jah\u00b7ren", "hier", "noch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.30": {"text": "So fleh ich dich inbr\u00fcnstig an, Quell aller Gnaden, schenke mir,", "tokens": ["So", "fleh", "ich", "dich", "in\u00b7br\u00fcns\u00b7tig", "an", ",", "Quell", "al\u00b7ler", "Gna\u00b7den", ",", "schen\u00b7ke", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$,", "NN", "PIAT", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-++--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.31": {"text": "Zusamt den meinen, diese Gnade, da\u00df wir aus allen Kr\u00e4ften, dir", "tokens": ["Zu\u00b7samt", "den", "mei\u00b7nen", ",", "die\u00b7se", "Gna\u00b7de", ",", "da\u00df", "wir", "aus", "al\u00b7len", "Kr\u00e4f\u00b7ten", ",", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "VVFIN", "$,", "PDAT", "NN", "$,", "KOUS", "PPER", "APPR", "PIAT", "NN", "$,", "PPER"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.32": {"text": "In deiner Creatur Betrachtung, Vewundrung, Preis und", "tokens": ["In", "dei\u00b7ner", "Crea\u00b7tur", "Be\u00b7trach\u00b7tung", ",", "Ve\u00b7wund\u00b7rung", ",", "Preis", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "NN", "$,", "NN", "KON"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "La\u00df uns, in ihnen, deine Lieb und deine weise Macht erheben,", "tokens": ["La\u00df", "uns", ",", "in", "ih\u00b7nen", ",", "dei\u00b7ne", "Lieb", "und", "dei\u00b7ne", "wei\u00b7se", "Macht", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "PPER", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.34": {"text": "Und, da\u00df wir dir gef\u00e4llig werden, nach allen Kr\u00e4ften uns be-", "tokens": ["Und", ",", "da\u00df", "wir", "dir", "ge\u00b7f\u00e4l\u00b7lig", "wer\u00b7den", ",", "nach", "al\u00b7len", "Kr\u00e4f\u00b7ten", "uns", "be"], "token_info": ["word", "punct", "word", 