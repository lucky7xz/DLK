{"dta.poem.4137": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "August.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann sich die hohen goldnen Halme neigen,", "tokens": ["Wann", "sich", "die", "ho\u00b7hen", "gold\u00b7nen", "Hal\u00b7me", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann um den Aehrenkranz in wilden Reigen", "tokens": ["Wann", "um", "den", "A\u00b7eh\u00b7ren\u00b7kranz", "in", "wil\u00b7den", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Schnitter mit den Schnitterinnen springen:", "tokens": ["Die", "Schnit\u00b7ter", "mit", "den", "Schnit\u00b7te\u00b7rin\u00b7nen", "sprin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann will ein Jeder um die Stirne schlingen", "tokens": ["Dann", "will", "ein", "Je\u00b7der", "um", "die", "Stir\u00b7ne", "schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "PIS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein buntes Band, und sich als M\u00e4her zeigen;", "tokens": ["Ein", "bun\u00b7tes", "Band", ",", "und", "sich", "als", "M\u00e4\u00b7her", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PRF", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer ist so arm, da\u00df er sich nicht zu eigen", "tokens": ["Wer", "ist", "so", "arm", ",", "da\u00df", "er", "sich", "nicht", "zu", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "PTKA", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Saatenfeld und Saamen k\u00f6nnt' erringen?", "tokens": ["Ein", "Saa\u00b7ten\u00b7feld", "und", "Saa\u00b7men", "k\u00f6nnt'", "er\u00b7rin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Hoffnung pfl\u00fcgt f\u00fcr Alle das Gefilde,", "tokens": ["Die", "Hoff\u00b7nung", "pfl\u00fcgt", "f\u00fcr", "Al\u00b7le", "das", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und flinke W\u00fcnsche streun mit vollen H\u00e4nden", "tokens": ["Und", "flin\u00b7ke", "W\u00fcn\u00b7sche", "streun", "mit", "vol\u00b7len", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die K\u00f6rner in den weichen Schoo\u00df der Erden.", "tokens": ["Die", "K\u00f6r\u00b7ner", "in", "den", "wei\u00b7chen", "Schoo\u00df", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dir ist das Jahr mit den zw\u00f6lf Monden milde,", "tokens": ["Dir", "ist", "das", "Jahr", "mit", "den", "zw\u00f6lf", "Mon\u00b7den", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drum will ich dir die sch\u00e4rfste Sichel spenden,", "tokens": ["Drum", "will", "ich", "dir", "die", "sch\u00e4rfs\u00b7te", "Si\u00b7chel", "spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die nimmer stumpf soll in der Ernte werden.", "tokens": ["Die", "nim\u00b7mer", "stumpf", "soll", "in", "der", "Ern\u00b7te", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VMFIN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}