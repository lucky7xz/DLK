{"textgrid.poem.41979": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebchen, ich rathe dir jetzt Verderbliches meinen Gen\u00fcssen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebchen, ich rathe dir jetzt Verderbliches meinen Gen\u00fcssen,", "tokens": ["Lieb\u00b7chen", ",", "ich", "ra\u00b7the", "dir", "jetzt", "Ver\u00b7derb\u00b7li\u00b7ches", "mei\u00b7nen", "Ge\u00b7n\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber den eignen Gewinn achtet die Liebe ja nicht.", "tokens": ["A\u00b7ber", "den", "eig\u00b7nen", "Ge\u00b7winn", "ach\u00b7tet", "die", "Lie\u00b7be", "ja", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Achtet die Liebe Verlust, wenn nur Einem fr\u00f6hlicher Vortheil", "tokens": ["Ach\u00b7tet", "die", "Lie\u00b7be", "Ver\u00b7lust", ",", "wenn", "nur", "Ei\u00b7nem", "fr\u00f6h\u00b7li\u00b7cher", "Vor\u00b7theil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Reift, und der Andre betr\u00fcbt t\u00e4uschende Saaten beklagt?", "tokens": ["Reift", ",", "und", "der", "And\u00b7re", "be\u00b7tr\u00fcbt", "t\u00e4u\u00b7schen\u00b7de", "Saa\u00b7ten", "be\u00b7klagt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "PIS", "VVFIN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Siehe, du k\u00fcssest mich oft und winkst mir am Fenster und nickst mir,", "tokens": ["Sie\u00b7he", ",", "du", "k\u00fcs\u00b7sest", "mich", "oft", "und", "winkst", "mir", "am", "Fens\u00b7ter", "und", "nickst", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "L\u00e4chelst verstohlen und reichst heimlich das H\u00e4ndchen mir dar,", "tokens": ["L\u00e4\u00b7chelst", "ver\u00b7stoh\u00b7len", "und", "reichst", "heim\u00b7lich", "das", "H\u00e4nd\u00b7chen", "mir", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVFIN", "ADJD", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Lispelst Worte der zagenden Scham und Worte der Sehnsucht,", "tokens": ["Lis\u00b7pelst", "Wor\u00b7te", "der", "za\u00b7gen\u00b7den", "Scham", "und", "Wor\u00b7te", "der", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Worte der siegenden Lust still dem Umschlungenen zu,", "tokens": ["Wor\u00b7te", "der", "sie\u00b7gen\u00b7den", "Lust", "still", "dem", "Um\u00b7schlun\u00b7ge\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--++-+---+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Reichst ein Bl\u00fcmchen mir jetzt und jetzt mir des wallenden Busens", "tokens": ["Reichst", "ein", "Bl\u00fcm\u00b7chen", "mir", "jetzt", "und", "jetzt", "mir", "des", "wal\u00b7len\u00b7den", "Bu\u00b7sens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "KON", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Fesselndes Band und lohnst jegliches z\u00e4rtliche Lied.", "tokens": ["Fes\u00b7seln\u00b7des", "Band", "und", "lohnst", "jeg\u00b7li\u00b7ches", "z\u00e4rt\u00b7li\u00b7che", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Ach, nie drohn in dem Auge dir jetzt die Gew\u00f6lke des Unmuths,", "tokens": ["Ach", ",", "nie", "drohn", "in", "dem", "Au\u00b7ge", "dir", "jetzt", "die", "Ge\u00b7w\u00f6l\u00b7ke", "des", "Un\u00b7muths", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "APPR", "ART", "NN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-++--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Spottende Launen umziehn nimmer den rosigen Mund.", "tokens": ["Spot\u00b7ten\u00b7de", "Lau\u00b7nen", "um\u00b7ziehn", "nim\u00b7mer", "den", "ro\u00b7si\u00b7gen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "Fruchtlos schwimm' ich dahin in dem ruhigen Ocean, kaum noch", "tokens": ["Frucht\u00b7los", "schwimm'", "ich", "da\u00b7hin", "in", "dem", "ru\u00b7hi\u00b7gen", "O\u00b7cean", ",", "kaum", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PAV", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADV"], "meter": "+-+---+-++--+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Ahnet mein Herz, da\u00df es einst rauhere Wellen gekannt.", "tokens": ["Ah\u00b7net", "mein", "Herz", ",", "da\u00df", "es", "einst", "rau\u00b7he\u00b7re", "Wel\u00b7len", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Ach, schon ward ich verwandt mit dem Gl\u00fcck durch s\u00fc\u00dfe Gewohnheit,", "tokens": ["Ach", ",", "schon", "ward", "ich", "ver\u00b7wandt", "mit", "dem", "Gl\u00fcck", "durch", "s\u00fc\u00b7\u00dfe", "Ge\u00b7wohn\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Nimmer gen\u00fcgt, was einst selig mich machte, mir jetzt.", "tokens": ["Nim\u00b7mer", "ge\u00b7n\u00fcgt", ",", "was", "einst", "se\u00b7lig", "mich", "mach\u00b7te", ",", "mir", "jetzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "ADV", "ADJD", "PPER", "VVFIN", "$,", "PPER", "ADV", "$."], "meter": "+--+-----+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Theile die Gaben der Huld, o theile sie! Lust und Erwartung,", "tokens": ["Thei\u00b7le", "die", "Ga\u00b7ben", "der", "Huld", ",", "o", "thei\u00b7le", "sie", "!", "Lust", "und", "Er\u00b7war\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "FM", "VVFIN", "PPER", "$.", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Sehnsucht, Zagen und Furcht w\u00fcrze mir jegliche Gunst.", "tokens": ["Sehn\u00b7sucht", ",", "Za\u00b7gen", "und", "Furcht", "w\u00fcr\u00b7ze", "mir", "jeg\u00b7li\u00b7che", "Gunst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "KON", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "L\u00e4chle mir heute mit schmachtendem Blick und k\u00fcsse mich morgen,", "tokens": ["L\u00e4ch\u00b7le", "mir", "heu\u00b7te", "mit", "schmach\u00b7ten\u00b7dem", "Blick", "und", "k\u00fcs\u00b7se", "mich", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Und ein zartes Geschenk kr\u00f6ne den anderen Tag:", "tokens": ["Und", "ein", "zar\u00b7tes", "Ge\u00b7schenk", "kr\u00f6\u00b7ne", "den", "an\u00b7de\u00b7ren", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Doch es umd\u00fcstre das folgende Licht mir des schwarzen Gew\u00f6lks Nacht,", "tokens": ["Doch", "es", "um\u00b7d\u00fcs\u00b7tre", "das", "fol\u00b7gen\u00b7de", "Licht", "mir", "des", "schwar\u00b7zen", "Ge\u00b7w\u00f6lks", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "---+--+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Und bang zage das Herz unter der dr\u00fcckenden Luft,", "tokens": ["Und", "bang", "za\u00b7ge", "das", "Herz", "un\u00b7ter", "der", "dr\u00fc\u00b7cken\u00b7den", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+---+---+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Zittre dem Blitze des Hohns, der herabf\u00e4hrt aus der Umh\u00fcllung,", "tokens": ["Zitt\u00b7re", "dem", "Blit\u00b7ze", "des", "Hohns", ",", "der", "her\u00b7ab\u00b7f\u00e4hrt", "aus", "der", "Um\u00b7h\u00fcl\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Nach dem erquickenden Lenz sehn' es von neuem sich hin.", "tokens": ["Nach", "dem", "er\u00b7qui\u00b7cken\u00b7den", "Lenz", "sehn'", "es", "von", "neu\u00b7em", "sich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA", "PRF", "PTKVZ", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Schlau ja sah ich dich sonst und gewandt, stets war ich dein Sch\u00fcler.", "tokens": ["Schlau", "ja", "sah", "ich", "dich", "sonst", "und", "ge\u00b7wandt", ",", "stets", "war", "ich", "dein", "Sch\u00fc\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "PRF", "ADV", "KON", "VVPP", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Listige, z\u00fcrnest du nicht, da\u00df ich zum Lehrer gereift?", "tokens": ["Lis\u00b7ti\u00b7ge", ",", "z\u00fcr\u00b7nest", "du", "nicht", ",", "da\u00df", "ich", "zum", "Leh\u00b7rer", "ge\u00b7reift", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Schutzlos schlummert der ruhige Geist in der Wiege des Zutrauns;", "tokens": ["Schutz\u00b7los", "schlum\u00b7mert", "der", "ru\u00b7hi\u00b7ge", "Geist", "in", "der", "Wie\u00b7ge", "des", "Zu\u00b7trauns", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--++-+--+---+", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Trauest du meinem Wort, Liebchen, so traue mir nicht!", "tokens": ["Trau\u00b7est", "du", "mei\u00b7nem", "Wort", ",", "Lieb\u00b7chen", ",", "so", "trau\u00b7e", "mir", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.2": {"line.1": {"text": "Liebchen, ich rathe dir jetzt Verderbliches meinen Gen\u00fcssen,", "tokens": ["Lieb\u00b7chen", ",