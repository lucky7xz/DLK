, "measure": "hexameter"}, "line.18": {"text": "Sehnsucht, Zagen und Furcht w\u00fcrze mir jegliche Gunst.", "tokens": ["Sehn\u00b7sucht", ",", "Za\u00b7gen", "und", "Furcht", "w\u00fcr\u00b7ze", "mir", "jeg\u00b7li\u00b7che", "Gunst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "KON", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "L\u00e4chle mir heute mit schmachtendem Blick und k\u00fcsse mich morgen,", "tokens": ["L\u00e4ch\u00b7le", "mir", "heu\u00b7te", "mit", "schmach\u00b7ten\u00b7dem", "Blick", "und", "k\u00fcs\u00b7se", "mich", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Und ein zartes Geschenk kr\u00f6ne den anderen Tag:", "tokens": ["Und", "ein", "zar\u00b7tes", "Ge\u00b7schenk", "kr\u00f6\u00b7ne", "den", "an\u00b7de\u00b7ren", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Doch es umd\u00fcstre das folgende Licht mir des schwarzen Gew\u00f6lks Nacht,", "tokens": ["Doch", "es", "um\u00b7d\u00fcs\u00b7tre", "das", "fol\u00b7gen\u00b7de", "Licht", "mir", "des", "schwar\u00b7zen", "Ge\u00b7w\u00f6lks", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "---+--+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Und bang zage das Herz unter der