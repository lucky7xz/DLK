{"dta.poem.21436": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch giebt es H\u00f6llen aus deren Haft", "tokens": ["Doch", "giebt", "es", "H\u00f6l\u00b7len", "aus", "de\u00b7ren", "Haft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "PRELAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Unm\u00f6glich jede Befreiung;", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "je\u00b7de", "Be\u00b7frei\u00b7ung", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hier hilft kein Beten, ohnm\u00e4chtig ist hier", "tokens": ["Hier", "hilft", "kein", "Be\u00b7ten", ",", "ohn\u00b7m\u00e4ch\u00b7tig", "ist", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "ADJD", "VAFIN", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des Welterl\u00f6sers Verzeihung.", "tokens": ["Des", "Wel\u00b7ter\u00b7l\u00f6\u00b7sers", "Ver\u00b7zei\u00b7hung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Kennst du die H\u00f6lle des Dante nicht,", "tokens": ["Kennst", "du", "die", "H\u00f6l\u00b7le", "des", "Dan\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die schrecklichen Terzetten?", "tokens": ["Die", "schreck\u00b7li\u00b7chen", "Ter\u00b7zet\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Wen da der Dichter hineingesperrt,", "tokens": ["Wen", "da", "der", "Dich\u00b7ter", "hin\u00b7ein\u00b7ge\u00b7sperrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den kann kein Gott mehr retten \u2013", "tokens": ["Den", "kann", "kein", "Gott", "mehr", "ret\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kein Gott, kein Heiland, erl\u00f6st ihn je", "tokens": ["Kein", "Gott", ",", "kein", "Hei\u00b7land", ",", "er\u00b7l\u00f6st", "ihn", "je"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus diesen singenden Flammen!", "tokens": ["Aus", "die\u00b7sen", "sin\u00b7gen\u00b7den", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nimm dich in Acht, da\u00df wir dich nicht", "tokens": ["Nimm", "dich", "in", "Acht", ",", "da\u00df", "wir", "dich", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "CARD", "$,", "KOUS", "PPER", "PRF", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu solcher H\u00f6lle verdammen.", "tokens": ["Zu", "sol\u00b7cher", "H\u00f6l\u00b7le", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}