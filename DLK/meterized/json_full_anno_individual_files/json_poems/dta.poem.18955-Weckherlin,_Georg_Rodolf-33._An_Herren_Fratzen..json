{"dta.poem.18955": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "33.  \n  An Herren Fratzen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Fratz/ ewer buhl sich stehts beklaget/", "tokens": ["Fratz", "/", "e\u00b7wer", "buhl", "sich", "stehts", "be\u00b7kla\u00b7get", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df jhr den Huren gern nachjaget;", "tokens": ["Da\u00df", "jhr", "den", "Hu\u00b7ren", "gern", "nach\u00b7ja\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So rund zu reden ist f\u00fcrwahr", "tokens": ["So", "rund", "zu", "re\u00b7den", "ist", "f\u00fcr\u00b7wahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar frembd/ Ob es schon sonnenklar.", "tokens": ["Gar", "frembd", "/", "Ob", "es", "schon", "son\u00b7nen\u00b7klar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "KOUS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}