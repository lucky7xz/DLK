{"dta.poem.16990": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "91.  Die geheimbe Rose.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Ros\u2019 ist meine Seel/ der Dorn de\u00df Fleischeslust/", "tokens": ["Die", "Ros'", "ist", "mei\u00b7ne", "Seel", "/", "der", "Dorn", "de\u00df", "Flei\u00b7sches\u00b7lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Fr\u00fchling Gottes gunst/ sein Zorn ist K\u00e4lt und Frost:", "tokens": ["Der", "Fr\u00fch\u00b7ling", "Got\u00b7tes", "gunst", "/", "sein", "Zorn", "ist", "K\u00e4lt", "und", "Frost", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "$(", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr bl\u00fchn ist guttes thun/ den Dorn jhr Fleisch nicht ach-", "tokens": ["Ihr", "bl\u00fchn", "ist", "gut\u00b7tes", "thun", "/", "den", "Dorn", "jhr", "Fleisch", "nicht", "ach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VAFIN", "ADJA", "VVINF", "$(", "ART", "NN", "PPOSAT", "NN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ten/", "tokens": ["ten", "/"], "token_info": ["word", "punct"], "pos": ["FM", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Mit Tugenden sich ziehrn/ und nach dem Him\u0304el trachten:", "tokens": ["Mit", "Tu\u00b7gen\u00b7den", "sich", "ziehrn", "/", "und", "nach", "dem", "Him\u0304el", "trach\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$(", "KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Nim\u0304t sie die Zeit wol wahr/ und bl\u00fcht weils Fr\u00fchling ist/", "tokens": ["Nim\u0304t", "sie", "die", "Zeit", "wol", "wahr", "/", "und", "bl\u00fcht", "weils", "Fr\u00fch\u00b7ling", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$(", "KON", "VVFIN", "ADV", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wird sie ewiglich f\u00fcr GOttes Ros\u2019 erkiest.", "tokens": ["So", "wird", "sie", "e\u00b7wig\u00b7lich", "f\u00fcr", "Got\u00b7tes", "Ros'", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}