{"dta.poem.21681": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.85", "pt:0.14"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Matrosen auf dem Bramaputra", "tokens": ["Wie", "Mat\u00b7ro\u00b7sen", "auf", "dem", "Bra\u00b7ma\u00b7put\u00b7ra"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN"], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Feuerpfeile durch die D\u00e4mmrung schie\u00dfen,", "tokens": ["Feu\u00b7er\u00b7pfei\u00b7le", "durch", "die", "D\u00e4mm\u00b7rung", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Um den Pfad der Barken so zu leiten \u2014", "tokens": ["Um", "den", "Pfad", "der", "Bar\u00b7ken", "so", "zu", "lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie Na", "tokens": ["Wie", "Na"], "token_info": ["word", "word"], "pos": ["PWAV", "NE"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Eine Lampe setzen, zu erforschen", "tokens": ["Ei\u00b7ne", "Lam\u00b7pe", "set\u00b7zen", ",", "zu", "er\u00b7for\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ihres Liebsten Schicksal fern im Lager \u2014", "tokens": ["Ih\u00b7res", "Liebs\u00b7ten", "Schick\u00b7sal", "fern", "im", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "APPRART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "So schleud\u2019re ich Brandpfeile der Gedanken", "tokens": ["So", "schleu\u00b7d'\u00b7re", "ich", "Brand\u00b7pfei\u00b7le", "der", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "In n\u00e4cht\u2019ge Zukunft hin. Der Liebe Leuchte", "tokens": ["In", "n\u00e4cht'\u00b7ge", "Zu\u00b7kunft", "hin", ".", "Der", "Lie\u00b7be", "Leuch\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Schwimmt auf der Sturmfluth der Begierde, nimmer", "tokens": ["Schwimmt", "auf", "der", "Sturm\u00b7fluth", "der", "Be\u00b7gier\u00b7de", ",", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "ADV"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Versinkend. Wie man Blumen, Kokosn\u00fcsse", "tokens": ["Ver\u00b7sin\u00b7kend", ".", "Wie", "man", "Blu\u00b7men", ",", "Ko\u00b7kos\u00b7n\u00fcs\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "$.", "PWAV", "PIS", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Zur S\u00e4nft\u2019gung wirft in Babelmandeb\u2019s Brandung,", "tokens": ["Zur", "S\u00e4nft'\u00b7gung", "wirft", "in", "Babel\u00b7man\u00b7de\u00b7b's", "Bran\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.12": {"text": "So streu ich Liederrosen in mein Leben.", "tokens": ["So", "streu", "ich", "Lie\u00b7der\u00b7ro\u00b7sen", "in", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}