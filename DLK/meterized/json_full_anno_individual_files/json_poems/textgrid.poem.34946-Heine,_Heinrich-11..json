{"textgrid.poem.34946": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von der Gleichheit der Gem\u00fctsart", "tokens": ["Von", "der", "Gleich\u00b7heit", "der", "Ge\u00b7m\u00fc\u00b7tsart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wechselseitig angezogen,", "tokens": ["Wech\u00b7sel\u00b7sei\u00b7tig", "an\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Waren wir einander immer", "tokens": ["Wa\u00b7ren", "wir", "ein\u00b7an\u00b7der", "im\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr als uns bewu\u00dft gewogen.", "tokens": ["Mehr", "als", "uns", "be\u00b7wu\u00dft", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Beide ehrlich und bescheiden,", "tokens": ["Bei\u00b7de", "ehr\u00b7lich", "und", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Konnten wir uns leicht verstehen;", "tokens": ["Konn\u00b7ten", "wir", "uns", "leicht", "ver\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Worte waren \u00fcberfl\u00fcssig,", "tokens": ["Wor\u00b7te", "wa\u00b7ren", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brauchten uns nur anzusehen.", "tokens": ["Brauch\u00b7ten", "uns", "nur", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O wie sehnlich w\u00fcnscht ich immer,", "tokens": ["O", "wie", "sehn\u00b7lich", "w\u00fcnscht", "ich", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da\u00df ich bei dir bleiben k\u00f6nnte", "tokens": ["Da\u00df", "ich", "bei", "dir", "blei\u00b7ben", "k\u00f6nn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als der tapfre Waffenbruder", "tokens": ["Als", "der", "tapf\u00b7re", "Waf\u00b7fen\u00b7bru\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines Dolcefarniente.", "tokens": ["Ei\u00b7nes", "Dol\u00b7ce\u00b7far\u00b7ni\u00b7en\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, mein liebster Wunsch war immer,", "tokens": ["Ja", ",", "mein", "liebs\u00b7ter", "Wunsch", "war", "im\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich immer bei dir bliebe!", "tokens": ["Da\u00df", "ich", "im\u00b7mer", "bei", "dir", "blie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles, was dir wohlgefiele,", "tokens": ["Al\u00b7les", ",", "was", "dir", "wohl\u00b7ge\u00b7fie\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles t\u00e4t ich dir zuliebe.", "tokens": ["Al\u00b7les", "t\u00e4t", "ich", "dir", "zu\u00b7lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "W\u00fcrde essen, was dir schmeckte,", "tokens": ["W\u00fcr\u00b7de", "es\u00b7sen", ",", "was", "dir", "schmeck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Sch\u00fcssel gleich entfernen,", "tokens": ["Und", "die", "Sch\u00fcs\u00b7sel", "gleich", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die dir nicht behagt. Ich w\u00fcrde", "tokens": ["Die", "dir", "nicht", "be\u00b7hagt", ".", "Ich", "w\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "PTKNEG", "VVPP", "$.", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch Zigarren rauchen lernen.", "tokens": ["Auch", "Zi\u00b7gar\u00b7ren", "rau\u00b7chen", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Manche polnische Geschichte,", "tokens": ["Man\u00b7che", "pol\u00b7ni\u00b7sche", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dein Lachen immer weckte,", "tokens": ["Die", "dein", "La\u00b7chen", "im\u00b7mer", "weck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollt ich wieder dir erz\u00e4hlen", "tokens": ["Wollt", "ich", "wie\u00b7der", "dir", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In Jud\u00e4as Dialekte.", "tokens": ["In", "Ju\u00b7d\u00e4\u00b7as", "Di\u00b7a\u00b7lek\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Ja, ich wollte zu dir kommen,", "tokens": ["Ja", ",", "ich", "woll\u00b7te", "zu", "dir", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht mehr in der Fremde schw\u00e4rmen \u2013", "tokens": ["Nicht", "mehr", "in", "der", "Frem\u00b7de", "schw\u00e4r\u00b7men", "\u2013"], 