{"dta.poem.12553": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "LxIII.  \n \u201eQuid novi ex Africa?\u201c", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewanderer steh'! und sage mir an, in welcher", "tokens": ["\u201e", "wan\u00b7de\u00b7rer", "steh'", "!", "und", "sa\u00b7ge", "mir", "an", ",", "in", "wel\u00b7cher"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PWAV", "VVFIN", "$.", "KON", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PRELS"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ihr das gepriesene Volk jener Borussen", "tokens": ["Ihr", "das", "ge\u00b7prie\u00b7se\u00b7ne", "Volk", "je\u00b7ner", "Bo\u00b7rus\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "PDAT", "NN"], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sind die Poeten noch nicht im Preise gestiegen,", "tokens": ["Sind", "die", "Po\u00b7et\u00b7en", "noch", "nicht", "im", "Prei\u00b7se", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PTKNEG", "APPRART", "NN", "VVPP", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Immer die F\u00e4hndriche noch doppelten Dich-", "tokens": ["Im\u00b7mer", "die", "F\u00e4hn\u00b7dri\u00b7che", "noch", "dop\u00b7pel\u00b7ten", "Dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "ADJA", "TRUNC"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Junkert man immer noch viel und sch\u00e4tzt", "tokens": ["Jun\u00b7kert", "man", "im\u00b7mer", "noch", "viel", "und", "sch\u00e4tzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PIS", "ADV", "ADV", "ADV", "KON", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nur nach der Gr\u00f6\u00dfe des Wurms, der sich", "tokens": ["Nur", "nach", "der", "Gr\u00f6\u00b7\u00dfe", "des", "Wurms", ",", "der", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PRF"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Mehrt die Canaille sich stark, seit j\u00fcngst in", "tokens": ["Mehrt", "die", "Ca\u00b7nail\u00b7le", "sich", "stark", ",", "seit", "j\u00fcngst", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PRF", "ADJD", "$,", "KOUS", "ADV", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df ein adliger Lump werde zum B\u00fcrger", "tokens": ["Da\u00df", "ein", "ad\u00b7li\u00b7ger", "Lump", "wer\u00b7de", "zum", "B\u00fcr\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VAFIN", "APPRART", "NN"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Wie viel Pfaffen, o sprich, wie viel Trompeter", "tokens": ["Wie", "viel", "Pfaf\u00b7fen", ",", "o", "sprich", ",", "wie", "viel", "Trom\u00b7pe\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "FM", "ADJD", "$,", "PWAV", "PIAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Wie viel Heilige stehn bei den Ministern", "tokens": ["Wie", "viel", "Hei\u00b7li\u00b7ge", "stehn", "bei", "den", "Mi\u00b7nis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+---+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Hat sich der Himmel geb\u00fchrlich bedankt schon", "tokens": ["Hat", "sich", "der", "Him\u00b7mel", "ge\u00b7b\u00fchr\u00b7lich", "be\u00b7dankt", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ART", "NN", "ADJD", "VVFIN", "ADV"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Besserer Feier, die Ihm seine Getreuen", "tokens": ["Bes\u00b7se\u00b7rer", "Fei\u00b7er", ",", "die", "Ihm", "sei\u00b7ne", "Ge\u00b7treu\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.13": {"text": "Dann von der Staatszeitung zweideutigem", "tokens": ["Dann", "von", "der", "Staats\u00b7zei\u00b7tung", "zwei\u00b7deu\u00b7ti\u00b7gem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA"], "meter": "++--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Wer doch erfreut nunmehr ihrer Umar-", "tokens": ["Wer", "doch", "er\u00b7freut", "nun\u00b7mehr", "ih\u00b7rer", "Um\u00b7a\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "ADV", "PPOSAT", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Z\u00e4hlt sie noch immer, o Glanz! drei Leser auf", "tokens": ["Z\u00e4hlt", "sie", "noch", "im\u00b7mer", ",", "o", "Glanz", "!", "drei", "Le\u00b7ser", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "FM", "NN", "$.", "CARD", "NN", "APPR"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "Schmiert sie dem russischen B\u00e4r immer noch", "tokens": ["Schmiert", "sie", "dem", "rus\u00b7si\u00b7schen", "B\u00e4r", "im\u00b7mer", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Seit sich der F\u00fcrsten Romantiker j\u00fcngst mit", "tokens": ["Seit", "sich", "der", "F\u00fcrs\u00b7ten", "Ro\u00b7man\u00b7ti\u00b7ker", "j\u00fcngst", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ART", "NN", "NN", "ADV", "APPR"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.18": {"text": "Enge verbunden, wie ist's um das Theater", "tokens": ["En\u00b7ge", "ver\u00b7bun\u00b7den", ",", "wie", "ist's", "um", "das", "The\u00b7a\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "PWAV", "VAFIN", "APPR", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.19": {"text": "Liest er noch immer so h\u00fcbsch, der Tick?", "tokens": ["Liest", "er", "noch", "im\u00b7mer", "so", "h\u00fcbsch", ",", "der", "Tick", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "Was machen die Alten?", "tokens": ["Was", "ma\u00b7chen", "die", "Al\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Welche Kom\u00f6die wird eben bei Hofe stu-", "tokens": ["Wel\u00b7che", "Ko\u00b7m\u00f6\u00b7die", "wird", "e\u00b7ben", "bei", "Ho\u00b7fe", "stu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "VAFIN", "ADV", "APPR", "NN", "TRUNC"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Ist Reinecke der Fuchs bei seinem erhabenen", "tokens": ["Ist", "Rein\u00b7e\u00b7cke", "der", "Fuchs", "bei", "sei\u00b7nem", "er\u00b7ha\u00b7be\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ART", "NE", "APPR", "PPOSAT", "ADJA"], "meter": "--+--+-+--+--", "measure": "anapaest.di.plus"}, "line.23": {"text": "Oder sein Schwager, der Petz, wieder ein-", "tokens": ["O\u00b7der", "sein", "Schwa\u00b7ger", ",", "der", "Petz", ",", "wie\u00b7der", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "ADV", "TRUNC"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}