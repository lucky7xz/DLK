{"dta.poem.19453": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Ueber einem Grabe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00fcthen schweben \u00fcber deinem Grabe.", "tokens": ["Bl\u00fc\u00b7then", "schwe\u00b7ben", "\u00fc\u00b7ber", "dei\u00b7nem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schnell umarmte dich der Tod, o Knabe,", "tokens": ["Schnell", "um\u00b7arm\u00b7te", "dich", "der", "Tod", ",", "o", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Den wir Alle liebten, die dich kannten,", "tokens": ["Den", "wir", "Al\u00b7le", "lieb\u00b7ten", ",", "die", "dich", "kann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dessen Augen wie zwei Sonnen brannten,", "tokens": ["Des\u00b7sen", "Au\u00b7gen", "wie", "zwei", "Son\u00b7nen", "brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KOKOM", "CARD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dessen Blicke Seelen unterjochten,", "tokens": ["Des\u00b7sen", "Bli\u00b7cke", "See\u00b7len", "un\u00b7ter\u00b7joch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dessen Pulse stark und feurig pochten,", "tokens": ["Des\u00b7sen", "Pul\u00b7se", "stark", "und", "feu\u00b7rig", "poch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dessen Worte schon die Herzen lenkten,", "tokens": ["Des\u00b7sen", "Wor\u00b7te", "schon", "die", "Her\u00b7zen", "lenk\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Den wir weinend gestern hier versenkten.", "tokens": ["Den", "wir", "wei\u00b7nend", "ge\u00b7stern", "hier", "ver\u00b7senk\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Maiennacht. Der Sterne mildes Schweigen ...", "tokens": ["Mai\u00b7en\u00b7nacht", ".", "Der", "Ster\u00b7ne", "mil\u00b7des", "Schwei\u00b7gen", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dort! ich seh' es aus der Erde steigen!", "tokens": ["Dort", "!", "ich", "seh'", "es", "aus", "der", "Er\u00b7de", "stei\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unterm Rasen quillt hervor es leise,", "tokens": ["Un\u00b7term", "Ra\u00b7sen", "quillt", "her\u00b7vor", "es", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Flatterflammen drehen sich im Kreise,", "tokens": ["Flat\u00b7ter\u00b7flam\u00b7men", "dre\u00b7hen", "sich", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ungelebtes Leben zuckt und lodert", "tokens": ["Un\u00b7ge\u00b7leb\u00b7tes", "Le\u00b7ben", "zuckt", "und", "lo\u00b7dert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aus der K\u00f6rperkraft, die hier vermodert,", "tokens": ["Aus", "der", "K\u00f6r\u00b7per\u00b7kraft", ",", "die", "hier", "ver\u00b7mo\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Abgem\u00e4hter Jugend letztes Walten,", "tokens": ["Ab\u00b7ge\u00b7m\u00e4h\u00b7ter", "Ju\u00b7gend", "letz\u00b7tes", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Letzte Glut verraucht in Wunschgestalten,", "tokens": ["Letz\u00b7te", "Glut", "ver\u00b7raucht", "in", "Wunschge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Eine blasse Jagd:", "tokens": ["Ei\u00b7ne", "blas\u00b7se", "Jagd", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Voran ein Zecher,", "tokens": ["Vo\u00b7ran", "ein", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In der Faust den \u00fcberf\u00fcllten Becher!", "tokens": ["In", "der", "Faust", "den", "\u00fc\u00b7berf\u00b7\u00fcll\u00b7ten", "Be\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weh'nde Locken will der Buhle fassen,", "tokens": ["Weh'n\u00b7de", "Lo\u00b7cken", "will", "der", "Buh\u00b7le", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die entflatternd nicht sich haschen lassen,", "tokens": ["Die", "ent\u00b7flat\u00b7ternd", "nicht", "sich", "ha\u00b7schen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PTKNEG", "PRF", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Lustgestachelt rast er hinter jenen,", "tokens": ["Lust\u00b7ge\u00b7sta\u00b7chelt", "rast", "er", "hin\u00b7ter", "je\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PDAT", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ein verh\u00fclltes M\u00e4dchen folgt in Thr\u00e4nen.", "tokens": ["Ein", "ver\u00b7h\u00fcll\u00b7tes", "M\u00e4d\u00b7chen", "folgt", "in", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch die Brandung mit verst\u00fcrmten Haaren", "tokens": ["Durch", "die", "Bran\u00b7dung", "mit", "ver\u00b7st\u00fcrm\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Seh' ich einen k\u00fchnen Schiffer fahren.", "tokens": ["Seh'", "ich", "ei\u00b7nen", "k\u00fch\u00b7nen", "Schif\u00b7fer", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einen jungen Krieger seh' ich toben,", "tokens": ["Ei\u00b7nen", "jun\u00b7gen", "Krie\u00b7ger", "seh'", "ich", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Helmbedeckt, das lichte Schwert erhoben.", "tokens": ["Helm\u00b7be\u00b7deckt", ",", "das", "lich\u00b7te", "Schwert", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Einer st\u00fcrzt sich auf die Rednerb\u00fchne,", "tokens": ["Ei\u00b7ner", "st\u00fcrzt", "sich", "auf", "die", "Red\u00b7ner\u00b7b\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Weites Volksgetos beherrscht der K\u00fchne.", "tokens": ["Wei\u00b7tes", "Volks\u00b7ge\u00b7tos", "be\u00b7herrscht", "der", "K\u00fch\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ein Gedr\u00e4ng, ein K\u00e4mpfen, Ringen, Streben!", "tokens": ["Ein", "Ge\u00b7dr\u00e4ng", ",", "ein", "K\u00e4mp\u00b7fen", ",", "Rin\u00b7gen", ",", "Stre\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Arme strecken sich und Kr\u00e4nze schweben \u2014", "tokens": ["Ar\u00b7me", "stre\u00b7cken", "sich", "und", "Kr\u00e4n\u00b7ze", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Kr\u00e4nze wenn du lebtest, dir beschieden,", "tokens": ["Kr\u00e4n\u00b7ze", "wenn", "du", "leb\u00b7test", ",", "dir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht erreichte!", "tokens": ["Nicht", "er\u00b7reich\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Knabe, schlaf' in Frieden!", "tokens": ["Kna\u00b7be", ",", "schlaf'", "in", "Frie\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}