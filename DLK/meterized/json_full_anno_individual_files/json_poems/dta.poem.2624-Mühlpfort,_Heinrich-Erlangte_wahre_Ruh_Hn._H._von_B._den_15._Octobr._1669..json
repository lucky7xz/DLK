{"dta.poem.2624": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erlangte wahre Ruh/  \n Hn. H. von B. den 15. Octobr. 1669.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Woldem der seine Ruh kan in sich selbsten finden/", "tokens": ["Wol\u00b7dem", "der", "sei\u00b7ne", "Ruh", "kan", "in", "sich", "selbs\u00b7ten", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "VMFIN", "APPR", "PRF", "ADV", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Den keine Regung nicht zu ihrem Sclaven macht/", "tokens": ["Den", "kei\u00b7ne", "Re\u00b7gung", "nicht", "zu", "ih\u00b7rem", "Scla\u00b7ven", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der seinen Willen kan vern\u00fcnfftig \u00fcberwinden/", "tokens": ["Der", "sei\u00b7nen", "Wil\u00b7len", "kan", "ver\u00b7n\u00fcnff\u00b7tig", "\u00fc\u00b7berw\u00b7in\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nit nach falschem Schein der hohen Dinge tracht.", "tokens": ["Und", "nit", "nach", "fal\u00b7schem", "Schein", "der", "ho\u00b7hen", "Din\u00b7ge", "tracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der/ wie die alte Welt/ ein gleiches Leben f\u00fchret/", "tokens": ["Der", "/", "wie", "die", "al\u00b7te", "Welt", "/", "ein", "glei\u00b7ches", "Le\u00b7ben", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOKOM", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Unschuld erster Zeit ihm zum Exempel stellt/", "tokens": ["Die", "Un\u00b7schuld", "ers\u00b7ter", "Zeit", "ihm", "zum", "Ex\u00b7em\u00b7pel", "stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Dem nie der Hoffarts-Geist das innre Hertz ber\u00fchret/", "tokens": ["Dem", "nie", "der", "Hoff\u00b7arts\u00b7Geist", "das", "inn\u00b7re", "Hertz", "be\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der grosser H\u00f6ffe Pracht f\u00fcr Dampff und Nebel h\u00e4lt:", "tokens": ["Der", "gros\u00b7ser", "H\u00f6f\u00b7fe", "Pracht", "f\u00fcr", "Dampff", "und", "Ne\u00b7bel", "h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der f\u00fcr sein h\u00f6chstes Gut ein ruhiges Gewissen/", "tokens": ["Der", "f\u00fcr", "sein", "h\u00f6chs\u00b7tes", "Gut", "ein", "ru\u00b7hi\u00b7ges", "Ge\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und f\u00fcr den sch\u00f6nsten Schmuck die g\u00f6ldne Freyheit sch\u00e4tzt/", "tokens": ["Und", "f\u00fcr", "den", "sch\u00f6ns\u00b7ten", "Schmuck", "die", "g\u00f6ld\u00b7ne", "Frey\u00b7heit", "sch\u00e4tzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der nicht auf Eitelkeit des P\u00f6fels ist beflissen/", "tokens": ["Der", "nicht", "auf", "Ei\u00b7tel\u00b7keit", "des", "P\u00f6\u00b7fels", "ist", "be\u00b7flis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und seinen gantzen Wunsch auf andrer Schmeicheln setzt;", "tokens": ["Und", "sei\u00b7nen", "gant\u00b7zen", "Wunsch", "auf", "an\u00b7drer", "Schmei\u00b7cheln", "setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der mit sich selbsten Freund die falschen Freunde hasset/", "tokens": ["Der", "mit", "sich", "selbs\u00b7ten", "Freund", "die", "fal\u00b7schen", "Freun\u00b7de", "has\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und Einfalt von der List zu unterscheiden wei\u00df/", "tokens": ["Und", "Ein\u00b7falt", "von", "der", "List", "zu", "un\u00b7ter\u00b7schei\u00b7den", "wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "In eine solche Form sein gantzes Thun verfasset/", "tokens": ["In", "ei\u00b7ne", "sol\u00b7che", "Form", "sein", "gant\u00b7zes", "Thun", "ver\u00b7fas\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die richtig Kugelrund als wie der Erden-Krei\u00df:", "tokens": ["Die", "rich\u00b7tig", "Ku\u00b7gel\u00b7rund", "als", "wie", "der", "Er\u00b7den\u00b7Krei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der sich zuviel nicht freut/ und nicht zu sehr betr\u00fcbet/", "tokens": ["Der", "sich", "zu\u00b7viel", "nicht", "freut", "/", "und", "nicht", "zu", "sehr", "be\u00b7tr\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "PTKNEG", "VVFIN", "$(", "KON", "PTKNEG", "PTKA", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bey hellem Wetter so als wie beym schwartzen sitzt/", "tokens": ["Bey", "hel\u00b7lem", "Wet\u00b7ter", "so", "als", "wie", "beym", "schwart\u00b7zen", "sitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "KOUS", "KOKOM", "APPRART", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Gott und der Tugend nur sich eintzig \u00fcbergibet/", "tokens": ["Gott", "und", "der", "Tu\u00b7gend", "nur", "sich", "eint\u00b7zig", "\u00fc\u00b7ber\u00b7gi\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "ADV", "PRF", "ADJD", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.20": {"text": "Und wolgemuthet steht wenn gleich der Himmel plitzt.", "tokens": ["Und", "wol\u00b7ge\u00b7mu\u00b7thet", "steht", "wenn", "gleich", "der", "Him\u00b7mel", "plitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVFIN", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der wird ein gr\u00fcnes Feld vor die Pall\u00e4st erwehlen/", "tokens": ["Der", "wird", "ein", "gr\u00fc\u00b7nes", "Feld", "vor", "die", "Pal\u00b7l\u00e4st", "er\u00b7weh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Weil dort die Anmuth selbst hier nichts als Kummer bl\u00fcht/", "tokens": ["Weil", "dort", "die", "An\u00b7muth", "selbst", "hier", "nichts", "als", "Kum\u00b7mer", "bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ADV", "PIS", "KOKOM", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der seine Stunden nicht darff nach der Uhr abzehlen/", "tokens": ["Der", "sei\u00b7ne", "Stun\u00b7den", "nicht", "darff", "nach", "der", "Uhr", "ab\u00b7zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "VMFIN", "APPR", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Noch da\u00df er halbbest\u00fcrtzt auf andrer wincken sieht:", "tokens": ["Noch", "da\u00df", "er", "halb\u00b7be\u00b7st\u00fcrtzt", "auf", "an\u00b7drer", "win\u00b7cken", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "APPR", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein Blumen-reiches Gra\u00df beliebt ihm vor Tapeten/", "tokens": ["Ein", "Blu\u00b7men\u00b7rei\u00b7ches", "Gra\u00df", "be\u00b7liebt", "ihm", "vor", "Ta\u00b7pe\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weil jene nur die Kunst und diese Flora mahlt/", "tokens": ["Weil", "je\u00b7ne", "nur", "die", "Kunst", "und", "die\u00b7se", "Flo\u00b7ra", "mahlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ART", "NN", "KON", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und darff drauf au\u00dfgestreckt vor keinem nicht err\u00f6then/", "tokens": ["Und", "darff", "drauf", "au\u00df\u00b7ge\u00b7streckt", "vor", "kei\u00b7nem", "nicht", "er\u00b7r\u00f6\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "VVPP", "APPR", "PIS", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und ob er noch so sehr von grosser Hoheit strahlt.", "tokens": ["Und", "ob", "er", "noch", "so", "sehr", "von", "gros\u00b7ser", "Ho\u00b7heit", "strahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So lebte Cicero wenn er sein Rom verlassen/", "tokens": ["So", "leb\u00b7te", "Ci\u00b7ce\u00b7ro", "wenn", "er", "sein", "Rom", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KOUS", "PPER", "PPOSAT", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und sich der W\u00fcrde B\u00fcrd\u2019 entband in Tusculan/", "tokens": ["Und", "sich", "der", "W\u00fcr\u00b7de", "B\u00fcrd'", "ent\u00b7band", "in", "Tus\u00b7cu\u00b7lan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "NN", "VVFIN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Es konte der Horaz mehr frische Geister fassen/", "tokens": ["Es", "kon\u00b7te", "der", "Ho\u00b7raz", "mehr", "fri\u00b7sche", "Geis\u00b7ter", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "Wenn er auf seinem ", "tokens": ["Wenn", "er", "auf", "sei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.33": {"text": "Entfreit von Schlo\u00df und Hof/ entfreit vom Lieb dienen/", "tokens": ["Ent\u00b7freit", "von", "Schlo\u00df", "und", "Hof", "/", "ent\u00b7freit", "vom", "Lieb", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$(", "APPR", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.34": {"text": "Sein eigen Herr und Knecht/ und doch de\u00df K\u00e4ysers Freund.", "tokens": ["Sein", "ei\u00b7gen", "Herr", "und", "Knecht", "/", "und", "doch", "de\u00df", "K\u00e4y\u00b7sers", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$(", "KON", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sagt nicht der Plinius da\u00df alle Sinnen gr\u00fcnen/", "tokens": ["Sagt", "nicht", "der", "Pli\u00b7nius", "da\u00df", "al\u00b7le", "Sin\u00b7nen", "gr\u00fc\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NE", "KOUS", "PIAT", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Wenn er zu Laurentz lebt und da die Sonn ihm scheint?", "tokens": ["Wenn", "er", "zu", "Lau\u00b7rentz", "lebt", "und", "da", "die", "Sonn", "ihm", "scheint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "KON", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie lobet Sannazar sein Gut die Marginille?", "tokens": ["Wie", "lo\u00b7bet", "San\u00b7na\u00b7zar", "sein", "Gut", "die", "Mar\u00b7gi\u00b7nil\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wie sang nicht f\u00fcr Florentz Politian im Thal?", "tokens": ["Wie", "sang", "nicht", "f\u00fcr", "Flo\u00b7rentz", "Po\u00b7li\u00b7ti\u00b7an", "im", "Thal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PTKNEG", "APPR", "NE", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sch\u00e4tzt nicht Lotichius der gr\u00fcnen G\u00e4rten stille/", "tokens": ["Sch\u00e4tzt", "nicht", "Lo\u00b7ti\u00b7chius", "der", "gr\u00fc\u00b7nen", "G\u00e4r\u00b7ten", "stil\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NE", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.40": {"text": "Und einen frischen Brunn mehr als der Printzen Stahl:", "tokens": ["Und", "ei\u00b7nen", "fri\u00b7schen", "Brunn", "mehr", "als", "der", "Print\u00b7zen", "Stahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PIAT", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der Weisen meister Schlu\u00df der stimmet hier zusammen/", "tokens": ["Der", "Wei\u00b7sen", "meis\u00b7ter", "Schlu\u00df", "der", "stim\u00b7met", "hier", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ART", "VVFIN", "ADV", "PTKVZ", "$("], "meter":