{"textgrid.poem.43364": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "1L: So warst du. Jetzt hier unten", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So warst du. Jetzt hier unten", "tokens": ["So", "warst", "du", ".", "Jetzt", "hier", "un\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$.", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bist du Geripp und Staub. Bewegungslos", "tokens": ["Bist", "du", "Ge\u00b7ripp", "und", "Staub", ".", "Be\u00b7we\u00b7gungs\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "NE", "KON", "NN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Steht, \u00fcber deinem modernden Gebein", "tokens": ["Steht", ",", "\u00fc\u00b7ber", "dei\u00b7nem", "mo\u00b7dern\u00b7den", "Ge\u00b7bein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Stumm blickend in der Zeiten Strom hinein,", "tokens": ["Stumm", "bli\u00b7ckend", "in", "der", "Zei\u00b7ten", "Strom", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur noch als H\u00fcterin", "tokens": ["Nur", "noch", "als", "H\u00fc\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Trauer und Erinnrung dieses Abbild", "tokens": ["Der", "Trau\u00b7er", "und", "E\u00b7rinn\u00b7rung", "die\u00b7ses", "Ab\u00b7bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verschwundner Sch\u00f6nheit. Jener s\u00fc\u00dfe Blick,", "tokens": ["Ver\u00b7schwund\u00b7ner", "Sch\u00f6n\u00b7heit", ".", "Je\u00b7ner", "s\u00fc\u00b7\u00dfe", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der zittern machte, wenn er still, wie jetzt,", "tokens": ["Der", "zit\u00b7tern", "mach\u00b7te", ",", "wenn", "er", "still", ",", "wie", "jetzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PTKVZ", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Auf einem Antlitz ruhte; jene Lippe,", "tokens": ["Auf", "ei\u00b7nem", "Ant\u00b7litz", "ruh\u00b7te", ";", "je\u00b7ne", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$.", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die wie ein voller Becher", "tokens": ["Die", "wie", "ein", "vol\u00b7ler", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Von Wonnen \u00fcbertr\u00e4ufte, jener Nacken,", "tokens": ["Von", "Won\u00b7nen", "\u00fc\u00b7bert\u00b7r\u00e4uf\u00b7te", ",", "je\u00b7ner", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Den Sehnsucht einst umarmte; jene weiche", "tokens": ["Den", "Sehn\u00b7sucht", "einst", "um\u00b7arm\u00b7te", ";", "je\u00b7ne", "wei\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Hand, die so oft gef\u00fchlt,", "tokens": ["Hand", ",", "die", "so", "oft", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Wie kalt und feucht die Hand ward, die sie dr\u00fcckte;", "tokens": ["Wie", "kalt", "und", "feucht", "die", "Hand", "ward", ",", "die", "sie", "dr\u00fcck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der Busen, dessen Wallen", "tokens": ["Der", "Bu\u00b7sen", ",", "des\u00b7sen", "Wal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Erblassen machte Den, der ihn erblickte,", "tokens": ["Er\u00b7blas\u00b7sen", "mach\u00b7te", "Den", ",", "der", "ihn", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Dies Alles ", "tokens": ["Dies", "Al\u00b7les"], "token_info": ["word", "word"], "pos": ["PDS", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Nur moderndes Gebein,", "tokens": ["Nur", "mo\u00b7dern\u00b7des", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "De\u00df Grauenbild der Marmor uns verbirgt.", "tokens": ["De\u00df", "Grau\u00b7en\u00b7bild", "der", "Mar\u00b7mor", "uns", "ver\u00b7birgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Also zerst\u00f6rt das Schicksal", "tokens": ["Al\u00b7so", "zer\u00b7st\u00f6rt", "das", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ein Antlitz auch, das uns das lebensvollste", "tokens": ["Ein", "Ant\u00b7litz", "auch", ",", "das", "uns", "das", "le\u00b7bens\u00b7volls\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Abbild des Himmels schien. O ew'ges R\u00e4thsel", "tokens": ["Ab\u00b7bild", "des", "Him\u00b7mels", "schien", ".", "O", "ew'\u00b7ges", "R\u00e4th\u00b7sel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "$.", "NE", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Des Menschendaseins! Heut ein Quell erhabner", "tokens": ["Des", "Men\u00b7schen\u00b7da\u00b7seins", "!", "Heut", "ein", "Quell", "er\u00b7hab\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gedanken, unaussprechlicher Gef\u00fchle,", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "un\u00b7aus\u00b7sprech\u00b7li\u00b7cher", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Prahlt Sch\u00f6nheit und verspricht \u2013", "tokens": ["Prahlt", "Sch\u00f6n\u00b7heit", "und", "ver\u00b7spricht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Licht in Nachtgebieten", "tokens": ["Ein", "Licht", "in", "Nacht\u00b7ge\u00b7bie\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Uns von der g\u00f6ttlichen Natur gesandt, \u2013", "tokens": ["Uns", "von", "der", "g\u00f6tt\u00b7li\u00b7chen", "Na\u00b7tur", "ge\u00b7sandt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von \u00fcberird'schen Loosen,", "tokens": ["Von", "\u00fc\u00b7ber\u00b7ird'\u00b7schen", "Loo\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Gl\u00fccksel'gen Inselreichen, goldnen Welten", "tokens": ["Gl\u00fcck\u00b7sel'\u00b7gen", "In\u00b7sel\u00b7rei\u00b7chen", ",", "gold\u00b7nen", "Wel\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ein sichres Unterpfand", "tokens": ["Ein", "sich\u00b7res", "Un\u00b7ter\u00b7pfand"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Dem Sterblichen zu bieten:", "tokens": ["Dem", "Sterb\u00b7li\u00b7chen", "zu", "bie\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und morgen sehn wir schaudernd,", "tokens": ["Und", "mor\u00b7gen", "sehn", "wir", "schau\u00b7dernd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Durch einen leichten Ansto\u00df hingerafft,", "tokens": ["Durch", "ei\u00b7nen", "leich\u00b7ten", "An\u00b7sto\u00df", "hin\u00b7ge\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Entstellt, was uns noch eben", "tokens": ["Ent\u00b7stellt", ",", "was", "uns", "noch", "e\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Hold schien und engelhaft,", "tokens": ["Hold", "schien", "und", "en\u00b7gel\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Und auch die Wunderkraft,", "tokens": ["Und", "auch", "die", "Wun\u00b7der\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Die Seelen zu entz\u00fcnden,", "tokens": ["Die", "See\u00b7len", "zu", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Die hier gewaltet, f\u00fchlen wir entschwinden.", "tokens": ["Die", "hier", "ge\u00b7wal\u00b7tet", ",", "f\u00fch\u00b7len", "wir", "ent\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein unerme\u00dflich Sehnen", "tokens": ["Ein", "un\u00b7er\u00b7me\u00df\u00b7lich", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hehre Phantasieen", "tokens": ["Und", "heh\u00b7re", "Phan\u00b7ta\u00b7si\u00b7e\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "L\u00e4\u00dft durch die Seele ziehen", "tokens": ["L\u00e4\u00dft", "durch", "die", "See\u00b7le", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In weisem Einklang holder T\u00f6ne Macht,", "tokens": ["In", "wei\u00b7sem", "Ein\u00b7klang", "hol\u00b7der", "T\u00f6\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df durch ein wonnig Meer wie traumverwirrt", "tokens": ["Da\u00df", "durch", "ein", "won\u00b7nig", "Meer", "wie", "traum\u00b7ver\u00b7wirrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJD", "NN", "KOKOM", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Geist getrieben wird,", "tokens": ["Der", "Geist", "ge\u00b7trie\u00b7ben", "wird", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie durch den Ocean", "tokens": ["Wie", "durch", "den", "O\u00b7cean"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Zu seiner Lust ein k\u00fchner Schwimmer irrt.", "tokens": ["Zu", "sei\u00b7ner", "Lust", "ein", "k\u00fch\u00b7ner", "Schwim\u00b7mer", "irrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch wenn an unser Ohr", "tokens": ["Doch", "wenn", "an", "un\u00b7ser", "Ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ein Mi\u00dfton schl\u00e4gt, verschwindet", "tokens": ["Ein", "Mi\u00df\u00b7ton", "schl\u00e4gt", ",", "ver\u00b7schwin\u00b7det"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Das Paradies, das uns entz\u00fcckt zuvor.", "tokens": ["Das", "Pa\u00b7ra\u00b7dies", ",", "das", "uns", "ent\u00b7z\u00fcckt", "zu\u00b7vor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie kannst du, Mensch, wofern du", "tokens": ["Wie", "kannst", "du", ",", "Mensch", ",", "wo\u00b7fern", "du"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "$,", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Schw\u00e4che so versunken", "tokens": ["In", "Schw\u00e4\u00b7che", "so", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur Staub und Schatten bist, so stolz empfinden?", "tokens": ["Nur", "Staub", "und", "Schat\u00b7ten", "bist", ",", "so", "stolz", "emp\u00b7fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VAFIN", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wohnt ein Himmelsfunken", "tokens": ["Und", "wohnt", "ein", "Him\u00b7mels\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In dir, wie kann dein bestes innres Leben,", "tokens": ["In", "dir", ",", "wie", "kann", "dein", "bes\u00b7tes", "inn\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PWAV", "VMFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So knechtisch hingegeben", "tokens": ["So", "knech\u00b7tisch", "hin\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "An niedre Macht, entstehen und verschwinden?", "tokens": ["An", "nied\u00b7re", "Macht", ",", "ent\u00b7ste\u00b7hen", "und", "ver\u00b7schwin\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So warst du. Jetzt hier unten", "tokens": ["So", "warst", "du", ".", "Jetzt", "hier", "un\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$.", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bist du Geripp und Staub. Bewegungslos", "tokens": ["Bist", "du", "Ge\u00b7ripp", "und", "Staub", ".", "Be\u00b7we\u00b7gungs\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "NE", "KON", "NN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Steht, \u00fcber deinem modernden Gebein", "tokens": ["Steht", ",", "\u00fc\u00b7ber", "dei\u00b7nem", "mo\u00b7dern\u00b7den", "Ge\u00b7bein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Stumm blickend in der Zeiten Strom hinein,", "tokens": ["Stumm", "bli\u00b7ckend", "in", "der", "Zei\u00b7ten", "Strom", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur noch als H\u00fcterin", "tokens": ["Nur", "noch", "als", "H\u00fc\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Trauer und Erinnrung dieses Abbild", "tokens": ["Der", "Trau\u00b7er", "und", "E\u00b7rinn\u00b7rung", "die\u00b7ses", "Ab\u00b7bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verschwundner Sch\u00f6nheit. Jener s\u00fc\u00dfe Blick,", "tokens": ["Ver\u00b7schwund\u00