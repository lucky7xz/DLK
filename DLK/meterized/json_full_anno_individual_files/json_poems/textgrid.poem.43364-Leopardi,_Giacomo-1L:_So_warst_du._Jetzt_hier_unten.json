or", "uns", "ver\u00b7birgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Also zerst\u00f6rt das Schicksal", "tokens": ["Al\u00b7so", "zer\u00b7st\u00f6rt", "das", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ein Antlitz auch, das uns das lebensvollste", "tokens": ["Ein", "Ant\u00b7litz", "auch", ",", "das", "uns", "das", "le\u00b7bens\u00b7volls\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Abbild des Himmels schien. O ew'ges R\u00e4thsel", "tokens": ["Ab\u00b7bild", "des", "Him\u00b7mels", "schien", ".", "O", "ew'\u00b7ges", "R\u00e4th\u00b7sel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "$.", "NE", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Des Menschendaseins! Heut ein Quell erhabner", "tokens": ["Des", "Men\u00b7schen\u00b7da\u00b7seins", "!", "Heut", "ein", "Quell", "er\u00b7hab\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gedanken, unaussprechlicher Gef\u00fchle,", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "un\u00b7aus\u00b7sprech\u00b7li\u00b7cher", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Prahlt Sch\u00f6nheit und verspri