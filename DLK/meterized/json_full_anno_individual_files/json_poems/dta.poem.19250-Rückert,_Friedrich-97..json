{"dta.poem.19250": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von allen Thieren ist ein Nutzen anzugeben,", "tokens": ["Von", "al\u00b7len", "Thie\u00b7ren", "ist", "ein", "Nut\u00b7zen", "an\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch au\u00dfer jenem Zweck, dem h\u00f6chsten, da\u00df sie leben.", "tokens": ["Auch", "au\u00b7\u00dfer", "je\u00b7nem", "Zweck", ",", "dem", "h\u00f6chs\u00b7ten", ",", "da\u00df", "sie", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,", "ART", "ADJA", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn wenn an einigen kein andrer Nutz erschienen,", "tokens": ["Denn", "wenn", "an", "ei\u00b7ni\u00b7gen", "kein", "an\u00b7drer", "Nutz", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PIAT", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die werden wenigstens zur Nahrung andern dienen.", "tokens": ["Die", "wer\u00b7den", "we\u00b7nigs\u00b7tens", "zur", "Nah\u00b7rung", "an\u00b7dern", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPRART", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und andere, die ganz und gar sonst sch\u00e4dlich w\u00e4ren,", "tokens": ["Und", "an\u00b7de\u00b7re", ",", "die", "ganz", "und", "gar", "sonst", "sch\u00e4d\u00b7lich", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ADV", "KON", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "N\u00fctzen indem sie sich von sch\u00e4dlichen ern\u00e4hren.", "tokens": ["N\u00fct\u00b7zen", "in\u00b7dem", "sie", "sich", "von", "sch\u00e4d\u00b7li\u00b7chen", "er\u00b7n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "PRF", "APPR", "ADJA", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Nun k\u00f6nnten sagen zwar die th\u00f6richten Vermessnen,", "tokens": ["Nun", "k\u00f6nn\u00b7ten", "sa\u00b7gen", "zwar", "die", "th\u00f6\u00b7rich\u00b7ten", "Ver\u00b7mess\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entbehrlich seien samt den Fressern die Gefressnen.", "tokens": ["Ent\u00b7behr\u00b7lich", "sei\u00b7en", "samt", "den", "Fres\u00b7sern", "die", "Ge\u00b7fress\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch h\u00f6chste Weisheit wollt' auch denen Leben g\u00f6nnen,", "tokens": ["Doch", "h\u00f6chs\u00b7te", "Weis\u00b7heit", "wollt'", "auch", "de\u00b7nen", "Le\u00b7ben", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "ADV", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die f\u00fcr nur oder durch den Tod nur leben k\u00f6nnen.", "tokens": ["Die", "f\u00fcr", "nur", "o\u00b7der", "durch", "den", "Tod", "nur", "le\u00b7ben", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "KON", "APPR", "ART", "NN", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}