{"dta.poem.5304": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zuf\u00e4llige Gedancken \u00fcber zwey nach  \n Gr\u00f6nland abseegelnde Schiffe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich j\u00fcngst auf einem kleinem H\u00fcgel,", "tokens": ["In\u00b7dem", "ich", "j\u00fcngst", "auf", "ei\u00b7nem", "klei\u00b7nem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Flachen Elbe-Strande, steh,", "tokens": ["Am", "Fla\u00b7chen", "El\u00b7be\u00b7Stran\u00b7de", ",", "steh", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, wie der glatten Fluthen Spiegel", "tokens": ["Und", ",", "wie", "der", "glat\u00b7ten", "Flut\u00b7hen", "Spie\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich sanft vor\u00fcber ziehet, seh;", "tokens": ["Sich", "sanft", "vor\u00b7\u00fc\u00b7ber", "zie\u00b7het", ",", "seh", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erblick ich, auf dem sich sanft senckenden Gew\u00e4sser,", "tokens": ["Er\u00b7blick", "ich", ",", "auf", "dem", "sich", "sanft", "sen\u00b7cken\u00b7den", "Ge\u00b7w\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "PRELS", "PRF", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ein grosses wol beseegelt Paar", "tokens": ["Ein", "gros\u00b7ses", "wol", "be\u00b7see\u00b7gelt", "Paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sehr starck-bemannter Wasser-Schl\u00f6sser,", "tokens": ["Sehr", "sta\u00b7rck\u00b7be\u00b7mann\u00b7ter", "Was\u00b7ser\u00b7Sch\u00b7l\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "So zu dem Wallfisch-Fang bestimmet war;", "tokens": ["So", "zu", "dem", "Wall\u00b7fischFang", "be\u00b7stim\u00b7met", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Wie beid\u2019, in stiller Farth, die Fluthen theilten,", "tokens": ["Wie", "beid'", ",", "in", "stil\u00b7ler", "Farth", ",", "die", "Flut\u00b7hen", "theil\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und, Land- und Strand vorbey, gem\u00e4hlig See-werts", "tokens": ["Und", ",", "Lan\u00b7d", "und", "Strand", "vor\u00b7bey", ",", "ge\u00b7m\u00e4h\u00b7lig", "See\u00b7werts"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "TRUNC", "KON", "NN", "PTKVZ", "$,", "ADJD", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "eilten.", "tokens": ["eil\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Indem ich nun, von ihrer Reise", "tokens": ["In\u00b7dem", "ich", "nun", ",", "von", "ih\u00b7rer", "Rei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Den weit entfernten Zweck erwege,", "tokens": ["Den", "weit", "ent\u00b7fern\u00b7ten", "Zweck", "er\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die, auf so manche Art und Weise,", "tokens": ["Die", ",", "auf", "so", "man\u00b7che", "Art", "und", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Sie drohende Gefahr, mit Grausen \u00fcberlege;", "tokens": ["Sie", "dro\u00b7hen\u00b7de", "Ge\u00b7fahr", ",", "mit", "Grau\u00b7sen", "\u00fc\u00b7berl\u00b7e\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So f\u00e4llt mir die Betrachtung bey:", "tokens": ["So", "f\u00e4llt", "mir", "die", "Be\u00b7trach\u00b7tung", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ich dencke, wie es m\u00f6glich sey,", "tokens": ["Ich", "den\u00b7cke", ",", "wie", "es", "m\u00f6g\u00b7lich", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Da\u00df diese Reisende, der sch\u00f6nsten Fr\u00fchlings-Zeit,", "tokens": ["Da\u00df", "die\u00b7se", "Rei\u00b7sen\u00b7de", ",", "der", "sch\u00f6ns\u00b7ten", "Fr\u00fch\u00b7lings\u00b7Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die jetzo wiederkehrt,", "tokens": ["Die", "jet\u00b7zo", "wie\u00b7der\u00b7kehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Und da der Erde Schmuck sich st\u00fcndlich fast vermehrt,", "tokens": ["Und", "da", "der", "Er\u00b7de", "Schmuck", "sich", "st\u00fcnd\u00b7lich", "fast", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "PRF", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da Wald und Feld bey uns in solcher Lieblichkeit", "tokens": ["Da", "Wald", "und", "Feld", "bey", "uns", "in", "sol\u00b7cher", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Bey aufgekl\u00e4hrten L\u00fcften bl\u00fchen,", "tokens": ["Bey", "auf\u00b7ge\u00b7kl\u00e4hr\u00b7ten", "L\u00fcf\u00b7ten", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "So gantz gelassen sich entziehen,", "tokens": ["So", "gantz", "ge\u00b7las\u00b7sen", "sich", "ent\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Um sich den ungest\u00fchmen Wellen", "tokens": ["Um", "sich", "den", "un\u00b7ge\u00b7st\u00fch\u00b7men", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Der unergr\u00fcndlich tieffen See,", "tokens": ["Der", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "tief\u00b7fen", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Des Winters Wuth, Reif, Hagel, Frost und Schnee", "tokens": ["Des", "Win\u00b7ters", "Wuth", ",", "Reif", ",", "Ha\u00b7gel", ",", "Frost", "und", "Schnee"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Und Boreas Gewalt in Gr\u00f6nland blos zu stellen.", "tokens": ["Und", "Bo\u00b7re\u00b7as", "Ge\u00b7walt", "in", "Gr\u00f6n\u00b7land", "blos", "zu", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "APPR", "NE", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}