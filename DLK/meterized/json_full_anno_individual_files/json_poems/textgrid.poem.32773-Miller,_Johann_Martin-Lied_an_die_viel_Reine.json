{"textgrid.poem.32773": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Lied an die viel Reine", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonder alle List,", "tokens": ["Son\u00b7der", "al\u00b7le", "List", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wie ein T\u00e4ublein, ist", "tokens": ["Wie", "ein", "T\u00e4ub\u00b7lein", ",", "ist"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NN", "$,", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die viel Reine, die vor allen", "tokens": ["Die", "viel", "Rei\u00b7ne", ",", "die", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir gefallen.", "tokens": ["Mir", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Hei! ihr soll ein Lied erschallen!", "tokens": ["Hei", "!", "ihr", "soll", "ein", "Lied", "er\u00b7schal\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Himmelblau und klar", "tokens": ["Him\u00b7mel\u00b7blau", "und", "klar"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ist ihr Augenpaar.", "tokens": ["Ist", "ihr", "Au\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "word"