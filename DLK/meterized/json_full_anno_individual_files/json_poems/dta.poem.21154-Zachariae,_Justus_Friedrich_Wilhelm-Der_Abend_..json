{"dta.poem.21154": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Abend .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Abendstern winkt unsrer Erde", "tokens": ["Der", "A\u00b7bends\u00b7tern", "winkt", "uns\u00b7rer", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Ruh am Horizont herauf;", "tokens": ["Die", "Ruh", "am", "Ho\u00b7ri\u00b7zont", "her\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Tages Arbeit und Beschwerde", "tokens": ["Des", "Ta\u00b7ges", "Ar\u00b7beit", "und", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt auf dem stillen Erdkrais auf.", "tokens": ["H\u00f6rt", "auf", "dem", "stil\u00b7len", "Erd\u00b7krais", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Landmann, dessen stille H\u00fctte", "tokens": ["Der", "Land\u00b7mann", ",", "des\u00b7sen", "stil\u00b7le", "H\u00fct\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gott des Schlafes gern bewohut,", "tokens": ["Der", "Gott", "des", "Schla\u00b7fes", "gern", "be\u00b7wo\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Tritt vor die Th\u00fcr mit schwerem Schritte,", "tokens": ["Tritt", "vor", "die", "Th\u00fcr", "mit", "schwe\u00b7rem", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sieht mit G\u00e4hnen in den Mond.", "tokens": ["Und", "sieht", "mit", "G\u00e4h\u00b7nen", "in", "den", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch in der Stadt im weiten Zimmer", "tokens": ["Doch", "in", "der", "Stadt", "im", "wei\u00b7ten", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sp\u00fclt man die gro\u00dfen Gl\u00e4ser aus,", "tokens": ["Sp\u00fclt", "man", "die", "gro\u00b7\u00dfen", "Gl\u00e4\u00b7ser", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bey des Wachslichts stolzem Schimmer", "tokens": ["Und", "bey", "des", "Wachs\u00b7lichts", "stol\u00b7zem", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erhebet sich der Abendschmaus.", "tokens": ["Er\u00b7he\u00b7bet", "sich", "der", "A\u00b7bendsc\u00b7hmaus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da schimmern Westen bey den Hauben,", "tokens": ["Da", "schim\u00b7mern", "Wes\u00b7ten", "bey", "den", "Hau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da herrscht und jauchzt der freye Spa\u00df;", "tokens": ["Da", "herrscht", "und", "jauchzt", "der", "frey\u00b7e", "Spa\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und treuer Saft aus rheinschen Trauben", "tokens": ["Und", "treu\u00b7er", "Saft", "aus", "rhein\u00b7schen", "Trau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "St\u00fcrzt unaufh\u00f6rlich in das Glas.", "tokens": ["St\u00fcrzt", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "in", "das", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch, Freund, was machst du mit dem Weine,", "tokens": ["Doch", ",", "Freund", ",", "was", "machst", "du", "mit", "dem", "Wei\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der schlechtgen\u00fctzt sein Lager dr\u00fcckt?", "tokens": ["Der", "schlecht\u00b7ge\u00b7n\u00fctzt", "sein", "La\u00b7ger", "dr\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und warum hat ihn von dem Rheine", "tokens": ["Und", "wa\u00b7rum", "hat", "ihn", "von", "dem", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "APPR", "ART", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der milde Weingott dir geschickt?", "tokens": ["Der", "mil\u00b7de", "Wein\u00b7gott", "dir", "ge\u00b7schickt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich seh schon, wie auf deinem Saale", "tokens": ["Ich", "seh", "schon", ",", "wie", "auf", "dei\u00b7nem", "Saa\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Trunkenheit, nicht Bacchus, rauscht;", "tokens": ["Die", "Trun\u00b7ken\u00b7heit", ",", "nicht", "Bac\u00b7chus", ",", "rauscht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "NE", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Freund, man entheiligt die Pokale,", "tokens": ["Freund", ",", "man", "en\u00b7thei\u00b7ligt", "die", "Po\u00b7ka\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wenn man sich so, wie ihr, berauscht.", "tokens": ["Wenn", "man", "sich", "so", ",", "wie", "ihr", ",", "be\u00b7rauscht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "$,", "PWAV", "PPER", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O! da\u00df in ungew\u00fcrzten Z\u00fcgen", "tokens": ["O", "!", "da\u00df", "in", "un\u00b7ge\u00b7w\u00fcrz\u00b7ten", "Z\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der edle Saft verschwendet wird;", "tokens": ["Der", "ed\u00b7le", "Saft", "ver\u00b7schwen\u00b7det", "wird", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df der Mensch auch im Vergn\u00fcgen", "tokens": ["Und", "da\u00df", "der", "Mensch", "auch", "im", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seiner Schande strafbar irrt!", "tokens": ["Zu", "sei\u00b7ner", "Schan\u00b7de", "straf\u00b7bar", "irrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nur Freunde, die sich gl\u00fccklich d\u00fcnken,", "tokens": ["Nur", "Freun\u00b7de", ",", "die", "sich", "gl\u00fcck\u00b7lich", "d\u00fcn\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie dem Becher Lieder weihn;", "tokens": ["Wenn", "sie", "dem", "Be\u00b7cher", "Lie\u00b7der", "weihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir, Freund, wir m\u00fc\u00dften mit dir trinken,", "tokens": ["Wir", ",", "Freund", ",", "wir", "m\u00fc\u00df\u00b7ten", "mit", "dir", "trin\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So w\u00fcrde dir dein Wein, erst Wein.", "tokens": ["So", "w\u00fcr\u00b7de", "dir", "dein", "Wein", ",", "erst", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}