{"dta.poem.1248": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die doppelten Sieben Wochen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Sieben Wochen sind nun hin/", "tokens": ["Sie\u00b7ben", "Wo\u00b7chen", "sind", "nun", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit ich/ Cloris/ von dir bin/", "tokens": ["Seit", "ich", "/", "Clo\u00b7ris", "/", "von", "dir", "bin", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "NE", "$(", "APPR", "PPER", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und noch einmahl sieben Wochen", "tokens": ["Und", "noch", "ein\u00b7mahl", "sie\u00b7ben", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat sich Sonn und Mond verkrochen/", "tokens": ["Hat", "sich", "Sonn", "und", "Mond", "ver\u00b7kro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "NN", "KON", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seit ich/ liebste Sch\u00e4fferin/", "tokens": ["Seit", "ich", "/", "liebs\u00b7te", "Sch\u00e4f\u00b7fe\u00b7rin", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von dir abgeschieden bin.", "tokens": ["Von", "dir", "ab\u00b7ge\u00b7schie\u00b7den", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin nimmer \u00e4hnlich mir/", "tokens": ["Ich", "bin", "nim\u00b7mer", "\u00e4hn\u00b7lich", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit ich/ Cloris/ bin von dir:", "tokens": ["Seit", "ich", "/", "Clo\u00b7ris", "/", "bin", "von", "dir", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "NE", "$(", "VAFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine vormahls rothe Wangen", "tokens": ["Mei\u00b7ne", "vor\u00b7mahls", "ro\u00b7the", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt des Todes Farb\u2019 umfangen/", "tokens": ["H\u00e4lt", "des", "To\u00b7des", "Fa\u00b7rb'", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVPP", "$("], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.5": {"text": "Und der Lippen Glantz stirbt hin", "tokens": ["Und", "der", "Lip\u00b7pen", "Glantz", "stirbt", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "PTKVZ"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Seit ich/ Cloris/ von dir bin.", "tokens": ["Seit", "ich", "/", "Clo\u00b7ris", "/", "von", "dir", "bin", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "P