{"textgrid.poem.48404": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "18. Christum lieben ist be\u00dfer denn Alles wi\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohn Eins ist alles nichts, was etwas ist und hei\u00dft,", "tokens": ["Ohn", "Eins", "ist", "al\u00b7les", "nichts", ",", "was", "et\u00b7was", "ist", "und", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "PIS", "$,", "PRELS", "PIS", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so viel der Sternenzelt in seinem Zirk' umschleust.", "tokens": ["so", "viel", "der", "Ster\u00b7nen\u00b7zelt", "in", "sei\u00b7nem", "Zirk'", "um\u00b7schleust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df Eins ist \u00fcber All, in allem doch beschlossen;", "tokens": ["Di\u00df", "Eins", "ist", "\u00fc\u00b7ber", "All", ",", "in", "al\u00b7lem", "doch", "be\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "APPR", "NN", "$,", "APPR", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "stets seine, ganz und frei, in alles doch gegossen,", "tokens": ["stets", "sei\u00b7ne", ",", "ganz", "und", "frei", ",", "in", "al\u00b7les", "doch", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "$,", "ADV", "KON", "ADJD", "$,", "APPR", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "ein lebensvoller Geist; sein Absein ist der Tod.", "tokens": ["ein", "le\u00b7bens\u00b7vol\u00b7ler", "Geist", ";", "sein", "Ab\u00b7sein", "ist", "der", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Wer ohne dieses ist, ist niemals ohne Not.", "tokens": ["Wer", "oh\u00b7ne", "die\u00b7ses", "ist", ",", "ist", "nie\u00b7mals", "oh\u00b7ne", "Not", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PDS", "VAFIN", "$,", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was bin ich doch bem\u00fcht um alles zu erlernen,", "tokens": ["Was", "bin", "ich", "doch", "be\u00b7m\u00fcht", "um", "al\u00b7les", "zu", "er\u00b7ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVFIN", "APPR", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "was nahe bei uns ist und was uns k\u00f6mpt von fernen,", "tokens": ["was", "na\u00b7he", "bei", "uns", "ist", "und", "was", "uns", "k\u00f6mpt", "von", "fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "PPER", "VAFIN", "KON", "PWS", "PPER", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "was hier und da und dort und \u00fcberall geschieht,", "tokens": ["was", "hier", "und", "da", "und", "dort", "und", "\u00fc\u00b7be\u00b7rall", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KON", "ADV", "KON", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "darnach ein geizigs Aug' aus Herzenshunger sieht?", "tokens": ["dar\u00b7nach", "ein", "gei\u00b7zigs", "Aug'", "aus", "Her\u00b7zens\u00b7hun\u00b7ger", "sieht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "K\u00f6nt' ich ein' iede Kunst, w\u00e4r' aller Reichtum meine,", "tokens": ["K\u00f6nt'", "ich", "ein'", "ie\u00b7de", "Kunst", ",", "w\u00e4r'", "al\u00b7ler", "Reich\u00b7tum", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "PIAT", "NN", "$,", "VAFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "h\u00e4tt' ich der Ehren Thron zu eigen ganz alleine;", "tokens": ["h\u00e4tt'", "ich", "der", "Eh\u00b7ren", "Thron", "zu", "ei\u00b7gen", "ganz", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "PTKA", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "gieng' alles mir nach Lust und w\u00fcst' ich keine Zeit,", "tokens": ["gieng'", "al\u00b7les", "mir", "nach", "Lust", "und", "w\u00fcst'", "ich", "kei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "NN", "KON", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "die mich von Jugend auf nicht herzlich h\u00e4tt' erfreut,", "tokens": ["die", "mich", "von", "Ju\u00b7gend", "auf", "nicht", "herz\u00b7lich", "h\u00e4tt'", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "APPR", "NN", "APPR", "PTKNEG", "ADJD", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "ja w\u00fcst ich, (welches doch noch Keinem ist gegeben,)", "tokens": ["ja", "w\u00fcst", "ich", ",", "(", "wel\u00b7ches", "doch", "noch", "Kei\u00b7nem", "ist", "ge\u00b7ge\u00b7ben", ",", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "$(", "PRELS", "ADV", "ADV", "PIS", "VAFIN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "da\u00df ich auch keinen Tod auf Erden solt' erleben,", "tokens": ["da\u00df", "ich", "auch", "kei\u00b7nen", "Tod", "auf", "Er\u00b7den", "solt'", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "mein Name reichte hin bis in die neue Welt,", "tokens": ["mein", "Na\u00b7me", "reich\u00b7te", "hin", "bis", "in", "die", "neu\u00b7e", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "an mir w\u00e4r' alles das, was man f\u00fcr Alles h\u00e4lt,", "tokens": ["an", "mir", "w\u00e4r'", "al\u00b7les", "das", ",", "was", "man", "f\u00fcr", "Al\u00b7les", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "PDS", "$,", "PRELS", "PIS", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "ganz alles h\u00e4tt' ich ganz: was w\u00e4re dieses Alles?", "tokens": ["ganz", "al\u00b7les", "h\u00e4tt'", "ich", "ganz", ":", "was", "w\u00e4\u00b7re", "die\u00b7ses", "Al\u00b7les", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "ADV", "$.", "PWS", "VAFIN", "PDAT", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein Alles auf den Schein, ein Conterfet des Schalles,", "tokens": ["Ein", "Al\u00b7les", "auf", "den", "Schein", ",", "ein", "Con\u00b7ter\u00b7fet", "des", "Schal\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "des Schatten leiblichs Bild, Verblendung des Gesichts,", "tokens": ["des", "Schat\u00b7ten", "leib\u00b7lichs", "Bild", ",", "Ver\u00b7blen\u00b7dung", "des", "Ge\u00b7sichts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "ein Schlauch an Leere voll, mit einem Worte Nichts.", "tokens": ["ein", "Schlauch", "an", "Lee\u00b7re", "voll", ",", "mit", "ei\u00b7nem", "Wor\u00b7te", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "$,", "APPR", "ART", "NN", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "O Alles \u00fcber All! O mehr als alles Alles,", "tokens": ["O", "Al\u00b7les", "\u00fc\u00b7ber", "All", "!", "O", "mehr", "als", "al\u00b7les", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "APPR", "NN", "$.", "NE", "PIAT", "KOKOM", "PIS", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "vor Allem allzeit da, ein Aufstand alles Falles,", "tokens": ["vor", "Al\u00b7lem", "all\u00b7zeit", "da", ",", "ein", "Auf\u00b7stand", "al\u00b7les", "Fal\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "ADV", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "nach Allem stets wie vor, ein Einzler an der Zahl,", "tokens": ["nach", "Al\u00b7lem", "stets", "wie", "vor", ",", "ein", "Einz\u00b7ler", "an", "der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "KOKOM", "APPR", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "doch \u00fcber alle Zahl und Zeiten allzumal,", "tokens": ["doch", "\u00fc\u00b7ber", "al\u00b7le", "Zahl", "und", "Zei\u00b7ten", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "f\u00fcr dem der sch\u00e4rfste Witz ist Aberwitz zu nennen,", "tokens": ["f\u00fcr", "dem", "der", "sch\u00e4rfs\u00b7te", "Witz", "ist", "A\u00b7ber\u00b7witz", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "VAFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "du aller Sch\u00e4tze Schatz, den nur die Seelen kennen,", "tokens": ["du", "al\u00b7ler", "Sch\u00e4t\u00b7ze", "Schatz", ",", "den", "nur", "die", "See\u00b7len", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "f\u00fcr dem die Ehre Schmach, die Wollust Unlust hei\u00dft,", "tokens": ["f\u00fcr", "dem", "die", "Eh\u00b7re", "Schmach", ",", "die", "Wol\u00b7lust", "Un\u00b7lust", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "ein geistgestalter Mensch, ein menschgestalter Geist,", "tokens": ["ein", "geist\u00b7ge\u00b7stal\u00b7ter", "Mensch", ",", "ein", "menschge\u00b7stal\u00b7ter", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "o Menschgott, Heiland, Heil! dem alle Dinge geben", "tokens": ["o", "Menschgott", ",", "Hei\u00b7land", ",", "Heil", "!", "dem", "al\u00b7le", "Din\u00b7ge", "ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["FM", "NN", "$,", "NN", "$,", "NN", "$.", "ART", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.32": {"text": "in Allem allen Preis, du alles Lebens Leben", "tokens": ["in", "Al\u00b7lem", "al\u00b7len", "Preis", ",", "du", "al\u00b7les", "Le\u00b7bens", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PIAT", "NN", "$,", "PPER", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "und alles Todes Tod! du bist es, Jesu, du,", "tokens": ["und", "al\u00b7les", "To\u00b7des", "Tod", "!", "du", "bist", "es", ",", "Je\u00b7su", ",", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$.", "PPER", "VAFIN", "PPER", "$,", "NE", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "ohn dem Nichts Alles ist und minder noch darzu.", "tokens": ["ohn", "dem", "Nichts", "Al\u00b7les", "ist", "und", "min\u00b7der", "noch", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VAFIN", "KON", "ADV", "ADV", "PAV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.35": {"text": "Ach Alles, la\u00df mein Nichts dir darumb doch gefallen,", "tokens": ["Ach", "Al\u00b7les", ",", "la\u00df", "mein", "Nichts", "dir", "da\u00b7rumb", "doch", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PIS", "$,", "VVIMP", "PPOSAT", "NN", "PPER", "PAV", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "dieweil es nichts wil ein in andern Sachen allen,", "tokens": ["die\u00b7weil", "es", "nichts", "wil", "ein", "in", "an\u00b7dern", "Sa\u00b7chen", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VMFIN", "ART", "APPR", "ADJA", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "gieb, Alles, mir, dem Nichts, in allem Rat und Tat,", "tokens": ["gieb", ",", "Al\u00b7les", ",", "mir", ",", "dem", "Nichts", ",", "in", "al\u00b7lem", "Rat", "und", "Tat", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PIS", "$,", "PPER", "$,", "PRELS", "PIS", "$,", "APPR", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "so hab' und kan ich mehr, als Alles kan und hat!", "tokens": ["so", "hab'", "und", "kan", "ich", "mehr", ",", "als", "Al\u00b7les", "kan", "und", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KON", "VMFIN", "PPER", "ADV", "$,", "KOUS", "PIS", "VMFIN", "KON", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ohn Eins ist alles nichts, was etwas ist und hei\u00dft,", "tokens": ["Ohn", "Eins", "ist", "al\u00b7les", "nichts", ",", "was", "et\u00b7was", "ist", "und", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "PIS", "$,", "PRELS", "PIS", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so viel der Sternenzelt in seinem Zirk' umschleust.", "tokens": ["so", "viel", "d