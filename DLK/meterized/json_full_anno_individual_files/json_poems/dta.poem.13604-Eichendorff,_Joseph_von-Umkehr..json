{"dta.poem.13604": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Umkehr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Leben kann man nicht von T\u00f6nen,", "tokens": ["Le\u00b7ben", "kann", "man", "nicht", "von", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Poesie geht ohne Schuh,", "tokens": ["Poe\u00b7sie", "geht", "oh\u00b7ne", "Schuh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und so wandt' ich denn der Sch\u00f6nen", "tokens": ["Und", "so", "wandt'", "ich", "denn", "der", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich auch den R\u00fccken zu:", "tokens": ["End\u00b7lich", "auch", "den", "R\u00fc\u00b7cken", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lange durch die Welt getrieben", "tokens": ["Lan\u00b7ge", "durch", "die", "Welt", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat mich nun die irre Hast,", "tokens": ["Hat", "mich", "nun", "die", "ir\u00b7re", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer doch bin ich geblieben", "tokens": ["Im\u00b7mer", "doch", "bin", "ich", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ein ungeschickter Gast.", "tokens": ["Nur", "ein", "un\u00b7ge\u00b7schick\u00b7ter", "Gast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ueberall zu sp\u00e4t zum Schmause", "tokens": ["Ue\u00b7be\u00b7rall", "zu", "sp\u00e4t", "zum", "Schmau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKA", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam ich, wenn die andern voll,", "tokens": ["Kam", "ich", ",", "wenn", "die", "an\u00b7dern", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "KOUS", "ART", "ADJA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trank die Neigen vor dem Hause,", "tokens": ["Trank", "die", "Nei\u00b7gen", "vor", "dem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wu\u00dft' nicht, wem ich's trinken soll.", "tokens": ["Wu\u00dft'", "nicht", ",", "wem", "ich's", "trin\u00b7ken", "soll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mu\u00dft' mich vor Fortuna b\u00fccken", "tokens": ["Mu\u00dft'", "mich", "vor", "For\u00b7tu\u00b7na", "b\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehrfurchtsvoll bis auf die Zeh'n,", "tokens": ["Ehr\u00b7furchts\u00b7voll", "bis", "auf", "die", "Zeh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vornehm wandt' sie mir den R\u00fccken,", "tokens": ["Vor\u00b7nehm", "wandt'", "sie", "mir", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lie\u00df mich so gebogen stehn.", "tokens": ["Lie\u00df", "mich", "so", "ge\u00b7bo\u00b7gen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und als ich mich aufgerichtet", "tokens": ["Und", "als", "ich", "mich", "auf\u00b7ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder frisch und frei und stolz,", "tokens": ["Wie\u00b7der", "frisch", "und", "frei", "und", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah ich Berg' und Thal gelichtet,", "tokens": ["Sah", "ich", "Ber\u00b7g'", "und", "Thal", "ge\u00b7lich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Bl\u00fchen jedes d\u00fcrre Holz.", "tokens": ["Bl\u00fc\u00b7hen", "je\u00b7des", "d\u00fcr\u00b7re", "Holz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Welt hat eine plumpe Pfote,", "tokens": ["Welt", "hat", "ei\u00b7ne", "plum\u00b7pe", "Pfo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandern kann man ohne Schuh \u2014", "tokens": ["Wan\u00b7dern", "kann", "man", "oh\u00b7ne", "Schuh"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deck' mit Deinem Morgenrothe", "tokens": ["Deck'", "mit", "Dei\u00b7nem", "Mor\u00b7gen\u00b7ro\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder nur den Wandrer zu!", "tokens": ["Wie\u00b7der", "nur", "den", "Wand\u00b7rer", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}