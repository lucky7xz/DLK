{"textgrid.poem.34512": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[bist mal f\u00fcr mich das rechte Weib]", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist mal f\u00fcr mich das rechte Weib,", "tokens": ["Bist", "mal", "f\u00fcr", "mich", "das", "rech\u00b7te", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "all meinen Sinnen gut,", "tokens": ["all", "mei\u00b7nen", "Sin\u00b7nen", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und taugst zu mir, so Seel wie Leib,", "tokens": ["und", "taugst", "zu", "mir", ",", "so", "Seel", "wie", "Leib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$,", "ADV", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "als fl\u00f6ss darin mein Blut!", "tokens": ["als", "fl\u00f6ss", "da\u00b7rin", "mein", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "All Heuchelei, von uns verlacht;", "tokens": ["All", "Heu\u00b7che\u00b7lei", ",", "von", "uns", "ver\u00b7lacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "fort mit dem Sch\u00f6ngethu \u2013", "tokens": ["fort", "mit