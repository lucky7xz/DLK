{"dta.poem.10891": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Stoppeln.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf dem gem\u00e4hten Rest der Halmen, auf den Stoppeln,", "tokens": ["Auf", "dem", "ge\u00b7m\u00e4h\u00b7ten", "Rest", "der", "Hal\u00b7men", ",", "auf", "den", "Stop\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seh ich mit Lust den Glanz der Sonnen sich verdoppeln.", "tokens": ["Seh", "ich", "mit", "Lust", "den", "Glanz", "der", "Son\u00b7nen", "sich", "ver\u00b7dop\u00b7peln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Es gl\u00e4nzet fast kein Gold so sch\u00f6n, so gelb, so glatt,", "tokens": ["Es", "gl\u00e4n\u00b7zet", "fast", "kein", "Gold", "so", "sch\u00f6n", ",", "so", "gelb", ",", "so", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie jetzt das Feld ein Gold, in gelben Stoppeln, hat.", "tokens": ["Wie", "jetzt", "das", "Feld", "ein", "Gold", ",", "in", "gel\u00b7ben", "Stop\u00b7peln", ",", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist, mit neuem Glanz, die neue Fl\u00e4ch erf\u00fcllt,", "tokens": ["Es", "ist", ",", "mit", "neu\u00b7em", "Glanz", ",", "die", "neu\u00b7e", "Fl\u00e4ch", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es blitzt auf jedem Halm ein kleines Sonnenbild.", "tokens": ["Es", "blitzt", "auf", "je\u00b7dem", "Halm", "ein", "klei\u00b7nes", "Son\u00b7nen\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie ich mich nun daran, zu Gottes Ruhm, vergn\u00fcgte,", "tokens": ["Wie", "ich", "mich", "nun", "da\u00b7ran", ",", "zu", "Got\u00b7tes", "Ruhm", ",", "ver\u00b7gn\u00fcg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "PAV", "$,", "APPR", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und man nicht lang hernach die Stoppeln unterpfl\u00fcgte:", "tokens": ["Und", "man", "nicht", "lang", "her\u00b7nach", "die", "Stop\u00b7peln", "un\u00b7ter\u00b7pfl\u00fcg\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKNEG", "ADJD", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sah ich, im Augenblick, das gelbe Feld sich schw\u00e4rzen.", "tokens": ["Sah", "ich", ",", "im", "Au\u00b7gen\u00b7blick", ",", "das", "gel\u00b7be", "Feld", "sich", "schw\u00e4r\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich fing mit meiner Kinder Chor", "tokens": ["Ich", "fing", "mit", "mei\u00b7ner", "Kin\u00b7der", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Hier\u00fcber l\u00e4chelnd an zu scherzen,", "tokens": ["Hier\u00b7\u00fc\u00b7ber", "l\u00e4\u00b7chelnd", "an", "zu", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und legt ihm dieses Thun, in einem R\u00e4thsel, vor:", "tokens": ["Und", "legt", "ihm", "die\u00b7ses", "Thun", ",", "in", "ei\u00b7nem", "R\u00e4th\u00b7sel", ",", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "$,", "APPR", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}