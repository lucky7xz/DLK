{"dta.poem.11316": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft des Jtali\u00e4nischen poeten  \n  Marini.  \n A. C. T.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war, trotz aller welt! zur poesie gebohren,", "tokens": ["Ich", "war", ",", "trotz", "al\u00b7ler", "welt", "!", "zur", "po\u00b7e\u00b7sie", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PIAT", "NN", "$.", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Cupido hatte mich zum liebes-dienst erkohren;", "tokens": ["Cu\u00b7pi\u00b7do", "hat\u00b7te", "mich", "zum", "lie\u00b7bes\u00b7dienst", "er\u00b7koh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit die reimen nun recht lieblich m\u00f6chten fliessen,", "tokens": ["Da\u00b7mit", "die", "rei\u00b7men", "nun", "recht", "lieb\u00b7lich", "m\u00f6ch\u00b7ten", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "VVFIN", "ADV", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Lie\u00df er mir einen kiel aus seinem fl\u00fcgel schliessen.", "tokens": ["Lie\u00df", "er", "mir", "ei\u00b7nen", "kiel", "aus", "sei\u00b7nem", "fl\u00fc\u00b7gel", "schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "