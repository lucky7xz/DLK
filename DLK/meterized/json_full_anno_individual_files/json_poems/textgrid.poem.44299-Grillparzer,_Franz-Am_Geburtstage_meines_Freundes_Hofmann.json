{"textgrid.poem.44299": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Am Geburtstage meines Freundes Hofmann", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Monden schwinden, Jahre fliehen", "tokens": ["Mon\u00b7den", "schwin\u00b7den", ",", "Jah\u00b7re", "flie\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Fittigen der Zeit!", "tokens": ["Auf", "den", "Fit\u00b7ti\u00b7gen", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaum siehst du die Rose gl\u00fchen,", "tokens": ["Kaum", "siehst", "du", "die", "Ro\u00b7se", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kaum siehst du das Veilchen bl\u00fchen,", "tokens": ["Kaum", "siehst", "du", "das", "Veil\u00b7chen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hat ein Windsto\u00df sie zerstreut.", "tokens": ["Hat", "ein", "Wind\u00b7sto\u00df", "sie", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heute strahlt Gesundheitsf\u00fclle", "tokens": ["Heu\u00b7te", "strahlt", "Ge\u00b7sund\u00b7heits\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus des Knaben muntern Blick!", "tokens": ["Aus", "des", "Kna\u00b7ben", "mun\u00b7tern", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen deckt des Grabes H\u00fclle", "tokens": ["Mor\u00b7gen", "deckt", "des", "Gra\u00b7bes", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn mit grauser Todesstille,", "tokens": ["Ihn", "mit", "grau\u00b7ser", "To\u00b7des\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erloschen ist sein Blick!", "tokens": ["Und", "er\u00b7lo\u00b7schen", "ist", "sein", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohl dem, der als Greis und Knabe", "tokens": ["Wohl", "dem", ",", "der", "als", "Greis", "und", "Kna\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "KOUS", "NN", "KON", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Nur der holden Tugend lebt,", "tokens": ["Nur", "der", "hol\u00b7den", "Tu\u00b7gend", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auch an dem nahen Grabe", "tokens": ["Und", "auch", "an", "dem", "na\u00b7hen", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An des Alters schwachem Stabe", "tokens": ["An", "des", "Al\u00b7ters", "schwa\u00b7chem", "Sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht vor dem Vergelter bebt!", "tokens": ["Nicht", "vor", "dem", "Ver\u00b7gel\u00b7ter", "bebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der, doch m\u00e4\u00dfig und bescheiden,", "tokens": ["Der", ",", "doch", "m\u00e4\u00b7\u00dfig", "und", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch des Lebens Lust genie\u00dft,", "tokens": ["Auch", "des", "Le\u00b7bens", "Lust", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zum Kelche seiner Leiden", "tokens": ["Und", "zum", "Kel\u00b7che", "sei\u00b7ner", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manchen Tropfen sanfter Freuden", "tokens": ["Man\u00b7chen", "Trop\u00b7fen", "sanf\u00b7ter", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weise und bedachtsam gie\u00dft.", "tokens": ["Wei\u00b7se", "und", "be\u00b7dacht\u00b7sam", "gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dem zum Guten und zum Sch\u00f6nen", "tokens": ["Dem", "zum", "Gu\u00b7ten", "und", "zum", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe nur die Brust erf\u00fcllt,", "tokens": ["Lie\u00b7be", "nur", "die", "Brust", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der der armen Waise St\u00f6hnen", "tokens": ["Der", "der", "ar\u00b7men", "Wai\u00b7se", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Witwe herbe Tr\u00e4nen", "tokens": ["Und", "der", "Wit\u00b7we", "her\u00b7be", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Helfend wie ein Engel stillt.", "tokens": ["Hel\u00b7fend", "wie", "ein", "En\u00b7gel", "stillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Er kann froh den Blick erheben,", "tokens": ["Er", "kann", "froh", "den", "Blick", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Denn er ist ein braver Mann,", "tokens": ["Denn", "er", "ist", "ein", "bra\u00b7ver", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig, ewig wird er leben,", "tokens": ["E\u00b7wig", ",", "e\u00b7wig", "wird", "er", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber Sternen wird er schweben,", "tokens": ["\u00dc\u00b7ber", "Ster\u00b7nen", "wird", "er", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn auch er hat recht getan!", "tokens": ["Denn", "auch", "er", "hat", "recht", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Teurer Freund, auch du sollst n\u00fctzen! \u2013", "tokens": ["Teu\u00b7rer", "Freund", ",", "auch", "du", "sollst", "n\u00fct\u00b7zen", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "PPER", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6re stets der Tugend Ruf,", "tokens": ["H\u00f6\u00b7re", "stets", "der", "Tu\u00b7gend", "Ruf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du sollst sie unterst\u00fctzen", "tokens": ["Denn", "du", "sollst", "sie", "un\u00b7ter\u00b7st\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und gekr\u00e4nkte Unschuld sch\u00fctzen,", "tokens": ["Und", "ge\u00b7kr\u00e4nk\u00b7te", "Un\u00b7schuld", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie das Wesen, das dich schuf!", "tokens": ["Wie", "das", "We\u00b7sen", ",", "das", "dich", "schuf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nicht nur n\u00fctzen; auch das Freuen", "tokens": ["Nicht", "nur", "n\u00fct\u00b7zen", ";", "auch", "das", "Freu\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVINF", "$.", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weigert dir die Gottheit nicht:", "tokens": ["Wei\u00b7gert", "dir", "die", "Got\u00b7theit", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Kraft den Musen leihen,", "tokens": ["Dei\u00b7ne", "Kraft", "den", "Mu\u00b7sen", "lei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dich ihrem Dienste weihen,", "tokens": ["Und", "dich", "ih\u00b7rem", "Diens\u00b7te", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei dir Freude, sei dir Pflicht!", "tokens": ["Sei", "dir", "Freu\u00b7de", ",", "sei", "dir", "Pflicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Handle immer brav und bieder! \u2013", "tokens": ["Hand\u00b7le", "im\u00b7mer", "brav", "und", "bie\u00b7der", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du recht und brav getan,", "tokens": ["Hast", "du", "recht", "und", "brav", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann singt einst der Chor der Br\u00fcder", "tokens": ["Dann", "singt", "einst", "der", "Chor", "der", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dir am Grabe Trauerlieder,", "tokens": ["Dir", "am", "Gra\u00b7be", "Trau\u00b7er\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbhier auch liegt ein braver Mann!\u00ab", "tokens": ["\u00bb", "hier", "auch", "liegt", "ein", "bra\u00b7ver", "Mann", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Monden schwinden, Jahre fliehen", "tokens": ["Mon\u00b7den", "schwin\u00b7den", ",", "Jah\u00b7re", "flie\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Fittigen der Zeit!", "tokens": ["Auf", "den", "Fit\u00b7ti\u00b7gen", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaum siehst du die Rose gl\u00fchen,", "tokens": ["Kaum", "siehst", "du", "die", "Ro\u00b7se", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kaum siehst du das Veilchen bl\u00fchen,", "tokens": ["Kaum", "siehst", "du", "das", "Veil\u00b7chen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hat ein Windsto\u00df sie zerstreut.", "tokens": ["Hat", "ein", "Wind\u00b7sto\u00df", "sie", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Heute strahlt Gesundheitsf\u00fclle", "tokens": ["Heu\u00b7te", "strahlt", "Ge\u00b7sund\u00b7heits\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus des Knaben muntern Blick!", "tokens": ["Aus", "des", "Kna\u00b7ben", "mun\u00b7tern", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen deckt des Grabes H\u00fclle", "tokens": ["Mor\u00b7gen", "deckt", "des", "Gra\u00b7bes", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn mit grauser Todesstille,", "tokens": ["Ihn", "mit", "grau\u00b7ser", "To\u00b7des\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erloschen ist sein Blick!", "tokens": ["Und", "er\u00b7lo\u00b7schen", "ist", "sein", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Wohl dem, der als Greis und Knabe", "tokens": ["Wohl", "dem", ",", "der", "als", "Greis", "und", "Kna\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "KOUS", "NN", "KON", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Nur der holden Tugend lebt,", "tokens": ["Nur", "der", "hol\u00b7den", "Tu\u00b7gend", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auch an dem nahen Grabe", "tokens": ["Und", "auch", "an", "dem", "na\u00b7hen", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An des Alters schwachem Stabe", "tokens": ["An", "des", "Al\u00b7ters", "schwa\u00b7chem", "Sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht vor dem Vergelter bebt!", "tokens": ["Nicht", "vor", "dem", "Ver\u00b7gel\u00b7ter", "bebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Der, doch m\u00e4\u00dfig und bescheiden,", "tokens": ["Der", ",", "doch", "m\u00e4\u00b7\u00dfig", "und", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch des Lebens Lust genie\u00dft,", "tokens": ["Auch", "des", "Le\u00b7bens", "Lust", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zum Kelche seiner Leiden", "tokens": ["Und", "zum", "Kel\u00b7che", "sei\u00b7ner", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manchen Tropfen sanfter Freuden", "tokens": ["Man\u00b7chen", "Trop\u00b7fen", "sanf\u00b7ter", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weise und bedachtsam gie\u00dft.", "tokens": ["Wei\u00b7se", "und", "be\u00b7dacht\u00b7sam", "gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Dem zum Guten und zum Sch\u00f6nen", "tokens": ["Dem", "zum", "Gu\u00b7ten", "und", "zum", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe nur die Brust erf\u00fcllt,", "tokens": ["Lie\u00b7be", "nur", "die", "Brust", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der der armen Waise St\u00f6hnen", "tokens": ["Der", "der", "ar\u00b7men", "Wai\u00b7se", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Witwe herbe Tr\u00e4nen", "tokens": ["Und", "der", "Wit\u00b7we", "her\u00b7be", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Helfend wie ein Engel stillt.", "tokens": ["Hel\u00b7fend", "wie", "ein", "En\u00b7gel", "stillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Er kann froh den Blick erheben,", "tokens": ["Er", "kann", "froh", "den", "Blick", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Denn er ist ein braver Mann,", "tokens": ["Denn", "er", "ist", "ein", "bra\u00b7ver", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig, ewig wird er leben,", "tokens": ["E\u00b7wig", ",", "e\u00b7wig", "wird", "er", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber Sternen wird er schweben,", "tokens": ["\u00dc\u00b7ber", "Ster\u00b7nen", "wird", "er", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn auch er hat recht getan!", "tokens": ["Denn", "auch", "er", "hat", "recht", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "Teurer Freund, auch du sollst n\u00fctzen! \u2013", "tokens": ["Teu\u00b7rer", "Freund", ",", "auch", "du", "sollst", "n\u00fct\u00b7zen", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "PPER", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6re stets der Tugend Ruf,", "tokens": ["H\u00f6\u00b7re", "stets", "der", "Tu\u00b7gend", "Ruf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du sollst sie unterst\u00fctzen", "tokens": ["Denn", "du", "sollst", "sie", "un\u00b7ter\u00b7st\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und gekr\u00e4nkte Unschuld sch\u00fctzen,", "tokens": ["Und", "ge\u00b7kr\u00e4nk\u00b7te", "Un\u00b7schuld", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie das Wesen, das dich schuf!", "tokens": ["Wie", "das", "We\u00b7sen", ",", "das", "dich", "schuf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Nicht nur n\u00fctzen; auch das Freuen", "tokens": ["Nicht", "nur", "n\u00fct\u00b7zen", ";", "auch", "das", "Freu\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVINF", "$.", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weigert dir die Gottheit nicht:", "tokens": ["Wei\u00b7gert", "dir", "die", "Got\u00b7theit", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Kraft den Musen leihen,", "tokens": ["Dei\u00b7ne", "Kraft", "den", "Mu\u00b7sen", "lei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dich ihrem Dienste weihen,", "tokens": ["Und", "dich", "ih\u00b7rem", "Diens\u00b7te", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei dir Freude, sei dir Pflicht!", "tokens": ["Sei", "dir", "Freu\u00b7de", ",", "sei", "dir", "Pflicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Handle immer brav und bieder! \u2013", "tokens": ["Hand\u00b7le", "im\u00b7mer", "brav", "und", "bie\u00b7der", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du recht und brav getan,", "tokens": ["Hast", "du", "recht", "und", "brav", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann singt einst der Chor der Br\u00fcder", "tokens": ["Dann", "singt", "einst", "der", "Chor", "der", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dir am Grabe Trauerlieder,", "tokens": ["Dir", "am", "Gra\u00b7be", "Trau\u00b7er\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbhier auch liegt ein braver Mann!\u00ab", "tokens": ["\u00bb", "hier", "auch", "liegt", "ein", "bra\u00b7ver", "Mann", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "