{"textgrid.poem.41594": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rubens, Gefild der Rast, Strom der Vergessenheiten,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rubens, Gefild der Rast, Strom der Vergessenheiten,", "tokens": ["Ru\u00b7bens", ",", "Ge\u00b7fild", "der", "Rast", ",", "Strom", "der", "Ver\u00b7ges\u00b7sen\u00b7hei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Ein Ruhbett bl\u00fchnden Fleischs und doch von Liebe leer,", "tokens": ["Ein", "Ruh\u00b7bett", "bl\u00fchn\u00b7den", "Fleischs", "und", "doch", "von", "Lie\u00b7be", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darin das Leben wogt in ruhelosen Weiten,", "tokens": ["Da\u00b7rin", "das", "Le\u00b7ben", "wogt", "in", "ru\u00b7he\u00b7lo\u00b7sen", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie im Azur die Luft und wie das Meer im Meer.", "tokens": ["Wie", "im", "A\u00b7zur", "die", "Luft", "und", "wie", "das", "Meer", "im", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "KON", "PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Da Vinci, Spiegel, draus sich tiefe Tr\u00e4ume heben,", "tokens": ["Da", "Vin\u00b7ci", ",", "Spie\u00b7gel", ",", "draus", "sich", "tie\u00b7fe", "Tr\u00e4u\u00b7me", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "NN", "$,", "PAV", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo selger Engelschar stillfrohes L\u00e4cheln gl\u00e4nzt,", "tokens": ["Wo", "sel\u00b7ger", "En\u00b7gel\u00b7schar", "still\u00b7fro\u00b7hes", "L\u00e4\u00b7cheln", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die in geheimem Duft das Schattenland durchschweben,", "tokens": ["Die", "in", "ge\u00b7hei\u00b7mem", "Duft", "das", "Schat\u00b7ten\u00b7land", "durch\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das sich mit Gletschern und mit schlanken Pinien kr\u00e4nzt.", "tokens": ["Das", "sich", "mit", "Glet\u00b7schern", "und", "mit", "schlan\u00b7ken", "Pi\u00b7ni\u00b7en", "kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "NN", "KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Rembrandt, ein Armenhaus, von Murmeln bang verd\u00fcstert,", "tokens": ["Rem\u00b7brandt", ",", "ein", "Ar\u00b7men\u00b7haus", ",", "von", "Mur\u00b7meln", "bang", "ver\u00b7d\u00fcs\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo aller Schmuck der Wand ein Kruzifix allein,", "tokens": ["Wo", "al\u00b7ler", "Schmuck", "der", "Wand", "ein", "Kru\u00b7zi\u00b7fix", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo weinendes Gebet aus Schmutz und Lumpen fl\u00fcstert,", "tokens": ["Wo", "wei\u00b7nen\u00b7des", "Ge\u00b7bet", "aus", "Schmutz", "und", "Lum\u00b7pen", "fl\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die kalt und hart durchstrahlt ein winterlicher Schein.", "tokens": ["Die", "kalt", "und", "hart", "durch\u00b7strahlt", "ein", "win\u00b7ter\u00b7li\u00b7cher", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Buonarotti, Nacht, wo in des Dunkels Schweigen", "tokens": ["Buo\u00b7na\u00b7rot\u00b7ti", ",", "Nacht", ",", "wo", "in", "des", "Dun\u00b7kels", "Schwei\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Sich Herakles' Gestalt mit Christusbildern mengt,", "tokens": ["Sich", "He\u00b7ra\u00b7kles'", "Ge\u00b7stalt", "mit", "Chris\u00b7tus\u00b7bil\u00b7dern", "mengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo Riesenwesen starr der D\u00e4mmerung entsteigen", "tokens": ["Wo", "Rie\u00b7sen\u00b7we\u00b7sen", "starr", "der", "D\u00e4m\u00b7me\u00b7rung", "ent\u00b7stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und die gestreckte Hand das Leichentuch zersprengt.", "tokens": ["Und", "die", "ge\u00b7streck\u00b7te", "Hand", "das", "Lei\u00b7chen\u00b7tuch", "zer\u00b7sprengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Faunen freche Glut, des Faustkampfs zornig Toben,", "tokens": ["Der", "Fau\u00b7nen", "fre\u00b7che", "Glut", ",", "des", "Faust\u00b7kampfs", "zor\u00b7nig", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du, dem aus schmutzgem Tro\u00df die Sch\u00f6nheit sich gebar,", "tokens": ["Du", ",", "dem", "aus", "schmutz\u00b7gem", "Tro\u00df", "die", "Sch\u00f6n\u00b7heit", "sich", "ge\u00b7bar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hinf\u00e4llger, gelber Mann, das Herz von Stolz gehoben,", "tokens": ["Hin\u00b7f\u00e4ll\u00b7ger", ",", "gel\u00b7ber", "Mann", ",", "das", "Herz", "von", "Stolz", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Puget, gramvoller F\u00fcrst im Reich der Str\u00e4flingsschar.", "tokens": ["Pu\u00b7get", ",", "gram\u00b7vol\u00b7ler", "F\u00fcrst", "im", "Reich", "der", "Str\u00e4f\u00b7lings\u00b7schar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Watteau, ein Karneval, wo manche edle Herzen", "tokens": ["Wat\u00b7tea\u00b7u", ",", "ein", "Kar\u00b7ne\u00b7val", ",", "wo", "man\u00b7che", "ed\u00b7le", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "PWAV", "PIAT", "ADJA", "NN"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wie Schmetterlinge irrn in wechselvollem Glanz,", "tokens": ["Wie", "Schmet\u00b7ter\u00b7lin\u00b7ge", "irrn", "in", "wech\u00b7sel\u00b7vol\u00b7lem", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gewande, leicht und bunt, erhellt von tausend Kerzen,", "tokens": ["Ge\u00b7wan\u00b7de", ",", "leicht", "und", "bunt", ",", "er\u00b7hellt", "von", "tau\u00b7send", "Ker\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die die Verz\u00fcckung spr\u00fchn dem tollen Wirbeltanz.", "tokens": ["Die", "die", "Ver\u00b7z\u00fc\u00b7ckung", "spr\u00fchn", "dem", "tol\u00b7len", "Wir\u00b7bel\u00b7tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Goya, ein schwerer Traum, wo Finsternisse z\u00fcrnen,", "tokens": ["Go\u00b7ya", ",", "ein", "schwe\u00b7rer", "Traum", ",", "wo", "Fins\u00b7ter\u00b7nis\u00b7se", "z\u00fcr\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geburten, die man kocht in zaubertrunkner Wut,", "tokens": ["Ge\u00b7bur\u00b7ten", ",", "die", "man", "kocht", "in", "zau\u00b7ber\u00b7trunk\u00b7ner", "Wut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Spiegel alte Fraun und junge, nackte Dirnen,", "tokens": ["Im", "Spie\u00b7gel", "al\u00b7te", "Fraun", "und", "jun\u00b7ge", ",", "nack\u00b7te", "Dir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Str\u00fcmpfe gl\u00e4ttend, sch\u00f6n f\u00fcr der D\u00e4monen Glut.", "tokens": ["Die", "Str\u00fcmp\u00b7fe", "gl\u00e4t\u00b7tend", ",", "sch\u00f6n", "f\u00fcr", "der", "D\u00e4\u00b7mo\u00b7nen", "Glut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ein Blutsee, Delacroix, mit b\u00f6sen Engelscharen,", "tokens": ["Ein", "Blut\u00b7see", ",", "De\u00b7la\u00b7croix", ",", "mit", "b\u00f6\u00b7sen", "En\u00b7gel\u00b7scha\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beschattet durch ein Holz von Fichten, ewig gr\u00fcn,", "tokens": ["Be\u00b7schat\u00b7tet", "durch", "ein", "Holz", "von", "Fich\u00b7ten", ",", "e\u00b7wig", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo in vergr\u00e4mter Luft fremd t\u00f6nende Fanfaren", "tokens": ["Wo", "in", "ver\u00b7gr\u00e4m\u00b7ter", "Luft", "fremd", "t\u00f6\u00b7nen\u00b7de", "Fan\u00b7fa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gleich einem Seufzerhauch von Weber fern vergl\u00fchn.", "tokens": ["Gleich", "ei\u00b7nem", "Seuf\u00b7zer\u00b7hauch", "von", "We\u00b7ber", "fern", "ver\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NE", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dies L\u00e4stern, dieser Fluch, dies Weh von Klages\u00e4ngen,", "tokens": ["Dies", "L\u00e4s\u00b7tern", ",", "die\u00b7ser", "Fluch", ",", "dies", "Weh", "von", "Kla\u00b7ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PDAT", "NN", "$,", "PDS", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies Heulen, dies Tedeum, dieser wilde Schmerz,", "tokens": ["Dies", "Heu\u00b7len", ",", "dies", "Te\u00b7de\u00b7um", ",", "die\u00b7ser", "wil\u00b7de", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PDS", "NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie sind ein Widerhall aus tausend irren G\u00e4ngen,", "tokens": ["Sie", "sind", "ein", "Wi\u00b7der\u00b7hall", "aus", "tau\u00b7send", "ir\u00b7ren", "G\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein g\u00f6ttlich Opium f\u00fcr unser sterblich Herz.", "tokens": ["Ein", "g\u00f6tt\u00b7lich", "O\u00b7pium", "f\u00fcr", "un\u00b7ser", "sterb\u00b7lich", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Es ist ein Ruf, den man durch tausend Wachen k\u00fcndet,", "tokens": ["Es", "ist", "ein", "Ruf", ",", "den", "man", "durch", "tau\u00b7send", "Wa\u00b7chen", "k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist ein Losungswort, das tausendfach erschallt,", "tokens": ["Es", "ist", "ein", "Lo\u00b7sungs\u00b7wort", ",", "das", "tau\u00b7send\u00b7fach", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist ein Leuchtturm, der auf tausend Festen z\u00fcndet,", "tokens": ["Es", "ist", "ein", "Leucht\u00b7turm", ",", "der", "auf", "tau\u00b7send", "Fes\u00b7ten", "z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Schrei von J\u00e4gern ists, verirrt im gro\u00dfen Wald.", "tokens": ["Ein", "Schrei", "von", "J\u00e4\u00b7gern", "ists", ",", "ver\u00b7irrt", "im", "gro\u00b7\u00dfen", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "$,", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Denn klarer kann sich, Herr, kein Zeugnis offenbaren,", "tokens": ["Denn", "kla\u00b7rer", "kann", "sich", ",", "Herr", ",", "kein", "Zeug\u00b7nis", "of\u00b7fen\u00b7ba\u00b7ren", ","], "toke