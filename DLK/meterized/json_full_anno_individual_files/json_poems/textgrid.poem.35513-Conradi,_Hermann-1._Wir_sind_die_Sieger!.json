{"textgrid.poem.35513": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1. Wir sind die Sieger!", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund! Noch spro\u00dft uns die Kraft", "tokens": ["Freund", "!", "Noch", "spro\u00dft", "uns", "die", "Kraft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und die Liebe ist jung!", "tokens": ["Und", "die", "Lie\u00b7be", "ist", "jung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bl\u00fcten duft'ger Erinnerung", "tokens": ["Bl\u00fc\u00b7ten", "duft'\u00b7ger", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Wird nimmer sammeln", "tokens": ["Wird", "nim\u00b7mer", "sam\u00b7meln"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "In kommenden Tagen,", "tokens": ["In", "kom\u00b7men\u00b7den", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der nur mit Stammeln,", "tokens": ["Der", "nur", "mit", "Stam\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Der nur mit Zagen", "tokens": ["Der", "nur", "mit", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Zu den G\u00f6ttern gefleht!", "tokens": ["Zu", "den", "G\u00f6t\u00b7tern", "ge\u00b7fleht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Und nie ein Gebet", "tokens": ["Und", "nie", "ein", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "In flammender Begeisterung,", "tokens": ["In", "flam\u00b7men\u00b7der", "Be\u00b7geis\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In bl\u00fchendem Hymnenschwung,", "tokens": ["In", "bl\u00fc\u00b7hen\u00b7dem", "Hym\u00b7nen\u00b7schwung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Gen Himmel gesandt,", "tokens": ["Gen", "Him\u00b7mel", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Auf da\u00df er ihn l\u00f6se", "tokens": ["Auf", "da\u00df", "er", "ihn", "l\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Von lastender Schuld!", "tokens": ["Von", "las\u00b7ten\u00b7der", "Schuld", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Auf da\u00df er ihn labe", "tokens": ["Auf", "da\u00df", "er", "ihn", "la\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Aus dem Gnadenkelch", "tokens": ["Aus", "dem", "Gna\u00b7den\u00b7kelch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Seiner Tr\u00f6sterhuld!", "tokens": ["Sei\u00b7ner", "Tr\u00f6s\u00b7ter\u00b7huld", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "Auf da\u00df er ihm leihe", "tokens": ["Auf", "da\u00df", "er", "ihm", "lei\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Verkl\u00e4rende Weihe \u2013", "tokens": ["Ver\u00b7kl\u00e4\u00b7ren\u00b7de", "Wei\u00b7he", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Lebendigen Odem,", "tokens": ["Le\u00b7ben\u00b7di\u00b7gen", "O\u00b7dem", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Wie er durchflutet", "tokens": ["Wie", "er", "durch\u00b7flu\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Der Seligen Brust!", "tokens": ["Der", "Se\u00b7li\u00b7gen", "Brust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.23": {"text": "Nicht bangende Lust,", "tokens": ["Nicht", "ban\u00b7gen\u00b7de", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.24": {"text": "Nicht karges Hoffen,", "tokens": ["Nicht", "kar\u00b7ges", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Nicht zaghaftes Wollen:", "tokens": ["Nicht", "zag\u00b7haf\u00b7tes", "Wol\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Den Tiefen der Seele", "tokens": ["Den", "Tie\u00b7fen", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Entlockt und entquollen,", "tokens": ["Ent\u00b7lockt", "und", "ent\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Str\u00f6me sie hin", "tokens": ["Str\u00f6\u00b7me", "sie", "hin"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.29": {"text": "In berauschendem Rollen:", "tokens": ["In", "be\u00b7rau\u00b7schen\u00b7dem", "Rol\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.30": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Wie Bergwasser tosen", "tokens": ["Wie", "Berg\u00b7was\u00b7ser", "to\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und jauchzend durchbrechen", "tokens": ["Und", "jauch\u00b7zend", "durch\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gigantischer Felsen", "tokens": ["Gi\u00b7gan\u00b7ti\u00b7scher", "Fel\u00b7sen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ehernen Wall:", "tokens": ["E\u00b7her\u00b7nen", "Wall", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "So flamme sie sch\u00e4umend", "tokens": ["So", "flam\u00b7me", "sie", "sch\u00e4u\u00b7mend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und siegend umspanne", "tokens": ["Und", "sie\u00b7gend", "um\u00b7span\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mit allm\u00e4chtigen Armen", "tokens": ["Mit", "all\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Ar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Sie Zeiten und All!", "tokens": ["Sie", "Zei\u00b7ten", "und", "All", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "La\u00df andere langsam,", "tokens": ["La\u00df", "an\u00b7de\u00b7re", "lang\u00b7sam", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In erwogenem Wandel,", "tokens": ["In", "er\u00b7wo\u00b7ge\u00b7nem", "Wan\u00b7del", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die Pfade schreiten,", "tokens": ["Die", "Pfa\u00b7de", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die ihnen gezeichnet", "tokens": ["Die", "ih\u00b7nen", "ge\u00b7zeich\u00b7net"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ein auglos Geschick:", "tokens": ["Ein", "aug\u00b7los", "Ge\u00b7schick", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Mit blitzender Br\u00fcnne,", "tokens": ["Mit", "blit\u00b7zen\u00b7der", "Br\u00fcn\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Im Blicke Flammen,", "tokens": ["Im", "Bli\u00b7cke", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "In der Seele die Freiheit,", "tokens": ["In", "der", "See\u00b7le", "die", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "La\u00df ", "tokens": ["La\u00df"], "token_info": ["word"], "pos": ["VVIMP"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Das heilige Ziel!", "tokens": ["Das", "hei\u00b7li\u00b7ge", "Ziel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "La\u00df andere fahrten", "tokens": ["La\u00df", "an\u00b7de\u00b7re", "fahr\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PIS", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den Nacken gebeugt", "tokens": ["Den", "Na\u00b7cken", "ge\u00b7beugt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und \u00e4ngstlich das Auge,", "tokens": ["Und", "\u00e4ngst\u00b7lich", "das", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das entgeistete, stumme,", "tokens": ["Das", "ent\u00b7geis\u00b7te\u00b7te", ",", "stum\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Zu Boden gekehrt,", "tokens": ["Zu", "Bo\u00b7den", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Von tauben Lasten", "tokens": ["Von", "tau\u00b7ben", "Las\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die Seele beschwert \u2013", "tokens": ["Die", "See\u00b7le", "be\u00b7schwert", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Des Weltalls Weiten", "tokens": ["Des", "Welt\u00b7alls", "Wei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Durchf\u00fchlen in k\u00fchnen", "tokens": ["Durch\u00b7f\u00fch\u00b7len", "in", "k\u00fch\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gedankenfahrt