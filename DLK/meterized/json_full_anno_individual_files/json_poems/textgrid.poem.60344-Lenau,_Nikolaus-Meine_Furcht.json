{"textgrid.poem.60344": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Meine Furcht", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O st\u00fcrzt, ihr Wolkenbr\u00fcche,", "tokens": ["O", "st\u00fcrzt", ",", "ihr", "Wol\u00b7ken\u00b7br\u00fc\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Abgrund nur hinab!", "tokens": ["Zum", "Ab\u00b7grund", "nur", "hin\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O rei\u00dft, ihr Sturmesfl\u00fcche,", "tokens": ["O", "rei\u00dft", ",", "ihr", "Stur\u00b7mes\u00b7fl\u00fc\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die W\u00e4lder in ihr Grab!", "tokens": ["Die", "W\u00e4l\u00b7der", "in", "ihr", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O flammt, ihr Blitzesgluten,", "tokens": ["O", "flammt", ",", "ihr", "Blit\u00b7zes\u00b7glu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O rase, Donnerklang!", "tokens": ["O", "ra\u00b7se", ",", "Don\u00b7ner\u00b7klang", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "