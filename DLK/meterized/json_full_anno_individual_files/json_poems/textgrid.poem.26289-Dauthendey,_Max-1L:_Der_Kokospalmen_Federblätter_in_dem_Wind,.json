{"textgrid.poem.26289": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Kokospalmen Federbl\u00e4tter in dem Wind,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Kokospalmen Federbl\u00e4tter in dem Wind,", "tokens": ["Der", "Ko\u00b7ko\u00b7spal\u00b7men", "Fe\u00b7der\u00b7bl\u00e4t\u00b7ter", "in", "dem", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie str\u00e4uben sich. Die Mandelb\u00e4ume rauschend sind.", "tokens": ["Sie", "str\u00e4u\u00b7ben", "sich", ".", "Die", "Man\u00b7del\u00b7b\u00e4u\u00b7me", "rau\u00b7schend", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$.", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Pisangsch\u00e4fte schaukeln sich erregt,", "tokens": ["Und", "Pi\u00b7sang\u00b7sch\u00e4f\u00b7te", "schau\u00b7keln", "sich", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Lauben alle sind schreckhaft bewegt.", "tokens": ["Die", "Lau\u00b7ben", "al\u00b7le", "sind", "schreck\u00b7haft", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich sehe \u00fcber das Gel\u00e4nder weit hinaus", "tokens": ["Ich", "se\u00b7he", "\u00fc\u00b7ber", "das", "Ge\u00b7l\u00e4n\u00b7der", "weit", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von meinem Altan in dem offenen Haus.", "tokens": ["Von", "mei\u00b7nem", "Al\u00b7tan", "in", "dem", "of\u00b7fe\u00b7nen", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der Wind, gleich einem Boten, tritt heran.", "tokens": ["Der", "Wind", ",", "gleich", "ei\u00b7nem", "Bo\u00b7ten", ",", "tritt", "he\u00b7ran", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich h\u00f6re Worte, die er wecken kann.", "tokens": ["Ich", "h\u00f6\u00b7re", "Wor\u00b7te", ",", "die", "er", "we\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es spricht dort aus den Lauben, laut und leis,", "tokens": ["Es", "spricht", "dort", "aus", "den", "Lau\u00b7ben", ",", "laut", "und", "leis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gar mancher, den ich bei den Toten wei\u00df.", "tokens": ["Gar", "man\u00b7cher", ",", "den", "ich", "bei", "den", "To\u00b7ten", "wei\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es ruft vom gro\u00dfen Wolkenhintergrunde", "tokens": ["Es", "ruft", "vom", "gro\u00b7\u00dfen", "Wol\u00b7ken\u00b7hin\u00b7ter\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sehnsucht, und es braust der B\u00e4ume Runde.", "tokens": ["Die", "Sehn\u00b7sucht", ",", "und", "es", "braust", "der", "B\u00e4u\u00b7me", "Run\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sitze ich so lauschend vor dem All,", "tokens": ["Und", "sit\u00b7ze", "ich", "so", "lau\u00b7schend", "vor", "dem", "All", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So spricht die Welt mit meinem eignen Munde.", "tokens": ["So", "spricht", "die", "Welt", "mit", "mei\u00b7nem", "eig\u00b7nen", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Eine ist des andern Wiederhall.", "tokens": ["Der", "Ei\u00b7ne", "ist", "des", "an\u00b7dern", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "ADJA", "NN", "$."]