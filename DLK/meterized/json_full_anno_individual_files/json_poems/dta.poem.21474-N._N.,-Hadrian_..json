{"dta.poem.21474": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Hadrian .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Du Freund von Hellas! Weiser! O Hadrian!", "tokens": ["Du", "Freund", "von", "Hel\u00b7las", "!", "Wei\u00b7ser", "!", "O", "Had\u00b7ri\u00b7an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NE", "$.", "NN", "$.", "NE", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als deinen Freund wegraffte die Flut des Nil,", "tokens": ["Als", "dei\u00b7nen", "Freund", "weg\u00b7raff\u00b7te", "die", "Flut", "des", "Nil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als du, im Schmerz, der Wunderblume", "tokens": ["Als", "du", ",", "im", "Schmerz", ",", "der", "Wun\u00b7der\u00b7blu\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jeglichen Strebens im Staub der Erde,", "tokens": ["Jeg\u00b7li\u00b7chen", "Stre\u00b7bens", "im", "Staub", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "So manchen Prachtbau weih\u2019test und rings befahlst", "tokens": ["So", "man\u00b7chen", "Pracht\u00b7bau", "weih'\u00b7test", "und", "rings", "be\u00b7fahlst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "KON", "ADV", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der schalen Welt, ", "tokens": ["Der", "scha\u00b7len", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zu ehren, ruchlos th\u00f6richt schalten,", "tokens": ["Zu", "eh\u00b7ren", ",", "ruch\u00b7los", "th\u00f6\u00b7richt", "schal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sinnender Tr\u00e4umer, dich viele Blinde!", "tokens": ["Sin\u00b7nen\u00b7der", "Tr\u00e4u\u00b7mer", ",", "dich", "vie\u00b7le", "Blin\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "PIAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Noch heute, stumm voll gl\u00e4nzender Hoheit, lebt", "tokens": ["Noch", "heu\u00b7te", ",", "stumm", "voll", "gl\u00e4n\u00b7zen\u00b7der", "Ho\u00b7heit", ",", "lebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "$,", "ADJD", "ADJD", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein holder Liebling, g\u00f6ttlichen Odem spr\u00fcht", "tokens": ["Dein", "hol\u00b7der", "Lieb\u00b7ling", ",", "g\u00f6tt\u00b7li\u00b7chen", "O\u00b7dem", "spr\u00fcht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sogar der Marmor noch, der kalte \u2014", "tokens": ["So\u00b7gar", "der", "Mar\u00b7mor", "noch", ",", "der", "kal\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Selig begl\u00fcckte, die sah\u2019n das Urbild!", "tokens": ["Se\u00b7lig", "be\u00b7gl\u00fcck\u00b7te", ",", "die", "sah'n", "das", "Ur\u00b7bild", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Und manchesmal wohl sah ich dem Menschengott", "tokens": ["Und", "man\u00b7ches\u00b7mal", "wohl", "sah", "ich", "dem", "Men\u00b7schen\u00b7gott"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In\u2019s stille Antlitz, Schauer und Lust zugleich", "tokens": ["In's", "stil\u00b7le", "Ant\u00b7litz", ",", "Schau\u00b7er", "und", "Lust", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "NE", "KON", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Empfand ich, Ehrfurcht, heil\u2019ge Liebe", "tokens": ["Emp\u00b7fand", "ich", ",", "Ehr\u00b7furcht", ",", "heil'\u00b7ge", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tief in dem Busen entgegenflammen.", "tokens": ["Tief", "in", "dem", "Bu\u00b7sen", "ent\u00b7ge\u00b7gen\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Gedanken, seltsam, nimmergewollt, und doch", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "selt\u00b7sam", ",", "nim\u00b7mer\u00b7ge\u00b7wollt", ",", "und", "doch"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "VVFIN", "$,", "KON", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In s\u00fc\u00dfem Bann mich haltend, befielen mich,", "tokens": ["In", "s\u00fc\u00b7\u00dfem", "Bann", "mich", "hal\u00b7tend", ",", "be\u00b7fie\u00b7len", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Besiegten mich; wie Geisterfl\u00fcgel", "tokens": ["Be\u00b7sieg\u00b7ten", "mich", ";", "wie", "Geis\u00b7ter\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PWAV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt\u2019 ich die Stimme des Herzens rauschen \u2026", "tokens": ["H\u00f6rt'", "ich", "die", "Stim\u00b7me", "des", "Her\u00b7zens", "rau\u00b7schen", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Ha, ich versteh\u2019 dich! Himmlischen Tiefsinns voll,", "tokens": ["Ha", ",", "ich", "ver\u00b7steh'", "dich", "!", "Himm\u00b7li\u00b7schen", "Tief\u00b7sinns", "voll", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "$.", "ADJA", "NN", "ADJD", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sprach deine That, was And\u2019re verschweigen! \u2014 Ach,", "tokens": ["Sprach", "dei\u00b7ne", "That", ",", "was", "An\u00b7d'\u00b7re", "ver\u00b7schwei\u00b7gen", "!", "Ach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PRELS", "PIS", "VVINF", "$.", "$(", "ITJ", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es flieht der Thor selbst dann das Wahre,", "tokens": ["Es", "flieht", "der", "Thor", "selbst", "dann", "das", "Wah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leuchtet es still im Gewand der Sch\u00f6nheit!", "tokens": ["Leuch\u00b7tet", "es", "still", "im", "Ge\u00b7wand", "der", "Sch\u00f6n\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}