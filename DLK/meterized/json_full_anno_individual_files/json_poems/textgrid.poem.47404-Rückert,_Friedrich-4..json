{"textgrid.poem.47404": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Eitelkeit der Welt erkennen, ist nicht schwer,", "tokens": ["Die", "Ei\u00b7tel\u00b7keit", "der", "Welt", "er\u00b7ken\u00b7nen", ",", "ist", "nicht", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn die Erkenntnis dr\u00e4ngt von allen Seiten her.", "tokens": ["Denn", "die", "Er\u00b7kennt\u00b7nis", "dr\u00e4ngt", "von", "al\u00b7len", "Sei\u00b7ten", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch nur die bessere Erkenntnis macht dich frei:", "tokens": ["Doch", "nur", "die", "bes\u00b7se\u00b7re", "Er\u00b7kennt\u00b7nis", "macht", "dich", "frei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df in der eitlen Welt dein Sein nicht eitel sei.", "tokens": ["Da\u00df", "in", "der", "eit\u00b7len", "Welt", "dein", "Sein", "nicht", "ei\u00b7tel", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Eitelkeit der Welt mu\u00dft du an dir erfahren,", "tokens": ["Die", "Ei\u00b7tel\u00b7keit", "der", "Welt", "mu\u00dft", "du", "an", "dir", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Um deine h\u00f6here Bestimmung zu gewahren.", "tokens": ["Um", "dei\u00b7ne", "h\u00f6\u00b7he\u00b7re", "Be\u00b7stim\u00b7mung", "zu", "ge\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nie, wie du g'n\u00fcgsam seist, thut dir die Welt genug,", "tokens": ["Nie", ",", "wie", "du", "g'\u00b7n\u00fcg\u00b7sam", "seist", ",", "thut", "dir", "die", "Welt", "ge\u00b7nug", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "Bis von ihr nahm dein Geist zum Himmel seinen Flug.", "tokens": ["Bis", "von", "ihr", "nahm", "dein", "Geist", "zum", "Him\u00b7mel", "sei\u00b7nen", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dann wirst du gern der Welt die Eitelkeit vergeben,", "tokens": ["Dann", "wirst", "du", "gern", "der", "Welt", "die", "Ei\u00b7tel\u00b7keit", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word"