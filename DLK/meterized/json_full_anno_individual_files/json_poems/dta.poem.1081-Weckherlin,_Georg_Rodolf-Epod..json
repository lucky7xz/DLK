{"dta.poem.1081": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wan es ein vortheil soll sein/", "tokens": ["Wan", "es", "ein", "vor\u00b7theil", "soll", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VMFIN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der welt seinen alten stamen", "tokens": ["Der", "welt", "sei\u00b7nen", "al\u00b7ten", "sta\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zuerz\u00f6hlen/ dessen schein", "tokens": ["Zu\u00b7er\u00b7z\u00f6h\u00b7len", "/", "des\u00b7sen", "schein"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$(", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ner machet vnsern namen:", "tokens": ["Sch\u00f6\u00b7ner", "ma\u00b7chet", "vn\u00b7sern", "na\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer zweiflet/ das nicht mit recht", "tokens": ["Wer", "zweif\u00b7let", "/", "das", "nicht", "mit", "recht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$(", "PDS", "PTKNEG", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dein f\u00fcrtrefliches geschlecht", "tokens": ["Dein", "f\u00fcr\u00b7tre\u00b7fli\u00b7ches", "ge\u00b7schlecht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Seinen Vrsprung zu beweisen/", "tokens": ["Sei\u00b7nen", "Vr\u00b7sprung", "zu", "be\u00b7wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich ab dem edlen gebl\u00fct/", "tokens": ["Sich", "ab", "dem", "ed\u00b7len", "ge\u00b7bl\u00fct", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Weisen vnd starcken gem\u00fcht", "tokens": ["Wei\u00b7sen", "vnd", "star\u00b7cken", "ge\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "VVPP"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.10": {"text": "Deiner Anherren kan preisen?", "tokens": ["Dei\u00b7ner", "An\u00b7her\u00b7ren", "kan", "prei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}