{"textgrid.poem.24583": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein Freund will des andern Gedancken in seiner Jugend von der Liebe wissen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar meine Jugend hat was sch\u00f6nes nur geliebt.", "tokens": ["Zwar", "mei\u00b7ne", "Ju\u00b7gend", "hat", "was", "sch\u00f6\u00b7nes", "nur", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PIS", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil rechte Liebe nun nichts als Vergn\u00fcgen giebt;", "tokens": ["Weil", "rech\u00b7te", "Lie\u00b7be", "nun", "nichts", "als", "Ver\u00b7gn\u00fc\u00b7gen", "giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jedoch die meinige mir viele Marter machte;", "tokens": ["Je\u00b7doch", "die", "mei\u00b7ni\u00b7ge", "mir", "vie\u00b7le", "Mar\u00b7ter", "mach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PPOSS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch wenn die Liebe mich nach meinem Wunsch bedachte:", "tokens": ["Auch", "wenn", "die", "Lie\u00b7be", "mich", "nach", "mei\u00b7nem", "Wunsch", "be\u00b7dach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So spricht nunmehr mein Geist/ von aller Dunst befreyt:", "tokens": ["So", "spricht", "nun\u00b7mehr", "mein", "Geist", "/", "von", "al\u00b7ler", "Dunst", "be\u00b7freyt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$(", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich liebte nicht was sch\u00f6n/ ein Bild der Eitelkeit", "tokens": ["Ich", "lieb\u00b7te", "nicht", "was", "sch\u00f6n", "/", "ein", "Bild", "der", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "ADJD", "$(", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Liebsten Tugend war ein Feind nach meinem Triebe/", "tokens": ["Der", "Liebs\u00b7ten", "Tu\u00b7gend", "war", "ein", "Feind", "nach", "mei\u00b7nem", "Trie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ihre S\u00fcnde nur der Entzweck meiner Liebe.", "tokens": ["Und", "ih\u00b7re", "S\u00fcn\u00b7de", "nur", "der", "Ent\u00b7zweck", "mei\u00b7ner", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwar meine Jugend hat was sch\u00f6nes nur geliebt.", "tokens": ["Zwar", "mei\u00b7ne", "Ju\u00b7gend", "hat", "was", "sch\u00f6\u00b7nes", "nur", "ge\u00b7lieb