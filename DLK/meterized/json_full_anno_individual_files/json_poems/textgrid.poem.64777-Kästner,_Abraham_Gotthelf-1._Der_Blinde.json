{"textgrid.poem.64777": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1. Der Blinde", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zween Kenner, die ein Werk von ", "tokens": ["Zween", "Ken\u00b7ner", ",", "die", "ein", "Werk", "von"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PRELS", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00f6rt' einst ein Blinder lachend an;", "tokens": ["H\u00f6rt'", "einst", "ein", "Blin\u00b7der", "la\u00b7chend", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie, sprach er, k\u00f6nnt ihr was so ungem\u00e4\u00dfigt loben,", "tokens": ["Wie", ",", "sprach", "er", ",", "k\u00f6nnt", "ihr", "was", "so", "un\u00b7ge\u00b7m\u00e4\u00b7\u00dfigt", "lo\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PIS", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo ich nichts Sanftes f\u00fchlen kann?", "tokens": ["Wo", "ich", "nichts", "Sanf\u00b7tes", "f\u00fch\u00b7len", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erkl\u00e4rt mir das Gew\u00e4sch von Zeichnung, Farbe, Schatten:", "tokens": ["Er\u00b7kl\u00e4rt", "mir", "das", "Ge\u00b7w\u00e4sch", "von", "Zeich\u00b7nung", ",", "Far\u00b7be", ",", "Schat\u00b