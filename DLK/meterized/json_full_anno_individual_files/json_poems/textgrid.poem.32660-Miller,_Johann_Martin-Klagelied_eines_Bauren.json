{"textgrid.poem.32660": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Klagelied eines Bauren", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ganze Dorf versammlet sich,", "tokens": ["Das", "gan\u00b7ze", "Dorf", "ver\u00b7samm\u00b7let", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Kirmestanz, im Reihen;", "tokens": ["Zum", "Kir\u00b7me\u00b7stanz", ",", "im", "Rei\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es freut sich alles, aber mich", "tokens": ["Es", "freut", "sich", "al\u00b7les", ",", "a\u00b7ber", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "$,", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann f\u00fcrder nichts erfreuen;", "tokens": ["Kann", "f\u00fcr\u00b7der", "nichts", "er\u00b7freu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "F\u00fcr mich ist Spiel und Tanz vorbei,", "tokens": ["F\u00fcr", "mich", "ist", "Spiel", "und", "Tanz", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das lachen ist vor\u00fcber;", "tokens": ["Das", "la\u00b7chen", "ist", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VAFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hasse Lieder und Schalmei,", "tokens": ["Ich", "has\u00b7se", "Lie\u00b7der", "und", "Schal\u00b7mei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Klagen sind mir lieber.", "tokens": ["Und", "Kla\u00b7gen", "sind", "mir", "lie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Denn ach! mein Hannchen fehlet mir;", "tokens": ["Denn", "ach", "!", "mein", "Hann\u00b7chen", "feh\u00b7let", "mir", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie kann ich sie vergessen:", "tokens": ["Nie", "kann", "ich", "sie", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df zu gut, was ich in ihr", "tokens": ["Ich", "wei\u00df", "zu", "gut", ",", "was", "ich", "in", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "$,", "PWS", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr einen Schatz besessen.", "tokens": ["F\u00fcr", "ei\u00b7nen", "Schatz", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Unschuldig war sie, wie ein Lamm,", "tokens": ["Un\u00b7schul\u00b7dig", "war", "sie", ",", "wie", "ein", "Lamm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "That niemand was zuleide,", "tokens": ["That", "nie\u00b7mand", "was", "zu\u00b7lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lebte fromm und tugendsam,", "tokens": ["Und", "leb\u00b7te", "fromm", "und", "tu\u00b7gend\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu aller Menschen Freude.", "tokens": ["Zu", "al\u00b7ler", "Men\u00b7schen", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie hatte Wangen, voll und rund,", "tokens": ["Sie", "hat\u00b7te", "Wan\u00b7gen", ",", "voll", "und", "rund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sanfter noch als Pfirschen,", "tokens": ["Und", "sanf\u00b7ter", "noch", "als", "Pfir\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein blaues Aug', und einen Mund,", "tokens": ["Ein", "blau\u00b7es", "Aug'", ",", "und", "ei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der r\u00f6ter war als Kirschen.", "tokens": ["Der", "r\u00f6\u00b7ter", "war", "als", "Kir\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Man konnte, sah sie einen an,", "tokens": ["Man", "konn\u00b7te", ",", "sah", "sie", "ei\u00b7nen", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "$,", "VVFIN", "PPER", "ART", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blicke nicht ertragen,", "tokens": ["Die", "Bli\u00b7cke", "nicht", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und, wenn sie lachte, mu\u00dfte man", "tokens": ["Und", ",", "wenn", "sie", "lach\u00b7te", ",", "mu\u00df\u00b7te", "man"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "VMFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Augen niederschlagen.", "tokens": ["Die", "Au\u00b7gen", "nie\u00b7der\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie bin ich neulich noch mit ihr", "tokens": ["Wie", "bin", "ich", "neu\u00b7lich", "noch", "mit", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Maientag gesprungen!", "tokens": ["Am", "Mai\u00b7en\u00b7tag", "ge\u00b7sprun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis an den Abend tanzten wir,", "tokens": ["Bis", "an", "den", "A\u00b7bend", "tanz\u00b7ten", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sch\u00e4kerten und sungen;", "tokens": ["Und", "sch\u00e4\u00b7ker\u00b7ten", "und", "sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da nahm sie meinen Hut, und wand,", "tokens": ["Da", "nahm", "sie", "mei\u00b7nen", "Hut", ",", "und", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschwinder, als ich's dachte,", "tokens": ["Ge\u00b7schwin\u00b7der", ",", "als", "ich's", "dach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um ihn ein pappelgr\u00fcnes Band,", "tokens": ["Um", "ihn", "ein", "pap\u00b7pel\u00b7gr\u00fc\u00b7nes", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sah sich um, und lachte.", "tokens": ["Und", "sah", "sich", "um", ",", "und", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O Gott! wer h\u00e4tte da gedacht,", "tokens": ["O", "Gott", "!", "wer", "h\u00e4t\u00b7te", "da", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich sie dankbar k\u00fc\u00dfte,", "tokens": ["Als", "ich", "sie", "dank\u00b7bar", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sich so bald die gr\u00fcne Tracht", "tokens": ["Da\u00df", "sich", "so", "bald", "die", "gr\u00fc\u00b7ne", "Tracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In schwarze wandeln m\u00fc\u00dfte?", "tokens": ["In", "schwar\u00b7ze", "wan\u00b7deln", "m\u00fc\u00df\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nun darfst du, liebes Band, um mich", "tokens": ["Nun", "darfst", "du", ",", "lie\u00b7bes", "Band", ",", "um", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "KOUI", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht mehr im Winde rauschen;", "tokens": ["Nicht", "mehr", "im", "Win\u00b7de", "rau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herunternehmen mu\u00df ich dich,", "tokens": ["Her\u00b7un\u00b7ter\u00b7neh\u00b7men", "mu\u00df", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gegen Flor vertauschen!", "tokens": ["Und", "ge\u00b7gen", "Flor", "ver\u00b7tau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Den Gottesacker will ich mir", "tokens": ["Den", "Got\u00b7te\u00b7sa\u00b7cker", "will", "ich", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum liebsten Ort erw\u00e4hlen,", "tokens": ["Zum", "liebs\u00b7ten", "Ort", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und manchen Abend mich von hier", "tokens": ["Und", "man\u00b7chen", "A\u00b7bend", "mich", "von", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "PPER", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Hannchens Grabe stehlen;", "tokens": ["Zu", "Hann\u00b7chens", "Gra\u00b7be", "steh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da will ich es mit Majoran", "tokens": ["Da", "will", "ich", "es", "mit", "Ma\u00b7jo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Ma\u00dflieb \u00fcbers\u00e4en,", "tokens": ["Und", "Ma\u00df\u00b7lieb", "\u00fc\u00b7bers\u00b7\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein schwarzes Kreuz, mit Versen dran,", "tokens": ["Ein", "schwar\u00b7zes", "Kreuz", ",", "mit", "Ver\u00b7sen", "dran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll in der Mitte stehen.", "tokens": ["Soll", "in", "der", "Mit\u00b7te", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ein Myrtenkranz soll an der Wand", "tokens": ["Ein", "Myr\u00b7ten\u00b7kranz", "soll", "an", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsrer Kirche prangen,", "tokens": ["In", "uns\u00b7rer", "Kir\u00b7che", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und neben ihm das gr\u00fcne Band", "tokens": ["Und", "ne\u00b7ben", "ihm", "das", "gr\u00fc\u00b7ne", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Angedenken hangen;", "tokens": ["Zum", "An\u00b7ge\u00b7den\u00b7ken", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "In jeder Predigt sitz' ich dann", "tokens": ["In", "je\u00b7der", "Pre\u00b7digt", "sitz'", "ich", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Kranze gegen\u00fcber,", "tokens": ["Dem", "Kran\u00b7ze", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seh' ihn mit nassen Augen an,", "tokens": ["Seh'", "ihn", "mit", "nas\u00b7sen", "Au\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und h\u00e4rme mich dar\u00fcber:", "tokens": ["Und", "h\u00e4r\u00b7me", "mich", "da\u00b7r\u00fc\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Bis endlich, wenn es Gott gef\u00e4llt,", "tokens": ["Bis", "end\u00b7lich", ",", "wenn", "es", "Gott", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "KOUS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es meinem Wunsch gelinget,", "tokens": ["Es", "mei\u00b7nem", "Wunsch", "ge\u00b7lin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und er mich auch aus dieser Welt", "tokens": ["Und", "er", "mich", "auch", "aus", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PRF", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meinem Hannchen bringet.", "tokens": ["Zu", "mei\u00b7nem", "Hann\u00b7chen", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Das ganze Dorf versammlet sich,", "tokens": ["Das", "gan\u00b7ze", "Dorf", "ver\u00b7samm\u00b7let", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Kirmestanz, im Reihen;", "tokens": ["Zum", "Kir\u00b7me\u00b7stanz", ",", "im", "Rei\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es freut sich alles, aber mich", "tokens": ["Es", "freut", "sich", "al\u00b7les", ",", "a\u00b7ber", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "$,", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann f\u00fcrder nichts erfreuen;", "tokens": ["Kann", "f\u00fcr\u00b7der", "nichts", "er\u00b7freu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "F\u00fcr mich ist Spiel und Tanz vorbei,", "tokens": ["F\u00fcr", "mich", "ist", "Spiel", "und", "Tanz", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das lachen ist vor\u00fcber;", "tokens": ["Das", "la\u00b7chen", "ist", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VAFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hasse Lieder und Schalmei,", "tokens": ["Ich", "has\u00b7se", "Lie\u00b7der", "und", "Schal\u00b7mei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Klagen sind mir lieber.", "tokens": ["Und", "Kla\u00b7gen", "sind", "mir", "lie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Denn ach! mein Hannchen fehlet mir;", "tokens": ["Denn", "ach", "!", "mein", "Hann\u00b7chen", "feh\u00b7let", "mir", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie kann ich sie vergessen:", "tokens": ["Nie", "kann", "ich", "sie", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df zu gut, was ich in ihr", "tokens": ["Ich", "wei\u00df", "zu", "gut", ",", "was", "ich", "in", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "$,", "PWS", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr einen Schatz besessen.", "tokens": ["F\u00fcr", "ei\u00b7nen", "Schatz", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Unschuldig war sie, wie ein Lamm,", "tokens": ["Un\u00b7schul\u00b7dig", "war", "sie", ",", "wie", "ein", "Lamm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "That niemand was zuleide,", "tokens": ["That", "nie\u00b7mand", "was", "zu\u00b7lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lebte fromm und tugendsam,", "tokens": ["Und", "leb\u00b7te", "fromm", "und", "tu\u00b7gend\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu aller Menschen Freude.", "tokens": ["Zu", "al\u00b7ler", "Men\u00b7schen", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Sie hatte Wangen, voll und rund,", "tokens": ["Sie", "hat\u00b7te", "Wan\u00b7gen", ",", "voll", "und", "rund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sanfter noch als Pfirschen,", "tokens": ["Und", "sanf\u00b7ter", "noch", "als", "Pfir\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein blaues Aug', und einen Mund,", "tokens": ["Ein", "blau\u00b7es", "Aug'", ",", "und", "ei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der r\u00f6ter war als Kirschen.", "tokens": ["Der", "r\u00f6\u00b7ter", "war", "als", "Kir\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Man konnte, sah sie einen an,", "tokens": ["Man", "konn\u00b7te", ",", "sah", "sie", "ei\u00b7nen", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "$,", "VVFIN", "PPER", "ART", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blicke nicht ertragen,", "tokens": ["Die", "Bli\u00b7cke", "nicht", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und, wenn sie lachte, mu\u00dfte man", "tokens": ["Und", ",", "wenn", "sie", "lach\u00b7te", ",", "mu\u00df\u00b7te", "man"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "VMFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Augen niederschlagen.", "tokens": ["Die", "Au\u00b7gen", "nie\u00b7der\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Wie bin ich neulich noch mit ihr", "tokens": ["Wie", "bin", "ich", "neu\u00b7lich", "noch", "mit", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Maientag gesprungen!", "tokens": ["Am", "Mai\u00b7en\u00b7tag", "ge\u00b7sprun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis an den Abend tanzten wir,", "tokens": ["Bis", "an", "den", "A\u00b7bend", "tanz\u00b7ten", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sch\u00e4kerten und sungen;", "tokens": ["Und", "sch\u00e4\u00b7ker\u00b7ten", "und", "sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Da nahm sie meinen Hut, und wand,", "tokens": ["Da", "nahm", "sie", "mei\u00b7nen", "Hut", ",", "und", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschwinder, als ich's dachte,", "tokens": ["Ge\u00b7schwin\u00b7der", ",", "als", "ich's", "dach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um ihn ein pappelgr\u00fcnes Band,", "tokens": ["Um", "ihn", "ein", "pap\u00b7pel\u00b7gr\u00fc\u00b7nes", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sah sich um, und lachte.", "tokens": ["Und", "sah", "sich", "um", ",", "und", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "O Gott! wer h\u00e4tte da gedacht,", "tokens": ["O", "Gott", "!", "wer", "h\u00e4t\u00b7te", "da", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich sie dankbar k\u00fc\u00dfte,", "tokens": ["Als", "ich", "sie", "dank\u00b7bar", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sich so bald die gr\u00fcne Tracht", "tokens": ["Da\u00df", "sich", "so", "bald", "die", "gr\u00fc\u00b7ne", "Tracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In schwarze wandeln m\u00fc\u00dfte?", "tokens": ["In", "schwar\u00b7ze", "wan\u00b7deln", "m\u00fc\u00df\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Nun darfst du, liebes Band, um mich", "tokens": ["Nun", "darfst", "du", ",", "lie\u00b7bes", "Band", ",", "um", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "KOUI", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht mehr im Winde rauschen;", "tokens": ["Nicht", "mehr", "im", "Win\u00b7de", "rau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herunternehmen mu\u00df ich dich,", "tokens": ["Her\u00b7un\u00b7ter\u00b7neh\u00b7men", "mu\u00df", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gegen Flor vertauschen!", "tokens": ["Und", "ge\u00b7gen", "Flor", "ver\u00b7tau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Den Gottesacker will ich mir", "tokens": ["Den", "Got\u00b7te\u00b7sa\u00b7cker", "will", "ich", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum liebsten Ort erw\u00e4hlen,", "tokens": ["Zum", "liebs\u00b7ten", "Ort", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und manchen Abend mich von hier", "tokens": ["Und", "man\u00b7chen", "A\u00b7bend", "mich", "von", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "PPER", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Hannchens Grabe stehlen;", "tokens": ["Zu", "Hann\u00b7chens", "Gra\u00b7be", "steh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Da will ich es mit Majoran", "tokens": ["Da", "will", "ich", "es", "mit", "Ma\u00b7jo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Ma\u00dflieb \u00fcbers\u00e4en,", "tokens": ["Und", "Ma\u00df\u00b7lieb", "\u00fc\u00b7bers\u00b7\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein schwarzes Kreuz, mit Versen dran,", "tokens": ["Ein", "schwar\u00b7zes", "Kreuz", ",", "mit", "Ver\u00b7sen", "dran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll in der Mitte stehen.", "tokens": ["Soll", "in", "der", "Mit\u00b7te", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Ein Myrtenkranz soll an der Wand", "tokens": ["Ein", "Myr\u00b7ten\u00b7kranz", "soll", "an", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsrer Kirche prangen,", "tokens": ["In", "uns\u00b7rer", "Kir\u00b7che", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und neben ihm das gr\u00fcne Band", "tokens": ["Und", "ne\u00b7ben", "ihm", "das", "gr\u00fc\u00b7ne", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Angedenken hangen;", "tokens": ["Zum", "An\u00b7ge\u00b7den\u00b7ken", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.29": {"line.1": {"text": "In jeder Predigt sitz' ich dann", "tokens": ["In", "je\u00b7der", "Pre\u00b7digt", "sitz'", "ich", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Kranze gegen\u00fcber,", "tokens": ["Dem", "Kran\u00b7ze", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seh' ihn mit nassen Augen an,", "tokens": ["Seh'", "ihn", "mit", "nas\u00b7sen", "Au\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und h\u00e4rme mich dar\u00fcber:", "tokens": ["Und", "h\u00e4r\u00b7me", "mich", "da\u00b7r\u00fc\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "Bis endlich, wenn es Gott gef\u00e4llt,", "tokens": ["Bis", "end\u00b7lich", ",", "wenn", "es", "Gott", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "KOUS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es meinem Wunsch gelinget,", "tokens": ["Es", "mei\u00b7nem", "Wunsch", "ge\u00b7lin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und er mich auch aus dieser Welt", "tokens": ["Und", "er", "mich", "auch", "aus", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PRF", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meinem Hannchen bringet.", "tokens": ["Zu", "mei\u00b7nem", "Hann\u00b7chen", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}