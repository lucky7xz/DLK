{"textgrid.poem.36880": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Macht und Vorsehung", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist mein Lied!", "tokens": ["Gott", "ist", "mein", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Er ist der Gott der St\u00e4rke;", "tokens": ["Er", "ist", "der", "Gott", "der", "St\u00e4r\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hehr ist sein Nam, und gro\u00df sind seine Werke,", "tokens": ["Hehr", "ist", "sein", "Nam", ",", "und", "gro\u00df", "sind", "sei\u00b7ne", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alle Himmel sein Gebiet.", "tokens": ["Und", "al\u00b7le", "Him\u00b7mel", "sein", "Ge\u00b7biet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er will und spricht's;", "tokens": ["Er", "will", "und", "spricht's", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So sind und leben Welten.", "tokens": ["So", "sind", "und", "le\u00b7ben", "Wel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und er gebeut; so fallen durch sein Schelten", "tokens": ["Und", "er", "ge\u00b7beut", ";", "so", "fal\u00b7len", "durch", "sein", "Schel\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Himmel wieder in ihr Nichts.", "tokens": ["Die", "Him\u00b7mel", "wie\u00b7der", "in", "ihr", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Licht ist sein Kleid,", "tokens": ["Licht", "ist", "sein", "Kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und seine Wahl das Beste;", "tokens": ["Und", "sei\u00b7ne", "Wahl", "das", "Bes\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er herrscht als Gott, und seines Thrones Feste", "tokens": ["Er", "herrscht", "als", "Gott", ",", "und", "sei\u00b7nes", "Thro\u00b7nes", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist Wahrheit und Gerechtigkeit.", "tokens": ["Ist", "Wahr\u00b7heit", "und", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Unendlich reich,", "tokens": ["Un\u00b7end\u00b7lich", "reich", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein Meer von Seligkeiten,", "tokens": ["Ein", "Meer", "von", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ohn Anfang Gott, und Gott in ewgen Zeiten!", "tokens": ["Ohn", "An\u00b7fang", "Gott", ",", "und", "Gott", "in", "ew\u00b7gen", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herr aller Welt, wer ist dir gleich?", "tokens": ["Herr", "al\u00b7ler", "Welt", ",", "wer", "ist", "dir", "gleich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was ist und war,", "tokens": ["Was", "ist", "und", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "KON", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In Himmel, Erd und Meere,", "tokens": ["In", "Him\u00b7mel", ",", "Erd", "und", "Mee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das kennet Gott, und seiner Werke Heere", "tokens": ["Das", "ken\u00b7net", "Gott", ",", "und", "sei\u00b7ner", "Wer\u00b7ke", "Hee\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind ewig vor ihm offenbar.", "tokens": ["Sind", "e\u00b7wig", "vor", "ihm", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er ist um mich,", "tokens": ["Er", "ist", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Schafft, da\u00df ich sicher ruhe;", "tokens": ["Schafft", ",", "da\u00df", "ich", "si\u00b7cher", "ru\u00b7he", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er schafft, was ich vor oder nachmals tue,", "tokens": ["Er", "schafft", ",", "was", "ich", "vor", "o\u00b7der", "nach\u00b7mals", "tue", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "APPR", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und er erforschet mich und dich.", "tokens": ["Und", "er", "er\u00b7for\u00b7schet", "mich", "und", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er ist dir nah,", "tokens": ["Er", "ist", "dir", "nah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Du sitzest oder gehest;", "tokens": ["Du", "sit\u00b7zest", "o\u00b7der", "ge\u00b7hest", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob du ans Meer, ob du gen Himmel fl\u00f6hest:", "tokens": ["Ob", "du", "ans", "Meer", ",", "ob", "du", "gen", "Him\u00b7mel", "fl\u00f6\u00b7hest", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So ist er allenthalben da.", "tokens": ["So", "ist", "er", "al\u00b7len\u00b7thal\u00b7ben", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er kennt mein Flehn", "tokens": ["Er", "kennt", "mein", "Flehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und allen Rat der Seele.", "tokens": ["Und", "al\u00b7len", "Rat", "der", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er wei\u00df, wie oft ich Gutes tu und fehle,", "tokens": ["Er", "wei\u00df", ",", "wie", "oft", "ich", "Gu\u00b7tes", "tu", "und", "feh\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "PPER", "NN", "NE", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eilt, mir gn\u00e4dig beizustehn.", "tokens": ["Und", "eilt", ",", "mir", "gn\u00e4\u00b7dig", "bei\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er wog mir dar,", "tokens": ["Er", "wog", "mir", "dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Was er mir geben wollte,", "tokens": ["Was", "er", "mir", "ge\u00b7ben", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schrieb auf sein Buch, wie lang ich leben sollte,", "tokens": ["Schrieb", "auf", "sein", "Buch", ",", "wie", "lang", "ich", "le\u00b7ben", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da ich noch unbereitet war.", "tokens": ["Da", "ich", "noch", "un\u00b7be\u00b7rei\u00b7tet", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nichts, nichts ist mein,", "tokens": ["Nichts", ",", "nichts", "ist", "mein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das Gott nicht angeh\u00f6re.", "tokens": ["Das", "Gott", "nicht", "an\u00b7ge\u00b7h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herr, immerdar soll deines Namens Ehre,", "tokens": ["Herr", ",", "im\u00b7mer\u00b7dar", "soll", "dei\u00b7nes", "Na\u00b7mens", "Eh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Lob in meinem Munde sein!", "tokens": ["Dein", "Lob", "in", "mei\u00b7nem", "Mun\u00b7de", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wer kann die Pracht", "tokens": ["Wer", "kann", "die", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Von deinen Wundern fassen?", "tokens": ["Von", "dei\u00b7nen", "Wun\u00b7dern", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein jeder Staub, den du hast werden lassen,", "tokens": ["Ein", "je\u00b7der", "Staub", ",", "den", "du", "hast", "wer\u00b7den", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "PPER", "VAFIN", "VAINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verk\u00fcndigt seines Sch\u00f6pfers Macht.", "tokens": ["Ver\u00b7k\u00fcn\u00b7digt", "sei\u00b7nes", "Sch\u00f6p\u00b7fers", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Der kleinste Halm", "tokens": ["Der", "kleins\u00b7te", "Halm"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ist deiner Weisheit Spiegel.", "tokens": ["Ist", "dei\u00b7ner", "Weis\u00b7heit", "Spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du, Luft und Meer, ihr Auen, Tal und H\u00fcgel,", "tokens": ["Du", ",", "Luft", "und", "Meer", ",", "ihr", "Au\u00b7en", ",", "Tal", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr seid sein Loblied und sein Psalm!", "tokens": ["Ihr", "seid", "sein", "Lob\u00b7lied", "und", "sein", "Psalm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Du tr\u00e4nkst das Land,", "tokens": ["Du", "tr\u00e4nkst", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "F\u00fchrst uns auf gr\u00fcne Weiden;", "tokens": ["F\u00fchrst", "uns", "auf", "gr\u00fc\u00b7ne", "Wei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Nacht und Tag, und Korn und Wein und Freuden", "tokens": ["Und", "Nacht", "und", "Tag", ",", "und", "Korn", "und", "Wein", "und", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Empfangen wir aus deiner Hand.", "tokens": ["Emp\u00b7fan\u00b7gen", "wir", "aus", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Kein Sperling f\u00e4llt,", "tokens": ["Kein", "Sper\u00b7ling", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Herr, ohne deinen Willen;", "tokens": ["Herr", ",", "oh\u00b7ne", "dei\u00b7nen", "Wil\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sollt ich mein Herz nicht mit dem Troste stillen,", "tokens": ["Sollt", "ich", "mein", "Herz", "nicht", "mit", "dem", "Tros\u00b7te", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df deine Hand mein Leben h\u00e4lt?", "tokens": ["Da\u00df", "dei\u00b7ne", "Hand", "mein", "Le\u00b7ben", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Ist Gott mein Schutz,", "tokens": ["Ist", "Gott", "mein", "Schutz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Will Gott mein Retter werden:", "tokens": ["Will", "Gott", "mein", "Ret\u00b7ter", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So frag ich nichts nach Himmel und nach Erden,", "tokens": ["So", "frag", "ich", "nichts", "nach", "Him\u00b7mel", "und", "nach", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PIS", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und biete selbst der H\u00f6lle Trutz.", "tokens": ["Und", "bie\u00b7te", "selbst", "der", "H\u00f6l\u00b7le", "Trutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Gott ist mein Lied!", "tokens": ["Gott", "ist", "mein", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Er ist der Gott der St\u00e4rke;", "tokens": ["Er", "ist", "der", "Gott", "der", "St\u00e4r\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hehr ist sein Nam, und gro\u00df sind seine Werke,", "tokens": ["Hehr", "ist", "sein", "Nam", ",", "und", "gro\u00df", "sind", "sei\u00b7ne", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alle Himmel sein Gebiet.", "tokens": ["Und", "al\u00b7le", "Him\u00b7mel", "sein", "Ge\u00b7biet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Er will und spricht's;", "tokens": ["Er", "will", "und", "spricht's", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So sind und leben Welten.", "tokens": ["So", "sind", "und", "le\u00b7ben", "Wel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und er gebeut; so fallen durch sein Schelten", "tokens": ["Und", "er", "ge\u00b7beut", ";", "so", "fal\u00b7len", "durch", "sein", "Schel\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Himmel wieder in ihr Nichts.", "tokens": ["Die", "Him\u00b7mel", "wie\u00b7der", "in", "ihr", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Licht ist sein Kleid,", "tokens": ["Licht", "ist", "sein", "Kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und seine Wahl das Beste;", "tokens": ["Und", "sei\u00b7ne", "Wahl", "das", "Bes\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er herrscht als Gott, und seines Thrones Feste", "tokens": ["Er", "herrscht", "als", "Gott", ",", "und", "sei\u00b7nes", "Thro\u00b7nes", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist Wahrheit und Gerechtigkeit.", "tokens": ["Ist", "Wahr\u00b7heit", "und", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Unendlich reich,", "tokens": ["Un\u00b7end\u00b7lich", "reich", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein Meer von Seligkeiten,", "tokens": ["Ein", "Meer", "von", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ohn Anfang Gott, und Gott in ewgen Zeiten!", "tokens": ["Ohn", "An\u00b7fang", "Gott", ",", "und", "Gott", "in", "ew\u00b7gen", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herr aller Welt, wer ist dir gleich?", "tokens": ["Herr", "al\u00b7ler", "Welt", ",", "wer", "ist", "dir", "gleich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Was ist und war,", "tokens": ["Was", "ist", "und", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "KON", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In Himmel, Erd und Meere,", "tokens": ["In", "Him\u00b7mel", ",", "Erd", "und", "Mee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das kennet Gott, und seiner Werke Heere", "tokens": ["Das", "ken\u00b7net", "Gott", ",", "und", "sei\u00b7ner", "Wer\u00b7ke", "Hee\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind ewig vor ihm offenbar.", "tokens": ["Sind", "e\u00b7wig", "vor", "ihm", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Er ist um mich,", "tokens": ["Er", "ist", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Schafft, da\u00df ich sicher ruhe;", "tokens": ["Schafft", ",", "da\u00df", "ich", "si\u00b7cher", "ru\u00b7he", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er schafft, was ich vor oder nachmals tue,", "tokens": ["Er", "schafft", ",", "was", "ich", "vor", "o\u00b7der", "nach\u00b7mals", "tue", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "APPR", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und er erforschet mich und dich.", "tokens": ["Und", "er", "er\u00b7for\u00b7schet", "mich", "und", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Er ist dir nah,", "tokens": ["Er", "ist", "dir", "nah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Du sitzest oder gehest;", "tokens": ["Du", "sit\u00b7zest", "o\u00b7der", "ge\u00b7hest", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob du ans Meer, ob du gen Himmel fl\u00f6hest:", "tokens": ["Ob", "du", "ans", "Meer", ",", "ob", "du", "gen", "Him\u00b7mel", "fl\u00f6\u00b7hest", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So ist er allenthalben da.", "tokens": ["So", "ist", "er", "al\u00b7len\u00b7thal\u00b7ben", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Er kennt mein Flehn", "tokens": ["Er", "kennt", "mein", "Flehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und allen Rat der Seele.", "tokens": ["Und", "al\u00b7len", "Rat", "der", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er wei\u00df, wie oft ich Gutes tu und fehle,", "tokens": ["Er", "wei\u00df", ",", "wie", "oft", "ich", "Gu\u00b7tes", "tu", "und", "feh\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "PPER", "NN", "NE", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eilt, mir gn\u00e4dig beizustehn.", "tokens": ["Und", "eilt", ",", "mir", "gn\u00e4\u00b7dig", "bei\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.24": {"line.1": {"text": "Er wog mir dar,", "tokens": ["Er", "wog", "mir", "dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Was er mir geben wollte,", "tokens": ["Was", "er", "mir", "ge\u00b7ben", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schrieb auf sein Buch, wie lang ich leben sollte,", "tokens": ["Schrieb", "auf", "sein", "Buch", ",", "wie", "lang", "ich", "le\u00b7ben", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da ich noch unbereitet war.", "tokens": ["Da", "ich", "noch", "un\u00b7be\u00b7rei\u00b7tet", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Nichts, nichts ist mein,", "tokens": ["Nichts", ",", "nichts", "ist", "mein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das Gott nicht angeh\u00f6re.", "tokens": ["Das", "Gott", "nicht", "an\u00b7ge\u00b7h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herr, immerdar soll deines Namens Ehre,", "tokens": ["Herr", ",", "im\u00b7mer\u00b7dar", "soll", "dei\u00b7nes", "Na\u00b7mens", "Eh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Lob in meinem Munde sein!", "tokens": ["Dein", "Lob", "in", "mei\u00b7nem", "Mun\u00b7de", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.26": {"line.1": {"text": "Wer kann die Pracht", "tokens": ["Wer", "kann", "die", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Von deinen Wundern fassen?", "tokens": ["Von", "dei\u00b7nen", "Wun\u00b7dern", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein jeder Staub, den du hast werden lassen,", "tokens": ["Ein", "je\u00b7der", "Staub", ",", "den", "du", "hast", "wer\u00b7den", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "PPER", "VAFIN", "VAINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verk\u00fcndigt seines Sch\u00f6pfers Macht.", "tokens": ["Ver\u00b7k\u00fcn\u00b7digt", "sei\u00b7nes", "Sch\u00f6p\u00b7fers", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.27": {"line.1": {"text": "Der kleinste Halm", "tokens": ["Der", "kleins\u00b7te", "Halm"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ist deiner Weisheit Spiegel.", "tokens": ["Ist", "dei\u00b7ner", "Weis\u00b7heit", "Spie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du, Luft und Meer, ihr Auen, Tal und H\u00fcgel,", "tokens": ["Du", ",", "Luft", "und", "Meer", ",", "ihr", "Au\u00b7en", ",", "Tal", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr seid sein Loblied und sein Psalm!", "tokens": ["Ihr", "seid", "sein", "Lob\u00b7lied", "und", "sein", "Psalm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.28": {"line.1": {"text": "Du tr\u00e4nkst das Land,", "tokens": ["Du", "tr\u00e4nkst", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "F\u00fchrst uns auf gr\u00fcne Weiden;", "tokens": ["F\u00fchrst", "uns", "auf", "gr\u00fc\u00b7ne", "Wei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Nacht und Tag, und Korn und Wein und Freuden", "tokens": ["Und", "Nacht", "und", "Tag", ",", "und", "Korn", "und", "Wein", "und", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Empfangen wir aus deiner Hand.", "tokens": ["Emp\u00b7fan\u00b7gen", "wir", "aus", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.29": {"line.1": {"text": "Kein Sperling f\u00e4llt,", "tokens": ["Kein", "Sper\u00b7ling", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Herr, ohne deinen Willen;", "tokens": ["Herr", ",", "oh\u00b7ne", "dei\u00b7nen", "Wil\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sollt ich mein Herz nicht mit dem Troste stillen,", "tokens": ["Sollt", "ich", "mein", "Herz", "nicht", "mit", "dem", "Tros\u00b7te", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df deine Hand mein Leben h\u00e4lt?", "tokens": ["Da\u00df", "dei\u00b7ne", "Hand", "mein", "Le\u00b7ben", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.30": {"line.1": {"text": "Ist Gott mein Schutz,", "tokens": ["Ist", "Gott", "mein", "Schutz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Will Gott mein Retter werden:", "tokens": ["Will", "Gott", "mein", "Ret\u00b7ter", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So frag ich nichts nach Himmel und nach Erden,", "tokens": ["So", "frag", "ich", "nichts", "nach", "Him\u00b7mel", "und", "nach", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PIS", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und biete selbst der H\u00f6lle Trutz.", "tokens": ["Und", "bie\u00b7te", "selbst", "der", "H\u00f6l\u00b7le", "Trutz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}