{"dta.poem.19593": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Kaiser Friedrich der Zweite.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "In den Armen seines J\u00fcngsten", "tokens": ["In", "den", "Ar\u00b7men", "sei\u00b7nes", "J\u00fcng\u00b7sten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Phantasirt der sieche Kaiser,", "tokens": ["Phan\u00b7ta\u00b7sirt", "der", "sie\u00b7che", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem treuen Herzen Manfreds", "tokens": ["An", "dem", "treu\u00b7en", "Her\u00b7zen", "Man\u00b7freds"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NE"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "K\u00e4mpft er seinen Todeskampf.", "tokens": ["K\u00e4mpft", "er", "sei\u00b7nen", "To\u00b7des\u00b7kampf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit den geisterhaften, blauen", "tokens": ["Mit", "den", "geis\u00b7ter\u00b7haf\u00b7ten", ",", "blau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Augen starrt er in die Weite,", "tokens": ["Au\u00b7gen", "starrt", "er", "in", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend seine fieberhei\u00dfe", "tokens": ["W\u00e4h\u00b7rend", "sei\u00b7ne", "fie\u00b7ber\u00b7hei\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rechte pre\u00dft des Sohnes Hand:", "tokens": ["Rech\u00b7te", "pre\u00dft", "des", "Soh\u00b7nes", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201emanfred, lausche meinen Worten!", "tokens": ["\u201e", "man\u00b7fred", ",", "lau\u00b7sche", "mei\u00b7nen", "Wor\u00b7ten", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcben auf dem Marmortische", "tokens": ["Dr\u00fc\u00b7ben", "auf", "dem", "Mar\u00b7mor\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Mit den Greifen liegt mein g\u00fcltig", "tokens": ["Mit", "den", "Grei\u00b7fen", "liegt", "mein", "g\u00fcl\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterschrieben Testament.", "tokens": ["Un\u00b7ter\u00b7schrie\u00b7ben", "Tes\u00b7ta\u00b7ment", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eine Kutte, drin zu sterben,", "tokens": ["Ei\u00b7ne", "Kut\u00b7te", ",", "drin", "zu", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schenkten mir die braven M\u00f6nche,", "tokens": ["Schenk\u00b7ten", "mir", "die", "bra\u00b7ven", "M\u00f6n\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich meine Seele rette", "tokens": ["Da\u00df", "ich", "mei\u00b7ne", "See\u00b7le", "ret\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trotz dem Bann des heil'gen Stuhls.", "tokens": ["Trotz", "dem", "Bann", "des", "heil'\u00b7gen", "Stuhls", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Manfred, meines Herzens Liebling,", "tokens": ["Man\u00b7fred", ",", "mei\u00b7nes", "Her\u00b7zens", "Lieb\u00b7ling", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "La\u00df den Herold auf den S\u00f6ller", "tokens": ["La\u00df", "den", "He\u00b7rold", "auf", "den", "S\u00f6l\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Treten und der Erde melden,", "tokens": ["Tre\u00b7ten", "und", "der", "Er\u00b7de", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der Hohenstaufe schied.", "tokens": ["Da\u00df", "der", "Ho\u00b7hen\u00b7stau\u00b7fe", "schied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Manfred mit den blonden Locken", "tokens": ["Man\u00b7fred", "mit", "den", "blon\u00b7den", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sarge pr\u00e4chtig ein die Kutte,", "tokens": ["Sar\u00b7ge", "pr\u00e4ch\u00b7tig", "ein", "die", "Kut\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchre sie mit Schaugepr\u00e4nge", "tokens": ["F\u00fch\u00b7re", "sie", "mit", "Schau\u00b7ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem Dome von Palerm!", "tokens": ["Nach", "dem", "Do\u00b7me", "von", "Pa\u00b7lerm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wei\u00dft du, Liebling, das Geheimni\u00df?", "tokens": ["Wei\u00dft", "du", ",", "Lieb\u00b7ling", ",", "das", "Ge\u00b7heim\u00b7ni\u00df", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese Nacht in einer S\u00e4nfte", "tokens": ["Die\u00b7se", "Nacht", "in", "ei\u00b7ner", "S\u00e4nf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tragen meine Sarazenen", "tokens": ["Tra\u00b7gen", "mei\u00b7ne", "Sa\u00b7ra\u00b7ze\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sacht mich an den Strand des Meers.", "tokens": ["Sacht", "mich", "an", "den", "Strand", "des", "Meers", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Meiner harrt ein schwellend Segel.", "tokens": ["Mei\u00b7ner", "harrt", "ein", "schwel\u00b7lend", "Se\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf des Schiffes Deck gelagert,", "tokens": ["Auf", "des", "Schif\u00b7fes", "Deck", "ge\u00b7la\u00b7gert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fahr' entgegen ich dem Morgen", "tokens": ["Fahr'", "ent\u00b7ge\u00b7gen", "ich", "dem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem neugebornen Strahl.", "tokens": ["Und", "dem", "neu\u00b7ge\u00b7bor\u00b7nen", "Strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Fern auf einem Vorgebirge,", "tokens": ["Fern", "auf", "ei\u00b7nem", "Vor\u00b7ge\u00b7bir\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das in blaue Flut hinausragt,", "tokens": ["Das", "in", "blau\u00b7e", "Flut", "hin\u00b7aus\u00b7ragt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Steht ein halb zertr\u00fcmmert Kloster", "tokens": ["Steht", "ein", "halb", "zer\u00b7tr\u00fcm\u00b7mert", "Klos\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "VVPP", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein schlanker Tempelbau.", "tokens": ["Und", "ein", "schlan\u00b7ker", "Tem\u00b7pel\u00b7bau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Zwischen Kloster und Rotunde", "tokens": ["Zwi\u00b7schen", "Klos\u00b7ter", "und", "Ro\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlagen wir das Zelt im Freien.", "tokens": ["Schla\u00b7gen", "wir", "das", "Zelt", "im", "Frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Selig athm' ich Meer und Himmel,", "tokens": ["Se\u00b7lig", "athm'", "ich", "Meer", "und", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis mich Schlummer \u00fcbermannt.\u201c", "tokens": ["Bis", "mich", "Schlum\u00b7mer", "\u00fc\u00b7berm\u00b7annt", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}