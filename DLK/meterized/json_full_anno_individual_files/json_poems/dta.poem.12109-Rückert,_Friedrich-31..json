{"dta.poem.12109": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein sch\u00f6ner Garten lag am Weg, ich stand davor;", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Gar\u00b7ten", "lag", "am", "Weg", ",", "ich", "stand", "da\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Mauer war zu hoch, und eng das Gitterthor.", "tokens": ["Die", "Mau\u00b7er", "war", "zu", "hoch", ",", "und", "eng", "das", "Git\u00b7ter\u00b7thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur soviel kann ich sehn als meine Neugier reizt,", "tokens": ["Nur", "so\u00b7viel", "kann", "ich", "sehn", "als", "mei\u00b7ne", "Neu\u00b7gier", "reizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "VVINF", "KOKOM", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anstatt befriedigt. Weh dem Reichen, der so geizt!", "tokens": ["An\u00b7statt", "be\u00b7frie\u00b7digt", ".", "Weh", "dem", "Rei\u00b7chen", ",", "der", "so", "geizt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "NN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn Eintritt mit dem Blick nicht einmal den Beschauern", "tokens": ["Wenn", "Ein\u00b7tritt", "mit", "dem", "Blick", "nicht", "ein\u00b7mal", "den", "Be\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "PTKNEG", "ADV", "ART", "NN"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du g\u00f6nnest, solltest du den Garten ganz vermauern.", "tokens": ["Du", "g\u00f6n\u00b7nest", ",", "soll\u00b7test", "du", "den", "Gar\u00b7ten", "ganz", "ver\u00b7mau\u00b7ern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}