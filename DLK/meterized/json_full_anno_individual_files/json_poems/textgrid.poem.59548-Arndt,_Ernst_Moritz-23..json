{"textgrid.poem.59548": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin so traurig in dem Herzen", "tokens": ["Ich", "bin", "so", "trau\u00b7rig", "in", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wei\u00df nicht mehr, wo hin noch her,", "tokens": ["Und", "wei\u00df", "nicht", "mehr", ",", "wo", "hin", "noch", "her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$,", "PWAV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meinem Innern braust von Schmerzen", "tokens": ["In", "mei\u00b7nem", "In\u00b7nern", "braust", "von", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein weites, kaltes w\u00fcstes Meer,", "tokens": ["Ein", "wei\u00b7tes", ",", "kal\u00b7tes", "w\u00fcs\u00b7tes", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es rei\u00dft mich Sehnsucht und Verlangen", "tokens": ["Es", "rei\u00dft", "mich", "Sehn\u00b7sucht", "und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom S\u00fcd zum Nord, vom Ost zum West,", "tokens": ["Vom", "S\u00fcd", "zum", "Nord", ",", "vom", "Ost", "zum", "West", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gleich einem Menschen, der von Schlangen", "tokens": ["Gleich", "ei\u00b7nem", "Men\u00b7schen", ",", "der", "von", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im Busen tr\u00fcg' ein ganzes Nest.", "tokens": ["Im", "Bu\u00b7sen", "tr\u00fcg'", "ein", "gan\u00b7zes", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin so traurig in dem Sinne,", "tokens": ["Ich", "bin", "so", "trau\u00b7rig", "in", "dem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sonst so still und freundlich war,", "tokens": ["Der", "sonst", "so", "still", "und", "freund\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So voll von Gottes s\u00fc\u00dfer Minne,", "tokens": ["So", "voll", "von", "Got\u00b7tes", "s\u00fc\u00b7\u00dfer", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Gottes Licht so hell und klar;", "tokens": ["Von", "Got\u00b7tes", "Licht", "so", "hell", "und", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bei Menschen f\u00fchl' ich mich verlassen,", "tokens": ["Bei", "Men\u00b7schen", "f\u00fchl'", "ich", "mich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und einsam fa\u00dft mich schlimme Not,", "tokens": ["Und", "ein\u00b7sam", "fa\u00dft", "mich", "schlim\u00b7me", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich kann mich selber nicht mehr fassen", "tokens": ["Ich", "kann", "mich", "sel\u00b7ber", "nicht", "mehr", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und w\u00fcnsche oft: O w\u00e4rst du tot!", "tokens": ["Und", "w\u00fcn\u00b7sche", "oft", ":", "O", "w\u00e4rst", "du", "tot", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$.", "NE", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn ach! mein Gott hat mich verlassen,", "tokens": ["Denn", "ach", "!", "mein", "Gott", "hat", "mich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ich zuerst mich selbst verlie\u00df", "tokens": ["Weil", "ich", "zu\u00b7erst", "mich", "selbst", "ver\u00b7lie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf des Lebens breite Stra\u00dfen", "tokens": ["Und", "auf", "des", "Le\u00b7bens", "brei\u00b7te", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich t\u00f6richt gnug verlocken lie\u00df.", "tokens": ["Mich", "t\u00f6\u00b7richt", "gnug", "ver\u00b7lo\u00b7cken", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im bunten, gaukelnden Gebrause,", "tokens": ["Im", "bun\u00b7ten", ",", "gau\u00b7keln\u00b7den", "Ge\u00b7brau\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo floh es hin, mein altes Gl\u00fcck?", "tokens": ["Wo", "floh", "es", "hin", ",", "mein", "al\u00b7tes", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie find' ich zu der stillen Klause", "tokens": ["Wie", "find'", "ich", "zu", "der", "stil\u00b7len", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Kinderunschuld mich zur\u00fcck?", "tokens": ["Der", "Kin\u00b7de\u00b7run\u00b7schuld", "mich", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O du, der in das Land der N\u00e4chte", "tokens": ["O", "du", ",", "der", "in", "das", "Land", "der", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebe selbst herabgesandt,", "tokens": ["Die", "Lie\u00b7be", "selbst", "her\u00b7ab\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sie uns allen Gnade br\u00e4chte", "tokens": ["Da\u00df", "sie", "uns", "al\u00b7len", "Gna\u00b7de", "br\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Heilung mit der milden Hand,", "tokens": ["Und", "Hei\u00b7lung", "mit", "der", "mil\u00b7den", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der sie ans harte Kreuz geschlagen,", "tokens": ["Der", "sie", "ans", "har\u00b7te", "Kreuz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Dornen blutig sie zerri\u00df,", "tokens": ["Mit", "Dor\u00b7nen", "blu\u00b7tig", "sie", "zer\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df wir in S\u00fcnden nicht verzagen,", "tokens": ["Da\u00df", "wir", "in", "S\u00fcn\u00b7den", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der unersch\u00f6pften Huld gewi\u00df.", "tokens": ["Der", "un\u00b7er\u00b7sch\u00f6pf\u00b7ten", "Huld", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du tr\u00f6ste, was den Trost verloren,", "tokens": ["Du", "tr\u00f6s\u00b7te", ",", "was", "den", "Trost", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du richte das Gefallne auf,", "tokens": ["Du", "rich\u00b7te", "das", "Ge\u00b7fall\u00b7ne", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und zu den steilen Himmelstoren", "tokens": ["Und", "zu", "den", "stei\u00b7len", "Him\u00b7mels\u00b7to\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gib Mut und Licht dem Pilgerlauf.", "tokens": ["Gib", "Mut", "und", "Licht", "dem", "Pil\u00b7ger\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist die G\u00fcte, du die Treue,", "tokens": ["Du", "bist", "die", "G\u00fc\u00b7te", ",", "du", "die", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin der Staub, ich bin das Nichts,", "tokens": ["Ich", "bin", "der", "Staub", ",", "ich", "bin", "das", "Nichts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das sehnend lechzt zur heitern Bl\u00e4ue", "tokens": ["Das", "seh\u00b7nend", "lechzt", "zur", "hei\u00b7tern", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des reinen Gl\u00fccks, des reinen Lichts.", "tokens": ["Des", "rei\u00b7nen", "Gl\u00fccks", ",", "des", "rei\u00b7nen", "Lichts", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich bin so traurig in dem Herzen", "tokens": ["Ich", "bin", "so", "trau\u00b7rig", "in", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text