{"dta.poem.926": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "DrEI BLICKE IN EINEN OPAL", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Blick in Opal: ein Dorf umkr\u00e4nzt von d\u00fcrrem Wein,", "tokens": ["Blick", "in", "O\u00b7pal", ":", "ein", "Dorf", "um\u00b7kr\u00e4nzt", "von", "d\u00fcr\u00b7rem", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der Stille grauer Wolken, gelber Felsenh\u00fcgel", "tokens": ["Der", "Stil\u00b7le", "grau\u00b7er", "Wol\u00b7ken", ",", "gel\u00b7ber", "Fel\u00b7sen\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und abendlicher Quellen K\u00fchle: Zwillingsspiegel", "tokens": ["Und", "a\u00b7bend\u00b7li\u00b7cher", "Quel\u00b7len", "K\u00fch\u00b7le", ":", "Zwil\u00b7lings\u00b7spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJA", "NN", "NN", "$.", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Umrahmt von Schatten und von schleimigem Gestein.", "tokens": ["Um\u00b7rahmt", "von", "Schat\u00b7ten", "und", "von", "schlei\u00b7mi\u00b7gem", "Ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Herbstes Weg und Kreuze gehn in Abend ein,", "tokens": ["Des", "Herbs\u00b7tes", "Weg", "und", "Kreu\u00b7ze", "gehn", "in", "A\u00b7bend", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Singende Pilger und die blutbefleckten Linnen.", "tokens": ["Sin\u00b7gen\u00b7de", "Pil\u00b7ger", "und", "die", "blut\u00b7be\u00b7fleck\u00b7ten", "Lin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Des Einsamen Gestalt kehrt also sich nach innen", "tokens": ["Des", "Ein\u00b7sa\u00b7men", "Ge\u00b7stalt", "kehrt", "al\u00b7so", "sich", "nach", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PRF", "APPR", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und geht, ein bleicher Engel, durch den leeren Hain.", "tokens": ["Und", "geht", ",", "ein", "blei\u00b7cher", "En\u00b7gel", ",", "durch", "den", "lee\u00b7ren", "Hain", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aus Schwarzem bl\u00e4st der F\u00f6hn. Mit Satyrn im Verein", "tokens": ["Aus", "Schwar\u00b7zem", "bl\u00e4st", "der", "F\u00f6hn", ".", "Mit", "Sa\u00b7tyrn", "im", "Ver\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$.", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind schlanke Weiblein; M\u00f6nche der Wollust bleiche Priester,", "tokens": ["Sind", "schlan\u00b7ke", "Weib\u00b7lein", ";", "M\u00f6n\u00b7che", "der", "Wol\u00b7lust", "blei\u00b7che", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$.", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ihr Wahnsinn schm\u00fcckt mit Lilien sich sch\u00f6n und d\u00fcster", "tokens": ["Ihr", "Wahn\u00b7sinn", "schm\u00fcckt", "mit", "Li\u00b7li\u00b7en", "sich", "sch\u00f6n", "und", "d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "PRF", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hebt die H\u00e4nde auf zu Gottes goldenem Schrein.", "tokens": ["Und", "hebt", "die", "H\u00e4n\u00b7de", "auf", "zu", "Got\u00b7tes", "gol\u00b7de\u00b7nem", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.4": {"line.1": {"text": "Der ihn befeuchtet, rosig h\u00e4ngt ein Tropfen Tau", "tokens": ["Der", "ihn", "be\u00b7feuch\u00b7tet", ",", "ro\u00b7sig", "h\u00e4ngt", "ein", "Trop\u00b7fen", "Tau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Rosmarin: hinflie\u00dft ein Hauch von Grabger\u00fcchen,", "tokens": ["Im", "Ros\u00b7ma\u00b7rin", ":", "hin\u00b7flie\u00dft", "ein", "Hauch", "von", "Grab\u00b7ge\u00b7r\u00fc\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Spit\u00e4lern, wirr erf\u00fcllt von Fieberschrein und Fl\u00fcchen.", "tokens": ["Spi\u00b7t\u00e4\u00b7lern", ",", "wirr", "er\u00b7f\u00fcllt", "von", "Fie\u00b7ber\u00b7schrein", "und", "Fl\u00fc\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gebein steigt aus dem Erbbegr\u00e4bnis morsch und grau.", "tokens": ["Ge\u00b7bein", "steigt", "aus", "dem", "Erb\u00b7be\u00b7gr\u00e4b\u00b7nis", "morsch", "und", "grau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "In blauem Schleim und Schleiern tanzt des Greisen Frau,", "tokens": ["In", "blau\u00b7em", "Schleim", "und", "Schlei\u00b7ern", "tanzt", "des", "Grei\u00b7sen", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das schmutzstarrende Haar erf\u00fcllt von schwarzen Tr\u00e4nen,", "tokens": ["Das", "schmutz\u00b7star\u00b7ren\u00b7de", "Haar", "er\u00b7f\u00fcllt", "von", "schwar\u00b7zen", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Knaben tr\u00e4umen wirr in d\u00fcrren Weidenstr\u00e4hnen", "tokens": ["Die", "Kna\u00b7ben", "tr\u00e4u\u00b7men", "wirr", "in", "d\u00fcr\u00b7ren", "Wei\u00b7den\u00b7str\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihre Stirnen sind von Aussatz kahl und rauh.", "tokens": ["Und", "ih\u00b7re", "Stir\u00b7nen", "sind", "von", "Aus\u00b7satz", "kahl", "und", "rauh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Durchs Bogenfenster sinkt ein Abend lind und lau.", "tokens": ["Durchs", "Bo\u00b7gen\u00b7fens\u00b7ter", "sinkt", "ein", "A\u00b7bend", "lind", "und", "lau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Heiliger tritt aus seinen schwarzen Wundenmalen.", "tokens": ["Ein", "Hei\u00b7li\u00b7ger", "tritt", "aus", "sei\u00b7nen", "schwar\u00b7zen", "Wun\u00b7den\u00b7ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Purpurschnecken kriechen aus zerbrochenen Schalen", "tokens": ["Die", "Pur\u00b7pur\u00b7schne\u00b7cken", "krie\u00b7chen", "aus", "zer\u00b7bro\u00b7che\u00b7nen", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und speien Blut in Dorngewinde starr und grau.", "tokens": ["Und", "spei\u00b7en", "Blut", "in", "Dorn\u00b7ge\u00b7win\u00b7de", "starr", "und", "grau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Blinden streuen in eiternde Wunden Weiherauch.", "tokens": ["Die", "Blin\u00b7den", "streu\u00b7en", "in", "ei\u00b7tern\u00b7de", "Wun\u00b7den", "Wei\u00b7her\u00b7auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Rotgoldene Gew\u00e4nder; Fackeln; Psalmensingen;", "tokens": ["Rot\u00b7gol\u00b7de\u00b7ne", "Ge\u00b7w\u00e4n\u00b7der", ";", "Fa\u00b7ckeln", ";", "Psal\u00b7men\u00b7sin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "$.", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und M\u00e4dchen, die wie Gift den Leib des Herrn umschlingen.", "tokens": ["Und", "M\u00e4d\u00b7chen", ",", "die", "wie", "Gift", "den", "Leib", "des", "Herrn", "um\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "KOKOM", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gestalten schreiten w\u00e4chsernstarr durch Glut und Rauch.", "tokens": ["Ge\u00b7stal\u00b7ten", "schrei\u00b7ten", "w\u00e4ch\u00b7serns\u00b7tarr", "durch", "Glut", "und", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Auss\u00e4tziger mittern\u00e4chtigen Tanz f\u00fchrt an ein Gauch", "tokens": ["Aus\u00b7s\u00e4t\u00b7zi\u00b7ger", "mit\u00b7ter\u00b7n\u00e4ch\u00b7ti\u00b7gen", "Tanz", "f\u00fchrt", "an", "ein", "Gauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "D\u00fcrrkn\u00f6chern. Garten wunderlicher Abenteuer;", "tokens": ["D\u00fcrr\u00b7kn\u00f6\u00b7chern", ".", "Gar\u00b7ten", "wun\u00b7der\u00b7li\u00b7cher", "A\u00b7bent\u00b7eu\u00b7er", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verzerrtes; Blumenfratzen, Lachen; Ungeheuer", "tokens": ["Ver\u00b7zerr\u00b7tes", ";", "Blu\u00b7men\u00b7frat\u00b7zen", ",", "La\u00b7chen", ";", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$.", "NN", "$,", "NN", "$.", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und rollendes Gestirn im schwarzen Dornenstrauch.", "tokens": ["Und", "rol\u00b7len\u00b7des", "Ge\u00b7stirn", "im", "schwar\u00b7zen", "Dor\u00b7nen\u00b7strauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "O Armut, Bettelsuppe, Brot und s\u00fc\u00dfer Lauch;", "tokens": ["O", "Ar\u00b7mut", ",", "Bet\u00b7tel\u00b7sup\u00b7pe", ",", "Brot", "und", "s\u00fc\u00b7\u00dfer", "Lauch", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Lebens Tr\u00e4umerei in H\u00fctten vor den W\u00e4ldern.", "tokens": ["Des", "Le\u00b7bens", "Tr\u00e4u\u00b7me\u00b7rei", "in", "H\u00fct\u00b7ten", "vor", "den", "W\u00e4l\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Grau h\u00e4rtet sich der Himmel \u00fcber gelben Feldern", "tokens": ["Grau", "h\u00e4r\u00b7tet", "sich", "der", "Him\u00b7mel", "\u00fc\u00b7ber", "gel\u00b7ben", "Fel\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und eine Abendglocke singt nach altem Brauch.", "tokens": ["Und", "ei\u00b7ne", "A\u00b7bend\u00b7glo\u00b7cke", "singt", "nach", "al\u00b7tem", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}