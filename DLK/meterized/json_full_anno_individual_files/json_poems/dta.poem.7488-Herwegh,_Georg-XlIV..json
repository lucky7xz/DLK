{"dta.poem.7488": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XlIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erreichbar nur dem Sturm und Sonnenbrand,", "tokens": ["Er\u00b7reich\u00b7bar", "nur", "dem", "Sturm", "und", "Son\u00b7nen\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von keines Wandrers Fu\u00dfe umgebogen,", "tokens": ["Von", "kei\u00b7nes", "Wand\u00b7rers", "Fu\u00b7\u00dfe", "um\u00b7ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In scheuen Kreisen nur vom Aar umflogen,", "tokens": ["In", "scheu\u00b7en", "Krei\u00b7sen", "nur", "vom", "Aar", "um\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie ein Johannes in der W\u00fcste, stand", "tokens": ["Wie", "ein", "Jo\u00b7han\u00b7nes", "in", "der", "W\u00fcs\u00b7te", ",", "stand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NE", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Bl\u00fcmchen einst auf kahler Alpenwand;", "tokens": ["Ein", "Bl\u00fcm\u00b7chen", "einst", "auf", "kah\u00b7ler", "Al\u00b7pen\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Himmel hatte, doppelt ihm gewogen,", "tokens": ["Der", "Him\u00b7mel", "hat\u00b7te", ",", "dop\u00b7pelt", "ihm", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es seinem Herzen n\u00e4her auferzogen,", "tokens": ["Es", "sei\u00b7nem", "Her\u00b7zen", "n\u00e4\u00b7her", "auf\u00b7er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch nur mit Klagen schaut' es in das Land.", "tokens": ["Doch", "nur", "mit", "Kla\u00b7gen", "schaut'", "es", "in", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u201ewarum, o Gott, in eines Felsen Schoos?", "tokens": ["\u201e", "wa\u00b7rum", ",", "o", "Gott", ",", "in", "ei\u00b7nes", "Fel\u00b7sen", "Schoos", "?"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "FM", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Warum, o Gott, mir solch ein einsam Loos?", "tokens": ["Wa\u00b7rum", ",", "o", "Gott", ",", "mir", "solch", "ein", "ein\u00b7sam", "Loos", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "FM", "NN", "$,", "PPER", "PIAT", "ART", "ADJD", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was sterb' ich nicht in holder Schwestern Mitten?\u201d", "tokens": ["Was", "sterb'", "ich", "nicht", "in", "hol\u00b7der", "Schwes\u00b7tern", "Mit\u00b7ten", "?", "\""], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Still, meine Blume, still! Was klagst Du noch?", "tokens": ["Still", ",", "mei\u00b7ne", "Blu\u00b7me", ",", "still", "!", "Was", "klagst", "Du", "noch", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "$,", "ADJD", "$.", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wohl bist Du einsam, aber sicher doch", "tokens": ["Wohl", "bist", "Du", "ein\u00b7sam", ",", "a\u00b7ber", "si\u00b7cher", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vor Menschenh\u00e4nden und vor Menschentritten.", "tokens": ["Vor", "Men\u00b7schen\u00b7h\u00e4n\u00b7den", "und", "vor", "Men\u00b7schen\u00b7trit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}