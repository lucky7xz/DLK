{"textgrid.poem.50896": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "16", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "'ne Schale Feuerwein ist gut, wenn man sich schlagen soll!", "tokens": ["'ne", "Scha\u00b7le", "Feu\u00b7er\u00b7wein", "ist", "gut", ",", "wenn", "man", "sich", "schla\u00b7gen", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,", "KOUS", "PIS", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und mehr noch, wenn das Leben man zu Markte tragen soll,", "tokens": ["Und", "mehr", "noch", ",", "wenn", "das", "Le\u00b7ben", "man", "zu", "Mark\u00b7te", "tra\u00b7gen", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "KOUS", "ART", "NN", "PIS", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ist eine \u00dcberzeugung wert: wof\u00fcr man steh und falle,", "tokens": ["Ist", "ei\u00b7ne", "\u00dc\u00b7berz\u00b7eu\u00b7gung", "wert", ":", "wo\u00b7f\u00fcr", "man", "steh", "und", "fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "$.", "PWAV", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Woran das junge Leben man, das sch\u00f6ne, wagen soll?", "tokens": ["Wo\u00b7ran", "das", "jun\u00b7ge", "Le\u00b7ben", "man", ",", "das", "sch\u00f6\u00b7ne", ",", "wa\u00b7gen", "soll", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PIS", "$,", "ART", "ADJA", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Doch mehr als Wein und Hochgef\u00fchl beim hellsten Zinkenschalle", "tokens": ["Doch", "mehr", "als", "Wein", "und", "Hoch\u00b7ge\u00b7f\u00fchl", "beim", "hells\u00b7ten", "Zin\u00b7ken\u00b7schal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "NN", "KON", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Begeistert mich, so ich der Lust der Welt entsagen soll,", "tokens": ["Be\u00b7geis\u00b7tert", "mich", ",", "so", "ich", "der", "Lust", "der", "Welt", "ent\u00b7sa\u00b7gen", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "PPER", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Wenn auf dem Mund ein Weibesku\u00df noch brennt! Ich frag euch alle:", "tokens": ["Wenn", "auf", "dem", "Mund", "ein", "Wei\u00b7bes\u00b7ku\u00df", "noch", "brennt", "!", "Ich", "frag", "euch", "al\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "O saget mir, nach ", "tokens": ["O", "sa\u00b7get", "mir", ",", "nach"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "'ne Schale Feuerwein ist gut, wenn man sich schlage