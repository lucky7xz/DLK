{"textgrid.poem.41831": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So la\u00df mir meine Tr\u00e4ume, meine Lieder!", "tokens": ["So", "la\u00df", "mir", "mei\u00b7ne", "Tr\u00e4u\u00b7me", ",", "mei\u00b7ne", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Gottheit selber hab' ich so erhoben", "tokens": ["Zur", "Got\u00b7theit", "sel\u00b7ber", "hab'", "ich", "so", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein hei\u00df Gebet im br\u00fcnstigen Geloben:", "tokens": ["Mein", "hei\u00df", "Ge\u00b7bet", "im", "br\u00fcns\u00b7ti\u00b7gen", "Ge\u00b7lo\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was du mir gabst, das lasse ich nicht wieder!", "tokens": ["Was", "du", "mir", "gabst", ",", "das", "las\u00b7se", "ich", "nicht", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So tr\u00e4umt ich denn, es fielen Rosen nieder,", "tokens": ["So", "tr\u00e4umt", "ich", "denn", ",", "es", "fie\u00b7len", "Ro\u00b7sen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sich zu einer holden Kette woben \u2013", "tokens": ["Die", "sich", "zu", "ei\u00b7ner", "hol\u00b7den", "Ket\u00b7te", "wo\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und darauf schien ein Sonnenglanz von oben", "tokens": ["Und", "da\u00b7rauf", "schien", "ein", "Son\u00b7nen\u00b7glanz", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und aus den Rosen wurden Sternenglieder.", "tokens": ["Und", "aus", "den", "Ro\u00b7sen", "wur\u00b7den", "Ster\u00b7nen\u00b7glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das war der Liebe g\u00f6ttlich s\u00fc\u00dfes Tr\u00e4umen", "tokens": ["Das", "war", "der", "Lie\u00b7be", "g\u00f6tt\u00b7lich", "s\u00fc\u00b7\u00dfes", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit seinen Wonnen, seinem Gl\u00fchen, Sehnen,", "tokens": ["Mit", "sei\u00b7nen", "Won\u00b7nen", ",", "sei\u00b7nem", "Gl\u00fc\u00b7hen", ",", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das bis hinauf drang zu des Himmels R\u00e4umen.", "tokens": ["Das", "bis", "hin\u00b7auf", "drang", "zu", "des", "Him\u00b7mels", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Aether dort ergl\u00e4nzten heil'ge Thr\u00e4nen,", "tokens": ["Im", "A\u00b7e\u00b7ther", "dort", "er\u00b7gl\u00e4nz\u00b7ten", "heil'\u00b7ge", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie Tauesperlen oft die Rasen s\u00e4umen,", "tokens": ["Wie", "Tau\u00b7es\u00b7per\u00b7len", "oft", "die", "Ra\u00b7sen", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So mochten sie sich an die Sterne lehnen.", "tokens": ["So", "moch\u00b7ten", "sie", "sich", "an", "die", "Ster\u00b7ne", "leh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So la\u00df mir meine Tr\u00e4ume, meine Lieder!", "tokens": ["So", "la\u00df", "mir", "mei\u00b7ne", "Tr\u00e4u\u00b7me", ",", "mei\u00b7ne", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "