{"textgrid.poem.33578": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus l\u00e4ngst vergessnen Augen seh' ich flie\u00dfen", "tokens": ["Aus", "l\u00e4ngst", "ver\u00b7gess\u00b7nen", "Au\u00b7gen", "seh'", "ich", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Viel stillgeweinte Tr\u00e4nen, schwermutreich,", "tokens": ["Viel", "still\u00b7ge\u00b7wein\u00b7te", "Tr\u00e4\u00b7nen", ",", "schwer\u00b7mut\u00b7reich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus fernen Zeiten seh' ich mich begr\u00fc\u00dfen", "tokens": ["Aus", "fer\u00b7nen", "Zei\u00b7ten", "seh'", "ich", "mich", "be\u00b7gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von scheuen Blicken, Wangen schmerzhaft bleich.", "tokens": ["Von", "scheu\u00b7en", "Bli\u00b7cken", ",", "Wan\u00b7gen", "schmerz\u00b7haft", "bleich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wagst du denn nicht, zu mir emporzuschauen", "tokens": ["Wagst", "du", "denn", "nicht", ",", "zu", "mir", "em\u00b7por\u00b7zu\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", 