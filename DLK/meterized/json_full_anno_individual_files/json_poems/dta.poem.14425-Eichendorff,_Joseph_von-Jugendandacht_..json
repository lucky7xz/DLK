{"dta.poem.14425": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da war's, als ob sich still Dein Auge h\u00fcbe,", "tokens": ["Da", "wa\u00b7r's", ",", "als", "ob", "sich", "still", "Dein", "Au\u00b7ge", "h\u00fc\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "PRF", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Lang'st sehnsuchtsvoll nach mir mit off'nen Armen,", "tokens": ["Lang'st", "sehn\u00b7suchts\u00b7voll", "nach", "mir", "mit", "off'\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fchlst selbst den Schmerz, den Du mir s\u00fc\u00df gegeben. \u2014", "tokens": ["F\u00fchlst", "selbst", "den", "Schmerz", ",", "den", "Du", "mir", "s\u00fc\u00df", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}