{"textgrid.poem.46281": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von dem capitain G. Bochern", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des hauptman Bochers stolz und pracht,", "tokens": ["Des", "haupt\u00b7man", "Bo\u00b7chers", "stolz", "und", "pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "gesehen vil und weit geh\u00f6ret,", "tokens": ["ge\u00b7se\u00b7hen", "vil", "und", "weit", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wird nicht durch seiner wafen macht,", "tokens": ["wird", "nicht", "durch", "sei\u00b7ner", "wa\u00b7fen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sondern durch seine f\u00fc\u00df vermehret.", "tokens": ["son\u00b7dern", "durch", "sei\u00b7ne", "f\u00fc\u00df", "ver\u00b7meh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Dan kein ring zieret seine hand,", "tokens": ["Dan", "kein", "ring", "zie\u00b7ret", "sei\u00b7ne", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "da mit gold seine sporn gemalet,", "tokens": ["da", "mit", "gold", "sei\u00b7ne", "sporn", "ge\u00b7ma\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "zwar durch den sporn wird seine schand,", "tokens": ["zwar", "durch", "den", "sporn", "wird", "sei\u00b7ne", "schand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "schand? nein, sein fersengeld bezahlet.", "tokens": ["schand", "?", "nein", ",", "sein", "fer\u00b7sen\u00b7geld", "be\u00b7zah\u00b7let", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PTKANT", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des hauptman Bochers stolz und pracht,", "tokens": ["Des", "haupt\u00b7man", "Bo\u00b7chers", "stolz", "und", "pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "gesehen vil und weit geh\u00f6ret,", "tokens": ["ge\u00b7se\u00b7hen", "vil", "und", "weit", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wird nicht durch seiner wafen macht,", "tokens": ["wird", "nicht", "durch", "sei\u00b7ner", "wa\u00b7fen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sondern d