{"textgrid.poem.36113": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "10. Als Leipzig nicht daheime war", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Vnd suchte ", "tokens": ["Vnd", "such\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Vnd mit dem ganzen Schmuck vnd Geiz- vnd Hoffartgeist", "tokens": ["Vnd", "mit", "dem", "gan\u00b7zen", "Schmuck", "vnd", "Gei\u00b7z", "vnd", "Hof\u00b7fart\u00b7geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "KON", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vnd was man sonsten sagt, das Leipzig sei, verreist.", "tokens": ["Vnd", "was", "man", "sons\u00b7ten", "sagt", ",", "das", "Leip\u00b7zig", "sei", ",", "ver\u00b7reist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADV", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df thete ", "tokens": ["Di\u00df", "the\u00b7te"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Da\u00df er an ", "tokens": ["Da\u00df", "er", "an"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Doch war es gut vor vns. Denn w\u00e4r ein solches Nest,", "tokens": ["Doch", "war", "es", "gut", "vor", "vns", ".", "Denn", "w\u00e4r", "ein", "sol\u00b7ches", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "$.", "KON", "VAFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie dieser Vogel sucht, in dieser Stadt gewest,", "tokens": ["Wie", "die\u00b7ser", "Vo\u00b7gel", "sucht", ",", "in", "die\u00b7ser", "Stadt", "ge\u00b7west", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VVFIN", "$,", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wir w\u00e4ren nimmermehr so gn\u00e4dig durchgekommen.", "tokens": ["Wir", "w\u00e4\u00b7ren", "nim\u00b7mer\u00b7mehr", "so", "gn\u00e4\u00b7dig", "durch\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So ware zwar Gefahr, doch schonte Gott der Frommen,", "tokens": ["So", "wa\u00b7re", "zwar", "Ge\u00b7fahr", ",", "doch", "schon\u00b7te", "Gott", "der", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$,", "ADV", "VVFIN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die hier verblieben warn, vnd nahm sich ihrer an,", "tokens": ["Die", "hier", "ver\u00b7blie\u00b7ben", "warn", ",", "vnd", "nahm", "sich", "ih\u00b7rer", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "$,", "KON", "VVFI