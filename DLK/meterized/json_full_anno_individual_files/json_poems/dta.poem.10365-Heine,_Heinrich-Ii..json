{"dta.poem.10365": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Gieb her die Larv', ich will mich jetzt maskieren", "tokens": ["Gieb", "her", "die", "La\u00b7rv'", ",", "ich", "will", "mich", "jetzt", "mas\u00b7kie\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "NN", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In einen Lumpenkerl, damit Halunken,", "tokens": ["In", "ei\u00b7nen", "Lum\u00b7pen\u00b7kerl", ",", "da\u00b7mit", "Ha\u00b7lun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die pr\u00e4chtig in Charaktermasken prunken,", "tokens": ["Die", "pr\u00e4ch\u00b7tig", "in", "Cha\u00b7rak\u00b7ter\u00b7mas\u00b7ken", "prun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht w\u00e4hnen Ich sey einer von den Ihren.", "tokens": ["Nicht", "w\u00e4h\u00b7nen", "Ich", "sey", "ei\u00b7ner", "von", "den", "Ih\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "VAFIN", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gieb her gemeine Worte und Manieren,", "tokens": ["Gieb", "her", "ge\u00b7mei\u00b7ne", "Wor\u00b7te", "und", "Ma\u00b7nie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich zeige mich in P\u00f6belart versunken,", "tokens": ["Ich", "zei\u00b7ge", "mich", "in", "P\u00f6\u00b7be\u00b7lart", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verl\u00e4ugne all die sch\u00f6nen Geistesfunken,", "tokens": ["Ver\u00b7l\u00e4ug\u00b7ne", "all", "die", "sch\u00f6\u00b7nen", "Geis\u00b7tes\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Womit jetzt fade Schlingel kokettiren.", "tokens": ["Wo\u00b7mit", "jetzt", "fa\u00b7de", "Schlin\u00b7gel", "ko\u00b7ket\u00b7ti\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So tanz' ich auf dem gro\u00dfen Maskenballe,", "tokens": ["So", "tanz'", "ich", "auf", "dem", "gro\u00b7\u00dfen", "Mas\u00b7ken\u00b7bal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Umschw\u00e4rmt von deutschen Rittern, M\u00f6nchen, K\u00f6n'gen,", "tokens": ["Um\u00b7schw\u00e4rmt", "von", "deut\u00b7schen", "Rit\u00b7tern", ",", "M\u00f6n\u00b7chen", ",", "K\u00f6n'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Von Harlekin gegr\u00fc\u00dft, erkannt von wen'gen.", "tokens": ["Von", "Har\u00b7le\u00b7kin", "ge\u00b7gr\u00fc\u00dft", ",", "er\u00b7kannt", "von", "wen'\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$,", "VVPP", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mit ihrem Holzschwert pr\u00fcgeln sie mich alle.", "tokens": ["Mit", "ih\u00b7rem", "Holz\u00b7schwert", "pr\u00fc\u00b7geln", "sie", "mich", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das ist der Spa\u00df. Denn wollt' ich mich entmummen,", "tokens": ["Das", "ist", "der", "Spa\u00df", ".", "Denn", "wollt'", "ich", "mich", "ent\u00b7mum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "KON", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "So m\u00fc\u00dfte all das Galgenpack verstummen.", "tokens": ["So", "m\u00fc\u00df\u00b7te", "all", "das", "Gal\u00b7gen\u00b7pack", "ver\u00b7stum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}