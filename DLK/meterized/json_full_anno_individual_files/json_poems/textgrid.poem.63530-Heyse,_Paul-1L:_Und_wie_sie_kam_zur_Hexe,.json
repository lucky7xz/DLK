{"textgrid.poem.63530": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und wie sie kam zur Hexe,", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wie sie kam zur Hexe,", "tokens": ["Und", "wie", "sie", "kam", "zur", "He\u00b7xe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Dornr\u00f6schen hold, Dornr\u00f6schen gut,", "tokens": ["Dorn\u00b7r\u00f6\u00b7schen", "hold", ",", "Dorn\u00b7r\u00f6\u00b7schen", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die stach sie in ihr Fingerlein,", "tokens": ["Die", "stach", "sie", "in", "ihr", "Fin\u00b7ger\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da flo\u00df das rote Blut.", "tokens": ["Da", "flo\u00df", "das", "ro\u00b7te", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie schlo\u00df die lichten Augen,", "tokens": ["Sie", "schlo\u00df", "die", "lich\u00b7ten", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Spindelstich das M\u00e4gdlein schlief,", "tokens": ["Vom", "Spin\u00b7del\u00b7stich", "das", "M\u00e4gd\u00b7lein", "schlief", ",