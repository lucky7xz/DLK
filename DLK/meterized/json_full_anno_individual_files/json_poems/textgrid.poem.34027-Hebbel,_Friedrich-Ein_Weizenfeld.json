{"textgrid.poem.34027": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein Weizenfeld", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil es die Aehre verschm\u00e4ht, sich mit der Farbe zu zieren,", "tokens": ["Weil", "es", "die", "A\u00b7eh\u00b7re", "ver\u00b7schm\u00e4ht", ",", "sich", "mit", "der", "Far\u00b7be", "zu", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "----+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hat die Natur ihr den Mohn dicht an die Seite gestellt;", "tokens": ["Hat", "die", "Na\u00b7tur", "ihr", "den", "Mohn", "dicht", "an", "die", "Sei\u00b7te", "ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jener hat sie die Kraft vertraut, den Menschen zu n\u00e4hren,", "tokens": ["Je\u00b7ner", "hat", "sie", "die", "Kraft", "ver\u00b7traut", ",", "den", "Men\u00b7schen", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Diesem verlieh sie den Reiz, welcher sein Auge erfreut.", "tokens": ["Die\u00b7sem", "ver\u00b7lieh", "sie", "den", "Reiz", ",", "wel\u00b7cher", "sein", "Au\u00b7ge", "er\u00b7freut", "."], "token_info":