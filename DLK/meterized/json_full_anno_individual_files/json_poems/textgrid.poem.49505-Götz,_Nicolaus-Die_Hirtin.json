{"textgrid.poem.49505": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Hirtin", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Himmel! was ich nicht gelitten!", "tokens": ["Him\u00b7mel", "!", "was", "ich", "nicht", "ge\u00b7lit\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bi\u00df in unsres Waldes Mitten;", "tokens": ["Bi\u00df", "in", "uns\u00b7res", "Wal\u00b7des", "Mit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bi\u00df zu den geheimsten Buchen", "tokens": ["Bi\u00df", "zu", "den", "ge\u00b7heims\u00b7ten", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drang mein Hirte, mich zu suchen.", "tokens": ["Drang", "mein", "Hir\u00b7te", ",", "mich", "zu", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freylich, wie ich selber finde,", "tokens": ["Frey\u00b7lich", ",", "wie", "ich", "sel\u00b7ber", "fin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hatt' er ungemeine Gr\u00fcnde,", "tokens": ["Hatt'", "er", "un\u00b7ge\u00b7mei\u00b7ne", "Gr\u00fcn\u0