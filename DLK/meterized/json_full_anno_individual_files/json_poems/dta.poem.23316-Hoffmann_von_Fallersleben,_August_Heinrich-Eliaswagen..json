{"dta.poem.23316": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Eliaswagen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll es erst die Nachwelt sagen,", "tokens": ["Soll", "es", "erst", "die", "Nach\u00b7welt", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die Mitwelt hat gedacht?", "tokens": ["Was", "die", "Mit\u00b7welt", "hat", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll kein Herz zu sagen wagen", "tokens": ["Soll", "kein", "Herz", "zu", "sa\u00b7gen", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN", "PTKZU", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ihm Leid und Freude macht?", "tokens": ["Was", "ihm", "Leid", "und", "Freu\u00b7de", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nein, ihr wagt nicht mal zu sagen", "tokens": ["Nein", ",", "ihr", "wagt", "nicht", "mal", "zu", "sa\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ihr habt's doch oft gedacht:", "tokens": ["Und", "ihr", "habt's", "doch", "oft", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df das f\u00fcnfte Rad am Wagen", "tokens": ["Da\u00df", "das", "f\u00fcnf\u00b7te", "Rad", "am", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist Europas f\u00fcnfte Macht.", "tokens": ["Ist", "Eu\u00b7ro\u00b7pas", "f\u00fcnf\u00b7te", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fcnftes Rad, f\u00fcrwahr, du solltest", "tokens": ["F\u00fcnf\u00b7tes", "Rad", ",", "f\u00fcr\u00b7wahr", ",", "du", "soll\u00b7test"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "$,", "PPER", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein Eliaswagen sein!", "tokens": ["Ein", "E\u00b7li\u00b7as\u00b7wa\u00b7gen", "sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "F\u00fcnfte Macht, wenn du es wolltest \u2014", "tokens": ["F\u00fcnf\u00b7te", "Macht", ",", "wenn", "du", "es", "woll\u00b7test"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "VMFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Europa w\u00e4re dein!", "tokens": ["Und", "Eu\u00b7ro\u00b7pa", "w\u00e4\u00b7re", "dein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was ich weiter k\u00f6nnte sagen,", "tokens": ["Was", "ich", "wei\u00b7ter", "k\u00f6nn\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darauf la\u00df ich mich nicht ein;", "tokens": ["Da\u00b7rauf", "la\u00df", "ich", "mich", "nicht", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PTKNEG", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "L\u00e4\u00dft man doch in unsern Tagen", "tokens": ["L\u00e4\u00dft", "man", "doch", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur zu gern f\u00fcnf grade sein.", "tokens": ["Nur", "zu", "gern", "f\u00fcnf", "gra\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "CARD", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}