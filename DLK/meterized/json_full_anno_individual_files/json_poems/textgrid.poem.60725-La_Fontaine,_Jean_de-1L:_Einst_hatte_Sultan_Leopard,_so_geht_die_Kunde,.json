pos": ["ART", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Und sagte: \u00bbUnsre Nachbarschaft macht dir Verdru\u00df,", "tokens": ["Und", "sag\u00b7te", ":", "\u00bb", "Uns\u00b7re", "Nach\u00b7bar\u00b7schaft", "macht", "dir", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Du f\u00fcrchtest diesen L\u00f6wenknaben;", "tokens": ["Du", "f\u00fcrch\u00b7test", "die\u00b7sen", "L\u00f6\u00b7wen\u00b7kna\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Alte starb, was kann ein Kind uns tun?", "tokens": ["Der", "Al\u00b7te", "starb", ",", "was", "kann", "ein", "Kind", "uns", "tun", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Du solltest Mitleid mit dem Waisenknaben haben,", "tokens": ["Du", "soll\u00b7test", "Mit\u00b7leid", "mit", "dem", "Wai\u00b7sen\u00b7kna\u00b7ben", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denn das Regieren g\u00f6nnt ihm nicht zu ruhn.", "tokens": ["Denn", "das", "Re\u00b7gie\u00b7ren", "g\u00f6nnt", "ihm", "nicht", "zu", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Er wird dem Schicksal danken, wenn er halten kann", "tokens": ["Er", "wird", "dem", "Schick\u00b7sal", "dan\u00b7ken", ",", "wenn", "er", "hal\u00b7ten", "kann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das, was er hat; und sicher denkt er nie daran,", "tokens": ["Das", ",", "was", "er", "hat", ";", "und", "si\u00b7cher", "denkt", "er", "nie", "da\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VAFIN", "$.", "KON", "ADJD", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gar auf Eroberungen auszuziehn.\u00ab", "tokens": ["Gar", "auf", "Er\u00b7o\u00b7be\u00b7run\u00b7gen", "aus\u00b7zu\u00b7ziehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Da sprach der Fuchs, dem diese Meinung haltlos schien:", "tokens": ["Da", "sprach", "der", "Fuchs", ",", "dem", "die\u00b7se", "Mei\u00b7nung", "halt\u00b7los", "schien", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$,", "PRELS", "PDAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "\u00bbherr, solcher Waise wein ich keine Tr\u00e4nen.", "tokens": ["\u00bb", "herr", ",", "sol\u00b7cher", "Wai\u00b7se", "wein", "ich", "kei\