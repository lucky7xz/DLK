Fein\u00b7den", "al\u00b7les", "tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PDAT", "NN", "APPR", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der, den sie f\u00fcrchteten, gewann.", "tokens": ["Der", ",", "den", "sie", "f\u00fcrch\u00b7te\u00b7ten", ",", "ge\u00b7wann", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Den L\u00f6wen macht als Freund euch hold,", "tokens": ["Den", "L\u00f6\u00b7wen", "macht", "als", "Freund", "euch", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ihr ihn wachsen lassen wollt.", "tokens": ["Wenn", "ihr", "ihn", "wach\u00b7sen", "las\u00b7sen", "wollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Einst hatte Sultan Leopard, so geht die Kunde,", "tokens": ["Einst", "hat\u00b7te", "Sul\u00b7tan", "Le\u00b7o\u00b7pard", ",", "so", "geht", "die", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NE", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seinen Wiesen viele Schaf und Rinder,", "tokens": ["In", "sei\u00b7nen", "Wie\u00b7sen", "vie\u00b7le", "Schaf", "und", "Rin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word"