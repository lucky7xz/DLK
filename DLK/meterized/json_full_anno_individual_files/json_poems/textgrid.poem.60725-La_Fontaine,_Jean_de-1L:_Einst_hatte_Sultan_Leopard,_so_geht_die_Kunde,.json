{"textgrid.poem.60725": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst hatte Sultan Leopard, so geht die Kunde,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst hatte Sultan Leopard, so geht die Kunde,", "tokens": ["Einst", "hat\u00b7te", "Sul\u00b7tan", "Le\u00b7o\u00b7pard", ",", "so", "geht", "die", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NE", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seinen Wiesen viele Schaf und Rinder,", "tokens": ["In", "sei\u00b7nen", "Wie\u00b7sen", "vie\u00b7le", "Schaf", "und", "Rin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In seinen W\u00e4ldern Hirsch und Reh nicht minder \u2013", "tokens": ["In", "sei\u00b7nen", "W\u00e4l\u00b7dern", "Hirsch", "und", "Reh", "nicht", "min\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kurzum, er war der gr\u00f6\u00dfte Herr in weiter Runde.", "tokens": ["Kur\u00b7zum", ",", "er", "war", "der", "gr\u00f6\u00df\u00b7te", "Herr", "in", "wei\u00b7ter", "Run\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Im Nachbarwalde ward ein Leu geboren.", "tokens": ["Im", "Nach\u00b7bar\u00b7wal\u00b7de", "ward", "ein", "Leu", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Sultan rief den Fuchs, den alten Praktikus,", "tokens": ["Der", "Sul\u00b7tan", "rief", "den", "Fuchs", ",", "den", "al\u00b7ten", "Prak\u00b7ti\u00b7kus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den er sich zum Vezir erkoren,", "tokens": ["Den", "er", "sich", "zum", "Ve\u00b7zir", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Und sagte: \u00bbUnsre Nachbarschaft macht dir Verdru\u00df,", "tokens": ["Und", "sag\u00b7te", ":", "\u00bb", "Uns\u00b7re", "Nach\u00b7bar\u00b7schaft", "macht", "dir", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Du f\u00fcrchtest diesen L\u00f6wenknaben;", "tokens": ["Du", "f\u00fcrch\u00b7test", "die\u00b7sen", "L\u00f6\u00b7wen\u00b7kna\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Alte starb, was kann ein Kind uns tun?", "tokens": ["Der", "Al\u00b7te", "starb", ",", "was", "kann", "ein", "Kind", "uns", "tun", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWS", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Du solltest Mitleid mit dem Waisenknaben haben,", "tokens": ["Du", "soll\u00b7test", "Mit\u00b7leid", "mit", "dem", "Wai\u00b7sen\u00b7kna\u00b7ben", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denn das Regieren g\u00f6nnt ihm nicht zu ruhn.", "tokens": ["Denn", "das", "Re\u00b7gie\u00b7ren", "g\u00f6nnt", "ihm", "nicht", "zu", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Er wird dem Schicksal danken, wenn er halten kann", "tokens": ["Er", "wird", "dem", "Schick\u00b7sal", "dan\u00b7ken", ",", "wenn", "er", "hal\u00b7ten", "kann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das, was er hat; und sicher denkt er nie daran,", "tokens": ["Das", ",", "was", "er", "hat", ";", "und", "si\u00b7cher", "denkt", "er", "nie", "da\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VAFIN", "$.", "KON", "ADJD", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gar auf Eroberungen auszuziehn.\u00ab", "tokens": ["Gar", "auf", "Er\u00b7o\u00b7be\u00b7run\u00b7gen", "aus\u00b7zu\u00b7ziehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da sprach der Fuchs, dem diese Meinung haltlos schien:", "tokens": ["Da", "sprach", "der", "Fuchs", ",", "dem", "die\u00b7se", "Mei\u00b7nung", "halt\u00b7los", "schien", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$,", "PRELS", "PDAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "\u00bbherr, solcher Waise wein ich keine Tr\u00e4nen.", "tokens": ["\u00bb", "herr", ",", "sol\u00b7cher", "Wai\u00b7se", "wein", "ich", "kei\u00b7ne", "Tr\u00e4\u00b7nen", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PIAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Man mu\u00df um seine Freundschaft sich bem\u00fchn,", "tokens": ["Man", "mu\u00df", "um", "sei\u00b7ne", "Freund\u00b7schaft", "sich", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn nicht, so mache man alsbald unsch\u00e4dlich ihn,", "tokens": ["Wenn", "nicht", ",", "so", "ma\u00b7che", "man", "als\u00b7bald", "un\u00b7sch\u00e4d\u00b7lich", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "ADV", "VVFIN", "PIS", "ADV", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eh er, erwachsen, uns mit Krallen und mit Z\u00e4hnen", "tokens": ["Eh", "er", ",", "er\u00b7wach\u00b7sen", ",", "uns", "mit", "Kral\u00b7len", "und", "mit", "Z\u00e4h\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "VVPP", "$,", "PPER", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu unserm Schaden \u00fcberf\u00e4llt.", "tokens": ["Zu", "un\u00b7serm", "Scha\u00b7den", "\u00fc\u00b7berf\u00b7\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verliert nicht einen Augenblick!", "tokens": ["Ver\u00b7liert", "nicht", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich hab sein Horoskop gestellt:", "tokens": ["Ich", "hab", "sein", "Ho\u00b7ros\u00b7kop", "ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das zeigte mir sein Schlachtengl\u00fcck.", "tokens": ["Das", "zeig\u00b7te", "mir", "sein", "Schlach\u00b7ten\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der beste L\u00f6we rings auf Erden", "tokens": ["Der", "bes\u00b7te", "L\u00f6\u00b7we", "rings", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wird er f\u00fcr seine Freunde sein.", "tokens": ["Wird", "er", "f\u00fcr", "sei\u00b7ne", "Freun\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Darum versucht, sein Freund zu werden,", "tokens": ["Da\u00b7rum", "ver\u00b7sucht", ",", "sein", "Freund", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "$,", "PPOSAT", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn nicht, so schw\u00e4chet ihn, so lange er noch klein.\u00ab", "tokens": ["Wenn", "nicht", ",", "so", "schw\u00e4\u00b7chet", "ihn", ",", "so", "lan\u00b7ge", "er", "noch", "klein", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "ADV", "PPER", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umsonst die Warnung, die den Herrn zum Handeln rief.", "tokens": ["Um\u00b7sonst", "die", "War\u00b7nung", ",", "die", "den", "Herrn", "zum", "Han\u00b7deln", "rief", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Sultan schlief, und jedermann im Lande schlief \u2013", "tokens": ["Der", "Sul\u00b7tan", "schlief", ",", "und", "je\u00b7der\u00b7mann", "im", "Lan\u00b7de", "schlief", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PIS", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis schlie\u00dflich aus dem L\u00f6wenjungen", "tokens": ["Bis", "schlie\u00df\u00b7lich", "aus", "dem", "L\u00f6\u00b7wen\u00b7jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein wahrer Leu geworden war.", "tokens": ["Ein", "wah\u00b7rer", "Leu", "ge\u00b7wor\u00b7den", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt sind die Glocken rings erklungen,", "tokens": ["Jetzt", "sind", "die", "Glo\u00b7cken", "rings", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Aufruhr tobt, man sieht Gefahr.", "tokens": ["Der", "Auf\u00b7ruhr", "tobt", ",", "man", "sieht", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PIS", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Besorgt befragt man den Vezir.", "tokens": ["Be\u00b7sorgt", "be\u00b7fragt", "man", "den", "Ve\u00b7zir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der seufzt und sagt: \u00bbWas wollet ihr?", "tokens": ["Der", "seufzt", "und", "sagt", ":", "\u00bb", "Was", "wol\u00b7let", "ihr", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$.", "$(", "PWS", "VMFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht tausend Mannen helfen hier.", "tokens": ["Nicht", "tau\u00b7send", "Man\u00b7nen", "hel\u00b7fen", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "CARD", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ruft nicht nach Hilfe ringsumher,", "tokens": ["Ruft", "nicht", "nach", "Hil\u00b7fe", "rings\u00b7um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Es kostet euch nur unn\u00fctz mehr.", "tokens": ["Es", "kos\u00b7tet", "euch", "nur", "un\u00b7n\u00fctz", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich halte Helfer nur f\u00fcr gut,", "tokens": ["Ich", "hal\u00b7te", "Hel\u00b7fer", "nur", "f\u00fcr", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Euch eure Hammel aufzuessen.", "tokens": ["Euch", "eu\u00b7re", "Ham\u00b7mel", "auf\u00b7zu\u00b7es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Vers\u00f6hnt den Leu, beschwichtigt seine Wut.", "tokens": ["Ver\u00b7s\u00f6hnt", "den", "Leu", ",", "be\u00b7schwich\u00b7tigt", "sei\u00b7ne", "Wut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ihn zu bekriegen, w\u00e4re zu vermessen,", "tokens": ["Ihn", "zu", "be\u00b7krie\u00b7gen", ",", "w\u00e4\u00b7re", "zu", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Er \u00fcbertrifft an Kraft uns und die andern alle,", "tokens": ["Er", "\u00fc\u00b7bert\u00b7rifft", "an", "Kraft", "uns", "und", "die", "an\u00b7dern", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "KON", "ART", "ADJA", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die hier verzehren w\u00fcrden unser Gut.", "tokens": ["Die", "hier", "ver\u00b7zeh\u00b7ren", "w\u00fcr\u00b7den", "un\u00b7ser", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Er spottet jeder Macht und jeder Falle.", "tokens": ["Er", "spot\u00b7tet", "je\u00b7der", "Macht", "und", "je\u00b7der", "Fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Nur drei Verb\u00fcndete hat er auf Posten,", "tokens": ["Nur", "drei", "Ver\u00b7b\u00fcn\u00b7de\u00b7te", "hat", "er", "auf", "Pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Sie hei\u00dfen St\u00e4rke, Wachsamkeit und Mut \u2013", "tokens": ["Sie", "hei\u00b7\u00dfen", "St\u00e4r\u00b7ke", ",", "Wach\u00b7sam\u00b7keit", "und", "Mut", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Drei gro\u00dfe Helfer, die ihn keinen Heller kosten.", "tokens": ["Drei", "gro\u00b7\u00dfe", "Hel\u00b7fer", ",", "die", "ihn", "kei\u00b7nen", "Hel\u00b7ler", "kos\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "PRELS", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Werft ihm sogleich ein Schaf in seine Krallen,", "tokens": ["Werft", "ihm", "sog\u00b7leich", "ein", "Schaf", "in", "sei\u00b7ne", "Kral\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Gen\u00fcgts ihm nicht, so eilt und tut", "tokens": ["Ge\u00b7n\u00fcgts", "ihm", "nicht", ",", "so", "eilt", "und", "tut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Noch mehr hinzu, auch Rinder stramm und fest,", "tokens": ["Noch", "mehr", "hin\u00b7zu", ",", "auch", "Rin\u00b7der", "stramm", "und", "fest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$,", "ADV", "NN", "VVFIN", "KON", "PTKVZ", "