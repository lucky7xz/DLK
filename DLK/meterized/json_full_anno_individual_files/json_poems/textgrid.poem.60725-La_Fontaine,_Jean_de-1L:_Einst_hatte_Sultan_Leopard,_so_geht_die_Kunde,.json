ne.16": {"text": "Die hier verzehren w\u00fcrden unser Gut.", "tokens": ["Die", "hier", "ver\u00b7zeh\u00b7ren", "w\u00fcr\u00b7den", "un\u00b7ser", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Er spottet jeder Macht und jeder Falle.", "tokens": ["Er", "spot\u00b7tet", "je\u00b7der", "Macht", "und", "je\u00b7der", "Fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Nur drei Verb\u00fcndete hat er auf Posten,", "tokens": ["Nur", "drei", "Ver\u00b7b\u00fcn\u00b7de\u00b7te", "hat", "er", "auf", "Pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Sie hei\u00dfen St\u00e4rke, Wachsamkeit und Mut \u2013", "tokens": ["Sie", "hei\u00b7\u00dfen", "St\u00e4r\u00b7ke", ",", "Wach\u00b7sam\u00b7keit", "und", "Mut", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Drei gro\u00dfe Helfer, die ihn keinen Heller kosten.", "tokens": ["Drei", "gro\u00b7\u00dfe", "Hel\u00b7fer", ",", "die", "ihn", "kei\u00b7nen", "Hel\u00b7ler", "kos\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "PRELS", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Werft ihm sogleich ein Schaf in seine Kralle