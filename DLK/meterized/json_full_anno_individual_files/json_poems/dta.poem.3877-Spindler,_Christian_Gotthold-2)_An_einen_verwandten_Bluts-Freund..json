{"dta.poem.3877": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "2) An einen verwandten Bluts-Freund.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "So ist nun mit der Zeit mein sehnlich banges", "tokens": ["So", "ist", "nun", "mit", "der", "Zeit", "mein", "sehn\u00b7lich", "ban\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "PPOSAT", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hoffen,", "tokens": ["Hof\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Gelahrt und hoher Freund, noch endlich ein-", "tokens": ["Ge\u00b7lahrt", "und", "ho\u00b7her", "Freund", ",", "noch", "end\u00b7lich", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJA", "NN", "$,", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "getroffen,", "tokens": ["ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Verleumdung, Ha\u00df und Neid hat unser Band", "tokens": ["Ver\u00b7leum\u00b7dung", ",", "Ha\u00df", "und", "Neid", "hat", "un\u00b7ser", "Band"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "verletzt,", "tokens": ["ver\u00b7letzt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Und uns auf lange Zeit in Mi\u00dfverstand gesetzt.", "tokens": ["Und", "uns", "auf", "lan\u00b7ge", "Zeit", "in", "Mi\u00df\u00b7ver\u00b7stand", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zwar dacht ich offt an dich, ich schrieb auch bey zwey", "tokens": ["Zwar", "dacht", "ich", "offt", "an", "dich", ",", "ich", "schrieb", "auch", "bey", "zwey"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "PPER", "VVFIN", "ADV", "APPR", "CARD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "mahlen,", "tokens": ["mah\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Hierdurch der Freundschaffts-Pflicht den wahren", "tokens": ["Hier\u00b7durch", "der", "Freund\u00b7schaffts\u00b7Pflicht", "den", "wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Zoll zu zahlen;", "tokens": ["Zoll", "zu", "zah\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Allein dein Ohr war taub, und meines Neiders", "tokens": ["Al\u00b7lein", "dein", "Ohr", "war", "taub", ",", "und", "mei\u00b7nes", "Nei\u00b7ders"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Gifft", "tokens": ["Gifft"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Beraubte mich dadurch der treuen Antworts-", "tokens": ["Be\u00b7raub\u00b7te", "mich", "da\u00b7durch", "der", "treu\u00b7en", "Ant\u00b7worts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PAV", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Schrifft.", "tokens": ["Schrifft", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Dich ri\u00df das Schicksal fort, man konte mir nicht", "tokens": ["Dich", "ri\u00df", "das", "Schick\u00b7sal", "fort", ",", "man", "kon\u00b7te", "mir", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PIS", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "sagen,", "tokens": ["sa\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Wo dich doch nach der Zeit das Gl\u00fccke zu getragen.", "tokens": ["Wo", "dich", "doch", "nach", "der", "Zeit", "das", "Gl\u00fc\u00b7cke", "zu", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Letzt kam ein lieber Brief, darinnen deutlich stund,", "tokens": ["Letzt", "kam", "ein", "lie\u00b7ber", "Brief", ",", "da\u00b7rin\u00b7nen", "deut\u00b7lich", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Freund! du erneuertest den alten Freundschaffts-", "tokens": ["Freund", "!", "du", "er\u00b7neu\u00b7er\u00b7test", "den", "al\u00b7ten", "Freund\u00b7schaffts"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.21": {"text": "Bund.", "tokens": ["Bund", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Ich dancke deiner Huld, den z\u00e4rtlich from\u0303en Trieben,", "tokens": ["Ich", "dan\u00b7cke", "dei\u00b7ner", "Huld", ",", "den", "z\u00e4rt\u00b7lich", "from\u0303en", "Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Ich will dich nichts als Freund, nein, selbst als Bru-", "tokens": ["Ich", "will", "dich", "nichts", "als", "Freund", ",", "nein", ",", "selbst", "als", "Bru"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "PIS", "KOKOM", "NN", "$,", "PTKANT", "$,", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "der lieben,", "tokens": ["der", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Du solst davon die Frucht in nechsten Zeiten sehn,", "tokens": ["Du", "solst", "da\u00b7von", "die", "Frucht", "in", "nechs\u00b7ten", "Zei\u00b7ten", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dir w\u00fcnscht mein treuer Kiel ein stetes Wohlergehn.", "tokens": ["Dir", "w\u00fcnscht", "mein", "treu\u00b7er", "Kiel", "ein", "ste\u00b7tes", "Woh\u00b7ler\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}