{"dta.poem.1407": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Des Leibes schn\u00f6den Koth pflegt Wasser abzubaden/", "tokens": ["Des", "Lei\u00b7bes", "schn\u00f6\u00b7den", "Koth", "pflegt", "Was\u00b7ser", "ab\u00b7zu\u00b7ba\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom S\u00fcnden-Kothe kan der Tauffe Flutt entladen;", "tokens": ["Vom", "S\u00fcn\u00b7den\u00b7Ko\u00b7the", "kan", "der", "Tauf\u00b7fe", "Flutt", "ent\u00b7la\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit du/ Pathe/ m\u00f6gst von S\u00fcnden sauber seyn/", "tokens": ["Da\u00b7mit", "du", "/", "Pa\u00b7the", "/", "m\u00f6gst", "von", "S\u00fcn\u00b7den", "sau\u00b7ber", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "NE", "$(", "VMFIN", "APPR", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schleust dieser Quell-Brunn dich/ GOtt gebe selig/ ein.", "tokens": ["Schleust", "die\u00b7ser", "Quell\u00b7Brunn", "dich", "/", "Gott", "ge\u00b7be", "se\u00b7lig", "/", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "wor