{"textgrid.poem.46197": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ueber meines bruders Ludwig Weckherlins seligen hintritt", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein liebster bruder, dir, der du des vatterlands", "tokens": ["Mein", "liebs\u00b7ter", "bru\u00b7der", ",", "dir", ",", "der", "du", "des", "vat\u00b7ter\u00b7lands"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und alles werts verlust nicht woltest \u00fcberleben,", "tokens": ["und", "al\u00b7les", "werts", "ver\u00b7lust", "nicht", "wol\u00b7test", "\u00fc\u00b7berl\u00b7e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVFIN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mu\u00df ich hie dise klag und wahre zeugnus geben,", "tokens": ["mu\u00df", "ich", "hie", "di\u00b7se", "klag", "und", "wah\u00b7re", "zeug\u00b7nus", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PDS", "VVFIN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df du ein wahre zier und seul des kirchenstands.", "tokens": ["da\u00df", "du", "ein", "wah\u00b7re", "zier", "und", "seul", "des", "kir\u00b7chen\u00b7stands", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "An alter bist du mir kaum zwei jahr vorgegangen,", "tokens": ["An", "al\u00b7ter", "bist", "du", "mir", "kaum", "zwei", "jahr", "vor\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VAFIN", "PPER", "PPER", "ADV", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "jedoch an geist und lehr blieb ich weit hinder dir,", "tokens": ["je\u00b7doch", "an", "geist", "und", "lehr", "blieb", "ich", "weit", "hin\u00b7der", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "darum hast du die kron von got so lang vor mir,", "tokens": ["da\u00b7rum", "hast", "du", "die", "kron", "von", "got", "so", "lang", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "APPR", "NE", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "die ich durch seine gnad auch k\u00fcrzlich werd, empfangen.", "tokens": ["die", "ich", "durch", "sei\u00b7ne", "gnad", "auch", "k\u00fcrz\u00b7lich", "werd", ",", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRELS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein liebster bruder, dir, der du des vatterlands", "tokens": ["Mein", "liebs\u00b7ter", "bru\u0