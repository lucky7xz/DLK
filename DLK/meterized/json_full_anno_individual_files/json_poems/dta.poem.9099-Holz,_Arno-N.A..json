{"dta.poem.9099": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachts um meinen Tempelhain", "tokens": ["Nachts", "um", "mei\u00b7nen", "Tem\u00b7pel\u00b7hain"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wachen siebzig Bronzek\u00fche.", "tokens": ["wa\u00b7chen", "sieb\u00b7zig", "Bron\u00b7ze\u00b7k\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tausend bunte Steinlampen flimmern.", "tokens": ["Tau\u00b7send", "bun\u00b7te", "Stein\u00b7lam\u00b7pen", "flim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf einem roten Thron aus Lack", "tokens": ["Auf", "ei\u00b7nem", "ro\u00b7ten", "Thron", "aus", "Lack"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sitz ich im Allerheiligsten.", "tokens": ["sitz", "ich", "im", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Ueber mir,", "tokens": ["Ue\u00b7ber", "mir", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "durch das Geb\u00e4lk aus Sandelholz,", "tokens": ["durch", "das", "Ge\u00b7b\u00e4lk", "aus", "San\u00b7del\u00b7holz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "im ausgestochnen Viereck,", "tokens": ["im", "aus\u00b7ge\u00b7stoch\u00b7nen", "Vier\u00b7eck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "stehn die Sterne.", "tokens": ["stehn", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Ich blinzle.", "tokens": ["Ich", "blinz\u00b7le", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Wenn ich jetzt aufst\u00fcnde,", "tokens": ["Wenn", "ich", "jetzt", "auf\u00b7st\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "zertr\u00fcmmerten meine elfenbeinernen Schultern das Dach,", "tokens": ["zer\u00b7tr\u00fcm\u00b7mer\u00b7ten", "mei\u00b7ne", "el\u00b7fen\u00b7bei\u00b7ner\u00b7nen", "Schul\u00b7tern", "das", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "und der eirunde Diamant vor meiner Stirn", "tokens": ["und", "der", "ei\u00b7run\u00b7de", "Di\u00b7a\u00b7mant", "vor", "mei\u00b7ner", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "stiesse den Mond ein.", "tokens": ["sties\u00b7se", "den", "Mond", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Die dicken Priester d\u00fcrfen ruhig schnarchen.", "tokens": ["Die", "di\u00b7cken", "Pries\u00b7ter", "d\u00fcr\u00b7fen", "ru\u00b7hig", "schnar\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ich stehe nicht auf.", "tokens": ["Ich", "ste\u00b7he", "nicht", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Ich sitze mit untergeschlagenen Beinen", "tokens": ["Ich", "sit\u00b7ze", "mit", "un\u00b7ter\u00b7ge\u00b7schla\u00b7ge\u00b7nen", "Bei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "und beschaue meinen Nabel.", "tokens": ["und", "be\u00b7schau\u00b7e", "mei\u00b7nen", "Na\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Der ist ein blutender Rubin", "tokens": ["Der", "ist", "ein", "blu\u00b7ten\u00b7der", "Ru\u00b7bin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in einem nackten Bauch aus Gold.", "tokens": ["in", "ei\u00b7nem", "nack\u00b7ten", "Bauch", "aus", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}