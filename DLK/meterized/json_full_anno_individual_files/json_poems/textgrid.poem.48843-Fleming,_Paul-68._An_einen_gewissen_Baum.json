{"textgrid.poem.48843": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "68. An einen gewissen Baum", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will zu deiner Hut ein' eigne Drias stellen,", "tokens": ["Ich", "will", "zu", "dei\u00b7ner", "Hut", "ein'", "eig\u00b7ne", "Drias", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "da\u00df kein geh\u00f6rnter Hirsch, kein B\u00e4r, kein wildes Schwein", "tokens": ["da\u00df", "kein", "ge\u00b7h\u00f6rn\u00b7ter", "Hirsch", ",", "kein", "B\u00e4r", ",", "kein", "wil\u00b7des", "Schwein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "zu sto\u00dfen sich erk\u00fchn' an dein bemostes Bein.", "tokens": ["zu", "sto\u00b7\u00dfen", "sich", "er\u00b7k\u00fchn'", "an", "dein", "be\u00b7mos\u00b7tes", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVFIN", "PRF", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da\u00df kein feindlichs Beil dich etwa m\u00f6ge f\u00e4llen,", "tokens": ["Und", "da\u00df", "kein", "feind\u00b7lichs", "Beil", "dich", "et\u00b7wa", "m\u00f6\u00b7ge", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "ADJA", "NN", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "so steht Silenus hier mit zweien Mitgesellen.", "tokens": ["so", "steht", "Si\u00b7le\u00b7nus", "hier", "mit", "zwei\u00b7en", "Mit\u00b7ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Boden gibt dir Saft, der Himmel Sonnenschein", "tokens": ["Der", "Bo\u00b7den", "gibt", "dir", "Saft", ",", "der", "Him\u00b7mel", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und dein gekraustes Haar soll stets durchsprungen sein", "tokens": ["und", "dein", "ge\u00b7kraus\u00b7tes", "Haar", "soll", "stets", "durch\u00b7sprun\u00b7gen", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VMFIN", "ADV", "VVPP", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "von aller V\u00f6gel Art, der lieblichen, der hellen.", "tokens": ["von", "al\u00b7ler", "V\u00f6\u00b7gel", "Art", ",", "der", "lieb\u00b7li\u00b7chen", ",", "der", "hel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df nun weit von dir, nun weit von der dahin,", "tokens": ["Ich", "mu\u00df", "nun", "weit", "von", "dir", ",", "nun", "weit", "von", "der", "da\u00