{"textgrid.poem.60326": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh, wie des Niagara Wellen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie des Niagara Wellen", "tokens": ["Sieh", ",", "wie", "des", "Nia\u00b7ga\u00b7ra", "Wel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NE", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Im Donnerfall zu Staub zerschellen,", "tokens": ["Im", "Don\u00b7ner\u00b7fall", "zu", "Staub", "zer\u00b7schel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie sie, spr\u00fchend nun zerflogen,", "tokens": ["Und", "wie", "sie", ",", "spr\u00fc\u00b7hend", "nun", "zer\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Empfangen goldne Sonnenstrahlen", "tokens": ["Emp\u00b7fan\u00b7gen", "gold\u00b7ne", "Son\u00b7nen\u00b7strah\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und auf den Abgrund lieblich malen", "tokens": ["Und", "auf", "den", "Ab\u00b7grund", "lieb\u00b7lich", "ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den farbenreichen Regenbogen.", "tokens": ["Den", "far\u00b7ben\u00b7rei\u00b7chen", "Re\u00b7gen\u00b7bo\u00b7gen", "."], "token_in