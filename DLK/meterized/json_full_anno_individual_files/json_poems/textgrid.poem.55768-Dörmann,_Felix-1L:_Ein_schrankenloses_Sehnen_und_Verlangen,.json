{"textgrid.poem.55768": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein schrankenloses Sehnen und Verlangen,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein schrankenloses Sehnen und Verlangen,", "tokens": ["Ein", "schran\u00b7ken\u00b7lo\u00b7ses", "Seh\u00b7nen", "und", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein stetes Suchen und ein stetes Meiden,", "tokens": ["Ein", "ste\u00b7tes", "Su\u00b7chen", "und", "ein", "ste\u00b7tes", "Mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein hoffnungsfreudiges Erwartungsbangen,", "tokens": ["Ein", "hoff\u00b7nungs\u00b7freu\u00b7di\u00b7ges", "Er\u00b7war\u00b7tungs\u00b7ban\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dann aufs neue der Entt\u00e4uschung Leiden,", "tokens": ["Und", "dann", "aufs", "neu\u00b7e", "der", "Ent\u00b7t\u00e4u\u00b7schung", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein zornig-wildes R\u00fctteln an den Ketten,", "tokens": ["Ein", "zor\u00b7nig\u00b7wil\u00b7des", "R\u00fct\u00b7teln", "an", "den", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "AP