ken_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Magst du nicht finden,", "tokens": ["Magst", "du", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wie auf dem Staube", "tokens": ["Wie", "auf", "dem", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wir uns das Bleibende gr\u00fcnden?", "tokens": ["Wir", "uns", "das", "Blei\u00b7ben\u00b7de", "gr\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ach! nicht hienieden,", "tokens": ["Ach", "!", "nicht", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "PTKNEG", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nicht wo in Gr\u00e4bern die Asche liegt", "tokens": ["Nicht", "wo", "in", "Gr\u00e4\u00b7bern", "die", "A\u00b7sche", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PWAV", "APPR", "NN", "ART", "NN", "VVFIN"], "meter": "++-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Suche den Frieden,", "tokens": ["Su\u00b7che", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nicht wo die Freude mit Winden fliegt.", "tokens": ["Nicht", "wo", "die", "Freu\u00b7de", "mit", "Win\u00b7den", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "po