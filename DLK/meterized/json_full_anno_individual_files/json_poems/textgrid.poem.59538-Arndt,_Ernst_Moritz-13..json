nct", "word", "word", "punct"], "pos": ["ITJ", "$.", "PTKNEG", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nicht wo in Gr\u00e4bern die Asche liegt", "tokens": ["Nicht", "wo", "in", "Gr\u00e4\u00b7bern", "die", "A\u00b7sche", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PWAV", "APPR", "NN", "ART", "NN", "VVFIN"], "meter": "++-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Suche den Frieden,", "tokens": ["Su\u00b7che", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nicht wo die Freude mit Winden fliegt.", "tokens": ["Nicht", "wo", "die", "Freu\u00b7de", "mit", "Win\u00b7den", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Arbeit und Tr\u00e4nen", "tokens": ["Ar\u00b7beit", "und", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Irdischem weihe,", "tokens": ["Ir\u00b7di\u00b7schem", "wei\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Aber dein Sehnen", "tokens": ["A\u00b7ber", "dein", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Stelle zur himmlischen Bl\u00e4ue.", "tokens": ["Stel\u00b7le", "zur", "himm\u00b7li\u00b7schen", "Bl\u00e4u\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Da gehn die Lichter,", "tokens": ["Da", "gehn", "die", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ewige Spiegel der reinsten Lust,", "tokens": ["E\u00b7wi\u00b7ge", "Spie\u00b7gel", "der", "reins\u00b7ten", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Liebende Richter,", "tokens": ["Lie\u00b7ben\u00b7de", "Rich\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Liebende Tr\u00f6ster der Menschenbrust;", "tokens": ["Lie\u00b7ben\u00b7de", "Tr\u00f6s\u00b7ter", "der", "Men\u00b7schen\u00b7brust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Dahin gerichtet,", "tokens": ["Da\u00b7hin", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Was dich bedr\u00e4nget!", "tokens": ["Was", "dich", "be\u00b7dr\u00e4n\u00b7get", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Da wird gelichtet,", "tokens": ["Da", "wird", "ge\u00b7lich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Was dir hier Nacht noch verh\u00e4nget.", "tokens": ["Was", "dir", "hier", "Nacht", "noch", "ver\u00b7h\u00e4n\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}