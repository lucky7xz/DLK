{"dta.poem.3292": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der May.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Der holde May hat endlich obgesiegt,", "tokens": ["Der", "hol\u00b7de", "May", "hat", "end\u00b7lich", "ob\u00b7ge\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Boreas mu\u00df lauem Weste weichen:", "tokens": ["Und", "Bo\u00b7re\u00b7as", "mu\u00df", "lau\u00b7em", "Wes\u00b7te", "wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der laue West lockt Floren, wo er fliegt,", "tokens": ["Der", "lau\u00b7e", "West", "lockt", "Flo\u00b7ren", ",", "wo", "er", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jhm br\u00fcnstig l\u00e4chelnd nachzuschleichen.", "tokens": ["Jhm", "br\u00fcns\u00b7tig", "l\u00e4\u00b7chelnd", "nach\u00b7zu\u00b7schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df uns den Wald, wo itzt manch spielend Reh", "tokens": ["La\u00df", "uns", "den", "Wald", ",", "wo", "itzt", "manch", "spie\u00b7lend", "Reh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PWAV", "ADV", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch B\u00fcsche rauscht; la\u00df uns die gr\u00fcnen Buchen", "tokens": ["Durch", "B\u00fc\u00b7sche", "rauscht", ";", "la\u00df", "uns", "die", "gr\u00fc\u00b7nen", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$.", "VVIMP", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Feld und Bach und den bethauten Klee,", "tokens": ["Und", "Feld", "und", "Bach", "und", "den", "be\u00b7thau\u00b7ten", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O Freund! auch wiederum besuchen.", "tokens": ["O", "Freund", "!", "auch", "wie\u00b7de\u00b7rum", "be\u00b7su\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Umw\u00f6lkt annoch der Unmuth unsern Blick,", "tokens": ["Um\u00b7w\u00f6lkt", "an\u00b7noch", "der", "Un\u00b7muth", "un\u00b7sern", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da \u00fcberall Natur und Erde lachen?", "tokens": ["Da", "\u00fc\u00b7be\u00b7rall", "Na\u00b7tur", "und", "Er\u00b7de", "la\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sey auch vergn\u00fcgt und la\u00df das wilde Gl\u00fcck", "tokens": ["Sey", "auch", "ver\u00b7gn\u00fcgt", "und", "la\u00df", "das", "wil\u00b7de", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "KON", "VVIMP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Zeiten mehr als eisern machen!", "tokens": ["Die", "Zei\u00b7ten", "mehr", "als", "ei\u00b7sern", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "KOKOM", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es zieh uns aus, was wir von ihm geborgt,", "tokens": ["Es", "zieh", "uns", "aus", ",", "was", "wir", "von", "ihm", "ge\u00b7borgt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und werf allein dem ihm verkauften Schwarme", "tokens": ["Und", "werf", "al\u00b7lein", "dem", "ihm", "ver\u00b7kauf\u00b7ten", "Schwar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die G\u00fcter zu, um die ich nie gesorgt!", "tokens": ["Die", "G\u00fc\u00b7ter", "zu", ",", "um", "die", "ich", "nie", "ge\u00b7sorgt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nackt flieh ich in der Weisheit Arme.", "tokens": ["Nackt", "flieh", "ich", "in", "der", "Weis\u00b7heit", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es bleibt mir doch der stets zufriedne Sinn", "tokens": ["Es", "bleibt", "mir", "doch", "der", "stets", "zu\u00b7fried\u00b7ne", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Muths genug, mein Gl\u00fcck in mir zu suchen,", "tokens": ["Und", "Muths", "ge\u00b7nug", ",", "mein", "Gl\u00fcck", "in", "mir", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,", "PPOSAT", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und edler Stolz, auch wann ich niedrig bin,", "tokens": ["Und", "ed\u00b7ler", "Stolz", ",", "auch", "wann", "ich", "nied\u00b7rig", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADV", "PWAV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unedle T\u00fccke zu verfluchen.", "tokens": ["U\u00b7ned\u00b7le", "T\u00fc\u00b7cke", "zu", "ver\u00b7flu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Es bleibt mir auch, vom Zufall unentwandt,", "tokens": ["Es", "bleibt", "mir", "auch", ",", "vom", "Zu\u00b7fall", "un\u00b7ent\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Saitenspiel der griechischen Cam\u00f6ne,", "tokens": ["Das", "Sai\u00b7ten\u00b7spiel", "der", "grie\u00b7chi\u00b7schen", "Ca\u00b7m\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das, trotz dem Gl\u00fcck, ich mit gedungner Hand", "tokens": ["Das", ",", "trotz", "dem", "Gl\u00fcck", ",", "ich", "mit", "ge\u00b7dung\u00b7ner", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "APPR", "ART", "NN", "$,", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu feigem Schmeicheln nicht verw\u00f6hne.", "tokens": ["Zu", "fei\u00b7gem", "Schmei\u00b7cheln", "nicht", "ver\u00b7w\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}