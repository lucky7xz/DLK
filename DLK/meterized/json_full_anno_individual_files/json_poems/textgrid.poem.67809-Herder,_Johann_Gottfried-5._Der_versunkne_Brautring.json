{"textgrid.poem.67809": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "5. Der versunkne Brautring", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Fischer reit' ich,", "tokens": ["Zum", "Fi\u00b7scher", "reit'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den Fischer besuch' ich,", "tokens": ["Den", "Fi\u00b7scher", "be\u00b7such'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sein Eidam w\u00e4r' ich gerne!", "tokens": ["Sein", "Ei\u00b7dam", "w\u00e4r'", "ich", "ger\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Am Hafestrande", "tokens": ["Am", "Ha\u00b7fest\u00b7ran\u00b7de"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sp\u00fclt' ich die Neze,", "tokens": ["Sp\u00fclt'", "ich", "die", "Ne\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Rein wusch ich mir die H\u00e4nde.", "tokens": ["Rein", "wusch", "ich", "mir", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weh! da entfiel mir", "tokens": ["Weh", "!", "da", "ent\u00b7fiel", "mir"