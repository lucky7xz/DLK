{"dta.poem.6527": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheinet offtermal mit Sonnen-Bildern", "tokens": ["Es", "schei\u00b7net", "off\u00b7ter\u00b7mal", "mit", "Son\u00b7nen\u00b7Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Himmel sich zu schm\u00fccken und zu schildern,", "tokens": ["Der", "Him\u00b7mel", "sich", "zu", "schm\u00fc\u00b7cken", "und", "zu", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ihrer Strahlen Meng\u2019 auf C\u00f6rper, die polirt,", "tokens": ["Wenn", "ih\u00b7rer", "Strah\u00b7len", "Meng'", "auf", "C\u00f6r\u00b7per", ",", "die", "po\u00b7lirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NE", "APPR", "NE", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und die geschickt, sie abzuweisen, fallen;", "tokens": ["Und", "die", "ge\u00b7schickt", ",", "sie", "ab\u00b7zu\u00b7wei\u00b7sen", ",", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "VVPP", "$,", "PPER", "VVIZU", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So sehn wir, da\u00df sie r\u00fcckwerts prallen,", "tokens": ["So", "sehn", "wir", ",", "da\u00df", "sie", "r\u00fcck\u00b7werts", "pral\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und da\u00df sie unsrer Augen-Strahl", "tokens": ["Und", "da\u00df", "sie", "uns\u00b7rer", "Au\u00b7gen\u00b7Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich zeigen noch zum andern mal.", "tokens": ["Sich", "zei\u00b7gen", "noch", "zum", "an\u00b7dern", "mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "APPRART", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da denn die glatten Fl\u00e4chen", "tokens": ["Da", "denn", "die", "glat\u00b7ten", "Fl\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gefrorner Wolcken ihrem Schein", "tokens": ["Ge\u00b7fror\u00b7ner", "Wol\u00b7cken", "ih\u00b7rem", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Recht hell-polirte Spiegel seyn,", "tokens": ["Recht", "hell\u00b7po\u00b7lir\u00b7te", "Spie\u00b7gel", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wodurch die himmlischen Revieren", "tokens": ["Wo\u00b7durch", "die", "himm\u00b7li\u00b7schen", "Re\u00b7vie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bey Tage selbst verschiedne Sonnen zieren;", "tokens": ["Bey", "Ta\u00b7ge", "selbst", "ver\u00b7schied\u00b7ne", "Son\u00b7nen", "zie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Indem der Strahlen Heer gehemmet, fortzugehn,", "tokens": ["In\u00b7dem", "der", "Strah\u00b7len", "Heer", "ge\u00b7hem\u00b7met", ",", "fort\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dadurch Parelien formiren.", "tokens": ["Da\u00b7durch", "Pa\u00b7re\u00b7li\u00b7en", "for\u00b7mi\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Man glaubt sodann viel Sonnen zu ersehn,", "tokens": ["Man", "glaubt", "so\u00b7dann", "viel", "Son\u00b7nen", "zu", "er\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die alle gleich hellgl\u00e4ntzend, und gleich sch\u00f6n.", "tokens": ["Die", "al\u00b7le", "gleich", "hell\u00b7gl\u00e4nt\u00b7zend", ",", "und", "gleich", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVPP", "$,", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}