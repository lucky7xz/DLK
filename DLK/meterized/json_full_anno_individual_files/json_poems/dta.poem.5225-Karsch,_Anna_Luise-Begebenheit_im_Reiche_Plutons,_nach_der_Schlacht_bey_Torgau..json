{"dta.poem.5225": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Begebenheit im Reiche Plutons,  \n nach der Schlacht bey Torgau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Im Reich der Schatten ging j\u00fcngst ein Ger\u00fccht", "tokens": ["Im", "Reich", "der", "Schat\u00b7ten", "ging", "j\u00fcngst", "ein", "Ge\u00b7r\u00fccht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "umher:", "tokens": ["um\u00b7her", ":"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Da\u00df auf der Welt ein K\u00f6nig w\u00e4r,", "tokens": ["Da\u00df", "auf", "der", "Welt", "ein", "K\u00f6\u00b7nig", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der gr\u00f6sser sey, als alle Helden,", "tokens": ["Der", "gr\u00f6s\u00b7ser", "sey", ",", "als", "al\u00b7le", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$,", "KOUS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von deren Thaten uns Plutarche Wunder melden.", "tokens": ["Von", "de\u00b7ren", "Tha\u00b7ten", "uns", "Plu\u00b7tar\u00b7che", "Wun\u00b7der", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Schatten Maupertuis kommt an und liest ein Buch", "tokens": ["Der", "Schat\u00b7ten", "Mau\u00b7per\u00b7tu\u00b7is", "kommt", "an", "und", "liest", "ein", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Von dieses Helden Thaten voll;", "tokens": ["Von", "die\u00b7ses", "Hel\u00b7den", "Tha\u00b7ten", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bald ist er Antonin, bald Mars und bald Apoll,", "tokens": ["Bald", "ist", "er", "An\u00b7to\u00b7nin", ",", "bald", "Mars", "und", "bald", "A\u00b7poll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "$,", "ADV", "NN", "KON", "ADV", "NE", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und jede Stirn wird Wiederspruch!", "tokens": ["Und", "je\u00b7de", "Stirn", "wird", "Wie\u00b7der\u00b7spruch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Schatten von dem Weltbezwinger,", "tokens": ["Der", "Schat\u00b7ten", "von", "dem", "Welt\u00b7be\u00b7zwin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der noch mit nervenlosem Finger", "tokens": ["Der", "noch", "mit", "ner\u00b7ven\u00b7lo\u00b7sem", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+----+-", "measure": "unknown.measure.tri"}, "line.12": {"text": "Den Staaten Plutons scheint zu drohn,", "tokens": ["Den", "Staa\u00b7ten", "Plu\u00b7tons", "scheint", "zu", "drohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Weint neidisch eine Geisterz\u00e4hre,", "tokens": ["Weint", "nei\u00b7disch", "ei\u00b7ne", "Geis\u00b7ter\u00b7z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df auf der Welt ein K\u00f6nig w\u00e4re,", "tokens": ["Da\u00df", "auf", "der", "Welt", "ein", "K\u00f6\u00b7nig", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Der gr\u00f6sser sey, als Philipps Sohn.", "tokens": ["Der", "gr\u00f6s\u00b7ser", "sey", ",", "als", "Phi\u00b7lipps", "Sohn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$,", "KOUS", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Achill stampft grimmig mit dem Fusse,", "tokens": ["A\u00b7chill", "stampft", "grim\u00b7mig", "mit", "dem", "Fus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Schw\u00f6rt bey dem Styx, da\u00df ihnen zum Verdrusse", "tokens": ["Schw\u00f6rt", "bey", "dem", "Styx", ",", "da\u00df", "ih\u00b7nen", "zum", "Ver\u00b7drus\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Der gr\u00f6\u00dfre Held erdichtet sey:", "tokens": ["Der", "gr\u00f6\u00df\u00b7re", "Held", "er\u00b7dich\u00b7tet", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "\u201dgro\u00df, schw\u00f6rt er, war nur ich, gro\u00df war nur Alexander!", "tokens": ["\"", "gro\u00df", ",", "schw\u00f6rt", "er", ",", "war", "nur", "ich", ",", "gro\u00df", "war", "nur", "A\u00b7lex\u00b7an\u00b7der", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "PPER", "$,", "ADJD", "VAFIN", "ADV", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Indem er schw\u00f6rt, entsteht ein gr\u00e4\u00dfliches Geschrey,", "tokens": ["In\u00b7dem", "er", "schw\u00f6rt", ",", "ent\u00b7steht", "ein", "gr\u00e4\u00df\u00b7li\u00b7ches", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Helden fliegen auseinander,", "tokens": ["Die", "Hel\u00b7den", "flie\u00b7gen", "aus\u00b7ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Gehn dem Get\u00fcmmel nach, und stehen lauter Ohr,", "tokens": ["Gehn", "dem", "Ge\u00b7t\u00fcm\u00b7mel", "nach", ",", "und", "ste\u00b7hen", "lau\u00b7ter", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Zu h\u00f6ren, was zehntausend Schatten sprechen,", "tokens": ["Zu", "h\u00f6\u00b7ren", ",", "was", "zehn\u00b7tau\u00b7send", "Schat\u00b7ten", "spre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Zehntausend ziehen ihn nun allen Helden vor,", "tokens": ["Zehn\u00b7tau\u00b7send", "zie\u00b7hen", "ihn", "nun", "al\u00b7len", "Hel\u00b7den", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PPER", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zehntausend wollen sich nicht an den Sieger r\u00e4chen,", "tokens": ["Zehn\u00b7tau\u00b7send", "wol\u00b7len", "sich", "nicht", "an", "den", "Sie\u00b7ger", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VMFIN", "PRF", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Von Torgau kommen sie, die armen Schatten, her.", "tokens": ["Von", "Tor\u00b7gau", "kom\u00b7men", "sie", ",", "die", "ar\u00b7men", "Schat\u00b7ten", ",", "her", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Starr steht nun Philipps Sohn, nun stampft Achill", "tokens": ["Starr", "steht", "nun", "Phi\u00b7lipps", "Sohn", ",", "nun", "stampft", "A\u00b7chill"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "NE", "NN", "$,", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "nicht mehr.", "tokens": ["nicht", "mehr", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$."], "meter": "-+", "measure": "iambic.single"}}}}}