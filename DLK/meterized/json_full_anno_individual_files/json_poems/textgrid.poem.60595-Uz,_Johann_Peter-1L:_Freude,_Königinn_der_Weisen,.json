{"textgrid.poem.60595": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freude, K\u00f6niginn der Weisen,", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freude, K\u00f6niginn der Weisen,", "tokens": ["Freu\u00b7de", ",", "K\u00f6\u00b7ni\u00b7ginn", "der", "Wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die, mit Bluhmen um ihr Haupt,", "tokens": ["Die", ",", "mit", "Bluh\u00b7men", "um", "ihr", "Haupt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich auf g\u00fcldner Leyer preisen,", "tokens": ["Dich", "auf", "g\u00fcld\u00b7ner", "Le\u00b7yer", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhig, wann die Thorheit schnaubt:", "tokens": ["Ru\u00b7hig", ",", "wann", "die", "Thor\u00b7heit", "schnaubt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6re mich von deinem Throne,", "tokens": ["H\u00f6\u00b7re", "mich", "von", "dei\u00b7nem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kind der Weisheit, deren Hand", "tokens": ["Kind", "der", "Weis\u00b7heit", ",", "de\u00b7ren", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Immer selbst in deine Krone", "tokens": ["Im\u00b7mer", "selbst", "in", "dei\u00b7ne", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihre sch\u00f6nsten Rosen band!", "tokens": ["Ih\u00b7re", "sch\u00f6ns\u00b7ten", "Ro\u00b7sen", "band", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rosen, die mit frischen Bl\u00e4ttern,", "tokens": ["Ro\u00b7sen", ",", "die", "mit", "fri\u00b7schen", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trotz dem Nord, unsterblich bl\u00fchn,", "tokens": ["Trotz", "dem", "Nord", ",", "uns\u00b7terb\u00b7lich", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trotz dem S\u00fcdwind, unter Wettern,", "tokens": ["Trotz", "dem", "S\u00fcd\u00b7wind", ",", "un\u00b7ter", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann die Wolken Flammen spr\u00fchn:", "tokens": ["Wann", "die", "Wol\u00b7ken", "Flam\u00b7men", "spr\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die dein lockicht Haar durchschlingen,", "tokens": ["Die", "dein", "lo\u00b7ckicht", "Haar", "durch\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht nur an Cytherens Brust,", "tokens": ["Nicht", "nur", "an", "Cy\u00b7the\u00b7rens", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wann die Grazien dir singen,", "tokens": ["Wann", "die", "Gra\u00b7zi\u00b7en", "dir", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder bey Ly\u00e4ens Lust.", "tokens": ["O\u00b7der", "bey", "Ly\u00b7\u00e4\u00b7ens", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie bekr\u00e4nzen dich in Zeiten,", "tokens": ["Sie", "be\u00b7kr\u00e4n\u00b7zen", "dich", "in", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die kein Sonnenblick erhellt,", "tokens": ["Die", "kein", "Son\u00b7nen\u00b7blick", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sahen dich das Gl\u00fcck bestreiten,", "tokens": ["Sa\u00b7hen", "dich", "das", "Gl\u00fcck", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Tyrannen unsrer Welt,", "tokens": ["Den", "Ty\u00b7ran\u00b7nen", "uns\u00b7rer", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der um seine Riesenglieder", "tokens": ["Der", "um", "sei\u00b7ne", "Rie\u00b7sen\u00b7glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Donnerndes Gew\u00f6lke zog,", "tokens": ["Don\u00b7nern\u00b7des", "Ge\u00b7w\u00f6l\u00b7ke", "zog", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit schrecklichem Gefieder", "tokens": ["Und", "mit", "schreck\u00b7li\u00b7chem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zwischen Erd und Himmel flog.", "tokens": ["Zwi\u00b7schen", "Erd", "und", "Him\u00b7mel", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dich und deine Rosen sahen", "tokens": ["Dich", "und", "dei\u00b7ne", "Ro\u00b7sen", "sa\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die Gegenden der Nacht", "tokens": ["Auch", "die", "Ge\u00b7gen\u00b7den", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich des Todes Throne nahen,", "tokens": ["Sich", "des", "To\u00b7des", "Thro\u00b7ne", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo das kalte Schrecken wacht.", "tokens": ["Wo", "das", "kal\u00b7te", "Schre\u00b7cken", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deinen Pfad, wo du gegangen,", "tokens": ["Dei\u00b7nen", "Pfad", ",", "wo", "du", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeichnete das sanfte Licht", "tokens": ["Zeich\u00b7ne\u00b7te", "das", "sanf\u00b7te", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Cynthiens mit vollen Wangen,", "tokens": ["Cyn\u00b7thiens", "mit", "vol\u00b7len", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die durch schwarze Schatten bricht.", "tokens": ["Die", "durch", "schwar\u00b7ze", "Schat\u00b7ten", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dir war dieser Herr des Lebens", "tokens": ["Dir", "war", "die\u00b7ser", "Herr", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War der Tod nicht f\u00fcrchterlich,", "tokens": ["War", "der", "Tod", "nicht", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er schwenkete vergebens", "tokens": ["Und", "er", "schwen\u00b7ke\u00b7te", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Wurfspie\u00df wider dich:", "tokens": ["Sei\u00b7nen", "Wurf\u00b7spie\u00df", "wi\u00b7der", "dich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil im traurigen Gefilde", "tokens": ["Weil", "im", "trau\u00b7ri\u00b7gen", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hoffnung dir zur Seite gieng,", "tokens": ["Hoff\u00b7nung", "dir", "zur", "Sei\u00b7te", "gieng", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit diamantnem Schilde", "tokens": ["Und", "mit", "di\u00b7a\u00b7mant\u00b7nem", "Schil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Ueber deinem Haupte hieng.", "tokens": ["Ue\u00b7ber", "dei\u00b7nem", "Haup\u00b7te", "hieng", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hab ich meine k\u00fchnen Saiten", "tokens": ["Hab", "ich", "mei\u00b7ne", "k\u00fch\u00b7nen", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein lautschallend Lob gelehrt,", "tokens": ["Dein", "laut\u00b7schal\u00b7lend", "Lob", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das vielleicht in sp\u00e4ten Zeiten", "tokens": ["Das", "viel\u00b7leicht", "in", "sp\u00e4\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ungebohrne Nachwelt h\u00f6rt:", "tokens": ["Un\u00b7ge\u00b7bohr\u00b7ne", "Nach\u00b7welt", "h\u00f6rt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hab ich den bebl\u00fchmten Pfaden,", "tokens": ["Hab", "ich", "den", "be\u00b7bl\u00fchm\u00b7ten", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo du wandelst, nachgesp\u00fcrt,", "tokens": ["Wo", "du", "wan\u00b7delst", ",", "nach\u00b7ge\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und von st\u00fcrmischen Gestaden", "tokens": ["Und", "von", "st\u00fcr\u00b7mi\u00b7schen", "Ge\u00b7sta\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einige zu dir gef\u00fchrt:", "tokens": ["Ei\u00b7ni\u00b7ge", "zu", "dir", "ge\u00b7f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "G\u00f6ttinn, o so sey, ich flehe,", "tokens": ["G\u00f6t\u00b7tinn", ",", "o", "so", "sey", ",", "ich", "fle\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADV", "VAFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinem Dichter immer hold,", "tokens": ["Dei\u00b7nem", "Dich\u00b7ter", "im\u00b7mer", "hold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er schimmernd Gl\u00fcck verschm\u00e4he,", "tokens": ["Da\u00df", "er", "schim\u00b7mernd", "Gl\u00fcck", "ver\u00b7schm\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reich in sich, auch ohne Gold;", "tokens": ["Reich", "in", "sich", ",", "auch", "oh\u00b7ne", "Gold", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sein Leben zwar verborgen,", "tokens": ["Da\u00df", "sein", "Le\u00b7ben", "zwar", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber ohne Sklaverey,", "tokens": ["A\u00b7ber", "oh\u00b7ne", "Skla\u00b7ve\u00b7rey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne Flecken, ohne Sorgen,", "tokens": ["Oh\u00b7ne", "Fle\u00b7cken", ",", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weisen Freunden theuer sey!", "tokens": ["Wei\u00b7sen", "Freun\u00b7den", "theu\u00b7er", "sey", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Freude, K\u00f6niginn der Weisen,", "tokens": ["Freu\u00b7de", ",", "K\u00f6\u00b7ni\u00b7ginn", "der", "Wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die, mit Bluhmen um ihr Haupt,", "tokens": ["Die", ",", "mit", "Bluh\u00b7men", "um", "ihr", "Haupt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich auf g\u00fcldner Leyer preisen,", "tokens": ["Dich", "auf", "g\u00fcld\u00b7ner", "Le\u00b7yer", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhig, wann die Thorheit schnaubt:", "tokens": ["Ru\u00b7hig", ",", "wann", "die", "Thor\u00b7heit", "schnaubt", ":"], "token_info": ["word", "punct", "w