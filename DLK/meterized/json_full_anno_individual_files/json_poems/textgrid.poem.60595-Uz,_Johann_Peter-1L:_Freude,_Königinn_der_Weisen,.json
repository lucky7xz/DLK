 sich, auch ohne Gold;", "tokens": ["Reich", "in", "sich", ",", "auch", "oh\u00b7ne", "Gold", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sein Leben zwar verborgen,", "tokens": ["Da\u00df", "sein", "Le\u00b7ben", "zwar", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber ohne Sklaverey,", "tokens": ["A\u00b7ber", "oh\u00b7ne", "Skla\u00b7ve\u00b7rey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne Flecken, ohne Sorgen,", "tokens": ["Oh\u00b7ne", "Fle\u00b7cken", ",", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weisen Freunden theuer sey!", "tokens": ["Wei\u00b7sen", "Freun\u00b7den", "theu\u00b7er", "sey", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}