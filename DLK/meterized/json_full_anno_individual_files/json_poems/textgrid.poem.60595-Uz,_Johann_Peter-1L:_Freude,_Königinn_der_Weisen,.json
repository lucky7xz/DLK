": {"text": "Trotz dem S\u00fcdwind, unter Wettern,", "tokens": ["Trotz", "dem", "S\u00fcd\u00b7wind", ",", "un\u00b7ter", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann die Wolken Flammen spr\u00fchn:", "tokens": ["Wann", "die", "Wol\u00b7ken", "Flam\u00b7men", "spr\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die dein lockicht Haar durchschlingen,", "tokens": ["Die", "dein", "lo\u00b7ckicht", "Haar", "durch\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht nur an Cytherens Brust,", "tokens": ["Nicht", "nur", "an", "Cy\u00b7the\u00b7rens", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wann die Grazien dir singen,", "tokens": ["Wann", "die", "Gra\u00b7zi\u00b7en", "dir", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder bey Ly\u00e4ens Lust.", "tokens": ["O\u00b7der", "bey", "Ly\u00b7\u00e4\u00b7ens", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sie bekr\u00e4nzen dich in Zeiten,", "tokens": ["Sie", "be\u00b7kr\u00e4n\u00b7zen", "dich", "in", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die kein Sonnenblick erhellt,", "tokens": ["Die", "kein", "Son\u00b7nen\u00b7blick", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sahen dich das Gl\u00fcck bestreiten,", "tokens": ["Sa\u00b7hen", "dich", "das", "Gl\u00fcck", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Tyrannen unsrer Welt,", "tokens": ["Den", "Ty\u00b7ran\u00b7nen", "uns\u00b7rer", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der um seine Riesenglieder", "tokens": ["Der", "um", "sei\u00b7ne", "Rie\u00b7sen\u00b7glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Donnerndes Gew\u00f6lke zog,", "tokens": ["Don\u00b7nern\u00b7des", "Ge\u00b7w\u00f6l\u00b7ke", "zog", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit schrecklichem Gefieder", "tokens": ["Und", "mit", "schreck\u00b7li\u00b7chem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zwischen Erd und Himmel flog.", "tokens": ["Zwi\u00b7schen", "Erd", "und", "Him\u00b7mel", "flog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Dich und deine Rosen sahen", "tokens": ["Dich", "und", "dei\u00b7ne", "Ro\u00b7sen", "sa\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die Gegenden der Nacht", "tokens": ["Auch", "die", "Ge\u00b7gen\u00b7den", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich des Todes Throne nahen,", "tokens": ["Sich", "des", "To\u00b7des", "Thro\u00b7ne", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo das kalte Schrecken wacht.", "tokens": ["Wo", "das", "kal\u00b7te", "Schre\u00b7cken", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deinen Pfad, wo du gegangen,", "tokens": ["Dei\u00b7nen", "Pfad", ",", "wo", "du", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeichnete das sanfte Licht", "tokens": ["Zeich\u00b7ne\u00b7te", "das", "sanf\u00b7te", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Cynthiens mit vollen Wangen,", "tokens": ["Cyn\u00b7thiens", "mit", "vol\u00b7len", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die durch schwarze Schatten bricht.", "tokens": ["Die", "durch", "schwar\u00b7ze", "Schat\u00b7ten", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Dir war dieser Herr des Lebens", "tokens": ["Dir", "war", "die\u00b7ser", "Herr", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War der Tod nicht f\u00fcrchterlich,", "tokens": ["War", "der", "Tod", "nicht", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er schwenkete vergebens", "tokens": ["Und", "er", "schwen\u00b7ke\u00b7te", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Wurfspie\u00df wider dich:", "tokens": ["Sei\u00b7nen", "Wurf\u00b7spie\u00df", "wi\u00b7der", "dich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil im traurigen Gefilde", "tokens": ["Weil", "im", "trau\u00b7ri\u00b7gen", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hoffnung dir zur Seite gieng,", "tokens": ["Hoff\u00b7nung", "dir", "zur", "Sei\u00b7te", "gieng", ","], "token_info": ["word", "word", "word", "word", "w