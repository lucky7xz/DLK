{"textgrid.poem.34217": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine Belladonna", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Belladonna, du stehst hier mitten zwischen den Dornen,", "tokens": ["Bel\u00b7la\u00b7don\u00b7na", ",", "du", "stehst", "hier", "mit\u00b7ten", "zwi\u00b7schen", "den", "Dor\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-++-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Darum zertret' ich dich nicht, gr\u00fcne und bl\u00fche nur fort!", "tokens": ["Da\u00b7rum", "zer\u00b7tret'", "ich", "dich", "nicht", ",", "gr\u00fc\u00b7ne", "und", "bl\u00fc\u00b7he", "nur", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PTKNEG", "$,", "ADJA", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Jene halten ja Wache und wehren dem l\u00fcsternen Kinde,", "tokens": ["Je\u00b7ne", "hal\u00b7ten", "ja", "Wa\u00b7che", "und", "weh\u00b7ren", "dem", "l\u00fcs\u00b7ter\u00b7nen", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Wie es die Dolde auch lockt, wie es die Beere auch reizt.", "tokens": ["Wie", "es", "die", "Dol\u00b7de", "auch", "lockt", ",", "wie", "es", "die", "Bee\u00b7re", "auch", "reizt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "wo