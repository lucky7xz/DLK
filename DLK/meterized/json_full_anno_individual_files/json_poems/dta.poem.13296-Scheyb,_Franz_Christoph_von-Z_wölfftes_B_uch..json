{"dta.poem.13296": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6rte, da\u00df man sich auf eine Reise freue,", "tokens": ["Ich", "h\u00f6r\u00b7te", ",", "da\u00df", "man", "sich", "auf", "ei\u00b7ne", "Rei\u00b7se", "freu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df ", "tokens": ["Und", "da\u00df"], "token_info": ["word", "word"], "pos": ["KON", "KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sie redten was von Pomp, von Schiffen und von Pracht;", "tokens": ["Sie", "red\u00b7ten", "was", "von", "Pomp", ",", "von", "Schif\u00b7fen", "und", "von", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NE", "$,", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "170Jedoch verstund ich nicht, was man vor Anstallt macht.", "tokens": ["ver\u00b7stund", "ich", "nicht", ",", "was", "man", "vor", "An\u00b7stallt", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}