{"dta.poem.13304": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eja\u201e, sagten sie, das hat die K\u00f6niginn im Sinn;", "tokens": ["\u201e", "ja", "\u201e", ",", "sag\u00b7ten", "sie", ",", "das", "hat", "die", "K\u00f6\u00b7ni\u00b7ginn", "im", "Sinn", ";"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eweil ich\u201e, bracht Eine vor, dabey gewesen bin,", "tokens": ["\u201e", "weil", "ich", "\u201e", ",", "bracht", "Ei\u00b7ne", "vor", ",", "da\u00b7bey", "ge\u00b7we\u00b7sen", "bin", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "$(", "$,", "VVFIN", "ART", "PTKVZ", "$,", "PAV", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eals sie von ungef\u00e4hr von dieser Reise sprache,", "tokens": ["\u201e", "als", "sie", "von", "un\u00b7ge\u00b7f\u00e4hr", "von", "die\u00b7ser", "Rei\u00b7se", "spra\u00b7che", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ejedoch die Deutlichkeit der Reden unterbrache;", "tokens": ["\u201e", "je\u00b7doch", "die", "Deut\u00b7lich\u00b7keit", "der", "Re\u00b7den", "un\u00b7ter\u00b7bra\u00b7che", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Mit was vor ausserordentlichen/\nin keiner Geschichte befindlichen/ allge-\nmeinen Freudens-Bezeugungen; mit\nwas vor Beleuchtungen und herrli-\nchen Triumpf-Geb\u00e4uden diese allhier\nVorbedeutungs-weis angef\u00fchrte Zu-\n\nr\u00fcckkunft an allen deutsch- und treu-\ngesinnten Orten/ insonderheit allhier\nden 24. Octob. 1745. sey gefeyret wor-\nden/ wird durch die noch frische un-\nausl\u00f6schliche Erinnerung best\u00e4ttiget.", "tokens": ["Mit", "was", "vor", "aus\u00b7ser\u00b7or\u00b7dent\u00b7li\u00b7chen", "/", "in", "kei\u00b7ner", "Ge\u00b7schich\u00b7te", "be\u00b7find\u00b7li\u00b7chen", "/", "all\u00b7ge", "mei\u00b7nen", "Freu\u00b7den\u00b7sBe\u00b7zeu\u00b7gun\u00b7gen", ";", "mit", "was", "vor", "Be\u00b7leuch\u00b7tun\u00b7gen", "und", "herr\u00b7li", "chen", "Tri\u00b7umpf\u00b7Ge\u00b7b\u00e4u\u00b7den", "die\u00b7se", "all\u00b7hier", "Vor\u00b7be\u00b7deu\u00b7tungs\u00b7weis", "an\u00b7ge\u00b7f\u00fchr\u00b7te", "Zu", "r\u00fcck\u00b7kunft", "an", "al\u00b7len", "deut\u00b7sch", "und", "treu", "ge\u00b7sinn\u00b7ten", "Or\u00b7ten", "/", "in\u00b7son\u00b7der\u00b7heit", "all\u00b7hier", "den", "24.", "Oc\u00b7tob", ".", "1745", ".", "sey", "ge\u00b7fey\u00b7ret", "wor", "den", "/", "wird", "durch", "die", "noch", "fri\u00b7sche", "un", "aus\u00b7l\u00f6schli\u00b7che", "E\u00b7rin\u00b7ne\u00b7rung", "be\u00b7st\u00e4t\u00b7ti\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "ordinal", "word", "punct", "number", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "VVINF", "$(", "APPR", "PIAT", "NN", "VVINF", "$(", "TRUNC", "PPOSAT", "NN", "$.", "APPR", "PRELS", "APPR", "NN", "KON", "TRUNC", "ADJA", "NN", "PDAT", "ADJA", "NN", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "TRUNC", "KON", "TRUNC", "ADJA", "NN", "$(", "ADV", "ADV", "ART", "ADJA", "NN", "$.", "CARD", "$.", "VAFIN", "VVPP", "TRUNC", "ART", "$(", "VAFIN", "APPR", "ART", "ADV", "ADJA", "TRUNC", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+--+--+--+-+-+-+-+-+-+-+-+-+--+--+-+-+-+-+-+--+--+--+--+-+-+-+--+-+-+-+-+-+-+-+-+-+-+-+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "265\u201dSo scheint es wahr zu seyn. Doch wei\u00df ich nichts gewi\u00df,", "tokens": ["\"", "So", "scheint", "es", "wahr", "zu", "seyn", ".", "Doch", "wei\u00df", "ich", "nichts", "ge\u00b7wi\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VAINF", "$.", "KON", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eweil sie die Richtigkeit uns nicht erfahren lie\u00df.", "tokens": ["\u201e", "weil", "sie", "die", "Rich\u00b7tig\u00b7keit", "uns", "nicht", "er\u00b7fah\u00b7ren", "lie\u00df", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "PPER", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}