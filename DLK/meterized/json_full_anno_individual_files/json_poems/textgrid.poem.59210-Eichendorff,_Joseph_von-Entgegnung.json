{"textgrid.poem.59210": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Entgegnung", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsei antik doch, sei teutonisch,", "tokens": ["\u00bb", "sei", "an\u00b7tik", "doch", ",", "sei", "teu\u00b7to\u00b7nisch", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "ADV", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lern, skandiere unverdrossen,", "tokens": ["Lern", ",", "skan\u00b7die\u00b7re", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freundchen, aber nur ironisch!", "tokens": ["Freund\u00b7chen", ",", "a\u00b7ber", "nur", "i\u00b7ron\u00b7isch", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und vor allem la\u00df die Possen,", "tokens": ["Und", "vor", "al\u00b7lem", "la\u00df", "die", "Pos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die man sonst genannt: romantisch.\u00ab \u2013", "tokens": ["Die", "man", "sonst", "ge\u00b7nannt", ":", "ro\u00b7man\u00b7tisch", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["ART", "PIS", "ADV", "VVPP", "$.", "ADJD", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Also h\u00f6rt man's ringsher schallen;", "tokens": ["Al\u00b7so", "h\u00f6rt", "man's", "rings\u00b7her", "schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber mich bed\u00fcnkt: pedantisch,", "tokens": ["A\u00b7ber", "mich", "be\u00b7d\u00fcnkt", ":", "pe\u00b7dan\u00b7tisch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sei das Schlimmste doch von allen.", "tokens": ["Sei", "das", "Schlimms\u00b7te", "doch", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wem der Herr den Kranz gewunden,", "tokens": ["Wem", "der", "Herr", "den", "Kranz", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird nach alledem nicht fragen,", "tokens": ["Wird", "nach", "al\u00b7le\u00b7dem", "nicht", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sondern mu\u00df, wie er's befunden,", "tokens": ["Son\u00b7dern", "mu\u00df", ",", "wie", "er's", "be\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "PWAV", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die eigne Weise sagen,", "tokens": ["Auf", "die", "eig\u00b7ne", "Wei\u00b7se", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stets aufs neu mit freud'gem Schrecken,", "tokens": ["Stets", "aufs", "neu", "mit", "freu\u00b7d'\u00b7gem", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ist sie auch die alte blieben,", "tokens": ["Ist", "sie", "auch", "die", "al\u00b7te", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich die sch\u00f6ne Welt entdecken,", "tokens": ["Sich", "die", "sch\u00f6\u00b7ne", "Welt", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewig jung ist, was wir lieben!", "tokens": ["E\u00b7wig", "jung", "ist", ",", "was", "wir", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oft durch des Theaters Ritzen", "tokens": ["Oft", "durch", "des", "The\u00b7a\u00b7ters", "Rit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bricht's mit wunderbarem Lichte,", "tokens": ["Bricht's", "mit", "wun\u00b7der\u00b7ba\u00b7rem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der Herr in feur'gen Blitzen", "tokens": ["Wenn", "der", "Herr", "in", "feur'\u00b7gen", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dichtend schreibt die Weltgeschichte,", "tokens": ["Dich\u00b7tend", "schreibt", "die", "Welt\u00b7ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das ist der Klang der Wehmut,", "tokens": ["Und", "das", "ist", "der", "Klang", "der", "Weh\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der durch alle Dichtergeister", "tokens": ["Der", "durch", "al\u00b7le", "Dich\u00b7ter\u00b7geis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schauernd geht, wenn sie in Demut", "tokens": ["Schau\u00b7ernd", "geht", ",", "wenn", "sie", "in", "De\u00b7mut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00dcber sich erkannt den Meister.", "tokens": ["\u00dc\u00b7ber", "sich", "er\u00b7kannt", "den", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbsei antik doch, sei teutonisch,", "tokens": ["