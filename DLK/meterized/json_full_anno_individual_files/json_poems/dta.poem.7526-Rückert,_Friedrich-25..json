{"dta.poem.7526": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich freue jeden Tag dem Abend mich entgegen,", "tokens": ["Ich", "freu\u00b7e", "je\u00b7den", "Tag", "dem", "A\u00b7bend", "mich", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jede Nacht im Traum mich auf den Morgensegen.", "tokens": ["Und", "je\u00b7de", "Nacht", "im", "Traum", "mich", "auf", "den", "Mor\u00b7gen\u00b7se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPRART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich freue still mich mit unungest\u00fcmer Lust,", "tokens": ["Ich", "freu\u00b7e", "still", "mich", "mit", "un\u00b7un\u00b7ge\u00b7st\u00fc\u00b7mer", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht ungeduldig ist die Freud' in meiner Brust.", "tokens": ["Nicht", "un\u00b7ge\u00b7dul\u00b7dig", "ist", "die", "Freud'", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich freu' mich auf die Stund' und auf den Augenblick,", "tokens": ["Ich", "freu'", "mich", "auf", "die", "Stund'", "und", "auf", "den", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf gro\u00df und kleines, mein und anderer Geschick.", "tokens": ["Auf", "gro\u00df", "und", "klei\u00b7nes", ",", "mein", "und", "an\u00b7de\u00b7rer", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJA", "$,", "PPOSAT", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vom Herbst den Winter durch freu' ich dem Lenz mich zu,", "tokens": ["Vom", "Herbst", "den", "Win\u00b7ter", "durch", "freu'", "ich", "dem", "Lenz", "mich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "VVFIN", "PPER", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aus dem Sommer durch den Herbst zur Winterruh.", "tokens": ["Und", "aus", "dem", "Som\u00b7mer", "durch", "den", "Herbst", "zur", "Win\u00b7ter\u00b7ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich freu' mich durch des Jahrs und durch des Lebens Zeit,", "tokens": ["Ich", "freu'", "mich", "durch", "des", "Jahrs", "und", "durch", "des", "Le\u00b7bens", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aus der Zeit hinaus mich in die Ewigkeit.", "tokens": ["Und", "aus", "der", "Zeit", "hin\u00b7aus", "mich", "in", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APZR", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}