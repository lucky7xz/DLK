{"textgrid.poem.44101": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gedenck auch nun einmahl, getreue Poesie,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gedenck auch nun einmahl, getreue Poesie,", "tokens": ["Ge\u00b7denck", "auch", "nun", "ein\u00b7mahl", ",", "ge\u00b7treu\u00b7e", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Sachen, die nicht so nach Welt und Thorheit schmecken,", "tokens": ["An", "Sa\u00b7chen", ",", "die", "nicht", "so", "nach", "Welt", "und", "Thor\u00b7heit", "schme\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und leide, da\u00df mein Fu\u00df dich von dem Wege zieh,", "tokens": ["Und", "lei\u00b7de", ",", "da\u00df", "mein", "Fu\u00df", "dich", "von", "dem", "We\u00b7ge", "zieh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf welchem Lust und Schein den Untergang verdecken.", "tokens": ["Auf", "wel\u00b7chem", "Lust", "und", "Schein", "den", "Un\u00b7ter\u00b7gang", "ver\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man r\u00fchmt dir allzeit nach, du seyst ein Himmelskind,", "tokens": ["Man", "r\u00fchmt", "dir", "all\u00b7zeit", "nach", ",", "du", "seyst", "ein", "Him\u00b7mel\u00b7skind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gieb th\u00e4tigen Beweis, dein Vaterland zu glauben;", "tokens": ["Gieb", "th\u00e4\u00b7ti\u00b7gen", "Be\u00b7weis", ",", "dein", "Va\u00b7ter\u00b7land", "zu", "glau\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nachdem Geschmack, Geruch und Farb und W\u00fcrckung sind,", "tokens": ["Nach\u00b7dem", "Ge\u00b7schmack", ",", "Ge\u00b7ruch", "und", "Farb", "und", "W\u00fcr\u00b7ckung", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Nachdem erf\u00e4hrt man auch den Boden reifer Trauben.", "tokens": ["Nach\u00b7dem", "er\u00b7f\u00e4hrt", "man", "auch", "den", "Bo\u00b7den", "rei\u00b7fer", "Trau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PIS", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du hast der Eitelkeit so dienstbahr aufgespielt,", "tokens": ["Du", "hast", "der", "Ei\u00b7tel\u00b7keit", "so", "dienst\u00b7bahr", "auf\u00b7ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Viel Feuer angesteckt, manch schl\u00fcpfrig Lied geschrieben", "tokens": ["Viel", "Feu\u00b7er", "an\u00b7ge\u00b7steckt", ",", "manch", "schl\u00fcpf\u00b7rig", "Lied", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVPP", "$,", "PIAT", "ADJD", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und manchen reichen Thor, der sonst sich anders f\u00fchlt,", "tokens": ["Und", "man\u00b7chen", "rei\u00b7chen", "Thor", ",", "der", "sonst", "sich", "an\u00b7ders", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,", "PRELS", "ADV", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch Lob und Schmeicheley zum Hochmuth angetrieben;", "tokens": ["Durch", "Lob", "und", "Schmei\u00b7che\u00b7ley", "zum", "Hoch\u00b7muth", "an\u00b7ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die S\u00fcnd ist zwar nicht klein, doch wird sie leicht verziehn,", "tokens": ["Die", "S\u00fcnd", "ist", "zwar", "nicht", "klein", ",", "doch", "wird", "sie", "leicht", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn Bu\u00df und Be\u00dferung die Arbeit heilig machen.", "tokens": ["Wenn", "Bu\u00df", "und", "Be\u00b7\u00dfe\u00b7rung", "die", "Ar\u00b7beit", "hei\u00b7lig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du must dein Saythenchor nach Davids Harfe ziehn,", "tokens": ["Du", "must", "dein", "Say\u00b7then\u00b7chor", "nach", "Da\u00b7vids", "Har\u00b7fe", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "O was bekommstu hier vor gro\u00df- und hohe Sachen!", "tokens": ["O", "was", "be\u00b7komms\u00b7tu", "hier", "vor", "gro\u00df", "und", "ho\u00b7he", "Sa\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "ADV", "APPR", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kein Maro, kein Homer, kein hoher Pindarus", "tokens": ["Kein", "Ma\u00b7ro", ",", "kein", "Ho\u00b7mer", ",", "kein", "ho\u00b7her", "Pin\u00b7da\u00b7rus"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NE", "$,", "PIAT", "NE", "$,", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.18": {"text": "Hat vor sein Heldenlied so reich- und starcken Zunder;", "tokens": ["Hat", "vor", "sein", "Hel\u00b7den\u00b7lied", "so", "reich", "und", "star\u00b7cken", "Zun\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "ADV", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du brauchst nicht erst den Geist, der jene treiben mu\u00df,", "tokens": ["Du", "brauchst", "nicht", "erst", "den", "Geist", ",", "der", "je\u00b7ne", "trei\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,", "PRELS", "PDS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Betracht und sch\u00e4ze nur des H\u00f6chsten Werck und Wunder!", "tokens": ["Be\u00b7tracht", "und", "sch\u00e4\u00b7ze", "nur", "des", "H\u00f6chs\u00b7ten", "Werck", "und", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du bist zu sehr verw\u00f6hnt und hast ein th\u00f6richt Ohr,", "tokens": ["Du", "bist", "zu", "sehr", "ver\u00b7w\u00f6hnt", "und", "hast", "ein", "th\u00f6\u00b7richt", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADV", "VVPP", "KON", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wofern dir Juppiter und Venus be\u00dfer klingen,", "tokens": ["Wo\u00b7fern", "dir", "Jup\u00b7pi\u00b7ter", "und", "Ve\u00b7nus", "be\u00b7\u00dfer", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Als wenn die Sulamith und Assaphs g\u00fcldnes Rohr", "tokens": ["Als", "wenn", "die", "Su\u00b7la\u00b7mith", "und", "As\u00b7saphs", "g\u00fcld\u00b7nes", "Rohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "KON", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vom gro\u00dfen Zebaoth und sch\u00f6nen Freunde singen.", "tokens": ["Vom", "gro\u00b7\u00dfen", "Ze\u00b7bao\u00b7th", "und", "sch\u00f6\u00b7nen", "Freun\u00b7de", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Liegt Elims Palmenstadt nicht h\u00f6her als Athen?", "tokens": ["Liegt", "E\u00b7lims", "Pal\u00b7men\u00b7stadt", "nicht", "h\u00f6\u00b7her", "als", "A\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "PTKNEG", "ADJD", "KOKOM", "NE", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Besch\u00e4mt nicht Hermons Thau des Pindus G\u00f6zenh\u00fcgel?", "tokens": ["Be\u00b7sch\u00e4mt", "nicht", "Her\u00b7mons", "Thau", "des", "Pin\u00b7dus", "G\u00f6\u00b7zen\u00b7h\u00fc\u00b7gel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Aurora macht den Vers bey weitem nicht so sch\u00f6n,", "tokens": ["Au\u00b7ro\u00b7ra", "macht", "den", "Vers", "bey", "wei\u00b7tem", "nicht", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "PIS", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Als wenn ihm David w\u00fcntscht der Morgenr\u00f6the Fl\u00fcgel.", "tokens": ["Als", "wenn", "ihm", "Da\u00b7vid", "w\u00fcnt\u00b7scht", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NE", "VVFIN", "ART", "NN", "NN", "$."], "meter": "----+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.29": {"text": "Was giebt Elysien? Verlogne Frucht und Lust.", "tokens": ["Was", "giebt", "E\u00b7ly\u00b7si\u00b7en", "?", "Ver\u00b7log\u00b7ne", "Frucht", "und", "Lust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "$.", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Komm, las dir Gottes Stadt vom liebsten J\u00fcnger zeigen;", "tokens": ["Komm", ",", "las", "dir", "Got\u00b7tes", "Stadt", "vom", "liebs\u00b7ten", "J\u00fcn\u00b7ger", "zei\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "NN", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ihr Schatten wirft dir schon viel Klarheit in die Brust,", "tokens": ["Ihr", "Schat\u00b7ten", "wirft", "dir", "schon", "viel", "Klar\u00b7heit", "in", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und was du hier gewinnst, das ist ein sehnlich Schweigen.", "tokens": ["Und", "was", "du", "hier", "ge\u00b7winnst", ",", "das", "ist", "ein", "sehn\u00b7lich", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,", "PDS", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie, sch\u00e4rfstu schon den Kiel zum Ri\u00dfe dieser Pracht?", "tokens": ["Wie", ",", "sch\u00e4rfs\u00b7tu", "schon", "den", "Kiel", "zum", "Ri\u00b7\u00dfe", "die\u00b7ser", "Pracht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "ADV", "ART", "NE", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sie l\u00e4st sich nicht so wohl erzehlen als genie\u00dfen;", "tokens": ["Sie", "l\u00e4st", "sich", "nicht", "so", "wohl", "er\u00b7zeh\u00b7len", "als", "ge\u00b7nie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "ADV", "ADV", "VVINF", "KOKOM", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Auch dazu weis ich Rath, komm mit und gieb fein Acht,", "tokens": ["Auch", "da\u00b7zu", "weis", "ich", "Rath", ",", "komm", "mit", "und", "gieb", "fein", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "PTKVZ", "PPER", "NN", "$,", "VVFIN", "APPR", "KON", "VVIMP", "ADJD", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Was dort auf Golgatha vor Seegensstr\u00f6me flie\u00dfen.", "tokens": ["Was", "dort", "auf", "Gol\u00b7ga\u00b7tha", "vor", "See\u00b7gens\u00b7str\u00f6\u00b7me", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NE", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es ist das rothe Meer in jen' gelobtes Land,", "tokens": ["Es", "ist", "das", "ro\u00b7the", "Meer", "in", "jen'", "ge\u00b7lob\u00b7tes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das unser Josua am Creuze scharf erfochten;", "tokens": ["Das", "un\u00b7ser", "Jo\u00b7sua", "am", "Creu\u00b7ze", "scharf", "er\u00b7foch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Hier \u00fcbe deine Kunst, hier wecke Geist und Hand,", "tokens": ["Hier", "\u00fc\u00b7be", "dei\u00b7ne", "Kunst", ",", "hier", "we\u00b7cke", "Geist", "und", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Zerrei\u00df auch, was mir sonst der Helicon geflochten;", "tokens": ["Zer\u00b7rei\u00df", "auch", ",", "was", "mir", "sonst", "der", "He\u00b7li\u00b7con", "ge\u00b7floch\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWS", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "li