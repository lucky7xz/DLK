{"dta.poem.2897": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "98.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir alle sind get\u00e4uscht von einer Zauberbinde,", "tokens": ["Wir", "al\u00b7le", "sind", "ge\u00b7t\u00e4uscht", "von", "ei\u00b7ner", "Zau\u00b7ber\u00b7bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die w\u00e4hnen, da\u00df die Lieb' auf Erden G\u00f6ttlich's finde.", "tokens": ["Die", "w\u00e4h\u00b7nen", ",", "da\u00df", "die", "Lieb'", "auf", "Er\u00b7den", "G\u00f6tt\u00b7lich's", "fin\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "ART", "NN", "APPR", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was lachst du \u00fcber den, der minder sch\u00f6n's erkor?", "tokens": ["Was", "lachst", "du", "\u00fc\u00b7ber", "den", ",", "der", "min\u00b7der", "sch\u00f6n's", "er\u00b7kor", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "$,", "PRELS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Binde schwebet ihm nur etwas dichter vor.", "tokens": ["Die", "Bin\u00b7de", "schwe\u00b7bet", "ihm", "nur", "et\u00b7was", "dich\u00b7ter", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "AD