{"textgrid.poem.42227": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Verg\u00e4nglichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Streck deine Beine, mein h\u00fcbscher Geno\u00df;", "tokens": ["Streck", "dei\u00b7ne", "Bei\u00b7ne", ",", "mein", "h\u00fcb\u00b7scher", "Ge\u00b7no\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Deine schwarzen Str\u00fcmpfe aus Fil d'Ecosse", "tokens": ["Dei\u00b7ne", "schwar\u00b7zen", "Str\u00fcmp\u00b7fe", "aus", "Fil", "d'\u00b7E\u00b7cos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NE", "NE"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Reichen dir weit bis \u00fcber die Kniee,", "tokens": ["Rei\u00b7chen", "dir", "weit", "bis", "\u00fc\u00b7ber", "die", "Kni\u00b7ee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wenn ich sie dir nicht noch h\u00f6her ziehe. \u2013", "tokens": ["Wenn", "ich", "sie", "dir", "nicht", "noch", "h\u00f6\u00b7her", "zie\u00b7he", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "PTKNEG", "ADV", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sie sind das Verf\u00e4nglichste wohl an dir,", "tokens": ["Sie", "sind", "das", "Ver\u00b7f\u00e4ng\u00b7lichs\u00b7te", "wohl", "an", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Deine schwarzen Str\u00fcmpfe; ich sterbe daf\u00fcr.", "tokens": ["Dei\u00b7ne", "schwar\u00b7zen", "Str\u00fcmp\u00b7fe", ";", "ich", "ster\u00b7be", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "PAV", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Hell schimmert die Haut durch die weiten Maschen,", "tokens": ["Hell", "schim\u00b7mert", "die", "Haut", "durch", "die", "wei\u00b7ten", "Ma\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Man m\u00f6chte von au\u00dfen schon daran naschen.", "tokens": ["Man", "m\u00f6ch\u00b7te", "von", "au\u00b7\u00dfen", "schon", "da\u00b7ran", "na\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "ADV", "ADV", "PAV", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Dabei legst du deine F\u00fc\u00dfe so friedlich", "tokens": ["Da\u00b7bei", "legst", "du", "dei\u00b7ne", "F\u00fc\u00b7\u00dfe", "so", "fried\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00dcbereinander, die blanken Lackschuhe appetitlich", "tokens": ["\u00dc\u00b7be\u00b7re\u00b7in\u00b7an\u00b7der", ",", "die", "blan\u00b7ken", "Lack\u00b7schu\u00b7he", "ap\u00b7pe\u00b7tit\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "ADJD"], "meter": "+---+--+-++-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Gestreckt \u2013 die Seligkeit, sie dir zu binden,", "tokens": ["Ge\u00b7streckt", "\u2013", "die", "Se\u00b7lig\u00b7keit", ",", "sie", "dir", "zu", "bin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "ART", "NN", "$,", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kann im Himmel nicht ihresgleichen finden.", "tokens": ["Kann", "im", "Him\u00b7mel", "nicht", "ih\u00b7res\u00b7glei\u00b7chen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Dein schwarzer Lockenkopf, deine blassen Wangen,", "tokens": ["Dein", "schwar\u00b7zer", "Lo\u00b7cken\u00b7kopf", ",", "dei\u00b7ne", "blas\u00b7sen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein splitternackter Mund, deine bangen", "tokens": ["Dein", "split\u00b7ter\u00b7nack\u00b7ter", "Mund", ",", "dei\u00b7ne", "ban\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tiefschwarzen Augen sind eine Pracht,", "tokens": ["Tief\u00b7schwar\u00b7zen", "Au\u00b7gen", "sind", "ei\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch haben nicht sie mich verr\u00fcckt gemacht.", "tokens": ["Doch", "ha\u00b7ben", "nicht", "sie", "mich", "ver\u00b7r\u00fcckt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Deine Unwiderstehlichkeit liegt in den Beinen.", "tokens": ["Dei\u00b7ne", "Un\u00b7wi\u00b7der\u00b7steh\u00b7lich\u00b7keit", "liegt", "in", "den", "Bei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Seh ich dich kommen, so m\u00f6cht ich weinen.", "tokens": ["Seh", "ich", "dich", "kom\u00b7men", ",", "so", "m\u00f6cht", "ich", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Du hebst die Knie in einem Takt,", "tokens": ["Du", "hebst", "die", "Knie", "in", "ei\u00b7nem", "Takt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der w\u00fcrgend mich an der Kehle packt.", "tokens": ["Der", "w\u00fcr\u00b7gend", "mich", "an", "der", "Keh\u00b7le", "packt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich will dir zum ewigen Angedenken", "tokens": ["Ich", "will", "dir", "zum", "e\u00b7wi\u00b7gen", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein Paar Strumpfb\u00e4nder in zartem Lila schenken", "tokens": ["Ein", "Paar", "Strumpf\u00b7b\u00e4n\u00b7der", "in", "zar\u00b7tem", "Li\u00b7la", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit goldenem Wappen, denn du bist in der Tat", "tokens": ["Mit", "gol\u00b7de\u00b7nem", "Wap\u00b7pen", ",", "denn", "du", "bist", "in", "der", "Tat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein M\u00e4dchen und ein junger Aristokrat.", "tokens": ["Ein", "M\u00e4d\u00b7chen", "und", "ein", "jun\u00b7ger", "A\u00b7ris\u00b7to\u00b7krat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.7": {"line.1": {"text": "Ein Knabe, der in seiner Anmut nicht leidet,", "tokens": ["Ein", "Kna\u00b7be", ",", "der", "in", "sei\u00b7ner", "An\u00b7mut", "nicht", "lei\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn er sich zuweilen als M\u00e4dchen verkleidet;", "tokens": ["Wenn", "er", "sich", "zu\u00b7wei\u00b7len", "als", "M\u00e4d\u00b7chen", "ver\u00b7klei\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "KOUS", "NN", "VVPP", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aber deine Mutter sagt mir, du seist", "tokens": ["A\u00b7ber", "dei\u00b7ne", "Mut\u00b7ter", "sagt", "mir", ",", "du", "seist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "PPER", "VAFIN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Durchdrungen von ritterlichem Geist.", "tokens": ["Durch\u00b7drun\u00b7gen", "von", "rit\u00b7ter\u00b7li\u00b7chem", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Du bestehest mit Glanz die schwierigsten Examen", "tokens": ["Du", "be\u00b7ste\u00b7hest", "mit", "Glanz", "die", "schwie\u00b7rigs\u00b7ten", "E\u00b7xa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und schw\u00e4rmest auch schon f\u00fcr die allerreizendsten Damen.", "tokens": ["Und", "schw\u00e4r\u00b7mest", "auch", "schon", "f\u00fcr", "die", "al\u00b7ler\u00b7rei\u00b7zends\u00b7ten", "Da\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Niemand glaubt mir in dieser Welt,", "tokens": ["Nie\u00b7mand", "glaubt", "mir", "in", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Wie mir das an dir, meinem Sch\u00fctzling gef\u00e4llt.", "tokens": ["Wie", "mir", "das", "an", "dir", ",", "mei\u00b7nem", "Sch\u00fctz\u00b7ling", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDS", "APPR", "PPER", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.9": {"line.1": {"text": "Noch bist du Cherub. Wenige Wochen,", "tokens": ["Noch", "bist", "du", "Che\u00b7rub", ".", "We\u00b7ni\u00b7ge", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "$.", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann ist wohl die Knospe schon aufgebrochen;", "tokens": ["Dann", "ist", "wohl", "die", "Knos\u00b7pe", "schon", "auf\u00b7ge\u00b7bro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dann blickst du mit grimmem Schauder auf mich,", "tokens": ["Dann", "blickst", "du", "mit", "grim\u00b7mem", "Schau\u00b7der", "auf", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+--+-+---", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der dir so z\u00e4rtlich die Locken strich.", "tokens": ["Der", "dir", "so", "z\u00e4rt\u00b7lich", "die", "Lo\u00b7cken", "strich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "