{"dta.poem.14718": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Unbekannte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da stand der Wandrer auf, es bl\u00fchten Sterne", "tokens": ["Da", "stand", "der", "Wand\u00b7rer", "auf", ",", "es", "bl\u00fch\u00b7ten", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon aus dem Dunkel \u00fcber'm stillen Land,", "tokens": ["Schon", "aus", "dem", "Dun\u00b7kel", "\u00fc\u00b7ber'm", "stil\u00b7len", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "\u201egesegn' euch Gott! mein Heimathland liegt ferne. \u2014\u201c", "tokens": ["\u201e", "ge\u00b7segn'", "euch", "Gott", "!", "mein", "Hei\u00b7mat\u00b7hland", "liegt", "fer\u00b7ne", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und als er von den beiden sich gewandt,", "tokens": ["Und", "als", "er", "von", "den", "bei\u00b7den", "sich", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "PIAT", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kam himmlisch Klingen von der Waldeswiese \u2014", "tokens": ["Kam", "himm\u00b7lisch", "Klin\u00b7gen", "von", "der", "Wal\u00b7des\u00b7wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So sternklar war noch keine Nacht wie diese.", "tokens": ["So", "stern\u00b7klar", "war", "noch", "kei\u00b7ne", "Nacht", "wie", "die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "PIAT", "NN", "KOKOM", "PDS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}