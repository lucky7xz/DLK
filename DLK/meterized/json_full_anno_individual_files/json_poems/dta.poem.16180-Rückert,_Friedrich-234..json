{"dta.poem.16180": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "234.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Lehrer lehrt dich, da\u00df es keine Wahrheit gebe,", "tokens": ["Ein", "Leh\u00b7rer", "lehrt", "dich", ",", "da\u00df", "es", "kei\u00b7ne", "Wahr\u00b7heit", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und geb' es eine, sie doch unerkennbar schwebe,", "tokens": ["Und", "geb'", "es", "ei\u00b7ne", ",", "sie", "doch", "un\u00b7er\u00b7kenn\u00b7bar", "schwe\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "$,", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn erkennbar, sei sie doch nicht mitzutheilen.", "tokens": ["Und", "wenn", "er\u00b7kenn\u00b7bar", ",", "sei", "sie", "doch", "nicht", "mit\u00b7zu\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was kann den Lehrling vom dreifachen Zweifel heilen?", "tokens": ["Was", "kann", "den", "Lehr\u00b7ling", "vom", "drei\u00b7fa\u00b7chen", "Zwei\u00b7fel", "hei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Des Lehrers Lehre selbst, die er als wahr ausspricht;", "tokens": ["Des", "Leh\u00b7rers", "Leh\u00b7re", "selbst", ",", "die", "er", "als", "wahr", "aus\u00b7spricht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$,", "PRELS", "PPER", "KOUS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn, seiner Lehre nach, ist sie auch Wahrheit nicht.", "tokens": ["Denn", ",", "sei\u00b7ner", "Leh\u00b7re", "nach", ",", "ist", "sie", "auch", "Wahr\u00b7heit", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "PTKVZ", "$,", "VAFIN", "PPER", "ADV", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nun wenn nicht dis, so ist das Gegentheil denn wahr,", "tokens": ["Nun", "wenn", "nicht", "dis", ",", "so", "ist", "das", "Ge\u00b7gen\u00b7theil", "denn", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PTKNEG", "PDS", "$,", "ADV", "VAFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df eine Wahrheit sei, erkenn- und mittheilbar.", "tokens": ["Da\u00df", "ei\u00b7ne", "Wahr\u00b7heit", "sei", ",", "er\u00b7kenn", "und", "mit\u00b7theil\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$,", "TRUNC", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}