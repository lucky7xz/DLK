{"textgrid.poem.61111": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "11. Jahreszeiten in Athen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimmer den Sommer verweil' in Athen. Glutvollen Sirocco", "tokens": ["Nim\u00b7mer", "den", "Som\u00b7mer", "ver\u00b7weil'", "in", "A\u00b7then", ".", "Glut\u00b7vol\u00b7len", "Si\u00b7roc\u00b7co"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "NE", "$.", "NN", "NE"], "meter": "+--+--+-+-+--+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Atmest du dann, und der Geist senket die Fl\u00fcgel verzagt.", "tokens": ["At\u00b7mest", "du", "dann", ",", "und", "der", "Geist", "sen\u00b7ket", "die", "Fl\u00fc\u00b7gel", "ver\u00b7zagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "ART", "NN", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Doch wann segnend der Herbst in r\u00f6tlichem Duft durch die Berge", "tokens": ["Doch", "wann", "seg\u00b7nend", "der", "Herbst", "in", "r\u00f6t\u00b7li\u00b7chem", "Duft", "durch", "die", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVPP", "ART", "NN", "APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Wandelt, und am Felshang tiefer die Traube sich br\u00e4unt,", "tokens": ["Wan\u00b7delt", ",", "und", "am", "Fels\u00b7hang", "tie\u00b7fer", "die", "Trau\u00b7be", "sich", "br\u00e4unt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "APPRART", "NN", "ADJD", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Wann der Ilissos rauscht, und die neuaufgr\u00fcnende Talflur", "tokens": ["Wann", "der", "I\u00b7lis\u00b7sos", "rauscht", ",", "und", "die", "neu\u00b7auf\u00b7gr\u00fc\u00b7nen\u00b7de", "Tal\u00b7flur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "--+-++--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Zwischen dem \u00d6lwald bunt mit Anemonen sich schm\u00fcckt,", "tokens": ["Zwi\u00b7schen", "dem", "\u00d6l\u00b7wald", "bunt", "mit", "A\u00b7ne\u00b7mo\u00b7nen", "sich", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "Welche Wonne gew\u00e4hrt es alsdann, mit dem Freunde der Jugend", "tokens": ["Wel\u00b7che", "Won\u00b7ne", "ge\u00b7w\u00e4hrt", "es", "als\u00b7dann", ",", "mit", "dem", "Freun\u00b7de", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "VVFIN", "PPER", "ADV", "$,", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Auf den kolonischen H\u00f6hn unter den Blumen zu ruhn", "tokens": ["Auf", "den", "ko\u00b7lo\u00b7ni\u00b7schen", "H\u00f6hn", "un\u00b7ter", "den", "Blu\u00b7men", "zu", "ruhn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "--+-+-+---+--+", "measure": "iambic.penta.chol"}, "line