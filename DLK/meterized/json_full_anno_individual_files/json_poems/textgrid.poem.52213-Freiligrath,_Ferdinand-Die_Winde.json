{"textgrid.poem.52213": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Winde", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr ungesehnen Str\u00f6me durch die Luft,", "tokens": ["Ihr", "un\u00b7ge\u00b7seh\u00b7nen", "Str\u00f6\u00b7me", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie triebt ihr eben froh noch euer Spiel;", "tokens": ["Wie", "triebt", "ihr", "e\u00b7ben", "froh", "noch", "eu\u00b7er", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr trugt die Biene, trugt der Blume Duft,", "tokens": ["Ihr", "trugt", "die", "Bie\u00b7ne", ",", "trugt", "der", "Blu\u00b7me", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wehtet hei\u00dfe M\u00e4dchenwangen k\u00fchl;", "tokens": ["Und", "weh\u00b7tet", "hei\u00b7\u00dfe", "M\u00e4d\u00b7chen\u00b7wan\u00b7gen", "k\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr jagtet W\u00f6lkchen durch der Feste Blau;", "tokens": ["Ihr", "jag\u00b7tet", "W\u00f6lk\u00b7chen", "durch", "der", "Fes\u00b7te", "Blau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von welken Blumen klopftet ihr den Tau;", "tokens": ["Von", "wel\u00b7ken", "Blu\u00b7men", "klopf\u00b7tet", "ihr", "den", "Tau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie Schneegest\u00f6ber \u2013 o der pr\u00e4cht'gen Schau! \u2013", "tokens": ["Wie", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "\u2013", "o", "der", "pr\u00e4cht'\u00b7gen", "Schau", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "$(", "FM", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Katalpabl\u00fcten risset ihr vom Stiel.", "tokens": ["Ka\u00b7tal\u00b7pa\u00b7bl\u00fc\u00b7ten", "ris\u00b7set", "ihr", "vom", "Stiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jetzt aber br\u00fcllt ihr wie der Katarakt,", "tokens": ["Jetzt", "a\u00b7ber", "br\u00fcllt", "ihr", "wie", "der", "Ka\u00b7ta\u00b7rakt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rast wie die Brandung, die ans Ufer prallt;", "tokens": ["Rast", "wie", "die", "Bran\u00b7dung", ",", "die", "ans", "U\u00b7fer", "prallt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Die Berge zittern, wie von Furcht gepackt,", "tokens": ["Die", "Ber\u00b7ge", "zit\u00b7tern", ",", "wie", "von", "Furcht", "ge\u00b7packt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und euch zu F\u00fc\u00dfen krachend st\u00fcrzt der Wald.", "tokens": ["Und", "euch", "zu", "F\u00fc\u00b7\u00dfen", "kra\u00b7chend", "st\u00fcrzt", "der", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vor euch, wie Adler, jagt der Wolken Flucht;", "tokens": ["Vor", "euch", ",", "wie", "Ad\u00b7ler", ",", "jagt", "der", "Wol\u00b7ken", "Flucht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PWAV", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auf Haus und H\u00fctte wirft sich eure Wucht;", "tokens": ["Auf", "Haus", "und", "H\u00fct\u00b7te", "wirft", "sich", "eu\u00b7re", "Wucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie trocknes Herbstlaub in der \u00f6den Schlucht", "tokens": ["Wie", "trock\u00b7nes", "Herbst\u00b7laub", "in", "der", "\u00f6\u00b7den", "Schlucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Hebt und zerbricht sie eures Zorns Gewalt.", "tokens": ["Hebt", "und", "zer\u00b7bricht", "sie", "eu\u00b7res", "Zorns", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die V\u00f6gel flattern, \u00e4ngstlich und verwirrt;", "tokens": ["Die", "V\u00f6\u00b7gel", "flat\u00b7tern", ",", "\u00e4ngst\u00b7lich", "und", "ver\u00b7wirrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Umsonst! zu Tode schmei\u00dft sie eure Wut.", "tokens": ["Um\u00b7sonst", "!", "zu", "To\u00b7de", "schmei\u00dft", "sie", "eu\u00b7re", "Wut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Regen rasselt, und ein Strombett wird", "tokens": ["Der", "Re\u00b7gen", "ras\u00b7selt", ",", "und", "ein", "Strom\u00b7bett", "wird"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ringsum das Feld, soweit die Ernte ruht.", "tokens": ["Ring\u00b7sum", "das", "Feld", ",", "so\u00b7weit", "die", "Ern\u00b7te", "ruht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gie\u00dfb\u00e4che taumeln von der H\u00fcgel H\u00f6h',", "tokens": ["Gie\u00df\u00b7b\u00e4\u00b7che", "tau\u00b7meln", "von", "der", "H\u00fc\u00b7gel", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Das Dorf ertrinkt, die Ebne wird zum See,", "tokens": ["Das", "Dorf", "er\u00b7trinkt", ",", "die", "Eb\u00b7ne", "wird", "zum", "See", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und banger Stimmen herzzerrei\u00dfend Weh", "tokens": ["Und", "ban\u00b7ger", "Stim\u00b7men", "herz\u00b7zer\u00b7rei\u00b7\u00dfend", "Weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVPP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Erhebt sich jammernd aus der w\u00fcsten Flut.", "tokens": ["Er\u00b7hebt", "sich", "jam\u00b7mernd", "aus", "der", "w\u00fcs\u00b7ten", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr saust aufs Meer; \u2013 da werden M\u00e4nner bleich;", "tokens": ["Ihr", "saust", "aufs", "Meer", ";", "\u2013", "da", "wer\u00b7den", "M\u00e4n\u00b7ner", "bleich", ";"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "$(", "ADV", "VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wohin ihr donnert, Angstruf und Gebet.", "tokens": ["Wo\u00b7hin", "ihr", "don\u00b7nert", ",", "Angs\u00b7truf", "und", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr schlagt die Wasser, einem Vogel gleich,", "tokens": ["Ihr", "schlagt", "die", "Was\u00b7ser", ",", "ei\u00b7nem", "Vo\u00b7gel", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der lustig badend in der Quelle steht.", "tokens": ["Der", "lus\u00b7tig", "ba\u00b7dend", "in", "der", "Quel\u00b7le", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr rei\u00dft entzwei den Mast und seine Fahn';", "tokens": ["Ihr", "rei\u00dft", "ent\u00b7zwei", "den", "Mast", "und", "sei\u00b7ne", "Fahn'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bis auf den Grund peitscht ihr den Ozean;", "tokens": ["Bis", "auf", "den", "Grund", "peitscht", "ihr", "den", "O\u00b7ze\u00b7an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Berghohe Wellen spr\u00fcht ihr himmelan,", "tokens": ["Berg\u00b7ho\u00b7he", "Wel\u00b7len", "spr\u00fcht", "ihr", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Und Tr\u00fcmmer sind's, was ihr zur K\u00fcste weht!", "tokens": ["Und", "Tr\u00fcm\u00b7mer", "sin\u00b7d's", ",", "was", "ihr", "zur", "K\u00fcs\u00b7te", "weht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Wozu dies Toben? \u2013 F\u00fcr die Freiheit nicht", "tokens": ["Wo\u00b7zu", "dies", "To\u00b7ben", "?", "\u2013", "F\u00fcr", "die", "Frei\u00b7heit", "nicht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "NN", "$.", "$(", "APPR", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu ringen braucht ihr, da\u00df ihr also tollt;", "tokens": ["Zu", "rin\u00b7gen", "braucht", "ihr", ",", "da\u00df", "ihr", "al\u00b7so", "tollt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr braucht kein Erz zu r\u00fctteln, bis es bricht;", "tokens": ["Ihr", "braucht", "kein", "Erz", "zu", "r\u00fct\u00b7teln", ",", "bis", "es", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr regt die Schwingen, wie und wo ihr wollt.", "tokens": ["Ihr", "regt", "die", "Schwin\u00b7gen", ",", "wie", "und", "wo", "ihr", "wollt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "KON", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Frei w\u00fchlt ihr auf der Tiefe Wogenschwall;", "tokens": ["Frei", "w\u00fchlt", "ihr", "auf", "der", "Tie\u00b7fe", "Wo\u00b7gen\u00b7schwall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "W\u00e4lder und W\u00fcsten f\u00fcllt ihr an mit Schall,", "tokens": ["W\u00e4l\u00b7der", "und", "W\u00fcs\u00b7ten", "f\u00fcllt", "ihr", "an", "mit", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "APPR", "APPR", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Dazu die Inseln, die das Meer umrollt!", "tokens": ["Da\u00b7zu", "die", "In\u00b7seln", ",", "die", "das", "Meer", "um\u00b7rollt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wohl seid ihr stark! \u2013 Doch in Europa liegt,", "tokens": ["Wohl", "seid", "ihr", "stark", "!", "\u2013", "Doch", "in", "Eu\u00b7ro\u00b7pa", "liegt", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "$(", "KON", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weh ihr, in Ketten eine st\u00e4rkere Kraft;", "tokens": ["Weh", "ihr", ",", "in", "Ket\u00b7ten", "ei\u00b7ne", "st\u00e4r\u00b7ke\u00b7re", "Kraft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Auf Thronen sitzt, was ihren Nacken biegt,", "tokens": ["Auf", "Thro\u00b7nen", "sitzt", ",", "was", "ih\u00b7ren", "Na\u00b7cken", "biegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcberwacht mit Zittern ihre Haft.", "tokens": ["Und", "\u00fc\u00b7ber\u00b7wacht", "mit", "Zit\u00b7tern", "ih\u00b7re", "Haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Krieger stehn in Waffen um sie her;", "tokens": ["Und", "Krie\u00b7ger", "stehn", "in", "Waf\u00b7fen", "um", "sie", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn sie empor will, ziehn sie mitleidsleer", "tokens": ["Wenn", "sie", "em\u00b7por", "will", ",", "ziehn", "sie", "mit\u00b7leids\u00b7leer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKVZ", "VMFIN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Bande fester, heben hoch den Speer \u2013", "tokens": ["Die", "Ban\u00b7de", "fes\u00b7ter", ",", "he\u00b7ben", "hoch", "den", "Speer", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "VVFIN", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Tod ihre Strafe,