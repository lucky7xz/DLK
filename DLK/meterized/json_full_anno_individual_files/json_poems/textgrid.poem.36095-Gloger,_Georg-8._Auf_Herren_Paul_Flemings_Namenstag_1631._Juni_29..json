{"textgrid.poem.36095": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "8. Auf Herren Paul Flemings Namenstag 1631. Juni 29.", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kan ich denn au\u00dfer mir was Festes auch wol finden,", "tokens": ["Kan", "ich", "denn", "au\u00b7\u00dfer", "mir", "was", "Fes\u00b7tes", "auch", "wol", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "PIS", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wormit ich, du mein Ich, dich heute m\u00f6ge binden?", "tokens": ["Wor\u00b7mit", "ich", ",", "du", "mein", "Ich", ",", "dich", "heu\u00b7te", "m\u00f6\u00b7ge", "bin\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PPER", "PPOSAT", "PPER", "$,", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bind ich das Herze nur, so darf ich gar kein Band.", "tokens": ["Bind", "ich", "das", "Her\u00b7ze", "nur", ",", "so", "darf", "ich", "gar", "kein", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDS", "VVFIN", "ADV", "$,", "ADV", "VMFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "La\u00df ich dich denn so los, wo bleibet mir mein Pfand?", "tokens": ["La\u00df", "ich", "dich", "denn", "so", "los", ",", "wo", "blei\u00b7bet", "mir", "mein", "Pfand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "ADV", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wo man Pfand begehrt, so giebt man zu verstehen,", "tokens": ["Doch", "wo", "man", "Pfand", "be\u00b7gehrt", ",", "so", "giebt", "man", "zu", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "NN", "VVPP", "$,", "ADV", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df man nicht trawen wil. Magst ungebunden gehen.", "tokens": ["Da\u00df", "man", "nicht", "tra\u00b7wen", "wil", ".", "Magst", "un\u00b7ge\u00b7bun\u00b7den", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$.", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn was mein Pfand soll sein, das hab' ich schon bey mir,", "tokens": ["Denn", "was", "mein", "Pfand", "soll", "sein", ",", "das", "hab'", "ich", "schon", "bey", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VMFIN", "VAINF", "$,", "PDS", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd was dein Band soll sein, das hastu auch bey dir.", "tokens": ["Vnd", "was", "dein", "Band", "soll", "sein", ",", "das", "has\u00b7tu", "auch", "bey", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VMFIN", "VAINF", "$,", "PDS", "VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So hab' ich Pfand bei mir, du Band bey dir gefunden.", "tokens": ["So", "hab'", "ich", "Pfand", "bei", "mir", ",", "du", "Band", "bey", "dir", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "PPER", "$,", "PPER", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich bleibe dir verpf\u00e4ndt, du bleibest mir verbunden.", "tokens": ["Ich", "blei\u00b7be", "dir", "ver\u00b7pf\u00e4ndt", ",", "du", "blei\u00b7best", "mir", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$,", "PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vnd weil noch in der Welt ist Sonn- vnd Mondenschein,", "tokens": ["Vnd", "weil", "noch", "in", "der", "Welt", "ist", "Sonn", "vnd", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "APPR", "ART", "NN", "VAFIN", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Soll vnser Pfand vnd Band nicht aufgehoben seyn.", "tokens": ["Soll", "vn\u00b7ser", "Pfand", "vnd", "Band", "nicht", "auf\u00b7ge\u00b7ho\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "KON", "NN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text"