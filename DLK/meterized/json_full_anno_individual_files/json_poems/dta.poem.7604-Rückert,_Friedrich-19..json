{"dta.poem.7604": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alswie der Schwan, der rein auf reinen Fluten schwimmt,", "tokens": ["Als\u00b7wie", "der", "Schwan", ",", "der", "rein", "auf", "rei\u00b7nen", "Flu\u00b7ten", "schwimmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Himmel unter sich sein Spiegelbild vernimmt,", "tokens": ["Im", "Him\u00b7mel", "un\u00b7ter", "sich", "sein", "Spie\u00b7gel\u00b7bild", "ver\u00b7nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn er lang' im See gezogen seine Kreise,", "tokens": ["Und", "wenn", "er", "lang'", "im", "See", "ge\u00b7zo\u00b7gen", "sei\u00b7ne", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Taucht unter, und zur\u00fcck l\u00e4\u00dft keine Spur der Reise;", "tokens": ["Taucht", "un\u00b7ter", ",", "und", "zu\u00b7r\u00fcck", "l\u00e4\u00dft", "kei\u00b7ne", "Spur", "der", "Rei\u00b7se", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "$,", "KON", "PTKVZ", "VVFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gl\u00fcckselig, wer so rein sich auf der Welt bewahrt,", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", ",", "wer", "so", "rein", "sich", "auf", "der", "Welt", "be\u00b7wahrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "ADV", "ADJD", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Abschied also nimmt, da\u00df Niemand es gewahrt.", "tokens": ["Und", "Ab\u00b7schied", "al\u00b7so", "nimmt", ",", "da\u00df", "Nie\u00b7mand", "es", "ge\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}