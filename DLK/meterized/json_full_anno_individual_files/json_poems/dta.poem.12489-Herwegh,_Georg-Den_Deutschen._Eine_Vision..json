{"dta.poem.12489": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Den Deutschen.  \n  Eine Vision.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hatt' ein seltsam Traumgesicht:", "tokens": ["Ich", "hatt'", "ein", "selt\u00b7sam", "Traum\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sa\u00df Gott Vater zu Gericht", "tokens": ["Da", "sa\u00df", "Gott", "Va\u00b7ter", "zu", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rief jedwede Nation", "tokens": ["Und", "rief", "jed\u00b7we\u00b7de", "Na\u00b7tion"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Herbei, vor seinen Sternenthron.", "tokens": ["Her\u00b7bei", ",", "vor", "sei\u00b7nen", "Ster\u00b7nen\u00b7thron", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die V\u00f6lker kamen in dichten Haufen,", "tokens": ["Die", "V\u00f6l\u00b7ker", "ka\u00b7men", "in", "dich\u00b7ten", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Just wie sie waren, angelaufen:", "tokens": ["Just", "wie", "sie", "wa\u00b7ren", ",", "an\u00b7ge\u00b7lau\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Britten, Russen und Franzosen,", "tokens": ["Die", "Brit\u00b7ten", ",", "Rus\u00b7sen", "und", "Fran\u00b7zo\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die letzten, wie immer, ohne Hosen;", "tokens": ["Die", "letz\u00b7ten", ",", "wie", "im\u00b7mer", ",", "oh\u00b7ne", "Ho\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PWAV", "ADV", "$,", "KOUI", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Selbst China und die Mongolei,", "tokens": ["Selbst", "Chi\u00b7na", "und", "die", "Mon\u00b7go\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch ein St\u00fcck Polen war dabei.", "tokens": ["Auch", "ein", "St\u00fcck", "Po\u00b7len", "war", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und als der Herr die V\u00f6lker z\u00e4hlte \u2014", "tokens": ["Und", "als", "der", "Herr", "die", "V\u00f6l\u00b7ker", "z\u00e4hl\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ei, sieh! das deutsche Reich noch fehlte.", "tokens": ["Ei", ",", "sieh", "!", "das", "deut\u00b7sche", "Reich", "noch", "fehl\u00b7te", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "$.", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201ewo bleiben denn meine Deutschen wieder?", "tokens": ["\u201e", "wo", "blei\u00b7ben", "denn", "mei\u00b7ne", "Deut\u00b7schen", "wie\u00b7der", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVINF", "KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Recken sie noch die faulen Glieder?", "tokens": ["Re\u00b7cken", "sie", "noch", "die", "fau\u00b7len", "Glie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sie k\u00f6nnten, seit ich sie begraben,", "tokens": ["Sie", "k\u00f6nn\u00b7ten", ",", "seit", "ich", "sie", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch endlich ausgeschlafen haben!\u201c", "tokens": ["Doch", "end\u00b7lich", "aus\u00b7ge\u00b7schla\u00b7fen", "ha\u00b7ben", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVPP", "VAINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drauf hie\u00df er 'nen Engel zur Erde springen,", "tokens": ["Drauf", "hie\u00df", "er", "'nen", "En\u00b7gel", "zur", "Er\u00b7de", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Siebenschl\u00e4fer heraufzubringen.", "tokens": ["Die", "Sie\u00b7ben\u00b7schl\u00e4\u00b7fer", "her\u00b7auf\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Engel lief in Deutschland herum:", "tokens": ["Der", "En\u00b7gel", "lief", "in", "Deutschland", "he\u00b7rum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War Alles still, war Alles stumm.", "tokens": ["War", "Al\u00b7les", "still", ",", "war", "Al\u00b7les", "stumm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKVZ", "$,", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201eihr Deutschen, wollt ihr nicht aufstahn?", "tokens": ["\u201e", "ihr", "Deut\u00b7schen", ",", "wollt", "ihr", "nicht", "auf\u00b7stahn", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "PTKNEG", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ewigkeit geht eben an!\u201c", "tokens": ["Die", "E\u00b7wig\u00b7keit", "geht", "e\u00b7ben", "an", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Engel blies in lichtem Zorn,", "tokens": ["Der", "En\u00b7gel", "blies", "in", "lich\u00b7tem", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie toll, in sein himmlisch J\u00e4gerhorn.", "tokens": ["Wie", "toll", ",", "in", "sein", "himm\u00b7lisch", "J\u00e4\u00b7ger\u00b7horn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Doch eh' sich die Deutschen zusammengefunden,", "tokens": ["Doch", "eh'", "sich", "die", "Deut\u00b7schen", "zu\u00b7sam\u00b7men\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "War l\u00e4ngst der j\u00fcngste Tag verschwunden,", "tokens": ["War", "l\u00e4ngst", "der", "j\u00fcngs\u00b7te", "Tag", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hatt' Alles seinen Lohn empfangen \u2014", "tokens": ["Hatt'", "Al\u00b7les", "sei\u00b7nen", "Lohn", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Deutschen ist Himmel und H\u00f6ll' entgangen!", "tokens": ["Den", "Deut\u00b7schen", "ist", "Him\u00b7mel", "und", "H\u00f6ll'", "ent\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}