{"dta.poem.172": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er belauret sie im Bade.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Philena wolte j\u00fcngst Fernanden recht begl\u00fccken/", "tokens": ["Phi\u00b7le\u00b7na", "wol\u00b7te", "j\u00fcngst", "Fern\u00b7an\u00b7den", "recht", "be\u00b7gl\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn dieses M\u00e4dgen kahm zu ihm aus freyen St\u00fccken/", "tokens": ["Denn", "die\u00b7ses", "M\u00e4d\u00b7gen", "kahm", "zu", "ihm", "aus", "frey\u00b7en", "St\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sprach: Wenn er/ mein Herr/ will meine Jungfer sehn", "tokens": ["Und", "sprach", ":", "Wenn", "er", "/", "mein", "Herr", "/", "will", "mei\u00b7ne", "Jung\u00b7fer", "sehn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "KOUS", "PPER", "$(", "PPOSAT", "NN", "$(", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gantz nackend/ mu\u00df er gleich nach unsern Garten gehn.", "tokens": ["Gantz", "na\u00b7ckend", "/", "mu\u00df", "er", "gleich", "nach", "un\u00b7sern", "Gar\u00b7ten", "gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$(", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er kunte nicht den Schlu\u00df der Rede gantz abwarten", "tokens": ["Er", "kun\u00b7te", "nicht", "den", "Schlu\u00df", "der", "Re\u00b7de", "gantz", "ab\u00b7war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+++-", "measure": "unknown.measure.septa"}, "line.6": {"text": "Er eilte als ein Pfeil nach den benannten Garten/", "tokens": ["Er", "eil\u00b7te", "als", "ein", "Pfeil", "nach", "den", "be\u00b7nann\u00b7ten", "Gar\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Th\u00fcr war angespehrt/ doch nicht ins Schlo\u00df gemacht/", "tokens": ["Die", "Th\u00fcr", "war", "an\u00b7ge\u00b7spehrt", "/", "doch", "nicht", "ins", "Schlo\u00df", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ADV", "PTKNEG", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df er sich ungesehn sogleich hinein gebracht.", "tokens": ["Da\u00df", "er", "sich", "un\u00b7ge\u00b7sehn", "sog\u00b7leich", "hin\u00b7ein", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er ging dem Bade zu/ allein es war verschlossen/", "tokens": ["Er", "ging", "dem", "Ba\u00b7de", "zu", "/", "al\u00b7lein", "es", "war", "ver\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "$(", "ADV", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So da\u00df ihn M\u00fch und Flei\u00df/ die er gebraucht/ verdrossen/", "tokens": ["So", "da\u00df", "ihn", "M\u00fch", "und", "Flei\u00df", "/", "die", "er", "ge\u00b7braucht", "/", "ver\u00b7dros\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NN", "KON", "NN", "$(", "PRELS", "PPER", "VVPP", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er fluchte der ", "tokens": ["Er", "fluch\u00b7te", "der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Und h\u00e4tte sich entfernt/ wenn er kein Loch erblickt/", "tokens": ["Und", "h\u00e4t\u00b7te", "sich", "ent\u00b7fernt", "/", "wenn", "er", "kein", "Loch", "er\u00b7blickt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADJD", "$(", "KOUS", "PPER", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wodurch er in dem Bad die nackte Sch\u00f6ne sehen/", "tokens": ["Wo\u00b7durch", "er", "in", "dem", "Bad", "die", "nack\u00b7te", "Sch\u00f6\u00b7ne", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NE", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und im Geb\u00fcsch versteckt verborgen kunte stehen/", "tokens": ["Und", "im", "Ge\u00b7b\u00fcsch", "ver\u00b7steckt", "ver\u00b7bor\u00b7gen", "kun\u00b7te", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Allda ersahe er ein gantz entbl\u00f6\u00dftes Kind/", "tokens": ["All\u00b7da", "er\u00b7sa\u00b7he", "er", "ein", "gantz", "ent\u00b7bl\u00f6\u00df\u00b7tes", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das an der Zierlichkeit ", "tokens": ["Das", "an", "der", "Zier\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Wenn sie unangelegt das k\u00fchle Bad gesegnet/", "tokens": ["Wenn", "sie", "un\u00b7an\u00b7ge\u00b7legt", "das", "k\u00fch\u00b7le", "Bad", "ge\u00b7seg\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "ADJA", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und dem ", "tokens": ["Und", "dem"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Ein Kind/ das z\u00e4rter war als man die ", "tokens": ["Ein", "Kind", "/", "das", "z\u00e4r\u00b7ter", "war", "als", "man", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "VAFIN", "KOKOM", "PIS", "ART"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Wenn sie den ", "tokens": ["Wenn", "sie", "den"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Der sch\u00f6nen Augen Strahl besch\u00e4mmt die hellen Sternen/", "tokens": ["Der", "sch\u00f6\u00b7nen", "Au\u00b7gen", "Strahl", "be\u00b7sch\u00e4mmt", "die", "hel\u00b7len", "Ster\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ja was/ die Sonne mu\u00df sich vor den Glantz entfernen/", "tokens": ["Ja", "was", "/", "die", "Son\u00b7ne", "mu\u00df", "sich", "vor", "den", "Glantz", "ent\u00b7fer\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWS", "$(", "ART", "NN", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Morgenr\u00f6th entf\u00e4rbt sich vor der Wangen-Gluht/", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6th", "ent\u00b7f\u00e4rbt", "sich", "vor", "der", "Wan\u00b7gen\u00b7Gluht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der beste ", "tokens": ["Der", "bes\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Die Br\u00fcste kr\u00f6hnt Rubin/ auf Liljen bl\u00fchn Corallen/", "tokens": ["Die", "Br\u00fcs\u00b7te", "kr\u00b7\u00f6hnt", "Ru\u00b7bin", "/", "auf", "Lil\u00b7jen", "bl\u00fchn", "Co\u00b7ral\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$(", "APPR", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Ein feurig ", "tokens": ["Ein", "feu\u00b7rig"], "token_info": ["word", "word"], "pos": ["ART", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Die wohlgew\u00f6lbte Schoo\u00df/ der L\u00fcste Paradies", "tokens": ["Die", "wohl\u00b7ge\u00b7w\u00f6lb\u00b7te", "Schoo\u00df", "/", "der", "L\u00fcs\u00b7te", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Den Augen s\u00fcsse Lust/ und Kitzelung verhie\u00df.", "tokens": ["Den", "Au\u00b7gen", "s\u00fcs\u00b7se", "Lust", "/", "und", "Kit\u00b7ze\u00b7lung", "ver\u00b7hie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es lag ein Myrthen-Wald um den Rubinen Schlosse/", "tokens": ["Es", "lag", "ein", "Myr\u00b7then\u00b7Wald", "um", "den", "Ru\u00b7bi\u00b7nen", "Schlos\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Den Eingang zu der Lust/ den rund gew\u00f6lbten Schoo\u00df/", "tokens": ["Den", "Ein\u00b7gang", "zu", "der", "Lust", "/", "den", "rund", "ge\u00b7w\u00f6lb\u00b7ten", "Schoo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.32": {"text": "Und diesen sch\u00f6nen Ort zur Gl\u00fcckes-Insul macht.", "tokens": ["Und", "die\u00b7sen", "sch\u00f6\u00b7nen", "Ort", "zur", "Gl\u00fc\u00b7ckes\u00b7In\u00b7sul", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Da ist das ", "tokens": ["Da", "ist", "das"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.34": {"text": "Der/ st\u00fcrmmt man noch so sehr/ den Posten nicht verliehret.", "tokens": ["Der", "/", "st\u00fcrmmt", "man", "noch", "so", "sehr", "/", "den", "Pos\u00b7ten", "nicht", "ver\u00b7lieh\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "VVFIN", "PIS", "ADV", "ADV", "ADV", "$(", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er hatte nun genug das nackte Bild besehn/", "tokens": ["Er", "hat\u00b7te", "nun", "ge\u00b7nug", "das", "nack\u00b7te", "Bild", "be\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und wolte unvermerckt zur Th\u00fcr hinaussen gehn/", "tokens": ["Und", "wol\u00b7te", "un\u00b7ver\u00b7merckt", "zur", "Th\u00fcr", "hin\u00b7aus\u00b7sen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "APPRART", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Als eben ", "tokens": ["Als", "e\u00b7ben"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.38": {"text": "Die in der Einsamkeit allein zu seyn gedachte;", "tokens": ["Die", "in", "der", "Ein\u00b7sam\u00b7keit", "al\u00b7lein", "zu", "seyn", "ge\u00b7dach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Sie schrieh/ Verr\u00e4ther geh/ zerbreche Hals und Bein/", "tokens": ["Sie", "schrieh", "/", "Ver\u00b7r\u00e4\u00b7ther", "geh", "/", "zer\u00b7bre\u00b7che", "Hals", "und", "Bein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NN", "VVFIN", "$(", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wer l\u00e4st ", "tokens": ["Wer", "l\u00e4st"], "token_info": ["word", "word"], "pos": ["PWS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.41": {"text": "Gewi\u00df/ ", "tokens": ["Ge\u00b7wi\u00df", "/"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$("], "meter": "-+", "measure": "iambic.single"}, "line.42": {"text": "Hier wolte sie vor ihn des Bades-Th\u00fcr verschliessen/", "tokens": ["Hier", "wol\u00b7te", "sie", "vor", "ihn", "des", "Ba\u00b7des\u00b7T\u00b7h\u00fcr", "ver\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Allein er war geschwind/ und drengte sich hinein/", "tokens": ["Al\u00b7lein", "er", "war", "ge\u00b7schwind", "/", "und", "dreng\u00b7te", "sich", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADJD", "$(", "KON", "VVFIN", "PRF", "APZR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Weil sie nun nackend war/ must sies zu frieden seyn.", "tokens": ["Weil", "sie", "nun", "na\u00b7ckend", "war", "/", "must", "sies", "zu", "frie\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$(", "VMFIN", "PIS", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Sie schalt ihn hefftig aus/ ein schelmischer Verr\u00e4hter/", "tokens": ["Sie", "schalt", "ihn", "heff\u00b7tig", "aus", "/", "ein", "schel\u00b7mi\u00b7scher", "Ver\u00b7r\u00e4h\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ein R\u00e4uber/ L\u00e4sterer/ ein b\u00f6ser Ubelth\u00e4ter/", "tokens": ["Ein", "R\u00e4u\u00b7ber", "/", "L\u00e4s\u00b7te\u00b7rer", "/", "ein", "b\u00f6\u00b7ser", "U\u00b7belt\u00b7h\u00e4\u00b7ter", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Das muste er seyn; doch gab sie sich zur Rruh/", "tokens": [