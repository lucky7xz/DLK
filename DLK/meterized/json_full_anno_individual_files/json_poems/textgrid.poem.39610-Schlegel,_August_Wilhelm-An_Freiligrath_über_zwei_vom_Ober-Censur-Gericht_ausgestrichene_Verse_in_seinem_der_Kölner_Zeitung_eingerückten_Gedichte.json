{"textgrid.poem.39610": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Freiligrath \u00fcber zwei vom Ober-Censur-Gericht ausgestrichene Verse in seinem der K\u00f6lner Zeitung einger\u00fcckten Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vernimm den treuen Rath,", "tokens": ["Ver\u00b7nimm", "den", "treu\u00b7en", "Rath", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich geb' ihn unverhohlen,", "tokens": ["Ich", "geb'", "ihn", "un\u00b7ver\u00b7hoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Reimreicher Freiligrath!", "tokens": ["Reim\u00b7rei\u00b7cher", "Frei\u00b7li\u00b7gra\u00b7th", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O reime nicht auf Sohlen!", "tokens": ["O", "rei\u00b7me", "nicht", "auf", "Soh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das gr\u00e4nzt an Hochverrath;", "tokens": ["Das", "gr\u00e4nzt", "an", "Hoch\u00b7ver\u00b7rath", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich stehe, wie auf Kohlen,", "tokens": ["Ich", "ste\u00b7he", ",", "wie", "auf", "Koh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bei so verwegner That.", "tokens": ["Bei", "so", "ver\u00b7weg\u00b7ner", "That", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Es ist ja leicht, die Jugend", "tokens": ["Es", "ist", "ja", "leicht", ",", "die", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zu paaren mit der Tugend;", "tokens": ["Zu", "paa\u00b7ren", "mit", "der", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Auch prangen alle Musen", "tokens": ["Auch", "pran\u00b7gen", "al\u00b7le", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Mit einem vollen Busen:", "tokens": ["Mit", "ei\u00b7nem", "vol\u00b7len", "Bu\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Das sind erlaubte Reime,", "tokens": ["Das", "sind", "er\u00b7laub\u00b7te", "Rei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "S\u00fc\u00df, gleich dem Honigseime.", "tokens": ["S\u00fc\u00df", ",", "gleich", "dem", "Ho\u00b7ni\u00b7gsei\u00b7me", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Wer aber s