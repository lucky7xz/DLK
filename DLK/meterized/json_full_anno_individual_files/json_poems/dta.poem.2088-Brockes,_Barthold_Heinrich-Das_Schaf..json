{"dta.poem.2088": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Schaf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man wird befinden, da\u00df sogar durch Bilder von der", "tokens": ["Man", "wird", "be\u00b7fin\u00b7den", ",", "da\u00df", "so\u00b7gar", "durch", "Bil\u00b7der", "von", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "VVINF", "$,", "KOUS", "ADV", "APPR", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sch\u00e4ferey", "tokens": ["Sch\u00e4\u00b7fe\u00b7rey"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Man froh und gleichsam ruhig werde, und inniglich ge-", "tokens": ["Man", "froh", "und", "gleich\u00b7sam", "ru\u00b7hig", "wer\u00b7de", ",", "und", "in\u00b7nig\u00b7lich", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ADJD", "KON", "ADJD", "ADJD", "VAFIN", "$,", "KON", "ADJD", "TRUNC"], "meter": "-+-+-+-+----+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "r\u00fchret sey.", "tokens": ["r\u00fch\u00b7ret", "sey", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "So la\u00dft uns denn in diesem Thier des Sch\u00f6pfers Huld", "tokens": ["So", "la\u00dft", "uns", "denn", "in", "die\u00b7sem", "Thier", "des", "Sch\u00f6p\u00b7fers", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "besonders sehen,", "tokens": ["be\u00b7son\u00b7ders", "se\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Jhm danken, und in unsrer Lust des Gebers Lieb und", "tokens": ["Jhm", "dan\u00b7ken", ",", "und", "in", "uns\u00b7rer", "Lust", "des", "Ge\u00b7bers", "Lieb", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KON", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Macht erh\u00f6hen!", "tokens": ["Macht", "er\u00b7h\u00f6\u00b7hen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}