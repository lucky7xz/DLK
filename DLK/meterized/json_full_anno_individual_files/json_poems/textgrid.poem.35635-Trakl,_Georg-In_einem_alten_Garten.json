{"textgrid.poem.35635": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "In einem alten Garten", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Resedaduft entschwebt im braunen Gr\u00fcn,", "tokens": ["Re\u00b7se\u00b7da\u00b7duft", "ent\u00b7schwebt", "im", "brau\u00b7nen", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Geflimmer schauert auf den sch\u00f6nen Weiher,", "tokens": ["Ge\u00b7flim\u00b7mer", "schau\u00b7ert", "auf", "den", "sch\u00f6\u00b7nen", "Wei\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Weiden stehn geh\u00fcllt in wei\u00dfe Schleier", "tokens": ["Die", "Wei\u00b7den", "stehn", "ge\u00b7h\u00fcllt", "in", "wei\u00b7\u00dfe", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Darinnen Falter irre Kreise ziehn.", "tokens": ["Da\u00b7rin\u00b7nen", "Fal\u00b7ter", "ir\u00b7re", "Krei\u00b7se", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verlassen sonnt sich die Terrasse dort,", "tokens": ["Ver\u00b7las\u00b7sen", "sonnt", "sich", "die", "Ter\u00b7ras\u00b7se", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "ADV", "$