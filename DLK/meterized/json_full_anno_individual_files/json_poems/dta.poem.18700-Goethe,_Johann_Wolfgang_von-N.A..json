{"dta.poem.18700": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Freygebiger wird betrogen,", "tokens": ["Frey\u00b7ge\u00b7bi\u00b7ger", "wird", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geizhafter ausgesogen,", "tokens": ["Geiz\u00b7haf\u00b7ter", "aus\u00b7ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verst\u00e4ndiger irrgeleitet,", "tokens": ["Ver\u00b7st\u00e4n\u00b7di\u00b7ger", "irr\u00b7ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vern\u00fcnftiger leer geweitet,", "tokens": ["Ver\u00b7n\u00fcnf\u00b7ti\u00b7ger", "leer", "ge\u00b7wei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Harte wird umgangen,", "tokens": ["Der", "Har\u00b7te", "wird", "um\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Gimpel wird gefangen.", "tokens": ["Der", "Gim\u00b7pel", "wird", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Beherrsche diese L\u00fcge,", "tokens": ["Be\u00b7herr\u00b7sche", "die\u00b7se", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Betrogener betr\u00fcge!", "tokens": ["Be\u00b7tro\u00b7ge\u00b7ner", "be\u00b7tr\u00fc\u00b7ge", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}}}}