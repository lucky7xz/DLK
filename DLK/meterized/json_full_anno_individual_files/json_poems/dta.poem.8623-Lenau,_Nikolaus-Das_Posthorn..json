{"dta.poem.8623": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das Posthorn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Still ist schon das ganze Dorf,", "tokens": ["Still", "ist", "schon", "das", "gan\u00b7ze", "Dorf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles schlafen gangen,", "tokens": ["Al\u00b7les", "schla\u00b7fen", "gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Auch die V\u00f6glein im Gezweig,", "tokens": ["Auch", "die", "V\u00f6\u00b7glein", "im", "Ge\u00b7zweig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die so lieblich sangen.", "tokens": ["Die", "so", "lieb\u00b7lich", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Dort in seiner Einsamkeit", "tokens": ["Dort", "in", "sei\u00b7ner", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt der Mond nun wieder,", "tokens": ["Kommt", "der", "Mond", "nun", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und er l\u00e4chelt still und bleich", "tokens": ["Und", "er", "l\u00e4\u00b7chelt", "still", "und", "bleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Gru\u00df hernieder;", "tokens": ["Sei\u00b7nen", "Gru\u00df", "her\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nur der Bach, der nimmer ruht,", "tokens": ["Nur", "der", "Bach", ",", "der", "nim\u00b7mer", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat ihn gleich vernommen,", "tokens": ["Hat", "ihn", "gleich", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "L\u00e4chelt ihm den Gru\u00df zur\u00fcck,", "tokens": ["L\u00e4\u00b7chelt", "ihm", "den", "Gru\u00df", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fcstert ihm: \u201eWillkommen!\u201c", "tokens": ["Fl\u00fcs\u00b7tert", "ihm", ":", "\u201e", "Will\u00b7kom\u00b7men", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Mich auch findest du noch wach,", "tokens": ["Mich", "auch", "fin\u00b7dest", "du", "noch", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieber Mond, wie diesen,", "tokens": ["Lie\u00b7ber", "Mond", ",", "wie", "die\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PWAV", "PDS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn auf immer hat die Ruh'", "tokens": ["Denn", "auf", "im\u00b7mer", "hat", "die", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich auch fortgewiesen.", "tokens": ["Mich", "auch", "fort\u00b7ge\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Mich umschlingt kein holder Traum", "tokens": ["Mich", "um\u00b7schlingt", "kein", "hol\u00b7der", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Zauberf\u00e4den,", "tokens": ["Mit", "den", "Zau\u00b7ber\u00b7f\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hab' mit meinem Schmerze noch", "tokens": ["Hab'", "mit", "mei\u00b7nem", "Schmer\u00b7ze", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Manches Wort zu reden.", "tokens": ["Man\u00b7ches", "Wort", "zu", "re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ferne, leise h\u00f6r' ich dort", "tokens": ["Fer\u00b7ne", ",", "lei\u00b7se", "h\u00f6r'", "ich", "dort"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines Posthorns Kl\u00e4nge,", "tokens": ["Ei\u00b7nes", "Post\u00b7horns", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Pl\u00f6tzlich wird mir um das Herz", "tokens": ["Pl\u00f6tz\u00b7lich", "wird", "mir", "um", "das", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun noch eins so enge.", "tokens": ["Nun", "noch", "eins", "so", "en\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADV", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "T\u00f6ne, Wandermelodei,", "tokens": ["T\u00f6\u00b7ne", ",", "Wan\u00b7der\u00b7me\u00b7lo\u00b7dei", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die \u00f6den Stra\u00dfen,", "tokens": ["Durch", "die", "\u00f6\u00b7den", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie so leicht einander doch", "tokens": ["Wie", "so", "leicht", "ein\u00b7an\u00b7der", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Menschen sich verlassen!", "tokens": ["Men\u00b7schen", "sich", "ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Lustig rollt der Wagen fort", "tokens": ["Lus\u00b7tig", "rollt", "der", "Wa\u00b7gen", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Stein' und Br\u00fccken,", "tokens": ["Ue\u00b7ber", "Stein'", "und", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Stand nicht wer an seinem Schlag", "tokens": ["Stand", "nicht", "wer", "an", "sei\u00b7nem", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "PWS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit verweinten Blicken?", "tokens": ["Mit", "ver\u00b7wein\u00b7ten", "Bli\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Mag er stehn! die Thr\u00e4ne kann", "tokens": ["Mag", "er", "stehn", "!", "die", "Thr\u00e4\u00b7ne", "kann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "ART", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht die Rosse halten;", "tokens": ["Nicht", "die", "Ros\u00b7se", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mag der rauhe Gei\u00dfelschlag", "tokens": ["Mag", "der", "rau\u00b7he", "Gei\u00b7\u00dfel\u00b7schlag"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm die Seele spalten!", "tokens": ["Ihm", "die", "See\u00b7le", "spal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Schon verhallt des Hornes Klang", "tokens": ["Schon", "ver\u00b7hallt", "des", "Hor\u00b7nes", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ferne meinem Lauschen,", "tokens": ["Fer\u00b7ne", "mei\u00b7nem", "Lau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich h\u00f6re wieder nur", "tokens": ["Und", "ich", "h\u00f6\u00b7re", "wie\u00b7der", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier das B\u00e4chlein rauschen.", "tokens": ["Hier", "das", "B\u00e4ch\u00b7lein", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Ich gedenke bang und schwer", "tokens": ["Ich", "ge\u00b7den\u00b7ke", "bang", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller meiner Lieben,", "tokens": ["Al\u00b7ler", "mei\u00b7ner", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die in ferner Heimat mir", "tokens": ["Die", "in", "fer\u00b7ner", "Hei\u00b7mat", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind zur\u00fcckgeblieben;", "tokens": ["Sind", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Diese sch\u00f6ne Sommernacht", "tokens": ["Die\u00b7se", "sch\u00f6\u00b7ne", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df vor\u00fcbergehen,", "tokens": ["Mu\u00df", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und mein Leben ohne sie", "tokens": ["Und", "mein", "Le\u00b7ben", "oh\u00b7ne", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsamkeit verwehen.", "tokens": ["Ein\u00b7sam\u00b7keit", "ver\u00b7we\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Mahnend ruft die Mitternacht", "tokens": ["Mah\u00b7nend", "ruft", "die", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir herab vom Thurme;", "tokens": ["Mir", "her\u00b7ab", "vom", "Thur\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ferne! denket mein! die Zeit", "tokens": ["Fer\u00b7ne", "!", "den\u00b7ket", "mein", "!", "die", "Zeit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "$.", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eilt dahin im Sturme!", "tokens": ["Eilt", "da\u00b7hin", "im", "Stur\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Unsre Gr\u00e4ber, denket mein!", "tokens": ["Uns\u00b7re", "Gr\u00e4\u00b7ber", ",", "den\u00b7ket", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind schon ungeduldig! \u2014", "tokens": ["Sind", "schon", "un\u00b7ge\u00b7dul\u00b7dig", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df wir nicht beisammen sind,", "tokens": ["Da\u00df", "wir", "nicht", "bei\u00b7sam\u00b7men", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich selber schuldig.", "tokens": ["Bin", "ich", "sel\u00b7ber", "schul\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}