{"textgrid.poem.38526": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Weissagung", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, dessen Seele Flammenbegeisterung", "tokens": ["Du", ",", "des\u00b7sen", "See\u00b7le", "Flam\u00b7men\u00b7be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "NN", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf Adlersfl\u00fcgeln zu den Gestirnen rei\u00dft,", "tokens": ["Auf", "Ad\u00b7ler\u00b7sfl\u00fc\u00b7geln", "zu", "den", "Ge\u00b7stir\u00b7nen", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Singst du, von Eichen rings umschauert,", "tokens": ["Singst", "du", ",", "von", "Ei\u00b7chen", "rings", "um\u00b7schau\u00b7ert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was dir im Busen der Gott gebietet:", "tokens": ["Was", "dir", "im", "Bu\u00b7sen", "der", "Gott", "ge\u00b7bie\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Willkommen, J\u00fcngling, in der geweihten Bahn!", "tokens": ["Will\u00b7kom\u00b7men", ",", "J\u00fcng\u00b7ling", ",", "in", "der", "ge\u00b7weih\u00b7ten", "Bahn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein Auge flammt von heiliger Kampfbegier,", "tokens": ["Dein", "Au\u00b7ge", "flammt", "von", "hei\u00b7li\u00b7ger", "Kampf\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu ringen nach dem Kranze, welcher,", "tokens": ["Zu", "rin\u00b7gen", "nach", "dem", "Kran\u00b7ze", ",", "wel\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hell wie Orion, den Siegern schimmert!", "tokens": ["Hell", "wie", "O\u00b7rion", ",", "den", "Sie\u00b7gern", "schim\u00b7mert", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn stets du singst, wozu dir dein Vaterland,", "tokens": ["Wenn", "stets", "du", "singst", ",", "wo\u00b7zu", "dir", "dein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Die Tugend dir, und himmlische Freiheitsgluth", "tokens": ["Die", "Tu\u00b7gend", "dir", ",", "und", "himm\u00b7li\u00b7sche", "Frei\u00b7heits\u00b7gluth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.r