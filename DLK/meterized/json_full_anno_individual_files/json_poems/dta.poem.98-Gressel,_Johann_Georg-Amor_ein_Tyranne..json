{"dta.poem.98": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Amor  ein Tyranne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Vertrau dich Amorn nicht/ er ist ein ertz Tyranne/", "tokens": ["Ver\u00b7trau", "dich", "A\u00b7morn", "nicht", "/", "er", "ist", "ein", "ertz", "Ty\u00b7ran\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "NE", "PTKNEG", "$(", "PPER", "VAFIN", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kommt er gleich schmeichelnd an/ und scheint dir Most", "tokens": ["Kommt", "er", "gleich", "schmei\u00b7chelnd", "an", "/", "und", "scheint", "dir", "Most"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$(", "KON", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "zu seyn/", "tokens": ["zu", "seyn", "/"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$("], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "So schencket er zuletzt doch Gall vor Honig ein/", "tokens": ["So", "schen\u00b7cket", "er", "zu\u00b7letzt", "doch", "Gall", "vor", "Ho\u00b7nig", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "NE", "APPR", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er ruhet eher nicht/ bis da\u00df dich in die Pfanne", "tokens": ["Er", "ru\u00b7het", "e\u00b7her", "nicht", "/", "bis", "da\u00df", "dich", "in", "die", "Pfan\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$(", "APPR", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Haut Gram und Eyversucht;", "tokens": ["Haut", "Gram", "und", "Ey\u00b7ver\u00b7sucht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und bis der blasse Tod dein armes Leben endet/", "tokens": ["Und", "bis", "der", "blas\u00b7se", "Tod", "dein", "ar\u00b7mes", "Le\u00b7ben", "en\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn man nun in das Grab den starren Leichnam senckt/", "tokens": ["Wenn", "man", "nun", "in", "das", "Grab", "den", "star\u00b7ren", "Leich\u00b7nam", "senckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Alsdann er sich gar offt/ doch viel zu sp\u00e4ht/ bedenckt/", "tokens": ["Als\u00b7dann", "er", "sich", "gar", "offt", "/", "doch", "viel", "zu", "sp\u00e4ht", "/", "be\u00b7denckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "$(", "ADV", "ADV", "PTKA", "ADJD", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und seinen harten Grimm in eine Liebe wendet;", "tokens": ["Und", "sei\u00b7nen", "har\u00b7ten", "Grimm", "in", "ei\u00b7ne", "Lie\u00b7be", "wen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Drum eile mit der Flucht/", "tokens": ["Drum", "ei\u00b7le", "mit", "der", "Flucht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Da\u00df dich das blinde Kind im Fluge nicht ereile/", "tokens": ["Da\u00df", "dich", "das", "blin\u00b7de", "Kind", "im", "Flu\u00b7ge", "nicht", "er\u00b7ei\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und an ein Felsen Hertz mit seiner Wehr anpfeile/", "tokens": ["Und", "an", "ein", "Fel\u00b7sen", "Hertz", "mit", "sei\u00b7ner", "Wehr", "an\u00b7pfei\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn du ihm wirst entgehn/ wird dir die Nach-Welt setzen", "tokens": ["Wenn", "du", "ihm", "wirst", "ent\u00b7gehn", "/", "wird", "dir", "die", "Nach\u00b7Welt", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVINF", "$(", "VAFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein pr\u00e4chtig Ehren-Mahl/ und diese Schrifft dran \u00e4tzen:", "tokens": ["Ein", "pr\u00e4ch\u00b7tig", "Eh\u00b7ren\u00b7Mahl", "/", "und", "die\u00b7se", "Schrifft", "dran", "\u00e4t\u00b7zen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$(", "KON", "PDAT", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Steh Leser! dieser Mensch ist nie verliebt gemacht/", "tokens": ["Steh", "Le\u00b7ser", "!", "die\u00b7ser", "Mensch", "ist", "nie", "ver\u00b7liebt", "ge\u00b7macht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "PDAT", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Cupiden hat er stets mit seiner Lust veracht.", "tokens": ["Cu\u00b7pi\u00b7den", "hat", "er", "stets", "mit", "sei\u00b7ner", "Lust", "ver\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}