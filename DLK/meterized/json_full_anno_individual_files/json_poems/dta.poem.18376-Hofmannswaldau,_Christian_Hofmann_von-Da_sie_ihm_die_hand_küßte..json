{"dta.poem.18376": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Da sie ihm die hand k\u00fc\u00dfte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df/ du weist es nicht/ was ich hiemit will schreiben/", "tokens": ["Ich", "wei\u00df", "/", "du", "weist", "es", "nicht", "/", "was", "ich", "hie\u00b7mit", "will", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$(", "PWS", "PPER", "PAV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum ietzt meine hand dir diese reimen schickt/", "tokens": ["Wa\u00b7rum", "ietzt", "mei\u00b7ne", "hand", "dir", "die\u00b7se", "rei\u00b7men", "schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "PPER", "PDS", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ich hier diesem brieff und blat wil einverleiben/", "tokens": ["Was", "ich", "hier", "die\u00b7sem", "brieff", "und", "blat", "wil", "ein\u00b7ver\u00b7lei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PDAT", "NN", "KON", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auff welche ziffer sey mein zeiger hinger\u00fcckt.", "tokens": ["Auff", "wel\u00b7che", "zif\u00b7fer", "sey", "mein", "zei\u00b7ger", "hin\u00b7ge\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du weist/ Ambrette/ wol/ da\u00df da das gl\u00fcck mich f\u00fchrte", "tokens": ["Du", "weist", "/", "Am\u00b7bret\u00b7te", "/", "wol", "/", "da\u00df", "da", "das", "gl\u00fcck", "mich", "f\u00fchr\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "NE", "$(", "ADV", "$(", "KOUS", "ADV", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit beyden h\u00e4uden selbst in deine zimmer ein/", "tokens": ["Mit", "bey\u00b7den", "h\u00e4u\u00b7den", "selbst", "in", "dei\u00b7ne", "zim\u00b7mer", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df da dein purpur-mund mit zauberey mich r\u00fchrte/", "tokens": ["Da\u00df", "da", "dein", "pur\u00b7pur\u00b7mund", "mit", "zau\u00b7be\u00b7rey", "mich", "r\u00fchr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df meine schlechte hand dein buhler muste seyn.", "tokens": ["Da\u00df", "mei\u00b7ne", "schlech\u00b7te", "hand", "dein", "buh\u00b7ler", "mus\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du spieltst verschwenderisch mit deinen anmuths-k\u00fcssen/", "tokens": ["Du", "spieltst", "ver\u00b7schwen\u00b7de\u00b7risch", "mit", "dei\u00b7nen", "an\u00b7muths\u00b7k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und streutest ohne zahl viel biesam-k\u00f6rner hin.", "tokens": ["Und", "streu\u00b7test", "oh\u00b7ne", "zahl", "viel", "bie\u00b7sam\u00b7k\u00f6r\u00b7ner", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du liest gantz ohne ma\u00df auf meine h\u00e4nde fliessen", "tokens": ["Du", "liest", "gantz", "oh\u00b7ne", "ma\u00df", "auf", "mei\u00b7ne", "h\u00e4n\u00b7de", "flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "APPR", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der lippen honigseim/ wovon ich truncken bin.", "tokens": ["Der", "lip\u00b7pen", "ho\u00b7ni\u00b7gseim", "/", "wo\u00b7von", "ich", "trun\u00b7cken", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$(", "PWAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein kluges weigern selbst schlug einen ku\u00df mir abe/", "tokens": ["Dein", "klu\u00b7ges", "wei\u00b7gern", "selbst", "schlug", "ei\u00b7nen", "ku\u00df", "mir", "a\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ADV", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und gab ihn meiner hand nicht ohne sondre lust.", "tokens": ["Und", "gab", "ihn", "mei\u00b7ner", "hand", "nicht", "oh\u00b7ne", "sond\u00b7re", "lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach da\u00df mein blasser mund nicht das genossen habe/", "tokens": ["Ach", "da\u00df", "mein", "blas\u00b7ser", "mund", "nicht", "das", "ge\u00b7nos\u00b7sen", "ha\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "PDS", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Worvon jetzt auf der hand der fliegen vorwitz kost!", "tokens": ["Wor\u00b7von", "jetzt", "auf", "der", "hand", "der", "flie\u00b7gen", "vor\u00b7witz", "kost", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du weist/ da\u00df vor das paar der beissenden rubinen", "tokens": ["Du", "weist", "/", "da\u00df", "vor", "das", "paar", "der", "beis\u00b7sen\u00b7den", "ru\u00b7bi\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "APPR", "ART", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Natur und liebe schon ein ander ziel gesetzt/", "tokens": ["Na\u00b7tur", "und", "lie\u00b7be", "schon", "ein", "an\u00b7der", "ziel", "ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "ART", "ADJD", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df sie den lippen nur zum becher sollen dienen/", "tokens": ["Da\u00df", "sie", "den", "lip\u00b7pen", "nur", "zum", "be\u00b7cher", "sol\u00b7len", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPRART", "ADJA", "VMFIN", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Die reiner himmels-thau verschwenderisch benetzt.", "tokens": ["Die", "rei\u00b7ner", "him\u00b7mels\u00b7\u00b7thau", "ver\u00b7schwen\u00b7de\u00b7risch", "be\u00b7netzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein ku\u00df bleibt nur ein pfand der purpur-rothen lippen/", "tokens": ["Ein", "ku\u00df", "bleibt", "nur", "ein", "pfand", "der", "pur\u00b7pur\u00b7ro\u00b7then", "lip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er paart sich ungereimt mit einer schlechten hand:", "tokens": ["Er", "paart", "sich", "un\u00b7ge\u00b7reimt", "mit", "ei\u00b7ner", "schlech\u00b7ten", "hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er st\u00f6\u00dfet seine krafft und sch\u00e4rffe ab in klippen/", "tokens": ["Er", "st\u00f6\u00b7\u00dfet", "sei\u00b7ne", "krafft", "und", "sch\u00e4rf\u00b7fe", "ab", "in", "klip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PTKVZ", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn er nicht wehlt den mund vor sein gelobtes land.", "tokens": ["Wenn", "er", "nicht", "wehlt", "den", "mund", "vor", "sein", "ge\u00b7lob\u00b7tes", "land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die biene selbst verliehrt den stachel an den hecken/", "tokens": ["Die", "bie\u00b7ne", "selbst", "ver\u00b7liehrt", "den", "sta\u00b7chel", "an", "den", "he\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn sich ihr leichter fu\u00df nicht auf die rosen setzt.", "tokens": ["Wenn", "sich", "ihr", "leich\u00b7ter", "fu\u00df", "nicht", "auf", "die", "ro\u00b7sen", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJD", "PTKVZ", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und wenn ein seiden-wurm in werck sich wil verstecken/", "tokens": ["Und", "wenn", "ein", "sei\u00b7den\u00b7wurm", "in", "werck", "sich", "wil", "ver\u00b7ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN", "PRF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So f\u00fchlet er/ wie sich sein krummer leib verletzt.", "tokens": ["So", "f\u00fch\u00b7let", "er", "/", "wie", "sich", "sein", "krum\u00b7mer", "leib", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PWAV", "PRF", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ein ku\u00df mu\u00df ohne krafft auf einer hand ersterben/", "tokens": ["Ein", "ku\u00df", "mu\u00df", "oh\u00b7ne", "krafft", "auf", "ei\u00b7ner", "hand", "ers\u00b7ter\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Er f\u00fchlet keinen mund/ der wieder k\u00fcst und bei\u00dft/", "tokens": ["Er", "f\u00fch\u00b7let", "kei\u00b7nen", "mund", "/", "der", "wie\u00b7der", "k\u00fcst", "und", "bei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "ART", "ADV", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der beste balsam mu\u00df wie schlechtes schmaltz verderben/", "tokens": ["Der", "bes\u00b7te", "bal\u00b7sam", "mu\u00df", "wie", "schlech\u00b7tes", "schmaltz", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "KOKOM", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn er nicht an der hand und nur an w\u00e4nden gleist.", "tokens": ["Wenn", "er", "nicht", "an", "der", "hand", "und", "nur", "an", "w\u00e4n\u00b7den", "gleist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "ART", "NN", "KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein ku\u00df lebt auf dem mund/ wie v\u00f6gel in den l\u00fcfften/", "tokens": ["Ein", "ku\u00df", "lebt", "auf", "dem", "mund", "/", "wie", "v\u00f6\u00b7gel", "in", "den", "l\u00fcff\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$(", "KOKOM", "NN", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Er nennet nicht die hand sein rechtes element/", "tokens": ["Er", "nen\u00b7net", "nicht", "die", "hand", "sein", "rech\u00b7tes", "e\u00b7le\u00b7ment", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er stirbt auf ihrem feld/ wie fisch in berg und kl\u00fcfften/", "tokens": ["Er", "stirbt", "auf", "ih\u00b7rem", "feld", "/", "wie", "fisch", "in", "berg", "und", "kl\u00fcff\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "PWAV", "ADJD", "APPR", "NE", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und f\u00fchlt das k\u00fctzeln nicht/ das mund und lippen kennt.", "tokens": ["Und", "f\u00fchlt", "das", "k\u00fct\u00b7zeln", "nicht", "/", "das", "mund", "und", "lip\u00b7pen", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "VVFIN", "PTKNEG", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein ku\u00df steht auf der hand wie klee auf rosen-st\u00f6cken/", "tokens": ["Ein", "ku\u00df", "steht", "auf", "der", "hand", "wie", "klee", "auf", "ro\u00b7sen\u00b7st\u00f6\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "KOKOM", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wie reimt sich aber doch di\u00df gantz ungleiche paar?", "tokens": ["Wie", "reimt", "sich", "a\u00b7ber", "doch", "di\u00df", "gantz", "un\u00b7glei\u00b7che", "paar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "ADV", "PDS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie steht ein goldnes band auff Mesolauen r\u00f6cken/", "tokens": ["Wie", "steht", "ein", "gold\u00b7nes", "band", "auff", "Me\u00b7so\u00b7lau\u00b7en", "r\u00f6\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wie ziert ein perlen-krantz ein wichsel-zopsicht haar?", "tokens": ["Wie", "ziert", "ein", "per\u00b7len\u00b7krantz", "ein", "wich\u00b7sel\u00b7zop\u00b7sicht", "haar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Drumb/ siehe/ sch\u00f6nstes kind/ und lerne anders paaren/", "tokens": ["Drumb", "/", "sie\u00b7he", "/", "sch\u00f6ns\u00b7tes", "kind", "/", "und", "ler\u00b7ne", "an\u00b7ders", "paa\u00b7ren", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "VVIMP", "$(", "ADJA", "NN", "$(", "KON", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ein mund nimt einen mund doch stets zum nachbar an.", "tokens": ["Ein", "mund", "nimt", "ei\u00b7nen", "mund", "doch", "stets", "zum", "nach\u00b7bar", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "ADV", "APPRART", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "La\u00df deine anmut doch noch ihre sch\u00e4tze sparen/", "tokens": ["La\u00df", "dei\u00b7ne", "an\u00b7mut", "doch", "noch", "ih\u00b7re", "sch\u00e4t\u00b7ze", "spa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "ADV", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ich wei\u00df schon allzuwohl/ was ihre k\u00fctzlung kan.", "tokens": ["Ich", "wei\u00df", "schon", "all\u00b7zu\u00b7wohl", "/", "was", "ih\u00b7re", "k\u00fctz\u00b7lung", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$(", "PWS", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die hand wird sich schon selbst ein ander feld erwehlen/", "tokens": ["Die", "hand", "wird", "sich", "schon", "selbst", "ein", "an\u00b7der", "feld", "er\u00b7weh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Sie kennet ihren feind und ihre schantze schon/", "tokens": ["Sie", "ken\u00b7net", "ih\u00b7ren", "feind", "und", "ih\u00b7re", "schant\u00b7ze", "schon", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Sie wird/ verg\u00f6nn es nur/ dir schon was anders stehlen/", "tokens": ["Sie", "wird", "/", "ver\u00b7g\u00f6nn", "es", "nur", "/", "dir", "schon", "was", "an\u00b7ders", "steh\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "VVFIN", "PPER", "ADV", "$(", "PPER", "ADV", "PWS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und suchen ihre lust und ihren eignen thron.", "tokens": ["Und", "su\u00b7chen", "ih\u00b7re", "lust", "und", "ih\u00b7ren", "eig\u00b7nen", "thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Sie wird dem munde schon die lippen \u00fcberla\u00dfen/", "tokens": ["Sie", "wird", "dem", "mun\u00b7de", "schon", "die", "lip\u00b7pen", "\u00fc\u00b7berl\u00b7a\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "VVFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Sie wehlet nur vor sich/ du weist ja/ sch\u00f6nste/ was/", "tokens": ["Sie", "weh\u00b7let", "nur", "vor", "sich", "/", "du", "weist", "ja", "/", "sch\u00f6ns\u00b7te", "/", "was", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PRF", "$(", "PPER", "VVFIN", "ADV", "$(", "VVFIN", "$(", "PWS", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Sie will das schwester-paar in ihre zirckel fa\u00dfen/", "tokens": ["Sie", "will", "das", "schwes\u00b7ter\u00b7paar", "in", "ih\u00b7re", "zir\u00b7ckel", "fa\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Auff denen ros\u2019 und nelck in schnee und marmel sa\u00df.", "tokens": ["Auff", "de\u00b7nen", "ros'", "und", "nelck", "in", "schnee", "und", "mar\u00b7mel", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "KON", "NN", "APPR", "ADJA", "KON", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.53": {"text": "So paaret jedes sich nachs himmels schlu\u00df und willen/", "tokens": ["So", "paa\u00b7ret", "je\u00b7des", "sich", "nachs", "him\u00b7mels", "schlu\u00df", "und", "wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "PRF", "ADJA", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Ein ku\u00df ist vor den mund/ die hand ist vor die brust/", "tokens": ["Ein", "ku\u00df", "ist", "vor", "den", "mund", "/", "die", "hand", "ist", "vor", "die", "brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$(", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "So kan vergn\u00fcgung uns bi\u00df an den hal\u00df erf\u00fcllen/", "tokens": ["So", "kan", "ver\u00b7gn\u00fc\u00b7gung", "uns", "bi\u00df", "an", "den", "hal\u00df", "er\u00b7f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Wir finden/ da\u00df di\u00df sey ein k\u00f6nigreich voll lust.", "tokens": ["Wir", "fin\u00b7den", "/", "da\u00df", "di\u00df", "sey", "ein", "k\u00f6\u00b7nig\u00b7reich", "voll", "lust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PDS", "VAFIN", "ART", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}