{"dta.poem.250": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Leib- Aria  der  Cecilia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fcsseste Lust verliebter Gedancken", "tokens": ["S\u00fcs\u00b7ses\u00b7te", "Lust", "ver\u00b7lieb\u00b7ter", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "R\u00fchret mein Hertze/ und machet es wancken/", "tokens": ["R\u00fch\u00b7ret", "mein", "Hert\u00b7ze", "/", "und", "ma\u00b7chet", "es", "wan\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVFIN", "$(", "KON", "VVFIN", "PPER", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Da\u00df es nicht stets an einem klebt", "tokens": ["Da\u00df", "es", "nicht", "stets", "an", "ei\u00b7nem", "klebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "ART", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uber gemeinen Brauch sich hebt;", "tokens": ["U\u00b7ber", "ge\u00b7mei\u00b7nen", "Brauch", "sich", "hebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und darneben nur gedenckt/", "tokens": ["Und", "dar\u00b7ne\u00b7ben", "nur", "ge\u00b7denckt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "VVPP", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wie es sich fein offt verschenckt.", "tokens": ["Wie", "es", "sich", "fein", "offt", "ver\u00b7schenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "ADV", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Wechseln im Lieben lieb ich vor allen/", "tokens": ["Wech\u00b7seln", "im", "Lie\u00b7ben", "lieb", "ich", "vor", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "VVFIN", "PPER", "APPR", "PIAT", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sch\u00f6nheit und Pracht mu\u00df mehren gefallen/", "tokens": ["Sch\u00f6n\u00b7heit", "und", "Pracht", "mu\u00df", "meh\u00b7ren", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "VVINF", "VVPP", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Edele Seelen fesselt nicht", "tokens": ["E\u00b7de\u00b7le", "See\u00b7len", "fes\u00b7selt", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "PTKNEG"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Einem allein geschwohrne Pflicht;", "tokens": ["Ei\u00b7nem", "al\u00b7lein", "ge\u00b7schwohr\u00b7ne", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Schw\u00fcre sind ein leichter Schertz", "tokens": ["Schw\u00fc\u00b7re", "sind", "ein", "leich\u00b7ter", "Schertz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcr ein recht verliebtes Hertz.", "tokens": ["F\u00fcr", "ein", "recht", "ver\u00b7lieb\u00b7tes", "Hertz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einer hat offt viel Rosen gebrochen/", "tokens": ["Ei\u00b7ner", "hat", "offt", "viel", "Ro\u00b7sen", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Offt haben viel\u2019 ein R\u00f6slein berochen/", "tokens": ["Offt", "ha\u00b7ben", "viel'", "ein", "R\u00f6s\u00b7lein", "be\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Balsam hat vielen ohne Neid", "tokens": ["Bal\u00b7sam", "hat", "vie\u00b7len", "oh\u00b7ne", "Neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PIAT", "APPR", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Seinen Geruch zugleich geweyht:", "tokens": ["Sei\u00b7nen", "Ge\u00b7ruch", "zu\u00b7gleich", "ge\u00b7weyht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Wie soll denn was vielen behagt?", "tokens": ["Wie", "soll", "denn", "was", "vie\u00b7len", "be\u00b7hagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "KON", "PWS", "PIS", "VVFIN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Nur seyn einem zugesagt.", "tokens": ["Nur", "seyn", "ei\u00b7nem", "zu\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ART", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}