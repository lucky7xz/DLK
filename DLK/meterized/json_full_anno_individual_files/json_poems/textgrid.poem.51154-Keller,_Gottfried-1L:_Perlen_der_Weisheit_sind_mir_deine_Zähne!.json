{"textgrid.poem.51154": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Perlen der Weisheit sind mir deine Z\u00e4hne!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Perlen der Weisheit sind mir deine Z\u00e4hne!", "tokens": ["Per\u00b7len", "der", "Weis\u00b7heit", "sind", "mir", "dei\u00b7ne", "Z\u00e4h\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie stets ich mich nach ihrem Scheine sehne!", "tokens": ["Wie", "stets", "ich", "mich", "nach", "ih\u00b7rem", "Schei\u00b7ne", "seh\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn \u00fcber dem Bem\u00fchn, sie zu erblicken,", "tokens": ["Denn", "\u00fc\u00b7ber", "dem", "Be\u00b7m\u00fchn", ",", "sie", "zu", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vertrocknet mir die letzte kleine Tr\u00e4ne.", "tokens": ["Ver\u00b7trock\u00b7net", "mir", "die", "letz\u00b7te", "klei\u00b7ne", "Tr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Indem ich dich zu holdem Lachen reize,", "tokens": ["In\u00b7dem", "ich", "dich", "zu", "hol\u00b7dem", "La\u