{"textgrid.poem.67528": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbkomm!\u00ab sprach der Gott und schwang die zarten Fl\u00fcgel;", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbkomm!\u00ab sprach der Gott und schwang die zarten Fl\u00fcgel;", "tokens": ["\u00bb", "komm", "!", "\u00ab", "sprach", "der", "Gott", "und", "schwang", "die", "zar\u00b7ten", "Fl\u00fc\u00b7gel", ";"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbdenn hinter uns ergrimmt die Barbarei.", "tokens": ["\u00bb", "denn", "hin\u00b7ter", "uns", "er\u00b7grimmt", "die", "Bar\u00b7ba\u00b7rei", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erschwingen wir dort jene stillen H\u00fcgel,", "tokens": ["Er\u00b7schwin\u00b7gen", "wir", "dort", "je\u00b7ne", "stil\u00b7len", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Deine Brust wird mancher Sorgen frei;", "tokens": ["Und", "Dei\u00b7ne", "Brust", "wird", "man\u00b7cher", "Sor\u00b7gen", "frei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erblicken wirst Du in der Zeiten Spiegel", "tokens": ["Er\u00b7bli\u00b7cken", "wirst", "Du", "in", "der", "Zei\u00b7ten", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein Bild, und was in ihm veredelt sei.", "tokens": ["Dein", "Bild", ",", "und", "was", "in", "ihm", "ver\u00b7e\u00b7delt", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PWS", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zerstieben kann in g\u00f6ttlichen Gestalten", "tokens": ["Zer\u00b7stie\u00b7ben", "kann", "in", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der irdne Stoff; ", "tokens": ["Der", "ird\u00b7ne", "Stoff", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Sie schwebten auf, vorbei der heitern H\u00f6he,", "tokens": ["Sie", "schweb\u00b7ten", "auf", ",", "vor\u00b7bei", "der", "hei\u00b7tern", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf der mit Castor Pollux sie empfing.", "tokens": ["Auf", "der", "mit", "Cas\u00b7tor", "Pol\u00b7lux", "sie", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NE", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbo da\u00df ich Euch, Ihr Himmlischen, noch sehe,", "tokens": ["\u00bb", "o", "da\u00df", "ich", "Euch", ",", "Ihr", "Himm\u00b7li\u00b7schen", ",", "noch", "se\u00b7he", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "KOUS", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gerettet,\u00ab sprach die Kunst, \u00bbauf meinen Wink!", "tokens": ["Ge\u00b7ret\u00b7tet", ",", "\u00ab", "sprach", "die", "Kunst", ",", "\u00bb", "auf", "mei\u00b7nen", "Wink", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "$(", "VVFIN", "ART", "NN", "$,", "$(", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O da\u00df an Euch der Menschen Blick ersp\u00e4he,", "tokens": ["O", "da\u00df", "an", "Euch", "der", "Men\u00b7schen", "Blick", "er\u00b7sp\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "APPR", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "We\u00df Gro\u00dfen sich die Menschheit unterfing!", "tokens": ["We\u00df", "Gro\u00b7\u00dfen", "sich", "die", "Menschheit", "un\u00b7ter\u00b7fing", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "So bl\u00fchten einst durch mich der G\u00f6tter S\u00f6hne,", "tokens": ["So", "bl\u00fch\u00b7ten", "einst", "durch", "mich", "der", "G\u00f6t\u00b7ter", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und um sie schallten Pindar's hohe T\u00f6ne.\u00ab", "tokens": ["Und", "um", "sie", "schall\u00b7ten", "Pin\u00b7da\u00b7r's", "ho\u00b7he", "T\u00f6\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "NE", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie schwebten nieder. Jedem Heiligthume", "tokens": ["Sie", "schweb\u00b7ten", "nie\u00b7der", ".", "Je\u00b7dem", "Hei\u00b7lig\u00b7thu\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Kunst umwebte Amor heil'ge Nacht.", "tokens": ["Der", "Kunst", "um\u00b7web\u00b7te", "A\u00b7mor", "heil'\u00b7ge", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sieh, wie zu seiner Freundin hohem Ruhme", "tokens": ["Sieh", ",", "wie", "zu", "sei\u00b7ner", "Freun\u00b7din", "ho\u00b7hem", "Ruh\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An seiner Fackel neuer Reiz erwacht!", "tokens": ["An", "sei\u00b7ner", "Fa\u00b7ckel", "neu\u00b7er", "Reiz", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus jedem Kunstwerk sprie\u00dfet auf die Blume", "tokens": ["Aus", "je\u00b7dem", "Kunst\u00b7werk", "sprie\u00b7\u00dfet", "auf", "die", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Genius in sanfter, voller Pracht.", "tokens": ["Des", "Ge\u00b7nius", "in", "sanf\u00b7ter", ",", "vol\u00b7ler", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Vor Amor's Fackel gl\u00e4nzen auf \u2013 Ideen,", "tokens": ["Vor", "A\u00b7mor's", "Fa\u00b7ckel", "gl\u00e4n\u00b7zen", "auf", "\u2013", "I\u00b7deen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "APPR", "$(", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die (glaubt's der Liebe!) Liebe nur kann sehen.", "tokens": ["Die", "(", "glaubt's", "der", "Lie\u00b7be", "!", ")", "Lie\u00b7be", "nur", "kann", "se\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "VVFIN", "ART", "NN", "$.", "$(", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbdort,\u00ab sprach er, \u00bbDein Apollo! Unversehret", "tokens": ["\u00bb", "dort", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "Dein", "A\u00b7pol\u00b7lo", "!", "Un\u00b7ver\u00b7seh\u00b7ret"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word"], "pos": ["$(", "ADV", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "PPOSAT", "NE", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Steht er im Glanz der G\u00f6ttersch\u00f6ne da;", "tokens": ["Steht", "er", "im", "Glanz", "der", "G\u00f6t\u00b7ter\u00b7sch\u00f6\u00b7ne", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu Delos zwar wird er nicht mehr verehret,", "tokens": ["Zu", "De\u00b7los", "zwar", "wird", "er", "nicht", "mehr", "ver\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Doch jedem Jugendherzen ist er nah.", "tokens": ["Doch", "je\u00b7dem", "Ju\u00b7gend\u00b7her\u00b7zen", "ist", "er", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was er der Menschheit Himmlisches bescheret,", "tokens": ["Was", "er", "der", "Menschheit", "Himm\u00b7li\u00b7sches", "be\u00b7sche\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Was ihm dem Hirten, ihm dem Gott geschah,", "tokens": ["Was", "ihm", "dem", "Hir\u00b7ten", ",", "ihm", "dem", "Gott", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "$,", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sein Lorbeer, seine Lyra, seine Ch\u00f6re,", "tokens": ["Sein", "Lor\u00b7beer", ",", "sei\u00b7ne", "Ly\u00b7ra", ",", "sei\u00b7ne", "Ch\u00f6\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sein heilig Bild ist aller Zeiten Lehre.\u00ab", "tokens": ["Sein", "hei\u00b7lig", "Bild", "ist", "al\u00b7ler", "Zei\u00b7ten", "Leh\u00b7re", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "PIAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbund in der Jungfrau Herz, wie schleicht sich leise,\u00ab", "tokens": ["\u00bb", "und", "in", "der", "Jung\u00b7frau", "Herz", ",", "wie", "schleicht", "sich", "lei\u00b7se", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "APPR", "ART", "NN", "NN", "$,", "PWAV", "VVFIN", "PRF", "ADJD", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sprach Carita, \u00bbder wunders\u00fc\u00dfe Traum", "tokens": ["Sprach", "Ca\u00b7ri\u00b7ta", ",", "\u00bb", "der", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfe", "Traum"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Endymion's! Diana, keusch und weise,", "tokens": ["En\u00b7dy\u00b7mi\u00b7on's", "!", "Di\u00b7a\u00b7na", ",", "keusch", "und", "wei\u00b7se", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gef\u00fchrt von Amor selbst, sie schwebet kaum", "tokens": ["Ge\u00b7f\u00fchrt", "von", "A\u0