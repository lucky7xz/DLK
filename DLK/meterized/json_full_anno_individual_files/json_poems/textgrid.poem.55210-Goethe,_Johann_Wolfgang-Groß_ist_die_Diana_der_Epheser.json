{"textgrid.poem.55210": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Gro\u00df ist die Diana der Epheser", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Ephesus ein Goldschmied sa\u00df", "tokens": ["Zu", "E\u00b7phe\u00b7sus", "ein", "Gold\u00b7schmied", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner Werkstatt, pochte,", "tokens": ["In", "sei\u00b7ner", "Werk\u00b7statt", ",", "poch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So gut er konnt, ohn Unterla\u00df,", "tokens": ["So", "gut", "er", "konnt", ",", "ohn", "Un\u00b7ter\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VMFIN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So zierlich er's vermochte.", "tokens": ["So", "zier\u00b7lich", "er's", "ver\u00b7moch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als Knab und J\u00fcngling kniet' er schon", "tokens": ["Als", "Knab", "und", "J\u00fcng\u00b7ling", "kniet'", "er", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Tempel vor der G\u00f6ttin Thron", "tokens": ["Im", "Tem\u00b7pel", "vor", "der", "G\u00f6t\u00b7tin", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und hatte den G\u00fcrtel unter den Br\u00fcsten,", "tokens": ["Und", "hat\u00b7te", "den", "G\u00fcr\u00b7tel", "un\u00b7ter", "den", "Br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Worin so manche Tiere nisten,", "tokens": ["Wo\u00b7rin", "so", "man\u00b7che", "Tie\u00b7re", "nis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Zu Hause treulich nachgefeilt,", "tokens": ["Zu", "Hau\u00b7se", "treu\u00b7lich", "nach\u00b7ge\u00b7feilt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie's ihm der Vater zugeteilt;", "tokens": ["Wie's", "ihm", "der", "Va\u00b7ter", "zu\u00b7ge\u00b7teilt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und leitete sein kunstreich Streben", "tokens": ["Und", "lei\u00b7te\u00b7te", "sein", "kuns\u00b7treich", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "In frommer Wirkung durch das Leben.", "tokens": ["In", "from\u00b7mer", "Wir\u00b7kung", "durch", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da h\u00f6rt er denn auf einmal laut", "tokens": ["Da", "h\u00f6rt", "er", "denn", "auf", "ein\u00b7mal", "laut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eines Gassenvolkes Windesbraut,", "tokens": ["Ei\u00b7nes", "Gas\u00b7sen\u00b7vol\u00b7kes", "Win\u00b7des\u00b7braut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Als g\u00e4b's einen Gott so im Gehirn,", "tokens": ["Als", "g\u00e4b's", "ei\u00b7nen", "Gott", "so", "im", "Ge\u00b7hirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da, hinter des Menschen alberner Stirn,", "tokens": ["Da", ",", "hin\u00b7ter", "des", "Men\u00b7schen", "al\u00b7ber\u00b7ner", "Stirn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der sei viel herrlicher als das Wesen,", "tokens": ["Der", "sei", "viel", "herr\u00b7li\u00b7cher", "als", "das", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "An dem wir die Breite der Gottheit lesen.", "tokens": ["An", "dem", "wir", "die", "Brei\u00b7te", "der", "Got\u00b7theit", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "