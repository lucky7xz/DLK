{"textgrid.poem.52811": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Menschenbewu\u00dftsein", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie mu\u00df ich meinem Sch\u00f6pfer danken,", "tokens": ["Wie", "mu\u00df", "ich", "mei\u00b7nem", "Sch\u00f6p\u00b7fer", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die ohne sittlichen Gedanken,", "tokens": ["Die", "oh\u00b7ne", "sitt\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Kothe nur sich w\u00e4lzt und scharrt,", "tokens": ["Im", "Ko\u00b7the", "nur", "sich", "w\u00e4lzt", "und", "scharrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin ein Mensch, vor Gott zu wandeln,", "tokens": ["Ich", "bin", "ein", "Mensch", ",", "vor", "Gott", "zu", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Schamgef\u00fchl bringt mir mein Handeln.", "tokens": ["Kein", "Scham\u00b7ge\u00b7f\u00fchl", "bringt", "mir", "mein", "Han\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4r' ich ein Ochse an der Krippe,", "tokens": ["W\u00e4r'", "ich", "ein", "O\u00b7chse", "an", "der", "Krip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich w\u00fc\u00dfte mir zu helfen nicht,", "tokens": ["Ich", "w\u00fc\u00df\u00b7te", "mir", "zu", "hel\u00b7fen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Wort belebte meine Lippe,", "tokens": ["Kein", "Wort", "be\u00b7leb\u00b7te", "mei\u00b7ne", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verstehen k\u00f6nnt' ich kein Gedicht.", "tokens": ["Ver\u00b7ste\u00b7hen", "k\u00f6nnt'", "ich", "kein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie m\u00fc\u00dft' ", "tokens": ["Wie", "m\u00fc\u00dft'"], "token_info": ["word", "word"], "pos": ["PWAV", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Wenn ich nur k\u00f6nnte sinnlos br\u00fclen!", "tokens": ["Wenn", "ich", "nur", "k\u00f6nn\u00b7te", "sinn\u00b7los", "br\u00fc\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+