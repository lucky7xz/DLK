{"dta.poem.6911": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Damit wir nun den Unterscheid", "tokens": ["Da\u00b7mit", "wir", "nun", "den", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der C\u00f6rper, der Jdee, und ihrer W\u00fcrcklichkeit", "tokens": ["Der", "C\u00f6r\u00b7per", ",", "der", "Jdee", ",", "und", "ih\u00b7rer", "W\u00fcrck\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Recht deutlich finden;", "tokens": ["Recht", "deut\u00b7lich", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "So darf man dieses nur erwegen:", "tokens": ["So", "darf", "man", "die\u00b7ses", "nur", "er\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PDAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie f\u00fchlen und erkennen, sey", "tokens": ["Sie", "f\u00fch\u00b7len", "und", "er\u00b7ken\u00b7nen", ",", "sey"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durchaus nicht einerley.", "tokens": ["Durc\u00b7haus", "nicht", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie sind verm\u00f6gend, sich an Tag zu legen,", "tokens": ["Sie", "sind", "ver\u00b7m\u00f6\u00b7gend", ",", "sich", "an", "Tag", "zu", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Durch unsrer Sinnen H\u00fclff; allein,", "tokens": ["Durch", "uns\u00b7rer", "Sin\u00b7nen", "H\u00fclff", ";", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es ist nur blo\u00df der Geist, dem C\u00f6rper kenntlich seyn.", "tokens": ["Es", "ist", "nur", "blo\u00df", "der", "Geist", ",", "dem", "C\u00f6r\u00b7per", "kennt\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$,", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es ist im Geist, so gar", "tokens": ["Es", "ist", "im", "Geist", ",", "so", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Der C\u00f6rper Handlung selbst, unc\u00f6rperlich.", "tokens": ["Der", "C\u00f6r\u00b7per", "Hand\u00b7lung", "selbst", ",", "un\u00b7c\u00f6r\u00b7per\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ein jegliche Jdee, ob sie gleich gantz gemein,", "tokens": ["Ein", "jeg\u00b7li\u00b7che", "Jdee", ",", "ob", "sie", "gleich", "gantz", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+++-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Kan nicht materialisch seyn,", "tokens": ["Kan", "nicht", "ma\u00b7te\u00b7ri\u00b7a\u00b7lisch", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.14": {"text": "Mit ihr vermischt kein C\u00f6rper sich,", "tokens": ["Mit", "ihr", "ver\u00b7mischt", "kein", "C\u00f6r\u00b7per", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und ein\u2019 Jdee geh\u00f6ret gantz dem Geist,", "tokens": ["Und", "ein'", "Jdee", "ge\u00b7h\u00f6\u00b7ret", "gantz", "dem", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.16": {"text": "Als welcher nur allein empfindet.", "tokens": ["Als", "wel\u00b7cher", "nur", "al\u00b7lein", "emp\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sie ist der C\u00f6rper nicht, an welchen sie sich bindet,", "tokens": ["Sie", "ist", "der", "C\u00f6r\u00b7per", "nicht", ",", "an", "wel\u00b7chen", "sie", "sich", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "$,", "APPR", "PWAT", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und auch das Werckzeng nicht, worein sein Druck sich findet.", "tokens": ["Und", "auch", "das", "Wer\u00b7ck\u00b7zeng", "nicht", ",", "wo\u00b7rein", "sein", "Druck", "sich", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKNEG", "$,", "PWAV", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Sie ist, Exempels-Weis\u2019, Geruch, Ton, Farb\u2019 und Licht,", "tokens": ["Sie", "ist", ",", "Ex\u00b7em\u00b7pels\u00b7Weis'", ",", "Ge\u00b7ruch", ",", "Ton", ",", "Fa\u00b7rb'", "und", "Licht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-++-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Die, da dieselbigen sich nicht", "tokens": ["Die", ",", "da", "die\u00b7sel\u00b7bi\u00b7gen", "sich", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PDAT", "PRF", "PTKNEG"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.21": {"text": "In der Materie befinden, und allein", "tokens": ["In", "der", "Ma\u00b7te\u00b7rie", "be\u00b7fin\u00b7den", ",", "und", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KON", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.22": {"text": "In ihrem Eindruck seyn,", "tokens": ["In", "ih\u00b7rem", "Ein\u00b7druck", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Worauf die Seele merckt; so finden sich darinnen", "tokens": ["Wo\u00b7rauf", "die", "See\u00b7le", "merckt", ";", "so", "fin\u00b7den", "sich", "da\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So wenig Vorw\u00fcrff\u2019 als auch Sinnen.", "tokens": ["So", "we\u00b7nig", "Vor\u00b7w\u00fcrff'", "als", "auch", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KOKOM", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und wird nur blo\u00df die Art zum Seyn", "tokens": ["Und", "wird", "nur", "blo\u00df", "die", "Art", "zum", "Seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "In selbiger gesp\u00fcret,", "tokens": ["In", "sel\u00b7bi\u00b7ger", "ge\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Die durch derselbigen Gelegenheit allein", "tokens": ["Die", "durch", "der\u00b7sel\u00b7bi\u00b7gen", "Ge\u00b7le\u00b7gen\u00b7heit", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "In unsern Seelen sich gebiehret.", "tokens": ["In", "un\u00b7sern", "See\u00b7len", "sich", "ge\u00b7bieh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}