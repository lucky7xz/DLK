{"textgrid.poem.42721": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[es trafen sich von ungef\u00e4hr]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es trafen sich von ungef\u00e4hr", "tokens": ["Es", "tra\u00b7fen", "sich", "von", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wolf, ein Mensch, sowie ein B\u00e4r,", "tokens": ["Ein", "Wolf", ",", "ein", "Mensch", ",", "so\u00b7wie", "ein", "B\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weil sie lange nichts gegessen,", "tokens": ["Und", "weil", "sie", "lan\u00b7ge", "nichts", "ge\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So haben sie sich aufgefressen.", "tokens": ["So", "ha\u00b7ben", "sie", "sich", "auf\u00b7ge\u00b7fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wolf den Menschen, der den B\u00e4r,", "tokens": ["Der", "Wolf", "den", "Men\u00b7schen", ",", "der", "den", "B\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der B\u00e4r den Wolf. \u2013 Es schmeckte sehr", "tokens": ["Der", "B\u00e4r", "den", "Wolf", ".", "\u2013", "Es", "schmeck\u00b7te", "sehr"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NE", "$.", "$(", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und blieb nichts \u00fcbrig als ein Tuch,", "tokens": ["Und", "blieb", "nichts", "\u00fcb\u00b7rig", "als", "ein", "Tuch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drei Haare und ein W\u00f6rterbuch.", "tokens": ["Drei", "Haa\u00b7re", "und", "ein", "W\u00f6r\u00b7ter\u00b7buch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das war der Nachla\u00df dieser drei.", "tokens": ["Das", "war", "der", "Nachl\u00b7a\u00df", "die\u00b7ser", "drei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PDAT", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der eine Mensch, der hie\u00df Karl May.", "tokens": ["Der", "ei\u00b7ne", "Mensch", ",", "der", "hie\u00df", "Karl", "May", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "PRELS", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es trafen sich von ungef\u00e4hr", "tokens": ["Es", "tra\u00b7fen", "sich", "von", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.