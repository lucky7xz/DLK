{"dta.poem.14186": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber'n Garten durch die L\u00fcfte", "tokens": ["Ue\u00b7ber'n", "Gar\u00b7ten", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt' ich Wanderv\u00f6gel zieh'n,", "tokens": ["H\u00f6rt'", "ich", "Wan\u00b7der\u00b7v\u00f6\u00b7gel", "zieh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das bedeutet Fr\u00fchlingsd\u00fcfte,", "tokens": ["Das", "be\u00b7deu\u00b7tet", "Fr\u00fch\u00b7lings\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unten f\u00e4ngt's schon an zu bl\u00fch'n.", "tokens": ["Un\u00b7ten", "f\u00e4ngt's", "schon", "an", "zu", "bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "APPR", "NE", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}