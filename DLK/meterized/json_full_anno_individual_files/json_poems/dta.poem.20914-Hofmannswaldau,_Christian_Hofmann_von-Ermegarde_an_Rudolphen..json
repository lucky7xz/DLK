{"dta.poem.20914": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ermegarde an Rudolphen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer ist ein kleiner Brief mit Schertz und", "tokens": ["Hjer", "ist", "ein", "klei\u00b7ner", "Brief", "mit", "Schertz", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ernst gef\u00fcllet/", "tokens": ["Ernst", "ge\u00b7f\u00fcl\u00b7let", "/"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der Gall\u2019 und Honigseim in seiner Scho\u00df", "tokens": ["Der", "Gall'", "und", "Ho\u00b7ni\u00b7gseim", "in", "sei\u00b7ner", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "enth\u00e4llt/", "tokens": ["ent\u00b7h\u00e4llt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Au\u00df welchen/ gl\u00e4ub es mir/ dir Todt und Leben qvillet/", "tokens": ["Au\u00df", "wel\u00b7chen", "/", "gl\u00e4ub", "es", "mir", "/", "dir", "Todt", "und", "Le\u00b7ben", "qvil\u00b7let", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "$(", "VVFIN", "PPER", "PPER", "$(", "PPER", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erwehle dir nunmehr die\u00df was dir wohlgef\u00e4llt.", "tokens": ["Er\u00b7weh\u00b7le", "dir", "nun\u00b7mehr", "die\u00df", "was", "dir", "wohl\u00b7ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDS", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich lasse dich itzund mich ohne Ma\u00dfqve schauen/", "tokens": ["Ich", "las\u00b7se", "dich", "it\u00b7zund", "mich", "oh\u00b7ne", "Ma\u00df\u00b7qve", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich stelle deinen Fall in hellen Farben f\u00fcr/", "tokens": ["Ich", "stel\u00b7le", "dei\u00b7nen", "Fall", "in", "hel\u00b7len", "Far\u00b7ben", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und willst du alzuviel auf deine Reuter trauen/", "tokens": ["Und", "willst", "du", "al\u00b7zu\u00b7viel", "auf", "dei\u00b7ne", "Reu\u00b7ter", "trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So hab ich mehr als du: die Hertzen seyn bey mir.", "tokens": ["So", "hab", "ich", "mehr", "als", "du", ":", "die", "Hert\u00b7zen", "seyn", "bey", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KOUS", "PPER", "$.", "ART", "NN", "VAINF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein Wort/ ein Blick von mir kann tausend Lantzen", "tokens": ["Ein", "Wort", "/", "ein", "Blick", "von", "mir", "kann", "tau\u00b7send", "Lant\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "APPR", "PPER", "VMFIN", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "stehlen/", "tokens": ["steh\u00b7len", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Die besten Bogen seyn auf mein Geboth gespannt/", "tokens": ["Die", "bes\u00b7ten", "Bo\u00b7gen", "seyn", "auf", "mein", "Ge\u00b7both", "ge\u00b7spannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Es wird mir nimmermehr an grossen Helden fehlen/", "tokens": ["Es", "wird", "mir", "nim\u00b7mer\u00b7mehr", "an", "gros\u00b7sen", "Hel\u00b7den", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Als Schlangen hab ich sie zu meiner Fahn gebannt.", "tokens": ["Als", "Schlan\u00b7gen", "hab", "ich", "sie", "zu", "mei\u00b7ner", "Fahn", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wa\u00df nur zwey Finger r\u00fchrt hat dir den Todt geschwo-", "tokens": ["Wa\u00df", "nur", "zwey", "Fin\u00b7ger", "r\u00fchrt", "hat", "dir", "den", "Todt", "ge\u00b7schwo"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "CARD", "NN", "VVFIN", "VAFIN", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "ren/", "tokens": ["ren", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Du bist mir allbereit im Geiste hingericht/", "tokens": ["Du", "bist", "mir", "all\u00b7be\u00b7reit", "im", "Geis\u00b7te", "hin\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Begr\u00fcst du mich als Feind/ so halt dich vor verlohren/", "tokens": ["Be\u00b7gr\u00fcst", "du", "mich", "als", "Feind", "/", "so", "halt", "dich", "vor", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "KOUS", "NN", "$(", "ADV", "VVFIN", "PRF", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Verschertze doch dein Volck und dich auch selber nicht.", "tokens": ["Ver\u00b7schert\u00b7ze", "doch", "dein", "Volck", "und", "dich", "auch", "sel\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "KON", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was nur nach Eisen reucht begehrt dich zuerdr\u00fccken/", "tokens": ["Was", "nur", "nach", "Ei\u00b7sen", "reucht", "be\u00b7gehrt", "dich", "zu\u00b7er\u00b7dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VVFIN", "VVPP", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Drum/ dencke wo du bist/ und endlich was du thust/", "tokens": ["Drum", "/", "den\u00b7cke", "wo", "du", "bist", "/", "und", "end\u00b7lich", "was", "du", "thust", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "VVFIN", "PWAV", "PPER", "VAFIN", "$(", "KON", "ADV", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich darf nur einen Blick nach deinem Lager schicken/", "tokens": ["Ich", "darf", "nur", "ei\u00b7nen", "Blick", "nach", "dei\u00b7nem", "La\u00b7ger", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So kehrt dein eigen Schwerdt sich gegen deine Brust.", "tokens": ["So", "kehrt", "dein", "ei\u00b7gen", "Schwerdt", "sich", "ge\u00b7gen", "dei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ach K\u00f6nig wilt du dich mit Hoffnungs Speisen neh-", "tokens": ["Ach", "K\u00f6\u00b7nig", "wilt", "du", "dich", "mit", "Hoff\u00b7nungs", "Spei\u00b7sen", "neh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "VMFIN", "PPER", "PRF", "APPR", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "ren/", "tokens": ["ren", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.27": {"text": "Sie blehen trefflich auf und geben keine Krafft/", "tokens": ["Sie", "ble\u00b7hen", "treff\u00b7lich", "auf", "und", "ge\u00b7ben", "kei\u00b7ne", "Krafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "KON", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wer ohne rechten Grund will alzuviel begehren/", "tokens": ["Wer", "oh\u00b7ne", "rech\u00b7ten", "Grund", "will", "al\u00b7zu\u00b7viel", "be\u00b7geh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dem wird auch was er hat/ noch endlich hingeraft.", "tokens": ["Dem", "wird", "auch", "was", "er", "hat", "/", "noch", "end\u00b7lich", "hin\u00b7ge\u00b7raft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PWS", "PPER", "VAFIN", "$(", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Kein Spiegel treuget mehr/ als den der Wahn uns", "tokens": ["Kein", "Spie\u00b7gel", "treu\u00b7get", "mehr", "/", "als", "den", "der", "Wahn", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$(", "KOUS", "ART", "ART", "NN", "PPER"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "zeiget/", "tokens": ["zei\u00b7get", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Gefahr mu\u00df hier ein Zwerg/ Gel\u00fcck ein Riese seyn/", "tokens": ["Ge\u00b7fahr", "mu\u00df", "hier", "ein", "Zwerg", "/", "Ge\u00b7l\u00fcck", "ein", "Rie\u00b7se", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ART", "NN", "$(", "NN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Man schaut wie unsre Lust aus Zucker Rosen steiget/", "tokens": ["Man", "schaut", "wie", "uns\u00b7re", "Lust", "aus", "Zu\u00b7cker", "Ro\u00b7sen", "stei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "PPOSAT", "NN", "APPR", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Man sp\u00fcret keine Nacht/ nur lauter Sonnenschein.", "tokens": ["Man", "sp\u00fc\u00b7ret", "kei\u00b7ne", "Nacht", "/", "nur", "lau\u00b7ter", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "$(", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es zeiget sich allhier ein Jahrmarckt voller Cronen/", "tokens": ["Es", "zei\u00b7get", "sich", "all\u00b7hier", "ein", "Jahr\u00b7marckt", "vol\u00b7ler", "Cro\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Die Scepter scheinen uns wie ein gemeiner Stab/", "tokens": ["Die", "Scep\u00b7ter", "schei\u00b7nen", "uns", "wie", "ein", "ge\u00b7mei\u00b7ner", "Stab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Lorber Kr\u00e4ntze seyn gemeiner als die Bohnen/", "tokens": ["Die", "Lor\u00b7ber", "Kr\u00e4nt\u00b7ze", "seyn", "ge\u00b7mei\u00b7ner", "als", "die", "Boh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "ADJA", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Hier ist kein Helden Fall und auch kein Todten Grab.", "tokens": ["Hier", "ist", "kein", "Hel\u00b7den", "Fall", "und", "auch", "kein", "Tod\u00b7ten", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "NN", "KON", "ADV", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Doch endlich will uns nur di\u00df Lust Schlo\u00df gantz ver-", "tokens": ["Doch", "end\u00b7lich", "will", "uns", "nur", "di\u00df", "Lust", "Schlo\u00df", "gantz", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "PDS", "NN", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-++--", "measure": "unknown.measure.penta"}, "line.40": {"text": "schwinden/", "tokens": ["schwin\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.41": {"text": "Der F\u00fcrhang f\u00e4llt herab/ das Spiel ist ausgemacht/", "tokens": ["Der", "F\u00fcr\u00b7hang", "f\u00e4llt", "her\u00b7ab", "/", "das", "Spiel", "ist", "aus\u00b7ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die Lampen leschen aus/ es ist nichts mehr dahinden/", "tokens": ["Die", "Lam\u00b7pen", "le\u00b7schen", "aus", "/", "es", "ist", "nichts", "mehr", "da\u00b7hin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "PPER", "VAFIN", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Man mercket nichts als Rauch/ und sp\u00fchret nichts als", "tokens": ["Man", "mer\u00b7cket", "nichts", "als", "Rauch", "/", "und", "sp\u00fch\u00b7ret", "nichts", "als"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIS", "KOKOM", "NN", "$(", "KON", "VVFIN", "PIS", "KOKOM"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Nacht.", "tokens": ["Nacht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.45": {"text": "Dann steht man gantz betr\u00fcbt mit wunder-schlaffen", "tokens": ["Dann", "steht", "man", "gantz", "be\u00b7tr\u00fcbt", "mit", "wun\u00b7der\u00b7schlaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "H\u00e4nden/", "tokens": ["H\u00e4n\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.47": {"text": "Und schaut was man gethan/ mit neuen Augen an;", "tokens": ["Und", "schaut", "was", "man", "ge\u00b7than", "/", "mit", "neu\u00