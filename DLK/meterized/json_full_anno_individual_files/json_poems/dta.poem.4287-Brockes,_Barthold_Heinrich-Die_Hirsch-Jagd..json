{"dta.poem.4287": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Hirsch-Jagd.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir sahen G\u00e4rten in der Luft wohl eh' in alten Zeiten", "tokens": ["Wir", "sa\u00b7hen", "G\u00e4r\u00b7ten", "in", "der", "Luft", "wohl", "eh'", "in", "al\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "prangen;", "tokens": ["pran\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Allein, wer sah wohl eine Jagd, wie hier, in freyen L\u00fcften", "tokens": ["Al\u00b7lein", ",", "wer", "sah", "wohl", "ei\u00b7ne", "Jagd", ",", "wie", "hier", ",", "in", "frey\u00b7en", "L\u00fcf\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWS", "VVFIN", "ADV", "ART", "NN", "$,", "PWAV", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "hangen?", "tokens": ["han\u00b7gen", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Es st\u00fcrzt der Hirsch, zusammt den Hunden, und dennoch", "tokens": ["Es", "st\u00fcrzt", "der", "Hirsch", ",", "zu\u00b7sammt", "den", "Hun\u00b7den", ",", "und", "den\u00b7noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "st\u00fcrzt er nicht herab,", "tokens": ["st\u00fcrzt", "er", "nicht", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Er scheint in seinem Sprung zu stutzen, und alle schreckt", "tokens": ["Er", "scheint", "in", "sei\u00b7nem", "Sprung", "zu", "stut\u00b7zen", ",", "und", "al\u00b7le", "schreckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "KON", "PIS", "VVFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "ihr nahes Grab.", "tokens": ["ihr", "na\u00b7hes", "Grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die Hunde hangen an den Z\u00e4hnen, sie suchen ihren Tod", "tokens": ["Die", "Hun\u00b7de", "han\u00b7gen", "an", "den", "Z\u00e4h\u00b7nen", ",", "sie", "su\u00b7chen", "ih\u00b7ren", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "zu fliehn,", "tokens": ["zu", "fliehn", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Und ihrem Fall, der unvermeidlich, durch reges Dreh\u2019n,", "tokens": ["Und", "ih\u00b7rem", "Fall", ",", "der", "un\u00b7ver\u00b7meid\u00b7lich", ",", "durch", "re\u00b7ges", "Dreh'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "sich zu entzieh'n,", "tokens": ["sich", "zu", "ent\u00b7zieh'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Die Wut ist hier in Furcht verwandelt. Nein, beyde", "tokens": ["Die", "Wut", "ist", "hier", "in", "Furcht", "ver\u00b7wan\u00b7delt", ".", "Nein", ",", "bey\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$.", "PTKANT", "$,", "PIS"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Leidenschaften scheinen,", "tokens": ["Lei\u00b7den\u00b7schaf\u00b7ten", "schei\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Wie wenig sie auch sonst vereinbar, in ihrem Blick, sich", "tokens": ["Wie", "we\u00b7nig", "sie", "auch", "sonst", "ver\u00b7ein\u00b7bar", ",", "in", "ih\u00b7rem", "Blick", ",", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PIS", "PPER", "ADV", "ADV", "ADJD", "$,", "APPR", "PPOSAT", "NN", "$,", "PRF"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "zu vereinen.", "tokens": ["zu", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Wir k\u00f6nnen eigentlich bemerken, wie sich zugleich der", "tokens": ["Wir", "k\u00f6n\u00b7nen", "ei\u00b7gent\u00b7lich", "be\u00b7mer\u00b7ken", ",", "wie", "sich", "zu\u00b7gleich", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "PWAV", "PRF", "ADV", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Hunde Geist,", "tokens": ["Hun\u00b7de", "Geist", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "In ihren aufgeri\u00dfnen Augen, sich w\u00fctig und auch furcht-", "tokens": ["In", "ih\u00b7ren", "auf\u00b7ge\u00b7ri\u00df\u00b7nen", "Au\u00b7gen", ",", "sich", "w\u00fc\u00b7tig", "und", "auch", "furcht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PRF", "ADJD", "KON", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "sam weist.", "tokens": ["sam", "weist", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Wie schwehr ist sonst ein Hund, der f\u00e4llt, recht nach dem", "tokens": ["Wie", "schwehr", "ist", "sonst", "ein", "Hund", ",", "der", "f\u00e4llt", ",", "recht", "nach", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "ADJD", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Leben vorzustellen!", "tokens": ["Le\u00b7ben", "vor\u00b7zu\u00b7stel\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Hier sieht man hundert Wendungen in so viel unterschiednen", "tokens": ["Hier", "sieht", "man", "hun\u00b7dert", "Wen\u00b7dun\u00b7gen", "in", "so", "viel", "un\u00b7ter\u00b7schied\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "CARD", "NN", "APPR", "ADV", "ADV", "VVINF"], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "F\u00e4llen.", "tokens": ["F\u00e4l\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Es l\u00e4\u00dft uns diese fremde Wahl von Stellungen, die alle", "tokens": ["Es", "l\u00e4\u00dft", "uns", "die\u00b7se", "frem\u00b7de", "Wahl", "von", "Stel\u00b7lun\u00b7gen", ",", "die", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "APPR", "NN", "$,", "PRELS", "PIS"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.26": {"text": "sch\u00f6n,", "tokens": ["sch\u00f6n", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.28": {"text": "sch\u00f6pflich, seh'n.", "tokens": ["sch\u00f6pf\u00b7lich", ",", "seh'", "n."], "token_info": ["word", "punct", "word", "abbreviation"], "pos": ["ADJD", "$,", "VVFIN", "NE"], "meter": "+-+", "measure": "trochaic.di"}}}}}