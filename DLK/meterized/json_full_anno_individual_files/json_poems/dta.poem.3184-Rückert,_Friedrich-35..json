{"dta.poem.3184": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott theilet, wie er will, die G\u00fcter aus hienieden;", "tokens": ["Gott", "thei\u00b7let", ",", "wie", "er", "will", ",", "die", "G\u00fc\u00b7ter", "aus", "hien\u00b7ie\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fragst du, warum er dem hat mehr als dem beschieden?", "tokens": ["Fragst", "du", ",", "wa\u00b7rum", "er", "dem", "hat", "mehr", "als", "dem", "be\u00b7schie\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "PDS", "VAFIN", "PIAT", "KOKOM", "ART", "ADJA", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wenn du nur wenig hast, ein andrer hat noch minder;", "tokens": ["Wenn", "du", "nur", "we\u00b7nig", "hast", ",", "ein", "an\u00b7drer", "hat", "noch", "min\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VAFIN", "$,", "ART", "ADJA", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist bei weitem nicht das \u00e4rmste seiner Kinder.", "tokens": ["Du", "bist", "bei", "wei\u00b7tem", "nicht", "das", "\u00e4rms\u00b7te", "sei\u00b7ner", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "PTKNEG", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch seiner Kinder auch das \u00e4rmste f\u00fchlt sich reich,", "tokens": ["Doch", "sei\u00b7ner", "Kin\u00b7der", "auch", "das", "\u00e4rms\u00b7te", "f\u00fchlt", "sich", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ART", "ADJA", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Gottes Kind ist, dies Gef\u00fchl macht alles gleich.", "tokens": ["Das", "Got\u00b7tes", "Kind", "ist", ",", "dies", "Ge\u00b7f\u00fchl", "macht", "al\u00b7les", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$,", "PDS", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du m\u00f6chtest theilen mit den Reichen wohl auf Erden", "tokens": ["Du", "m\u00f6ch\u00b7test", "thei\u00b7len", "mit", "den", "Rei\u00b7chen", "wohl", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die F\u00fclle, nicht auch mit den Armen die Beschwerden?", "tokens": ["Die", "F\u00fcl\u00b7le", ",", "nicht", "auch", "mit", "den", "Ar\u00b7men", "die", "Be\u00b7schwer\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn Alles aber gleich getheilet Allen w\u00fcrde,", "tokens": ["Wenn", "Al\u00b7les", "a\u00b7ber", "gleich", "ge\u00b7thei\u00b7let", "Al\u00b7len", "w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVPP", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Leicht k\u00e4m' auf dich von Gut noch minder, mehr noch B\u00fcrde.", "tokens": ["Leicht", "k\u00e4m'", "auf", "dich", "von", "Gut", "noch", "min\u00b7der", ",", "mehr", "noch", "B\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PRF", "APPR", "NN", "ADV", "ADV", "$,", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drum la\u00df, wie's ist getheilt, und nimm an Lust und Leid", "tokens": ["Drum", "la\u00df", ",", "wie's", "ist", "ge\u00b7theilt", ",", "und", "nimm", "an", "Lust", "und", "Leid"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "PWAV", "VAFIN", "VVPP", "$,", "KON", "VVIMP", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Br\u00fcder Antheil ohn' Hartherzigkeit und Neid.", "tokens": ["Der", "Br\u00fc\u00b7der", "An\u00b7theil", "ohn'", "Har\u00b7ther\u00b7zig\u00b7keit", "und", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Den Reichen la\u00df sein Gut, wenn er's allein will tragen,", "tokens": ["Den", "Rei\u00b7chen", "la\u00df", "sein", "Gut", ",", "wenn", "er's", "al\u00b7lein", "will", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "PPOSAT", "NN", "$,", "KOUS", "PIS", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tragen hilf so viel du kannst des Armen Plagen.", "tokens": ["Und", "tra\u00b7gen", "hilf", "so", "viel", "du", "kannst", "des", "Ar\u00b7men", "Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVIMP", "ADV", "ADV", "PPER", "VMFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}