unct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$,", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die scharfe Sichel, die das Gold der Garben f\u00e4llt,", "tokens": ["Die", "schar\u00b7fe", "Si\u00b7chel", ",", "die", "das", "Gold", "der", "Gar\u00b7ben", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Taucht blinkend nieder, leuchtet auf und funkelt weiter.", "tokens": ["Taucht", "blin\u00b7kend", "nie\u00b7der", ",", "leuch\u00b7tet", "auf", "und", "fun\u00b7kelt", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fernhin bedeckt von Arbeit \u00e4ndert sich das Feld", "tokens": ["Fern\u00b7hin", "be\u00b7deckt", "von", "Ar\u00b7beit", "\u00e4n\u00b7dert", "sich", "das", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In jedem Augenblicke, ernst zugleich und heiter.", "tokens": ["In", "je\u00b7dem", "Au\u00b7gen\u00b7bli\u00b7cke", ",", "ernst", "zu\u00b7gleich", "und", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJD", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text"