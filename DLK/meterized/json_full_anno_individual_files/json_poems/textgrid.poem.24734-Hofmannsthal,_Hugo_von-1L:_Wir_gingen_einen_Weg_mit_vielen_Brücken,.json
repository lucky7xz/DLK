{"textgrid.poem.24734": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir gingen einen Weg mit vielen Br\u00fccken,", "genre": "verse", "period": "N.A.", "pub_year": 1897, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir gingen einen Weg mit vielen Br\u00fccken,", "tokens": ["Wir", "gin\u00b7gen", "ei\u00b7nen", "Weg", "mit", "vie\u00b7len", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und vor uns gingen drei, die ruhig sangen.", "tokens": ["Und", "vor", "uns", "gin\u00b7gen", "drei", ",", "die", "ru\u00b7hig", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "CARD", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sage dies, damit du dich entsinnst.", "tokens": ["Ich", "sa\u00b7ge", "dies", ",", "da\u00b7mit", "du", "dich", "ent\u00b7sinnst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "KOUS", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da sagtest du und zeigtest nach dem Berg,", "tokens": ["Da", "sag\u00b7test", "du", "und", "zeig\u00b7test", "nach", "dem", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Schatten trug von Wolken und den Schatten", "tokens": ["Der", "Schat\u00b7ten", "trug", "von", "Wol\u00b7ken", "und", "den", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der steilen W\u00e4nde mit unsicheren Pfaden,", "tokens": ["Der", "stei\u00b7len", "W\u00e4n\u00b7de", "mit", "un\u00b7si\u00b7che\u00b7ren", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Du sagtest: \u00bbW\u00e4ren dort wir zwei allein!\u00ab", "tokens": ["Du", "sag\u00b7test", ":", "\u00bb", "W\u00e4\u00b7ren", "dort", "wir", "zwei", "al\u00b7lein", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VAFIN", "ADV", "PPER", "CARD", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und deine Worte hatten einen Ton", "tokens": ["Und", "dei\u00b7ne", "Wor\u00b7te", "hat\u00b7ten", "ei\u00b7nen", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So fremd wie Duft von Sandelholz und Myrrhen.", "tokens": ["So", "fremd", "wie", "Duft", "von", "San\u00b7del\u00b7holz", "und", "Myr\u00b7rhen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "\u2013 Auch deine Augen waren nicht wie sonst. \u2013", "tokens": ["\u2013", "Auch", "dei\u00b7ne", "Au\u00b7gen", "wa\u00b7ren", "nicht", "wie", "sonst", ".", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "VAFIN", "PTKNEG", "KOKOM", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und mir geschah, da\u00df eine trunkene Luft", "tokens": ["Und", "mir", "ge\u00b7schah", ",", "da\u00df", "ei\u00b7ne", "trun\u00b7ke\u00b7ne", "Luft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Mich fa\u00dfte, so wie wenn die Erde bebt", "tokens": ["Mich", "fa\u00df\u00b7te", ",", "so", "wie", "wenn", "die", "Er\u00b7de", "bebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "KOKOM", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und umgest\u00fcrztes prunkvolles Ger\u00e4t", "tokens": ["Und", "um\u00b7ge\u00b7st\u00fcrz\u00b7tes", "prunk\u00b7vol\u00b7les", "Ge\u00b7r\u00e4t"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Rings rollt und Wasser aus dem Boden quillt", "tokens": ["Rings", "rollt", "und", "Was\u00b7ser", "aus", "dem", "Bo\u00b7den", "quillt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und einer taumelnd steht und doppelt sieht:", "tokens": ["Und", "ei\u00b7ner", "tau\u00b7melnd", "steht", "und", "dop\u00b7pelt", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Denn ich war da und war zugleich auch dort,", "tokens": ["Denn", "ich", "war", "da", "und", "war", "zu\u00b7gleich", "auch", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "KON", "VAFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Mit dir im Arm, und alle Lust davon", "tokens": ["Mit", "dir", "im", "Arm", ",", "und", "al\u00b7le", "Lust", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "NN", "$,", "KON", "PIAT", "NN", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "War irgendwie vermengt mit aller Lust,", "tokens": ["War", "ir\u00b7gend\u00b7wie", "ver\u00b7mengt", "mit", "al\u00b7ler", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Die dieser gro\u00dfe Berg mit vielen Kl\u00fcften", "tokens": ["Die", "die\u00b7ser", "gro\u00b7\u00dfe", "Berg", "mit", "vie\u00b7len", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PDAT", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Hingibt, wenn einer ruhig wie der Adler", "tokens": ["Hin\u00b7gibt", ",", "wenn", "ei\u00b7ner", "ru\u00b7hig", "wie", "der", "Ad\u00b7ler"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Mit ausgespannten Fl\u00fcgeln ihn umfl\u00f6ge.", "tokens": ["Mit", "aus\u00b7ge\u00b7spann\u00b7ten", "Fl\u00fc\u00b7geln", "ihn", "um\u00b7fl\u00f6\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ich war mit dir im Arm auf jenem Berg,", "tokens": ["Ich", "war", "mit", "dir", "im", "Arm", "auf", "je\u00b7nem", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPRART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ich hatte alles Wissen seiner H\u00f6he,", "tokens": ["Ich", "hat\u00b7te", "al\u00b7les", "Wis\u00b7sen", "sei\u00b7ner", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Der Einsamkeit, des nie betretnen Pfades", "tokens": ["Der", "Ein\u00b7sam\u00b7keit", ",", "des", "nie", "be\u00b7tret\u00b7nen", "Pfa\u00b7des"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Und dich im Arm und alle Lust davon ...", "tokens": ["Und", "dich", "im", "Arm", "und", "al\u00b7le", "Lust", "da\u00b7von", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "KON", "PIAT", "NN", "PAV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Und als ich heut im Lusthaus beim Erwachen", "tokens": ["Und", "als", "ich", "heut", "im", "Lust\u00b7haus", "beim", "Er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "An einer k\u00fchlen Wand das Bild der G\u00f6tter", "tokens": ["An", "ei\u00b7ner", "k\u00fch\u00b7len", "Wand", "das", "Bild", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und ihrer wunderbaren Freuden sah:", "tokens": ["Und", "ih\u00b7rer", "wun\u00b7der\u00b7ba\u00b7ren", "Freu\u00b7den", "sah", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Wie sie mit leichtem Fu\u00dfe, kaum mehr lastend,", "tokens": ["Wie", "sie", "mit", "leich\u00b7tem", "Fu\u00b7\u00dfe", ",", "kaum", "mehr", "las\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Vom d\u00fcnnen Dache weinumrankter Lauben", "tokens": ["Vom", "d\u00fcn\u00b7nen", "Da\u00b7che", "wei\u00b7num\u00b7rank\u00b7ter", "Lau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Ins Blaue tretend aufzuschweben schienen,", "tokens": ["Ins", "Blau\u00b7e", "tre\u00b7tend", "auf\u00b7zu\u00b7schwe\u00b7ben", "schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJD", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Wie Flammen ohne Schwere, mit dem Laut", "tokens": ["Wie", "Flam\u00b7men", "oh\u00b7ne", "Schwe\u00b7re", ",", "mit", "dem", "Laut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Von Liedern und dem Klang der hellen Leier", "tokens": ["Von", "Lie\u00b7dern", "und", "dem", "Klang", "der", "hel\u00b7len", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Emporgeweht: da wurde es mir so,", "tokens": ["Em\u00b7por\u00b7ge\u00b7weht", ":", "da", "wur\u00b7de", "es", "mir", "so", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VAFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Als d\u00fcrft ich jenen letzten, die noch nah", "tokens": ["Als", "d\u00fcrft", "ich", "je\u00b7nen", "letz\u00b7ten", ",", "die", "noch", "nah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "PDAT", "ADJA", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Der Erde schienen, freundlich ihr Gewand", "tokens": ["Der", "Er\u00b7de", "schie\u00b7nen", ",", "freund\u00b7lich", "ihr", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Anr\u00fchren, wie ein Gastfreund tuen darf", "tokens": ["An\u00b7r\u00fch\u00b7ren", ",", "wie", "ein", "Gast\u00b7freund", "tu\u00b7en", "darf"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Von gleichem Rang und \u0