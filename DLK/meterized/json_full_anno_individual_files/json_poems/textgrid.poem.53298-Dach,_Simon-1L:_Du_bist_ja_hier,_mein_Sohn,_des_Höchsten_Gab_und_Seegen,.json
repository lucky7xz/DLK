{"textgrid.poem.53298": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist ja hier, mein Sohn, des H\u00f6chsten Gab und Seegen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist ja hier, mein Sohn, des H\u00f6chsten Gab und Seegen,", "tokens": ["Du", "bist", "ja", "hier", ",", "mein", "Sohn", ",", "des", "H\u00f6chs\u00b7ten", "Gab", "und", "See\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Mutter, der du vor warst eine schwehre Last,", "tokens": ["Die", "Mut\u00b7ter", ",", "der", "du", "vor", "warst", "ei\u00b7ne", "schweh\u00b7re", "Last", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat ihrer Treuen Pfand, dich, hertzlich nun umfasst", "tokens": ["Hat", "ih\u00b7rer", "Treu\u00b7en", "Pfand", ",", "dich", ",", "hertz\u00b7lich", "nun", "um\u00b7fasst"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,", "PPER", "$,", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd ist allein bem\u00fcht, dich an die Brust zu legen.", "tokens": ["Vnd", "ist", "al\u00b7lein", "be\u00b7m\u00fcht", ",", "dich", "an", "die", "Brust", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$,", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wird aber dein die Welt, dies Elend, auch so pflegen?", "tokens": ["Wird", "a\u00b7ber", "dein", "die", "Welt", ",", "dies", "E\u00b7lend", ",", "auch", "so", "pfle\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ART", "NN", "$,", "PDS", "NN", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit nichten! Du bist hier ein Pilgrimm und ein Gast.", "tokens": ["Mit", "nich\u00b7ten", "!", "Du", "bist", "hier", "ein", "Pil\u00b7grimm", "und", "ein", "Gast", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Beste, das du hier stets zu gewarten hast,", "tokens": ["Das", "Bes\u00b7te", ",", "das", "du", "hier", "stets", "zu", "ge\u00b7war\u00b7ten", "hast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist Arbeit, M\u00fch und Noth, ist Hagel, Blitz und Regen.", "tokens": ["Ist", "Ar\u00b7beit", ",", "M\u00fch", "und", "Noth", ",", "ist", "Ha\u00b7gel", ",", "Blitz", "und", "Re\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN",