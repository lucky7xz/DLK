aic.tetra"}, "line.4": {"text": "Liegt das Recht gebunden,", "tokens": ["Liegt", "das", "Recht", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd es wird an allem Ort", "tokens": ["Vnd", "es", "wird", "an", "al\u00b7lem", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PIS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Krieg und Mord", "tokens": ["Krieg", "und", "Mord"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Raub und Brand gefunden.", "tokens": ["Raub", "und", "Brand", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Vnd wir wolten trawrig seyn", "tokens": ["Vnd", "wir", "wol\u00b7ten", "traw\u00b7rig", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn uns vnsre Kinder sterben,", "tokens": ["Wenn", "uns", "vns\u00b7re", "Kin\u00b7der", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die f\u00fcr dieser Erden Pein", "tokens": ["Die", "f\u00fcr", "die\u00b7ser", "Er\u00b7den", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch den Himmel erben,", "tokens": ["Doch", "den", "Him\u00b7mel", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd der Ausserwehlten Stand", "tokens": ["Vnd", "der", "Aus\u00b7ser\u00b7wehl\u00b7ten", "Stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gottes Hand", "tokens": ["Got\u00b7tes", "Hand"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Durch den Glauben werben?", "tokens": ["Durch", "den", "Glau\u00b7ben", "wer\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "F\u00fchrt kein heidnisches Geschrey,", "tokens": ["F\u00fchrt", "kein", "heid\u00b7ni\u00b7sches", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Christus w\u00fcrdigt sie zu k\u00fcssen,", "tokens": ["Chris\u00b7tus", "w\u00fcr\u00b7digt", "sie", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt, da\u00df ihr der Himmel sey", "tokens": ["Sagt", ",", "da\u00df", "ihr", "der", "Him\u00b7mel", "sey"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vns nicht aus zu schliessen,", "tokens": ["Vns", "nicht", "aus", "zu", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Also doch da\u00df wir vorhin", "tokens": ["Al\u00b7so", "doch", "da\u00df", "wir", "vor\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "An dem Sinn", "tokens": ["An", "dem", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.7": {"text": "Kinder werden m\u00fcssen.", "tokens": ["Kin\u00b7der", "wer\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAINF", "VMFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Die jhr jetzt des Grabes Wust", "tokens": ["Die", "jhr", "jetzt", "des", "Gra\u00b7bes", "Wust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Einbringt \u00e4ngstig von Geb\u00e4rden,", "tokens": ["Ein\u00b7bringt", "\u00e4ngs\u00b7tig", "von", "Ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollen euch in h\u00f6chster Lust", "tokens": ["Sol\u00b7len", "euch", "in", "h\u00f6chs\u00b7ter", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich wieder werden,", "tokens": ["End\u00b7lich", "wie\u00b7der", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn der letzten Trommten Schall", "tokens": ["Wenn", "der", "letz\u00b7ten", "Tromm\u00b