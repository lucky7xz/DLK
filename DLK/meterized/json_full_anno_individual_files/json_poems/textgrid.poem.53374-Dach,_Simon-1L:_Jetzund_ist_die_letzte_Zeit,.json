{"textgrid.poem.53374": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jetzund ist die letzte Zeit,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzund ist die letzte Zeit,", "tokens": ["Je\u00b7tzund", "ist", "die", "letz\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wachet, Christus wird vernommen,", "tokens": ["Wa\u00b7chet", ",", "Chris\u00b7tus", "wird", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der in seiner Herrlicheit", "tokens": ["Der", "in", "sei\u00b7ner", "Herr\u00b7li\u00b7cheit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Gericht sol kommen", "tokens": ["Zum", "Ge\u00b7richt", "sol", "kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Seine Welt-Posaun erschallt", "tokens": ["Sei\u00b7ne", "Welt\u00b7Po\u00b7saun", "er\u00b7schallt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Also bald,", "tokens": ["Al\u00b7so", "bald", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Wie verlangt die Frommen!", "tokens": ["Wie", "ver\u00b7langt", "die", "From\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Offtmals wird der Monde Blut,", "tokens": ["Offt\u00b7mals", "wird", "der", "Mon\u00b7de", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Offtmals mu\u00df die Sonn' erbleichen,", "tokens": ["Offt\u00b7mals", "mu\u00df", "die", "Sonn'", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Natur musst Kraft und Muht,", "tokens": ["Die", "Na\u00b7tur", "musst", "Kraft", "und", "Muht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Gestirne weichen,", "tokens": ["Die", "Ge\u00b7stir\u00b7ne", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und da\u00df stoltze Hertz entf\u00e4llt", "tokens": ["Und", "da\u00df", "stolt\u00b7ze", "Hertz", "ent\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Welt", "tokens": ["In", "der", "Welt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Allen K\u00f6nigreichen.", "tokens": ["Al\u00b7len", "K\u00f6\u00b7nig\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Trew vnd Redlicheit erliegt,", "tokens": ["Trew", "vnd", "Red\u00b7lic\u00b7heit", "er\u00b7liegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd die Lieb ist l\u00e4ngst verschwunden,", "tokens": ["Vnd", "die", "Lieb", "ist", "l\u00e4ngst", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil Gewalt vnd Vnrecht siegt,", "tokens": ["Weil", "Ge\u00b7walt", "vnd", "Vn\u00b7recht", "siegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt das Recht gebunden,", "tokens": ["Liegt", "das", "Recht", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd es wird an allem Ort", "tokens": ["Vnd", "es", "wird", "an", "al\u00b7lem", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PIS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Krieg und Mord", "tokens": ["Krieg", "und", "Mord"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Raub und Brand gefunden.", "tokens": ["Raub", "und", "Brand", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Vnd wir wolten trawrig seyn", "tokens": ["Vnd", "wir", "wol\u00b7ten", "traw\u00b7rig", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn uns vnsre Kinder sterben,", "tokens": ["Wenn", "uns", "vns\u00b7re", "Kin\u00b7der", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die f\u00fcr dieser Erden Pein", "tokens": ["Die", "f\u00fcr", "die\u00b7ser", "Er\u00b7den", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch den Himmel erben,", "tokens": ["Doch", "den", "Him\u00b7mel", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd der Ausserwehlten Stand", "tokens": ["Vnd", "der", "Aus\u00b7ser\u00b7wehl\u00b7ten", "Stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gottes Hand", "tokens": ["Got\u00b7tes", "Hand"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Durch den Glauben werben?", "tokens": ["Durch", "den", "Glau\u00b7ben", "wer\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "F\u00fchrt kein heidnisches Geschrey,", "tokens": ["F\u00fchrt", "kein", "heid\u00b7ni\u00b7sches", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Christus w\u00fcrdigt sie zu k\u00fcssen,", "tokens": ["Chris\u00b7tus", "w\u00fcr\u00b7digt", "sie", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt, da\u00df ihr der Himmel sey", "tokens": ["Sagt", ",", "da\u00df", "ihr", "der", "Him\u00b7mel", "sey"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vns nicht aus zu schliessen,", "tokens": ["Vns", "nicht", "aus", "zu", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Also doch da\u00df wir vorhin", "tokens": ["Al\u00b7so", "doch", "da\u00df", "wir", "vor\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "An dem Sinn", "tokens": ["An", "dem", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.7": {"text": "Kinder werden m\u00fcssen.", "tokens": ["Kin\u00b7der", "wer\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAINF", "VMFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Die jhr jetzt des Grabes Wust", "tokens": ["Die", "jhr", "jetzt", "des", "Gra\u00b7bes", "Wust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Einbringt \u00e4ngstig von Geb\u00e4rden,", "tokens": ["Ein\u00b7bringt", "\u00e4ngs\u00b7tig", "von", "Ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollen euch in h\u00f6chster Lust", "tokens": ["Sol\u00b7len", "euch", "in", "h\u00f6chs\u00b7ter", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich wieder werden,", "tokens": ["End\u00b7lich", "wie\u00b7der", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn der letzten Trommten Schall", "tokens": ["Wenn", "der", "letz\u00b7ten", "Tromm\u00b7ten", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vberall", "tokens": ["Vbe\u00b7rall"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Vns rufft aus der Erden.", "tokens": ["Vns", "rufft", "aus", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Wacht viellieber allezeit", "tokens": ["Wacht", "viel\u00b7lie\u00b7ber", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df, wenn Christus wird erscheinen,", "tokens": ["Da\u00df", ",", "wenn", "Chris\u00b7tus", "wird", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "NE", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr im Glauben fertig seyd", "tokens": ["Ihr", "im", "Glau\u00b7ben", "fer\u00b7tig", "seyd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "ADJD", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als die lieben Seinen,", "tokens": ["Als", "die", "lie\u00b7ben", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Welcher schl\u00e4fft, er k\u00f6mpt behend,", "tokens": ["Wel\u00b7cher", "schl\u00e4fft", ",", "er", "k\u00f6mpt", "be\u00b7hend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Wird ohn End", "tokens": ["Wird", "ohn", "End"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "In der Hellen weinen.", "tokens": ["In", "der", "Hel\u00b7len", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Jetzund ist die letzte Zeit,", "tokens": ["Je\u00b7tzund", "ist", "die", "letz\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wachet, Christus wird vernommen,", "tokens": ["Wa\u00b7chet", ",", "Chris\u00b7tus", "wird", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": 