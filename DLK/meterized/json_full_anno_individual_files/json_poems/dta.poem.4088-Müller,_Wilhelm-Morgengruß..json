{"dta.poem.4088": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Morgengru\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo steckst du gleich das K\u00f6pfchen hin,", "tokens": ["Wo", "steckst", "du", "gleich", "das", "K\u00f6pf\u00b7chen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als w\u00e4r' dir was geschehen?", "tokens": ["Als", "w\u00e4r'", "dir", "was", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verdrie\u00dft dich denn mein Gru\u00df so schwer?", "tokens": ["Ver\u00b7drie\u00dft", "dich", "denn", "mein", "Gru\u00df", "so", "schwer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verst\u00f6rt dich denn mein Blick so sehr?", "tokens": ["Ver\u00b7st\u00f6rt", "dich", "denn", "mein", "Blick", "so", "sehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So mu\u00df ich wieder gehen.", "tokens": ["So", "mu\u00df", "ich", "wie\u00b7der", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O la\u00df mich nur von ferne stehn,", "tokens": ["O", "la\u00df", "mich", "nur", "von", "fer\u00b7ne", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach deinem lieben Fenster sehn,", "tokens": ["Nach", "dei\u00b7nem", "lie\u00b7ben", "Fens\u00b7ter", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von ferne, ganz von ferne!", "tokens": ["Von", "fer\u00b7ne", ",", "ganz", "von", "fer\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADV", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du blondes K\u00f6pfchen, komm hervor!", "tokens": ["Du", "blon\u00b7des", "K\u00f6pf\u00b7chen", ",", "komm", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hervor aus eurem runden Thor,", "tokens": ["Her\u00b7vor", "aus", "eu\u00b7rem", "run\u00b7den", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr blauen Morgensterne!", "tokens": ["Ihr", "blau\u00b7en", "Mor\u00b7gens\u00b7ter\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr schlummertrunknen Aeugelein,", "tokens": ["Ihr", "schlum\u00b7mer\u00b7trunk\u00b7nen", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr thaubetr\u00fcbten Bl\u00fcmelein,", "tokens": ["Ihr", "thau\u00b7be\u00b7tr\u00fcb\u00b7ten", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was scheuet ihr die Sonne?", "tokens": ["Was", "scheu\u00b7et", "ihr", "die", "Son\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat es die Nacht so gut gemeint,", "tokens": ["Hat", "es", "die", "Nacht", "so", "gut", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ihr euch schlie\u00dft und b\u00fcckt und weint,", "tokens": ["Da\u00df", "ihr", "euch", "schlie\u00dft", "und", "b\u00fcckt", "und", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach ihrer stillen Wonne?", "tokens": ["Nach", "ih\u00b7rer", "stil\u00b7len", "Won\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun sch\u00fcttelt ab der Tr\u00e4ume Flor,", "tokens": ["Nun", "sch\u00fct\u00b7telt", "ab", "der", "Tr\u00e4u\u00b7me", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hebt euch frisch und frei empor", "tokens": ["Und", "hebt", "euch", "frisch", "und", "frei", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Gottes hellen Morgen!", "tokens": ["In", "Got\u00b7tes", "hel\u00b7len", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Lerche wirbelt in der Luft,", "tokens": ["Die", "Ler\u00b7che", "wir\u00b7belt", "in", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und aus dem tiefen Herzen ruft", "tokens": ["Und", "aus", "dem", "tie\u00b7fen", "Her\u00b7zen", "ruft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Liebe Leid und Sorgen.", "tokens": ["Die", "Lie\u00b7be", "Leid", "und", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}