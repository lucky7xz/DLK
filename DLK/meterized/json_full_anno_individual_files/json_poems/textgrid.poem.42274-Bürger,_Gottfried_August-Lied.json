{"textgrid.poem.42274": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mit dem Fr\u00fchlingsangesichte,", "tokens": ["Du", "mit", "dem", "Fr\u00fch\u00b7lings\u00b7an\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du sch\u00f6nes blondes Himmelskind,", "tokens": ["Du", "sch\u00f6\u00b7nes", "blon\u00b7des", "Him\u00b7mel\u00b7skind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An deiner Anmut Rosenlichte", "tokens": ["An", "dei\u00b7ner", "An\u00b7mut", "Ro\u00b7sen\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sieht sich mein Auge noch halb blind!", "tokens": ["Sieht", "sich", "mein", "Au\u00b7ge", "noch", "halb", "blind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach etwas durst' ich lang' im stillen;", "tokens": ["Nach", "et\u00b7was", "dur\u00b7st'", "ich", "lang'", "im", "stil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ADV", "APPRART", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nach Einem Labeku\u00df von dir.", "tokens": ["Nach", "Ei\u00b7nem", "La\u00b7be\u00b7ku\u00d