{"dta.poem.7082": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wird ihn nicht der Glantz von allen Lichtes-Sch\u00e4tzen", "tokens": ["Wie", "wird", "ihn", "nicht", "der", "Glantz", "von", "al\u00b7len", "Lich\u00b7tes\u00b7Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entz\u00fccken, in Erstaunen setzen,", "tokens": ["Ent\u00b7z\u00fc\u00b7cken", ",", "in", "Er\u00b7stau\u00b7nen", "set\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er von oben auf einmal", "tokens": ["Wenn", "er", "von", "o\u00b7ben", "auf", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch vieler Sonnen-Heer, an vieler Atmospheeren", "tokens": ["Durch", "vie\u00b7ler", "Son\u00b7nen\u00b7Heer", ",", "an", "vie\u00b7ler", "At\u00b7mo\u00b7sphee\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bewolckter Lufft gebrochnen Strahl,", "tokens": ["Be\u00b7wolck\u00b7ter", "Lufft", "ge\u00b7broch\u00b7nen", "Strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Viel tausend Morgenr\u00f6then siehet,", "tokens": ["Viel", "tau\u00b7send", "Mor\u00b7gen\u00b7r\u00f6\u00b7then", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von deren jegliche, zu ihres Sch\u00f6pffers Ehren,", "tokens": ["Von", "de\u00b7ren", "jeg\u00b7li\u00b7che", ",", "zu", "ih\u00b7res", "Sch\u00f6pf\u00b7fers", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jm Gr\u00e4ntzen-losen Raum, in bunter Klarheit gl\u00fchet!", "tokens": ["Jm", "Gr\u00e4nt\u00b7zen\u00b7lo\u00b7sen", "Raum", ",", "in", "bun\u00b7ter", "Klar\u00b7heit", "gl\u00fc\u00b7het", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}