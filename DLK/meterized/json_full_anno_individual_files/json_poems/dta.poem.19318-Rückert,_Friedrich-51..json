{"dta.poem.19318": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So sprach ich, als ich j\u00fcngst gieng durch die Flur am Abend \u2014", "tokens": ["So", "sprach", "ich", ",", "als", "ich", "j\u00fcngst", "gieng", "durch", "die", "Flur", "am", "A\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie war f\u00fcr Aug' und Ohr und jeden Sinn so labend;", "tokens": ["Sie", "war", "f\u00fcr", "Aug'", "und", "Ohr", "und", "je\u00b7den", "Sinn", "so", "la\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "KON", "PIAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich aber dachte, was der Filosofen gr\u00f6ster", "tokens": ["Ich", "a\u00b7ber", "dach\u00b7te", ",", "was", "der", "Fi\u00b7lo\u00b7so\u00b7fen", "gr\u00f6s\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PRELS", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von der Natur gedacht, f\u00fcr mich ein leid'ger Tr\u00f6ster:", "tokens": ["Von", "der", "Na\u00b7tur", "ge\u00b7dacht", ",", "f\u00fcr", "mich", "ein", "lei\u00b7d'\u00b7ger", "Tr\u00f6s\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Da\u00df ein mislungener Versuch mit viel Beschwerden", "tokens": ["Da\u00df", "ein", "mis\u00b7lun\u00b7ge\u00b7ner", "Ver\u00b7such", "mit", "viel", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Sie des Begriffes sei, sich \u00e4u\u00dferlich zu werden. \u2014", "tokens": ["Sie", "des", "Be\u00b7grif\u00b7fes", "sei", ",", "sich", "\u00e4u\u00b7\u00dfer\u00b7lich", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "NN", "VAFIN", "$,", "PRF", "ADJD", "PTKZU", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So sprach ich: O wieviel des Sch\u00f6nen doch entsprang", "tokens": ["So", "sprach", "ich", ":", "O", "wie\u00b7viel", "des", "Sch\u00f6\u00b7nen", "doch", "ent\u00b7sprang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NE", "VVFIN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr mich aus dem Versuch, der dem Begriff mislang.", "tokens": ["F\u00fcr", "mich", "aus", "dem", "Ver\u00b7such", ",", "der", "dem", "Be\u00b7griff", "mis\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Und w\u00e4re dem Begriff nun der Versuch gelungen,", "tokens": ["Und", "w\u00e4\u00b7re", "dem", "Be\u00b7griff", "nun", "der", "Ver\u00b7such", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Welch eine Herrlichkeit w\u00e4r' erst daraus entsprungen!", "tokens": ["Welch", "ei\u00b7ne", "Herr\u00b7lich\u00b7keit", "w\u00e4r'", "erst", "da\u00b7raus", "ent\u00b7sprun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Welch h\u00f6here Natur, worin von all den Ch\u00f6ren,", "tokens": ["Welch", "h\u00f6\u00b7he\u00b7re", "Na\u00b7tur", ",", "wo\u00b7rin", "von", "all", "den", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PWAV", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die meinen Sinn zerstreun, den Geist nichts w\u00fcrde st\u00f6ren!", "tokens": ["Die", "mei\u00b7nen", "Sinn", "zer\u00b7streun", ",", "den", "Geist", "nichts", "w\u00fcr\u00b7de", "st\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,", "ART", "NN", "PIS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Welch h\u00f6here Natur, worin von all den Ch\u00f6ren,", "tokens": ["Welch", "h\u00f6\u00b7he\u00b7re", "Na\u00b7tur", ",", "wo\u00b7rin", "von", "all", "den", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PWAV", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die meinen Sinn erfreun, ich sehn nichts w\u00fcrd' und h\u00f6ren!", "tokens": ["Die", "mei\u00b7nen", "Sinn", "er\u00b7freun", ",", "ich", "sehn", "nichts", "w\u00fcrd'", "und", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,", "PPER", "VVFIN", "PIS", "VAFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich, dem Begriff zum Spott, will h\u00f6ren, sehn und singen,", "tokens": ["Ich", ",", "dem", "Be\u00b7griff", "zum", "Spott", ",", "will", "h\u00f6\u00b7ren", ",", "sehn", "und", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "APPRART", "NN", "$,", "VMFIN", "VVINF", "$,", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und danken, da\u00df ihm Gott lie\u00df den Versuch mislingen.", "tokens": ["Und", "dan\u00b7ken", ",", "da\u00df", "ihm", "Gott", "lie\u00df", "den", "Ver\u00b7such", "mis\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "PPER", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}}}}