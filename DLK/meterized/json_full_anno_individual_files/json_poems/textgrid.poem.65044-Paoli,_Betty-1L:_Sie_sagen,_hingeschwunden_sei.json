{"textgrid.poem.65044": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie sagen, hingeschwunden sei", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sagen, hingeschwunden sei", "tokens": ["Sie", "sa\u00b7gen", ",", "hin\u00b7ge\u00b7schwun\u00b7den", "sei"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sch\u00f6nheit, die dein Haupt bekr\u00e4nzte,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", ",", "die", "dein", "Haupt", "be\u00b7kr\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als noch ein wolkenloser Mai", "tokens": ["Als", "noch", "ein", "wol\u00b7ken\u00b7lo\u00b7ser", "Mai"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Jungfrau Leben heiter gl\u00e4nzte.", "tokens": ["Der", "Jung\u00b7frau", "Le\u00b7ben", "hei\u00b7ter", "gl\u00e4nz\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gleichgiltig bald, bald heuchelnd spricht", "tokens": ["Gleich\u00b7gil\u00b7tig", "bald", ",", "bald", "heu\u00b7chelnd", "spricht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr achselzuckendes Bedauern:", "tokens": ["Ihr", "ach\u00b7sel\u00b7zu\u00b7cken\u00b7des", "Be\u00b7dau\u00b7ern", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie Schade, da\u00df die\u00df Angesicht", "tokens": ["Wie", "Scha\u00b7de", ",", "da\u00df", "die\u00df", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbso fr\u00fch versehrt von Gram und Trauern!\u00ab", "tokens": ["\u00bb", "so", "fr\u00fch", "ver\u00b7sehrt", "von", "Gram", "und", "Trau\u00b7ern", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "VVPP", "APPR", "NE", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch wo ihr Auge blo\u00df die Spur", "tokens": ["Doch", "wo", "ihr", "Au\u00b7ge", "blo\u00df", "die", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Welken sieht und vom Verbl\u00fchen,", "tokens": ["Vom", "Wel\u00b7ken", "sieht", "und", "vom", "Ver\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da sieht das meine wieder nur", "tokens": ["Da", "sieht", "das", "mei\u00b7ne", "wie\u00b7der", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verkl\u00e4rung wundersam ergl\u00fchen.", "tokens": ["Ver\u00b7kl\u00e4\u00b7rung", "wun\u00b7der\u00b