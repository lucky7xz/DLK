{"dta.poem.9592": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff ihre schwartze und sauersehende augen.  \n Aus dem Englischen.  \n C. E.  \n 1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr schwartzen augen ihr/ eur schatten-voller grund", "tokens": ["Ihr", "schwart\u00b7zen", "au\u00b7gen", "ihr", "/", "eur", "schat\u00b7ten\u00b7vol\u00b7ler", "grund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht mein verh\u00e4ngni\u00df mir im gl\u00fcck un\u0303 ungl\u00fcck kund/", "tokens": ["Macht", "mein", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "mir", "im", "gl\u00fcck", "u\u00f1", "un\u00b7gl\u00fcck", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "APPRART", "NN", "KON", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann ihr in liebe last die strahlen auff mich schiessen/", "tokens": ["Wann", "ihr", "in", "lie\u00b7be", "last", "die", "strah\u00b7len", "auff", "mich", "schies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "VVFIN", "ART", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So seh ich vor mir nichts denn g\u00fcldne berge stehn;", "tokens": ["So", "seh", "ich", "vor", "mir", "nichts", "denn", "g\u00fcld\u00b7ne", "ber\u00b7ge", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "PIS", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach! aber/ wenn ihr mich ver\u00e4chtlich wolt begr\u00fcssen/", "tokens": ["Ach", "!", "a\u00b7ber", "/", "wenn", "ihr", "mich", "ver\u00b7\u00e4cht\u00b7lich", "wolt", "be\u00b7gr\u00fcs\u00b7sen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "$(", "KOUS", "PPER", "PRF", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So heist ihr mich so fort zum finstern grabe gehn.", "tokens": ["So", "heist", "ihr", "mich", "so", "fort", "zum", "fins\u00b7tern", "gra\u00b7be", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "PTKVZ", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr schwartzen augen ihr/ in euren dunckeln gr\u00fcnden", "tokens": ["Ihr", "schwart\u00b7zen", "au\u00b7gen", "ihr", "/", "in", "eu\u00b7ren", "dun\u00b7ckeln", "gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "$(", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kan ich itzt gl\u00fcck und todt/ itzt h\u00f6ll und himmel finden.", "tokens": ["Kan", "ich", "itzt", "gl\u00fcck", "und", "todt", "/", "itzt", "h\u00f6ll", "und", "him\u00b7mel", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$(", "ADV", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die sonne/ die den prei\u00df beym sternen-heer beh\u00e4lt/", "tokens": ["Die", "son\u00b7ne", "/", "die", "den", "prei\u00df", "beym", "ster\u00b7nen\u00b7heer", "be\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Siebt/ gegen euch gesetzt/ gantz finster und verstellt/", "tokens": ["Siebt", "/", "ge\u00b7gen", "euch", "ge\u00b7setzt", "/", "gantz", "fins\u00b7ter", "und", "ver\u00b7stellt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "APPR", "PPER", "VVPP", "$(", "ADV", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mu\u00df mit licht und glantz doch eurer sch\u00f6nheit weichen;", "tokens": ["Und", "mu\u00df", "mit", "licht", "und", "glantz", "doch", "eu\u00b7rer", "sch\u00f6n\u00b7heit", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "KON", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr gehet fr\u00fch und spat in gleichem schmuck verh\u00fcllt/", "tokens": ["Ihr", "ge\u00b7het", "fr\u00fch", "und", "spat", "in", "glei\u00b7chem", "schmuck", "ver\u00b7h\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie kan bey tage nur uns ihre strahlen reichen/", "tokens": ["Sie", "kan", "bey", "ta\u00b7ge", "nur", "uns", "ih\u00b7re", "strah\u00b7len", "rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da ihr zu nachts auch seyd mit flammen angef\u00fcllt.", "tokens": ["Da", "ihr", "zu", "nachts", "auch", "seyd", "mit", "flam\u00b7men", "an\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADV", "VAFIN", "APPR", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr schwartzen augen ihr/ in euren dunckeln gr\u00fcnden", "tokens": ["Ihr", "schwart\u00b7zen", "au\u00b7gen", "ihr", "/", "in", "eu\u00b7ren", "dun\u00b7ckeln", "gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "$(", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kan ich itzt gl\u00fcck und todt\u2019/ itzt h\u00f6ll und himmel finden.", "tokens": ["Kan", "ich", "itzt", "gl\u00fcck", "und", "todt'", "/", "itzt", "h\u00f6ll", "und", "him\u00b7mel", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$(", "ADV", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So wie ein schlauer dieb in finstern winckeln steht/", "tokens": ["So", "wie", "ein", "schlau\u00b7er", "dieb", "in", "fins\u00b7tern", "win\u00b7ckeln", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "APPR", "VVINF", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann er bey dunckler nacht auff raub und morden geht/", "tokens": ["Wann", "er", "bey", "dunck\u00b7ler", "nacht", "auff", "raub", "und", "mor\u00b7den", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "APPR", "NN", "KON", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lauscht Cupido hier in euren schwartzen h\u00f6len/", "tokens": ["So", "lauscht", "Cu\u00b7pi\u00b7do", "hier", "in", "eu\u00b7ren", "schwart\u00b7zen", "h\u00f6\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ist auff diebstal nur/ auf list und mord bedacht/", "tokens": ["Und", "ist", "auff", "diebs\u00b7tal", "nur", "/", "auf", "list", "und", "mord", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJD", "ADV", "$(", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Den er zu \u00fcben sucht an meiner theuren seelen/", "tokens": ["Den", "er", "zu", "\u00fc\u00b7ben", "sucht", "an", "mei\u00b7ner", "theu\u00b7ren", "see\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die von mir h\u00f6her wird/ denn Indisch gold/ geacht.", "tokens": ["Die", "von", "mir", "h\u00f6\u00b7her", "wird", "/", "denn", "In\u00b7disch", "gold", "/", "ge\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJD", "VAFIN", "$(", "KON", "NN", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr schwartzen augen ihr/ in euren dunckeln gr\u00fcnden/", "tokens": ["Ihr", "schwart\u00b7zen", "au\u00b7gen", "ihr", "/", "in", "eu\u00b7ren", "dun\u00b7ckeln", "gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kan ich itzt gl\u00fcck und todt/ itzt h\u00f6ll und himmel finden.", "tokens": ["Kan", "ich", "itzt", "gl\u00fcck", "und", "todt", "/", "itzt", "h\u00f6ll", "und", "him\u00b7mel", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$(", "ADV", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer einen steiffen blick in eure zirckel f\u00fchrt/", "tokens": ["Wer", "ei\u00b7nen", "steif\u00b7fen", "blick", "in", "eu\u00b7re", "zir\u00b7ckel", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird sehen/ da\u00df allhier nur list und macht regiert/", "tokens": ["Wird", "se\u00b7hen", "/", "da\u00df", "all\u00b7hier", "nur", "list", "und", "macht", "re\u00b7giert", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$(", "KOUS", "ADV", "ADV", "ADJD", "KON", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df Cupido selbst in euren zauber-krey\u00dfen", "tokens": ["Und", "da\u00df", "Cu\u00b7pi\u00b7do", "selbst", "in", "eu\u00b7ren", "zau\u00b7ber\u00b7krey\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NE", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch seinen wunder-pfeil in schwartzer kunst sich \u00fcbt;", "tokens": ["Durch", "sei\u00b7nen", "wun\u00b7der\u00b7pfeil", "in", "schwart\u00b7zer", "kunst", "sich", "\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja da\u00df er seine macht vollkommen zu beweisen/", "tokens": ["Ja", "da\u00df", "er", "sei\u00b7ne", "macht", "voll\u00b7kom\u00b7men", "zu", "be\u00b7wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "PPOSAT", "VVFIN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Offt manchen schwartzen dunst uns vor die augen schiebt.", "tokens": ["Offt", "man\u00b7chen", "schwart\u00b7zen", "dunst", "uns", "vor", "die", "au\u00b7gen", "schiebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", 