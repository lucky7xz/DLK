{"textgrid.poem.41047": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Im Winter", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Nachtigall und Lerche singen,", "tokens": ["Wenn", "Nach\u00b7ti\u00b7gall", "und", "Ler\u00b7che", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da schweigt versch\u00e4met mein Gesang,", "tokens": ["Da", "schweigt", "ver\u00b7sch\u00e4\u00b7met", "mein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein armes Lied will nur gelingen", "tokens": ["Mein", "ar\u00b7mes", "Lied", "will", "nur", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Nacht und Tod im Winter bang.", "tokens": ["Bei", "Nacht", "und", "Tod", "im", "Win\u00b7ter", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da liebt es jene \u00f6den N\u00e4chte,", "tokens": ["Da", "liebt", "es", "je\u00b7ne", "\u00f6\u00b7den", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die schwarze, stille Einsamkeit,", "tokens": ["Die", "schwar\u00b7