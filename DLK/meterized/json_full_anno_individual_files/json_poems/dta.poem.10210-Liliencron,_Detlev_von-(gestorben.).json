{"dta.poem.10210": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "(gestorben.)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Nun ist ihm wohl. Er schaut das neue Land,", "tokens": ["Nun", "ist", "ihm", "wohl", ".", "Er", "schaut", "das", "neu\u00b7e", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bleibt, \u201eDas h\u00e4tt\u2019 ich nicht erwartet\u201c, stehn.", "tokens": ["Und", "bleibt", ",", "\u201e", "Das", "h\u00e4tt'", "ich", "nicht", "er\u00b7war\u00b7tet", "\u201c", ",", "stehn", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "$(", "PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$(", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Eine stirbt verlassen und verbannt,", "tokens": ["Der", "Ei\u00b7ne", "stirbt", "ver\u00b7las\u00b7sen", "und", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bei Andern Pomp und Trauerfahnenwehn.", "tokens": ["Bei", "An\u00b7dern", "Pomp", "und", "Trau\u00b7er\u00b7fah\u00b7nen\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Nachbarweiber, menschlich, halten Stand", "tokens": ["Die", "Nach\u00b7bar\u00b7wei\u00b7ber", ",", "menschlich", ",", "hal\u00b7ten", "Stand"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Stunden viel, die \u201esch\u00f6ne Leich\u2019\u201c zu sehn.", "tokens": ["Der", "Stun\u00b7den", "viel", ",", "die", "\u201e", "sch\u00f6\u00b7ne", "Leich'", "\u201c", "zu", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "$(", "ADJA", "NN", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und hinterdrein die Freunde, wehentbrannt,", "tokens": ["Und", "hin\u00b7ter\u00b7drein", "die", "Freun\u00b7de", ",", "we\u00b7hent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vermitteln einen Skat im Weitergehn.", "tokens": ["Ver\u00b7mit\u00b7teln", "ei\u00b7nen", "Skat", "im", "Wei\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}