{"textgrid.poem.55113": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "9", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum ich wieder zum Papier mich wende?", "tokens": ["Wa\u00b7rum", "ich", "wie\u00b7der", "zum", "Pa\u00b7pier", "mich", "wen\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das mu\u00dft du, Liebster, so bestimmt nicht fragen:", "tokens": ["Das", "mu\u00dft", "du", ",", "Liebs\u00b7ter", ",", "so", "be\u00b7stimmt", "nicht", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "NN", "$,", "ADV", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn eigentlich hab ich dir nichts zu sagen;", "tokens": ["Denn", "ei\u00b7gent\u00b7lich", "hab", "ich", "dir", "nichts", "zu", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Doch kommt's zuletzt in deine lieben H\u00e4nde.", "tokens": ["Doch", "kommt's", "zu\u00b7letzt", "in", "dei\u00b7ne", "lie\u00b7ben", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Weil ich nicht kommen kann, soll, was ich sende,", "tokens": ["Weil", "ich", "nicht", "kom\u00b7men", "kann", ",", "soll", ",", "was", "ich", "sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "VMFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein ungeteiltes Herz hin\u00fcbertragen", "tokens": ["Dein", "un\u00b7ge\u00b7teil\u00b7tes", "Herz", "hin\u00b7\u00fc\u00b7ber\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Wonnen, Hoffnungen, Entz\u00fccken, Plagen:", "tokens": ["Mit", "Won\u00b7nen", ",", "Hoff\u00b7nun\u00b7gen", ",", "Ent\u00b7z\u00fc\u00b7cken", ",", "Pla\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das alles hat nicht Anfang, hat nicht Ende.", "tokens": ["Das", "al\u00b7les", "hat", "nicht", "An\u00b7fang", ",", "hat", "nicht", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PTKNEG", "NN", "$,", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich mag vom heut'gen Tag dir nichts vertrauen,", "tokens": ["Ich", "mag", "vom", "heut'\u00b7gen", "Tag", "dir", "nichts", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sich im Sinnen, W\u00fcnschen, W\u00e4hnen,", "tokens": ["Wie", "sich", "im", "Sin\u00b7nen"