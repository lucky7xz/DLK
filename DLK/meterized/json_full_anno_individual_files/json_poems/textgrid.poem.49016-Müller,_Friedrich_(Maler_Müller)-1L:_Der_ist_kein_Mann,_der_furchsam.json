{"textgrid.poem.49016": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der ist kein Mann, der furchsam", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ist kein Mann, der furchsam", "tokens": ["Der", "ist", "kein", "Mann", ",", "der", "furch\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "zu hohem Laster schweigt:", "tokens": ["zu", "ho\u00b7hem", "Las\u00b7ter", "schweigt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Felsen liegt, ihr V\u00e4ter,", "tokens": ["Am", "Fel\u00b7sen", "liegt", ",", "ihr", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Fr\u00e4uelein erbleicht \u2013", "tokens": ["Ein", "Fr\u00e4u\u00b7e\u00b7lein", "er\u00b7bleicht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verf\u00fchrt von falschen Schw\u00fcren", "tokens": ["Ver\u00b7f\u00fchrt", "von", "fal\u00b7schen", "Schw\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durchstach sie sich das Herz.", "tokens": ["Durch\u00b7stach", "sie", "sich", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu Frankreichs stolzem Turme", "tokens": ["Zu", "Fran\u00b7kreichs", "stol\u00b7zem", "Tur\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "floh der Verr\u00e4ter hin.", "tokens": ["floh", "der", "Ver\u00b7r\u00e4\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Der sitzt in goldnem Saale", "tokens": ["Der", "sitzt", "in", "gold\u00b7nem", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Bei Harfenton und Tanz", "tokens": ["Bei", "Har\u00b7fen\u00b7ton", "und", "Tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "am Busen einer Dirne,", "tokens": ["am", "Bu\u00b7sen", "ei\u00b7ner", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "von Bourbons falsch Geschlecht;", "tokens": ["von", "Bour\u00b7bons", "falsch", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "und lacht noch seiner Taten", "tokens": ["und", "lacht", "noch", "sei\u00b7ner", "Ta\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "und prahlt damit und h\u00f6hnt", "tokens": ["und", "prahlt", "da\u00b7mit", "und", "h\u00f6hnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "der zarten Liebe Treue", "tokens": ["der", "zar\u00b7ten", "Lie\u00b7be", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "beim goldenen Pokal.", "tokens": ["beim", "gol\u00b7de\u00b7nen", "Po\u00b7kal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Ha, freu' dich nur \u2013 die Rache", "tokens": ["Ha", ",", "freu'", "dich", "nur", "\u2013", "die", "Ra\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "$(", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "folgt an der Ferse dir!", "tokens": ["folgt", "an", "der", "Fer\u00b7se", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Es heulen St\u00fcrme, Donner,", "tokens": ["Es", "heu\u00b7len", "St\u00fcr\u00b7me", ",", "Don\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "ihm tausend Fl\u00fcche nach.", "tokens": ["ihm", "tau\u00b7send", "Fl\u00fc\u00b7che", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "So niedrig ist's, wenn Schande", "tokens": ["So", "nied\u00b7rig", "ist's", ",", "wenn", "Schan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "im stolzen Helme steckt,", "tokens": ["im", "stol\u00b7zen", "Hel\u00b7me", "steckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "wenn Adels goldne R\u00fcstung", "tokens": ["wenn", "A\u00b7dels", "gold\u00b7ne", "R\u00fcs\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "ein Sklavenherz bedeckt!", "tokens": ["ein", "Skla\u00b7ven\u00b7herz", "be\u00b7deckt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Der Ritter \u2013 gleich der Schlange,", "tokens": ["Der", "Rit\u00b7ter", "\u2013", "gleich", "der", "Schlan\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "die in der Sonne kriecht,", "tokens": ["die", "in", "der", "Son\u00b7ne", "kriecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "die au\u00dfen glei\u00dft, doch giftig", "tokens": ["die", "au\u00b7\u00dfen", "glei\u00dft", ",", "doch", "gif\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "mit schwarzer Zunge sticht \u2013", "tokens": ["mit", "schwar\u00b7zer", "Zun\u00b7ge", "sticht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "mit stolzer Stirne schreitet", "tokens": ["mit", "stol\u00b7zer", "Stir\u00b7ne", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "im hellen Ehrenkleid", "tokens": ["im", "hel\u00b7len", "Eh\u00b7ren\u00b7kleid"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "der tapfern Vaters-V\u00e4ter", "tokens": ["der", "tap\u00b7fern", "Va\u00b7ter\u00b7sV\u00e4\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "und doch nicht Laster scheut.", "tokens": ["und", "doch", "nicht", "Las\u00b7ter", "scheut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Er, der das Schild der Schwachen", "tokens": ["Er", ",", "der", "das", "Schild", "der", "Schwa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "sollt' sein, rei\u00dft von der Brust", "tokens": ["sollt'", "sein", ",", "rei\u00dft", "von", "der", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "VAINF", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.35": {"text": "der Mutter selbst die Tochter", "tokens": ["der", "Mut\u00b7ter", "selbst", "die", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "zu geiler Liebeslust.", "tokens": ["zu", "gei\u00b7ler", "Lie\u00b7bes\u00b7lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Entrei\u00dfet ihm sein Wappen,", "tokens": ["Ent\u00b7rei\u00b7\u00dfet", "ihm", "sein", "Wap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "das er so tief entehrt!", "tokens": ["das", "er", "so", "tief", "ent\u00b7ehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Was n\u00fctzt dem feigen Knaben", "tokens": ["Was", "n\u00fctzt", "dem", "fei\u00b7gen", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "ein scharfgeschliffen Schwert.", "tokens": ["ein", "scharf\u00b7ge\u00b7schlif\u00b7fen", "Schwert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "O, sammelt euch, ihr V\u00e4ter,", "tokens": ["O", ",", "sam\u00b7melt", "euch", ",", "ihr", "V\u00e4\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "o, sammelt euch und tilgt", "tokens": ["o", ",", "sam\u00b7melt", "euch", "und", "tilgt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["FM", "$,", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "die Laster, die nicht euer \u2013", "tokens": ["die", "Las\u00b7ter", ",", "die", "nicht", "eu\u00b7er", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "PPOSAT", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "seht, wie der Nachbar h\u00f6hnt!", "tokens": ["seht", ",", "wie", "der", "Nach\u00b7bar", "h\u00f6hnt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Schlagt eure H\u00e4nd' zusammen", "tokens": ["Schlagt", "eu\u00b7re", "H\u00e4nd'", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "beim freundschaftlichen Wein", "tokens": ["beim", "freund\u00b7schaft\u00b7li\u00b7chen", "Wein"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.47": {"text": "und schw\u00f6rt, den auszurotten,", "tokens": ["und", "schw\u00f6rt", ",", "den", "aus\u00b7zu\u00b7rot\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "der Deutschlands Treue bricht!", "tokens": ["der", "Deutschlands", "Treu\u00b7e", "bricht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.49": {"text": "Du aber, Ritterknabe,", "tokens": ["Du", "a\u00b7ber", ",", "Rit\u00b7ter\u00b7kna\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "sieh her und folge mir:", "tokens": ["sieh", "her", "und", "fol\u00b7ge", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Leg' niemals junger Unschuld", "tokens": ["Leg'", "nie\u00b7mals", "jun\u00b7ger", "Un\u00b7schuld"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "die Schlingen falscher Schw\u00fcr!", "tokens": ["die", "Schlin\u00b7gen", "fal\u00b7scher", "Schw\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Gerecht und gro\u00df zu handeln,", "tokens": ["Ge\u00b7recht", "und", "gro\u00df", "zu", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "ist adelige Pflicht;", "tokens": ["ist", "a\u00b7de\u00b7li\u00b7ge", "Pflicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.55": {"text": "wenn du willst niedrig denken,", "tokens": ["wenn", "du", "willst", "nied\u00b7rig", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "so f\u00fchr' den Adler nicht.", "tokens": ["so", "f\u00fchr'", "den", "Ad\u00b7ler", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.57": {"text": "F\u00fcr dich schickt's sich, o Knabe,", "tokens": ["F\u00fcr", "dich", "schickt's", "sich", ",", "o", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "$,", "FM", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.58": {"text": "nah der Gefahr zu stehn,", "tokens": ["nah", "der", "Ge\u00b7fahr", "zu", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.59": {"text": "dem Feind ohn' Furcht und Schrecken", "tokens": ["dem", "Feind", "ohn'", "Furcht", "und", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "In's Angesicht zu sehn.", "tokens": ["In's", "An\u00b7ge\u00b7sicht", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.61": {"text": "Gedr\u00fcckte Schw\u00e4ch' zu st\u00e4rken", "tokens": ["Ge\u00b7dr\u00fcck\u00b7te", "Schw\u00e4ch'", "zu", "st\u00e4r\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.62": {"text": "sei deines Pfades Spur;", "tokens": ["sei", "dei\u00b7nes", "Pfa\u00b7des", "Spur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.63": {"text": "zum Schilde der Bedr\u00e4ngten", "tokens": ["zum", "Schil\u00b7de", "der", "Be\u00b7dr\u00e4ng\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.64": {"text": "erschuf dich die Natur.", "tokens": ["er\u00b7schuf", "dich", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.65": {"text": "F\u00fchlst du der Liebe Flammen", "tokens": ["F\u00fchlst", "du", "der", "Lie\u00b7be", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.66": {"text": "einst, sag' es ohne Scheu", "tokens": ["einst", ",", "sag'", "es", "oh\u00b7ne", "Scheu"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.67": {"text": "vor aller Welt dem Fr\u00e4ulein", "tokens": ["vor", "al\u00b7ler", "Welt", "dem", "Fr\u00e4u\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.68": {"text": "und bleib ihr immer treu.", "tokens": ["und", "bleib", "ihr", "im\u00b7mer", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.69": {"text": "Verfluch mit mir die Memme,", "tokens": ["Ver\u00b7fluch", "mit", "mir", "die", "Mem\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.70": {"text": "die Schwacher Ehre raubt.", "tokens": ["die", "Schwa\u00b7cher", "Eh\u00b7re", "raubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.71": {"text": "So bleibst du deutschen Stammes", "tokens": ["So", "bleibst", "du", "deut\u00b7schen", "Stam\u00b7mes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.72": {"text": "und deiner V\u00e4ter wert,", "tokens": ["und", "dei\u00b7ner", "V\u00e4\u00b7ter", "wert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.73": {"text": "der V\u00e4ter, die die Tugend", "tokens": ["der", "V\u00e4\u00b7ter", ",", "die", "die", "Tu\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.74": {"text": "und Heldenmut geehrt.", "tokens": ["und", "Hel\u00b7den\u00b7mut", "ge\u00b7ehrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der ist kein Mann, der furchsam", "tokens": ["Der", "ist", "kein", "Mann", ",", "der", "furch\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "zu hohem Laster schweigt:", "tokens": ["zu", "ho\u00b7hem", "Las\u00b7ter", "schweigt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Felsen liegt, ihr V\u00e4ter,", "tokens": ["Am", "Fel\u00b7sen", "liegt", ",", "ihr", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Fr\u00e4uelein erbleicht \u2013", "tokens": ["Ein", "Fr\u00e4u\u00b7e\u00b7lein", "er\u00b7bleicht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verf\u00fchrt von falschen Schw\u00fcren", "tokens": ["Ver\u0