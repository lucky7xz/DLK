N", "VAINF", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.35": {"text": "der Mutter selbst die Tochter", "tokens": ["der", "Mut\u00b7ter", "selbst", "die", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "zu geiler Liebeslust.", "tokens": ["zu", "gei\u00b7ler", "Lie\u00b7bes\u00b7lust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Entrei\u00dfet ihm sein Wappen,", "tokens": ["Ent\u00b7rei\u00b7\u00dfet", "ihm", "sein", "Wap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "das er so tief entehrt!", "tokens": ["das", "er", "so", "tief", "ent\u00b7ehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Was n\u00fctzt dem feigen Knaben", "tokens": ["Was", "n\u00fctzt", "dem", "fei\u00b7gen", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "ein scharfgeschliffen Schwert.", "tokens": ["ein", "scharf\u00b7ge\u00b7schlif\u00b7fen", "Schwert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "O, sammelt euch, ihr V\u00e4ter,", "tokens": ["O", ",", "sam\u00b7melt", "euch", ",", "ihr", "V\u00e4\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+