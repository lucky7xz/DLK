{"textgrid.poem.31359": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den K\u00f6nig von Preussen", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was blickst du doch zu Boden schweigend nieder,", "tokens": ["Was", "blickst", "du", "doch", "zu", "Bo\u00b7den", "schwei\u00b7gend", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch ein Portal siegprangend eingef\u00fchrt?", "tokens": ["Durch", "ein", "Por\u00b7tal", "sieg\u00b7pran\u00b7gend", "ein\u00b7ge\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Du wendest dich, begr\u00fc\u00dft vom Schall der Lieder,", "tokens": ["Du", "wen\u00b7dest", "dich", ",", "be\u00b7gr\u00fc\u00dft", "vom", "Schall", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und deine sch\u00f6ne Brust, sie scheint ger\u00fchrt.", "tokens": ["Und", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Brust", ",", "sie", "scheint", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Blick auf, o Herr! Du kehrst als Sieger wieder,", "tokens": ["Blick", "auf", ",", "o", "Herr", "!", "Du", "kehrst", "als", "Sie\u00b7ger", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "KOUS", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie hoch auch jener C\u00e4sar triumphiert:", "tokens": ["Wie", "hoch", "auch", "je\u00b7ner", "C\u00e4\u00b7sar", "tri\u00b7um\u00b7phiert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PDAT", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihm ist die Schar der G\u00f6tter zugefallen,", "tokens": ["Ihm", "ist", "die", "Schar", "der", "G\u00f6t\u00b7ter", "zu\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Jedoch den Menschen hast du wohlgefallen.", "tokens": ["Je\u00b7doch", "den", "Men\u00b7schen", "hast", "du", "wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du hast ihn treu, den Kampf, als Held getragen,", "tokens": ["Du", "hast", "ihn", "treu", ",", "den", "Kampf", ",", "als", "Held", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "ART", "NN", "$,", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem du, um nichtgen Ruhms, dich nicht geweiht!", "tokens": ["Dem", "du", ",", "um", "nicht\u00b7gen", "Ruhms", ",", "dich", "nicht", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "KOUI", "ADJA", "NN", "$,", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du h\u00e4ttest noch, in den Entscheidungstagen,", "tokens": ["Du", "h\u00e4t\u00b7test", "noch", ",", "in", "den", "Ent\u00b7schei\u00b7dungs\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der h\u00f6chsten Friedensopfer keins gescheut.", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Frie\u00b7den\u00b7sop\u00b7fer", "keins", "ge\u00b7scheut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die sch\u00f6nste Tugend, la\u00df michs k\u00fchn dir sagen,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Tu\u00b7gend", ",", "la\u00df", "michs", "k\u00fchn", "dir", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVIMP", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hat mit dem Gl\u00fcck des Krieges dich entzweit:", "tokens": ["Hat", "mit", "dem", "Gl\u00fcck", "des", "Krie\u00b7ges", "dich", "ent\u00b7zweit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du brauchtest Wahrheit weniger zu lieben,", "tokens": ["Du", "brauch\u00b7test", "Wahr\u00b7heit", "we\u00b7ni\u00b7ger", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Sieger w\u00e4rst du auf dem Schlachtfeld blieben.", "tokens": ["Und", "Sie\u00b7ger", "w\u00e4rst", "du", "auf", "dem", "Schlacht\u00b7feld", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "La\u00df denn zerknickt die Saat, von Waffenst\u00fcrmen,", "tokens": ["La\u00df", "denn", "zer\u00b7knickt", "die", "Saat", ",", "von", "Waf\u00b7fen\u00b7st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die H\u00fctten la\u00df ein Raub der Flammen sein;", "tokens": ["Die", "H\u00fct\u00b7ten", "la\u00df", "ein", "Raub", "der", "Flam\u00b7men", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du hast die Brust geboten, sie zu schirmen:", "tokens": ["Du", "hast", "die", "Brust", "ge\u00b7bo\u00b7ten", ",", "sie", "zu", "schir\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Lethe wollen wir die Asche weihn.", "tokens": ["Dem", "Le\u00b7the", "wol\u00b7len", "wir", "die", "A\u00b7sche",