], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Es ", "tokens": ["Es"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Ersehnter Einheit f\u00fcr uns schl\u00e4gt,", "tokens": ["Er\u00b7sehn\u00b7ter", "Ein\u00b7heit", "f\u00fcr", "uns", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wenn keine Kr\u00e4merwage mehr, wie Pfunde,", "tokens": ["Wenn", "kei\u00b7ne", "Kr\u00e4\u00b7mer\u00b7wa\u00b7ge", "mehr", ",", "wie", "Pfun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Europa's Schicksal w\u00e4gt.", "tokens": ["Eu\u00b7ro\u00b7pa's", "Schick\u00b7sal", "w\u00e4gt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Schon schaut mein Geist das nie Geschaute,", "tokens": ["Schon", "schaut", "mein", "Geist", "das", "nie", "Ge\u00b7schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz wird segelgleich geschwellt,", "tokens": ["Mein", "Herz", "wird", "se\u00b7gel\u00b7gleich", "ge\u00b7schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon ist die Flotte aufgestellt,", "tokens": ["Schon", "ist", "die", "Flot\u00b7te", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die unser Volk erbaute;", "tokens": ["Die", "un\u00b7ser", "Volk", "er\u00b7bau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schon lehn' ich selbst, ein deutscher Argonaute,", "tokens": ["Schon", "lehn'", "ich", "selbst", ",", "ein", "deut\u00b7scher", "Ar\u00b7go\u00b7nau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An einem Mast, und k\u00e4mpfe mit der Laute", "tokens": ["An", "ei\u00b7nem", "Mast", ",", "und", "k\u00e4mp\u00b7fe", "mit", "der", "Lau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ums goldne Vlie\u00df der Welt.", "tokens": ["Ums", "gold\u00b7ne", "Vlie\u00df", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}