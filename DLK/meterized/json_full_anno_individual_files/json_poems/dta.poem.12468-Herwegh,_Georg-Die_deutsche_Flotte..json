{"dta.poem.12468": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die deutsche Flotte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erwach', mein Volk, mit neuen Sinnen!", "tokens": ["Er\u00b7wach'", ",", "mein", "Volk", ",", "mit", "neu\u00b7en", "Sin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Blick' in des Schicksals goldnes Buch,", "tokens": ["Blick'", "in", "des", "Schick\u00b7sals", "gold\u00b7nes", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Lies aus den Sternen dir den Spruch:", "tokens": ["Lies", "aus", "den", "Ster\u00b7nen", "dir", "den", "Spruch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Erwach', mein Volk, hei\u00df' deine T\u00f6chter spinnen!", "tokens": ["Er\u00b7wach'", ",", "mein", "Volk", ",", "hei\u00df'", "dei\u00b7ne", "T\u00f6ch\u00b7ter", "spin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wir brauchen wieder einmal deutsches Linnen", "tokens": ["Wir", "brau\u00b7chen", "wie\u00b7der", "ein\u00b7mal", "deut\u00b7sches", "Lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu deutschem Segeltuch.", "tokens": ["Zu", "deut\u00b7schem", "Se\u00b7gel\u00b7tuch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hinweg die feige Knechtsgeberde;", "tokens": ["Hin\u00b7weg", "die", "fei\u00b7ge", "Knechts\u00b7ge\u00b7ber\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zerbrich der Heimat Schneckenhaus,", "tokens": ["Zer\u00b7brich", "der", "Hei\u00b7mat", "Schne\u00b7cken\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zieh' muthig in die Welt hinaus,", "tokens": ["Zieh'", "mut\u00b7hig", "in", "die", "Welt", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie dein eigen werde!", "tokens": ["Da\u00df", "sie", "dein", "ei\u00b7gen", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum wirf den Anker aus!", "tokens": ["Drum", "wirf", "den", "An\u00b7ker", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "War Hellas einst von bessrem Stamme,", "tokens": ["War", "Hel\u00b7las", "einst", "von", "bess\u00b7rem", "Stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als du? von bessrem Stamme Rom?", "tokens": ["Als", "du", "?", "von", "bess\u00b7rem", "Stam\u00b7me", "Rom", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$.", "APPR", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Hermann, dein gepries'ner Ohm,", "tokens": ["Da\u00df", "Her\u00b7mann", ",", "dein", "ge\u00b7pries'\u00b7ner", "Ohm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Volk, dich nicht verdamme \u2014", "tokens": ["Mein", "Volk", ",", "dich", "nicht", "ver\u00b7dam\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hinaus ins Meer mit Kreuz und Oriflamme!", "tokens": ["Hin\u00b7aus", "ins", "Meer", "mit", "Kreuz", "und", "O\u00b7ri\u00b7flam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sei m\u00fcndig und entlaufe deiner Amme,", "tokens": ["Sei", "m\u00fcn\u00b7dig", "und", "ent\u00b7lau\u00b7fe", "dei\u00b7ner", "Am\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie seinem Quell dein Strom!", "tokens": ["Wie", "sei\u00b7nem", "Quell", "dein", "Strom", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wohl ist sie dein, die sch\u00f6nste Flotte,", "tokens": ["Wohl", "ist", "sie", "dein", ",", "die", "sch\u00f6ns\u00b7te", "Flot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die je ein sterblich Aug' entz\u00fcckt:", "tokens": ["Die", "je", "ein", "sterb\u00b7lich", "Aug'", "ent\u00b7z\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der M\u00fcnster Schiffe, wie geschm\u00fcckt", "tokens": ["Der", "M\u00fcns\u00b7ter", "Schif\u00b7fe", ",", "wie", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hast Du sie deinem Gotte!", "tokens": ["Hast", "Du", "sie", "dei\u00b7nem", "Got\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du l\u00e4chelst ob der Feinde schwachem Spotte,", "tokens": ["Du", "l\u00e4\u00b7chelst", "ob", "der", "Fein\u00b7de", "schwa\u00b7chem", "Spot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn sie auf schwankem Brett, die freche Rotte,", "tokens": ["Wenn", "sie", "auf", "schwan\u00b7kem", "Brett", ",", "die", "fre\u00b7che", "Rot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Frucht der Erde pfl\u00fcckt.", "tokens": ["Die", "Frucht", "der", "Er\u00b7de", "pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Wenn erst das Salz dein Ruder netzt,", "tokens": ["Wenn", "erst", "das", "Salz", "dein", "Ru\u00b7der", "netzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und all die Sterne, die sich jetzt", "tokens": ["Und", "all", "die", "Ster\u00b7ne", ",", "die", "sich", "jetzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "ART", "NN", "$,", "PRELS", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stolz \u00fcber'm Haubt dir wiegen,", "tokens": ["Stolz", "\u00fc\u00b7ber'm", "Haubt", "dir", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gleich schmucken Sklaven dir zu F\u00fc\u00dfen liegen;", "tokens": ["Gleich", "schmu\u00b7cken", "Skla\u00b7ven", "dir", "zu", "F\u00fc\u00b7\u00dfen", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So zwischen zweien Himmeln hinzufliegen \u2014", "tokens": ["So", "zwi\u00b7schen", "zwei\u00b7en", "Him\u00b7meln", "hin\u00b7zu\u00b7flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Di\u00df Ziel ist dir gesetzt!", "tokens": ["Di\u00df", "Ziel", "ist", "dir", "ge\u00b7setzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O blick' hinaus ins Schrankenlose!", "tokens": ["O", "blick'", "hin\u00b7aus", "ins", "Schran\u00b7ken\u00b7lo\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Best\u00fcrmt dein Herz nicht hohe Lust,", "tokens": ["Be\u00b7st\u00fcrmt", "dein", "Herz", "nicht", "ho\u00b7he", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn, wie an einer M\u00e4dchenbrust", "tokens": ["Wenn", ",", "wie", "an", "ei\u00b7ner", "M\u00e4d\u00b7chen\u00b7brust"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die aufgebl\u00fchte Rose,", "tokens": ["Die", "auf\u00b7ge\u00b7bl\u00fch\u00b7te", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Sonne zittert in des Meeres Schoo\u00dfe?", "tokens": ["Die", "Son\u00b7ne", "zit\u00b7tert", "in", "des", "Mee\u00b7res", "Schoo\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und rauschen nicht der Tiefe tausend Moose", "tokens": ["Und", "rau\u00b7schen", "nicht", "der", "Tie\u00b7fe", "tau\u00b7send", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dir zu: du mu\u00dft! du mu\u00dft!?", "tokens": ["Dir", "zu", ":", "du", "mu\u00dft", "!", "du", "mu\u00dft", "!?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$.", "PPER", "VMFIN", "$.", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gleicht nicht das heil'ge Meer dem weiten", "tokens": ["Gleicht", "nicht", "das", "heil'\u00b7ge", "Meer", "dem", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Friedhof der Welt, dar\u00fcber hin", "tokens": ["Fried\u00b7hof", "der", "Welt", ",", "da\u00b7r\u00fc\u00b7ber", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PAV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wogen Decken von Rubin", "tokens": ["Die", "Wo\u00b7gen", "De\u00b7cken", "von", "Ru\u00b7bin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "NE"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und gr\u00fcne H\u00fcgel breiten?", "tokens": ["Und", "gr\u00fc\u00b7ne", "H\u00fc\u00b7gel", "brei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Um deiner Todten Asche mu\u00dft du streiten!", "tokens": ["Um", "dei\u00b7ner", "Tod\u00b7ten", "A\u00b7sche", "mu\u00dft", "du", "strei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ha! schlummern nicht aus deiner Hansa Zeiten", "tokens": ["Ha", "!", "schlum\u00b7mern", "nicht", "aus", "dei\u00b7ner", "Han\u00b7sa", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auch deutsche Helden drin?", "tokens": ["Auch", "deut\u00b7sche", "Hel\u00b7den", "drin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wiegt sich nicht auf krystallnem Stuhle", "tokens": ["Wiegt", "sich", "nicht", "auf", "krys\u00b7tall\u00b7nem", "Stuh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Im Meer der Nere\u00efden Schaar,", "tokens": ["Im", "Meer", "der", "Ne\u00b7re\u00ef\u00b7den", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die sich ihr Schicksal Jahr um Jahr", "tokens": ["Die", "sich", "ihr", "Schick\u00b7sal", "Jahr", "um", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "PPOSAT", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Abspinnt von goldner Spule?", "tokens": ["Ab\u00b7spinnt", "von", "gold\u00b7ner", "Spu\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Lockt sie dich nicht, der Becher nicht von Thule,", "tokens": ["Lockt", "sie", "dich", "nicht", ",", "der", "Be\u00b7cher", "nicht", "von", "Thu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "$,", "ART", "NN", "PTKNEG", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das wilde Meer, der Freiheit Hohe-Schule,", "tokens": ["Das", "wil\u00b7de", "Meer", ",", "der", "Frei\u00b7heit", "Ho\u00b7he\u00b7Schu\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Lockt dich nicht die Gefahr? \u2014", "tokens": ["Lockt", "dich", "nicht", "die", "Ge\u00b7fahr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$.", "$("], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.9": {"line.1": {"text": "Das Meer wird uns vom Herzen sp\u00fclen", "tokens": ["Das", "Meer", "wird", "uns", "vom", "Her\u00b7zen", "sp\u00fc\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": 