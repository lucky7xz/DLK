{"textgrid.poem.48735": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "22. An denselben F\u00fcrstl. Holsteinischen Rat und Gesandten, zu eben selbiger Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als eure Ankunft, Herr, uns wurde kund getan,", "tokens": ["Als", "eu\u00b7re", "An\u00b7kunft", ",", "Herr", ",", "uns", "wur\u00b7de", "kund", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "NN", "$,", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da hub die tr\u00fcbe Luft sich an bald abzuhellen,", "tokens": ["da", "hub", "die", "tr\u00fc\u00b7be", "Luft", "sich", "an", "bald", "ab\u00b7zu\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PRF", "APPR", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das ausgeschlagne Feld dem Lenzen gleichzustellen;", "tokens": ["das", "aus\u00b7ge\u00b7schlag\u00b7ne", "Feld", "dem", "Len\u00b7zen", "gleich\u00b7zu\u00b7stel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die ganze Stadt ward froh, als wie man denken kan.", "tokens": ["die", "gan\u00b7ze", "Stadt", "ward", "froh", ",", "als", "wie", "man", "den\u00b7ken", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "KOUS", "PWAV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kein Haus, das stellte sich, als ging es di\u00df nicht an;", "tokens": ["Kein", "Haus", ",", "das", "stell\u00b7te", "sich", ",", "als", "ging", "es", "di\u00df", "nicht", "an", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PDS", "VVFIN", "PRF", "$,", "KOUS", "VVFIN", "PPER", "PDS", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das Land gab in der Lust auch einen Mitgesellen,", "tokens": ["das", "Land", "gab", "in", "der", "Lust", "auch", "ei\u00b7nen", "Mit\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die abgest\u00fcrmte See bez\u00e4hmet' ihre Wellen,", "tokens": ["die", "ab\u00b7ge\u00b7st\u00fcrm\u00b7te", "See", "be\u00b7z\u00e4h\u00b7met'", "ih\u00b7re", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die leichte Federschaar war fr\u00f6lich mitte dran,", "tokens": ["die", "leich\u00b7te", "Fe\u00b7der\u00b7schaar", "war", "fr\u00f6\u00b7lich", "mit\u00b7te", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "das frohe Wild trat vor aus seinen gr\u00fcnen Heinen;", "tokens": ["das", "fro\u00b7he", "Wil