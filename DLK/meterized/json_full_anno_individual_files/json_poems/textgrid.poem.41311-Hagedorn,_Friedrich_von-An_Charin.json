{"textgrid.poem.41311": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Charin", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Pandus, der so zu dir schleicht,", "tokens": ["Dein", "Pan\u00b7dus", ",", "der", "so", "zu", "dir", "schleicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat Eulenaugen, und sie schielen;", "tokens": ["Hat", "Eu\u00b7len\u00b7au\u00b7gen", ",", "und", "sie", "schie\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Kinn ist spitz; er lacht nicht leicht,", "tokens": ["Sein", "Kinn", "ist", "spitz", ";", "er", "lacht", "nicht", "leicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wird stets mit der Zunge spielen.", "tokens": ["Und", "wird", "stets", "mit", "der", "Zun\u00b7ge", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wei\u00df, da\u00df du ihm g\u00fcnstig bist:", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "du", "ihm", "g\u00fcns\u00b7tig", "bist", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "