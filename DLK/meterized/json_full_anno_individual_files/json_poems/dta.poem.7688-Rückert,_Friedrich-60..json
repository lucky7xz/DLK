{"dta.poem.7688": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn es dir nicht bequem, behaglich ist und gut,", "tokens": ["Wenn", "es", "dir", "nicht", "be\u00b7quem", ",", "be\u00b7hag\u00b7lich", "ist", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADJD", "$,", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In unbemerktem Fall rinnt deines Daseyns Flut;", "tokens": ["In", "un\u00b7be\u00b7merk\u00b7tem", "Fall", "rinnt", "dei\u00b7nes", "Da\u00b7seyns", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So scheuest du dich wol das Kleinste zu verr\u00fccken", "tokens": ["So", "scheu\u00b7est", "du", "dich", "wol", "das", "Kleins\u00b7te", "zu", "ver\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An des Gewohnten auch bedeutunglosen St\u00fccken,", "tokens": ["An", "des", "Ge\u00b7wohn\u00b7ten", "auch", "be\u00b7deu\u00b7tun\u00b7glo\u00b7sen", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aus Furcht, zu r\u00fchren an verborgnen Talisman,", "tokens": ["Aus", "Furcht", ",", "zu", "r\u00fch\u00b7ren", "an", "ver\u00b7borg\u00b7nen", "Ta\u00b7lis\u00b7man", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch de\u00df Zertr\u00fcmmerung dein Gl\u00fcck zertr\u00fcmmern kann.", "tokens": ["Durch", "de\u00df", "Zer\u00b7tr\u00fcm\u00b7me\u00b7rung", "dein", "Gl\u00fcck", "zer\u00b7tr\u00fcm\u00b7mern", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}