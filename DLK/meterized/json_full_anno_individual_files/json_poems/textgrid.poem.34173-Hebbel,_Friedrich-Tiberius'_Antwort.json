{"textgrid.poem.34173": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Tiberius' Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgro\u00dfer C\u00e4sar, du hast den Jesus Christus gekreuzigt,", "tokens": ["\u00bb", "gro\u00b7\u00dfer", "C\u00e4\u00b7sar", ",", "du", "hast", "den", "Je\u00b7sus", "Chris\u00b7tus", "ge\u00b7kreu\u00b7zigt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NE", "$,", "PPER", "VAFIN", "ART", "NE", "NE", "VVPP", "$,"], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Aber die Lehre, sie lebt, ja, sie verbreitet sich stark!\u00ab", "tokens": ["A\u00b7ber", "die", "Leh\u00b7re", ",", "sie", "lebt", ",", "ja", ",", "sie", "ver\u00b7brei\u00b7tet", "sich", "stark", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVFIN", "$,", "PTKANT", "$,", "PPER", "VVFIN", "PRF", "ADJD", "$.", "$("], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Blo\u00dfe Schuld des Pilatus, denn h\u00e4tt' er die Zw\u00f6lf, die Apostel,", "tokens": ["Blo\u00b7\u00dfe", "Schuld", "des", "Pi\u00b7la\u00b7tus", ",", "denn", "h\u00e4tt'", "er", "die", "Zw\u00f6lf", ",", "die", "A\u00b7pos\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "KON", "VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Mit ihm gekreuzigt, so w\u00e4r' Alles auf ewig vorbei.", "tokens":