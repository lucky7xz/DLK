{"textgrid.poem.52466": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "48. Vertrauen", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer giebt uns unsern Kinderglauben", "tokens": ["Wer", "giebt", "uns", "un\u00b7sern", "Kin\u00b7der\u00b7glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An eine treue Welt zur\u00fcck?", "tokens": ["An", "ei\u00b7ne", "treu\u00b7e", "Welt", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, schlie\u00dft den allzu scharfen Blick!", "tokens": ["Ach", ",", "schlie\u00dft", "den", "all\u00b7zu", "schar\u00b7fen", "Blick", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ART", "PTKA", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was uns die Zuversicht kann rauben,", "tokens": ["Was", "uns", "die", "Zu\u00b7ver\u00b7sicht", "kann", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zerst\u00f6rt des Herzens Gl\u00fcck.", "tokens": ["Zer\u00b7st\u00f6rt", "des", "Her\u00b7zens", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dein denkt mein Geist mit Wohlgefallen,", "tokens": ["Dein", "denkt", "mein", "Geist", "mit", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Zeit, wenn, fremd in kl\u00fcg'rer Welt,", "tokens": ["O", "Zeit", ",", "wenn", ",", "fremd", "in", "kl\u00fcg'\u00b7rer", "Welt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man traut zu jedem sich gesellt,", "tokens": ["Man", "traut", "zu", "je\u00b7dem", "sich", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PIS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und arglos, wie die Nachtigallen,", "tokens": ["Und", "arg\u00b7los", ",", "wie", "die", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In offne Schlingen f\u00e4llt.", "tokens": ["In", "off\u00b7ne", "Schlin\u00b7gen", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Gl\u00fcck, noch kindlich hinzulangen", "tokens": ["O", "Gl\u00fcck", ",", "noch", "kind\u00b7lich", "hin\u00b7zu\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Blumen, eh' man sie benennt,", "tokens": ["Nach", "Blu\u00b7men", ",", "eh'", "man", "sie", "be\u00b7nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach Freuden, die man halb nur kennt;", "tokens": ["Nach", "Freu\u00b7den", ",", "die", "man", "halb", "nur", "kennt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIS", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn unser Blick, kaum aufgegangen,", "tokens": ["Wenn", "un\u00b7ser", "Blick", ",", "kaum", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht Schein und Wesen trennt!", "tokens": ["Nicht", "Schein", "und", "We\u00b7sen", "trennt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr Tage, wo wir kl\u00fcger werden,", "tokens": ["Ihr", "Ta\u00b7ge", ",", "wo", "wir", "kl\u00fc\u00b7ger", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schw\u00fcl ist euer Mittagslicht,", "tokens": ["Wie", "schw\u00fcl", "ist", "eu\u00b7er", "Mit\u00b7tags\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn die Erfahrung warnend spricht:", "tokens": ["Wenn", "die", "Er\u00b7fah\u00b7rung", "war\u00b7nend", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vollkommen weilet nichts auf Erden!", "tokens": ["Voll\u00b7kom\u00b7men", "wei\u00b7let", "nichts", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was bl\u00fchet, w\u00e4hret nicht.", "tokens": ["Was", "bl\u00fc\u00b7het", ",", "w\u00e4h\u00b7ret", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wohl dann dem liebenden Gem\u00fcte,", "tokens": ["Wohl", "dann", "dem", "lie\u00b7ben\u00b7den", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das sein Vertrauen rein bewahrt,", "tokens": ["Das", "sein", "Ver\u00b7trau\u00b7en", "rein", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, sein Gef\u00fchl sei noch so zart,", "tokens": ["Und", ",", "sein", "Ge\u00b7f\u00fchl", "sei", "noch", "so", "zart", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nie zweifelt an des Edeln G\u00fcte,", "tokens": ["Nie", "zwei\u00b7felt", "an", "des", "E\u00b7deln", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word",