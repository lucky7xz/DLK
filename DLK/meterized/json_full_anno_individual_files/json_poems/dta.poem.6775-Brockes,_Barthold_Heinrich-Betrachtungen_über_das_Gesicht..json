{"dta.poem.6775": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In jeder edlen Kunst und Wissenschafft,", "tokens": ["In", "je\u00b7der", "ed\u00b7len", "Kunst", "und", "Wis\u00b7sen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gebraucht man allerdings der Augen rege Krafft,", "tokens": ["Ge\u00b7braucht", "man", "al\u00b7ler\u00b7dings", "der", "Au\u00b7gen", "re\u00b7ge", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In allen n\u00fctzlichen Experimenten", "tokens": ["In", "al\u00b7len", "n\u00fctz\u00b7li\u00b7chen", "Ex\u00b7pe\u00b7ri\u00b7men\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind sie nothwendige Agenten.", "tokens": ["Sind", "sie", "noth\u00b7wen\u00b7di\u00b7ge", "A\u00b7gen\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Spricht man darum mit Recht, da\u00df sie vern\u00fcnfftig seyn?", "tokens": ["Spricht", "man", "da\u00b7rum", "mit", "Recht", ",", "da\u00df", "sie", "ver\u00b7n\u00fcnff\u00b7tig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PAV", "APPR", "NN", "$,", "KOUS", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn von der Bau-Kunst wir ein Meister-St\u00fcck erh\u00f6hn,", "tokens": ["Wenn", "von", "der", "Bau\u00b7Kunst", "wir", "ein", "Meis\u00b7ter\u00b7St\u00fcck", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wenn wir Stein und Tuch, durch Kunst belebet sehn;", "tokens": ["Und", "wenn", "wir", "Stein", "und", "Tuch", ",", "durch", "Kunst", "be\u00b7le\u00b7bet", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "KON", "NN", "$,", "APPR", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Besteht es in der Hand, besteht es in den Augen,", "tokens": ["Be\u00b7steht", "es", "in", "der", "Hand", ",", "be\u00b7steht", "es", "in", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df wir solch k\u00fcnstlich Werck wohl anzugeben taugen?", "tokens": ["Da\u00df", "wir", "solch", "k\u00fcnst\u00b7lich", "Werck", "wohl", "an\u00b7zu\u00b7ge\u00b7ben", "tau\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "ADV", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}