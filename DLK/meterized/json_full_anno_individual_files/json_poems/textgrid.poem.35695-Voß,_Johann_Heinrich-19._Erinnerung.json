{"textgrid.poem.35695": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "19. Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch gr\u00fcne Linden blinkt die Abendr\u00f6te;", "tokens": ["Durch", "gr\u00fc\u00b7ne", "Lin\u00b7den", "blinkt", "die", "A\u00b7bend\u00b7r\u00f6\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Duft des Grases, das die Sense m\u00e4hte,", "tokens": ["Der", "Duft", "des", "Gra\u00b7ses", ",", "das", "die", "Sen\u00b7se", "m\u00e4h\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Haucht lieblich her vom Erlenbach;", "tokens": ["Haucht", "lieb\u00b7lich", "her", "vom", "Er\u00b7len\u00b7bach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Apfelbaum wehn junge Bl\u00fcten nieder;", "tokens": ["Vom", "Ap\u00b7fel\u00b7baum", "wehn", "jun\u00b7ge", "Bl\u00fc\u00b7ten", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Freundin Nachtigall seufzt ihre Lieder,", "tokens": ["Und", "Freun\u00b7din", "Nach\u00b7ti\u00b7gall", "seufzt", "ih\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "p