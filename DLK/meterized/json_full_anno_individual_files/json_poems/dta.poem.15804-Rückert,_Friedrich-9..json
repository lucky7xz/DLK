{"dta.poem.15804": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du klagest auch, o Freund, nicht recht mit dem zufrieden,", "tokens": ["Du", "kla\u00b7gest", "auch", ",", "o", "Freund", ",", "nicht", "recht", "mit", "dem", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "FM", "NN", "$,", "PTKNEG", "ADV", "APPR", "ART", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was dir in deinem Kreis zu wirken war beschieden.", "tokens": ["Was", "dir", "in", "dei\u00b7nem", "Kreis", "zu", "wir\u00b7ken", "war", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wol freilich anders siehst du das Gewirkte jetzt,", "tokens": ["Wol", "frei\u00b7lich", "an\u00b7ders", "siehst", "du", "das", "Ge\u00b7wirk\u00b7te", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Als da du Muth und Kraft zuerst ans Werk gesetzt.", "tokens": ["Als", "da", "du", "Muth", "und", "Kraft", "zu\u00b7erst", "ans", "Werk", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "KON", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer ist zufrieden denn? Dich tr\u00f6st' es immerhin,", "tokens": ["Wer", "ist", "zu\u00b7frie\u00b7den", "denn", "?", "Dich", "tr\u00f6st'", "es", "im\u00b7mer\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "KON", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin zufrieden, da\u00df ich nicht zufrieden bin.", "tokens": ["Ich", "bin", "zu\u00b7frie\u00b7den", ",", "da\u00df", "ich", "nicht", "zu\u00b7frie\u00b7den", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zufrieden bin ich nicht mit dem was ich gethan,", "tokens": ["Zu\u00b7frie\u00b7den", "bin", "ich", "nicht", "mit", "dem", "was", "ich", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "APPR", "ART", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zufrieden nur damit, zu thun soviel ich kan.", "tokens": ["Zu\u00b7frie\u00b7den", "nur", "da\u00b7mit", ",", "zu", "thun", "so\u00b7viel", "ich", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PAV", "$,", "PTKZU", "VVINF", "VMFIN", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}