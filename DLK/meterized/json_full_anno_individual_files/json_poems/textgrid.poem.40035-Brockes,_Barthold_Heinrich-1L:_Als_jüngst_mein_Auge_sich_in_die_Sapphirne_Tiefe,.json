{"textgrid.poem.40035": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als j\u00fcngst mein Auge sich in die Sapphirne Tiefe,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als j\u00fcngst mein Auge sich in die Sapphirne Tiefe,", "tokens": ["Als", "j\u00fcngst", "mein", "Au\u00b7ge", "sich", "in", "die", "Sap\u00b7phir\u00b7ne", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die weder Grund, noch Strand, noch Ziel, noch End' umschrenckt,", "tokens": ["Die", "we\u00b7der", "Grund", ",", "noch", "Strand", ",", "noch", "Ziel", ",", "noch", "End'", "um\u00b7schrenckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "KON", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ins unerforschte Meer des holen Luft-Raums, senckt',", "tokens": ["Ins", "un\u00b7er\u00b7forschte", "Meer", "des", "ho\u00b7len", "Luft\u00b7Raums", ",", "sen\u00b7ckt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und mein verschlung'ner Blick bald hie- bald dahin liefe,", "tokens": ["Und", "mein", "ver\u00b7schlung'\u00b7ner", "Blick", "bald", "hie", "bald", "da\u00b7hin", "lie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "TRUNC", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch immer tiefer sanck; entsatze sich mein Geist,", "tokens": ["Doch", "im\u00b7mer", "tie\u00b7fer", "san\u00b7ck", ";", "ent\u00b7sat\u00b7ze", "sich", "mein", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$.", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Es schwindelte mein Aug', es stockte meine Seele", "tokens": ["Es", "schwin\u00b7del\u00b7te", "mein", "Aug'", ",", "es", "stock\u00b7te", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ob der unendlichen, unm\u00e4\u00dfig-tiefen H\u00f6le,", "tokens": ["Ob", "der", "un\u00b7end\u00b7li\u00b7chen", ",", "un\u00b7m\u00e4\u00b7\u00dfig\u00b7tie\u00b7fen", "H\u00f6\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die, wohl mit Recht, ein Bild der Ewigkeiten heisse,", "tokens": ["Die", ",", "wohl", "mit", "Recht", ",", "ein", "Bild", "der", "E\u00b7wig\u00b7kei\u00b7ten", "heis\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "APPR", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So nur aus Gott allein, ohn' End' und Anfang, stammen.", "tokens": ["So", "nur", "aus", "Gott", "al\u00b7lein", ",", "ohn'", "End'", "und", "An\u00b7fang", ",", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "ADV", "$,", "APPR", "NN", "KON", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es schlug des Abgrunds Raum, wie eine dicke Fluth", "tokens": ["Es", "schlug", "des", "Ab\u00b7grunds", "Raum", ",", "wie", "ei\u00b7ne", "di\u00b7cke", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Des Boden-losen Meers auf sinckend Eisen thut,", "tokens": ["Des", "Bo\u00b7den\u00b7lo\u00b7sen", "Meers", "auf", "sin\u00b7ckend", "Ei\u00b7sen", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In einem Augenblick, auf meinen Geist zusammen.", "tokens": ["In", "ei\u00b7nem", "Au\u00b7gen\u00b7blick", ",", "auf", "mei\u00b7nen", "Geist", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die ungeheure Gruft voll unsichtbaren Lichts,", "tokens": ["Die", "un\u00b7ge\u00b7heu\u00b7re", "Gruft", "voll", "un\u00b7sicht\u00b7ba\u00b7ren", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+"