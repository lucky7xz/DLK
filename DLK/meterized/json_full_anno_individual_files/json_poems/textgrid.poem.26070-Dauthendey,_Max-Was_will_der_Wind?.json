{"textgrid.poem.26070": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Was will der Wind?", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief aus der Nacht, die nirgends endet,", "tokens": ["Tief", "aus", "der", "Nacht", ",", "die", "nir\u00b7gends", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sieht eine Kerze neben mir in mein Gesicht,", "tokens": ["Sieht", "ei\u00b7ne", "Ker\u00b7ze", "ne\u00b7ben", "mir", "in", "mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ihren Schein wie eine Glorie lautlos spendet,", "tokens": ["Die", "ih\u00b7ren", "Schein", "wie", "ei\u00b7ne", "Glo\u00b7rie", "laut\u00b7los", "spen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KOKOM", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und lebt als heller Geist vor meinem Augenlicht.", "tokens": ["Und", "lebt", "als", "hel\u00b7ler", "Geist", "vor", "mei\u00b7nem", "Au\u00b7gen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Wind kreist um das Haus, das er bespricht,", "tokens": ["Der", "Wind", "kreist", "um", "das", "Haus", ",", "das", "er", "be\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie einer, der Beschw\u00f6rung wei\u00df und Bann.", "tokens": ["Wie", "ei\u00b7ner", ",", "der", "Be\u00b7schw\u00f6\u00b7rung", "wei\u00df", "und", "Bann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "ART", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was will der Wind? Was will denn ich und was das Licht?", "tokens": ["Was", "will", "der", "Wind", "?", "Was", "will", "denn", "ich", "und", "was", "das", "Licht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$.", "PWS", "VMFIN", "KON", "PPER", "KON", "PWS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo wohnt der Geist, der einst uns drei ersann?", "tokens": ["Wo", "wohnt", "der", "Geist", ",", "der", "einst", "uns", "drei", "er\u00b7sann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "CARD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So fragt die Stirn voll Wissenslust,", "tokens": ["So", "fragt", "die", "Stirn", "voll", "Wis\u00b7sens\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"te