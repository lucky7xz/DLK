{"dta.poem.4029": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Main .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl manches Land der lebenden Erde m\u00f6cht'", "tokens": ["Wohl", "man\u00b7ches", "Land", "der", "le\u00b7ben\u00b7den", "Er\u00b7de", "m\u00f6cht'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich sehn, und \u00f6fters \u00fcber die Berg' enteilt", "tokens": ["Ich", "sehn", ",", "und", "\u00f6f\u00b7ters", "\u00fc\u00b7ber", "die", "Ber\u00b7g'", "ent\u00b7eilt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KON", "ADV", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Herz mir und die W\u00fcnsche wandern", "tokens": ["Das", "Herz", "mir", "und", "die", "W\u00fcn\u00b7sche", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber das Meer, zu den Ufern, die mir", "tokens": ["Ue\u00b7ber", "das", "Meer", ",", "zu", "den", "U\u00b7fern", ",", "die", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "PRELS", "PPER"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Vor andern, so ich kenne, gepriesen sind,", "tokens": ["Vor", "an\u00b7dern", ",", "so", "ich", "ken\u00b7ne", ",", "ge\u00b7prie\u00b7sen", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "ADV", "PPER", "VVFIN", "$,", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch lieb ist in der Ferne nicht eines mir,", "tokens": ["Doch", "lieb", "ist", "in", "der", "Fer\u00b7ne", "nicht", "ei\u00b7nes", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "ART", "NN", "PTKNEG", "ART", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie jenes, wo die G\u00f6tters\u00f6hne", "tokens": ["Wie", "je\u00b7nes", ",", "wo", "die", "G\u00f6t\u00b7ter\u00b7s\u00f6h\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PDS", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlafen, das trauernde Land der Griechen.", "tokens": ["Schla\u00b7fen", ",", "das", "trau\u00b7ern\u00b7de", "Land", "der", "Grie\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ach! einmal dort an Suniums K\u00fcste m\u00f6cht'", "tokens": ["Ach", "!", "ein\u00b7mal", "dort", "an", "Su\u00b7ni\u00b7ums", "K\u00fcs\u00b7te", "m\u00f6cht'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "ADV", "APPR", "NN", "NN", "VMFIN"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Ich landen, deine S\u00e4ulen, Olympion!", "tokens": ["Ich", "lan\u00b7den", ",", "dei\u00b7ne", "S\u00e4u\u00b7len", ",", "O\u00b7lym\u00b7pi\u00b7on", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPOSAT", "NN", "$,", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erfragen, dort, noch eh der Nordsturm", "tokens": ["Er\u00b7fra\u00b7gen", ",", "dort", ",", "noch", "eh", "der", "Nord\u00b7sturm"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "$,", "ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hin in den Schutt der Athenertempel", "tokens": ["Hin", "in", "den", "Schutt", "der", "A\u00b7the\u00b7ner\u00b7tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Und ihrer G\u00f6tterbilder auch dich begr\u00e4bt;", "tokens": ["Und", "ih\u00b7rer", "G\u00f6t\u00b7ter\u00b7bil\u00b7der", "auch", "dich", "be\u00b7gr\u00e4bt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Denn lang schon einsam stehst du, o Stolz der Welt,", "tokens": ["Denn", "lang", "schon", "ein\u00b7sam", "stehst", "du", ",", "o", "Stolz", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADJD", "VVFIN", "PPER", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die nicht mehr ist! &#8212; und o ihr sch\u00f6nen", "tokens": ["Die", "nicht", "mehr", "ist", "!", "&#8212;", "und", "o", "ihr", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "XML_entity", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADV", "VAFIN", "$.", "$(", "KON", "FM", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Inseln J\u00f6niens, wo die L\u00fcfte", "tokens": ["In\u00b7seln", "J\u00f6\u00b7ni\u00b7ens", ",", "wo", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Vom Meere k\u00fchl, an warme Gestade wehn,", "tokens": ["Vom", "Mee\u00b7re", "k\u00fchl", ",", "an", "war\u00b7me", "Ge\u00b7sta\u00b7de", "wehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn unter kr\u00e4ft'ger Sonne die Traube reift,", "tokens": ["Wenn", "un\u00b7ter", "kr\u00e4ft'\u00b7ger", "Son\u00b7ne", "die", "Trau\u00b7be", "reift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ach! wo ein goldner Herbst dem armen", "tokens": ["Ach", "!", "wo", "ein", "gold\u00b7ner", "Herbst", "dem", "ar\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Volk' in Ges\u00e4nge die Seufzer wandelt,", "tokens": ["Vol\u00b7k'", "in", "Ge\u00b7s\u00e4n\u00b7ge", "die", "Seuf\u00b7zer", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Wenn die Betr\u00fcbten jetzt ihr Limonenwald,", "tokens": ["Wenn", "die", "Be\u00b7tr\u00fcb\u00b7ten", "jetzt", "ihr", "Li\u00b7mo\u00b7nen\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und ihr Granatbaum, purpurner Aepfel voll,", "tokens": ["Und", "ihr", "Gra\u00b7nat\u00b7baum", ",", "pur\u00b7pur\u00b7ner", "A\u00b7e\u00b7pfel", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und s\u00fc\u00dfer Wein und Pauck' und Zithar", "tokens": ["Und", "s\u00fc\u00b7\u00dfer", "Wein", "und", "Pauck'", "und", "Zi\u00b7thar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NE", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zum labyrinthischen Tanze ladet. &#8212;", "tokens": ["Zum", "la\u00b7by\u00b7rin\u00b7thi\u00b7schen", "Tan\u00b7ze", "la\u00b7det", ".", "&#8212;"], "token_info": ["word", "word", "word", "word", "punct", "XML_entity"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Zu euch vielleicht, ihr Inseln! ger\u00e4th noch einst", "tokens": ["Zu", "euch", "viel\u00b7leicht", ",", "ihr", "In\u00b7seln", "!", "ge\u00b7r\u00e4\u00b7th", "noch", "einst"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "$,", "PPOSAT", "NN", "$.", "VVFIN", "ADV", "ADV"], "meter": "---+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein heimathloser S\u00e4nger; denn wandern mu\u00df", "tokens": ["Ein", "hei\u00b7math\u00b7lo\u00b7ser", "S\u00e4n\u00b7ger", ";", "denn", "wan\u00b7dern", "mu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "VVFIN", "VMFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von Fremden er zu Fremden und die", "tokens": ["Von", "Frem\u00b7den", "er", "zu", "Frem\u00b7den", "und", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "APPR", "NN", "KON", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erde, die freie, sie mu\u00df ja leider", "tokens": ["Er\u00b7de", ",", "die", "frei\u00b7e", ",", "sie", "mu\u00df", "ja", "lei\u00b7der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "PPER", "VMFIN", "ADV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Statt ", "tokens": ["Statt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und wenn er stirbt \u2014 doch nimmer verge\u00df ich dich,", "tokens": ["Und", "wenn", "er", "stirbt", "doch", "nim\u00b7mer", "ver\u00b7ge\u00df", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$(", "ADV", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So fern ich wandre, sch\u00f6ner Main! und", "tokens": ["So", "fern", "ich", "wand\u00b7re", ",", "sch\u00f6\u00b7ner", "Main", "!", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,", "ADJA", "NN", "$.", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deine Gestade, die vielbegl\u00fcckten.", "tokens": ["Dei\u00b7ne", "Ge\u00b7sta\u00b7de", ",", "die", "viel\u00b7be\u00b7gl\u00fcck\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PIS", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Gastfreundlich nahmst du, Stolzer! bei dir mich auf", "tokens": ["Gast\u00b7freund\u00b7lich", "nahmst", "du", ",", "Stol\u00b7zer", "!", "bei", "dir", "mich", "auf"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$.", "APPR", "PPER", "PRF", "APPR"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und heitertest das Auge dem Fremdlinge,", "tokens": ["Und", "hei\u00b7ter\u00b7test", "das", "Au\u00b7ge", "dem", "Fremd\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und still hingleitende Ges\u00e4nge", "tokens": ["Und", "still", "hing\u00b7lei\u00b7ten\u00b7de", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lehrtest du mich und ger\u00e4uschlos Leben.", "tokens": ["Lehr\u00b7test", "du", "mich", "und", "ge\u00b7r\u00e4uschlos", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "KON", "ADJD", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "O ruhig mit den Sternen, du Gl\u00fccklicher!", "tokens": ["O", "ru\u00b7hig", "mit", "den", "Ster\u00b7nen", ",", "du", "Gl\u00fcck\u00b7li\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "$,", "PPER", "NN", "$."], "meter": "-+---+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wallst du von deinem Morgen zum Abend fort,", "tokens": ["Wallst", "du", "von", "dei\u00b7nem", "Mor\u00b7gen", "zum", "A\u00b7bend", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Bruder zu, dem Rhein; und dann mit", "tokens": ["Dem", "Bru\u00b7der", "zu", ",", "dem", "Rhein", ";", "und", "dann", "mit"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "NE", "$.", "KON", "ADV", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ihm in den Ocean freudig nieder!", "tokens": ["Ihm", "in", "den", "O\u00b7cean", "freu\u00b7dig", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}