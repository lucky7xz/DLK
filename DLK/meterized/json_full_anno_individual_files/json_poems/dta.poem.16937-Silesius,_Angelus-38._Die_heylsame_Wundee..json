{"dta.poem.16937": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "38.  Die heylsame Wundee.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wunde die mein GOtt f\u00fcr mich ins Hertz em-", "tokens": ["Die", "Wun\u00b7de", "die", "mein", "Gott", "f\u00fcr", "mich", "ins", "Hertz", "em"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPOSAT", "NN", "APPR", "PPER", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "pf\u00e4ngt/", "tokens": ["pf\u00e4ngt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Verursacht/ da\u00df Er mir sein Blut und Wasser schenkt:", "tokens": ["Ver\u00b7ur\u00b7sacht", "/", "da\u00df", "Er", "mir", "sein", "Blut", "und", "Was\u00b7ser", "schenkt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "PPER", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Trink ich mich dessen Voll/ so haben meine Wunden/", "tokens": ["Trink", "ich", "mich", "des\u00b7sen", "Voll", "/", "so", "ha\u00b7ben", "mei\u00b7ne", "Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PDS", "ADJD", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr wahres Balsam\u00f6l/ und besten Heiltrank funden.", "tokens": ["Ih