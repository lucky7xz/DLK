{"dta.poem.21789": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Nacht-last/ Tages-lust.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht", "tokens": ["Die", "Nacht"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "die sonst den Buhlern f\u00fcgt und s\u00fcsse Hoffnung mach", "tokens": ["die", "sonst", "den", "Buh\u00b7lern", "f\u00fcgt", "und", "s\u00fcs\u00b7se", "Hoff\u00b7nung", "mach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die einem Liebendem sagt alle Wollust zu/", "tokens": ["die", "ei\u00b7nem", "Lie\u00b7ben\u00b7dem", "sagt", "al\u00b7le", "Wol\u00b7lust", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "PIAT", "NN", "PTKZU", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "bringt mir nur lauter Schmerzen", "tokens": ["bringt", "mir", "nur", "lau\u00b7ter", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "und raubet mir das Licht/", "tokens": ["und", "rau\u00b7bet", "mir", "das", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "das meinem tr\u00fcben Herzen", "tokens": ["das", "mei\u00b7nem", "tr\u00fc\u00b7ben", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "des Trostes Straal verspricht.", "tokens": ["des", "Tros\u00b7tes", "Straal", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Tag", "tokens": ["Der", "Tag"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "dem sonst kein Pafos-kind recht g\u00fcnstig werden mag", "tokens": ["dem", "sonst", "kein", "Pa\u00b7fos\u00b7kind", "recht", "g\u00fcns\u00b7tig", "wer\u00b7den", "mag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN", "ADV", "ADJD", "VAINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der g\u00f6ldnen Strahlen/ die der Venus schaden tuht.", "tokens": ["der", "g\u00f6ld\u00b7nen", "Strah\u00b7len", "/", "die", "der", "Ve\u00b7nus", "scha\u00b7den", "tuht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erteilt mir lauter Freuden", "tokens": ["Er\u00b7teilt", "mir", "lau\u00b7ter", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "und g\u00f6nnet mir das Gl\u00fckk", "tokens": ["und", "g\u00f6n\u00b7net", "mir", "das", "Gl\u00fckk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "die Augen satt zu weiden", "tokens": ["die", "Au\u00b7gen", "satt", "zu", "wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "in meiner Liebsten Blikk.", "tokens": ["in", "mei\u00b7ner", "Liebs\u00b7ten", "Blikk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn iezt", "tokens": ["Wenn", "iezt"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Apollens Feuer-gold der Berge Haubt erhizzt.", "tokens": ["A\u00b7pol\u00b7lens", "Feu\u00b7er\u00b7gold", "der", "Ber\u00b7ge", "Haubt", "er\u00b7hizzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "die auffgewekkte Welt entsaget ihrer Ruh:", "tokens": ["die", "auff\u00b7ge\u00b7wekk\u00b7te", "Welt", "ent\u00b7sa\u00b7get", "ih\u00b7rer", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "r\u00fchrt mich Rosillen Wange", "tokens": ["r\u00fchrt", "mich", "Ro\u00b7sil\u00b7len", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "mit einem feuchten Ku\u00df\u2019", "tokens": ["mit", "ei\u00b7nem", "feuch\u00b7ten", "Ku\u00df'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "und dieses w\u00e4hrt so lange", "tokens": ["und", "die\u00b7ses", "w\u00e4hrt", "so", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "bi\u00df auff den Hesperus.", "tokens": ["bi\u00df", "auff", "den", "Hes\u00b7pe\u00b7rus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "So bald", "tokens": ["So", "bald"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "der Sonnen Kerze wird in Thetis Schosse kalt", "tokens": ["der", "Son\u00b7nen", "Ker\u00b7ze", "wird", "in", "The\u00b7tis", "Schos\u00b7se", "kalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "NE", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in d\u00fcstrer Wolken-Lufft f\u00fchrt auff den bleichen Mohn", "tokens": ["in", "d\u00fcst\u00b7rer", "Wol\u00b7ken\u00b7Lufft", "f\u00fchrt", "auff", "den", "blei\u00b7chen", "Mohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "denn wird mir erst die Nacht", "tokens": ["denn", "wird", "mir", "erst", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "das Kind der Erebinnen", "tokens": ["das", "Kind", "der", "E\u00b7re\u00b7bin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "zur rechten Nacht gemacht.", "tokens": ["zur", "rech\u00b7ten", "Nacht", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum geh", "tokens": ["Drum", "geh"], "token_info": ["word", "word"], "pos": ["PAV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "verha\u00dftes Sternen-Heer gleich nim\u0303er au\u00df der See.", "tokens": ["ver\u00b7ha\u00df\u00b7tes", "Ster\u00b7nen\u00b7Heer", "gleich", "nim\u0303er", "au\u00df", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "geliebter Lucifer tritt auff Olympens Bahn.", "tokens": ["ge\u00b7lieb\u00b7ter", "Lu\u00b7ci\u00b7fer", "tritt", "auff", "O\u00b7lym\u00b7pens", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Tag der mich so liebet/", "tokens": ["Der", "Tag", "der", "mich", "so", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Nacht/ die mich betr\u00fcbet/", "tokens": ["Die", "Nacht", "/", "die", "mich", "be\u00b7tr\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "weich\u2019 in die H\u00f6ll\u2019 hin ein.", "tokens": ["weich'", "in", "die", "H\u00f6ll'", "hin", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}