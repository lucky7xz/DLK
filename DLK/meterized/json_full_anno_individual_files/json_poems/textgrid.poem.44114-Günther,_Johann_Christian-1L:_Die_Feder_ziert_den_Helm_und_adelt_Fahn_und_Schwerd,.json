{"textgrid.poem.44114": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Feder ziert den Helm und adelt Fahn und Schwerd,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Feder ziert den Helm und adelt Fahn und Schwerd,", "tokens": ["Die", "Fe\u00b7der", "ziert", "den", "Helm", "und", "a\u00b7delt", "Fahn", "und", "Schwerd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Blut fleckt nur den Schild, der keine Dinte mahlet.", "tokens": ["Das", "Blut", "fleckt", "nur", "den", "Schild", ",", "der", "kei\u00b7ne", "Din\u00b7te", "mah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was hilft es, da\u00df der Mars mit dem Triumphe prahlet,", "tokens": ["Was", "hilft", "es", ",", "da\u00df", "der", "Mars", "mit", "dem", "Tri\u00b7um\u00b7phe", "prah\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ihm der Phoebus nicht den Lorbeerkranz gew\u00e4hrt.", "tokens": ["Wenn", "ihm", "der", "Phoe\u00b7bus", "nicht", "den", "Lor\u00b7beer\u00b7kranz", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word",