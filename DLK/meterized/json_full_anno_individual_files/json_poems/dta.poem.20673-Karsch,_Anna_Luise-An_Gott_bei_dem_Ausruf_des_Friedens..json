{"dta.poem.20673": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Gott  \n bei  \n  dem Ausruf des Friedens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was h\u00f6r ich? rauschen goldne Fl\u00fcgel?                 ", "tokens": ["Was", "h\u00f6r", "ich", "?", "rau\u00b7schen", "gold\u00b7ne", "Fl\u00fc\u00b7gel", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Posaunet in zertheilter Luft", "tokens": ["Po\u00b7sau\u00b7net", "in", "zer\u00b7theil\u00b7ter", "Luft"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Seraph, welcher \u00fcber alle Grabesh\u00fcgel", "tokens": ["Ein", "Se\u00b7raph", ",", "wel\u00b7cher", "\u00fc\u00b7ber", "al\u00b7le", "Gra\u00b7bes\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Daher f\u00e4hrt, und die Todten ruft?", "tokens": ["Da\u00b7her", "f\u00e4hrt", ",", "und", "die", "Tod\u00b7ten", "ruft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was reisset mich empor? ich f\u00fchle", "tokens": ["Was", "reis\u00b7set", "mich", "em\u00b7por", "?", "ich", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den nahen Himmel; bin ich schon", "tokens": ["Den", "na\u00b7hen", "Him\u00b7mel", ";", "bin", "ich", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hoch \u00fcber der Geb\u00fcrge Gipfel, \u00fcber St\u00fchle", "tokens": ["Hoch", "\u00fc\u00b7ber", "der", "Ge\u00b7b\u00fcr\u00b7ge", "Gip\u00b7fel", ",", "\u00fc\u00b7ber", "St\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Zepterf\u00fchrer weggeflohn?", "tokens": ["Der", "Zep\u00b7ter\u00b7f\u00fch\u00b7rer", "weg\u00b7ge\u00b7flohn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6r ich, Du Gott der Erdeng\u00f6tter", "tokens": ["H\u00f6r", "ich", ",", "Du", "Gott", "der", "Er\u00b7den\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "$,", "PPER", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich loben durch den ganzen Raum", "tokens": ["Dich", "lo\u00b7ben", "durch", "den", "gan\u00b7zen", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der neuen Sch\u00f6pfung, selbst von Deines Glanzes", "tokens": ["Der", "neu\u00b7en", "Sch\u00f6p\u00b7fung", ",", "selbst", "von", "Dei\u00b7nes", "Glan\u00b7zes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sp\u00f6tter,", "tokens": ["Sp\u00f6t\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der Deine Wunder nannte Traum?", "tokens": ["Der", "Dei\u00b7ne", "Wun\u00b7der", "nann\u00b7te", "Traum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Erblick ich Myriaden Sterne", "tokens": ["Er\u00b7blick", "ich", "My\u00b7ria\u00b7den", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Um Deines Sonnesthrones Fu\u00df?", "tokens": ["Um", "Dei\u00b7nes", "Son\u00b7nest\u00b7hro\u00b7nes", "Fu\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hellleuchtend, da\u00df davor ich zitternd in der Ferne", "tokens": ["Hell\u00b7leuch\u00b7tend", ",", "da\u00df", "da\u00b7vor", "ich", "zit\u00b7ternd", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PAV", "PPER", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Angesicht bedecken mu\u00df?", "tokens": ["Mein", "An\u00b7ge\u00b7sicht", "be\u00b7de\u00b7cken", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Horch ich erstaunt dem hohen Liede,", "tokens": ["Horch", "ich", "er\u00b7staunt", "dem", "ho\u00b7hen", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der S\u00e4nger Deines Namens zu?", "tokens": ["Der", "S\u00e4n\u00b7ger", "Dei\u00b7nes", "Na\u00b7mens", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott, welch ein Saytenspiel! es t\u00f6net ", "tokens": ["Gott", ",", "welch", "ein", "Say\u00b7ten\u00b7spiel", "!", "es", "t\u00f6\u00b7net"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PWAT", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Friede!                 ", "tokens": ["Frie\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und Kronengeber, den giebst Du!!", "tokens": ["Und", "Kro\u00b7nen\u00b7ge\u00b7ber", ",", "den", "giebst", "Du", "!!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du l\u00e4ssest Deinem Volke wieder                 ", "tokens": ["Du", "l\u00e4s\u00b7sest", "Dei\u00b7nem", "Vol\u00b7ke", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ruhe schmecken, rufest laut", "tokens": ["Die", "Ru\u00b7he", "schme\u00b7cken", ",", "ru\u00b7fest", "laut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns aus dem Schmerzensschlaf zum Jubel neuer Lieder", "tokens": ["Uns", "aus", "dem", "Schmer\u00b7zens\u00b7schlaf", "zum", "Ju\u00b7bel", "neu\u00b7er", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bei den Alt\u00e4ren, Dir gebaut.", "tokens": ["Bei", "den", "Al\u00b7t\u00e4\u00b7ren", ",", "Dir", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wir lagen, gleich den Blumenstengeln,", "tokens": ["Wir", "la\u00b7gen", ",", "gleich", "den", "Blu\u00b7men\u00b7sten\u00b7geln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie der Nordost niederbeugt;", "tokens": ["Wenn", "sie", "der", "Nord\u00b7ost", "nie\u00b7der\u00b7beugt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hebst uns auf, und h\u00f6rst dein Lob von allen Engeln,                 ", "tokens": ["Du", "hebst", "uns", "auf", ",", "und", "h\u00f6rst", "dein", "Lob", "von", "al\u00b7len", "En\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn unsre stumme Freude schweigt.", "tokens": ["Wenn", "uns\u00b7re", "stum\u00b7me", "Freu\u00b7de", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}