{"textgrid.poem.57875": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die Rhododendrenriede", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt wird die Eilenriede sch\u00f6n,", "tokens": ["Jetzt", "wird", "die", "Ei\u00b7len\u00b7rie\u00b7de", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trotz winterlicher Repressalien,", "tokens": ["Trotz", "win\u00b7ter\u00b7li\u00b7cher", "Re\u00b7pres\u00b7sa\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die Buchen und Eichen schlagen aus,", "tokens": ["Die", "Bu\u00b7chen", "und", "Ei\u00b7chen", "schla\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auch Rhododendren und Azalien.", "tokens": ["Auch", "Rho\u00b7do\u00b7den\u00b7dren", "und", "A\u00b7za\u00b7li\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NE", "$."], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Menschheit wird ganz aufgeregt,", "tokens": ["Die", "Menschheit", "wird", "ganz", "auf\u00b7ge\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie fr\u00fcher bei den Bacchanalien,", "tokens": ["Wie", "fr\u00fc\u00b7her", "bei", "den", "Bac\u00b7