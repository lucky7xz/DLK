{"textgrid.poem.40803": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbihr, denen in die H\u00e4nde ward gegeben,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbihr, denen in die H\u00e4nde ward gegeben,", "tokens": ["\u00bb", "ihr", ",", "de\u00b7nen", "in", "die", "H\u00e4n\u00b7de", "ward", "ge\u00b7ge\u00b7ben", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sich's die H\u00e4nd' etwa nicht selbst genommen,", "tokens": ["Wenn", "sich's", "die", "H\u00e4nd'", "et\u00b7wa", "nicht", "selbst", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Recht, zu schalten \u00fcber Menschenleben,", "tokens": ["Das", "Recht", ",", "zu", "schal\u00b7ten", "\u00fc\u00b7ber", "Men\u00b7schen\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKZU", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kennt ihr des Menschenlebens Sinn und Frommen?", "tokens": ["Kennt", "ihr", "des", "Men\u00b7schen\u00b7le\u00b7bens", "Sinn", "und", "From\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich rath' euch, wallt aus eurer goldnen Klause", "tokens": ["Ich", "ra\u00b7th'", "euch", ",", "wallt", "aus", "eu\u00b7rer", "gold\u00b7nen", "Klau\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Einmal hinaus in Fr\u00fchlings Sonnenblicke,", "tokens": ["Ein\u00b7mal", "hin\u00b7aus", "in", "Fr\u00fch\u00b7lings", "Son\u00b7nen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APZR", "APPR", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Doch la\u00dft mir fein den Doctorhut zu Hause,", "tokens": ["Doch", "la\u00dft", "mir", "fein", "den", "Doc\u00b7tor\u00b7hut", "zu", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die gr\u00fcne Brille, Codex und Perr\u00fccke!", "tokens": ["Die", "gr\u00fc\u00b7ne", "Bril\u00b7le", ",", "Co\u00b7dex", "und", "Per\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-++-++-", "measure": "unknown.measure.hexa"}}, "stanza.3": {"line.1": {"text": "Und wenn, von all dem Licht und Glanz entborget,", "tokens": ["Und", "wenn", ",", "von", "all", "dem", "Licht", "und", "Glanz", "ent\u00b7bor\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "APPR", "PIAT", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein leiser Abglanz schlich in eure Seele,", "tokens": ["Ein", "lei\u00b7ser", "Ab\u00b7glanz", "schlich", "in", "eu\u00b7re", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann ist es Zeit, dann weilet nicht, und sorget,", "tokens": ["Dann", "ist", "es", "Zeit", ",", "dann", "wei\u00b7let", "nicht", ",", "und", "sor\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PTKNEG", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Flinte, Beil und Messer euch nicht fehle.", "tokens": ["Da\u00df", "Flin\u00b7te", ",", "Beil", "und", "Mes\u00b7ser", "euch", "nicht", "feh\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Seht dort den Rosenstrauch im Duftmeer fluthen!", "tokens": ["Seht", "dort", "den", "Ro\u00b7sen\u00b7strauch", "im", "Duft\u00b7meer", "flut\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Messer her, vom Stamme ihn zu trennen!", "tokens": ["Das", "Mes\u00b7ser", "her", ",", "vom", "Stam\u00b7me", "ihn", "zu", "tren\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er liegt im Staub und scheint nun zu verbluten", "tokens": ["Er", "liegt", "im", "Staub", "und", "scheint", "nun", "zu", "ver\u00b7blu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus so viel Wunden, als da Knospen brennen.", "tokens": ["Aus", "so", "viel", "Wun\u00b7den", ",", "als", "da", "Knos\u00b7pen", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$,", "KOUS", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Seht ihr die Lerche hoch im Fr\u00fchroth schimmern?", "tokens": ["Seht", "ihr", "die", "Ler\u00b7che", "hoch", "im", "Fr\u00fch\u00b7roth", "schim\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Feuerrohr herbei, und streckt sie nieder!", "tokens": ["Das", "Feu\u00b7er\u00b7rohr", "her\u00b7bei", ",", "und", "streckt", "sie", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor euch im Rasengr\u00fcn mit leisem Wimmern", "tokens": ["Vor", "euch", "im", "Ra\u00b7sen\u00b7gr\u00fcn", "mit", "lei\u00b7sem", "Wim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Versiegt die holde Quelle s\u00fc\u00dfer Lieder.", "tokens": ["Ver\u00b7siegt", "die", "hol\u00b7de", "Quel\u00b7le", "s\u00fc\u00b7\u00dfer", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Seht dort der Linde Haupt die Wolken gr\u00fc\u00dfen!", "tokens": ["Seht", "dort", "der", "Lin\u00b7de", "Haupt", "die", "Wol\u00b7ken", "gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NE", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Axt herbei, den Stamm ihr zu zerkl\u00fcften!", "tokens": ["Die", "Axt", "her\u00b7bei", ",", "den", "Stamm", "ihr", "zu", "zer\u00b7kl\u00fcf\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da liegt die Riesenleiche euch zu F\u00fc\u00dfen,", "tokens": ["Da", "liegt", "die", "Rie\u00b7sen\u00b7lei\u00b7che", "euch", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Sterber\u00f6cheln ist ein s\u00fc\u00dfes D\u00fcften.", "tokens": ["Ihr", "Ster\u00b7be\u00b7r\u00f6\u00b7cheln", "ist", "ein", "s\u00fc\u00b7\u00dfes", "D\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und will euch Wehmut nun ins Herz, so lenket", "tokens": ["Und", "will", "euch", "Weh\u00b7mut", "nun", "ins", "Herz", ",", "so", "len\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "NN", "ADV", "APPRART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Heimw\u00e4rts den Pfad, und nehmt an eurer Schwelle", "tokens": ["Heim\u00b7w\u00e4rts", "den", "Pfad", ",", "und", "nehmt", "an", "eu\u00b7rer", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Den S\u00e4ugling aus der Gattin Arm, und senket", "tokens": ["Den", "S\u00e4ug\u00b7ling", "aus", "der", "Gat\u00b7tin", "Arm", ",", "und", "sen\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eu'r sinnend Haupt zu seiner Lockenhelle.", "tokens": ["Eu'r", "sin\u00b7nend", "Haupt", "zu", "sei\u00b7ner", "Lo\u00b7cken\u00b7hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und denkt des Baums, zerspellt zu todten Tr\u00fcmmern,", "tokens": ["Und", "denkt", "des", "Baums", ",", "zer\u00b7spellt", "zu", "tod\u00b7ten", "Tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und denkt der Knosp', erbla\u00dft im Todesbeben,", "tokens": ["Und", "denkt", "der", "Knosp'", ",", "er\u00b7bla\u00dft", "im", "To\u00b7des\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und denkt des Liedes, aufgel\u00f6s't in Wimmern,", "tokens": ["Und", "denkt", "des", "Lie\u00b7des", ",", "auf\u00b7ge\u00b7l\u00f6s't", "in", "Wim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ahnt es leise, was ein Menschenleben!\u00ab", "tokens": ["Und", "ahnt", "es", "lei\u00b7se", ",", "was", "ein", "Men\u00b7schen\u00b7le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "PRELS", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "\u00bbihr, denen in die H\u00e4nde ward gegeben,", "tokens": ["\u00bb", "ihr", ",", "de\u00b7nen", "in", "die", "H\u00e4n\u00b7de", "ward", "ge\u00b7ge\u00b7ben", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sich's die H\u00e4nd' etwa nicht selbst genommen,", "tokens": ["Wenn", "sich's", "die", "H\u00e4nd'", "et\u00b7wa", "nicht", "selbst", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Re