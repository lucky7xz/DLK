 ["Und", "noch", "lan\u00b7ge", "h\u00f6r\u00b7te", "den", "Ruf", "der", "Hei\u00b7mat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bliebe davon ein Hauch in meinen Liedern!", "tokens": ["Blie\u00b7be", "da\u00b7von", "ein", "Hauch", "in", "mei\u00b7nen", "Lie\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Schlaf, melodischer Freund, woher die Fl\u00f6te?", "tokens": ["Schlaf", ",", "me\u00b7lo\u00b7di\u00b7scher", "Freund", ",", "wo\u00b7her", "die", "Fl\u00f6\u00b7te", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ist sie ein Ast des Walds, durchhaucht vom Gotte,", "tokens": ["Ist", "sie", "ein", "Ast", "des", "Walds", ",", "durch\u00b7haucht", "vom", "Got\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6rt ich im Traum des heiligen Pan Syringe?", "tokens": ["H\u00f6rt", "ich", "im", "Traum", "des", "hei\u00b7li\u00b7gen", "Pan", "Sy\u00b7rin\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}