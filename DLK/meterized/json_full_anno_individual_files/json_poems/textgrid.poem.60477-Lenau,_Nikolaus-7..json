{"textgrid.poem.60477": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schl\u00e4frig hangen die sonnenm\u00fcden Bl\u00e4tter,", "tokens": ["Schl\u00e4f\u00b7rig", "han\u00b7gen", "die", "son\u00b7nen\u00b7m\u00fc\u00b7den", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Alles schweigt im Walde, nur eine Biene", "tokens": ["Al\u00b7les", "schweigt", "im", "Wal\u00b7de", ",", "nur", "ei\u00b7ne", "Bie\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Summt dort an der Bl\u00fcte mit mattem Eifer;", "tokens": ["Summt", "dort", "an", "der", "Bl\u00fc\u00b7te", "mit", "mat\u00b7tem", "Ei\u00b7fer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sie auch lie\u00df vom sommerlichen Get\u00f6ne,", "tokens": ["Sie", "auch", "lie\u00df", "vom", "som\u00b7mer\u00b7li\u00b7chen", "Ge\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Eingeschlafen vielleicht im Scho\u00df der Blume.", "tokens": ["Ein\u00b7ge\u00b7schla\u00b7fen", "viel\u00b7leicht", "im", "Scho\u00df", "der", "Blu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Hier, noch Fr\u00fchlings, rauschte die muntre Quelle;", "tokens": ["Hier", ",", "noch", "Fr\u00fch\u00b7lings", ",", "rauschte", "die", "mun\u00b7tre", "Quel\u00b7le", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Still versiegend ist in die Luft zergangen", "tokens": ["Still", "ver\u00b7sie\u00b7gend", "ist", "in", "die", "Luft", "zer\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VAFIN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.8": {"text": "All ihr frisches Geplauder, helles Schimmern.", "tokens": ["All", "ihr", "fri\u00b7sches", "Ge\u00b7plau\u00b7der", ",", "hel\u00b7les", "Schim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Traurig kahlt die St\u00e4tte, wo einst ein Quell flo\u00df;", "tokens": ["Trau\u00b7rig", "kahlt", "die", "St\u00e4t\u00b7te", ",", "wo", "einst", "ein", "Quell", "flo\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.10": {"text": "Horchen mu\u00df ich noch dem gewohnten Rauschen,", "tokens": ["Hor\u00b7chen", "mu\u00df", "ich", "noch", "dem", "ge\u00b7wohn\u00b7ten", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Ich vermisse den Bach, wie liebe Gr\u00fc\u00dfe,", "tokens": ["Ich", "ver\u00b7mis\u00b7se", "den", "Bach", ",", "wie", "lie\u00b7be", "Gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Die sonst fernher kamen, nun ausgeblieben.", "tokens": ["Die", "sonst", "fern\u00b7her", "ka\u00b7men", ",", "nun", "aus\u00b7ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "-++-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Alles still, einschl\u00e4fernd, des dichten Mooses", "tokens": ["Al\u00b7les", "still", ",", "ein\u00b7schl\u00e4\u00b7fernd", ",", "des", "dich\u00b7ten", "Moo\u00b7ses"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIS", "PTKVZ", "$,", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.14": {"text": "Sanft nachgiebige Schwellung ist so ruhlich;", "tokens": ["Sanft", "nach\u00b7gie\u00b7bi\u00b7ge", "Schwel\u00b7lung", "ist", "so", "ruh\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "M\u00f6ge hier mich holder Schlummer beschleichen,", "tokens": ["M\u00f6\u00b7ge", "hier", "mich", "hol\u00b7der", "Schlum\u00b7mer", "be\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Mir die Schl\u00fcssel zu meinen Sch\u00e4tzen stehlen", "tokens": ["Mir", "die", "Schl\u00fcs\u00b7sel", "zu", "mei\u00b7nen", "Sch\u00e4t\u00b7zen", "steh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und die Waffen entwenden meines Zornes,", "tokens": ["Und", "die", "Waf\u00b7fen", "ent\u00b7wen\u00b7den", "mei\u00b7nes", "Zor\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Da\u00df die Seele, rings nach au\u00dfen vergessend,", "tokens": ["Da\u00df", "die", "See\u00b7le", ",", "rings", "nach", "au\u00b7\u00dfen", "ver\u00b7ges\u00b7send", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "APPR", "ADV", "VVPP", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Sich in ihre Tiefen hinein erinnre.", "tokens": ["Sich", "in", "ih\u00b7re", "Tie\u00b7fen", "hin\u00b7ein", "e\u00b7rinn\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "APZR", "VVFIN", "$."], "meter": "--+-+--++--", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Preisen will ich den Schlummer, bis er leise", "tokens": ["Prei\u00b7sen", "will", "ich", "den", "Schlum\u00b7mer", ",", "bis", "er", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "ADJD"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.21": {"text": "Naht in diesem Dunkel und mir das Aug schlie\u00dft.", "tokens": ["Naht", "in", "die\u00b7sem", "Dun\u00b7kel", "und", "mir", "das", "Aug", "schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.22": {"text": "Schlaf, du kindlicher Gott, du Gott der Kindheit!", "tokens": ["Schlaf", ",", "du", "kind\u00b7li\u00b7cher", "Gott", ",", "du", "Gott", "der", "Kind\u00b7heit", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Du Verj\u00fcnger der Welt, die, dein entbehrend,", "tokens": ["Du", "Ver\u00b7j\u00fcn\u00b7ger", "der", "Welt", ",", "die", ",", "dein", "ent\u00b7beh\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "PRELS", "$,", "PPOSAT", "VVPP", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Rasch in wenig Stunden w\u00e4re gealtert.", "tokens": ["Rasch", "in", "we\u00b7nig", "Stun\u00b7den", "w\u00e4\u00b7re", "ge\u00b7al\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Wundert\u00e4tiger Freund, Erl\u00f6ser des Herzens!", "tokens": ["Wun\u00b7der\u00b7t\u00e4\u00b7ti\u00b7ger", "Freund", ",", "Er\u00b7l\u00f6\u00b7ser", "des", "Her\u00b7zens", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Rings umstellt und bewacht am hellen Tage", "tokens": ["Rings", "um\u00b7stellt", "und", "be\u00b7wacht", "am", "hel\u00b7len", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "VVPP", "APPRART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.27": {"text": "Ist das Herz in der Brust und unzug\u00e4nglich", "tokens": ["Ist", "das", "Herz", "in", "der", "Brust", "und", "un\u00b7zu\u00b7g\u00e4ng\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "KON", "ADJD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "F\u00fcr die leiseren Genien des Lebens,", "tokens": ["F\u00fcr", "die", "lei\u00b7se\u00b7ren", "Ge\u00b7ni\u00b7en", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Denn ihm wandeln voran auf allen Wegen", "tokens": ["Denn", "ihm", "wan\u00b7deln", "vo\u00b7ran", "auf", "al\u00b7len", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "APPR", "PIAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.30": {"text": "Die Gedanken, bewaffnet, als Liktoren,", "tokens": ["Die", "Ge\u00b7dan\u00b7ken", ",", "be\u00b7waff\u00b7net", ",", "als", "Lik\u00b7to\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "KOUS", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.31": {"text": "Schreckend und verscheuchend lieblichen Zauber.", "tokens": ["Schre\u00b7ckend", "und", "ver\u00b7scheu\u00b7chend", "lieb\u00b7li\u00b7chen", "Zau\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "Aber in der Stille der Nacht, des Schlummers,", "tokens": ["A\u00b7ber", "in", "der", "Stil\u00b7le", "der", "Nacht", ",", "des", "Schlum\u00b7mers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.33": {"text": "Wacht die Seele heimlich und lauscht wie Hero,", "tokens": ["Wacht", "die", "See\u00b7le", "heim\u00b7lich", "und", "lauscht", "wie", "He\u00b7ro", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "KON", "VVFIN", "KOKOM", "NE", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "Bis verborgen ihr Gott ihr naht, her\u00fcber", "tokens": ["Bis", "ver\u00b7bor\u00b7gen", "ihr", "Gott", "ihr", "naht", ",", "her\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "VVPP", "PPOSAT", "NN", "PPER", "VVFIN", "$,", "ADV"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.35": {"text": "Schwimmend durch das wallende Meer der Tr\u00e4ume.", "tokens": ["Schwim\u00b7mend", "durch", "das", "wal\u00b7len\u00b7de", "Meer", "der", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.2": {"line.1": {"text": "Eine Fl\u00f6te klang mir im Schlaf zuweilen,", "tokens": ["Ei\u00b7ne", "Fl\u00f6\u00b7te", "klang", "mir", "im", "Schlaf", "zu\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wie ein Gesang der Urwelt, Sehnsucht weckend,", "tokens": ["Wie", "ein", "Ge\u00b7sang", "der", "Ur\u00b7welt", ",", "Sehn\u00b7sucht", "we\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich s\u00fc\u00df ersch\u00fcttert erwacht' in Tr\u00e4nen", "tokens": ["Da\u00df", "ich", "s\u00fc\u00df", "er\u00b7sch\u00fct\u00b7tert", "er\u00b7wacht'", "in", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "VVFIN", "APPR", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und noch lange h\u00f6rte den Ruf der Heimat;", "tokens": ["Und", "noch", "lan\u00b7ge", "h\u00f6r\u00b7te", "den", "Ruf", "der", "Hei\u00b7mat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bliebe davon ein Hauch in meinen Liedern!", "tokens": ["Blie\u00b7be", "da\u00b7von", "ein", "Hauch", "in", "mei\u00b7nen", "Lie\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Schlaf, melodischer Freund, woher die Fl\u00f6te?", "tokens": ["Schlaf", ",", "me\u00b7lo\u00b7di\u00b7scher", "Freund", ",", "wo\u00b7her", "die", "Fl\u00f6\u00b7te", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ist sie ein Ast des Walds, durchhaucht vom Gotte,", "tokens": ["Ist", "sie", "ein", "Ast", "des", "Walds", ",", "durch\u00b7haucht", "vom", "Got\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6rt ich im Traum des heiligen Pan Syringe?", "tokens": ["H\u00f6rt", "ich", "im", "Traum", "des", "hei\u00b7li\u00b7gen", "Pan", "Sy\u00b7rin\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Schl\u00e4frig hangen die sonnenm\u00fcden Bl\u00e4tter,", "tokens": ["Schl\u00e4f\u00b7rig", "han\u00b7gen", "die", "son\u00b7nen\u00b7m\u00fc\u00b7den", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Alles schweigt im Walde, nur eine Biene", "tokens": ["Al\u00b7les", "schweigt", "im", "Wal\u00b7de", ",", "nur", "ei\u00b7ne", "Bie\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Summt dort an der Bl\u00fcte mit mattem Eifer;", "tokens": ["Summt", "dort", "an", "der", "Bl\u00fc\u00b7t