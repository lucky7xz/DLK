oken_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schaue nur das Spiel der Erden", "tokens": ["Schau\u00b7e", "nur", "das", "Spiel", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit geheilten Augen an.", "tokens": ["Mit", "ge\u00b7heil\u00b7ten", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was wird endlich dieses werden,", "tokens": ["Was", "wird", "end\u00b7lich", "die\u00b7ses", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PDAT", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das uns so beth\u00f6ren kann?", "tokens": ["Das", "uns", "so", "be\u00b7th\u00f6\u00b7ren", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Der aus Nichts gemachte Schein", "tokens": ["Der", "aus", "Nichts", "ge\u00b7mach\u00b7te", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIS", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird in Nichts verkehret sein.", "tokens": ["Wird", "in", "Nichts", "ver\u00b7keh\u00b7ret", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df den Purpur aus den H\u00e4nden,", "tokens": ["La\u00df", "den", "Pur\u00b7pur", "aus", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den dein Irrthum scheinbar macht;", "tokens": ["Den", "dein", "Irr\u00b7thum", "schein\u00b7bar", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df kein falsches Licht dich blenden;", "tokens": ["La\u00df", "kein", "fal\u00b7sches", "Licht", "dich", "blen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meide jener Blumen Pracht,", "tokens": ["Mei\u00b7de", "je\u00b7ner", "Blu\u00b7men", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die der Garten in sich hegt,", "tokens": ["Die", "der", "Gar\u00b7ten", "in", "sich", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Der f\u00fcr Fr\u00fcchte Dornen tr\u00e4gt.", "tokens": ["Der", "f\u00fcr", "Fr\u00fcch\u00b7te", "Dor\u00b7nen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lerne zeitig dieses hassen,", "tokens": ["Ler\u00b7ne", "zei\u00b7tig", "die\u00b7ses", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was du ewig hassen mu\u00dft!", "tokens": ["Was", "du", "e\u00b7wig", "has\u00b7sen", "mu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kannst du denn die Welt nicht lassen? \u2013", "tokens": ["Kannst", "du", "denn", "die", "Welt", "nicht", "las\u00b7sen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, was ist doch ihre Lust?", "tokens": ["Ach", ",", "was", "ist", "doch", "ih\u00b7re", "Lust", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute Gras und morgen Heu,", "tokens": ["Heu\u00b7te", "Gras", "und", "mor\u00b7gen", "Heu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Heute Blumen, morgen Spreu!", "tokens": ["Heu\u00b7te", "Blu\u00b7men", ",", "mor\u00b7gen", "Spreu", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Aegypten unsrer Herzen,", "tokens": ["Das", "A\u00b7e\u00b7gyp\u00b7ten", "uns\u00b7rer", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das jetzt Ehr' und Lust verspricht,", "tokens": ["Das", "jetzt", "Ehr'", "und", "Lust", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Macht uns morgen Angst und Schmerzen,", "tokens": ["Macht", "uns", "mor\u00b7gen", "Angst", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aendert sich und kennt uns nicht.", "tokens": ["A\u00b7en\u00b7dert", "sich", "und", "kennt", "uns", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Suche nun ein fester Land;", "tokens": ["Su\u00b7che", "nun", "ein", "fes\u00b7ter", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn hier wohnt nur Unbestand.", "tokens": ["Denn", "hier", "wohnt", "nur", "Un\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf, o Seele, zu den Sternen,", "tokens": ["Auf", ",", "o", "See\u00b7le", ",", "zu", "den", "Ster\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "FM", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der Sonne wahrer Ruh'!", "tokens": ["Zu", "der", "Son\u00b7ne", "wah\u00b7rer", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schau ges\u00e4ubert dort von fernen", "tokens": ["Schau", "ge\u00b7s\u00e4u\u00b7bert", "dort", "von", "fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Welt Gebrechen zu,", "tokens": ["Die\u00b7ser", "Welt", "Ge\u00b7bre\u00b7chen", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die in ihren Banden lacht,", "tokens": ["Die", "in", "ih\u00b7ren", "Ban\u00b7den", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf ihr Elend nur bedacht!", "tokens": ["Auf", "ihr", "E\u00b7lend", "nur", "be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dort empf\u00e4hst du Trost f\u00fcr Thr\u00e4nen,", "tokens": ["Dort", "em\u00b7pf\u00e4hst", "du", "Trost", "f\u00fcr", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grund f\u00fcr Firni\u00df, Lust f\u00fcr Noth,", "tokens": ["Grund", "f\u00fcr", "Fir\u00b7ni\u00df", ",", "Lust", "f\u00fcr", "Noth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "