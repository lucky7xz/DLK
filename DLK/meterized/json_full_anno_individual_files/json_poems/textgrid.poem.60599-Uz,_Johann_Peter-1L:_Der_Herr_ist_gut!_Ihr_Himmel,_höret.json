 ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ich dem H\u00f6chsten theuer bin!", "tokens": ["Der", "ich", "dem", "H\u00f6chs\u00b7ten", "theu\u00b7er", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Ich fliehe des Ver\u00e4chters Pfade,", "tokens": ["Ich", "flie\u00b7he", "des", "Ver\u00b7\u00e4ch\u00b7ters", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der, im Gew\u00fchl der Welt verstrickt,", "tokens": ["Der", ",", "im", "Ge\u00b7w\u00fchl", "der", "Welt", "ver\u00b7strickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Herrn vergi\u00dft, und seine Gnade,", "tokens": ["Den", "Herrn", "ver\u00b7gi\u00dft", ",", "und", "sei\u00b7ne", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die H\u00fclfe, die er ihm geschickt.", "tokens": ["Die", "H\u00fcl\u00b7fe", ",", "die", "er", "ihm", "ge\u00b7schickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Ich aber will den sp\u00e4tsten Tagen", "tok