": ["PWAV", "ADJD", "VAFIN", "$,", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herr, unter deinen Fl\u00fcgeln ruhn!", "tokens": ["Herr", ",", "un\u00b7ter", "dei\u00b7nen", "Fl\u00fc\u00b7geln", "ruhn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Ich preise dich, Fels meiner St\u00e4rke,", "tokens": ["Ich", "prei\u00b7se", "dich", ",", "Fels", "mei\u00b7ner", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Gott, meine Zuflucht, mein Panier!", "tokens": ["Gott", ",", "mei\u00b7ne", "Zu\u00b7flucht", ",", "mein", "Pa\u00b7nier", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wenn ich auf deine F\u00fchrung merke,", "tokens": ["Wenn", "ich", "auf", "dei\u00b7ne", "F\u00fch\u00b7rung", "mer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie weis' und g\u00f6ttlich ist sie mir!", "tokens": ["Wie", "weis'", "und", "g\u00f6tt\u00b7lich", "ist", "sie", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "ADJD", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Du f\u00fchrtest mich auf dunkeln Wegen,", "tokens": ["Du", "f\u00fchr\u00b7test", "mich", "auf", "du