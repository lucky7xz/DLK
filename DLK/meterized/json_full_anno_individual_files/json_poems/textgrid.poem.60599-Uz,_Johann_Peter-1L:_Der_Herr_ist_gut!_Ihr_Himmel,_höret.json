{"textgrid.poem.60599": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Herr ist gut! Ihr Himmel, h\u00f6ret", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr ist gut! Ihr Himmel, h\u00f6ret", "tokens": ["Der", "Herr", "ist", "gut", "!", "Ihr", "Him\u00b7mel", ",", "h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und jauchzt mir nach: Der Herr ist gut!", "tokens": ["Und", "jauchzt", "mir", "nach", ":", "Der", "Herr", "ist", "gut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hat mein Leid in Lust verkehret!", "tokens": ["Er", "hat", "mein", "Leid", "in", "Lust", "ver\u00b7keh\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott ists, der gro\u00dfe Dinge thut!", "tokens": ["Gott", "ists", ",", "der", "gro\u00b7\u00dfe", "Din\u00b7ge", "thut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu ihm, von dem wir H\u00fclfe haben,", "tokens": ["Zu", "ihm", ",", "von", "dem", "wir", "H\u00fcl\u00b7fe", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PRELS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Gott rief ich in meiner Noth,", "tokens": ["Zu", "Gott", "rief", "ich", "in", "mei\u00b7ner", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als gro\u00dfe Wasser mich umgaben,", "tokens": ["Als", "gro\u00b7\u00dfe", "Was\u00b7ser", "mich", "um\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und keine Hand mir H\u00fclfe both.", "tokens": ["Und", "kei\u00b7ne", "Hand", "mir", "H\u00fcl\u00b7fe", "both", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verderben hatte seinen Rachen", "tokens": ["Ver\u00b7der\u00b7ben", "hat\u00b7te", "sei\u00b7nen", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon wider mich weit aufgethan:", "tokens": ["Schon", "wi\u00b7der", "mich", "weit", "auf\u00b7ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah den stolzen Sp\u00f6tter lachen,", "tokens": ["Ich", "sah", "den", "stol\u00b7zen", "Sp\u00f6t\u00b7ter", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott aber sah mich huldreich an;", "tokens": ["Gott", "a\u00b7ber", "sah", "mich", "huld\u00b7reich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sah Fluthen \u00fcber Fluthen schwellen,", "tokens": ["Sah", "Flut\u00b7hen", "\u00fc\u00b7ber", "Flut\u00b7hen", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und rettete mit starker Macht", "tokens": ["Und", "ret\u00b7te\u00b7te", "mit", "star\u00b7ker", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich mitten durch die schwarzen Wellen", "tokens": ["Mich", "mit\u00b7ten", "durch", "die", "schwar\u00b7zen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und alle Schrecken banger Nacht.", "tokens": ["Und", "al\u00b7le", "Schre\u00b7cken", "ban\u00b7ger", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gott ist mit mir! Was kann mir schaden?", "tokens": ["Gott", "ist", "mit", "mir", "!", "Was", "kann", "mir", "scha\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was kann mir Staub und Asche thun?", "tokens": ["Was", "kann", "mir", "Staub", "und", "A\u00b7sche", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie gut ists, aller Sorg entladen,", "tokens": ["Wie", "gut", "ists", ",", "al\u00b7ler", "Sorg", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herr, unter deinen Fl\u00fcgeln ruhn!", "tokens": ["Herr", ",", "un\u00b7ter", "dei\u00b7nen", "Fl\u00fc\u00b7geln", "ruhn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich preise dich, Fels meiner St\u00e4rke,", "tokens": ["Ich", "prei\u00b7se", "dich", ",", "Fels", "mei\u00b7ner", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Gott, meine Zuflucht, mein Panier!", "tokens": ["Gott", ",", "mei\u00b7ne", "Zu\u00b7flucht", ",", "mein", "Pa\u00b7nier", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wenn ich auf deine F\u00fchrung merke,", "tokens": ["Wenn", "ich", "auf", "dei\u00b7ne", "F\u00fch\u00b7rung", "mer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie weis' und g\u00f6ttlich ist sie mir!", "tokens": ["Wie", "weis'", "und", "g\u00f6tt\u00b7lich", "ist", "sie", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "ADJD", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du f\u00fchrtest mich auf dunkeln Wegen,", "tokens": ["Du", "f\u00fchr\u00b7test", "mich", "auf", "dun\u00b7keln", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verbargst vor mir dein Angesicht,", "tokens": ["Ver\u00b7bargst", "vor", "mir", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und warest doch bey mir zugegen,", "tokens": ["Und", "wa\u00b7rest", "doch", "bey", "mir", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und in der Finsterni\u00df mein Licht.", "tokens": ["Und", "in", "der", "Fins\u00b7ter\u00b7ni\u00df", "mein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ihr g\u00fcldnen Seile treuer Liebe,", "tokens": ["Ihr", "g\u00fcld\u00b7nen", "Sei\u00b7le", "treu\u00b7er", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zieht mich zu meinem Sch\u00f6pfer hin!", "tokens": ["Zieht", "mich", "zu", "mei\u00b7nem", "Sch\u00f6p\u00b7fer", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sch\u00e4m ich mich der niedern Triebe,", "tokens": ["Wie", "sch\u00e4m", "ich", "mich", "der", "nie\u00b7dern", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ich dem H\u00f6chsten theuer bin!", "tokens": ["Der", "ich", "dem", "H\u00f6chs\u00b7ten", "theu\u00b7er", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich fliehe des Ver\u00e4chters Pfade,", "tokens": ["Ich", "flie\u00b7he", "des", "Ver\u00b7\u00e4ch\u00b7ters", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der, im Gew\u00fchl der Welt verstrickt,", "tokens": ["Der", ",", "im", "Ge\u00b7w\u00fchl", "der", "Welt", "ver\u00b7strickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Herrn vergi\u00dft, und seine Gnade,", "tokens": ["Den", "Herrn", "ver\u00b7gi\u00dft", ",", "und", "sei\u00b7ne", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die H\u00fclfe, die er ihm geschickt.", "tokens": ["Die", "H\u00fcl\u00b7fe", ",", "die", "er", "ihm", "ge\u00b7schickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich aber will den sp\u00e4tsten Tagen", "tokens": ["Ich", "a\u00b7ber", "will", "den", "sp\u00e4ts\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des gro\u00dfen Retters in der Noth,", "tokens": ["Des", "gro\u00b7\u00dfen", "Ret\u00b7ters", "in", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des weisen Vaters G\u00fcte sagen,", "tokens": ["Des", "wei\u00b7sen", "Va\u00b7ters", "G\u00fc\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lob des Gottes Zebaoth!", "tokens": ["Das", "Lob", "des", "Got\u00b7tes", "Ze\u00b7bao\u00b7th", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Der Herr ist gut! Ihr Himmel, h\u00f6ret", "tokens": ["Der", "Herr", "ist", "gut", "!", "Ihr", "Him\u00b7mel", ",", "h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "po