nct"], "pos": ["PRF", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine keusche Sch\u00f6nheit macht,", "tokens": ["Dei\u00b7ne", "keu\u00b7sche", "Sch\u00f6n\u00b7heit", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df mein Mund auch weinend lacht.", "tokens": ["da\u00df", "mein", "Mund", "auch", "wei\u00b7nend", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Eben di\u00df ist mir ein Zeichen", "tokens": ["E\u00b7ben", "di\u00df", "ist", "mir", "ein", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VAFIN", "PPER", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "deiner ungef\u00e4rbten Gunst,", "tokens": ["dei\u00b7ner", "un\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Gunst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn du mich in dieser Brunst", "tokens": ["wenn", "du", "mich", "in", "die\u00b7ser", "Brunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "nicht ganz h\u00fclflos l\u00e4\u00dft erbleichen,", "tokens": ["nicht", "ganz", "h\u00fcl\u00b7flos", "l\u00e4\u00dft", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und weil du mich nicht kanst k\u00fcssen,", "tokens": ["und", "weil", "du", "mich", "nicht", "kanst", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "mich doch l\u00e4ssest noch begr\u00fc\u00dfen.", "tokens": ["mich", "doch", "l\u00e4s\u00b7sest", "noch", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Nun, erfreue mich, o Sch\u00f6ne,", "tokens": ["Nun", ",", "er\u00b7freu\u00b7e", "mich", ",", "o", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df ich, wie ich vor getan,", "tokens": ["da\u00df", "ich", ",", "wie", "ich", "vor", "ge\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "APPR", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "so auch ferner sagen kan:", "tokens": ["so", "auch", "fer\u00b7ner", "sa\u00b7gen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die getreue ", "tokens": ["Die", "ge\u00b7treu\u00b7e"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "tut stets, was ich mich stets freue.", "tokens": ["tut", "stets", ",", "was", "ich", "mich", "stets", "freu\u00b7e", "."], "token_info": ["word", "word", "punct", "word", "wo