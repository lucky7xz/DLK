{"textgrid.poem.48666": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "29. Das getreue Elsgen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist unverwant mein Herze,", "tokens": ["Es", "ist", "un\u00b7ver\u00b7want", "mein", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "das ich trage gegen dir,", "tokens": ["das", "ich", "tra\u00b7ge", "ge\u00b7gen", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "es ist unverwant in mir,", "tokens": ["es", "ist", "un\u00b7ver\u00b7want", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "du mein Trost und auch mein Schmerze.", "tokens": ["du", "mein", "Trost", "und", "auch", "mein", "Schmer\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was sich regt in meinem Blute", "tokens": ["Was", "sich", "regt", "in", "mei\u00b7nem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wei\u00df von keinem Wankelmute.", "tokens": ["wei\u00df", "von", "kei\u00b7nem", "Wan\u00b7kel\u00b7mu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lasse dich di\u00df nicht betr\u00fcben,", "tokens": ["Las\u00b7se", "dich", "di\u00df", "nicht", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df ich dir ohn' Unterla\u00df", "tokens": ["da\u00df", "ich", "dir", "ohn'", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "von der Pein, die mich macht bla\u00df,", "tokens": ["von", "der", "Pein", ",", "die", "mich", "macht", "bla\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "seither habe nicht geschrieben.", "tokens": ["sei\u00b7ther", "ha\u00b7be", "nicht", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Gem\u00fcte redt die F\u00fclle,", "tokens": ["Das", "Ge\u00b7m\u00fc\u00b7te", "redt", "die", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "schweigt gleich Mund und Feder stille.", "tokens": ["schweigt", "gleich", "Mund", "und", "Fe\u00b7der", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Siehst du, wie die festen Eichen", "tokens": ["Siehst", "du", ",", "wie", "die", "fes\u00b7ten", "Ei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "f\u00fcr den St\u00fcrmen sicher sind,", "tokens": ["f\u00fcr", "den", "St\u00fcr\u00b7men", "si\u00b7cher", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wie der schwache Nordenwind", "tokens": ["wie", "der", "schwa\u00b7che", "Nor\u00b7den\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "von den Felsen ab mu\u00df weichen?", "tokens": ["von", "den", "Fel\u00b7sen", "ab", "mu\u00df", "wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein stark Herze, das dich meint,", "tokens": ["Mein", "stark", "Her\u00b7ze", ",", "das", "dich", "meint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "bleibt, weil uns die Sonne scheint.", "tokens": ["bleibt", ",", "weil", "uns", "die", "Son\u00b7ne", "scheint", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Geu\u00df die Stralen deiner lieben,", "tokens": ["Geu\u00df", "die", "Stra\u00b7len", "dei\u00b7ner", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "deiner s\u00fc\u00dfen Treflichkeit", "tokens": ["dei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Tref\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "in mein Herze, das sich freut,", "tokens": ["in", "mein", "Her\u00b7ze", ",", "das", "sich", "freut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "sich um dich auch zu betr\u00fcben.", "tokens": ["sich", "um", "dich", "auch", "zu", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine keusche Sch\u00f6nheit macht,", "tokens": ["Dei\u00b7ne", "keu\u00b7sche", "Sch\u00f6n\u00b7heit", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df mein Mund auch weinend lacht.", "tokens": ["da\u00df", "mein", "Mund", "auch", "wei\u00b7nend", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eben di\u00df ist mir ein Zeichen", "tokens": ["E\u00b7ben", "di\u00df", "ist", "mir", "ein", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VAFIN", "PPER", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "deiner ungef\u00e4rbten Gunst,", "tokens": ["dei\u00b7ner", "un\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Gunst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn du mich in dieser Brunst", "tokens": ["wenn", "du", "mich", "in", "die\u00b7ser", "Brunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "nicht ganz h\u00fclflos l\u00e4\u00dft erbleichen,", "tokens": ["nicht", "ganz", "h\u00fcl\u00b7flos", "l\u00e4\u00dft", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und weil du mich nicht kanst k\u00fcssen,", "tokens": ["und", "weil", "du", "mich", "nicht", "kanst", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "mich doch l\u00e4ssest noch begr\u00fc\u00dfen.", "tokens": ["mich", "doch", "l\u00e4s\u00b7sest", "noch", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun, erfreue mich, o Sch\u00f6ne,", "tokens": ["Nun", ",", "er\u00b7freu\u00b7e", "mich", ",", "o", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "FM