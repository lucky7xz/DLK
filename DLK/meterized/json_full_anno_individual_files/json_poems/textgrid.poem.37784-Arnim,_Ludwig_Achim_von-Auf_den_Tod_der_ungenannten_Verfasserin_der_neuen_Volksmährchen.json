{"textgrid.poem.37784": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod der ungenannten Verfasserin der neuen Volksm\u00e4hrchen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Masken Fest verstummt im B\u00fcchersaale,", "tokens": ["Der", "Mas\u00b7ken", "Fest", "ver\u00b7stummt", "im", "B\u00fc\u00b7cher\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Stunde schlug, was mag sie so betr\u00fcben?", "tokens": ["Die", "Stun\u00b7de", "schlug", ",", "was", "mag", "sie", "so", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie eilen fort und Staub ist nur geblieben", "tokens": ["Sie", "ei\u00b7len", "fort", "und", "Staub", "ist", "nur", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Bildern alter Zeiten bei dem Mahle.", "tokens": ["Von", "Bil\u00b7dern", "al\u00b7ter", "Zei\u00b7ten", "bei", "dem", "Mah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Wagen rollend mit dem Fackelstrahle", "tokens": ["Die", "Wa\u00b7gen", "rol\u00b7lend", "mit", "dem", "Fa\u00b7ckel\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch dunkle Gassen Trauerzeilen schrieben", "tokens": ["Durch", "dunk\u00b7le", "Gas\u00b7sen", "Trau\u00b7er\u00b7zei\u00b7len", "schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Asche, als die letzten Funken trieben", "tokens": ["In", "A\u00b7sche", ",", "als", "die", "letz\u00b7ten", "Fun\u00b7ken", "trie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nicht, wie erste, aus dem festen Stahle.", "tokens": ["Und", "nicht", ",", "wie", "ers\u00b7te", ",", "aus", "dem", "fes\u00b7ten", "Stah\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "ADJA", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und diese Funken sich zusammenfinden", "tokens": ["Und", "die\u00b7se", "Fun\u00b7ken", "sich", "zu\u00b7sam\u00b7men\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beim Haus der Dichterin, am Leichenwagen,", "tokens": ["Beim", "Haus", "der", "Dich\u00b7te\u00b7rin", ",", "am", "Lei\u00b7chen\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Masken ihr als Mutter Kr\u00e4nze winden.", "tokens": ["Die", "Mas\u00b7ken", "ihr", "als", "Mut\u00b7ter", "Kr\u00e4n\u00b7ze", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KOUS", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch keine kann der Todten Namen sagen!", "tokens": ["Doch", "kei\u00b7ne", "kann", "der", "Tod\u00b7ten", "Na\u00b7men", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die alle reif zur Geistestauf' getragen,", "tokens": ["Die", "al\u00b7le", "reif", "zur", "Geis\u00b7tes\u00b7tauf'", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Will selbst auf Erden namenlos verschwinden.", "tokens": ["Will", "selbst", "auf", "Er\u00b7den", "na\u00b7men\u00b7los", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Masken Fest verstummt im B\u00fcchersaale,", "tokens": ["Der", "Mas\u00b7ken", "Fest", "ver\u00b7stummt", "im", "B\u00fc\u00b7cher\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Stunde schlug, was mag sie so betr\u00fcben?", "tokens": ["Die", "Stun\u00b7de", "schlug", ",", "was", "mag",