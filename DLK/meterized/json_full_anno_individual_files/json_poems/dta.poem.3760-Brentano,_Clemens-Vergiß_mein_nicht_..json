{"dta.poem.3760": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Vergi\u00df mein nicht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist es nicht eine harte Pein;               ", "tokens": ["Ist", "es", "nicht", "ei\u00b7ne", "har\u00b7te", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Liebende nicht beysammen seyn,", "tokens": ["Wenn", "Lie\u00b7ben\u00b7de", "nicht", "bey\u00b7sam\u00b7men", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dr\u00fcck mich fest in dein Herz hinein,", "tokens": ["Dr\u00fcck", "mich", "fest", "in", "dein", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Wachsen heraus Vergi\u00df nicht mein.", "tokens": ["Wach\u00b7sen", "he\u00b7raus", "Ver\u00b7gi\u00df", "nicht", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "NN", "PTKNEG", "PPOSAT", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}