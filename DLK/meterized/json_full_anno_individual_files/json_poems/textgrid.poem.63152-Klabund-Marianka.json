tetra"}, "line.5": {"text": "Doch bei keinem konnt ich bleiben,", "tokens": ["Doch", "bei", "kei\u00b7nem", "konnt", "ich", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Marianka, Marianka!", "tokens": ["Ma\u00b7ri\u00b7an\u00b7ka", ",", "Ma\u00b7ri\u00b7an\u00b7ka", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Muss wie Spreu im Winde treiben,", "tokens": ["Muss", "wie", "Spreu", "im", "Win\u00b7de", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Marianka, Marianka!", "tokens": ["Ma\u00b7ri\u00b7an\u00b7ka", ",", "Ma\u00b7ri\u00b7an\u00b7ka", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hej, ich liebe alles Wilde,", "tokens": ["Hej", ",", "ich", "lie\u00b7be", "al\u00b7les", "Wil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Marianka, Marianka!", "tokens": ["Ma\u00b7ri\u00b7an\u00b7ka", ",", "Ma\u00b7ri\u00b7an\u00b7ka", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchre B\u00f6ses gern im Schilde,", "tokens": ["F\u00fch\u00b7re", "B\u00f6\u00b7ses", "gern", "im", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Marianka, Marianka!", "tokens": ["Ma\u00b7ri\u00b7an\u00b7ka", ",", "Ma\u00b7ri\u00b7an\u00b7ka", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer mich liebt, muss alles wagen,", "tokens": ["Wer", "mich", "liebt", ",", "muss", "al\u00b7les", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Marianka, Marianka!", "tokens": ["Ma\u00b7ri\u00b7an\u00b7ka", ",", "Ma\u00b7ri\u00b7an\u00b7ka", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Janos hat den Ju erschlagen.", "tokens": ["Ja\u00b7nos", "hat", "den", "Ju", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Marianka, Marianka!", "tokens": ["Ma\u00b7ri\u00b7an\u00b7ka", ",", "Ma\u00b7ri\u00b7an\u00b7ka", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn ich einst ein Kind werd haben,", "tokens": ["Wenn", "ich", "einst", "ein", "Kind", "werd", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Marianka, Marianka,", "tokens": ["Ma\u00b7ri\u00b7an\u00b7ka", ",", "Ma\u00b7ri\u00b7an\u00b7ka", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollt ihr lebend mich begraben,", "tokens": ["Sollt", "ihr", "le\u00b7bend", "mich", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Marianka, Marianka.", "tokens": ["Ma\u00b7ri\u00b7an\u00b7ka", ",", "Ma\u00b7ri\u00b7an\u00b7ka", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn mein Blut wird Fr\u00fcchte tragen,", "tokens": ["Denn", "mein", "Blut", "wird", "Fr\u00fcch\u00b7te", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Marianka, Marianka!", "tokens": ["Ma\u00b7ri\u00b7an\u00b7ka", ",", "Ma\u00b7ri\u00b7an\u00b7ka", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mein Herz wird ewig schlagen,", "tokens": ["Und", "mein", "Herz", "wird", "e\u00b7wig", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Marianka, Marianka!", "tokens": ["Ma\u00b7ri\u00b7an\u00b7ka", ",", "Ma\u00b7ri\u00b7an\u00b7ka", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wollt ihr wissen meinen Namen?", "tokens": ["Wollt", "ihr", "wis\u00b7sen", "mei\u00b7nen", "Na\u00b7men", "?"], "token_info":