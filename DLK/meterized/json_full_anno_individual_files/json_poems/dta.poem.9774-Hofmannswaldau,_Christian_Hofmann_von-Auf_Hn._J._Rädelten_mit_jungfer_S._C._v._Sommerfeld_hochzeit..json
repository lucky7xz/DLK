{"dta.poem.9774": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hn. J. R\u00e4delten mit jungfer S. C. v.  \n Sommerfeld hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der sommer reist nunmehr der blumen purpur ein/", "tokens": ["Der", "som\u00b7mer", "reist", "nun\u00b7mehr", "der", "blu\u00b7men", "pur\u00b7pur", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "ART", "NN", "ADJD", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die felder br\u00fcsten sich mit k\u00f6rner-vollen \u00e4hren", "tokens": ["Die", "fel\u00b7der", "br\u00fcs\u00b7ten", "sich", "mit", "k\u00f6r\u00b7ner\u00b7vol\u00b7len", "\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und lehren: da\u00df der mensch soll wie der acker seyn/", "tokens": ["Und", "leh\u00b7ren", ":", "da\u00df", "der", "mensch", "soll", "wie", "der", "ac\u00b7ker", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "KOUS", "ART", "NN", "VMFIN", "KOKOM", "ART", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nach der blumen-zeit auch rechte frucht gew\u00e4hren.", "tokens": ["Und", "nach", "der", "blu\u00b7men\u00b7zeit", "auch", "rech\u00b7te", "frucht", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn wie ein rosen-strauch den stoltzen carmasin", "tokens": ["Denn", "wie", "ein", "ro\u00b7sen\u00b7strauch", "den", "stolt\u00b7zen", "car\u00b7ma\u00b7sin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auch mit den bl\u00e4ttern l\u00e4st in tr\u00fcber lufft verschwinden:", "tokens": ["Auch", "mit", "den", "bl\u00e4t\u00b7tern", "l\u00e4st", "in", "tr\u00fc\u00b7ber", "lufft", "ver\u00b7schwin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So l\u00e4st die jugend offt den lebens-glantz verbl\u00fchn/", "tokens": ["So", "l\u00e4st", "die", "ju\u00b7gend", "offt", "den", "le\u00b7bens\u00b7glantz", "ver\u00b7bl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo frucht und bl\u00fcte sich nicht vor der zeit verbinden.", "tokens": ["Wo", "frucht", "und", "bl\u00fc\u00b7te", "sich", "nicht", "vor", "der", "zeit", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "PRF", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie hat/ hochwerthe braut/ in ihrer fr\u00fchlings-zeit", "tokens": ["Sie", "hat", "/", "hoch\u00b7wert\u00b7he", "braut", "/", "in", "ih\u00b7rer", "fr\u00fch\u00b7lings\u00b7zeit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$(", "ADJA", "VVFIN", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mehr blumen/ als der lentz narcissen/ auffgetragen:", "tokens": ["Mehr", "blu\u00b7men", "/", "als", "der", "lentz", "nar\u00b7cis\u00b7sen", "/", "auff\u00b7ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$(", "KOUS", "ART", "NN", "VVINF", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie nun ein sommer-feld nach blumen fr\u00fcchte streut/", "tokens": ["Wie", "nun", "ein", "som\u00b7mer\u00b7feld", "nach", "blu\u00b7men", "fr\u00fcch\u00b7te", "streut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ADJA", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So denckt auch ihre frucht im sommer auszuschlagen/", "tokens": ["So", "denckt", "auch", "ih\u00b7re", "frucht", "im", "som\u00b7mer", "aus\u00b7zu\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum f\u00e4ngt sie mit bedacht ein ander leben an/", "tokens": ["Drum", "f\u00e4ngt", "sie", "mit", "be\u00b7dacht", "ein", "an\u00b7der", "le\u00b7ben", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "VVFIN", "ART", "ADJD", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sucht glut und sonnenschein in ihres liebsten augen;", "tokens": ["Sucht", "glut", "und", "son\u00b7nen\u00b7schein", "in", "ih\u00b7res", "liebs\u00b7ten", "au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und wie der morgen-thau die felder k\u00fchlen kan/", "tokens": ["Und", "wie", "der", "mor\u00b7gen\u00b7\u00b7thau", "die", "fel\u00b7der", "k\u00fch\u00b7len", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So will sie wieder krafft aus seinen lippen saugen.", "tokens": ["So", "will", "sie", "wie\u00b7der", "krafft", "aus", "sei\u00b7nen", "lip\u00b7pen", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und dieses nicht umsonst: denn wenn der himmel blitzt/", "tokens": ["Und", "die\u00b7ses", "nicht", "um\u00b7sonst", ":", "denn", "wenn", "der", "him\u00b7mel", "blitzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PTKNEG", "ADV", "$.", "KON", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wenn donner/ wind und sturm auff alle felder schlagen;", "tokens": ["Wenn", "don\u00b7ner", "/", "wind", "und", "sturm", "auff", "al\u00b7le", "fel\u00b7der", "schla\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "XY", "KON", "ADJD", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So wei\u00df sie/ da\u00df ihr feld in stetem friede sitzt", "tokens": ["So", "wei\u00df", "sie", "/", "da\u00df", "ihr", "feld", "in", "ste\u00b7tem", "frie\u00b7de", "sitzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und sonder alle furcht kan seine fr\u00fcchte tragen.", "tokens": ["Und", "son\u00b7der", "al\u00b7le", "furcht", "kan", "sei\u00b7ne", "fr\u00fcch\u00b7te", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VMFIN", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Drum w\u00fcnsch ich: lebet wohl und brauchet eurer zeit.", "tokens": ["Drum", "w\u00fcnsch", "ich", ":", "le\u00b7bet", "wohl", "und", "brau\u00b7chet", "eu\u00b7rer", "zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$.", "VVFIN", "ADV", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der himmel lasse nichts als gold und perlen thauen/", "tokens": ["Der", "him\u00b7mel", "las\u00b7se", "nichts", "als", "gold", "und", "per\u00b7len", "thau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "KOKOM", "NN", "KON", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Bi\u00df da\u00df eu\u2019r sommerfeld gef\u00fcllte k\u00f6rner streut/", "tokens": ["Bi\u00df", "da\u00df", "eu'r", "som\u00b7mer\u00b7feld", "ge\u00b7f\u00fcll\u00b7te", "k\u00f6r\u00b7ner", "streut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und ihr die fruchtbarkeit la\u00dft in der wiege schauen.", "tokens": ["Und", "ihr", "die", "frucht\u00b7bar\u00b7keit", "la\u00dft", "in", "der", "wie\u00b7ge", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}