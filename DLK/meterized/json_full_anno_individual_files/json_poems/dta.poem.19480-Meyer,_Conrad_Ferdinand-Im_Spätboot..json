{"dta.poem.19480": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Im Sp\u00e4tboot.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Aus der Schiffsbank mach' ich meinen Pf\u00fchl,", "tokens": ["Aus", "der", "Schiffs\u00b7bank", "mach'", "ich", "mei\u00b7nen", "Pf\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Endlich wird die hei\u00dfe Stirne k\u00fchl!", "tokens": ["End\u00b7lich", "wird", "die", "hei\u00b7\u00dfe", "Stir\u00b7ne", "k\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "O wie s\u00fc\u00df erkaltet mir das Herz!", "tokens": ["O", "wie", "s\u00fc\u00df", "er\u00b7kal\u00b7tet", "mir", "das", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "O wie weich verstummen Lust und Schmerz!", "tokens": ["O", "wie", "weich", "ver\u00b7stum\u00b7men", "Lust", "und", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ueber mir des Rohres schwarzer Rauch", "tokens": ["Ue\u00b7ber", "mir", "des", "Roh\u00b7res", "schwar\u00b7zer", "Rauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Wiegt und biegt sich in des Windes Hauch.", "tokens": ["Wiegt", "und", "biegt", "sich", "in", "des", "Win\u00b7des", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "H\u00fcben hier und wieder dr\u00fcben dort", "tokens": ["H\u00fc\u00b7ben", "hier", "und", "wie\u00b7der", "dr\u00fc\u00b7ben", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "H\u00e4lt das Boot an manchem kleinen Port:", "tokens": ["H\u00e4lt", "das", "Boot", "an", "man\u00b7chem", "klei\u00b7nen", "Port", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Bei der Schiffslaterne kargem Schein", "tokens": ["Bei", "der", "Schiffs\u00b7la\u00b7ter\u00b7ne", "kar\u00b7gem", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Steigt ein Schatten aus und niemand ein.", "tokens": ["Steigt", "ein", "Schat\u00b7ten", "aus", "und", "nie\u00b7mand", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Nur der Steurer noch, der wacht und steht!", "tokens": ["Nur", "der", "Steu\u00b7rer", "noch", ",", "der", "wacht", "und", "steht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Nur der Wind, der mir im Haare weht!", "tokens": ["Nur", "der", "Wind", ",", "der", "mir", "im", "Haa\u00b7re", "weht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Schmerz und Lust erleiden sanften Tod", "tokens": ["Schmerz", "und", "Lust", "er\u00b7lei\u00b7den", "sanf\u00b7ten", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Einen Schlumm'rer tr\u00e4gt das dunkle Boot.", "tokens": ["Ei\u00b7nen", "Schlum\u00b7m'\u00b7rer", "tr\u00e4gt", "das", "dunk\u00b7le", "Boot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}