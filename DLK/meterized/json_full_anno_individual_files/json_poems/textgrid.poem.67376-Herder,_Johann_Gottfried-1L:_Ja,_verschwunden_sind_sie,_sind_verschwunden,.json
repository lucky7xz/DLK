{"textgrid.poem.67376": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, verschwunden sind sie, sind verschwunden,", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, verschwunden sind sie, sind verschwunden,", "tokens": ["Ja", ",", "ver\u00b7schwun\u00b7den", "sind", "sie", ",", "sind", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVPP", "VAFIN", "PPER", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jene kurzen, jene sch\u00f6nen Stunden,", "tokens": ["Je\u00b7ne", "kur\u00b7zen", ",", "je\u00b7ne", "sch\u00f6\u00b7nen", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die auch ich am Pausilipp erlebt.", "tokens": ["Die", "auch", "ich", "am", "Pau\u00b7si\u00b7lipp", "er\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "APPRART", "NE", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Holder Traum von Grotten, Felsen, H\u00fcgeln,", "tokens": ["Hol\u00b7der", "Traum", "von", "Grot\u00b7ten", ",", "Fel\u00b7sen", ",", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Inseln und der Sonne sch\u00f6nen Spiegeln,", "tokens": ["In\u00b7seln", "und", "der", "Son\u00b7ne", "sch\u00f6\u00b7nen", "Spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seen, Meer \u2013 Du bist mir fortgeschwebt!", "tokens": ["Seen", ",", "Meer", "\u2013", "Du", "bist", "mir", "fort\u00b7ge\u00b7schwebt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$(", "PPER", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Fortgeschwebt die zaubernde Sirene,", "tokens": ["Fort\u00b7ge\u00b7schwebt", "die", "zau\u00b7bern\u00b7de", "Si\u00b7re\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die mich ohne s\u00fc\u00dfer Fl\u00f6ten T\u00f6ne", "tokens": ["Die", "mich", "oh\u00b7ne", "s\u00fc\u00b7\u00dfer", "Fl\u00f6\u00b7ten", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schwesterlich in ihre Arme nahm;", "tokens": ["Schwes\u00b7ter\u00b7lich", "in", "ih\u00b7re", "Ar\u00b7me", "nahm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und mein Herz schlug voller und geschwinder,", "tokens": ["Und", "mein", "Herz", "schlug", "vol\u00b7ler", "und", "ge\u00b7schwin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJA", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und mein Blut flo\u00df reiner und gelinder,", "tokens": ["Und", "mein", "Blut", "flo\u00df", "rei\u00b7ner", "und", "ge\u00b7lin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJA", "KON", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da ihr Athem mir entgegen kam.", "tokens": ["Da", "ihr", "A\u00b7them", "mir", "ent\u00b7ge\u00b7gen", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sehnend sah ich ihres Busens Wellen", "tokens": ["Seh\u00b7nend", "sah", "ich", "ih\u00b7res", "Bu\u00b7sens", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sanfter sich und reger zu mir schwellen,", "tokens": ["Sanf\u00b7ter", "sich", "und", "re\u00b7ger", "zu", "mir", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "KON", "ADJA", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schwamm dann mit der Fl\u00e4che sanft dahin;", "tokens": ["Schwamm", "dann", "mit", "der", "Fl\u00e4\u00b7che", "sanft", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "ADJD", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sah den sch\u00f6nen Kranz von Fels und H\u00fcgeln,", "tokens": ["Sah", "den", "sch\u00f6\u00b7nen", "Kranz", "von", "Fels", "und", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sah die Sterne, sah den Mond sich spiegeln", "tokens": ["Sah", "die", "Ster\u00b7ne", ",", "sah", "den", "Mond", "sich", "spie\u00b7geln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In der s\u00fc\u00dfen Freudegeberin.", "tokens": ["In", "der", "s\u00fc\u00b7\u00dfen", "Freu\u00b7de\u00b7ge\u00b7be\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sah die Inseln in den Wellen schweben,", "tokens": ["Sah", "die", "In\u00b7seln", "in", "den", "Wel\u00b7len", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tr\u00e4umt' auf ihnen ein begl\u00fccktes Leben,", "tokens": ["Tr\u00e4umt'", "auf", "ih\u00b7nen", "ein", "be\u00b7gl\u00fcck\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unbekannt und aller Welt entflohn;", "tokens": ["Un\u00b7be\u00b7kannt", "und", "al\u00b7ler", "Welt", "ent\u00b7flohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sammelt' nur um mich den Kreis der Meinen \u2013", "tokens": ["Sam\u00b7melt'", "nur", "um", "mich", "den", "Kreis", "der", "Mei\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PRF", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ach, Ihr Wellen, oft saht Ihr mich weinen", "tokens": ["Ach", ",", "Ihr", "Wel\u00b7len", ",", "oft", "saht", "Ihr", "mich", "wei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Um sie, f\u00fcr sie, zu der G\u00f6ttin Thron!", "tokens": ["Um", "sie", ",", "f\u00fcr", "sie", ",", "zu", "der", "G\u00f6t\u00b7tin", "Thron", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "APPR", "PPER", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn die Abendr\u00f6th' im stillen Meere", "tokens": ["Wenn", "die", "A\u00b7bendr\u00f6\u00b7th'", "im", "stil\u00b7len", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sanft verschwebte, und mit seinem Heere", "tokens": ["Sanft", "ver\u00b7schweb\u00b7te", ",", "und", "mit", "sei\u00b7nem", "Hee\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gl\u00e4nzender der Mond zum Himmel stieg,", "tokens": ["Gl\u00e4n\u00b7zen\u00b7der", "der", "Mond", "zum", "Him\u00b7mel", "stieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach, da flossen mit so neuem Sehnen", "tokens": ["Ach", ",", "da", "flos\u00b7sen", "mit", "so", "neu\u00b7em", "Seh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Unschuldvolle, jugendliche Thr\u00e4nen;", "tokens": ["Un\u00b7schuld\u00b7vol\u00b7le", ",", "ju\u00b7gend\u00b7li\u00b7che", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Nur ein Seufzer sprach, und Alles schwieg.", "tokens": ["Nur", "ein", "Seuf\u00b7zer", "sprach", ",", "und", "Al\u00b7les", "schwieg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "KON", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Nimmer, nimmer sollt Ihr mir entschwinden,", "tokens": ["Nim\u00b7mer", ",", "nim\u00b7mer", "sollt", "Ihr", "mir", "ent\u00b7schwin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Immer wird mein Herz Euch wiederfinden,", "tokens": ["Im\u00b7mer", "wird", "mein", "Herz", "Euch", "wie\u00b7der\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "S\u00fc\u00dfe Tr\u00e4ume, rein und zart und sch\u00f6n.", "tokens": ["S\u00fc\u00b7\u00dfe", "Tr\u00e4u\u00b7me", ",", "rein", "und", "zart", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nie wird Euch mein Auge wiedersehen,", "tokens": ["Nie", "wird", "Euch", "mein", "Au\u00b7ge", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Doch ein Hauch wird lispelnd zu Euch wehen:", "tokens": ["Doch", "ein", "Hauch", "wird", "lis\u00b7pelnd", "zu", "Euch", "we\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "\u00bbich, auch ich war in Arkadien.\u00ab", "tokens": ["\u00bb", "ich", ",", "auch", "ich", "war", "in", "Ar\u00b7ka\u00b7di\u00b7en", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "$,", "ADV", "PPER", "VAFIN", "APPR", "NE", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Ja, verschwunden sind sie, sind verschwunden,", "tokens": ["Ja", ",", "ver\u00b7schwun\u00b7den", "sind", "sie", ",", "sind", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVPP", "VAFIN", "PPER", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jene kurzen, jene sch\u00f6nen Stunden,", "tokens": ["Je\u00b7ne", "kur\u00b7zen", ",", "je\u00b7ne", "sch\u00f6\u00b7nen", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word",