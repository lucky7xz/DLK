: "Sanfter sich und reger zu mir schwellen,", "tokens": ["Sanf\u00b7ter", "sich", "und", "re\u00b7ger", "zu", "mir", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "KON", "ADJA", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schwamm dann mit der Fl\u00e4che sanft dahin;", "tokens": ["Schwamm", "dann", "mit", "der", "Fl\u00e4\u00b7che", "sanft", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "ADJD", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sah den sch\u00f6nen Kranz von Fels und H\u00fcgeln,", "tokens": ["Sah", "den", "sch\u00f6\u00b7nen", "Kranz", "von", "Fels", "und", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sah die Sterne, sah den Mond sich spiegeln", "tokens": ["Sah", "die", "Ster\u00b7ne", ",", "sah", "den", "Mond", "sich", "spie\u00b7geln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In der s\u00fc\u00dfen Freudegeberin.", "tokens": ["In", "der", "s\u00fc\u00b7\u00dfen", "Freu\u00b7de\u00b7ge\u00b7be\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Sah die Inseln in den Wellen schweben,", "tokens": ["Sah", "die", "In\u00b7seln", "in", "den", "Wel\u00b7len", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word",