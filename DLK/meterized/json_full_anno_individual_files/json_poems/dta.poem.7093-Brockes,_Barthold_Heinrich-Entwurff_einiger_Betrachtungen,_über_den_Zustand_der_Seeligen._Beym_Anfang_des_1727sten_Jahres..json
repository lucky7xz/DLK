{"dta.poem.7093": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn sie sodann gemeinschafftlich ersehn:", "tokens": ["Wenn", "sie", "so\u00b7dann", "ge\u00b7mein\u00b7schafft\u00b7lich", "er\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie der Zusammenhang der grossen Kette nimmer", "tokens": ["Wie", "der", "Zu\u00b7sam\u00b7men\u00b7hang", "der", "gros\u00b7sen", "Ket\u00b7te", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "ADV"], "meter": "+-+--+-+-+-++", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Getrennet worden sey: wie alles immer", "tokens": ["Ge\u00b7tren\u00b7net", "wor\u00b7den", "sey", ":", "wie", "al\u00b7les", "im\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAPP", "VAFIN", "$.", "PWAV", "PIS", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So ordentlich, so wundersch\u00f6n,", "tokens": ["So", "or\u00b7dent\u00b7lich", ",", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht nur allein auf unsrer Erden,", "tokens": ["Nicht", "nur", "al\u00b7lein", "auf", "uns\u00b7rer", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "In andern auch geschehn, die man nicht z\u00e4hlen kann;", "tokens": ["In", "an\u00b7dern", "auch", "ge\u00b7schehn", ",", "die", "man", "nicht", "z\u00e4h\u00b7len", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVPP", "$,", "PRELS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie sehnlich wird sodann", "tokens": ["Wie", "sehn\u00b7lich", "wird", "so\u00b7dann"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der Sch\u00f6pffer nicht gelobt und angebetet werden?", "tokens": ["Der", "Sch\u00f6pf\u00b7fer", "nicht", "ge\u00b7lobt", "und", "an\u00b7ge\u00b7be\u00b7tet", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}