{"dta.poem.17256": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "106.  Von den M\u00e4rtyrern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der M\u00e4rtrer Lebenslauff ist wenig aufgeschrieben;", "tokens": ["Der", "M\u00e4r\u00b7trer", "Le\u00b7bens\u00b7lauff", "ist", "we\u00b7nig", "auf\u00b7ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Tugenden die man zur Leidenszeit gesp\u00fcrt/", "tokens": ["Die", "Tu\u00b7gen\u00b7den", "die", "man", "zur", "Lei\u00b7dens\u00b7zeit", "ge\u00b7sp\u00fcrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIS", "APPRART", "NN", "VVPP", "$("], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Die Lobt und preist man nur/ und sind statt jenes blieben:", "tokens": ["Die", "Lobt", "und", "preist", "man", "nur", "/", "und", "sind", "statt", "je\u00b7nes", "blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PIS", "ADV", "$(", "KON", "VAFIN", "APPR", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dieweil ein sch\u00f6ner Tod das gantze Leben ziehrt.", "tokens": ["Die\u00b7weil", "ein", "sch\u00f6\u00b7ner", "Tod", "das", "gant\u00b7ze", "Le\u00b7ben", "ziehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}