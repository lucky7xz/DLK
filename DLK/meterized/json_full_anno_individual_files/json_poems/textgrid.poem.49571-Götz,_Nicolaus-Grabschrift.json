{"textgrid.poem.49571": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrift", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ambrosius, der Prediger, liegt hier.", "tokens": ["A\u00b7mbro\u00b7si\u00b7us", ",", "der", "Pre\u00b7di\u00b7ger", ",", "liegt", "hier", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In jedes Kind von Sch\u00f6nheit sich verlieben,", "tokens": ["In", "je\u00b7des", "Kind", "von", "Sch\u00f6n\u00b7heit", "sich", "ver\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verstund der Mann, so gut, als sein Brevier.", "tokens": ["Ver\u00b7stund", "der", "Mann", ",", "so", "gut", ",", "als", "sein", "Bre\u00b7vier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Bestellete, durch jegliches Quartier", "tokens": ["Be\u00b7stel\u00b7le\u00b7te", ",", "durch", "jeg\u00b7li\u00b7ches", "Quar\u00b7tier"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Parochie, sein K\u00fcster Kasimir;", "tokens": ["Der", "Pa\u00b7ro\u00b7chie",