{"textgrid.poem.46512": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[im Verluste zu gewinnen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Verluste zu gewinnen,", "tokens": ["Im", "Ver\u00b7lus\u00b7te", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ein schwieriges Beginnen,", "tokens": ["Ist", "ein", "schwie\u00b7ri\u00b7ges", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gelinget andern nie,", "tokens": ["Und", "ge\u00b7lin\u00b7get", "an\u00b7dern", "nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als der Lieb' und Poesie.", "tokens": ["Als", "der", "Lieb'", "und", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Liebe l\u00e4\u00dft sich nichts entrinnen,", "tokens": ["Lie\u00b7be", "l\u00e4\u00dft", "sich", "nichts", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat nicht au\u00dfen, sondern innen;", "tokens": ["Hat", "nicht", "au\u00b7\u00dfen", ",", "son\u00b7dern", "in\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN"