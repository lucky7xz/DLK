{"textgrid.poem.25491": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch heute wird die Stunde kommen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch heute wird die Stunde kommen,", "tokens": ["Noch", "heu\u00b7te", "wird", "die", "Stun\u00b7de", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo dich mein Vaterarm umschlingt,", "tokens": ["Wo", "dich", "mein", "Va\u00b7te\u00b7rarm", "um\u00b7schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, was der Fr\u00fchling mir genommen,", "tokens": ["Und", ",", "was", "der", "Fr\u00fch\u00b7ling", "mir", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Herbst vollkommner wieder bringt;", "tokens": ["Der", "Herbst", "voll\u00b7komm\u00b7ner", "wie\u00b7der", "bringt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Mutter mit verj\u00fcngten Wangen,", "tokens": ["Die", "Mut\u00b7ter", "mit", "ver\u00b7j\u00fcng\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dich, gewachsen an Verstand.", "tokens": ["Und", "dich", ",", "ge\u00b7wach\u00b7sen", "an", "Ver\u00b7stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "K\u00f6nnt' ich vom Gl\u00fccke mehr verlangen?", "tokens": ["K\u00f6nnt'", "ich", "vom", "Gl\u00fc\u00b7cke", "mehr", "ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So manchen Kranz es mir auch wand,", "tokens": ["So", "man\u00b7chen", "Kranz", "es", "mir", "auch", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den sch\u00f6nsten werd' ich heut empfangen.", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "werd'", "ich", "heut", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sieh hier dein trauliches Closett!", "tokens": ["Sieh", "hier", "dein", "trau\u00b7li\u00b7ches", "Clo\u00b7sett", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wie l\u00e4chelt dich es an! Wie nett", "tokens": ["Wie", "l\u00e4\u00b7chelt", "dich", "es", "an", "!", "Wie", "nett"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Hat sich's geschm\u00fcckt, dich zu empfangen!", "tokens": ["Hat", "sich's", "ge\u00b7schm\u00fcckt", ",", "dich", "zu", "emp\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wie still und \u00f6de war es hier,", "tokens": ["Wie", "still", "und", "\u00f6\u00b7de", "war", "es", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Seit deine Stimm' und dein Klavier", "tokens": ["Seit", "dei\u00b7ne", "Stimm'", "und", "dein", "Kla\u00b7vier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nicht mehr in diesen W\u00e4nden klangen!", "tokens": ["Nicht", "mehr", "in", "die\u00b7sen", "W\u00e4n\u00b7den", "klan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Jetzt hat sich's wieder neu belebt;", "tokens": ["Jetzt", "hat", "sich's", "wie\u00b7der", "neu", "be\u00b7lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Hoch \u00fcber deinem Kopfe schwebt", "tokens": ["Hoch", "\u00fc\u00b7ber", "dei\u00b7nem", "Kop\u00b7fe", "schwebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der Dohmpfaff, freundlich dich zu necken;", "tokens": ["Der", "Dohm\u00b7pfaff", ",", "freund\u00b7lich", "dich", "zu", "ne\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Das H\u00fcndchen zupft an deinen R\u00f6cken,", "tokens": ["Das", "H\u00fcnd\u00b7chen", "zupft", "an", "dei\u00b7nen", "R\u00f6\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Zu melden, da\u00df es auch noch lebt.", "tokens": ["Zu", "mel\u00b7den", ",", "da\u00df", "es", "auch", "noch", "lebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Zwar welch ein Abstand: dieser Raum,", "tokens": ["Zwar", "welch", "ein", "Ab\u00b7stand", ":", "die\u00b7ser", "Raum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWAT", "ART", "NN", "$.", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "(er fasset ja uns drei nur kaum,)", "tokens": ["(", "er", "fas\u00b7set", "ja", "uns", "drei", "nur", "kaum", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "PPER", "CARD", "ADV", "ADV", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und jener Prachtsal in Pyrmont,", "tokens": ["Und", "je\u00b7ner", "Prach\u00b7tsal", "in", "Pyr\u00b7mont", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Von Sternen ersten Rangs besonnt! \u2013", "tokens": ["Von", "Ster\u00b7nen", "ers\u00b7ten", "Rangs", "be\u00b7sonnt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Nicht wahr, noch d\u00fcnkt es dich ein Traum?", "tokens": ["Nicht", "wahr", ",", "noch", "d\u00fcnkt", "es", "dich", "ein", "Traum", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Dein K\u00f6nig selbst in diesem Kreise,", "tokens": ["Dein", "K\u00f6\u00b7nig", "selbst", "in", "die\u00b7sem", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Der bis zu dir herab sich l\u00e4\u00dft,", "tokens": ["Der", "bis", "zu", "dir", "her\u00b7ab", "sich", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Musik und Tanz, und Fest auf Fest! \u2013", "tokens": ["Mu\u00b7sik", "und", "Tanz", ",", "und", "Fest", "auf", "Fest", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "NN", "APPR", "NN", "$.", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.29": {"text": "Wie leicht bringt das nicht aus dem Gleise!", "tokens": ["Wie", "leicht", "bringt", "das", "nicht", "aus", "dem", "Glei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PDS", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.30": {"text": "Doch nun zur\u00fcck ins stille Nest!", "tokens": ["Doch", "nun", "zu\u00b7r\u00fcck", "ins", "stil\u00b7le", "Nest", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Hier bilde dich, o Wilhelmine!", "tokens": ["Hier", "bil\u00b7de", "dich", ",", "o", "Wil\u00b7hel\u00b7mi\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Sey mit dir selber hier zu Haus!", "tokens": ["Sey", "mit", "dir", "sel\u00b7ber", "hier", "zu", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.33": {"text": "Hier zieh den B\u00fcchern, wie die Biene", "tokens": ["Hier", "zieh", "den", "B\u00fc\u00b7chern", ",", "wie", "die", "Bie\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Den Blumen, ihren Honig aus.", "tokens": ["Den", "Blu\u00b7men", ",", "ih\u00b7ren", "Ho\u00b7nig", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Hier gib der Stimme Fl\u00f6tent\u00f6ne,", "tokens": ["Hier", "gib", "der", "Stim\u00b7me", "Fl\u00f6\u00b7ten\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Den Messingsaiten Silberklang;", "tokens": ["Den", "Mes\u00b7sings\u00b7ai\u00b7ten", "Sil\u00b7ber\u00b7klang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Hier sch\u00f6pfe aus der Hippokrene", "tokens": ["Hier", "sch\u00f6p\u00b7fe", "aus", "der", "Hip\u00b7po\u00b7kre\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Des Herzens, r\u00fchrenden Gesang.", "tokens": ["Des", "Her\u00b7zens", ",", "r\u00fch\u00b7ren\u00b7den", "Ge\u00b7sang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "La\u00df Flora's Garten, Circens Insel,", "tokens": ["La\u00df", "Flo\u00b7ra's", "Gar\u00b7ten", ",", "Cir\u00b7cens", "In\u00b7sel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NE", "NN", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Bald durch den Bleistift oder Pinsel,", "tokens": ["Bald", "durch", "den", "Blei\u00b7stift", "o\u00b7der", "Pin\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Bald durch der Nadel Kunst entstehn;", "tokens": ["Bald", "durch", "der", "Na\u00b7del", "Kunst", "ent\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Doch la\u00df dieselbe Hand auch willig,", "tokens": ["Doch", "la\u00df", "die\u00b7sel\u00b7be", "Hand", "auch", "wil\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PDAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Auf einem K\u00fcchentuch' von Zwillich", "tokens": ["Auf", "ei\u00b7nem", "K\u00fc\u00b7chen\u00b7tuch'", "von", "Zwil\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Die M\u00f6rserkeul' im Nothfall' drehn.", "tokens": ["Die", "M\u00f6r\u00b7ser\u00b7keul'", "im", "Noth\u00b7fall'", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Dem Gatten schmecken ", "tokens": ["Dem", "Gat\u00b7ten", "schme\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.46": {"text": "Dann zehnmal s\u00fc\u00dfer, liebes Kind!", "tokens": ["Dann", "zehn\u00b7mal", "s\u00fc\u00b7\u00dfer", ",", "lie\u00b7bes", "Kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Kurz, lerne nur die Kunst: Geschwind,", "tokens": ["Kurz", ",", "ler\u00b7ne", "nur", "die", "Kunst", ":", "Ge\u00b7schwind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "ADV", "ART", "NN", "$.", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Gleich einem Proteus, dich verwandeln,", "tokens": ["Gleich", "ei\u00b7nem", "Pro\u00b7teus", ",", "dich", "ver\u00b7wan\u00b7deln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Und, statt zu sprechen blo\u00df, auch handeln,", "tokens": ["Und", ",", "statt", "zu", "spre\u00b7chen", "blo\u00df", ",", "auch", "han\u00b7deln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "PTKZU", "VVINF", "ADV", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Weil That die Herzen nur gewinnt.", "tokens": ["Weil", "That", "die", "Her\u00b7zen", "nur", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Im L\u00e4rm' und Drang' der gro\u00dfen Welt", "tokens": ["Im", "L\u00e4rm'", "und", "Drang'", "der", "gro\u00b7\u00dfen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "L\u00e4\u00dft sich das Handeln schwer erlernen.", "tokens": ["L\u00e4\u00dft", "sich", "das", "Han\u00b7deln", "schwer", "er\u00b7ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Wen Lust, wen Zwang zur\u00fcck nicht h\u00e4lt,", "tokens": ["Wen", "Lust", ",", "wen", "Zwang", "zu\u00b7r\u00fcck", "nicht", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"