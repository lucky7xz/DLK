{"textgrid.poem.56154": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei allem Abschied voran, als w\u00e4re er hinter", "tokens": ["Sei", "al\u00b7lem", "Ab\u00b7schied", "vo\u00b7ran", ",", "als", "w\u00e4\u00b7re", "er", "hin\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "NN", "PTKVZ", "$,", "KOKOM", "VAFIN", "PPER", "APPR"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "dir, wie der Winter, der eben geht.", "tokens": ["dir", ",", "wie", "der", "Win\u00b7ter", ",", "der", "e\u00b7ben", "geht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn unter Wintern ist einer so endlos Winter,", "tokens": ["Denn", "un\u00b7ter", "Win\u00b7tern", "ist", "ei\u00b7ner", "so", "end\u00b7los", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "da\u00df, \u00fcberwinternd, dein Herz \u00fcberhaupt \u00fcbersteht.", "tokens": ["da\u00df", ",", "\u00fc\u00b7berw\u00b7in\u00b7ternd", ",", "dein", "Herz", "\u00fc\u00b7ber\u00b7haupt", "\u00fc\u00b7bers\u00b7teht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVPP", "$,", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sei immer tot in Eurydike \u2013, singender steige,", "tokens": ["Sei", "im\u00b7mer", "tot", "in", "Eu\u00b7ry\u00b7di\u00b7ke", "\u2013", ",", "sin\u00b7gen\u00b7der", "stei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "NE", "$(", "$,", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "preisender steige zur\u00fcck in den reinen Bezug.", "tokens": ["prei\u00b7sen\u00b7der", "stei\u00b7ge", "zu\u00b7r\u00fcck", "in", "den", "rei\u00b7nen", "Be\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Hier, unter Schwindenden, sei, im Reiche der Neige,", "tokens": ["Hier", ",", "un\u00b7ter", "Schwin\u00b7den\u00b7den", ",", "sei", ",", "im", "Rei\u00b7che", "der", "Nei\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "$,", "VAFIN", "$,", "APPRART", "NE", "ART", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "sei ein klingendes Glas, das sich im Klang schon zerschlug.", "tokens": ["sei", "ein", "klin\u00b7gen\u00b7des", "Glas", ",", "das", "sich", "im", "Klang", "schon", "zer\u00b7schlug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Sei \u2013 und wisse zugleich des Nicht-Seins Bedingung,", "tokens": ["Sei", "\u2013", "und", "wis\u00b7se", "zu\u00b7gleich", "des", "Nicht\u00b7Seins"