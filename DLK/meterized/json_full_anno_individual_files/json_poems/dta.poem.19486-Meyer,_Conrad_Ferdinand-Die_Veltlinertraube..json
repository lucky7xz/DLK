{"dta.poem.19486": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Veltlinertraube.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fctend liegt ein hei\u00dfes Schweigen", "tokens": ["Br\u00fc\u00b7tend", "liegt", "ein", "hei\u00b7\u00dfes", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Thal und Bergesjoch,", "tokens": ["Ue\u00b7ber", "Thal", "und", "Ber\u00b7ges\u00b7joch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Evo\u00eb und Winzerreigen", "tokens": ["E\u00b7vo\u00eb", "und", "Win\u00b7zer\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schlummern in der Traube noch.", "tokens": ["Schlum\u00b7mern", "in", "der", "Trau\u00b7be", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Purpurne Veltlinertraube,", "tokens": ["Pur\u00b7pur\u00b7ne", "Velt\u00b7li\u00b7ner\u00b7trau\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Kochend in der Sonne Schein,", "tokens": ["Ko\u00b7chend", "in", "der", "Son\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heute m\u00f6cht' ich unterm Laube", "tokens": ["Heu\u00b7te", "m\u00f6cht'", "ich", "un\u00b7term", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine vollste Beere sein!", "tokens": ["Dei\u00b7ne", "volls\u00b7te", "Bee\u00b7re", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein unb\u00e4ndiges Gebl\u00fcte,", "tokens": ["Mein", "un\u00b7b\u00e4n\u00b7di\u00b7ges", "Ge\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Strotzend von der Scholle Kraft,", "tokens": ["Strot\u00b7zend", "von", "der", "Schol\u00b7le", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trunken von des Himmels G\u00fcte,", "tokens": ["Trun\u00b7ken", "von", "des", "Him\u00b7mels", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprengte schier der H\u00fclse Haft!", "tokens": ["Spreng\u00b7te", "schier", "der", "H\u00fcl\u00b7se", "Haft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aus der Laube niederhangend,", "tokens": ["Aus", "der", "Lau\u00b7be", "nie\u00b7der\u00b7han\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gluthdurchwogt und \u00fcppig rund,", "tokens": ["Gluth\u00b7durch\u00b7wogt", "und", "\u00fcp\u00b7pig", "rund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwebt' ich dunkelpurpurprangend", "tokens": ["Schwebt'", "ich", "dun\u00b7kel\u00b7pur\u00b7pur\u00b7pran\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber einem rothen Mund!", "tokens": ["Ue\u00b7ber", "ei\u00b7nem", "ro\u00b7then", "Mund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}