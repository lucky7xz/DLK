{"textgrid.poem.39744": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wahrlich, auch die Zeitungsbl\u00e4tter", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahrlich, auch die Zeitungsbl\u00e4tter", "tokens": ["Wahr\u00b7lich", ",", "auch", "die", "Zei\u00b7tungs\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben heut' Aprilenwetter,", "tokens": ["Ha\u00b7ben", "heut'", "Ap\u00b7ri\u00b7len\u00b7wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gestern blies noch gar zu lind,", "tokens": ["Ge\u00b7stern", "blies", "noch", "gar", "zu", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gar zu lau darin der Wind.", "tokens": ["Gar", "zu", "lau", "da\u00b7rin", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "PAV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Selig hie\u00dfen die Monarchen,", "tokens": ["Se\u00b7lig", "hie\u00b7\u00dfen", "die", "Mon\u00b7ar\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df die Kriegesfurien schnarchen;", "tokens": ["Da\u00df", "die", "Krie\u00b7ges\u00b7fu\u00b7ri\u00b7en", "schnar\u00b7chen", ";"], "token_info": ["word", "word", "word"