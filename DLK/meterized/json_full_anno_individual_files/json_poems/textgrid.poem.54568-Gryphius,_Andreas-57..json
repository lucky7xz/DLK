{"textgrid.poem.54568": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Klesel f\u00fchlt er auch die bitter-s\u00fcsse Pein/", "tokens": ["Herr", "Kle\u00b7sel", "f\u00fchlt", "er", "auch", "die", "bit\u00b7ter\u00b7s\u00fcs\u00b7se", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit der die Liebe qu\u00e4lt? setzt dem verletzten Hertzen", "tokens": ["Mit", "der", "die", "Lie\u00b7be", "qu\u00e4lt", "?", "setzt", "dem", "ver\u00b7letz\u00b7ten", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "$.", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die strenge Hitze zu mit immer-neuem Schmertzen?", "tokens": ["Die", "stren\u00b7ge", "Hit\u00b7ze", "zu", "mit", "im\u00b7mer\u00b7neu\u00b7em", "Schmert\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hochwerthe Jungfrau Braut sie red ihm Trost-Wort ein:", "tokens": ["Hoch\u00b7wert\u00b7he", "Jung\u00b7frau", "Braut", "sie", "red", "ihm", "Trost\u00b7Wort", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PPER", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text"