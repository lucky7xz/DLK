{"dta.poem.5647": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sorten allzumal,", "tokens": ["Die", "Sor\u00b7ten", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der unterschiedlichen Gestallten ohne Zahl,", "tokens": ["Der", "un\u00b7ter\u00b7schied\u00b7li\u00b7chen", "Ge\u00b7stall\u00b7ten", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf so viel Art verschrenck\u2019t,", "tokens": ["Auf", "so", "viel", "Art", "ver\u00b7schren\u00b7ck't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die man in C\u00f6rpern glaubt zu finden,", "tokens": ["Die", "man", "in", "C\u00f6r\u00b7pern", "glaubt", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NE", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und alles, was man meint von ihnen zu ergr\u00fcnden,", "tokens": ["Und", "al\u00b7les", ",", "was", "man", "meint", "von", "ih\u00b7nen", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PIS", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bestehet eigentlich nur in dem Geist, der dencket.", "tokens": ["Be\u00b7ste\u00b7het", "ei\u00b7gent\u00b7lich", "nur", "in", "dem", "Geist", ",", "der", "den\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "So manche Art und Eigenschafften,", "tokens": ["So", "man\u00b7che", "Art", "und", "Ei\u00b7gen\u00b7schaff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die, wie man meint, an C\u00f6rpern hafften;", "tokens": ["Die", ",", "wie", "man", "meint", ",", "an", "C\u00f6r\u00b7pern", "haff\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PIS", "VVFIN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So manche Eigenschaft, und manche Art zu seyn,", "tokens": ["So", "man\u00b7che", "Ei\u00b7gen\u00b7schaft", ",", "und", "man\u00b7che", "Art", "zu", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "KON", "PIAT", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Trifft man allein", "tokens": ["Trifft", "man", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "In unsern Geistern an: Und der Ver\u00e4nderungen", "tokens": ["In", "un\u00b7sern", "Geis\u00b7tern", "an", ":", "Und", "der", "Ver\u00b7\u00e4n\u00b7de\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So mannichfaltige Beschaffenheit", "tokens": ["So", "man\u00b7nich\u00b7fal\u00b7ti\u00b7ge", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und Unterschiedlichkeit,", "tokens": ["Und", "Un\u00b7ter\u00b7schied\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "So aus den C\u00f6rpern je entsprungen,", "tokens": ["So", "aus", "den", "C\u00f6r\u00b7pern", "je", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die blos Bewegungen der Sinnen sind,", "tokens": ["Die", "blos", "Be\u00b7we\u00b7gun\u00b7gen", "der", "Sin\u00b7nen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Die werden blos im Geist das, was der Mensch empfindt.", "tokens": ["Die", "wer\u00b7den", "blos", "im", "Geist", "das", ",", "was", "der", "Mensch", "emp\u00b7findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPRART", "NN", "PDS", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}