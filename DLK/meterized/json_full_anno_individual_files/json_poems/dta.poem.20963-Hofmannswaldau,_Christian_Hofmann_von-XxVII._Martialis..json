{"dta.poem.20963": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XxVII.  \n Martialis.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der den ", "tokens": ["Der", "den"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Der mit der Musenschaar zu bulen hat begehret/", "tokens": ["Der", "mit", "der", "Mu\u00b7sen\u00b7schaar", "zu", "bu\u00b7len", "hat", "be\u00b7ge\u00b7hret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Vor dem Apollo selbst nicht allzusicher sa\u00df/", "tokens": ["Vor", "dem", "A\u00b7pol\u00b7lo", "selbst", "nicht", "all\u00b7zu\u00b7si\u00b7cher", "sa\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADV", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "K\u00fcst unter diesem Stein ein halb verfaultes Aa\u00df.", "tokens": ["K\u00fcst", "un\u00b7ter", "die\u00b7sem", "Stein", "ein", "halb", "ver\u00b7faul\u00b7tes", "Aa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}