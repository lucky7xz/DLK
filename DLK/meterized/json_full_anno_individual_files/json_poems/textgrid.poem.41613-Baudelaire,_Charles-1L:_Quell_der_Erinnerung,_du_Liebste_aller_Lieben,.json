bic.hexa"}, "line.4": {"text": "Mir war, als atmete ich deines Blutes Duft.", "tokens": ["Mir", "war", ",", "als", "at\u00b7me\u00b7te", "ich", "dei\u00b7nes", "Blu\u00b7tes", "Duft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie sind die Sonnen sch\u00f6n im warmen Abendblauen!", "tokens": ["Wie", "sind", "die", "Son\u00b7nen", "sch\u00f6n", "im", "war\u00b7men", "A\u00b7bend\u00b7blau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Nacht war um uns her, wie stiller Zelle Raum,", "tokens": ["Die", "Nacht", "war", "um", "uns", "her", ",", "wie", "stil\u00b7ler", "Zel\u00b7le", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "PTKVZ", "$,", "PWAV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchs Dunkel riet mein Blick noch deiner Augen S\u00fc\u00dfe,", "tokens": ["Durchs", "Dun\u00b7kel", "riet", "mein", "Blick", "noch", "dei\u00b7ner", "Au\u00b7gen", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und deinen Hauch trank ich \u2013 o Gift, o selger Traum!", "tokens": ["Und", "dei\u00b7nen", "Hauch", "trank", "ich", "\u2013", "o", "Gift", ",", "o", "sel\u00b7ger", "Tra