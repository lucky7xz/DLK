{"textgrid.poem.41613": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Quell der Erinnerung, du Liebste aller Lieben,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Quell der Erinnerung, du Liebste aller Lieben,", "tokens": ["Quell", "der", "E\u00b7rin\u00b7ne\u00b7rung", ",", "du", "Liebs\u00b7te", "al\u00b7ler", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "NN", "PIAT", "ADJA", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "O du, all meine Lust, o du, all meine Pflicht!", "tokens": ["O", "du", ",", "all", "mei\u00b7ne", "Lust", ",", "o", "du", ",", "all", "mei\u00b7ne", "Pflicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PIAT", "PPOSAT", "NN", "$,", "FM", "PPER", "$,", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist dir Gedenken an der K\u00fcsse Gl\u00fcck geblieben.", "tokens": ["Ist", "dir", "Ge\u00b7den\u00b7ken", "an", "der", "K\u00fcs\u00b7se", "Gl\u00fcck", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An W\u00e4rme des Kamins, an g\u00fctig Abendlicht?", "tokens": ["An", "W\u00e4r\u00b7me", "des", "Ka\u00b7mins", ",", "an", "g\u00fc\u00b7tig", "A\u00b7bend\u00b7licht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "APPR", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Quell der Erinnerung, du Liebste aller Lieben!", "tokens": ["Quell", "der", "E\u00b7rin\u00b7ne\u00b7rung", ",", "du", "Liebs\u00b7te", "al\u00b7ler", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "NN", "PIAT", "ADJA", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Die Abende erhellt von sanfter Kohlenglut,", "tokens": ["Die", "A\u00b7ben\u00b7de", "er\u00b7hellt", "von", "sanf\u00b7ter", "Koh\u00b7len\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die D\u00e4mmrung vom Balkon in rosger L\u00fcfte Wehen \u2013", "tokens": ["Die", "D\u00e4mm\u00b7rung", "vom", "Bal\u00b7kon", "in", "ros\u00b7ger", "L\u00fcf\u00b7te", "We\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wie war dein Busen s\u00fc\u00df, wie war dein Herz mir gut!", "tokens": ["Wie", "war", "dein", "Bu\u00b7sen", "s\u00fc\u00df", ",", "wie", "war", "dein", "Herz", "mir", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir sagten Dinge uns, die nimmermehr vergehen,", "tokens": ["Wir", "sag\u00b7ten", "Din\u00b7ge", "uns", ",", "die", "nim\u00b7mer\u00b7mehr", "ver\u00b7