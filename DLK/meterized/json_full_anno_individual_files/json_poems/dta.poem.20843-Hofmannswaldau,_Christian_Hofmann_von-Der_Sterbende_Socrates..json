{"dta.poem.20843": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn man die Lust in Gr\u00e4ntzen fast/", "tokens": ["Wenn", "man", "die", "Lust", "in", "Gr\u00e4nt\u00b7zen", "fast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unter vieler Regungs-Last/", "tokens": ["Und", "un\u00b7ter", "vie\u00b7ler", "Re\u00b7gungs\u00b7Last", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dazu uns Fleisch und Blut verleitet/", "tokens": ["Da\u00b7zu", "uns", "Fleisch", "und", "Blut", "ver\u00b7lei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seel in ihrer Freyheit h\u00e4lt/", "tokens": ["Die", "Seel", "in", "ih\u00b7rer", "Frey\u00b7heit", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und aus den schn\u00f6den Schrancken stelt/", "tokens": ["Und", "aus", "den", "schn\u00f6\u00b7den", "Schran\u00b7cken", "stelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da sie die Wollust stets bestreitet.", "tokens": ["Da", "sie", "die", "Wol\u00b7lust", "stets", "be\u00b7strei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}