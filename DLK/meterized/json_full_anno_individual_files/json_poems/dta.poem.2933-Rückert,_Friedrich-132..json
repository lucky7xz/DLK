{"dta.poem.2933": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "132.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Staunen seh' ich, da\u00df ihr zwei Gesichter macht,", "tokens": ["Mit", "Stau\u00b7nen", "seh'", "ich", ",", "da\u00df", "ihr", "zwei", "Ge\u00b7sich\u00b7ter", "macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein grollendes und eins das nur gezwungen lacht.", "tokens": ["Ein", "grol\u00b7len\u00b7des", "und", "eins", "das", "nur", "ge\u00b7zwun\u00b7gen", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "PIS", "ART", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer schuldig, frag' ich nicht, und wer unschuldig sei;", "tokens": ["Wer", "schul\u00b7dig", ",", "frag'", "ich", "nicht", ",", "und", "wer", "un\u00b7schul\u00b7dig", "sei", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "VVIMP", "PPER", "PTKNEG", "$,", "KON", "PWS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwei Liebende, entzweit, sind schuldig alle zwei.", "tokens": ["Zwei", "Lie\u00b7ben\u00b7de", ",", "ent\u00b7zweit", ",", "sind", "schul\u00b7dig", "al\u00b7le", "zwei", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "VVFIN", "$,", "VAFIN", "ADJD", "PIAT", "CARD", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Hab' ich in gleichem Fall nicht auch gemacht Gesichter?", "tokens": ["Hab'", "ich", "in", "glei\u00b7chem", "Fall", "nicht", "auch", "ge\u00b7macht", "Ge\u00b7sich\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "PTKNEG", "ADV", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Deswegen bin ich nur ein g\u00fcltigerer Richter.", "tokens": ["Des\u00b7we\u00b7gen", "bin", "ich", "nur", "ein", "g\u00fcl\u00b7ti\u00b7ge\u00b7rer", "Rich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mein Richterspruch ist, da\u00df ihr diesmal euch vers\u00f6hnt,", "tokens": ["Mein", "Rich\u00b7ter\u00b7spruch", "ist", ",", "da\u00df", "ihr", "dies\u00b7mal", "euch", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Gesichter euch in Zukunft abgew\u00f6hnt.", "tokens": ["Und", "die", "Ge\u00b7sich\u00b7ter", "euch", "in", "Zu\u00b7kunft", "ab\u00b7ge\u00b7w\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}