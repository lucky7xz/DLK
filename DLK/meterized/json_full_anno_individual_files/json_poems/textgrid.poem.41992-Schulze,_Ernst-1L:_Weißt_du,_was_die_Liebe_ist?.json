{"textgrid.poem.41992": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wei\u00dft du, was die Liebe ist?", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft du, was die Liebe ist?", "tokens": ["Wei\u00dft", "du", ",", "was", "die", "Lie\u00b7be", "ist", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, ein Kind mit leisen Schwingen;", "tokens": ["Ach", ",", "ein", "Kind", "mit", "lei\u00b7sen", "Schwin\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schw\u00e4rmend bald und bald voll Scherz", "tokens": ["Schw\u00e4r\u00b7mend", "bald", "und", "bald", "voll", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "KON", "ADV", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcht es sich in jedes Herz", "tokens": ["M\u00fcht", "es", "sich", "in", "je\u00b7des", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Los' und luftig einzudringen.", "tokens": ["Los'", "und", "luf\u00b7tig", "ein\u00b7zu\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zagend jetzt und jetzt voll Muth,", "tokens": ["Za\u00b7gend", "jetzt", "und", "jetzt", "voll", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KON", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Laulich jetzt und jetzt nur Gluth,", "tokens": ["Lau\u00b7lich", "jetzt", "und", "jetzt", "nur", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kennst du es? das ist die Liebe.", "tokens": ["Kennst", "du", "es", "?", "das", "ist", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$.", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sag mir, wo die Liebe wohnt?", "tokens": ["Sag", "mir", ",", "wo", "die", "Lie\u00b7be", "wohnt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Lenzes Duftgefilden", "tokens": ["In", "des", "Len\u00b7zes", "Duft\u00b7ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Baut sie sich ein gr\u00fcnes Haus,", "tokens": ["Baut", "sie", "sich", "ein", "gr\u00fc\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schm\u00fcckt es bunt mit Bl\u00fcthen aus", "tokens": ["Schm\u00fcckt", "es", "bunt", "mit", "Bl\u00fc\u00b7then", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit zarten Traumgebilden.", "tokens": ["Und", "mit", "zar\u00b7ten", "Traum\u00b7ge\u00b7bil\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach, du brauchst es nur zu sehn", "tokens": ["Ach", ",", "du", "brauchst", "es", "nur", "zu", "sehn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und schon haucht der Weste Wehn", "tokens": ["Und", "schon", "haucht", "der", "Wes\u00b7te", "Wehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dir ins Ohr: Hier wohnt die Liebe!", "tokens": ["Dir", "ins", "Ohr", ":", "Hier", "wohnt", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kennst du ihren Zeitvertreib?", "tokens": ["Kennst", "du", "ih\u00b7ren", "Zeit\u00b7ver\u00b7treib", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00e4ndelnd kos't sie mit dem Weste,", "tokens": ["T\u00e4n\u00b7delnd", "kos't", "sie", "mit", "dem", "Wes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wiegt sich auf der Bl\u00fcthen Duft,", "tokens": ["Wiegt", "sich", "auf", "der", "Bl\u00fc\u00b7then", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Baut sich tr\u00e4umend in der Luft", "tokens": ["Baut", "sich", "tr\u00e4u\u00b7mend", "in", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zauberische Goldpal\u00e4ste,", "tokens": ["Zau\u00b7be\u00b7ri\u00b7sche", "Gold\u00b7pa\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mischt zu Schmetterlingen sich:", "tokens": ["Mischt", "zu", "Schmet\u00b7ter\u00b7lin\u00b7gen", "sich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch nicht lange t\u00e4uscht sie dich,", "tokens": ["Doch", "nicht", "lan\u00b7ge", "t\u00e4uscht", "sie", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn ihr Spiel verr\u00e4th die Liebe.", "tokens": ["Denn", "ihr", "Spiel", "ver\u00b7r\u00e4\u00b7th", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch wie lebt das zarte Kind?", "tokens": ["Doch", "wie", "lebt", "das", "zar\u00b7te", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Wie ein Bienchen schwelgt sie immer", "tokens": ["Wie", "ein", "Bien\u00b7chen", "schwelgt", "sie", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Bl\u00fcthen weichem Schoo\u00df,", "tokens": ["In", "der", "Bl\u00fc\u00b7then", "wei\u00b7chem", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcttert sich mit D\u00fcften gro\u00df", "tokens": ["F\u00fct\u00b7tert", "sich", "mit", "D\u00fcf\u00b7ten", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit warmem Sonnenschimmer;", "tokens": ["Und", "mit", "war\u00b7mem", "Son\u00b7nen\u00b7schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Thr\u00e4nen die Aurora thaut", "tokens": ["Thr\u00e4\u00b7nen", "die", "Au\u00b7ro\u00b7ra", "thaut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NE", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Weste Klagelaut", "tokens": ["Und", "der", "Wes\u00b7te", "Kla\u00b7ge\u00b7laut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Sind die stete Kost der Liebe.", "tokens": ["Sind", "die", "ste\u00b7te", "Kost", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wei\u00df sie auch was Thr\u00e4nen sind?", "tokens": ["Wei\u00df", "sie", "auch", "was", "Thr\u00e4\u00b7nen", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PWS", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn des Lenzes Rosenwangen", "tokens": ["Wenn", "des", "Len\u00b7zes", "Ro\u00b7sen\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleichend nach und nach vergl\u00fchn,", "tokens": ["Blei\u00b7chend", "nach", "und", "nach", "ver\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "KON", "APPR", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Kr\u00e4nze nicht mehr bl\u00fchn,", "tokens": ["Wenn", "die", "Kr\u00e4n\u00b7ze", "nicht", "mehr", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die um seine Stirne prangen,", "tokens": ["Die", "um", "sei\u00b7ne", "Stir\u00b7ne", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn er scheidet von der Flur,", "tokens": ["Wenn", "er", "schei\u00b7det", "von", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, dann weinet die Natur,", "tokens": ["Ach", ",", "dann", "wei\u00b7net", "die", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und es weint mit ihr die Liebe.", "tokens": ["Und", "es", "weint", "mit", "ihr", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber kennt sie auch den Tod?", "tokens": ["A\u00b7ber", "kennt", "sie", "auch", "den", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn im Hain die St\u00fcrme brausen,", "tokens": ["Wenn", "im", "Hain", "die", "St\u00fcr\u00b7me", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn, vom rauhen Hauch ber\u00fchrt,", "tokens": ["Wenn", ",", "vom", "rau\u00b7hen", "Hauch", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder zarte Halm erfriert", "tokens": ["Je\u00b7der", "zar\u00b7te", "Halm", "er\u00b7friert"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und des Winters M\u00e4chte hausen,", "tokens": ["Und", "des", "Win\u00b7ters", "M\u00e4ch\u00b7te", "hau\u00b7sen", ","], "tok