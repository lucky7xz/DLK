{"dta.poem.11388": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den gl\u00fccklichen geburts-tag Jhro  \n Hochf\u00fcrstl. Durchl. Carl Philipp,  \n Pfaltzgr. am Rhein.  \n G. W. B. U. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.71", "sv:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott lob! da\u00df ich den tag, den sch\u00f6nen tag gefunden,", "tokens": ["Gott", "lob", "!", "da\u00df", "ich", "den", "tag", ",", "den", "sch\u00f6\u00b7nen", "tag", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "KOUS", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dich der welt geschenckt, und mir die s\u00fcssen stunden", "tokens": ["Der", "dich", "der", "welt", "ge\u00b7schenckt", ",", "und", "mir", "die", "s\u00fcs\u00b7sen", "stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,", "KON", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jtzt schenckt, o grosser Carl! in den ich deinem ruhm", "tokens": ["Jtzt", "schenckt", ",", "o", "gros\u00b7ser", "Carl", "!", "in", "den", "ich", "dei\u00b7nem", "ruhm"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "FM", "ADJA", "NE", "$.", "APPR", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum opffer bringen kan, was ihm zum eigenthum", "tokens": ["Zum", "opf\u00b7fer", "brin\u00b7gen", "kan", ",", "was", "ihm", "zum", "ei\u00b7gen\u00b7thum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$,", "PWS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aus mir, ja aus der brust der gantzen welt, geh\u00f6ret,", "tokens": ["Aus", "mir", ",", "ja", "aus", "der", "brust", "der", "gant\u00b7zen", "welt", ",", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein hertze, das den glantz von deiner tugend ehret.", "tokens": ["Ein", "hert\u00b7ze", ",", "das", "den", "glantz", "von", "dei\u00b7ner", "tu\u00b7gend", "eh\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zwar wird mein fl\u00f6ten-thon vor dich zu niedrig seyn:", "tokens": ["Zwar", "wird", "mein", "fl\u00f6\u00b7ten\u00b7thon", "vor", "dich", "zu", "nied\u00b7rig", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "PTKA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du bist in allem gro\u00df, ich bin in allem klein.", "tokens": ["Du", "bist", "in", "al\u00b7lem", "gro\u00df", ",", "ich", "bin", "in", "al\u00b7lem", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "ADJD", "$,", "PPER", "VAFIN", "APPR", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch wenn die furcht schon will den vorsatz hintertreiben,", "tokens": ["Doch", "wenn", "die", "furcht", "schon", "will", "den", "vor\u00b7satz", "hin\u00b7ter\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So heisset mich dennoch die liebe von dir schreiben.", "tokens": ["So", "heis\u00b7set", "mich", "den\u00b7noch", "die", "lie\u00b7be", "von", "dir", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein jeder redet wohl, wo so viel tugend steht:", "tokens": ["Ein", "je\u00b7der", "re\u00b7det", "wohl", ",", "wo", "so", "viel", "tu\u00b7gend", "steht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$,", "PWAV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zu preisen dich, o Held! ist jeder ein poet:", "tokens": ["Zu", "prei\u00b7sen", "dich", ",", "o", "Held", "!", "ist", "je\u00b7der", "ein", "poet", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "$,", "FM", "NN", "$.", "VAFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Die Musen d\u00f6rffen nicht die schwachen sinnen st\u00e4rcken.", "tokens": ["Die", "Mu\u00b7sen", "d\u00f6rf\u00b7fen", "nicht", "die", "schwa\u00b7chen", "sin\u00b7nen", "st\u00e4r\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was andre suchen weit, find ich in deinen wercken:", "tokens": ["Was", "and\u00b7re", "su\u00b7chen", "weit", ",", "find", "ich", "in", "dei\u00b7nen", "wer\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "ADJD", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich schau in einem tag in dir mehr wunder an,", "tokens": ["Ich", "schau", "in", "ei\u00b7nem", "tag", "in", "dir", "mehr", "wun\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als ich zu deinem lob im jahre schreiben kan.", "tokens": ["Als", "ich", "zu", "dei\u00b7nem", "lob", "im", "jah\u00b7re", "schrei\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Eh\u2019 ich dich seh, als kind, machst du dich schon zum helden:", "tokens": ["Eh'", "ich", "dich", "seh", ",", "als", "kind", ",", "machst", "du", "dich", "schon", "zum", "hel\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "KOUS", "NN", "$,", "VVFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Eh\u2019 ich dich ehr\u2019 als held, will mir dein thun schon melden,", "tokens": ["Eh'", "ich", "dich", "ehr'", "als", "held", ",", "will", "mir", "dein", "thun", "schon", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "KOKOM", "VVFIN", "$,", "VMFIN", "PPER", "PPOSAT", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Da\u00df du ein halb-gott bist: Du sitzst im sternen-dach,", "tokens": ["Da\u00df", "du", "ein", "halb\u00b7gott", "bist", ":", "Du", "sitz\u00b7st", "im", "ster\u00b7nen\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Eh\u2019 dir mein schneller sinn folgt auf der erde nach.", "tokens": ["Eh'", "dir", "mein", "schnel\u00b7ler", "sinn", "folgt", "auf", "der", "er\u00b7de", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Kaum kan ich deinen ruhm am Donau-strom erreichen;", "tokens": ["Kaum", "kan", "ich", "dei\u00b7nen", "ruhm", "am", "Do\u00b7nau\u00b7strom", "er\u00b7rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So prangst du schon am Rhein mit neuen sieges-zeichen.", "tokens": ["So", "prangst", "du", "schon", "am", "Rhein", "mit", "neu\u00b7en", "sie\u00b7ges\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wo es am sch\u00e4rffsten geht, da reist dein helden-sinn", "tokens": ["Wo", "es", "am", "sch\u00e4rffs\u00b7ten", "geht", ",", "da", "reist", "dein", "hel\u00b7den\u00b7sinn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vor allen anderen des kaysers schwager hin.", "tokens": ["Vor", "al\u00b7len", "an\u00b7de\u00b7ren", "des", "kay\u00b7sers", "schwa\u00b7ger", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PIS", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du streitest nur um ehr, und sch\u00e4tzest dich vergn\u00fcget,", "tokens": ["Du", "strei\u00b7test", "nur", "um", "ehr", ",", "und", "sch\u00e4t\u00b7zest", "dich", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,", "KON", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wann deine tapfferkeit vor fremde wohlfarth sieget:", "tokens": ["Wann", "dei\u00b7ne", "tapf\u00b7fer\u00b7keit", "vor", "frem\u00b7de", "wohl\u00b7farth", "sie\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du schweigst alleine still, wann jeder von dir spricht;", "tokens": ["Du", "schweigst", "al\u00b7lei\u00b7ne", "still", ",", "wann", "je\u00b7der", "von", "dir", "spricht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "PWAV", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Was jeder an dir sieht, siehst du alleine nicht.", "tokens": ["Was", "je\u00b7der", "an", "dir", "sieht", ",", "siehst", "du", "al\u00b7lei\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und wann in schmeicheley die meisten sich ergetzen,", "tokens": ["Und", "wann", "in", "schmei\u00b7che\u00b7ley", "die", "meis\u00b7ten", "sich", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NE", "ART", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Kan auch ein sittsam lob dein z\u00e4rtlich ohr verletzen.", "tokens": ["Kan", "auch", "ein", "sitt\u00b7sam", "lob", "dein", "z\u00e4rt\u00b7lich", "ohr", "ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJD", "NN", "PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Durch dieses hast du dir gemachet eine bahn,", "tokens": ["Durch", "die\u00b7ses", "hast", "du", "dir", "ge\u00b7ma\u00b7chet", "ei\u00b7ne", "bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "PPER", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die zwar ein jeder sucht, doch keiner finden kan;", "tokens": ["Die", "zwar", "ein", "je\u00b7der", "sucht", ",", "doch", "kei\u00b7ner", "fin\u00b7den", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "PIS", "VVFIN", "$,", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und sind sie jemand schon, hast du den lauff vollendet,", "tokens": ["Und", "sind", "sie", "je\u00b7mand", "schon", ",", "hast", "du", "den", "lauff", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIS", "ADV", "$,", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Eh\u2019 er den schweren fu\u00df zum ersten schritt gewendet.", "tokens": ["Eh'", "er", "den", "schwe\u00b7ren", "fu\u00df", "zum", "ers\u00b7ten", "schritt", "ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "PTKVZ", "APPRART", "ADJA", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ja eh\u2019 ein lorbeer-zweig um deine stirn vergr\u00fcnt,", "tokens": ["Ja", "eh'", "ein", "lor\u00b7beer\u00b7zweig", "um", "dei\u00b7ne", "stirn", "ver\u00b7gr\u00fcnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Hast du den andern schon l\u00e4ngst wiederum verdient.", "tokens": ["Hast", "du", "den", "an\u00b7dern", "schon", "l\u00e4ngst", "wie\u00b7de\u00b7rum", "ver\u00b7dient", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der erden gr\u00f6ster gott verg\u00f6ttert dein gebl\u00fcthe;", "tokens": ["Der", "er\u00b7den", "gr\u00f6s\u00b7ter", "gott", "ver\u00b7g\u00f6t\u00b7tert", "dein", "ge\u00b7bl\u00fc\u00b7the", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Doch aber noch vielmehr dein himmlisches gem\u00fcthe:", "tokens": ["Doch", "a\u00b7ber", "noch", "viel\u00b7mehr", "dein", "himm\u00b7li\u00b7sches", "ge\u00b7m\u00fc\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dein haus ist hoch genung; Doch wenn es m\u00f6glich w\u00e4r,", "tokens": ["Dein", "haus", "ist", "hoch", "ge\u00b7nung", ";", "Doch", "wenn", "es", "m\u00f6g\u00b7lich", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "$.", "KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df es k\u00f6nt h\u00f6her seyn, so k\u00e4m es von dir her.", "tokens": ["Da\u00df", "es", "k\u00f6nt", "h\u00f6\u00b7her", "seyn", ",", "so", "k\u00e4m", "es", "von", "dir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "VAINF", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ja wenn annoch die zeit, die goldne zeit, regierte,", "tokens": ["Ja", "wenn", "an\u00b7noch", "die", "zeit", ",", "die", "gold\u00b7ne", "zeit", ",", "re\u00b7gier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "KOUS", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.42": {"text": "Da wei\u00dfheit und verdienst allein zum throne f\u00fchrte,", "tokens": ["Da", "wei\u00df\u00b7heit", "und", "ver\u00b7dienst", "al\u00b7lein", "zum", "thro\u00b7ne", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VVFIN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und sich des purpurs pracht nur schlo\u00df in tugend ein;", "tokens": ["Und", "sich", "des", "pur\u00b7purs", "pracht", "nur", "schlo\u00df", "in", "tu\u00b7gend", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So w\u00fcrdest du schon l\u00e4ngst ein grosser k\u00f6nig seyn;", "tokens": ["So", "w\u00fcr\u00b7dest", "du", "schon", "l\u00e4ngst", "ein", "gros\u00b7ser", "k\u00f6\u00b7nig", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Doch ob dich schon kein reich zu dieser ehr erhaben,", "tokens": ["Doch", "ob", "dich", "schon", "kein", "reich", "zu", "die\u00b7ser", "ehr", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIAT", "ADJD", "APPR", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "So bist du es gleichwohl nach deinen hohen gaben.", "tokens": ["So", "bist", "du", "es", "gleich\u00b7wohl", "nach", "dei\u00b7nen", "ho\u00b7hen", "ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wer, grosser Carl! dich kennt, wer dich nur siehet an,", "tokens": ["Wer", ",", "gros\u00b7ser", "Carl", "!", "dich", "kennt", ",", "wer", "dich", "nur", "sie\u00b7het", "an", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "ADJA", "NE", "$.", "PPER", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Der nennt alsbald mit lust sich deinen unterthan.", "tokens": ["Der", "nennt", "als\u00b7bald", "mit", "lust", "sich", "dei\u00b7nen", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "VVFIN", "PRF", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Je wen\u2019ger d\u00fcnckst du dich, je mehr du bist erhoben:", "tokens": ["Je", "wen'\u00b7ger", "d\u00fcnckst", "du", "dich", ",", "je", "mehr", "du", "bist", "er\u00b7ho\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "PRF", "$,", "ADV", "ADV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Was vor ein fremder wird dich sehen, und nicht loben?", "tokens": ["Was", "vor", "ein", "frem\u00b7der", "wird", "dich", "se\u00b7hen", ",", "und", "nicht", "lo\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "VAFIN", "PPER", "VVINF", "$,", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Indem sich deine gunst so gn\u00e4dig auf ihn kehrt,", "tokens": ["In\u00b7dem", "sich", "dei\u00b7ne", "gunst", "so", "gn\u00e4\u00b7dig", "auf", "ihn", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Als wie das tieffste thal der sonnen strahl durchf\u00e4hrt,", "tokens": ["Als", "wie", "das", "tieffs\u00b7te", "thal", "der", "son\u00b7nen", "strahl", "durch\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Und dennoch, ob er schon die niedrigkeit ber\u00fchret,", "tokens": ["Und", "den\u00b7noch", ",", "ob", "er", "schon", "die", "nied\u00b7rig\u00b7keit", "be\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Von seiner majest\u00e4t dabey doch nichts verliehret.", "tokens": ["Von", "sei\u00b7ner", "ma\u00b7jes\u00b7t\u00e4t", "da\u00b7bey", "doch", "nichts", "ver\u00b7lieh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PAV", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "O gro\u00df-gesinnter Held! hier schweigt die feder still,", "tokens": ["O", "gro\u00df\u00b7ge\u00b7sinn\u00b7ter", "Held", "!", "hier", "schweigt", "die", "fe\u00b7der", "still", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Dieweil das hertze selbst zu sylben werden will,", "tokens": ["Die\u00b7weil", "das", "hert\u00b7ze", "selbst", "zu", "syl\u00b7ben", "wer\u00b7den", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PTKZU", "VVINF", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Ja gar die seel zum reim, wenn sie bey sich erweget,", "tokens": ["Ja", "gar", "die", "seel", "zum", "reim", ",", "wenn", "sie", "bey", "sich", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "APPRART", "NN", "$,", "KOUS", "PPER", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.58": {"text": "Wie lieblich deine gunst mich zu bestrahlen pfleget.", "tokens": ["Wie", "lieb\u00b7lich", "dei\u00b7ne", "gunst", "mich", "zu", "be\u00b7strah\u00b7len", "pfle\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Wie offt hab ich gesagt: Rom liebte den August,", "tokens": ["Wie", "offt", "hab", "ich", "ge\u00b7sagt", ":", "Rom", "lieb\u00b7te", "den", "Au\u00b7gust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$.", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Doch Titus ward auf ihn genennt des landes lust;", "tokens": ["Doch", "Ti\u00b7tus", "ward", "auf", "ihn", "ge\u00b7nennt", "des", "lan\u00b7des", "lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Di\u00df aber wei\u00df ich wohl, da\u00df niemand auf der erden,", "tokens": ["Di\u00df", "a\u00b7ber", "wei\u00df", "ich", "wohl", ",", "da\u00df", "nie\u00b7mand", "auf", "der", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Wie unser grosser Carl, geliebt wird jemahls werden.", "tokens": ["Wie", "un\u00b7ser", "gros\u00b7ser", "Carl", ",", "ge\u00b7liebt", "wird", "je\u00b7mahls", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NE", "$,", "VVPP", "VAFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Und wenn ich di\u00df gesagt, klag ich mich selber an,", "tokens": ["Und", "wenn", "ich", "di\u00df", "ge\u00b7sagt", ",", "klag", "ich", "mich", "sel\u00b7ber", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDS", "VVPP", "$,", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Da\u00df ich zur gn\u00fcge nicht dein lob ausbreiten kan,", "tokens": ["Da\u00df", "ich", "zur", "gn\u00fc\u00b7ge", "nicht", "dein", "lob", "aus\u00b7brei\u00b7ten", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Und da\u00df in meiner brust nicht tausend hertzen leben,", "tokens": ["Und", "da\u00df", "in", "mei\u00b7ner", "brust", "nicht", "tau\u00b7send", "hert\u00b7zen", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "PTKNEG", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Um tausend hertzen dir zum opffer hinzugeben.", "tokens": ["Um", "tau\u00b7send", "hert\u00b7zen", "dir", "zum", "opf\u00b7fer", "hin\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PPER", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Wiewohl, der himmel schon hat meinen wunsch ersetzt,", "tokens": ["Wie\u00b7wohl", ",", "der", "him\u00b7mel", "schon", "hat", "mei\u00b7nen", "wunsch", "er\u00b7setzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ART", "NN", "ADV", "VAFIN", "PPOSAT", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Wenn er dir eines schenckt, was man vor tausend sch\u00e4tzt,", "tokens": ["Wenn", "er", "dir", "ei\u00b7nes", "schenckt", ",", "was", "man", "vor", "tau\u00b7send", "sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVFIN", "$,", "PRELS", "PIS", "APPR", "CARD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Indem Theresia ihr edles hertz dir giebet,", "tokens": ["In\u00b7dem", "The\u00b7re\u00b7sia", "ihr", "ed\u00b7les", "hertz", "dir", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.70": {"text": "Und dich, o krieges-gott! der liebe mutter liebet.", "tokens": ["Und", "dich", ",", "o", "krie\u00b7ge\u00b7sgott", "!", "der", "lie\u00b7be", "mut\u00b7ter", "lie\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "O sch\u00f6n! o gleiches paar! du stellest uns in dir", "tokens": ["O", "sch\u00f6n", "!", "o", "glei\u00b7ches", "paar", "!", "du", "stel\u00b7lest", "uns", "in", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$.", "FM", "FM", "FM", "$.", "PPER", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Der tugend ebenbild, der sch\u00f6nheit abri\u00df f\u00fcr:", "tokens": ["Der", "tu\u00b7gend", "e\u00b7ben\u00b7bild", ",", "der", "sch\u00f6n\u00b7heit", "ab\u00b7ri\u00df", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Was man in einem sucht, kan man in beyden finden,", "tokens": ["Was", "man", "in", "ei\u00b7nem", "sucht", ",", "kan", "man", "in", "bey\u00b7den", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "ART", "VVFIN", "$,", "VMFIN", "PIS", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Weil sich in deinem ja verdienst und lohn verbinden.", "tokens": ["Weil", "sich", "in", "dei\u00b7nem", "ja", "ver\u00b7dienst", "und", "lohn", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "PPOSAT", "ADV", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Was aber s\u00e4umst du doch, zu zeigen uns das gl\u00fcck,", "tokens": ["Was", "a\u00b7ber", "s\u00e4umst", "du", "doch", ",", "zu", "zei\u00b7gen", "uns", "das", "gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ADV", "$,", "PTKZU", "VVINF", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Das durch verzug ein jahr uns macht zum angenblick?", "tokens": ["Das", "durch", "ver\u00b7zug", "ein", "jahr", "uns", "macht", "zum", "an\u00b7gen\u00b7blick", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Ach! la\u00df die pfeiler nicht von unsrer hoffnung wancken,", "tokens": ["Ach", "!", "la\u00df", "die", "pfei\u00b7ler", "nicht", "von", "uns\u00b7rer", "hoff\u00b7nung", "wan\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Und tr\u00f6st uns in der that, und nicht nur in gedancken!", "tokens": ["Und", "tr\u00f6st", "uns", "in", "der", "that", ",", "und", "nicht", "nur", "in", "ge\u00b7dan\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "VVFIN", "$,", "KON", "PTKNEG", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Ey brich, o werther tag! ey brich doch einmahl an,", "tokens": ["Ey", "brich", ",", "o", "wert\u00b7her", "tag", "!", "ey", "brich", "doch", "ein\u00b7mahl", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "FM", "ADJA", "NN", "$.", "NE", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Damit ich bald an dir vergn\u00fcget sagen kan:", "tokens": ["Da\u00b7mit", "ich", "bald", "an", "dir", "ver\u00b7gn\u00fc\u00b7get", "sa\u00b7gen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Da\u00df ich zwey gute tag in Schlesien gezehlet,", "tokens": ["Da\u00df", "ich", "zwey", "gu\u00b7te", "tag", "in", "Schle\u00b7si\u00b7en", "ge\u00b7zeh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Der unsern Carl uns schenckt, der unsern Carl verm\u00e4hlet.", "tokens": ["Der", "un\u00b7sern", "Carl", "uns", "schenckt", ",", "der", "un\u00b7sern", "Carl", "ver\u00b7m\u00e4h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}