{"textgrid.poem.43966": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich seh dich zwar, du angenehmer Morgen,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh dich zwar, du angenehmer Morgen,", "tokens": ["Ich", "seh", "dich", "zwar", ",", "du", "an\u00b7ge\u00b7neh\u00b7mer", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zwar nicht sonder Z\u00e4rtligkeit,", "tokens": ["Und", "zwar", "nicht", "son\u00b7der", "Z\u00e4rt\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und diese zwar zu Lust und Leid", "tokens": ["Und", "die\u00b7se", "zwar", "zu", "Lust", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergangner Ruh und gegenw\u00e4rtger Sorgen;", "tokens": ["Ver\u00b7gang\u00b7ner", "Ruh", "und", "ge\u00b7gen\u00b7w\u00e4rt\u00b7ger", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn wenn bey deinem Blick mir ins Ged\u00e4chtn\u00fc\u00df f\u00e4llt,", "tokens": ["Denn", "wenn", "bey", "dei\u00b7nem", "Blick", "mir", "ins", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie oft dein holder Stern auf Leonorens Wangen", "tokens": ["Wie", "oft", "dein", "hol\u00b7der", "Stern", "auf", "Le\u00b7o\u00b7no\u00b7rens", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch seinen Widerschein mir doppelt aufgegangen,", "tokens": ["Durch", "sei\u00b7nen", "Wi\u00b7der\u00b7schein", "mir", "dop\u00b7pelt", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So f\u00fchl ich einen Trost, der Noth und Kummer h\u00e4lt.", "tokens": ["So", "f\u00fchl", "ich", "ei\u00b7nen", "Trost", ",", "der", "Noth", "und", "Kum\u00b7mer", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich lies den Schlaf vergebens auf mich warthen,", "tokens": ["Ich", "lies", "den", "Schlaf", "ver\u00b7ge\u00b7bens", "auf", "mich", "wart\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn mein Flei\u00df die finstre Nacht", "tokens": ["Und", "wenn", "mein", "Flei\u00df", "die", "finst\u00b7re", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Ku\u00df und B\u00fcchern zugebracht,", "tokens": ["Mit", "Ku\u00df", "und", "B\u00fc\u00b7chern", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So zogstu mich gleichwohl noch in den Garthen;", "tokens": ["So", "zogs\u00b7tu", "mich", "gleich\u00b7wohl", "noch", "in", "den", "Gar\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da tr\u00e4ufelte mir erst das s\u00fc\u00dfe Mannabrodt", "tokens": ["Da", "tr\u00e4u\u00b7fel\u00b7te", "mir", "erst", "das", "s\u00fc\u00b7\u00dfe", "Man\u00b7na\u00b7brodt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch reicher als dein Thau vom allerliebsten Munde,", "tokens": ["Noch", "rei\u00b7cher", "als", "dein", "Thau", "vom", "al\u00b7ler\u00b7liebs\u00b7ten", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da macht ich oftermahls mit unserm s\u00fc\u00dfen Bunde,", "tokens": ["Da", "macht", "ich", "of\u00b7ter\u00b7mahls", "mit", "un\u00b7serm", "s\u00fc\u00b7\u00dfen", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich glaub aus Eifersucht, Auroren noch so roth.", "tokens": ["Ich", "glaub", "aus", "Ei\u00b7fer\u00b7sucht", ",", "Au\u00b7ro\u00b7ren", "noch", "so", "roth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "NE", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dies war ein Rest der ehmals g\u00fcldnen Zeiten,", "tokens": ["Dies", "war", "ein", "Rest", "der", "eh\u00b7mals", "g\u00fcld\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die blos die Liebe wieder schenckt,", "tokens": ["Die", "blos", "die", "Lie\u00b7be", "wie\u00b7der", "schenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebe, so auf nichts gedenckt,", "tokens": ["Die", "Lie\u00b7be", ",", "so", "auf", "nichts", "ge\u00b7denckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als durch die Bahn des Lebens froh zu schreiten.", "tokens": ["Als", "durch", "die", "Bahn", "des", "Le\u00b7bens", "froh", "zu", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da hatt ich noch ein Herz, dem kont ich mich vertraun,", "tokens": ["Da", "hatt", "ich", "noch", "ein", "Herz", ",", "dem", "kont", "ich", "mich", "ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PDS", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da scheut ich keinen Fall, der unser treu Gespr\u00e4che", "tokens": ["Da", "scheut", "ich", "kei\u00b7nen", "Fall", ",", "der", "un\u00b7ser", "treu", "Ge\u00b7spr\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "PRELS", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch Argwohn oder Neid und L\u00fcgen unterbr\u00e4che;", "tokens": ["Durch", "Arg\u00b7wohn", "o\u00b7der", "Neid", "und", "L\u00fc\u00b7gen", "un\u00b7ter\u00b7br\u00e4\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da sprach ich oft mit Recht: Hier last uns H\u00fctten baun!", "tokens": ["Da", "sprach", "ich", "oft", "mit", "Recht", ":", "Hier", "last", "uns", "H\u00fct\u00b7ten", "baun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$.", "ADV", "VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da sagt ich ihr die heimlichsten Gedancken,", "tokens": ["Da", "sagt", "ich", "ihr", "die", "heim\u00b7lichs\u00b7ten", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und was auch ihr von Freud und Gram", "tokens": ["Und", "was", "auch", "ihr", "von", "Freud", "und", "Gram"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst niemahls auf die Zunge kam,", "tokens": ["Sonst", "nie\u00b7mahls", "auf", "die", "Zun\u00b7ge", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das brach vor mir des Herzens enge Schrancken;", "tokens": ["Das", "brach", "vor", "mir", "des", "Her\u00b7zens", "en\u00b7ge", "Schran\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Geister \u00fcbten sich bey selbst gela\u00dfner Ruh,", "tokens": ["Die", "Geis\u00b7ter", "\u00fcb\u00b7ten", "sich", "bey", "selbst", "ge\u00b7la\u00df\u00b7ner", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An Scherz und Redligkeit einander zu besiegen,", "tokens": ["An", "Scherz", "und", "Red\u00b7lig\u00b7keit", "ein\u00b7an\u00b7der", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Leiber wusten auch ihr Theil davon zu kriegen", "tokens": ["Die", "Lei\u00b7ber", "wus\u00b7ten", "auch", "ihr", "Theil", "da\u00b7von", "zu", "krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "PAV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sazten durch den Ku\u00df einander feurig zu.", "tokens": ["Und", "saz\u00b7ten", "durch", "den", "Ku\u00df", "ein\u00b7an\u00b7der", "feu\u00b7rig", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ach Schweidniz, ach du Bild von Salems Thoren,", "tokens": ["Ach", "Schweid\u00b7niz", ",", "ach", "du", "Bild", "von", "Sa\u00b7lems", "Tho\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "XY", "PPER", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Lustplaz meiner jungen Zeit,", "tokens": ["Du", "Lust\u00b7plaz", "mei\u00b7ner", "jun\u00b7gen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sich den Musen ganz geweiht,", "tokens": ["Die", "sich", "den", "Mu\u00b7sen", "ganz", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was hab ich nicht mit dir vor Fried und Heil verloren!", "tokens": ["Was", "hab", "ich", "nicht", "mit", "dir", "vor", "Fried", "und", "Heil", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich seh durch Thr\u00e4n und Angst, und sieh, du bist nicht da,", "tokens": ["Ich", "seh", "durch", "Thr\u00e4n", "und", "Angst", ",", "und", "sieh", ",", "du", "bist", "nicht", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "KON", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Tages tausendmahl mit gr\u00f6\u00dfrer Angst zur\u00fccke", "tokens": ["Des", "Ta\u00b7ges", "tau\u00b7send\u00b7mahl", "mit", "gr\u00f6\u00df\u00b7rer", "Angst", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als jen gefangnes Volck, das mit betr\u00fcbtem Blicke", "tokens": ["Als", "jen", "ge\u00b7fang\u00b7nes", "Volck", ",", "das", "mit", "be\u00b7tr\u00fcb\u00b7tem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Gegend Canaans aus Babels Fenstern sah.", "tokens": ["Die", "Ge\u00b7gend", "Ca\u00b7naans", "aus", "Ba\u00b7bels", "Fens\u00b7tern", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Jezt hab ich nichts, Verdru\u00df und Angst zu stillen,", "tokens": ["Jezt", "hab", "ich", "nichts", ",", "Ver\u00b7dru\u00df", "und", "Angst", "zu", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als etwan die Verzweiflungslust;", "tokens": ["Als", "et\u00b7wan", "die", "Ver\u00b7zwei\u00b7flungs\u00b7lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Jedoch was qu\u00e4l ich selbst die Brust?", "tokens": ["Je\u00b7doch", "was", "qu\u00e4l", "ich", "selbst", "die", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verliert euch nur, ihr angenehmen Grillen,", "tokens": ["Ver\u00b7liert", "euch", "nur", ",", "ihr", "an\u00b7ge\u00b7neh\u00b7men", "Gril\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verliert euch, bis mir einst ein be\u00dfrer Gl\u00fccksstern scheint.", "tokens": ["Ver\u00b7liert", "euch", ",", "bis", "mir", "einst", "ein", "be\u00df\u00b7rer", "Gl\u00fccks\u00b7stern", "scheint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "wor