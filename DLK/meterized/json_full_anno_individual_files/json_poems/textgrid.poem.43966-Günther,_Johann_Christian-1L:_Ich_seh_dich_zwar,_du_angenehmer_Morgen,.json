it,", "tokens": ["Und", "zwar", "nicht", "son\u00b7der", "Z\u00e4rt\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und diese zwar zu Lust und Leid", "tokens": ["Und", "die\u00b7se", "zwar", "zu", "Lust", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergangner Ruh und gegenw\u00e4rtger Sorgen;", "tokens": ["Ver\u00b7gang\u00b7ner", "Ruh", "und", "ge\u00b7gen\u00b7w\u00e4rt\u00b7ger", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn wenn bey deinem Blick mir ins Ged\u00e4chtn\u00fc\u00df f\u00e4llt,", "tokens": ["Denn", "wenn", "bey", "dei\u00b7nem", "Blick", "mir", "ins", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie oft dein holder Stern auf Leonorens Wangen", "tokens": ["Wie", "oft", "dein", "hol\u00b7der", "Stern", "auf", "Le\u00b7o\u00b7no\u00b7rens", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch seinen Widerschein mir doppelt aufgegangen,", "tokens": ["Durch", "sei\u00b7nen", "Wi\u00b7der\u00b7schein", "mir", "dop\u00b7pelt", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["wo