{"dta.poem.18223": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ringeln wie Opferduft", "tokens": ["Rin\u00b7geln", "wie", "Op\u00b7fer\u00b7duft"], "token_info": ["word", "word", "word"], "pos": ["NN", "KOKOM", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sich aus dem Erdengetriebe", "tokens": ["Sich", "aus", "dem", "Er\u00b7den\u00b7ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Empor in die Gottesluft", "tokens": ["Em\u00b7por", "in", "die", "Got\u00b7tes\u00b7luft"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und tragen auf ihren Schwingen,", "tokens": ["Und", "tra\u00b7gen", "auf", "ih\u00b7ren", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dem Alltagsleben entflohn,", "tokens": ["Dem", "All\u00b7tags\u00b7le\u00b7ben", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Die Seele mit Singen und Klingen", "tokens": ["Die", "See\u00b7le", "mit", "Sin\u00b7gen", "und", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Bis hinauf zu des Vaters Thron.", "tokens": ["Bis", "hin\u00b7auf", "zu", "des", "Va\u00b7ters", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wenn dann die Zeit erf\u00fcllet,", "tokens": ["Und", "wenn", "dann", "die", "Zeit", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.9": {"text": "Flattert sie erdenw\u00e4rts,", "tokens": ["Flat\u00b7tert", "sie", "er\u00b7den\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Und was ihr der Himmel enth\u00fcllet,", "tokens": ["Und", "was", "ihr", "der", "Him\u00b7mel", "ent\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Fl\u00fcstert sie dir in das Herz.", "tokens": ["Fl\u00fcs\u00b7tert", "sie", "dir", "in", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und dieses stimmt dann wieder", "tokens": ["Und", "die\u00b7ses", "stimmt", "dann", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Saiten zum h\u00f6heren Chor,", "tokens": ["Die", "Sai\u00b7ten", "zum", "h\u00f6\u00b7he\u00b7ren", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und jubelnd steigen die Lieder", "tokens": ["Und", "ju\u00b7belnd", "stei\u00b7gen", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von neuem zum Himmel empor.", "tokens": ["Von", "neu\u00b7em", "zum", "Him\u00b7mel", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Und so in stetem Kreisen,", "tokens": ["Und", "so", "in", "ste\u00b7tem", "Krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "T\u00f6nend durch Raum und Zeit,", "tokens": ["T\u00f6\u00b7nend", "durch", "Raum", "und", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Verhallen nie die Weisen,", "tokens": ["Ver\u00b7hal\u00b7len", "nie", "die", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Sie schweben und flattern voll Wonne", "tokens": ["Sie", "schwe\u00b7ben", "und", "flat\u00b7tern", "voll", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Den leuchtenden Wolken zu \u2014", "tokens": ["Den", "leuch\u00b7ten\u00b7den", "Wol\u00b7ken", "zu"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bist immer und ewig du!", "tokens": ["Bist", "im\u00b7mer", "und", "e\u00b7wig", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "ADJD", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}