{"textgrid.poem.46250": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sah, als ihr gesicht, der morgenr\u00f6tin gleich,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah, als ihr gesicht, der morgenr\u00f6tin gleich,", "tokens": ["Ich", "sah", ",", "als", "ihr", "ge\u00b7sicht", ",", "der", "mor\u00b7gen\u00b7r\u00f6\u00b7tin", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$,", "PRELS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "als ihre zwillingbrust, so wei\u00df als schnee zu sehen,", "tokens": ["als", "ih\u00b7re", "zwil\u00b7ling\u00b7brust", ",", "so", "wei\u00df", "als", "schnee", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "KOKOM", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und ihren glatten hals vil tausend ringlein reich", "tokens": ["und", "ih\u00b7ren", "glat\u00b7ten", "hals", "vil", "tau\u00b7send", "rin\u00b7glein", "reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "CARD", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "von ihrem krausen gold umgeben, sie aufstehen.", "tokens": ["von", "ih\u00b7rem", "krau\u00b7sen", "gold", "um\u00b7ge\u00b7ben", ",", "sie", "auf\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aufstehen sah ich sie, so kunstlos als liebreich,", "tokens": ["Auf\u00b7ste\u00b7hen", "sah", "ich", "sie", ",", "so", "kunst\u00b7los", "als", "lieb\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "mit solcher sch\u00f6nheit schatz ohn m\u00fch, ohn sorg versehen,", "tokens": ["mit", "sol\u00b7cher", "sch\u00f6n\u00b7heit", "schatz", "ohn", "m\u00fch", ",", "ohn", "sorg", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "APPR", "ADJD", "$,", "KOUI", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "da\u00df sie so sch\u00f6n, so fr\u00fch in der lieb k\u00f6nigreich", "tokens": ["da\u00df", "sie", "so", "sch\u00f6n", ",", "so", "fr\u00fch", "in", "der", "lieb", "k\u00f6\u00b7nig\u00b7reich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "APPR", "ART", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "kont andren um mittag gezieret weit vorgehen.", "tokens": ["kont", "an\u00b7dren", "um", "mit\u00b7tag", "ge\u00b7zie\u00b7ret", "weit", "vor\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "VVPP", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Alsbald ich sie ersah, \u00bbO wunder!\u00ab schrie ich bald:", "token