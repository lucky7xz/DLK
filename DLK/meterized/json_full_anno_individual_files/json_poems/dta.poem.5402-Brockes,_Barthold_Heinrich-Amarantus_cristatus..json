{"dta.poem.5402": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Amarantus cristatus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch kann man sonder Lust nicht sehn,", "tokens": ["Noch", "kann", "man", "son\u00b7der", "Lust", "nicht", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sonderlich geformt, wie sch\u00f6n", "tokens": ["Wie", "son\u00b7der\u00b7lich", "ge\u00b7formt", ",", "wie", "sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "VVPP", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der purpurfarben\u2019 Amarant,", "tokens": ["Der", "pur\u00b7pur\u00b7fa\u00b7rben'", "A\u00b7ma\u00b7rant", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der insgemein cristatus wird genannt.", "tokens": ["Der", "ins\u00b7ge\u00b7mein", "cris\u00b7ta\u00b7tus", "wird", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er hat fast keine Form; sein Blatt besteht aus Spitzen,", "tokens": ["Er", "hat", "fast", "kei\u00b7ne", "Form", ";", "sein", "Blatt", "be\u00b7steht", "aus", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$.", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die sonderbar vereint zusammen sitzen,", "tokens": ["Die", "son\u00b7der\u00b7bar", "ver\u00b7eint", "zu\u00b7sam\u00b7men", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und in sich selbst, aufs neue, Spitzen reich.", "tokens": ["Und", "in", "sich", "selbst", ",", "aufs", "neu\u00b7e", ",", "Spit\u00b7zen", "reich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADV", "$,", "APPRART", "ADJA", "$,", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der meisten Form jedoch ist einem Hahn-Kamm gleich;", "tokens": ["Der", "meis\u00b7ten", "Form", "je\u00b7doch", "ist", "ei\u00b7nem", "Hahn\u00b7Kamm", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kein duncklel-rohter Sammt,", "tokens": ["Kein", "dun\u00b7ck\u00b7lel\u00b7roh\u00b7ter", "Sammt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Ja fast kein feuriger Rubin,", "tokens": ["Ja", "fast", "kein", "feu\u00b7ri\u00b7ger", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "PIAT", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Kann in so vollen Farben gl\u00fchn,", "tokens": ["Kann", "in", "so", "vol\u00b7len", "Far\u00b7ben", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Als diese Blum\u2019 in rohtem Glantze flammt.", "tokens": ["Als", "die\u00b7se", "Blum'", "in", "roh\u00b7tem", "Glant\u00b7ze", "flammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wenn ich nun die dem Hahn-Kamm gleiche Blume,", "tokens": ["Wenn", "ich", "nun", "die", "dem", "Hahn\u00b7Kamm", "glei\u00b7che", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Mit aufmercksamen Augen, sehe;", "tokens": ["Mit", "auf\u00b7merck\u00b7sa\u00b7men", "Au\u00b7gen", ",", "se\u00b7he", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So deucht mich, da\u00df ein Hahn mit sanfter Stimme kr\u00e4he,", "tokens": ["So", "deucht", "mich", ",", "da\u00df", "ein", "Hahn", "mit", "sanf\u00b7ter", "Stim\u00b7me", "kr\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Um aus dem Schlaf der Unempfindlichkeit,", "tokens": ["Um", "aus", "dem", "Schlaf", "der", "Un\u00b7emp\u00b7find\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Zu dessen Ehre, Preis und Ruhme,", "tokens": ["Zu", "des\u00b7sen", "Eh\u00b7re", ",", "Preis", "und", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der alle Vollenkommenheit,", "tokens": ["Der", "al\u00b7le", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der aller Dinge Schmuck und Pracht,", "tokens": ["Der", "al\u00b7ler", "Din\u00b7ge", "Schmuck", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Blos durch ein Wort, hervorgebracht,", "tokens": ["Blos", "durch", "ein", "Wort", ",", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mich zu erwecken,", "tokens": ["Mich", "zu", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Und seine Gegenwart in allen zu entdecken.", "tokens": ["Und", "sei\u00b7ne", "Ge\u00b7gen\u00b7wart", "in", "al\u00b7len", "zu", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}