{"textgrid.poem.36579": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Gl\u00fcckliche", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich ist, wer nimmer liebet,", "tokens": ["Gl\u00fcck\u00b7lich", "ist", ",", "wer", "nim\u00b7mer", "lie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PWS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer der Liebe lacht;", "tokens": ["Wer", "der", "Lie\u00b7be", "lacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn wer sich der Lieb' ergiebet,", "tokens": ["Denn", "wer", "sich", "der", "Lieb'", "er\u00b7gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Seufzet, sehnt sich, ist betr\u00fcbet,", "tokens": ["Seuf\u00b7zet", ",", "sehnt", "sich", ",", "ist", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PRF", "$,", "VAFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Winselt Tag und Nacht;", "tokens": ["Win\u00b7selt", "Tag", "und", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Sein Gewinsel, sein Gesehne,", "tokens": ["Sein", "Ge\u00b7win\u00b7sel", ",", "sein", "Ge\u00b7seh\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was er denkt, und thut, und spricht,", "tokens": ["Was", "er", "denkt", ",", "und", "thut", ",", "und", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wirkt ein einziges Gesicht!", "tokens": ["Wirkt", "ein", "ein\u00b7zi\u00b7ges", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Alles andre Sch\u00f6ne", "tokens": ["Al\u00b7les", "and\u00b7re", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "R\u00fchrt ihn nicht.", "tokens": ["R\u00fchrt", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Gl\u00fccklich ist, wer nimmer liebet,", "tokens": ["Gl\u00fcck\u00b7lich", "ist", ",", "wer", "nim\u00b7mer", "lie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PWS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer der Liebe lacht;", "tokens": ["Wer", "der", "Lie\u00b7be", "lacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn wer sich der Lieb' ergiebet,", "tokens": ["Denn", "wer", "sich", "der", "Lieb'", "er\u00b7gie\u00b7bet", ","], "token_info": ["word", "word", "wor