{"dta.poem.1836": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nicht h\u00f6rt auff beyden Theilen/", "tokens": ["Wer", "nicht", "h\u00f6rt", "auff", "bey\u00b7den", "Thei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kan sich leichtlich \u00fcbereilen:", "tokens": ["Kan", "sich", "leicht\u00b7lich", "\u00fc\u00b7be\u00b7rei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob schon billig sein Erkennen/", "tokens": ["Ob", "schon", "bil\u00b7lig", "sein", "Er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ists doch nicht gerecht zu nennen.", "tokens": ["Ists", "doch", "nicht", "ge\u00b7recht", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}