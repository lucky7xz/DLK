, "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Und in einer Morgenr\u00f6te", "tokens": ["Und", "in", "ei\u00b7ner", "Mor\u00b7gen\u00b7r\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt geschwommen eine Wiege,", "tokens": ["Kommt", "ge\u00b7schwom\u00b7men", "ei\u00b7ne", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Kind im Wogenkriege", "tokens": ["Und", "ein", "Kind", "im", "Wo\u00b7gen\u00b7krie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt drin selig, ohne N\u00f6te,", "tokens": ["Liegt", "drin", "se\u00b7lig", ",", "oh\u00b7ne", "N\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Spielt mit seinem Puppenvater,", "tokens": ["Spielt", "mit", "sei\u00b7nem", "Pup\u00b7pen\u00b7va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Neben ihm ein schwarzer Kater.", "tokens": ["Ne\u00b7ben", "ihm", "ein", "schwar\u00b7zer", "Ka\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Endlich ist die Flut verflossen;", "tokens": ["End\u00b7lich", "ist", "die", "Flut", "ver\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "w