, "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo sind Korn und Milch geblieben?", "tokens": ["Wo", "sind", "Korn", "und", "Milch", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles hat der Strom vertrieben.", "tokens": ["Al\u00b7les", "hat", "der", "Strom", "ver\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Ach, die N\u00e4chte! Firstverklettert,", "tokens": ["Ach", ",", "die", "N\u00e4ch\u00b7te", "!", "First\u00b7ver\u00b7klet\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$.", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halb verfroren auf den D\u00e4chern,", "tokens": ["Halb", "ver\u00b7fro\u00b7ren", "auf", "den", "D\u00e4\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nackt, im Frost von Nordsturmf\u00e4chern,", "tokens": ["Nackt", ",", "im", "Frost", "von", "Nord\u00b7sturm\u00b7f\u00e4\u00b7chern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Balkensturz zerschmettert.", "tokens": ["Und", "im", "Bal\u00b7ken\u00b7sturz", "zer\u00b7schmet\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tote Mutter treibt an K\u00fcsten,", "tokens": ["To\u00b7te", "Mut\u00b7ter", "treibt", "an", "K\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat ihr Kind noch an den Br\u00fcsten.", "tokens": ["Hat", "ihr", "Kind", "noch", "an", "den", "Br\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Dort der Greis in seinem Bette,", "tokens": ["Dort", "der", "Greis", "in", "sei\u00b7nem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das zum Kahn ihm ist geworden,", "tokens": ["Das", "zum", "Kahn", "ihm", "ist", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "PPER", "VAFIN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ihn sicher mag umborden,", "tokens": ["Das", "ihn", "si\u00b7cher", "mag", "um\u00b7bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fehlt ihm auch die Ankerkette.", "tokens": ["Fehlt", "ihm", "auch", "die", "An\u00b7ker\u00b7ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zitternd fleht er hoch zum Himmel", "tokens": ["Zit\u00b7ternd", "fleht", "er", "ho