+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mault, als k\u00f6nnt sie nichts vers\u00e4umen:", "tokens": ["Mault", ",", "als", "k\u00f6nnt", "sie", "nichts", "ver\u00b7s\u00e4u\u00b7men", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "VVFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bin ein b\u00fcschen eingeschlafen,", "tokens": ["Bin", "ein", "b\u00fc\u00b7schen", "ein\u00b7ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nichts zu tun bei meinen Schafen.", "tokens": ["Nichts", "zu", "tun", "bei", "mei\u00b7nen", "Scha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Sieben Tage hats gedauert,", "tokens": ["Sie\u00b7ben", "Ta\u00b7ge", "hats", "ge\u00b7dau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieben N\u00e4chte blieb das Wasser,", "tokens": ["Sie\u00b7ben", "N\u00e4ch\u00b7te", "blieb", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis der gro\u00dfe L\u00e4nderhasser,", "tokens": ["Bis", "der", "gro\u00b7\u00dfe", "L\u00e4n\u00b7der\u00b7has\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der stets vor den Deichen lauert,", "tokens": ["Der", "stets", "vor", "den", "Dei\u00b7chen", "lau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich verlaufen hat, verloren,", "tokens": ["Sich", "ver\u00b7lau\u00b7fen", "hat", ",", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "VVPP", "VAFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sein altes Bett erkoren.", "tokens": ["Und", "sein", "al\u00b7tes", "Bett", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Tage, N\u00e4chte, d\u00fcster, dunkel:", "tokens": ["Ta\u00b7ge", ",", "N\u00e4ch\u00b7te", ",", "d\u00fcs\u00b7ter", ",", "dun\u00b7kel", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer wird all die Angst erl\u00f6sen?", "tokens": ["Wer", "wird", "all", "die", "Angst", "er\u00b7l\u00f6\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIAT", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einsam blinzelt eines b\u00f6sen,", "tokens": ["Ein\u00b7sam", "blin\u00b7zelt", "ei\u00b7nes", "b\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Giftigen lila Sterns Gefunkel.", "tokens": ["Gif\u00b7ti\u00b7gen", "li\u00b7la", "Sterns", "Ge\u00b7fun\u