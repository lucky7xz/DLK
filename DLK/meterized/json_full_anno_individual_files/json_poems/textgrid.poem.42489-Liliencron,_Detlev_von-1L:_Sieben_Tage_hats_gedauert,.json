{"textgrid.poem.42489": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieben Tage hats gedauert,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieben Tage hats gedauert,", "tokens": ["Sie\u00b7ben", "Ta\u00b7ge", "hats", "ge\u00b7dau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieben N\u00e4chte blieb das Wasser,", "tokens": ["Sie\u00b7ben", "N\u00e4ch\u00b7te", "blieb", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis der gro\u00dfe L\u00e4nderhasser,", "tokens": ["Bis", "der", "gro\u00b7\u00dfe", "L\u00e4n\u00b7der\u00b7has\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der stets vor den Deichen lauert,", "tokens": ["Der", "stets", "vor", "den", "Dei\u00b7chen", "lau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich verlaufen hat, verloren,", "tokens": ["Sich", "ver\u00b7lau\u00b7fen", "hat", ",", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "VVPP", "VAFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sein altes Bett erkoren.", "tokens": ["Und", "sein", "al\u00b7tes", "Bett", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tage, N\u00e4chte, d\u00fcster, dunkel:", "tokens": ["Ta\u00b7ge", ",", "N\u00e4ch\u00b7te", ",", "d\u00fcs\u00b7ter", ",", "dun\u00b7kel", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer wird all die Angst erl\u00f6sen?", "tokens": ["Wer", "wird", "all", "die", "Angst", "er\u00b7l\u00f6\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIAT", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einsam blinzelt eines b\u00f6sen,", "tokens": ["Ein\u00b7sam", "blin\u00b7zelt", "ei\u00b7nes", "b\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Giftigen lila Sterns Gefunkel.", "tokens": ["Gif\u00b7ti\u00b7gen", "li\u00b7la", "Sterns", "Ge\u00b7fun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "NN", "$."], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Typhon-Orgel, Noah-Lieder,", "tokens": ["Ty\u00b7phon\u00b7Or\u00b7gel", ",", "No\u00b7ah\u00b7Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gischt, Tumult, Schaum, auf und nieder.", "tokens": ["Gischt", ",", "Tu\u00b7mult", ",", "Schaum", ",", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "APPR", "KON", "PTKVZ", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Viele Tausend sind ertrunken,", "tokens": ["Vie\u00b7le", "Tau\u00b7send", "sind", "er\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unz\u00e4hlbares Vieh gestorben;", "tokens": ["Un\u00b7z\u00e4hl\u00b7ba\u00b7res", "Vieh", "ge\u00b7stor\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4dte, D\u00f6rfer sind verdorben,", "tokens": ["St\u00e4d\u00b7te", ",", "D\u00f6r\u00b7fer", "sind", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind versp\u00fclt und sind versunken.", "tokens": ["Sind", "ver\u00b7sp\u00fclt", "und", "sind", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo sind Korn und Milch geblieben?", "tokens": ["Wo", "sind", "Korn", "und", "Milch", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles hat der Strom vertrieben.", "tokens": ["Al\u00b7les", "hat", "der", "Strom", "ver\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, die N\u00e4chte! Firstverklettert,", "tokens": ["Ach", ",", "die", "N\u00e4ch\u00b7te", "!", "First\u00b7ver\u00b7klet\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$.", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halb verfroren auf den D\u00e4chern,", "tokens": ["Halb", "ver\u00b7fro\u00b7ren", "auf", "den", "D\u00e4\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nackt, im Frost von Nordsturmf\u00e4chern,", "tokens": ["Nackt", ",", "im", "Frost", "von", "Nord\u00b7sturm\u00b7f\u00e4\u00b7chern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Balkensturz zerschmettert.", "tokens": ["Und", "im", "Bal\u00b7ken\u00b7sturz", "zer\u00b7schmet\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tote Mutter treibt an K\u00fcsten,", "tokens": ["To\u00b7te", "Mut\u00b7ter", "treibt", "an", "K\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat ihr Kind noch an den Br\u00fcsten.", "tokens": ["Hat", "ihr", "Kind", "noch", "an", "den", "Br\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dort der Greis in seinem Bette,", "tokens": ["Dort", "der", "Greis", "in", "sei\u00b7nem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das zum Kahn ihm ist geworden,", "tokens": ["Das", "zum", "Kahn", "ihm", "ist", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "PPER", "VAFIN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ihn sicher mag umborden,", "tokens": ["Das", "ihn", "si\u00b7cher", "mag", "um\u00b7bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fehlt ihm auch die Ankerkette.", "tokens": ["Fehlt", "ihm", "auch", "die", "An\u00b7ker\u00b7ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zitternd fleht er hoch zum Himmel", "tokens": ["Zit\u00b7ternd", "fleht", "er", "hoch", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf der Fahrt durchs Fischgewimmel.", "tokens": ["Auf", "der", "Fahrt", "durchs", "Fischge\u00b7wim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Schiffe poltern durch die Marschen,", "tokens": ["Schif\u00b7fe", "pol\u00b7tern", "durch", "die", "Mar\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich her vom Meer verirrten,", "tokens": ["Die", "sich", "her", "vom", "Meer", "ver\u00b7irr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APZR", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich in Baum und Strauch verwirrten", "tokens": ["Sich", "in", "Baum", "und", "Strauch", "ver\u00b7wirr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "NE", "KON", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Sande dann verharschen.", "tokens": ["Und", "im", "San\u00b7de", "dann", "ver\u00b7har\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4usertr\u00fcmmer, hell in Flammen,", "tokens": ["H\u00e4u\u00b7ser\u00b7tr\u00fcm\u00b7mer", ",", "hell", "in", "Flam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Prasseln chaoswild zusammen.", "tokens": ["Pras\u00b7seln", "chaos\u00b7wild", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "\u00dcber Wind und Hagelst\u00f6\u00dfe:", "tokens": ["\u00dc\u00b7ber", "Wind", "und", "Ha\u00b7gel\u00b7st\u00f6\u00b7\u00dfe", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welch Geschrei, Gekreisch und Jammern,", "tokens": ["Welch", "Ge\u00b7schrei", ",", "Ge\u00b7kreisch", "und", "Jam\u00b7mern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die sich an die Sparren klammern:", "tokens": ["Die", "sich", "an", "die", "Spar\u00b7ren", "klam\u00b7mern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hilfe! Hilfe unsrer Bl\u00f6\u00dfe!", "tokens": ["Hil\u00b7fe", "!", "Hil\u00b7fe", "uns\u00b7rer", "Bl\u00f6\u00b7\u00dfe", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Pferden\u00fcstern tauchen, schnaufen", "tokens": ["Pfer\u00b7de\u00b7n\u00fcs\u00b7tern", "tau\u00b7chen", ",", "schnau\u00b7fen"], "token_info": ["word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus den w\u00fcsten Wellentraufen.", "tokens": ["Aus", "den", "w\u00fcs\u00b7ten", "Wel\u00b7len\u00b7trau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Den Altar der Kirchen kl\u00fcften", "tokens": ["Den", "Al\u00b7tar", "der", "Kir\u00b7chen", "kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weit der salzigen See Gewalten:", "tokens": ["Weit", "der", "sal\u00b7zi\u00b7gen", "See", "Ge\u00b7wal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Rei\u00dfen S\u00e4rge weg aus Spalten,", "tokens": ["Rei\u00b7\u00dfen", "S\u00e4r\u00b7ge", "weg", "aus", "Spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heben Steine von den Gr\u00fcften.", "tokens": ["He\u00b7ben", "Stei\u00b7ne", "von", "den", "Gr\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alte Knochen, neue Leichen", "tokens": ["Al\u00b7te", "Kno\u00b7chen", ",", "neu\u00b7e", "Lei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steuern eins im Sintflutzeichen.", "tokens": ["Steu\u00b7ern", "eins", "im", "Sint\u00b7flut\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und in einer Morgenr\u00f6te", "tokens": ["Und", "in", "ei\u00b7ner", "Mor\u00b7gen\u00b7r\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt geschwommen eine Wiege,", "tokens": ["Kommt", "ge\u00b7schwom\u00b7men", "ei\u00b7ne", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Kind im Wogenkriege", "tokens": ["Und", "ein", "Kind", "im", "Wo\u00b7gen\u00b7krie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt drin selig, ohne N\u00f6te,", "tokens": ["Liegt", "drin", "se\u00b7lig", ",", "oh\u00b7ne", "N\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Spielt mit seinem Puppenvater,", "tokens": ["Spielt", "mit", "sei\u00b7nem", "Pup\u00b7pen\u00b7va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Neben ihm ein schwarzer Kater.", "tokens": ["Ne\u00b7ben", "ihm", "ein", "schwar\u00b7zer", "Ka\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Endlich ist die Flut verflossen;", "tokens": ["End\u00b7lich", "ist", "die", "Flut", "ver\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles eilt nun, um zu landen,", "tokens": ["Al\u00b7les", "eilt", "nun", ",", "um", "zu", "lan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was noch lebend ist vorhanden,", "tokens": ["Was", "noch", "le\u00b7bend", "ist", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der Schwall noch nicht zergossen.", "tokens": ["Was", "der", "Schwall", "noch", "nicht", "zer\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Liebe, das Erbarmen", "tokens": ["Und", "die", "Lie\u00b7be", ",", "das", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Walten bald mit regen Armen.", "tokens": ["Wal\u00b7ten", "bald", "mit", "re\u00b7gen", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Jenes Haus, wills grad zerkrachen?", "tokens": ["Je\u00b7nes", "Haus", ",", "wills", "grad", "zer\u00b7kra\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbheda! lebt hier noch die Sippe?", "tokens": ["\u00bb", "he\u00b7da", "!", "lebt", "hier", "noch", "die", "Sip\u00b7pe", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner mehr an Herd und Krippe?", "tokens": ["Kei\u00b7ner", "mehr", "an", "Herd", "und", "Krip\u00b7pe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir sind da,