{"textgrid.poem.25723": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Cosmopolit wie du zu seyn,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cosmopolit wie du zu seyn,", "tokens": ["Cos\u00b7mo\u00b7po\u00b7lit", "wie", "du", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mag freilich oft das Herz erfreun,", "tokens": ["Mag", "frei\u00b7lich", "oft", "das", "Herz", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir aber, mu\u00df ich sorgen,", "tokens": ["Mir", "a\u00b7ber", ",", "mu\u00df", "ich", "sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "VMFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bleibt's ewig wohl verborgen.", "tokens": ["Bleibt's", "e\u00b7wig", "wohl", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ist Eurer Menschenfreundlichkeit", "tokens": ["Ist", "Eu\u00b7rer", "Men\u00b7schen\u00b7freund\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Land zu lang, ein Land zu breit?", "tokens": ["Ein", "Land", "zu", "lang", ",", "ein", "Land", "zu", "breit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKA", "ADJD", "$,", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Geh\u00f6rt zu Eurer Sph\u00e4re.", "tokens": ["Ge\u00b7h\u00f6rt", "zu", "Eu\u00b7rer", "Sph\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich kann, fehlt meinem Nachbar nur", "tokens": ["Ich", "kann", ",", "fehlt", "mei\u00b7nem", "Nach\u00b7bar", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Scheffel Saat f\u00fcr seine Flur,", "tokens": ["Ein", "Schef\u00b7fel", "Saat", "f\u00fcr", "sei\u00b7ne", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Den Scheffel kaum ihm geben,", "tokens": ["Den", "Schef\u00b7fel", "kaum", "ihm", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und soll f\u00fcrs Weltall leben?", "tokens": ["Und", "soll", "f\u00fcrs", "Wel\u00b7tall", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Daf\u00fcr zu schwatzen? Das geht an!", "tokens": ["Da\u00b7f\u00fcr", "zu", "schwat\u00b7zen", "?", "Das", "geht", "an", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "$.", "PDS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Daf\u00fcr zu schreiben? Ei wohlan!", "tokens": ["Da\u00b7f\u00fcr", "zu", "schrei\u00b7ben", "?", "Ei", "wo\u00b7hlan", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "$.", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das k\u00f6nnten, w\u00e4r's vonn\u00f6then,", "tokens": ["Das", "k\u00f6nn\u00b7ten", ",", "w\u00e4r's", "von\u00b7n\u00f6\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Vor allen, wir Poeten.", "tokens": ["Vor", "al\u00b7len", ",", "wir", "Po\u00b7et\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Allein daf\u00fcr zu leben? \u2013 Hum!", "tokens": ["Al\u00b7lein", "da\u00b7f\u00fcr", "zu", "le\u00b7ben", "?", "\u2013", "Hum", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "PAV", "PTKZU", "VVINF", "$.", "$(", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wer l\u00e4\u00dft dem Nachbar, um und um", "tokens": ["Wer", "l\u00e4\u00dft", "dem", "Nach\u00b7bar", ",", "um", "und", "um"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "KOUI", "KON", "KOUI"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Von den Cosmopoliten,", "tokens": ["Von", "den", "Cos\u00b7mo\u00b7po\u00b7li\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN"