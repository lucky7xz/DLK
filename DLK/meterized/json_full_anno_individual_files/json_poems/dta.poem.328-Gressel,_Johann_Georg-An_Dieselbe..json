{"dta.poem.328": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An Dieselbe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.71", "nl:0.28"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Holde Sonne meiner Seelen", "tokens": ["Hol\u00b7de", "Son\u00b7ne", "mei\u00b7ner", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leget sich dein Streuben nicht?", "tokens": ["Le\u00b7get", "sich", "dein", "Streu\u00b7ben", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df ich mich zu tode qu\u00e4hlen", "tokens": ["Mu\u00df", "ich", "mich", "zu", "to\u00b7de", "qu\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheint mir garkein Gnaden-Licht?", "tokens": ["Scheint", "mir", "gar\u00b7kein", "Gna\u00b7den\u00b7Licht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Werden mich die Straff-", "tokens": ["Wer\u00b7den", "mich", "die", "Straff"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "TRUNC"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Doch mit ihren Strahlen t\u00f6dten?", "tokens": ["Doch", "mit", "ih\u00b7ren", "Strah\u00b7len", "t\u00f6d\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6nste legehin dein Hassen", "tokens": ["Sch\u00f6ns\u00b7te", "le\u00b7ge\u00b7hin", "dein", "Has\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blicke mich mit Anmuth an/", "tokens": ["Bli\u00b7cke", "mich", "mit", "An\u00b7muth", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich deine Brust umfassen", "tokens": ["La\u00df", "mich", "dei\u00b7ne", "Brust", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich mich erg\u00f6tzen kan.", "tokens": ["Da\u00df", "ich", "mich", "er\u00b7g\u00f6t\u00b7zen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df von ihren ", "tokens": ["La\u00df", "von", "ih\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Nectar auf die Lippen thauen.", "tokens": ["Nec\u00b7tar", "auf", "die", "Lip\u00b7pen", "thau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gib mir deinen Mund und Br\u00fcste", "tokens": ["Gib", "mir", "dei\u00b7nen", "Mund", "und", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Holde ", "tokens": ["Hol\u00b7de"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mache da\u00df der Platz der L\u00fcste", "tokens": ["Ma\u00b7che", "da\u00df", "der", "Platz", "der", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir ein frohes Eden sey.", "tokens": ["Mir", "ein", "fro\u00b7hes", "E\u00b7den", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6r doch auf den zu betr\u00fcben", "tokens": ["H\u00f6r", "doch", "auf", "den", "zu", "be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der dich ehrt mit seinen Lieben.", "tokens": ["Der", "dich", "ehrt", "mit", "sei\u00b7nen", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Meine Liebe ist best\u00e4ndig", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", "ist", "be\u00b7st\u00e4n\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie verschwehrt den Unbestand/", "tokens": ["Sie", "ver\u00b7schwehrt", "den", "Un\u00b7be\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie wird auch nicht Wetter-wendig/", "tokens": ["Sie", "wird", "auch", "nicht", "Wet\u00b7ter\u00b7wen\u00b7dig", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jhr ist nichts als Treu bekannt/", "tokens": ["Ihr", "ist", "nichts", "als", "Treu", "be\u00b7kannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "KOKOM", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dich best\u00e4ndig zu verehren", "tokens": ["Dich", "be\u00b7st\u00e4n\u00b7dig", "zu", "ver\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird ihr nur der Tod verwehren.", "tokens": ["Wird", "ihr", "nur", "der", "Tod", "ver\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}