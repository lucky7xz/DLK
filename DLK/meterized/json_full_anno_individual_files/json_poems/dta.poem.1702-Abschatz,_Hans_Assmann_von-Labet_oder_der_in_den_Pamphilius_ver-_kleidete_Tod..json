{"dta.poem.1702": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Labet  oder der in den  Pamphilius  ver-  \n kleidete Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wer will sich auff Labet mit mir zu spielen wagen?", "tokens": ["Wer", "will", "sich", "auff", "La\u00b7bet", "mit", "mir", "zu", "spie\u00b7len", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "APPR", "NN", "APPR", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich lasse Gr\u00fcn und Roth der Jugend manchmahl h", "tokens": ["Ich", "las\u00b7se", "Gr\u00fcn", "und", "Roth", "der", "Ju\u00b7gend", "manch\u00b7mahl", "h"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dennoch bleibet mir zulezte der Gewinn.", "tokens": ["Und", "den\u00b7noch", "blei\u00b7bet", "mir", "zu\u00b7lez\u00b7te", "der", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Spielt Hertzen/ wie ihr wollt/ ich werde ", "tokens": ["Spielt", "Hert\u00b7zen", "/", "wie", "ihr", "wollt", "/", "ich", "wer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "$(", "PWAV", "PPER", "VMFIN", "$(", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gewohnt gew\u00e4hltes Ae\u00df und K\u00f6nig wegzuschlagen/", "tokens": ["Ge\u00b7wohnt", "ge\u00b7w\u00e4hl\u00b7tes", "A\u00b7e\u00df", "und", "K\u00f6\u00b7nig", "weg\u00b7zu\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJA", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Die Schellen bleiben euch vergebens in der Hand/", "tokens": ["Die", "Schel\u00b7len", "blei\u00b7ben", "euch", "ver\u00b7ge\u00b7bens", "in", "der", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo ihr die Karte nicht bescheiden angewandt/", "tokens": ["Wo", "ihr", "die", "Kar\u00b7te", "nicht", "be\u00b7schei\u00b7den", "an\u00b7ge\u00b7wandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKNEG", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn meiner Eicheln Brett euch mu\u00df zu Bette tragen.", "tokens": ["Wenn", "mei\u00b7ner", "Ei\u00b7cheln", "Brett", "euch", "mu\u00df", "zu", "Bet\u00b7te", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}