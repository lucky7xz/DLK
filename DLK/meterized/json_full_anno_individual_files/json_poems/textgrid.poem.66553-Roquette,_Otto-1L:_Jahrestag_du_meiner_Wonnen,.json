{"textgrid.poem.66553": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jahrestag du meiner Wonnen,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jahrestag du meiner Wonnen,", "tokens": ["Jah\u00b7res\u00b7tag", "du", "mei\u00b7ner", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei mir wie ein Fest begr\u00fc\u00dft!", "tokens": ["Sei", "mir", "wie", "ein", "Fest", "be\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward das Gl\u00fcck auch, kaum begonnen,", "tokens": ["Ward", "das", "Gl\u00fcck", "auch", ",", "kaum", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hart wie eine Schuld geb\u00fc\u00dft!", "tokens": ["Hart", "wie", "ei\u00b7ne", "Schuld", "ge\u00b7b\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Komm', erinnernd mir zu malen", "tokens": ["Komm'", ",", "e\u00b7rin\u00b7nernd", "mir", "zu", "ma\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVPP", "PPER", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aller Leidenschaft Gew\u00fchl,", "tokens": ["Al\u00b7ler", "Lei\u00b7den\u00b7schaft", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So der Freuden, wie der Qualen", "tokens": ["So", "der", "Freu\u00b7den", ",", "wie", "der", "Qua\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unaussprechliches Gef\u00fchl!", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7li\u00b7ches", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df mich dann, wie du verflie\u00dfest,", "tokens": ["La\u00df", "mich", "dann", ",", "wie", "du", "ver\u00b7flie\u00b7\u00dfest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still der reinen Kraft vertraun,", "tokens": ["Still", "der", "rei\u00b7nen", "Kraft", "ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Kreis, den du beschlie\u00dfest,", "tokens": ["Und", "den", "Kreis", ",", "den", "du", "be\u00b7schlie\u00b7\u00dfest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In verkl\u00e4rtem Lichte schaun!", "tokens": ["In", "ver\u00b7kl\u00e4r\u00b7tem", "Lich\u00b7te", "schaun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "