{"dta.poem.10861": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Die Meise und der Sperling.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hatte die behertzte Meise", "tokens": ["Es", "hat\u00b7te", "die", "be\u00b7hertz\u00b7te", "Mei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das warme Jahr durch ihre Speise", "tokens": ["Das", "war\u00b7me", "Jahr", "durch", "ih\u00b7re", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nach eignem W\u00fcnschen und Verlangen", "tokens": ["Nach", "eig\u00b7nem", "W\u00fcn\u00b7schen", "und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vollauf und ohne M\u00fch empfangen.", "tokens": ["Vol\u00b7lauf", "und", "oh\u00b7ne", "M\u00fch", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bald fieng der Nordwind an zu rasen,", "tokens": ["Bald", "fi\u00b7eng", "der", "Nord\u00b7wind", "an", "zu", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Es wurde durch sein kaltes Blasen", "tokens": ["Es", "wur\u00b7de", "durch", "sein", "kal\u00b7tes", "Bla\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Berges Gipfel silberwei\u00df,", "tokens": ["Des", "Ber\u00b7ges", "Gip\u00b7fel", "sil\u00b7ber\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Bach, der Teich, der Flu\u00df zu Ei\u00df,", "tokens": ["Der", "Bach", ",", "der", "Teich", ",", "der", "Flu\u00df", "zu", "Ei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das Feld als Stein, und durch die K\u00e4lte", "tokens": ["Das", "Feld", "als", "Stein", ",", "und", "durch", "die", "K\u00e4l\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOUS", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sah man in vielen B\u00e4umen Sp\u00e4lte.", "tokens": ["Sah", "man", "in", "vie\u00b7len", "B\u00e4u\u00b7men", "Sp\u00e4l\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ey Vogel, nimmst du so verlieb", "tokens": ["Ey", "Vo\u00b7gel", ",", "nimmst", "du", "so", "ver\u00b7lieb"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit der mit Ei\u00df gew\u00fcrtzten Speise?", "tokens": ["Mit", "der", "mit", "Ei\u00df", "ge\u00b7w\u00fcrtz\u00b7ten", "Spei\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "So sprach der kleine Saatedieb,", "tokens": ["So", "sprach", "der", "klei\u00b7ne", "Saa\u00b7te\u00b7di\u00b7eb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Der Sperling, zu der muntern Meise.", "tokens": ["Der", "Sper\u00b7ling", ",", "zu", "der", "mun\u00b7tern", "Mei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ich f\u00fcrchte sehr, du m\u00fcssest sterben,", "tokens": ["Ich", "f\u00fcrch\u00b7te", "sehr", ",", "du", "m\u00fcs\u00b7sest", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und durch der K\u00e4lte Grimm verderben.", "tokens": ["Und", "durch", "der", "K\u00e4l\u00b7te", "Grimm", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Was hilft dich izt dein stetes Springen,", "tokens": ["Was", "hilft", "dich", "izt", "dein", "ste\u00b7tes", "Sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Dein H\u00fcpfen, Fliegen, und dein Singen,", "tokens": ["Dein", "H\u00fcp\u00b7fen", ",", "Flie\u00b7gen", ",", "und", "dein", "Sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Dein Zizipa, dein Zizipa?", "tokens": ["Dein", "Zi\u00b7zi\u00b7pa", ",", "dein", "Zi\u00b7zi\u00b7pa", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sing eh: O weh! mein End ist nah!", "tokens": ["Sing", "eh", ":", "O", "weh", "!", "mein", "End", "ist", "nah", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "$.", "NE", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sieh doch, wie hab ich es so gut,", "tokens": ["Sieh", "doch", ",", "wie", "hab", "ich", "es", "so", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ich zeuge t\u00e4glich frisches Blut;", "tokens": ["Ich", "zeu\u00b7ge", "t\u00e4g\u00b7lich", "fri\u00b7sches", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Von Ueberflu\u00df an Speltz und Gersten", "tokens": ["Von", "Ue\u00b7berf\u00b7lu\u00df", "an", "Speltz", "und", "Gers\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "M\u00f6gt ich, du siehst es selbst, zerbersten.", "tokens": ["M\u00f6gt", "ich", ",", "du", "siehst", "es", "selbst", ",", "zer\u00b7bers\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die aufgeweckte Meise spricht:", "tokens": ["Die", "auf\u00b7ge\u00b7weck\u00b7te", "Mei\u00b7se", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der Meisen Stamm vergehet nicht,", "tokens": ["Der", "Mei\u00b7sen", "Stamm", "ver\u00b7ge\u00b7het", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "So lang im Boden W\u00fcrmer leben,", "tokens": ["So", "lang", "im", "Bo\u00b7den", "W\u00fcr\u00b7mer", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und M\u00fccken in den L\u00fcften schweben.", "tokens": ["Und", "M\u00fc\u00b7cken", "in", "den", "L\u00fcf\u00b7ten", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Mein Freund, es w\u00e4r ein gleiches Wunder,", "tokens": ["Mein", "Freund", ",", "es", "w\u00e4r", "ein", "glei\u00b7ches", "Wun\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Gieng ein Geschlecht, mein oder deines, unter.", "tokens": ["Gieng", "ein", "Ge\u00b7schlecht", ",", "mein", "o\u00b7der", "dei\u00b7nes", ",", "un\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "KON", "PPOSAT", "$,", "APPR", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Nein! Wer nichts nach dem Morgen fragt,", "tokens": ["Nein", "!", "Wer", "nichts", "nach", "dem", "Mor\u00b7gen", "fragt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PWS", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.32": {"text": "Der lebt vergn\u00fcgt, und unverzagt.", "tokens": ["Der", "lebt", "ver\u00b7gn\u00fcgt", ",", "und", "un\u00b7ver\u00b7zagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVPP", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Der holde Lentz mit seinen Sch\u00e4tzen", "tokens": ["Der", "hol\u00b7de", "Lentz", "mit", "sei\u00b7nen", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Wird meinen Mangel schon ersetzen.", "tokens": ["Wird", "mei\u00b7nen", "Man\u00b7gel", "schon", "er\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Ich singe schon, als w\u00e4r er da,", "tokens": ["Ich", "sin\u00b7ge", "schon", ",", "als", "w\u00e4r", "er", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Mein Zizipa, mein Zizipa.", "tokens": ["Mein", "Zi\u00b7zi\u00b7pa", ",", "mein", "Zi\u00b7zi\u00b7pa", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}