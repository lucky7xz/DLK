{"dta.poem.4206": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch einige Rosen-Gedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie alles, was in der Natur an Bluhmen liebliches", "tokens": ["Wie", "al\u00b7les", ",", "was", "in", "der", "Na\u00b7tur", "an", "Bluh\u00b7men", "lieb\u00b7li\u00b7ches"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "$,", "PRELS", "APPR", "ART", "NN", "APPR", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "zu sehn,", "tokens": ["zu", "sehn", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "So sch\u00f6n, da\u00df es nicht auszudr\u00fccken;", "tokens": ["So", "sch\u00f6n", ",", "da\u00df", "es", "nicht", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ist vor andern noch, sehr sch\u00f6n,", "tokens": ["So", "ist", "vor", "an\u00b7dern", "noch", ",", "sehr", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PIS", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was wir an der gepriesnen Rose, der Bluhmen K\u00f6niginn,", "tokens": ["Was", "wir", "an", "der", "ge\u00b7pri\u00b7es\u00b7nen", "Ro\u00b7se", ",", "der", "Bluh\u00b7men", "K\u00f6\u00b7ni\u00b7ginn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "erblicken.", "tokens": ["er\u00b7bli\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Je \u00f6fter wir die s\u00fcsse Gluht im neuen Sommer wieder", "tokens": ["Je", "\u00f6f\u00b7ter", "wir", "die", "s\u00fcs\u00b7se", "Gluht", "im", "neu\u00b7en", "Som\u00b7mer", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "schauen,", "tokens": ["schau\u00b7en", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Je \u00f6fter kann sich unser Geist in neuer Lust daran erbauen,", "tokens": ["Je", "\u00f6f\u00b7ter", "kann", "sich", "un\u00b7ser", "Geist", "in", "neu\u00b7er", "Lust", "da\u00b7ran", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.10": {"text": "Das Auge neue Sch\u00f6nheit finden. Ich danke Gott, da\u00df", "tokens": ["Das", "Au\u00b7ge", "neu\u00b7e", "Sch\u00f6n\u00b7heit", "fin\u00b7den", ".", "Ich", "dan\u00b7ke", "Gott", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$.", "PPER", "VVFIN", "NN", "$,", "KOUS"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "ich von neuen", "tokens": ["ich", "von", "neu\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPR", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Mich deiner wieder kann, geliebte Ros\u2019, erfreuen,", "tokens": ["Mich", "dei\u00b7ner", "wie\u00b7der", "kann", ",", "ge\u00b7lieb\u00b7te", "Ros'", ",", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADV", "VMFIN", "$,", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und was f\u00fcr Lieblichkeit und Anmuht in deinem Bau und", "tokens": ["Und", "was", "f\u00fcr", "Lieb\u00b7lich\u00b7keit", "und", "An\u00b7muht", "in", "dei\u00b7nem", "Bau", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Wesen stecken,", "tokens": ["We\u00b7sen", "ste\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Dem, Der dich so vortrefflich schuff, zum Lobe, Ruhm", "tokens": ["Dem", ",", "Der", "dich", "so", "vor\u00b7treff\u00b7lich", "schuff", ",", "zum", "Lo\u00b7be", ",", "Ruhm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDS", "$,", "ART", "PPER", "ADV", "ADJD", "PTKVZ", "$,", "APPRART", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "und Preis, entdecken.", "tokens": ["und", "Preis", ",", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mir brachten meine Kinder j\u00fcngst aus meinem Gar-", "tokens": ["Mir", "brach\u00b7ten", "mei\u00b7ne", "Kin\u00b7der", "j\u00fcngst", "aus", "mei\u00b7nem", "Ga\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "ten sch\u00f6ne Rosen,", "tokens": ["ten", "sch\u00f6\u00b7ne", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und zwar bey ganzen H\u00e4nden voll, die auserlesen sch\u00f6n", "tokens": ["Und", "zwar", "bey", "gan\u00b7zen", "H\u00e4n\u00b7den", "voll", ",", "die", "au\u00b7ser\u00b7le\u00b7sen", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ADJD", "$,", "PRELS", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "und frisch,", "tokens": ["und", "frisch", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Voll holder Gluht und s\u00fcssem Balsam, so Blick als Nase", "tokens": ["Voll", "hol\u00b7der", "Gluht", "und", "s\u00fcs\u00b7sem", "Bal\u00b7sam", ",", "so", "Blick", "als", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "KON", "ADJA", "NN", "$,", "ADV", "NN", "KOUS", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "liebzukosen.", "tokens": ["lieb\u00b7zu\u00b7ko\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ich legte sie, weil ihrer viele, zu anfangs vor mir auf den", "tokens": ["Ich", "leg\u00b7te", "sie", ",", "weil", "ih\u00b7rer", "vie\u00b7le", ",", "zu", "an\u00b7fangs", "vor", "mir", "auf", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "PIAT", "$,", "APPR", "ADV", "APPR", "PPER", "APPR", "ART"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Tisch,", "tokens": ["Tisch", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Und lie\u00df den recht erquickten Blick begierig auf den sch\u00f6-", "tokens": ["Und", "lie\u00df", "den", "recht", "er\u00b7quick\u00b7ten", "Blick", "be\u00b7gie\u00b7rig", "auf", "den", "sch\u00f6"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJD", "ADJA", "NN", "ADJD", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "nen Haufen,", "tokens": ["nen", "Hau\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Die allersch\u00f6nsten auszusuchen, bedachtsam hin und wie-", "tokens": ["Die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "aus\u00b7zu\u00b7su\u00b7chen", ",", "be\u00b7dacht\u00b7sam", "hin", "und", "wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVINF", "$,", "ADJD", "PTKVZ", "KON", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "der laufen,", "tokens": ["der", "lau\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Bis ich zuletzt darauf verfiel, in eine Ordnung sie zu legen.", "tokens": ["Bis", "ich", "zu\u00b7letzt", "da\u00b7rauf", "ver\u00b7fiel", ",", "in", "ei\u00b7ne", "Ord\u00b7nung", "sie", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PAV", "VVFIN", "$,", "APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ich legte sie in eine R\u00fcnde, die gliche nicht nur einem", "tokens": ["Ich", "leg\u00b7te", "sie", "in", "ei\u00b7ne", "R\u00fcn\u00b7de", ",", "die", "gli\u00b7che", "nicht", "nur", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "PTKNEG", "ADV", "ART"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Kranz;", "tokens": ["Kranz", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Sie zeigte mir von ungefehr, was vor verschiednen Schmuck", "tokens": ["Sie", "zeig\u00b7te", "mir", "von", "un\u00b7ge\u00b7fehr", ",", "was", "vor", "ver\u00b7schied\u00b7nen", "Schmuck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJD", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "und Glanz", "tokens": ["und", "Glanz"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Die Rosen in verschiedner Lag\u2019, in unterschiedner Stellung", "tokens": ["Die", "Ro\u00b7sen", "in", "ver\u00b7schied\u00b7ner", "Lag'", ",", "in", "un\u00b7ter\u00b7schied\u00b7ner", "Stel\u00b7lung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "hegen,", "tokens": ["he\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "So ich bishero nicht entdeckt. Von vorn, von hinten,", "tokens": ["So", "ich", "bis\u00b7he\u00b7ro", "nicht", "ent\u00b7deckt", ".", "Von", "vorn", ",", "von", "hin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "PTKNEG", "VVPP", "$.", "APPR", "ADV", "$,", "APPR", "ADV", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Seiten-werts", "tokens": ["Sei\u00b7ten\u00b7werts"], "token_info": ["word"], "pos": ["NE"], "meter": "+--", "measure": "dactylic.init"}, "line.10": {"text": "War Farb und Bildung unterschiedlich,", "tokens": ["War", "Farb", "und", "Bil\u00b7dung", "un\u00b7ter\u00b7schied\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und doch, in ihrem Unterscheid, gleich angenehm, gleich", "tokens": ["Und", "doch", ",", "in", "ih\u00b7rem", "Un\u00b7ter\u00b7scheid", ",", "gleich", "an\u00b7ge\u00b7nehm", ",", "gleich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "sch\u00f6n und niedlich.", "tokens": ["sch\u00f6n", "und", "nied\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "In einer Tiefe wie Rubinen sah\u2019 ich das kleine g\u00fcld\u2019ne", "tokens": ["In", "ei\u00b7ner", "Tie\u00b7fe", "wie", "Ru\u00b7bi\u00b7nen", "sah'", "ich", "das", "klei\u00b7ne", "g\u00fcld'\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KOKOM", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Herz", "tokens": ["Herz"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der vordersten beschattet schimmern, der Bl\u00e4tter Wirbel", "tokens": ["Der", "vor\u00b7ders\u00b7ten", "be\u00b7schat\u00b7tet", "schim\u00b7mern", ",", "der", "Bl\u00e4t\u00b7ter", "Wir\u00b7bel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VVPP", "VVFIN", "$,", "ART", "NN", "NN"], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "drehte sich,", "tokens": ["dreh\u00b7te", "sich", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Und zog die Augen in sich ein; inzwischen, da\u00df die von der", "tokens": ["Und", "zog", "die", "Au\u00b7gen", "in", "sich", "ein", ";", "in\u00b7zwi\u00b7schen", ",", "da\u00df", "die", "von", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PRF", "PTKVZ", "$.", "ADV", "$,", "KOUS", "ART", "APPR", "ART"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Seiten", "tokens": ["Sei\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Das Licht auf bl\u00e4ulich-weissen Bl\u00e4ttern, theils durch,", "tokens": ["Das", "Licht", "auf", "bl\u00e4u\u00b7lich\u00b7weis\u00b7sen", "Bl\u00e4t\u00b7tern", ",", "theils", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "theils auf sich liessen gleiten,", "tokens": ["theils", "auf", "sich", "lies\u00b7sen", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und halb sich zeigten, halb verdeckten. Die angenehme", "tokens": ["Und", "halb", "sich", "zeig\u00b7ten", ",", "halb", "ver\u00b7deck\u00b7ten", ".", "Die", "an\u00b7ge\u00b7neh\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "PRF", "VVFIN", "$,", "ADJD", "VVPP", "$.", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Farbe glich,", "tokens": ["Far\u00b7be", "glich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Indem sie bl\u00e4u- und r\u00f6htlich bla\u00df,", "tokens": ["In\u00b7dem", "sie", "bl\u00e4u", "und", "r\u00f6ht\u00b7lich", "bla\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "TRUNC", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dem lieblichen Rubin-Bala\u00df.", "tokens": ["Dem", "lieb\u00b7li\u00b7chen", "Ru\u00b7bin\u00b7Ba\u00b7la\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Bey denen nun, die abwerts liegen,", "tokens": ["Bey", "de\u00b7nen", "nun", ",", "die", "ab\u00b7