{"textgrid.poem.57106": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An P.B.-H.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist ein stiller See im Hochgebirg.", "tokens": ["Du", "bist", "ein", "stil\u00b7ler", "See", "im", "Hoch\u00b7ge\u00b7birg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn klarer Himmel freundlich in dich schaut,", "tokens": ["Wenn", "kla\u00b7rer", "Him\u00b7mel", "freund\u00b7lich", "in", "dich", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "so ruht dein Spiegel lauter bis zum Grund.", "tokens": ["so", "ruht", "dein", "Spie\u00b7gel", "lau\u00b7ter", "bis", "zum", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch wenn ein unverseh'ner Wind dich trifft,", "tokens": ["Doch", "wenn", "ein", "un\u00b7ver\u00b7seh'\u00b7ner", "Wind", "dich", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "so \u00fcberl\u00e4uft ein schmerzhaft Zittern dich,", "tokens": ["so", "\u00fc\u00b7berl\u00b7\u00e4uft", "ein", "schmerz\u00b7haft", "Zit\u00b7tern", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und wie mit einem grauen Netz verh\u00fcllst", "tokens": ["und", "wie", "mit", "ei\u00b7nem", "grau\u00b7en", "Netz", "ver\u00b7h\u00fcllst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "du ein in seiner Tiefe scheues Herz.", "tokens": ["du", "ein", "in", "sei\u00b7ner", "Tie\u00b7fe", "scheu\u00b7es", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du bist ein stiller See im Hochgebirg.", "tokens": ["Du", "bist", "ein", "stil\u00b7ler", "See", "im", "Hoch\u00b7ge\u00b7birg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn klarer Himmel freundlich in dich schaut,", "tokens": ["Wenn", "kla\u00b7rer", "Him\u00b7mel", "freund\u00b7lich", "in", "dich", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "so ruht dein Spiegel lauter bis zum Grund.", "tokens": ["so", "ruht", "dein", "Spie\u00b7gel", "lau\u00b7ter", "bis", "zum", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch wenn ein unverse