{"textgrid.poem.40030": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sahe j\u00fcngst, mit fast erstauntem Blick,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe j\u00fcngst, mit fast erstauntem Blick,", "tokens": ["Ich", "sa\u00b7he", "j\u00fcngst", ",", "mit", "fast", "er\u00b7staun\u00b7tem", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein von Natur und Kunst vereintes Meisterst\u00fcck,", "tokens": ["Ein", "von", "Na\u00b7tur", "und", "Kunst", "ver\u00b7ein\u00b7tes", "Meis\u00b7ter\u00b7st\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein nie gebohren Fleisch, ein todtes Leben,", "tokens": ["Ein", "nie", "ge\u00b7boh\u00b7ren", "Fleisch", ",", "ein", "tod\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem die Zergliedrungs-Kunst so Farb' als Form gegeben.", "tokens": ["Dem", "die", "Zer\u00b7gliedrungs\u00b7Kunst", "so", "Fa\u00b7rb'", "als", "Form", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ein Wunder-Werck vom Wachs; wodurch der kleinen Welt,", "tokens": ["Ein", "Wun\u00b7der\u00b7\u00b7Werck", "vom", "Wachs", ";", "wo\u00b7durch", "der", "klei\u00b7nen", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Menschen C\u00f6rper, Fleisch, Blut, Adern, Muskeln, Sehnen,", "tokens": ["Des", "Men\u00b7schen", "C\u00f6r\u00b7per", ",", "Fleisch", ",", "Blut", ",", "A\u00b7dern", ",", "Mus\u00b7keln", ",", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gehirn und Eingeweid' so k\u00fcnstlich vorgestellt,", "tokens": ["Ge\u00b7hirn", "und", "Ein\u00b7ge\u00b7weid'", "so", "k\u00fcnst\u00b7lich", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wunderbar formirt; da\u00df es unfehlbar denen,", "tokens": ["So", "wun\u00b7der\u00b7bar", "for\u00b7mirt", ";", "da\u00df", "es", "un\u00b7fehl\u00b7bar", "de\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$.", "KOUS", "PPER", "ADJD", "PDS", "$,"], "meter": "-+---+--+----", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Die es, als Menschen, schauen,", "tokens": ["Die", "es", ",", "als", "Men\u00b7schen", ",", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ein' holde Furcht erweckt, ein angenehmes Grauen.", "tokens": ["Ein'", "hol\u00b7de", "Furcht", "er\u00b7weckt", ",", "ein", "an\u00b7ge\u00b7neh\u00b7mes", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es leitete die\u00df Werck, voll Ehrfurcht, meinen Sinn", "tokens": ["Es", "lei\u00b7te\u00b7te", "die\u00df", "Werck", ",", "voll", "Ehr\u00b7furcht", ",", "mei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "NN", "$,", "ADJD", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf dieses Kunst-St\u00fccks Urblid hin:", "tokens": ["Auf", "die\u00b7ses", "Kunst\u00b7St\u00fccks", "Ur\u00b7blid", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich dacht' auf die Vollkommenheiten,", "tokens": ["Ich", "dacht'", "auf", "die", "Voll\u00b7kom\u00b7men\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Womit Gott unsern Leib, in so vollkomm'nem Grad,", "tokens": ["Wo\u00b7mit", "Gott", "un\u00b7sern", "Leib", ",", "in", "so", "voll\u00b7kom\u00b7m'\u00b7nem", "Grad", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "So wunderbarlich zu bereiten,", "tokens": ["So", "wun\u00b7der\u00b7bar\u00b7lich", "zu", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wunderbar gew\u00fcrdigt hat.", "tokens": ["So", "wun\u00b7der\u00b7bar", "ge\u00b7w\u00fcr\u00b7digt", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Viel hundert tausend Kleinigkeiten,", "tokens": ["Viel", "hun\u00b7dert", "tau\u00b7send", "Klei\u00b7nig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Woraus der Leib besteht, die ungez\u00e4hlte Menge", "tokens": ["Wo\u00b7raus", "der", "Leib", "be\u00b7steht", ",", "die", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der gantz mit Blut gef\u00fcllt-fast unsichtbaren G\u00e4nge", "tokens": ["Der", "gantz", "mit", "Blut", "ge\u00b7f\u00fcll\u00b7tfast", "un\u00b7sicht\u00b7ba\u00b7ren", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verwirrten meinen Geist, erf\u00fcllten meine Brust", "tokens": ["Ver\u00b7wirr\u00b7ten", "mei\u00b7nen", "Geist", ",", "er\u00b7f\u00fcll\u00b7ten", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit einer frohen Angst, mit einer bangen Lust.", "tokens": ["Mit", "ei\u00b7ner", "fro\u00b7hen", "Angst", ",", "mit", "ei\u00b7ner", "ban\u00b7gen", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich sprach mit recht ger\u00fchrter Seelen:", "tokens": ["Ich", "sprach", "mit", "recht", "ge\u00b7r\u00fchr\u00b7ter", "See\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Lungen luftigs Fleisch, des Magens scharfe Kraft,", "tokens": ["Der", "Lun\u00b7gen", "luf\u00b7tigs", "Fleisch", ",", "des", "Ma\u00b7gens", "schar\u00b7fe", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Hertzens Feur und Druck, der Leber Eigenschaft,", "tokens": ["Des", "Hert\u00b7zens", "Feur", "und", "Druck", ",", "der", "Le\u00b7ber", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Haut, N\u00e4gel, Fleisch, und Bein,", "tokens": ["Haut", ",", "N\u00e4\u00b7gel", ",", "Fleisch", ",", "und", "Bein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Nerven unsichtbare H\u00f6len,", "tokens": ["Der", "Ner\u00b7ven", "un\u00b7sicht\u00b7ba\u00b7re", "H\u00f6\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die voller Geistigkeit, und nicht zu z\u00e4hlen,", "tokens": ["Die", "vol\u00b7ler", "Geis\u00b7tig\u00b7keit", ",", "und", "nicht", "zu", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht zu begreifen seyn,", "tokens": ["Nicht", "zu", "be\u00b7grei\u00b7fen", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Beweisen ein allm\u00e4chtigs Wunder-Wesen.", "tokens": ["Be\u00b7wei\u00b7sen", "ein", "all\u00b7m\u00e4ch\u00b7tigs", "Wun\u00b7der\u00b7We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch wenn man recht erweg't; was Menschen insgemein", "tokens": ["Doch", "wenn", "man", "recht", "er\u00b7weg't", ";", "was", "Men\u00b7schen", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADJD", "VVPP", "$.", "PWS", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr eitle Dinge thun, nach welchem Tand sie sterben;", "tokens": ["F\u00fcr", "eit\u00b7le", "Din\u00b7ge", "thun", ",", "nach", "wel\u00b7chem", "Tand", "sie", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,", "APPR", "PWAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So scheint der gantze Mensch, in seinem gantzen Leben,", "tokens": ["So", "scheint", "der", "gant\u00b7ze", "Mensch", ",", "in", "sei\u00b7nem", "gant\u00b7zen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So vieler Kunst nicht werth zu seyn.", "tokens": ["So", "vie\u00b7ler", "Kunst", "nicht", "werth", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKNEG", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, sprach zu mir hierauf ein grosser Geist,", "tokens": ["Ja", ",", "sprach", "zu", "mir", "hier\u00b7auf", "ein", "gros\u00b7ser", "Geist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "APPR", "PPER", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der wohl mit allem Recht der Stadt Orackel heisst:", "tokens": ["Der", "wohl", "mit", "al\u00b7lem", "Recht", "der", "Stadt", "O\u00b7rac\u00b7kel", "heisst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PIS", "NN", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die\u00df scheinet wahr, doch mir f\u00e4llt dieses ein:", "tokens": ["Die\u00df", "schei\u00b7net", "wahr", ",", "doch", "mir", "f\u00e4llt", "die\u00b7ses", "ein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie liebreich mu\u00df doch unser Sch\u00f6pfer seyn,", "tokens": ["Wie", "lieb\u00b7reich", "mu\u00df", "doch", "un\u00b7ser", "Sch\u00f6p\u00b7fer", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der, wenn wir aller Glieder Gaben", "tokens": ["Der", ",", "wenn", "wir", "al\u00b7ler", "Glie\u00b7der", "Ga\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Thorheit angewendet haben,", "tokens": ["Zur", "Thor\u00b7heit", "an\u00b7ge\u00b7wen\u00b7det", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ja gar an Ihm h\u00f6chst-gr\u00f6blich uns verschuldet,", "tokens": ["Ja", "gar", "an", "Ihm", "h\u00f6chst\u00b7gr\u00f6b\u00b7lich", "uns", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Uns dennoch duldet!", "tokens": ["Uns", "den\u00b7noch", "dul\u00b7det", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER",