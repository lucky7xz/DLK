{"dta.poem.13238": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich selber halte mich zu diesem Ziel bereit,", "tokens": ["\u201e", "ich", "sel\u00b7ber", "hal\u00b7te", "mich", "zu", "die\u00b7sem", "Ziel", "be\u00b7reit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "VVFIN", "PRF", "APPR", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eich ziehe mit ins Feld; begleitet mich zum Streit!", "tokens": ["\u201e", "ich", "zie\u00b7he", "mit", "ins", "Feld", ";", "be\u00b7glei\u00b7tet", "mich", "zum", "Streit", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "APPRART", "NN", "$.", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201egOtt gebe meinem Arm Gewicht, dem Feind Erbeben!", "tokens": ["\u201e", "gOtt", "ge\u00b7be", "mei\u00b7nem", "Arm", "Ge\u00b7wicht", ",", "dem", "Feind", "Er\u00b7be\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eder Thron war lang genug den Waffen Prei\u00df gegeben.", "tokens": ["\u201e", "der", "Thron", "war", "lang", "ge\u00b7nug", "den", "Waf\u00b7fen", "Prei\u00df", "ge\u00b7ge\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "wor