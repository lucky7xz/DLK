{"dta.poem.10487": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Nun ist es Zeit, da\u00df ich mit Verstand", "tokens": ["Nun", "ist", "es", "Zeit", ",", "da\u00df", "ich", "mit", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mich aller Thorheit entled'ge;", "tokens": ["Mich", "al\u00b7ler", "Thor\u00b7heit", "ent\u00b7le\u00b7d'\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab' so lang als ein Com\u00f6diant", "tokens": ["Ich", "hab'", "so", "lang", "als", "ein", "Co\u00b7m\u00f6\u00b7di\u00b7ant"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit dir gespielt die Com\u00f6die.", "tokens": ["Mit", "dir", "ge\u00b7spielt", "die", "Co\u00b7m\u00f6\u00b7die", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die pr\u00e4cht'gen Coulissen, sie waren bemalt", "tokens": ["Die", "pr\u00e4cht'\u00b7gen", "Cou\u00b7lis\u00b7sen", ",", "sie", "wa\u00b7ren", "be\u00b7malt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im hochromantischen Style,", "tokens": ["Im", "hoc\u00b7hro\u00b7man\u00b7ti\u00b7schen", "Sty\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mein Rittermantel hat goldig gestrahlt,", "tokens": ["Mein", "Rit\u00b7ter\u00b7man\u00b7tel", "hat", "gol\u00b7dig", "ge\u00b7strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich f\u00fchlte die feinsten Gef\u00fchle.", "tokens": ["Ich", "f\u00fchl\u00b7te", "die", "feins\u00b7ten", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und nun ich mich gar s\u00e4uberlich", "tokens": ["Und", "nun", "ich", "mich", "gar", "s\u00e4u\u00b7ber\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "PRF", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des tollen Tands entled'ge.", "tokens": ["Des", "tol\u00b7len", "Tands", "ent\u00b7le\u00b7d'\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch immer elend f\u00fchl' ich mich,", "tokens": ["Noch", "im\u00b7mer", "e\u00b7lend", "f\u00fchl'", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als spielt' ich noch immer Com\u00f6die.", "tokens": ["Als", "spielt'", "ich", "noch", "im\u00b7mer", "Co\u00b7m\u00f6\u00b7die", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "ADV", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Ach Gott! im Scherz und unbewu\u00dft", "tokens": ["Ach", "Gott", "!", "im", "Scherz", "und", "un\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "APPRART", "NN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach ich was ich gef\u00fchlet;", "tokens": ["Sprach", "ich", "was", "ich", "ge\u00b7f\u00fch\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' mit dem eignen Tod in der Brust", "tokens": ["Ich", "hab'", "mit", "dem", "eig\u00b7nen", "Tod", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den sterbenden Fechter gespielet.", "tokens": ["Den", "ster\u00b7ben\u00b7den", "Fech\u00b7ter", "ge\u00b7spie\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}