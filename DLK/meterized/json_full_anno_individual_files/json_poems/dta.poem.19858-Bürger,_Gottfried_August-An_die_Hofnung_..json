{"dta.poem.19858": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An die Hofnung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Wolth\u00e4tigste der Feen!               ", "tokens": ["Wolt\u00b7h\u00e4\u00b7tigs\u00b7te", "der", "Feen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.2": {"text": "Du, mit dem weichen Sin,", "tokens": ["Du", ",", "mit", "dem", "wei\u00b7chen", "Sin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Himmel ausersehen,", "tokens": ["Vom", "Him\u00b7mel", "au\u00b7ser\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Menschentr\u00f6sterin!", "tokens": ["Zur", "Men\u00b7schen\u00b7tr\u00f6s\u00b7te\u00b7rin", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sch\u00f6n, wie die Morgenstunde,", "tokens": ["Sch\u00f6n", ",", "wie", "die", "Mor\u00b7gen\u00b7stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit rosigem Gesicht,", "tokens": ["Mit", "ro\u00b7si\u00b7gem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mit dem Purpurmunde,", "tokens": ["Und", "mit", "dem", "Pur\u00b7pur\u00b7mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Der Honigrede spricht!", "tokens": ["Der", "Ho\u00b7ni\u00b7gre\u00b7de", "spricht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du, die mich oft erheitert,", "tokens": ["Du", ",", "die", "mich", "oft", "er\u00b7hei\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vernim, o Hofnung, mich!", "tokens": ["Ver\u00b7nim", ",", "o", "Hof\u00b7nung", ",", "mich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKVZ", "$,", "FM", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein freies Herz erweitert", "tokens": ["Mein", "frei\u00b7es", "Herz", "er\u00b7wei\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Lobges\u00e4ngen sich.", "tokens": ["Zu", "Lob\u00b7ge\u00b7s\u00e4n\u00b7gen", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie lodern mit dem Feuer", "tokens": ["Sie", "lo\u00b7dern", "mit", "dem", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des frommen Danks empor.", "tokens": ["Des", "from\u00b7men", "Danks", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O neig auf meine Leier", "tokens": ["O", "neig", "auf", "mei\u00b7ne", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dein algef\u00e4llig Ohr!", "tokens": ["Dein", "al\u00b7ge\u00b7f\u00e4l\u00b7lig", "Ohr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als, mit dem goldnen Alter,", "tokens": ["Als", ",", "mit", "dem", "gold\u00b7nen", "Al\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Unschuld Gl\u00fck entwich,", "tokens": ["Der", "Un\u00b7schuld", "Gl\u00fck", "ent\u00b7wich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sandten die Erhalter", "tokens": ["Da", "sand\u00b7ten", "die", "Er\u00b7hal\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gequ\u00e4lter Menschen dich:", "tokens": ["Ge\u00b7qu\u00e4l\u00b7ter", "Men\u00b7schen", "dich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df du das Ungl\u00fck schw\u00e4chtest,", "tokens": ["Da\u00df", "du", "das", "Un\u00b7gl\u00fck", "schw\u00e4ch\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Lasters Riesensohn,", "tokens": ["Des", "Las\u00b7ters", "Rie\u00b7sen\u00b7sohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Freuden wiederbr\u00e4chtest,", "tokens": ["Und", "Freu\u00b7den", "wie\u00b7der\u00b7br\u00e4ch\u00b7test", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die mit der Unschuld flohn.", "tokens": ["Die", "mit", "der", "Un\u00b7schuld", "flohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun wandelt im Geleite", "tokens": ["Nun", "wan\u00b7delt", "im", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir ewig Ruhe nach.", "tokens": ["Dir", "e\u00b7wig", "Ru\u00b7he", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Aufruhr und im Streite", "tokens": ["Im", "Auf\u00b7ruhr", "und", "im", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit grausem Ungemach,", "tokens": ["Mit", "grau\u00b7sem", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ertheilest du dem M\u00fcden,", "tokens": ["Er\u00b7thei\u00b7lest", "du", "dem", "M\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Eh gar sein Mut erschlaft,", "tokens": ["Eh", "gar", "sein", "Mut", "er\u00b7schlaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erquickung oder Frieden,", "tokens": ["Er\u00b7quic\u00b7kung", "o\u00b7der", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und neue Heldenkraft.", "tokens": ["Und", "neu\u00b7e", "Hel\u00b7den\u00b7kraft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du scheuchest von dem Krieger", "tokens": ["Du", "scheu\u00b7chest", "von", "dem", "Krie\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Grauen der Gefar,", "tokens": ["Das", "Grau\u00b7en", "der", "Ge\u00b7far", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und tr\u00f6stest arme Pfl\u00fcger,", "tokens": ["Und", "tr\u00f6s\u00b7test", "ar\u00b7me", "Pfl\u00fc\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im d\u00fcrren Mangeljahr.", "tokens": ["Im", "d\u00fcr\u00b7ren", "Man\u00b7gel\u00b7jahr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aus Wind und lauem Regen,", "tokens": ["Aus", "Wind", "und", "lau\u00b7em", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus Sonnenschein und Thau,", "tokens": ["Aus", "Son\u00b7nen\u00b7schein", "und", "Thau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Verk\u00fcndest du den Segen", "tokens": ["Ver\u00b7k\u00fcn\u00b7dest", "du", "den", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der zartbesprosten Au.", "tokens": ["Der", "zart\u00b7be\u00b7spros\u00b7ten", "Au", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Von deinem Fl\u00fcgel d\u00fcftet", "tokens": ["Von", "dei\u00b7nem", "Fl\u00fc\u00b7gel", "d\u00fcf\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Balsam f\u00fcr den Schmerz.", "tokens": ["Ein", "Bal\u00b7sam", "f\u00fcr", "den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei seinem Weben l\u00fcftet", "tokens": ["Bei", "sei\u00b7nem", "We\u00b7ben", "l\u00fcf\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich das beklomne Herz.", "tokens": ["Sich", "das", "be\u00b7klom\u00b7ne", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein Odem hauchet Kr\u00e4fte", "tokens": ["Dein", "O\u00b7dem", "hau\u00b7chet", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verwelktem Elend ein;", "tokens": ["Ver\u00b7welk\u00b7tem", "E\u00b7lend", "ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erstorbne kalte S\u00e4fte", "tokens": ["E\u00b7rstorb\u00b7ne", "kal\u00b7te", "S\u00e4f\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Belebt dein milder Schein.", "tokens": ["Be\u00b7lebt", "dein", "mil\u00b7der", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du bist es, die dem Kranken", "tokens": ["Du", "bist", "es", ",", "die", "dem", "Kran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Todesqualen stilt;", "tokens": ["Die", "To\u00b7des\u00b7qua\u00b7len", "stilt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit wonnigen Gedanken", "tokens": ["Mit", "won\u00b7ni\u00b7gen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Zukunft ihn erf\u00fclt;", "tokens": ["Von", "Zu\u00b7kunft", "ihn", "er\u00b7f\u00fclt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In seinen letzten Tr\u00e4umen", "tokens": ["In", "sei\u00b7nen", "letz\u00b7ten", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Paradies ihm zeigt,", "tokens": ["Das", "Pa\u00b7ra\u00b7dies", "ihm", "zeigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und unter gr\u00fcnen B\u00e4umen", "tokens": ["Und", "un\u00b7ter", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Lebensschale reicht.", "tokens": ["Die", "Le\u00b7bens\u00b7scha\u00b7le", "reicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die du den armen Sklaven", "tokens": ["Die", "du", "den", "ar\u00b7men", "Skla\u00b7ven"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im dunkeln Schacht erfreust;", "tokens": ["Im", "dun\u00b7keln", "Schacht", "er\u00b7freust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von unverdienten Strafen", "tokens": ["Von", "un\u00b7ver\u00b7dien\u00b7ten", "Stra\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erl\u00f6sung prophezeist;", "tokens": ["Er\u00b7l\u00f6\u00b7sung", "pro\u00b7phe\u00b7zeist", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem im Tyrhenermeere", "tokens": ["Dem", "im", "Ty\u00b7rhen\u00b7er\u00b7mee\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Last des Ruders hebst,", "tokens": ["Die", "Last", "des", "Ru\u00b7ders", "hebst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und \u00fcber der Galeere,", "tokens": ["Und", "\u00fc\u00b7ber", "der", "Ga\u00b7lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie Fr\u00fchlingswehen, schwebst;", "tokens": ["Wie", "Fr\u00fch\u00b7lings\u00b7we\u00b7hen", ",", "schwebst", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O G\u00f6ttin! Deine Stimme", "tokens": ["O", "G\u00f6t\u00b7tin", "!", "Dei\u00b7ne", "Stim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "T\u00f6nt der Verzweifelung,", "tokens": ["T\u00f6nt", "der", "Ver\u00b7zwei\u00b7fe\u00b7lung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "In ihrem tauben Grimme,", "tokens": ["In", "ih\u00b7rem", "tau\u00b7ben", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", 