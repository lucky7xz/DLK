{"dta.poem.5861": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch kamen endlich Weise, die", "tokens": ["Doch", "ka\u00b7men", "end\u00b7lich", "Wei\u00b7se", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit unabl\u00e4\u00dflich grosser M\u00fch", "tokens": ["Mit", "un\u00b7ab\u00b7l\u00e4\u00df\u00b7lich", "gros\u00b7ser", "M\u00fch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Creatur Geheimni\u00df auszufinden,", "tokens": ["Der", "Crea\u00b7tur", "Ge\u00b7heim\u00b7ni\u00df", "aus\u00b7zu\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und zu ergr\u00fcnden", "tokens": ["Und", "zu", "er\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Sich stets bestrebeten. Allein,", "tokens": ["Sich", "stets", "be\u00b7stre\u00b7be\u00b7ten", ".", "Al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "$.", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Indem sie sich gedachten zu erkl\u00e4ren;", "tokens": ["In\u00b7dem", "sie", "sich", "ge\u00b7dach\u00b7ten", "zu", "er\u00b7kl\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So stimmten ihre Lehren", "tokens": ["So", "stimm\u00b7ten", "ih\u00b7re", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht mit einander \u00fcberein.", "tokens": ["Nicht", "mit", "ein\u00b7an\u00b7der", "\u00fc\u00b7be\u00b7re\u00b7in", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PRF", "PTKVZ", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Man h\u00f6r\u2019 Hesiodum, er spricht: Es werden", "tokens": ["Man", "h\u00f6r'", "He\u00b7sio\u00b7dum", ",", "er", "spricht", ":", "Es", "wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "NE", "$,", "PPER", "VVFIN", "$.", "PPER", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Die Elementen all\u2019 erzeuget aus der Erden.", "tokens": ["Die", "E\u00b7le\u00b7men\u00b7ten", "all'", "er\u00b7zeu\u00b7get", "aus", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Hingegen Thales will, da\u00df aus der Fluht allein", "tokens": ["Hin\u00b7ge\u00b7gen", "Tha\u00b7les", "will", ",", "da\u00df", "aus", "der", "Fluht", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VMFIN", "$,", "KOUS", "APPR", "ART", "NN", "ADV"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Dieselbigen entstanden seyn.", "tokens": ["Die\u00b7sel\u00b7bi\u00b7gen", "ent\u00b7stan\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Anaximenes spricht: die Lufft hat sie gemacht,", "tokens": ["An\u00b7a\u00b7xi\u00b7me\u00b7nes", "spricht", ":", "die", "Lufft", "hat", "sie", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Das Feur, spricht Heraclit, hat sie hervor gebracht.", "tokens": ["Das", "Feur", ",", "spricht", "Her\u00b7a\u00b7clit", ",", "hat", "sie", "her\u00b7vor", "ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "NE", "$,", "VAFIN", "PPER", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Pythagoras erbauet sie", "tokens": ["Py\u00b7tha\u00b7go\u00b7ras", "er\u00b7bau\u00b7et", "sie"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Aus Zahlen und der Harmonie,", "tokens": ["Aus", "Zah\u00b7len", "und", "der", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der setzet Nacht und Licht, Gerad und Ungerad;", "tokens": ["Der", "set\u00b7zet", "Nacht", "und", "Licht", ",", "Ge\u00b7rad", "und", "Un\u00b7ge\u00b7rad", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da Anaxagoras, Leucippus, Epieur,", "tokens": ["Da", "A\u00b7na\u00b7xa\u00b7go\u00b7ras", ",", "Leu\u00b7cip\u00b7pus", ",", "E\u00b7pie\u00b7ur", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "NE", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Sonnen-St\u00e4ubgen nur", "tokens": ["Die", "Son\u00b7nen\u00b7St\u00e4ub\u00b7gen", "nur"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Zum Urquell aller Dinge hat.", "tokens": ["Zum", "Ur\u00b7quell", "al\u00b7ler", "Din\u00b7ge", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wenn Aristoteles die Lufft, die Fluht,", "tokens": ["Wenn", "A\u00b7ris\u00b7to\u00b7te\u00b7les", "die", "Lufft", ",", "die", "Fluht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Die Erde, sammt der Gluht,", "tokens": ["Die", "Er\u00b7de", ",", "sammt", "der", "Gluht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Zu seinem Grunde setzt; hat Plato die Jdee", "tokens": ["Zu", "sei\u00b7nem", "Grun\u00b7de", "setzt", ";", "hat", "Pla\u00b7to", "die", "Jdee"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "VAFIN", "NE", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.24": {"text": "Und die Materie.", "tokens": ["Und", "die", "Ma\u00b7te\u00b7rie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}