{"dta.poem.14451": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Durch's Leben schleichen feindlich fremde Stunden,", "tokens": ["Durch's", "Le\u00b7ben", "schlei\u00b7chen", "feind\u00b7lich", "frem\u00b7de", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Aengsten aus der Brust hinunterlauschen,", "tokens": ["Wo", "A\u00b7engs\u00b7ten", "aus", "der", "Brust", "hin\u00b7un\u00b7ter\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Verworr'ne Worte mit dem Abgrund tauschen,", "tokens": ["Ver\u00b7worr'\u00b7ne", "Wor\u00b7te", "mit", "dem", "Ab\u00b7grund", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drin bodenlose Nacht nur ward erfunden.", "tokens": ["Drin", "bo\u00b7den\u00b7lo\u00b7se", "Nacht", "nur", "ward", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}