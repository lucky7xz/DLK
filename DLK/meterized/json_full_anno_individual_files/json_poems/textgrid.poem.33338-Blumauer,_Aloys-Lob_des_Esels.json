{"textgrid.poem.33338": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Lob des Esels", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gutes Thier, auf dessen Haut wir schreiben,", "tokens": ["Du", "gu\u00b7tes", "Thier", ",", "auf", "des\u00b7sen", "Haut", "wir", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das uns bald tr\u00e4gt, bald f\u00fchrt,", "tokens": ["Das", "uns", "bald", "tr\u00e4gt", ",", "bald", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nein, l\u00e4nger will ich dir nicht schuldig bleiben", "tokens": ["Nein", ",", "l\u00e4n\u00b7ger", "will", "ich", "dir", "nicht", "schul\u00b7dig", "blei\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJD", "VMFIN", "PPER", "PPER", "PTKNEG", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Lob, das dir geb\u00fchrt.", "tokens": ["Das", "Lob", ",", "das", "dir", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Man spottet deiner Ohren widerrechtlich,", "tokens": ["Man", "spot\u00b7tet", "dei\u00b7ner", "Oh\u00b7ren", "wi\u00b7der\u00b7recht\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00f6hnt dich, armer Tropf!", "tokens": ["Und", "h\u00f6hnt", "dich", ",", "ar\u00b7mer", "Tropf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch tr\u00f6ste dich; sie wurden nur ver\u00e4chtlich", "tokens": ["Doch", "tr\u00f6s\u00b7te", "dich", ";", "sie", "wur\u00b7den", "nur", "ver\u00b7\u00e4cht\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An eines K\u00f6nigs Kopf.", "tokens": ["An", "ei\u00b7nes", "K\u00f6\u00b7nigs", "Kopf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wer es dir etwa verargen k\u00f6nnte,", "tokens": ["Und", "wer", "es", "dir", "et\u00b7wa", "ver\u00b7ar\u00b7gen", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df du so langsam bist,", "tokens": ["Da\u00df", "du", "so", "lang\u00b7sam", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der denke, da\u00df der Spruch: ", "tokens": ["Der", "den\u00b7ke", ",", "da\u00df", "der", "Spruch", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Weisen Losung ist.", "tokens": ["Der", "Wei\u00b7sen", "Lo\u00b7sung", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du bist aus allen Thieren, die wir reiten,", "tokens": ["Du", "bist", "aus", "al\u00b7len", "Thie\u00b7ren", ",", "die", "wir", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein ein Sonntagskind;", "tokens": ["Al\u00b7lein", "ein", "Sonn\u00b7tags\u00b7kind", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sahst dereinst den Engel schon von weiten,", "tokens": ["Du", "sahst", "de\u00b7reinst", "den", "En\u00b7gel", "schon", "von", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Bileam war blind.", "tokens": ["Und", "Bi\u00b7leam", "war", "blind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Du bist das Bild der nun in unsern Tagen", "tokens": ["Du", "bist", "das", "Bild", "der", "nun", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gepries'nen Duldsamkeit!", "tokens": ["Ge\u00b7pries'\u00b7nen", "Duld\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir gilt es gleich Gold oder Mist zu tragen,", "tokens": ["Dir", "gilt", "es", "gleich", "Gold", "o\u00b7der", "Mist", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und h\u00e4ltst, wenn man dich bl\u00e4ut.", "tokens": ["Und", "h\u00e4ltst", ",", "wenn", "man", "dich", "bl\u00e4ut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du bist das Thier, das seinem Herrn zur Speise", "tokens": ["Du", "bist", "das", "Thier", ",", "das", "sei\u00b7nem", "Herrn", "zur", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mehl tr\u00e4gt und Disteln fri\u00dft:", "tokens": ["Mehl", "tr\u00e4gt", "und", "Dis\u00b7teln", "fri\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer l\u00e4ugnet nun, da\u00df du auf diese Weise", "tokens": ["Wer", "l\u00e4ug\u00b7net", "nun", ",", "da\u00df", "du", "auf", "die\u00b7se", "Wei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der beste B\u00fcrger bist!", "tokens": ["Der", "bes\u00b7te", "B\u00fcr\u00b7ger", "bist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auch ist kein Thier an Freunden und Bekannten", "tokens": ["Auch", "ist", "kein", "Thier", "an", "Freun\u00b7den", "und", "Be\u00b7kann\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So reich, als du es bist,", "tokens": ["So", "reich", ",", "als", "du", "es", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Obgleich von deinen Br\u00fcdern und Verwandten", "tokens": ["Ob\u00b7gleich", "von", "dei\u00b7nen", "Br\u00fc\u00b7dern", "und", "Ver\u00b7wand\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht jeder Disteln fri\u00dft.", "tokens": ["Nicht", "je\u00b7der", "Dis\u00b7teln", "fri\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und singst du gleich nicht so, wie die Nachtigallen,", "tokens": ["Und", "singst", "du", "gleich", "nicht", "so", ",", "wie", "die", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So ist doch laut dein Ton!", "tokens": ["So", "ist", "doch", "laut", "dein", "Ton", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "D'rum braucht man auf dem Weg des Ruhms vor allen", "tokens": ["D'\u00b7rum", "braucht", "man", "auf", "dem", "Weg", "des", "Ruhms", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "APPR", "ART", "NN", "ART", "NN", "APPR", "PIAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Dich nun zum Postillon.", "tokens": ["Dich", "nun", "zum", "Pos\u00b7til\u00b7lon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.9": {"line.1": {"text": "Bei alle dem ist dir kein Thier auf Erden", "tokens": ["Bei", "al\u00b7le", "dem", "ist", "dir", "kein", "Thier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PDS", "VAFIN", "PPER", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich an Gen\u00fcgsamkeit;", "tokens": ["Gleich", "an", "Ge\u00b7n\u00fcg\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du tr\u00e4gst trotz all den Plagen und Beschwerden,", "tokens": ["Du", "tr\u00e4gst", "trotz", "all", "den", "Pla\u00b7gen", "und", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein simples, graues Kleid.", "tokens": ["Ein", "sim\u00b7ples", ",", "grau\u00b7es", "Kleid", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du lebst mit deinen Disteln hier zufrieden,", "tokens": ["Du", "lebst", "mit", "dei\u00b7nen", "Dis\u00b7teln", "hier", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dir dein Flei\u00df gewinnt,", "tokens": ["Die", "dir", "dein", "Flei\u00df", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mancher, ach! fri\u00dft Ananas hienieden,", "tokens": ["Und", "man\u00b7cher", ",", "ach", "!", "fri\u00dft", "A\u00b7na\u00b7nas", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "ITJ", "$.", "VVFIN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Disteln nicht verdient.", "tokens": ["Der", "Dis\u00b7teln", "nicht", "ver\u00b7dient", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du gutes Thier, auf dessen Haut wir schreiben,", "tokens": ["Du", "gu\u00b7tes", "Thier", ",", "auf", "des\u00b7sen", "Haut", "wir", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word