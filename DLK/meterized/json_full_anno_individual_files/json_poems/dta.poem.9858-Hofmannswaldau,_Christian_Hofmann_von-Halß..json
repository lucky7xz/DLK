{"dta.poem.9858": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Hal\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seht meine perlen an/ die Venus selbst getr\u00e4ncket", "tokens": ["Seht", "mei\u00b7ne", "per\u00b7len", "an", "/", "die", "Ve\u00b7nus", "selbst", "ge\u00b7tr\u00e4n\u00b7cket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$(", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In ihrem liebes-scho\u00df: Seht was sie mir geschencket/", "tokens": ["In", "ih\u00b7rem", "lie\u00b7bes\u00b7scho\u00df", ":", "Seht", "was", "sie", "mir", "ge\u00b7schen\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VVFIN", "PWS", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als umb der mutter hal\u00df Cupido sich geschwencket/", "tokens": ["Als", "umb", "der", "mut\u00b7ter", "hal\u00df", "Cu\u00b7pi\u00b7do", "sich", "ge\u00b7schwen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "NE", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seine s\u00fcsse pein ins helffenbein versencket;", "tokens": ["Und", "sei\u00b7ne", "s\u00fcs\u00b7se", "pein", "ins", "helf\u00b7fen\u00b7bein", "ver\u00b7sen\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier lieget schnee und glut im gleichen krey\u00df geschwencket/", "tokens": ["Hier", "lie\u00b7get", "schnee", "und", "glut", "im", "glei\u00b7chen", "krey\u00df", "ge\u00b7schwen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich bin der thurm/ an dem der liebe r\u00fcstzeug hencket.", "tokens": ["Ich", "bin", "der", "thurm", "/", "an", "dem", "der", "lie\u00b7be", "r\u00fcst\u00b7zeug", "hen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "APPR", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}