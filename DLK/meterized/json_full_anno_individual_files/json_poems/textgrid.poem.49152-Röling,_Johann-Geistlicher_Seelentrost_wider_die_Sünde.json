den?", "tokens": ["Wer", "will", "mich", "von", "dir", "schei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Tod? Der ist mehr Freund als Feind;", "tokens": ["Der", "Tod", "?", "Der", "ist", "mehr", "Freund", "als", "Feind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "VAFIN", "PIAT", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Welt? Die will ich meiden;", "tokens": ["Die", "Welt", "?", "Die", "will", "ich", "mei\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Teufel? Dessen Wort gilt nicht;", "tokens": ["Der", "Teu\u00b7fel", "?", "Des\u00b7sen", "Wort", "gilt", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die H\u00f6ll'? Die ist zu Grund' gericht.", "tokens": ["Die", "H\u00f6ll'", "?", "Die", "ist", "zu", "Grund'", "ge\u00b7richt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Du hast mich gar zu lieb, mein Hort,", "tokens": ["Du", "hast", "mich", "gar", "zu", "lieb", ",", "mein", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich dich, Jesu, wieder,", "tokens": ["Und", "ich", "dich", ",", "Je\u00b7su", ",", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "PRF", "$,", "NE", "$,", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum schl\u00e4get weder hier noch dort", "tokens": ["Drum", "schl\u00e4\u00b7get", "we\u00b7der", "hier", "noch", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "KON", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts unser B\u00fcndnis nieder.", "tokens": ["Nichts", "un\u00b7ser", "B\u00fcnd\u00b7nis", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du bist ja mein und ich bin dein,", "tokens": ["Du", "bist", "ja", "mein", "und", "ich", "bin", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "KON", "PPER", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie k\u00f6nnen wir geschieden sein?", "tokens": ["Wie", "k\u00f6n\u00b7nen", "wir", "ge\u00b7schie\u00b7den", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Schreib', Herr, dies meinem Herzen ein", "tokens": ["Schreib'", ",", "Herr", ",", "dies", "mei\u00b7nem", "Her\u00b7zen", "ein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PDS", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinen Liebesfingern,", "tokens": ["Mit", "dei\u00b7nen", "Lie\u00b7bes\u00b7fin\u00b7gern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wird die schwere S\u00fcndenpein", "tokens": ["So", "wird", "die", "schwe\u00b7re", "S\u00fcn\u00b7den\u00b7pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich unvermerket ringern.", "tokens": ["Sich", "un\u00b7ver\u00b7mer\u00b7ket", "rin\u00b7gern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie gro\u00df die Schuld, so freu' ich mich,", "tokens": ["Wie", "gro\u00df", "die", "Schuld", ",", "so", "freu'", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Seh' ich nur deinen Gnadenstrich.", "tokens": ["Seh'", "ich", "nur", "dei\u00b7nen", "Gna\u00b7den\u00b7strich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}