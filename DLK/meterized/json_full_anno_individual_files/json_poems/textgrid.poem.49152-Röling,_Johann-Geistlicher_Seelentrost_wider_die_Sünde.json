, "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich an dich gedenke", "tokens": ["Wenn", "ich", "an", "dich", "ge\u00b7den\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Seel' in deinem Blut,", "tokens": ["Und", "mei\u00b7ne", "Seel'", "in", "dei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O liebster Heiland, tr\u00e4nke.", "tokens": ["O", "liebs\u00b7ter", "Hei\u00b7land", ",", "tr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Tr\u00f6pflein nur von diesem Saft", "tokens": ["Ein", "Tr\u00f6pf\u00b7lein", "nur", "von", "die\u00b7sem", "Saft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schl\u00e4gt aller meiner Feinde Kraft.", "tokens": ["Schl\u00e4gt", "al\u00b7ler", "mei\u00b7ner", "Fein\u00b7de", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dann ruf' ich aus getrost und froh:", "tokens": ["Dann", "ruf'", "ich", "aus", "ge\u00b7trost", "und", "froh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wi