{"textgrid.poem.49152": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Geistlicher Seelentrost wider die S\u00fcnde", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir f\u00e4llet, Jesu, immer ein", "tokens": ["Mir", "f\u00e4l\u00b7let", ",", "Je\u00b7su", ",", "im\u00b7mer", "ein"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NE", "$,", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Menge meiner S\u00fcnden,", "tokens": ["Die", "Men\u00b7ge", "mei\u00b7ner", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und macht mir oft so gro\u00dfe Pein,", "tokens": ["Und", "macht", "mir", "oft", "so", "gro\u00b7\u00dfe", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich mich nicht kann finden;", "tokens": ["Da\u00df", "ich", "mich", "nicht", "kann", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Teufel ruht auch nicht dabei", "tokens": ["Der", "Teu\u00b7fel", "ruht", "auch", "nicht", "da\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und spricht, da\u00df ich sein eigen sei.", "tokens": ["Und", "spricht", ",", "da\u00df", "ich", "sein", "ei\u00b7gen", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie elend bin ich denn daran!", "tokens": ["Wie", "e\u00b7lend", "bin", "ich", "denn", "da\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinauf darf ich nicht schauen,", "tokens": ["Hin\u00b7auf", "darf", "ich", "nicht", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um mich ist Nichts, das helfen kann,", "tokens": ["Um", "mich", "ist", "Nichts", ",", "das", "hel\u00b7fen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "PIS", "$,", "PDS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unter mir ist Grauen,", "tokens": ["Und", "un\u00b7ter", "mir", "ist", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ja, wo ich mich hin kehr' und wend',", "tokens": ["Ja", ",", "wo", "ich", "mich", "hin", "kehr'", "und", "wend'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "PPER", "PRF", "APZR", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da seh' ich alles Trostes End'.", "tokens": ["Da", "seh'", "ich", "al\u00b7les", "Tros\u00b7tes", "End'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch fass' ich wieder einen Muth,", "tokens": ["Doch", "fass'", "ich", "wie\u00b7der", "ei\u00b7nen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich an dich gedenke", "tokens": ["Wenn", "ich", "an", "dich", "ge\u00b7den\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Seel' in deinem Blut,", "tokens": ["Und", "mei\u00b7ne", "Seel'", "in", "dei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O liebster Heiland, tr\u00e4nke.", "tokens": ["O", "liebs\u00b7ter", "Hei\u00b7land", ",", "tr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Tr\u00f6pflein nur von diesem Saft", "tokens": ["Ein", "Tr\u00f6pf\u00b7lein", "nur", "von", "die\u00b7sem", "Saft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schl\u00e4gt aller meiner Feinde Kraft.", "tokens": ["Schl\u00e4gt", "al\u00b7ler", "mei\u00b7ner", "Fein\u00b7de", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann ruf' ich aus getrost und froh:", "tokens": ["Dann", "ruf'", "ich", "aus", "ge\u00b7trost", "und", "froh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer will mich nun verdammen?", "tokens": ["Wer", "will", "mich", "nun", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ich ges\u00fcndigt, wann und wo,", "tokens": ["Was", "ich", "ge\u00b7s\u00fcn\u00b7digt", ",", "wann", "und", "wo", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PWAV", "KON", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das tilgest du zusammen,", "tokens": ["Das", "til\u00b7gest", "du", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du, der du gingst zur H\u00f6lle ein,", "tokens": ["Du", ",", "der", "du", "gingst", "zur", "H\u00f6l\u00b7le", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf das ich sollte seelig sein.", "tokens": ["Auf", "das", "ich", "soll\u00b7te", "see\u00b7lig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du hast mich, Herr, mit dir vereint,", "tokens": ["Du", "hast", "mich", ",", "Herr", ",", "mit", "dir", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer will mich von dir scheiden?", "tokens": ["Wer", "will", "mich", "von", "dir", "schei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Tod? Der ist mehr Freund als Feind;", "tokens": ["Der", "Tod", "?", "Der", "ist", "mehr", "Freund", "als", "Feind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "VAFIN", "PIAT", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Welt? Die will ich meiden;", "tokens": ["Die", "Welt", "?", "Die", "will", "ich", "mei\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Teufel? Dessen Wort gilt nicht;", "tokens": ["Der", "Teu\u00b7fel", "?", "Des\u00b7sen", "Wort", "gilt", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die H\u00f6ll'? Die ist zu Grund' gericht.", "tokens": ["Die", "H\u00f6ll'", "?", "Die", "ist", "zu", "Grund'", "ge\u00b7richt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du hast mich gar zu lieb, mein Hort,", "tokens": ["Du", "hast", "mich", "gar", "zu", "lieb", ",", "mein", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich dich, Jesu, wieder,", "tokens": ["Und", "ich", "dich", ",", "Je\u00b7su", ",", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "PRF", "$,", "NE", "$,", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum schl\u00e4get weder hier noch dort", "tokens": ["Drum", "schl\u00e4\u00b7get", "we\u00b7der", "hier", "noch", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "KON", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts unser B\u00fcndnis nieder.", "tokens": ["Nichts", "un\u00b7ser", "B\u00fcnd\u00b7nis", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du bist ja mein und ich bin dein,", "tokens": ["Du", "bist", "ja", "mein", "und", "ich", "bin", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "KON", "PPER", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie k\u00f6nnen wir geschieden sein?", "tokens": ["Wie", "k\u00f6n\u00b7nen", "wir", "ge\u00b7schie\u00b7den", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Schreib', Herr, dies meinem Herzen ein", "tokens": ["Schreib'", ",", "Herr", ",", "dies", "mei\u00b7nem", "Her\u00b7zen", "ein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PDS", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinen Liebesfingern,", "tokens": ["Mit", "dei\u00b7nen", "Lie\u00b7bes\u00b7fin\u00b7gern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So wird die schwere S\u00fcndenpein", "tokens": ["So", "wird", "die", "schwe\u00b7re", "S\u00fcn\u00b7den\u00b7pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich unvermerket ringern.", "tokens": ["Sich", "un\u00b7ver\u00b7mer\u00b7ket", "rin\u00b7gern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie gro\u00df die Schuld, so freu' ich mich,", "tokens": ["Wie", "gro\u00df", "die", "Schuld", ",", "so", "freu'", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Seh' ich nur deinen Gnadenstrich.", "tokens": ["Seh'", "ich", "nur", "dei\u00b7nen", "Gna\u00b7den\u00b7strich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mir f\u00e4llet, Jesu, immer ein", "tokens": ["Mir", "f\u00e4l\u00b7le