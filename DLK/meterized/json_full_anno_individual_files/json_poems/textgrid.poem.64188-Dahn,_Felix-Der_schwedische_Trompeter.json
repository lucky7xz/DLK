{"textgrid.poem.64188": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der schwedische Trompeter", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was klingt so hell und heiter zu Librach auf der Au?", "tokens": ["Was", "klingt", "so", "hell", "und", "hei\u00b7ter", "zu", "Lib\u00b7rach", "auf", "der", "Au", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das ist ein Schwedenreiter mit der Sch\u00e4rpe gelb und blau.", "tokens": ["Das", "ist", "ein", "Schwe\u00b7den\u00b7rei\u00b7ter", "mit", "der", "Sch\u00e4r\u00b7pe", "gelb", "und", "blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Das war ein frommer Beter, ein tapfrer Degen auch", "tokens": ["Das", "war", "ein", "from\u00b7mer", "Be\u00b7ter", ",", "ein", "tapf\u00b7rer", "De\u00b7gen", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der wackere Trompeter: \u2013 das war so Schwedenbrauch.", "tokens": ["Der", "wa\u00b7cke\u00b7re", "Trom\u00b7pe\u00b7ter", ":", "\u2013", "das", "war", "so", "Schwe\u00b7den\u00b7brauch", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "PDS", "VAFIN", "ADV", "NN", "$."], "meter": "-+--+---+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Zum Wrangel soll er's tragen von des K\u00f6nigs eigner Hand,", "tokens": ["Zum", "Wran\u00b7gel", "soll", "er's", "tra\u00b7gen", "von", "des", "K\u00f6\u00b7nigs", "eig\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PIS", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wie sie den Tilly schlagen, der Magdeburg verbrannt.", "tokens": ["Wie", "sie", "den", "Til\u00b7ly", "schla\u00b7gen", ",", "der", "Mag\u00b7de\u00b7burg", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NE", "VVINF", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Er zieht auf schlimmer Reise: und doch, dem Feind zum Spott,", "tokens": ["Er", "zieht", "auf", "schlim\u00b7mer", "Rei\u00b7se", ":", "und", "doch", ",", "dem", "Feind", "zum", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "KON", "ADV", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bl\u00e4st er die k\u00fchne Weise: \u00bbEine feste Burg ist Gott!\u00ab", "tokens": ["Bl\u00e4st", "er", "die", "k\u00fch\u00b7ne", "Wei\u00b7se", ":", "\u00bb", "Ei\u00b7ne", "fes\u00b7te", "Burg", "ist", "Gott", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$(", "ART", "ADJA", "NN", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Er bl\u00e4st so laut und helle, es schallt den Wald entlang,", "tokens": ["Er", "bl\u00e4st", "so", "laut", "und", "hel\u00b7le", ",", "es", "schallt", "den", "Wald", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJA", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es klingt so scharf und schnelle wie Schwertschlag jeder Klang. \u2013", "tokens": ["Es", "klingt", "so", "scharf", "und", "schnel\u00b7le", "wie", "Schwert\u00b7schlag", "je\u00b7der", "Klang", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "KOKOM", "NN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "La\u00df ab, du guter Reiter, zieh' r\u00fcckw\u00e4rts rasch und stumm,", "tokens": ["La\u00df", "ab", ",", "du", "gu\u00b7ter", "Rei\u00b7ter", ",", "zieh'", "r\u00fcck\u00b7w\u00e4rts", "rasch", "und", "stumm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "O reit' und blas' nicht weiter, denn Feinde sind ringsum.", "tokens": ["O", "reit'", "und", "blas'", "nicht", "wei\u00b7ter", ",", "denn", "Fein\u00b7de", "sind", "ring\u00b7sum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$,", "KON", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Deine Botschaft ist verraten dem Grafen Isolan:", "tokens": ["Dei\u00b7ne", "Bot\u00b7schaft", "ist", "ver\u00b7ra\u00b7ten", "dem", "Gra\u00b7fen", "I\u00b7so\u00b7lan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Es l\u00fcstet die Kroaten nach K\u00f6nig Gustavs Plan.", "tokens": ["Es", "l\u00fcs\u00b7tet", "die", "Kroa\u00b7ten", "nach", "K\u00f6\u00b7nig", "Gus\u00b7tavs", "Plan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "NE", "NE", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Du lockst mit deinen Kl\u00e4ngen die Feinde selbst herbei: \u2013", "tokens": ["Du", "lockst", "mit", "dei\u00b7nen", "Kl\u00e4n\u00b7gen", "die", "Fein\u00b7de", "selbst", "her\u00b7bei", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sechs aus dem Walde sprengen und von jeder Flanke drei.", "tokens": ["Sechs", "aus", "dem", "Wal\u00b7de", "spren\u00b7gen", "und", "von", "je\u00b7der", "Flan\u00b7ke", "drei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ART", "NN", "VVFIN", "KON", "APPR", "PIAT", "NN", "CARD", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}}, "stanza.9": {"line.1": {"text": "Von links und rechts sie traben heran mit Hurra jetzt,", "tokens": ["Von", "links", "und", "rechts", "sie", "tra\u00b7ben", "he\u00b7ran", "mit", "Hur\u00b7ra", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "PPER", "VVFIN", "PTKVZ", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und vorn der breite Graben: \u2013 kein Ro\u00df dar\u00fcber setzt.", "tokens": ["Und", "vorn", "der", "brei\u00b7te", "Gra\u00b7ben", ":", "\u2013", "kein", "Ro\u00df", "da\u00b7r\u00fc\u00b7ber", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$.", "$(", "PIAT", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Er richt't sich auf im B\u00fcgel, er blickt um sich mit Zorn,", "tokens": ["Er", "richt't", "sich", "auf", "im", "B\u00fc\u00b7gel", ",", "er", "blickt", "um", "sich", "mit", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPRART", "NN", "$,", "PPER", "VVFIN", "APPR", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er gibt dem Ro\u00df die Z\u00fcgel, er gibt dem Ro\u00df den Sporn.", "tokens": ["Er", "gibt", "dem", "Ro\u00df", "die", "Z\u00fc\u00b7gel", ",", "er", "gibt", "dem", "Ro\u00df", "den", "Sporn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbgreif' aus, mein Rapp, mit Springen, jetzt gilt es scharfen Trott,", "tokens": ["\u00bb", "greif'", "aus", ",", "mein", "Rapp", ",", "mit", "Sprin\u00b7gen", ",", "jetzt", "gilt", "es", "schar\u00b7fen", "Trott", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn Gott will, kann's gelingen \u2013 eine feste Burg ist Gott!\u00ab", "tokens": ["Wenn", "Gott", "will", ",", "kann's", "ge\u00b7lin\u00b7gen", "\u2013", "ei\u00b7ne", "fes\u00b7te", "Burg", "ist", "Gott", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "VMFIN", "$,", "VMFIN", "VVINF", "$(", "ART", "ADJA", "NN", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.12": {"line.1": {"text": "Und mit verh\u00e4ngtem Z\u00fcgel zum Graben geht's im Flug.", "tokens": ["Und", "mit", "ver\u00b7h\u00e4ng\u00b7tem", "Z\u00fc\u00b7gel", "zum", "Gra\u00b7ben", "geht's", "im", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPRART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbglaubst du, dein Rapp' hat Fl\u00fcgel?\u00ab lacht der Kroaten Zug.", "tokens": ["\u00bb", "glaubst", "du", ",", "dein", "Rapp'", "hat", "Fl\u00fc\u00b7gel", "?", "\u00ab", "lacht", "der", "Kroa\u00b7ten", "Zug", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$.", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Dicht hinterher sie brausen mit Schie\u00dfen und mit Schrei'n: \u2013", "tokens": ["Dicht", "hin\u00b7ter\u00b7her", "sie", "brau\u00b7sen", "mit", "Schie\u00b7\u00dfen", "und", "mit", "Schrei'n", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADV", "PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hei! wie die Kugeln sausen und die Rosse hinterdrein:", "tokens": ["Hei", "!", "wie", "die", "Ku\u00b7geln", "sau\u00b7sen", "und", "die", "Ros\u00b7se", "hin\u00b7ter\u00b7drein", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ART", "NN", "VVINF", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}}, "stanza.14": {"line.1": {"text": "Nun bis zur Sattelkappe im Sprung den Kopf er biegt,", "tokens": ["Nun", "bis", "zur", "Sat\u00b7tel\u00b7kap\u00b7pe", "im", "Sprung", "den", "Kopf", "er", "biegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und hui! der treue Rappe hoch \u00fcber den Graben fliegt.", "tokens": ["Und", "hui", "!", "der", "treu\u00b7e", "Rap\u00b7pe", "hoch", "\u00fc\u00b7ber", "den", "Gra\u00b7ben", "fliegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Die Kroaten halten am Rande, sie fluchen ob der Schmach,", "tokens": ["Die", "Kroa\u00b7ten", "hal\u00b7ten", "am", "Ran\u00b7de", ",", "sie", "flu\u00b7chen", "ob", "der", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "KOUS", "ART", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "'s ist eine kecke Bande: \u2013 doch keiner tut's ihm nach.", "tokens": ["'s", "ist", "ei\u00b7ne", "ke\u00b7cke", "Ban\u00b7de", ":", "\u2013", "doch", "kei\u00b7ner", "tut's", "ihm", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$(", "ADV", "PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.16": {"line.1": {"text": "Doch er zieht dr\u00fcben weiter, im Schritt, dem Feind zum Spott,", "tokens": ["Doch", "er", "zieht", "dr\u00fc\u00b7ben", "wei\u00b7ter", ",", "im", "Schritt", ",", "dem", "Feind", "zum", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKVZ", "$,", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und fromm bl\u00e4st er und heiter: \u00bbEine feste Burg ist Gott!\u00ab", "tokens": ["Und", "fromm", "bl\u00e4st", "er", "und", "hei\u00b7ter", ":", "\u00bb", "Ei\u00b7ne", "fes\u00b7te", "Burg", "ist", "Gott", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "wo