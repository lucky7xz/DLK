{"dta.poem.2924": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "123.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df es nicht, ob so sich allgemein verh\u00e4lt", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "ob", "so", "sich", "all\u00b7ge\u00b7mein", "ver\u00b7h\u00e4lt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADV", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das menschliche Gem\u00fct, wie meines ist bestellt,", "tokens": ["Das", "menschli\u00b7che", "Ge\u00b7m\u00fct", ",", "wie", "mei\u00b7nes", "ist", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Das in der Freude schon das Ende f\u00fchlt der Lust,", "tokens": ["Das", "in", "der", "Freu\u00b7de", "schon", "das", "En\u00b7de", "f\u00fchlt", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in der Trauer sich des Trostes ist bewust;", "tokens": ["Und", "in", "der", "Trau\u00b7er", "sich", "des", "Tros\u00b7tes", "ist", "be\u00b7wust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Soda\u00df im Gegensatz von ungewisser Dauer", "tokens": ["So\u00b7da\u00df", "im", "Ge\u00b7gen\u00b7satz", "von", "un\u00b7ge\u00b7wis\u00b7ser", "Dau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschwimmen alswie Licht und Schatten Lust und Trauer.", "tokens": ["Ver\u00b7schwim\u00b7men", "als\u00b7wie", "Licht", "und", "Schat\u00b7ten", "Lust", "und", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}