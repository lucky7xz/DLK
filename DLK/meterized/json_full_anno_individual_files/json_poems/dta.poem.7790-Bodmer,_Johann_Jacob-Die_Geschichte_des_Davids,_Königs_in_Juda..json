{"dta.poem.7790": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil aber nun die Zeit und das getobte Brausen,", "tokens": ["Weil", "a\u00b7ber", "nun", "die", "Zeit", "und", "das", "ge\u00b7tob\u00b7te", "Brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Elementen Krieg, des Windes tolles Sausen", "tokens": ["Der", "E\u00b7le\u00b7men\u00b7ten", "Krieg", ",", "des", "Win\u00b7des", "tol\u00b7les", "Sau\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sich wiederum gelegt, und alles war in Ruh,", "tokens": ["Sich", "wie\u00b7de\u00b7rum", "ge\u00b7legt", ",", "und", "al\u00b7les", "war", "in", "Ruh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$,", "KON", "PIS", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So da\u00df die Morgenr\u00f6th anbrechen wollte nu;", "tokens": ["So", "da\u00df", "die", "Mor\u00b7gen\u00b7r\u00f6th", "an\u00b7bre\u00b7chen", "woll\u00b7te", "nu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVINF", "VMFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Merckt David, da\u00df die drey anheben einzuschlaffen,", "tokens": ["Merckt", "Da\u00b7vid", ",", "da\u00df", "die", "drey", "an\u00b7he\u00b7ben", "ein\u00b7zu\u00b7schlaf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "KOUS", "ART", "CARD", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Drum steht er leise auf, und geht nach seinen Schafen.", "tokens": ["Drum", "steht", "er", "lei\u00b7se", "auf", ",", "und", "geht", "nach", "sei\u00b7nen", "Scha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die D\u00e4mmerung vermag zu stillen nicht die Lust,", "tokens": ["Die", "D\u00e4m\u00b7me\u00b7rung", "ver\u00b7mag", "zu", "stil\u00b7len", "nicht", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df er die sehen k\u00f6nn, von der ihm nun bewust,", "tokens": ["Da\u00df", "er", "die", "se\u00b7hen", "k\u00f6nn", ",", "von", "der", "ihm", "nun", "be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVINF", "VMFIN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df sie ihm w\u00e4re hold, er must noch l\u00e4nger harren,", "tokens": ["Da\u00df", "sie", "ihm", "w\u00e4\u00b7re", "hold", ",", "er", "must", "noch", "l\u00e4n\u00b7ger", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "ADJD", "$,", "PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und seine keusche Freud bis auf den Tag versparen.", "tokens": ["Und", "sei\u00b7ne", "keu\u00b7sche", "Freud", "bis", "auf", "den", "Tag", "ver\u00b7spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Weile ward ihm lang, die Klippen sah er an,", "tokens": ["Die", "Wei\u00b7le", "ward", "ihm", "lang", ",", "die", "Klip\u00b7pen", "sah", "er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ob deren ihre Spitz ihm nicht was melden kan.", "tokens": ["Ob", "de\u00b7ren", "ih\u00b7re", "Spitz", "ihm", "nicht", "was", "mel\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "NN", "PPER", "PTKNEG", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Indeme wird der Stein, den er ansieht, err\u00f6thet,", "tokens": ["In\u00b7de\u00b7me", "wird", "der", "Stein", ",", "den", "er", "an\u00b7sieht", ",", "er\u00b7r\u00f6t\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und drauf die Finsterni\u00df zu weichen angen\u00f6thet,", "tokens": ["Und", "drauf", "die", "Fins\u00b7ter\u00b7ni\u00df", "zu", "wei\u00b7chen", "an\u00b7ge\u00b7n\u00f6\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Allm\u00e4hlich wird es hell, der Purpur breit sich aus,", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "wird", "es", "hell", ",", "der", "Pur\u00b7pur", "breit", "sich", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "$,", "ART", "NN", "ADJD", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und scheinet in die H\u00f6l, in dieses Klippenhaus.", "tokens": ["Und", "schei\u00b7net", "in", "die", "H\u00f6l", ",", "in", "die\u00b7ses", "Klip\u00b7pen\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das Feld wird hierauf wach. Zuerst t\u00f6nt aus den B\u00fcschen", "tokens": ["Das", "Feld", "wird", "hier\u00b7auf", "wach", ".", "Zu\u00b7erst", "t\u00f6nt", "aus", "den", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PAV", "PTKVZ", "$.", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Der Nachtigallen Lied, ihr angenehmes Zischen", "tokens": ["Der", "Nach\u00b7ti\u00b7gal\u00b7len", "Lied", ",", "ihr", "an\u00b7ge\u00b7neh\u00b7mes", "Zi\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sagt gute Nacht der Nacht, der stillen Einsamkeit,", "tokens": ["Sagt", "gu\u00b7te", "Nacht", "der", "Nacht", ",", "der", "stil\u00b7len", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und rufft der immer zu, da\u00df sie doch nicht zu weit", "tokens": ["Und", "rufft", "der", "im\u00b7mer", "zu", ",", "da\u00df", "sie", "doch", "nicht", "zu", "weit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "PTKA", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Von ihnen sich begeb, dann diese sie belieben.", "tokens": ["Von", "ih\u00b7nen", "sich", "be\u00b7geb", ",", "dann", "die\u00b7se", "sie", "be\u00b7lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVFIN", "$,", "ADV", "PDAT", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Hier auf der V\u00f6gel Heer vermehret sich zu \u00fcben,", "tokens": ["Hier", "auf", "der", "V\u00f6\u00b7gel", "Heer", "ver\u00b7meh\u00b7ret", "sich", "zu", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und zwitschert in die Wett, die Nachtigall wird still,", "tokens": ["Und", "zwit\u00b7schert", "in", "die", "Wett", ",", "die", "Nach\u00b7ti\u00b7gall", "wird", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil sie nicht ihre Kunst bey andern mengen will.", "tokens": ["Weil", "sie", "nicht", "ih\u00b7re", "Kunst", "bey", "an\u00b7dern", "men\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "APPR", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Man h\u00f6ret hin und her die muntern Hunde bellen,", "tokens": ["Man", "h\u00f6\u00b7ret", "hin", "und", "her", "die", "mun\u00b7tern", "Hun\u00b7de", "bel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "KON", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Schall geht in die Berg, die mehr und mehr sich hellen", "tokens": ["Der", "Schall", "geht", "in", "die", "Berg", ",", "die", "mehr", "und", "mehr", "sich", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "KON", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Weil nach der Morgenr\u00f6th der Sonnen helles Kleid", "tokens": ["Weil", "nach", "der", "Mor\u00b7gen\u00b7r\u00f6th", "der", "Son\u00b7nen", "hel\u00b7les", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Auf ihren Gipfeln wird sehr herrlich ausgebreit.", "tokens": ["Auf", "ih\u00b7ren", "Gip\u00b7feln", "wird", "sehr", "herr\u00b7lich", "aus\u00b7ge\u00b7breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der sch\u00f6ne Morgenthau war auch bereits gefallen,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Mor\u00b7gent\u00b7hau", "war", "auch", "be\u00b7reits", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die angenehme Zunfft der Hirten that erschallen,", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "Zunfft", "der", "Hir\u00b7ten", "that", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bald hie, bald da herum, ein jeder triebe aus.", "tokens": ["Bald", "hie", ",", "bald", "da", "he\u00b7rum", ",", "ein", "je\u00b7der", "trie\u00b7be", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "PTKVZ", "$,", "ART", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der Peitschen ihr Geklatsch erschallt von Hau\u00df zu Hau\u00df,", "tokens": ["Der", "Peit\u00b7schen", "ihr", "Ge\u00b7klatsch", "er\u00b7schallt", "von", "Hau\u00df", "zu", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und wehrt dem dum\u0303en Vieh wann das wollt Abweg nehmen.", "tokens": ["Und", "wehrt", "dem", "dum\u0303en", "Vieh", "wann", "das", "wollt", "Ab\u00b7weg", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PWAV", "PDS", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Des Davids seine Heerd wollt sich auch schon bequemen,", "tokens": ["Des", "Da\u00b7vids", "sei\u00b7ne", "Heerd", "wollt", "sich", "auch", "schon", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPOSAT", "NN", "VMFIN", "PRF", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sein Hund war zwar bereit; doch aber nicht der Hirt,", "tokens": ["Sein", "Hund", "war", "zwar", "be\u00b7reit", ";", "doch", "a\u00b7ber", "nicht", "der", "Hirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$.", "ADV", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der von der, so die H\u00f6l bedeckt, gehalten wird.", "tokens": ["Der", "von", "der", ",", "so", "die", "H\u00f6l", "be\u00b7deckt", ",", "ge\u00b7hal\u00b7ten", "wird", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "$,", "ADV", "ART", "NN", "VVPP", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Er wartet flei\u00dfig auf, bis seine Sonn erscheinet,", "tokens": ["Er", "war\u00b7tet", "flei\u00b7\u00dfig", "auf", ",", "bis", "sei\u00b7ne", "Sonn", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "F\u00fcr welche er nicht gnung geschm\u00fcckt zu seyn vermeinet,", "tokens": ["F\u00fcr", "wel\u00b7che", "er", "nicht", "gnung", "ge\u00b7schm\u00fcckt", "zu", "seyn", "ver\u00b7mei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PTKNEG", "ADV", "VVPP", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ziert sich darauf, so gut sein Hirtenkleid es g\u00f6nnt,", "tokens": ["Ziert", "sich", "da\u00b7rauf", ",", "so", "gut", "sein", "Hir\u00b7ten\u00b7kleid", "es", "g\u00f6nnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "$,", "ADV", "ADJD", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Sein schon gesalbtes Haupt mit Blumen er bekr\u00f6nt.", "tokens": ["Sein", "schon", "ge\u00b7salb\u00b7tes", "Haupt", "mit", "Blu\u00b7men", "er", "be\u00b7kr\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}