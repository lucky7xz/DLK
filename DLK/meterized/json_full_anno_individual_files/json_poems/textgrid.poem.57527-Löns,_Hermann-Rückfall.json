{"textgrid.poem.57527": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckfall", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem bl\u00fchenden Holunderstrauch", "tokens": ["In", "dem", "bl\u00fc\u00b7hen\u00b7den", "Ho\u00b7lun\u00b7der\u00b7strauch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Singt der Sp\u00f6tter laut sein buntes Lied,", "tokens": ["Singt", "der", "Sp\u00f6t\u00b7ter", "laut", "sein", "bun\u00b7tes", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ein berauschend-schlummers\u00fc\u00dfer Hauch", "tokens": ["Ein", "be\u00b7rau\u00b7schen\u00b7dschlum\u00b7mer\u00b7s\u00fc\u00b7\u00dfer", "Hauch"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Leis um meine m\u00fcde Seele zieht.", "tokens": ["Leis", "um", "mei\u00b7ne", "m\u00fc\u00b7de", "See\u00b7le", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und die wei\u00dfen Dolden b\u00fccken sich,", "tokens": ["Und", "die", "wei\u00b7\u00dfen", "Dol\u00b7den", "b\u00fc\u00b7cken", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gleich als winkten sie mir liebend zu:", "tokens": ["Gleich", "als", "wink\u00b7ten", "sie", "mir", "lie\u00b7bend", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hier in unserem Schatten strecke dich,", "tokens": ["Hier", "in", "un\u00b7se\u00b7rem", "Schat\u00b7ten", "stre\u00b7cke", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bleicher Tr\u00e4umer, hier ist k\u00fchle Ruh'!", "tokens": ["Blei\u00b7cher", "Tr\u00e4u\u00b7mer", ",", "hier", "ist", "k\u00fch\u00b7le", "Ruh'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "La\u00df dein Rauschen, dunkler Holderbaum,", "tokens": ["La\u00df", "dein", "Rau\u00b7schen", ",", "dunk\u00b7ler", "Hol\u00b7der\u00b7baum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Grauer Vogel, ende bald dein Lied,", "tokens": ["Grau\u00b7er", "Vo\u00b7gel", ",", "en\u00b7de", "bald", "dein", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Da\u00df der hoffnungslose Hoffnungstraum", "tokens": ["Da\u00df", "der", "hoff\u00b7nungs\u00b7lo\u00b7se", "Hoff\u00b7nungs\u00b7traum"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Nicht noch einmal in mein Herz einzieht!", "tokens": ["Nicht", "noch", "ein\u00b7mal", "in", "mein", "Herz", "ein\u00b7zieht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Denn ich habe mich daran gew\u00f6hnt,", "tokens": ["Denn", "ich", "ha\u00b7be", "mich", "da\u00b7ran", "ge\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df mir Ruhe nie bescheret sei,", "tokens": ["Da\u00df", "mir", "Ru\u00b7he", "nie", "be\u00b7sche\u00b7ret", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und mir selbst die Hoffnung fortgeh\u00f6hnt", "tokens": ["Und", "mir", "selbst", "die", "Hoff\u00b7nung", "fort\u00b7ge\u00b7h\u00f6hnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und die alte Friedenstr\u00e4umerei.", "tokens": ["Und", "die", "al\u00b7te", "Frie\u00b7dens\u00b7tr\u00e4u\u00b7me\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "In dem bl\u00fchenden Holunderstrauch", "tokens": ["In", "dem", "bl\u00fc\u00b7hen\u00b7den", "Ho\u00b7lun\u00b7der\u00b7strauch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"