: "Des Tages beste Stunden mu\u00df.", "tokens": ["Des", "Ta\u00b7ges", "bes\u00b7te", "Stun\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Ach h\u00e4tt' ich nur wie andre Sachsen", "tokens": ["Ach", "h\u00e4tt'", "ich", "nur", "wie", "and\u00b7re", "Sach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Recht am Docieren eine Lust!", "tokens": ["Recht", "am", "Do\u00b7cie\u00b7ren", "ei\u00b7ne", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wie der Schnabel mir gewachsen,", "tokens": ["Doch", "wie", "der", "Schna\u00b7bel", "mir", "ge\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kann ich ihn so nicht brauchen just.", "tokens": ["Kann", "ich", "ihn", "so", "nicht", "brau\u00b7chen", "just", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Und immer hab' ich diese Klage", "tokens": ["Und", "im\u00b7mer", "hab'", "ich", "die\u00b7se", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu hauchen in den Morgenwind,", "tokens": ["Zu", "hau\u00b7chen", "in", "den", "Mor\u00b7gen\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wozu die l\u00e4st'gen Spatzentage", "tokens": ["Wo\u00b7zu", "die", "l\u00e4st'\u00b7gen", "Spat\u00b7zen\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Nachtigallenleben sind.", "tokens": ["Im", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7le\u00b7ben", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}