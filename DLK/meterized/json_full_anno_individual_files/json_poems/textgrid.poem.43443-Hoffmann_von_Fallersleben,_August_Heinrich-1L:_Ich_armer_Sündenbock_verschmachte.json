{"textgrid.poem.43443": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich armer S\u00fcndenbock verschmachte", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich armer S\u00fcndenbock verschmachte", "tokens": ["Ich", "ar\u00b7mer", "S\u00fcn\u00b7den\u00b7bock", "ver\u00b7schmach\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser hei\u00dfen H\u00f6llenglut,", "tokens": ["In", "die\u00b7ser", "hei\u00b7\u00dfen", "H\u00f6l\u00b7len\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und doch, wenn ich es recht betrachte,", "tokens": ["Und", "doch", ",", "wenn", "ich", "es", "recht", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So geht's mir immer noch zu gut.", "tokens": ["So", "geht's", "mir", "im\u00b7mer", "noch", "zu", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich habe mit Rescripten weiland", "tokens": ["Ich", "ha\u00b7be", "mit", "Re\u00b7scrip\u00b7ten", "wei\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geplagt die ganze Monarchie:", "tokens": ["Ge\u00b7plagt", "die", "gan\u00b7ze", "Mon\u00b7ar\u00b7chie", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich war gewi\u00df f\u00fcr sie kein Heiland,", "tokens": ["Ich", "war", "ge\u00b7wi\u00df", "f\u00fcr", "sie", "kein", "Hei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch plagten sie mich nie.", "tokens": ["Und", "den\u00b7noch", "plag\u00b7ten", "sie", "mich", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich habe mit Berichterstatten", "tokens": ["Ich", "ha\u00b7be", "mit", "Be\u00b7rich\u00b7ter\u00b7stat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gepeinigt manchen braven Mann,", "tokens": ["Ge\u00b7pei\u00b7nigt", "man\u00b7chen", "bra\u00b7ven", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was sie dann berichtet hatten,", "tokens": ["Und", "was", "sie", "dann", "be\u00b7rich\u00b7tet", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das sah ich niemals weiter an.", "tokens": ["Das", "sah", "ich", "nie\u00b7mals", "wei\u00b7ter", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich habe durch Conduitenlisten", "tokens": ["Ich", "ha\u00b7be", "durch", "Con\u00b7dui\u00b7ten\u00b7lis\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und durch geheime Polizei", "tokens": ["Und", "durch", "ge\u00b7hei\u00b7me", "Po\u00b7li\u00b7zei"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verleitet viele gute Christen", "tokens": ["Ver\u00b7lei\u00b7tet", "vie\u00b7le", "gu\u00b7te", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Lug und Trug und Heuchelei.", "tokens": ["Zu", "Lug", "und", "Trug", "und", "Heu\u00b7che\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich habe mit Censurerlassen", "tokens": ["Ich", "ha\u00b7be", "mit", "Cen\u00b7su\u00b7rer\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gehemmt den Fortschritt unsrer Zeit:", "tokens": ["Ge\u00b7hemmt", "den", "Fort\u00b7schritt", "uns\u00b7rer", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich zwang die Welt, mich recht zu hassen,", "tokens": ["Ich", "zwang", "die", "Welt", ",", "mich", "recht", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch bracht' ich's nicht so weit.", "tokens": ["Und", "den\u00b7noch", "bracht'", "ich's", "nicht", "so", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich habe jeden Stand beleidigt,", "tokens": ["Ich", "ha\u00b7be", "je\u00b7den", "Stand", "be\u00b7lei\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und als der Tod mich abgesetzt,", "tokens": ["Und", "als", "der", "Tod", "mich", "ab\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da haben sie mich noch vertheidigt,", "tokens": ["Da", "ha\u00b7ben", "sie", "mich", "noch", "ver\u00b7thei\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gelobt und benedeit zuletzt.", "tokens": ["Ge\u00b7lobt", "und", "be\u00b7ne\u00b7deit", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich habe mit des Fortschritts Schlangen", "tokens": ["Ich", "ha\u00b7be", "mit", "des", "Fort\u00b7schritts", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gek\u00e4mpfet wie Laokoon:", "tokens": ["Ge\u00b7k\u00e4mp\u00b7fet", "wie", "Lao\u00b7ko\u00b7on", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Zeit ist ruhig fortgegangen,", "tokens": ["Die", "Zeit", "ist", "ru\u00b7hig", "fort\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herr wie ich hat nichts davon.", "tokens": ["Mein", "Herr", "wie", "ich", "hat", "nichts", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "PPER", "VAFIN", "PIS", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich hab' an meinen Herren immer,", "tokens": ["Ich", "hab'", "an", "mei\u00b7nen", "Her\u00b7ren", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "An unsern Herrngott nie gedacht:", "tokens": ["An", "un\u00b7sern", "Herrn\u00b7gott", "nie", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der liebe Herrgott h\u00e4tt' auch nimmer", "tokens": ["Der", "lie\u00b7be", "Herr\u00b7gott", "h\u00e4tt'", "auch", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Herrn Minister mich gemacht.", "tokens": ["Zum", "Herrn", "Mi\u00b7nis\u00b7ter", "mich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun schmacht' ich in der H\u00f6lle Schl\u00fcnden", "tokens": ["Nun", "schmacht'", "ich", "in", "der", "H\u00f6l\u00b7le", "Schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geschm\u00fcckt mit Ordensband und Stern;", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "Or\u00b7dens\u00b7band", "und", "Stern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gern m\u00f6cht' ich b\u00fc\u00dfen meine S\u00fcnden,", "tokens": ["Gern", "m\u00f6cht'", "ich", "b\u00fc\u00b7\u00dfen", "mei\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch b\u00fc\u00df' ich erst f\u00fcr meinen Herrn.", "tokens": ["Doch", "b\u00fc\u00df'", "ich", "erst", "f\u00fcr", "mei\u00b7nen", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich war auf Erden nie mein eigen,", "tokens": ["Ich", "war", "auf", "Er\u00b7den", "nie", "mein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So schlage doch der Teufel drein!", "tokens": ["So", "schla\u00b7ge", "doch", "der", "Teu\u00b7fel", "drein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich kann mich nicht mehr anders zeigen,", "tokens": ["Ich", "kann", "mich", "nicht", "mehr", "an\u00b7ders", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df immerfort Minister sein.", "tokens": ["Mu\u00df", "im\u00b7mer\u00b7fort", "Mi\u00b7nis\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich armer S\u00fcndenbock verschmachte", "tokens": ["Ich", "ar\u00b7mer", "S\u00fcn\u00b7den\u00b7bock", "ver\u00b7schmach\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser hei\u00dfen H\u00f6llenglut,", "tokens": ["In", "die\u00b7ser", "hei\u00b7\u00dfen", "H\u00f6l\u00b7len\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und doch, wenn ich es recht betrachte,", "tokens": ["Und", "doch", ",", "wenn", "ich", "es", "recht", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "P