{"dta.poem.19167": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geschichten hab ich viel gelesen und geh\u00f6rt,", "tokens": ["Ge\u00b7schich\u00b7ten", "hab", "ich", "viel", "ge\u00b7le\u00b7sen", "und", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die vielfach angeregt mich haben und verst\u00f6rt.", "tokens": ["Die", "viel\u00b7fach", "an\u00b7ge\u00b7regt", "mich", "ha\u00b7ben", "und", "ver\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "PPER", "VAFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Geschichten m\u00f6cht' ich euch anregende erz\u00e4hlen,", "tokens": ["Ge\u00b7schich\u00b7ten", "m\u00f6cht'", "ich", "euch", "an\u00b7re\u00b7gen\u00b7de", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch wollte nie mein Geist sich dieser Form verm\u00e4hlen.", "tokens": ["Doch", "woll\u00b7te", "nie", "mein", "Geist", "sich", "die\u00b7ser", "Form", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPOSAT", "NN", "PRF", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und mich begeistern k\u00f6nnt' Eine Geschichte nur,", "tokens": ["Und", "mich", "be\u00b7geis\u00b7tern", "k\u00f6nnt'", "Ei\u00b7ne", "Ge\u00b7schich\u00b7te", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Beherrscht' ich deinen Stoff, Geschichte der Natur!", "tokens": ["Be\u00b7herrscht'", "ich", "dei\u00b7nen", "Stoff", ",", "Ge\u00b7schich\u00b7te", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Inzwischen hat genug Beruhigung gesogen", "tokens": ["I\u00b7nzwi\u00b7schen", "hat", "ge\u00b7nug", "Be\u00b7ru\u00b7hi\u00b7gung", "ge\u00b7so\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Befriedigung mein Geist aus deinen Regenbogen.", "tokens": ["Be\u00b7frie\u00b7di\u00b7gung", "mein", "Geist", "aus", "dei\u00b7nen", "Re\u00b7gen\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Regenbogen wirkt in sanfter Farbenpracht", "tokens": ["Der", "Re\u00b7gen\u00b7bo\u00b7gen", "wirkt", "in", "sanf\u00b7ter", "Far\u00b7ben\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Friedensfahne beigelegter Wolkenschlacht,", "tokens": ["Die", "Frie\u00b7dens\u00b7fah\u00b7ne", "bei\u00b7ge\u00b7leg\u00b7ter", "Wol\u00b7ken\u00b7schlacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie nach der Leidenschaft ged\u00e4mpfter Wetterschw\u00fcle", "tokens": ["Wie", "nach", "der", "Lei\u00b7den\u00b7schaft", "ge\u00b7d\u00e4mpf\u00b7ter", "Wet\u00b7ter\u00b7schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mein hei\u00dfes Aug' erfrischt der Weltbetrachtung K\u00fchle.", "tokens": ["Mein", "hei\u00b7\u00dfes", "Aug'", "er\u00b7frischt", "der", "Welt\u00b7be\u00b7trach\u00b7tung", "K\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}