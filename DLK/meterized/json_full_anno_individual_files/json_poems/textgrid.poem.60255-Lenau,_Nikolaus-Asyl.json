{"textgrid.poem.60255": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Asyl", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hohe Klippen, ringsgeschlossen,", "tokens": ["Ho\u00b7he", "Klip\u00b7pen", ",", "rings\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenig k\u00fcmmerliche F\u00f6hren,", "tokens": ["We\u00b7nig", "k\u00fcm\u00b7mer\u00b7li\u00b7che", "F\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00fcbe fl\u00fcsternde Genossen,", "tokens": ["Tr\u00fc\u00b7be", "fl\u00fcs\u00b7tern\u00b7de", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die hier keinen Vogel h\u00f6ren;", "tokens": ["Die", "hier", "kei\u00b7nen", "Vo\u00b7gel", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nichts vom freudigen Gesange", "tokens": ["Nichts", "vom", "freu\u00b7di\u00b7gen", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den sch\u00f6nen Fr\u00fchlingszeiten;", "tokens": ["In", "den", "sch\u00f6\u00b7nen", "Fr\u00fch\u00b7lings\u00b7zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "me