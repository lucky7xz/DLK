{"dta.poem.12980": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.57", "nl:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr helden! pralet nicht mit euren blut'gen siegen:", "tokens": ["Ihr", "hel\u00b7den", "!", "pra\u00b7let", "nicht", "mit", "eu\u00b7ren", "blut'\u00b7gen", "sie\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist ein schlechter ruhm, den man auf leichen baut.", "tokens": ["Es", "ist", "ein", "schlech\u00b7ter", "ruhm", ",", "den", "man", "auf", "lei\u00b7chen", "baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der fast die halbe welt vor seinen f\u00fc\u00dfen schaut,", "tokens": ["Der", "fast", "die", "hal\u00b7be", "welt", "vor", "sei\u00b7nen", "f\u00fc\u00b7\u00dfen", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mu\u00df offt vor einer hur auf seinen knien liegen.", "tokens": ["Mu\u00df", "offt", "vor", "ei\u00b7ner", "hur", "auf", "sei\u00b7nen", "kni\u00b7en", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und rei\u00dft die wohllust nicht das schwerd aus seiner hand;", "tokens": ["Und", "rei\u00dft", "die", "wohl\u00b7lust", "nicht", "das", "schwerd", "aus", "sei\u00b7ner", "hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So mu\u00df er doch so wol, als feige memmen, sterben.", "tokens": ["So", "mu\u00df", "er", "doch", "so", "wol", ",", "als", "fei\u00b7ge", "mem\u00b7men", ",", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADV", "$,", "KOUS", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die ehre hilfft ihn nichts, so seine glieder erben,", "tokens": ["Die", "eh\u00b7re", "hilfft", "ihn", "nichts", ",", "so", "sei\u00b7ne", "glie\u00b7der", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPER", "PIS", "$,", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die f\u00e4ulnis setzt uns all\u2019 in einen gleichen stand.", "tokens": ["Die", "f\u00e4ul\u00b7nis", "setzt", "uns", "all'", "in", "ei\u00b7nen", "glei\u00b7chen", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}