{"textgrid.poem.67135": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An eine F\u00fcrstin von Dessau", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus stillem Hause senden die G\u00f6tter oft", "tokens": ["Aus", "stil\u00b7lem", "Hau\u00b7se", "sen\u00b7den", "die", "G\u00f6t\u00b7ter", "oft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf kurze Zeit zu Fremden die Lieblinge,", "tokens": ["Auf", "kur\u00b7ze", "Zeit", "zu", "Frem\u00b7den", "die", "Lieb\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Damit, erinnert, sich am edlen", "tokens": ["Da\u00b7mit", ",", "e\u00b7rin\u00b7nert", ",", "sich", "am", "ed\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "VVFIN", "$,", "PRF", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bilde der Sterblichen Herz erfreue.", "tokens": ["Bil\u00b7de", "der", "Sterb\u00b7li\u00b7chen", "Herz", "er\u00b7freu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "So kommst du aus Luisiums Hainen auch,", "tokens": ["So", "kommst", "du", "aus", "Lui\u00b7si\u00b7ums", "Hai\u00b7nen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "NE", "ADV