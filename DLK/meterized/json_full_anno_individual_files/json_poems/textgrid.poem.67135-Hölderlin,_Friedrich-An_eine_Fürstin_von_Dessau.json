measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Denn wo die Reinen wandeln, vernehmlicher", "tokens": ["Denn", "wo", "die", "Rei\u00b7nen", "wan\u00b7deln", ",", "ver\u00b7nehm\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist da der Geist, und offen und heiter bl\u00fchn", "tokens": ["Ist", "da", "der", "Geist", ",", "und", "of\u00b7fen", "und", "hei\u00b7ter", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "KON", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Lebens d\u00e4mmernde Gestalten", "tokens": ["Des", "Le\u00b7bens", "d\u00e4m\u00b7mern\u00b7de", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da, wo ein sicheres Licht erscheinet.", "tokens": ["Da", ",", "wo", "ein", "si\u00b7che\u00b7res", "Licht", "er\u00b7schei\u00b7net", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und wie auf dunkler Wolke der schweigende,", "tokens": ["Und", "wie", "auf", "dunk\u00b7ler", "Wol\u00b7ke", "der", "schwei\u00b7gen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der sch\u00f6ne Bogen bl\u00fchet, ein Zeichen ist", "tokens": ["Der", "sch\u00f6\u00b7ne", "Bo\u00b7gen", "bl\u00f