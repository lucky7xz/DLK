{"dta.poem.19364": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum ist Redlichkeit von Rede so benannt?", "tokens": ["Wa\u00b7rum", "ist", "Red\u00b7lich\u00b7keit", "von", "Re\u00b7de", "so", "be\u00b7nannt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil aus der Rede nur das Innre wird erkannt.", "tokens": ["Weil", "aus", "der", "Re\u00b7de", "nur", "das", "Inn\u00b7re", "wird", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Redlichkeit besteht darin, da\u00df einerlei", "tokens": ["Die", "Red\u00b7lich\u00b7keit", "be\u00b7steht", "da\u00b7rin", ",", "da\u00df", "ei\u00b7ner\u00b7lei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit seiner Aeu\u00dferung dein Innerliches sei.", "tokens": ["Mit", "sei\u00b7ner", "A\u00b7e\u00b7u\u00b7\u00dfe\u00b7rung", "dein", "In\u00b7ner\u00b7li\u00b7ches", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Die Redlichkeit besteht nicht in Wohlredenheit,", "tokens": ["Die", "Red\u00b7lich\u00b7keit", "be\u00b7steht", "nicht", "in", "Wohl\u00b7re\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Ueberredungskunst, Ausred' und Redestreit.", "tokens": ["In", "Ue\u00b7ber\u00b7re\u00b7dungs\u00b7kunst", ",", "Aus\u00b7red'", "und", "Re\u00b7des\u00b7treit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Redlichkeit besteht darin: Ein Wort, ein Mann;", "tokens": ["Die", "Red\u00b7lich\u00b7keit", "be\u00b7steht", "da\u00b7rin", ":", "Ein", "Wort", ",", "ein", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$.", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil man den Redlichen beim Worte halten kann.", "tokens": ["Weil", "man", "den", "Red\u00b7li\u00b7chen", "beim", "Wor\u00b7te", "hal\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Darin bestehet sie, da\u00df sich dein Herz beredet", "tokens": ["Da\u00b7rin", "be\u00b7ste\u00b7het", "sie", ",", "da\u00df", "sich", "dein", "Herz", "be\u00b7re\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "PRF", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit seiner Pflicht, und thut das was dein Mund geredet.", "tokens": ["Mit", "sei\u00b7ner", "Pflicht", ",", "und", "thut", "das", "was", "dein", "Mund", "ge\u00b7re\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PDS", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}