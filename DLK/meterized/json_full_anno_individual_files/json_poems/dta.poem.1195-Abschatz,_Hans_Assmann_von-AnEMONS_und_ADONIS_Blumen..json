{"dta.poem.1195": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "AnEMONS  \n und  \n  ADONIS  \n Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lege dir mein Haubt zu deinen F\u00fcssen:", "tokens": ["Ich", "le\u00b7ge", "dir", "mein", "Haubt", "zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bestraffe mich/ ich will gedultig b\u00fcssen/", "tokens": ["Be\u00b7straf\u00b7fe", "mich", "/", "ich", "will", "ge\u00b7dul\u00b7tig", "b\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPER", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wofern dein Recht f\u00fcr schuldig kan erkennen", "tokens": ["Wo\u00b7fern", "dein", "Recht", "f\u00fcr", "schul\u00b7dig", "kan", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJD", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den/ der da liebt/ was Liebens werth zu nennen.", "tokens": ["Den", "/", "der", "da", "liebt", "/", "was", "Lie\u00b7bens", "werth", "zu", "nen\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ART", "ADV", "VVFIN", "$(", "PWS", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach! straffet sich nicht selber mein Verbrechen?", "tokens": ["Ach", "!", "straf\u00b7fet", "sich", "nicht", "sel\u00b7ber", "mein", "Ver\u00b7bre\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PRF", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergehn vor Lieb/ und nichts von Liebe sprechen", "tokens": ["Ver\u00b7gehn", "vor", "Lieb", "/", "und", "nichts", "von", "Lie\u00b7be", "spre\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$(", "KON", "PIS", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist Pein genung/ wo keine Schuld zu kennen/", "tokens": ["Ist", "Pein", "ge\u00b7nung", "/", "wo", "kei\u00b7ne", "Schuld", "zu", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "$(", "PWAV", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als da\u00df man liebt/ was liebens werth zu nennen.", "tokens": ["Als", "da\u00df", "man", "liebt", "/", "was", "lie\u00b7bens", "werth", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "VVFIN", "$(", "PWS", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}