{"dta.poem.10610": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "An die Sternen/ da\u00df sie jhm den Weg zeigen wollen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Fackeln dieser Welt/ jhr ewig brennend Fewer/", "tokens": ["Ihr", "Fa\u00b7ckeln", "die\u00b7ser", "Welt", "/", "jhr", "e\u00b7wig", "bren\u00b7nend", "Fe\u00b7wer", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$(", "PPER", "ADJD", "VVPP", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Liechter in der Lufft/ jhr Himmels \u00e4ugelein/", "tokens": ["Ihr", "Liech\u00b7ter", "in", "der", "Lufft", "/", "jhr", "Him\u00b7mels", "\u00e4u\u00b7ge\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$(", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fchrt mich zu meinem Lieb: Kompt jhr mir nicht zu stewer/", "tokens": ["F\u00fchrt", "mich", "zu", "mei\u00b7nem", "Lieb", ":", "Kompt", "jhr", "mir", "nicht", "zu", "ste\u00b7wer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wirdt mein brennendt Hertz an statt der Sternen sein.", "tokens": ["So", "wirdt", "mein", "bren\u00b7nendt", "Hertz", "an", "statt", "der", "Ster\u00b7nen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJD", "NN", "APPR", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}