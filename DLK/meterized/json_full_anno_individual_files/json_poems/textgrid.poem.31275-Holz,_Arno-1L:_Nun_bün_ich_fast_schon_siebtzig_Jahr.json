{"textgrid.poem.31275": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun b\u00fcn ich fast schon siebtzig Jahr/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun b\u00fcn ich fast schon siebtzig Jahr/", "tokens": ["Nun", "b\u00fcn", "ich", "fast", "schon", "siebt\u00b7zig", "Jahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "CARD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das Leben hat mich wie zerschmissen;", "tokens": ["das", "Le\u00b7ben", "hat", "mich", "wie", "zer\u00b7schmis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "bald wei\u00df kein M\u00e4ntsch mehr/ wer ich war/", "tokens": ["bald", "wei\u00df", "kein", "M\u00e4ntsch", "mehr", "/", "wer", "ich", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV", "$(", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "kaum drohstet nachts mich noch mein Kissen.", "tokens": ["kaum", "drohs\u00b7tet", "nachts", "mich", "noch", "mein", "Kis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Welt ihr Seiffen-Ball zersprang/", "tokens": ["Der", "Welt", "ihr", "Seif\u00b7fen\u00b7Ball", "zer\u00b7sprang", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mein Lauten-Spihl ward Harffen-Klang!", "tokens": ["mein", "Lau\u00b7ten\u00b7Spihl", "ward", "Ha\u00b7rf\u00b7fen\u00b7Klang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ich b\u00fcn au\u00df Staub und mu\u00df vergehn/", "tokens": ["Ich", "b\u00fcn", "au\u00df", "Staub", "und", "mu\u00df", "ver\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "kein Bisam-B\u00fcxgen wird mir nizzen.", "tokens": ["kein", "Bi\u00b7sam\u00b7B\u00fcx\u00b7gen", "wird", "mir", "niz\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was soll mir ", "tokens": ["Was", "soll", "mir"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "Von Fern her seh ich ", "tokens": ["Von", "Fern", "her", "seh", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APZR", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Nur Eins wird noch von mir gepreisst:", "tokens": ["Nur", "Eins", "wird", "noch", "von", "mir", "ge\u00b7preisst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die grosse Kunst/ die St\u00e4rben heisst!", "tokens": ["Die", "gros\u00b7se", "Kunst", "/", "die", "St\u00e4r\u00b7ben", "heisst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Leib/ dih\u00df f\u00fcr so f\u00e4ste Hau\u00df/", "tokens": ["Mein", "Leib", "/", "dih\u00df", "f\u00fcr", "so", "f\u00e4s\u00b7te", "Hau\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PDS", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ligt spakk darnihder/ fast zerbrochen/", "tokens": ["ligt", "spakk", "dar\u00b7nih\u00b7der", "/", "fast", "zer\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PAV", "$(", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die Ahdern trukkneten ihm au\u00df/", "tokens": ["die", "Ah\u00b7dern", "truk\u00b7kne\u00b7ten", "ihm", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "ich h\u00e4nge kaum noch in den Knochen.", "tokens": ["ich", "h\u00e4n\u00b7ge", "kaum", "noch", "in", "den", "Kno\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich kr\u00fcmmt der Grie\u00df/ mich narbt die Gicht/", "tokens": ["Mich", "kr\u00fcmmt", "der", "Grie\u00df", "/", "mich", "narbt", "die", "Gicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "erb\u00e4rmlich b\u00fcn ich zugericht!", "tokens": ["er\u00b7b\u00e4rm\u00b7lich", "b\u00fcn", "ich", "zu\u00b7ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Alln\u00e4chtlich dappt er sich schon f\u00fcr/", "tokens": ["All\u00b7n\u00e4cht\u00b7lich", "dappt", "er", "sich", "schon", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der alte au\u00dfgefeimte Rakker.", "tokens": ["der", "al\u00b7te", "au\u00df\u00b7ge\u00b7feim\u00b7te", "Rak\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald knaxt die Diehle/ bald die Dh\u00fcr/", "tokens": ["Bald", "knaxt", "die", "Dieh\u00b7le", "/", "bald", "die", "Dh\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Wind heult hohl vom Stoppel-Akker.", "tokens": ["der", "Wind", "heult", "hohl", "vom", "Stop\u00b7pel\u00b7Ak\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Itzt bocht es an und will herein \u2013", "tokens": ["Itzt", "bocht", "es", "an", "und", "will", "her\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VMFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mir gr\u00e4hst ins innerste Gebein!", "tokens": ["mir", "gr\u00e4hst", "ins", "in\u00b7ners\u00b7te", "Ge\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Was w\u00fcrde strakks mit mir geschehn/", "tokens": ["Was", "w\u00fcr\u00b7de", "strakks", "mit", "mir", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wann meine Augen itzt verr\u00f6nnen?", "tokens": ["wann", "mei\u00b7ne", "Au\u00b7gen", "itzt", "ver\u00b7r\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der allerweiseste ", "tokens": ["Der", "al\u00b7ler\u00b7wei\u00b7ses\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "hat nichts darvon verrahten k\u00f6nnen.", "tokens": ["hat", "nichts", "dar\u00b7von", "ver\u00b7rah\u00b7ten", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PAV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da h\u00fclfft kein Jammer/ kein Geschrey/", "tokens": ["Da", "h\u00fclfft", "kein", "Jam\u00b7mer", "/", "kein", "Ge\u00b7schrey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$(", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mein Hertz ist gantz darvon entzwey!", "tokens": ["mein", "Hertz", "ist", "gantz", "dar\u00b7von", "ent\u00b7zwey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Eins ist mir sicher und gewi\u00df:", "tokens": ["Eins", "ist", "mir", "si\u00b7cher", "und", "ge\u00b7wi\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "acht Bretter werden mich \u00fcmbh\u00e4gen/", "tokens": ["acht", "Bret\u00b7ter", "wer\u00b7den", "mich", "\u00fcmb\u00b7h\u00e4\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wird wie au\u00df Sonne seyn darg\u00e4gen!", "tokens": ["wird", "wie", "au\u00df", "Son\u00b7ne", "seyn", "dar\u00b7g\u00e4\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "APPR", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Fleisch/ das l\u00fcderlich geprasst/", "tokens": ["Mein", "Fleisch", "/", "das", "l\u00fc\u00b7der\u00b7lich", "ge\u00b7prasst", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "f\u00e4ult dan al\u00df Wurm- und Schlangen-Mast!", "tokens": ["f\u00e4ult", "dan", "al\u00df", "Wur\u00b7m", "und", "Schlan\u00b7gen\u00b7Mast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "TRUNC", "KON", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Zwar das geehrte Testament", "tokens": ["Zwar", "das", "ge\u00b7ehr\u00b7te", "Tes\u00b7ta\u00b7ment"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "verheisst uns dr\u00f6hstlich die Posaune:", "tokens": ["ver\u00b7heisst", "uns", "dr\u00f6hst\u00b7lich", "die", "Po\u00b7sau\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "uns wekkt/ wenn alles sich gewendt/", "tokens": ["uns", "wekkt", "/", "wenn", "al\u00b7les", "sich", "ge\u00b7wendt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PIS", "PRF", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die gleichsahm himmlische Karthaune.", "tokens": ["die", "gleich\u00b7sahm", "himm\u00b7li\u00b7sche", "Kar\u00b7thau\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sey sein Gebein auch lengst zerst\u00e4ubt/", "tokens": ["Sey", "sein", "Ge\u00b7bein", "auch", "lengst", "zer\u00b7st\u00e4ubt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "der wird erh\u00f6ht/ wer dran gegl\u00e4ubt!", "tokens": ["der", "wird", "er\u00b7h\u00f6ht", "/", "wer", "dran", "ge\u00b7gl\u00e4ubt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "VVPP", "$(", "PWS", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch s\u00e4lbst gesezzt/ da\u00df dih\u00df geschicht/", "tokens": ["Doch", "s\u00e4lbst", "ge\u00b7sezzt", "/", "da\u00df", "dih\u00df", "ge\u00b7schicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$(", "KOUS", "PDS", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ich war ein arger Satans-Brahten/", "tokens": ["ich", "war", "ein", "ar\u00b7ger", "Sa\u00b7tans\u00b7Brah\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "vihlleicht so h\u00e4lt sich das Gericht", "tokens": ["vihl\u00b7leicht", "so", "h\u00e4lt", "sich", "das", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "an meine nichts wie Frefel-Dhaten.", "tokens": ["an", "mei\u00b7ne", "nichts", "wie", "Fre\u00b7fel\u00b7\u00b7D\u00b7ha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Die Zunge kl\u00e4bt mir und verdorrt/", "tokens": ["Die", "Zun\u00b7ge", "kl\u00e4bt", "mir", "und", "ver\u00b7dorrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "dan schlukkt mich ein der Schwefel-Port!", "tokens": ["dan", "schlukkt", "mich", "ein", "der", "Schwe\u00b7fel\u00b7Port", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Ein Rabe draussen krokkt ", "tokens": ["Ein", "Ra\u00b7be", "draus\u00b7sen", "krokkt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "wer wei\u00df/ ob ich ihn r\u00e4cht verstehe?", "tokens": ["wer", "wei\u00df", "/", "ob", "ich", "ihn", "r\u00e4cht", "ver\u00b7ste\u00b7he", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PPER", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob ich dih\u00df volle Stunden-Gla\u00df", "tokens": ["Ob", "ich", "dih\u00df", "vol\u00b7le", "Stun\u00b7den\u00b7Gla\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "noch ein-mahl abgeloffen sehe?", "tokens": ["noch", "ein\u00b7mahl", "ab\u00b7ge\u00b7lof\u00b7fen", "se\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ob sich das blancke Morgen-Licht", "tokens": ["Ob", "sich", "das", "blan\u00b7cke", "Mor\u00b7gen\u00b7Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "noch ein-mahl \u00fcmb mein Lager flicht!", "tokens": ["noch", "ein\u00b7mahl", "\u00fcmb", "mein", "La\u00b7ger", "flicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "O HERR/ wie dr\u00fckkt auff mir Dein Joch!", "tokens": ["O", "HeRR", "/", "wie", "dr\u00fckkt", "auff", "mir", "Dein", "Joch", "!"], "token_info": ["word", "word", "punct"