{"textgrid.poem.47707": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[handschuhe strickt' ich]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Handschuhe strickt' ich,", "tokens": ["Hand\u00b7schu\u00b7he", "strickt'", "ich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ein feines Paar;", "tokens": ["Ein", "fei\u00b7nes", "Paar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nicht in die H\u00f6he,", "tokens": ["Nicht", "in", "die", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Flei\u00dfige, blickt' ich,", "tokens": ["Die", "Flei\u00b7\u00dfi\u00b7ge", ",", "blickt'", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "So lange, bis ich", "tokens": ["So", "lan\u00b7ge", ",", "bis", "ich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Im Reinen war.", "tokens": ["Im", "Rei\u00b7nen", "war", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und nun, wer soll sie,", "tokens": ["Und", "nun", ",", "wer", "soll", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "VMFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Das ist die Frage,", "tokens": ["Das", "ist", "die", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Wer soll sie haben? \u2013", "tokens": ["Wer", "soll", "sie", "ha\u00b7ben", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Der junge Bruder? \u2013", "tokens": ["Der", "jun\u00b7ge", "Bru\u00b7der", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Und wird er sie", "tokens": ["Und", "wird", "er", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Geh\u00f6rig sch\u00e4tzen,", "tokens": ["Ge\u00b7h\u00f6\u00b7rig", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Die Schwestergaben? \u2013", "tokens": ["Die", "Schwes\u00b7ter\u00b7ga\u00b7ben", "?", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Sie werden, scheint's,", "tokens": ["Sie", "wer\u00b7den", ",", "scheint's", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Weit inniger", "tokens": ["Weit", "in\u00b7ni\u00b7ger"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Dem blaugeaugten,", "tokens": ["