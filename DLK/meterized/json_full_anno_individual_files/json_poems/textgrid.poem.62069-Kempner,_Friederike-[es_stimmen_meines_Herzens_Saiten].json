{"textgrid.poem.62069": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[es stimmen meines Herzens Saiten]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es stimmen meines Herzens Saiten,", "tokens": ["Es", "stim\u00b7men", "mei\u00b7nes", "Her\u00b7zens", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Herr, Dir an ein Dankgebet,", "tokens": ["O", "Herr", ",", "Dir", "an", "ein", "Dank\u00b7ge\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und tausend Stimmen es begleiten,", "tokens": ["Und", "tau\u00b7send", "Stim\u00b7men", "es", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sing es fr\u00fch und sp\u00e4t.", "tokens": ["Ich", "sing", "es", "fr\u00fch", "und", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was sing' ich denn? Ich singe: \u00bbErhaben,", "tokens": ["Was", "sing'", "ich", "denn", "?", "Ich", "sin\u00b7ge", ":", "\u00bb", "Er\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "$.", "$(", "VVPP", "$,"], "meter": "-+-+-+--+-", "