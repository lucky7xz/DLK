{"textgrid.poem.33683": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Sommerabend", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hab' ich diese Sonntagsruhe gern,", "tokens": ["Wie", "hab'", "ich", "die\u00b7se", "Sonn\u00b7tags\u00b7ru\u00b7he", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn alles still ist ringsum auf den Gassen!", "tokens": ["Wenn", "al\u00b7les", "still", "ist", "ring\u00b7sum", "auf", "den", "Gas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die T\u00fcren zu, die H\u00e4user ganz verlassen,", "tokens": ["Die", "T\u00fc\u00b7ren", "zu", ",", "die", "H\u00e4u\u00b7ser", "ganz", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Brunnen rauscht, es kr\u00e4ht ein Hahn von fern \u2013", "tokens": ["Ein", "Brun\u00b7nen", "rauscht", ",", "es", "kr\u00e4ht", "ein", "Hahn", "von", "fern", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf einmal zuckt der Blitze greller Schein,", "tokens": ["Auf", "ein\u00b7mal", "zuckt", "der", "Blit\u00b7ze", "grel\u00b7ler", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Sturmwind braust, es \u00e4chzt die Wetterfahne,", "tokens": ["Der", "Sturm\u00b7wind", "braust", ",", "es", "\u00e4chzt", "die", "Wet\u00b7ter\u00b7fah\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wir sind allein,", "tokens": ["Wir", "sind", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Nur Blumend\u00fcfte wehn zu uns herein", "tokens": ["Nur", "Blu\u00b7men\u00b7d\u00fcf\u00b7te", "wehn", "zu", "uns", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Durchs Gittertor der alten Steinaltane.", "tokens": ["Durchs", "Git\u00b7ter\u00b7tor", "der", "al\u00b7ten", "Stei\u00b7nal\u00b7ta\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier trennt uns nichts mehr, trautes Kind!", "tokens": ["Hier", "trennt", "uns", "nichts", "mehr", ",", "trau\u00b7tes", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mag um uns her, was will, geschehen,", "tokens": ["Mag", "um", "uns", "her", ",", "was", "will", ",", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "PTKVZ", "$,", "PWS", "VMFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir lassen still die Zeit vor\u00fcberwehen", "tokens": ["Wir", "las\u00b7sen", "still", "die", "Zeit", "vor\u00b7\u00fc\u00b7berw\u00b7e\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bleiben immer, wie wir heute sind:", "tokens": ["Und", "blei\u00b7ben", "im\u00b7mer", ",", "wie", "wir", "heu\u00b7te", "sind", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vertieft in uns, im seligsten Verein,", "tokens": ["Ver\u00b7tieft", "in", "uns", ",", "im", "se\u00b7ligs\u00b7ten", "Ver\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Und nichts mehr gibt es, was an Trennung mahne.", "tokens": ["Und", "nichts", "mehr", "gibt", "es", ",", "was", "an", "Tren\u00b7nung", "mah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wir sind allein,", "tokens": ["Wir", "sind", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und Blumend\u00fcfte wehn zu uns herein", "tokens": ["Und", "Blu\u00b7men\u00b7d\u00fcf\u00b7te", "wehn", "zu", "uns", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Durchs Gittertor der alten Steinaltane.", "tokens": ["Durchs", "Git\u00b7ter\u00b7tor", "der", "al\u00b7ten", "Stei\u00b7nal\u00b7ta\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie hab' ich diese Sonntagsruhe gern,", "tokens": ["Wie", "hab'", "ich", "die\u00b7se", "Sonn\u00b7tags\u00b7ru\u00b7he", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn alles still ist ringsum auf den Gassen!", "tokens": ["Wenn", "al\u00b7les", "still", "ist", "ring\u00b7sum", "auf", "den", "Gas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die T\u00fcren zu, die H\u00e4user ganz verlassen,", "tokens": ["Die", "T\u00fc\u00b7ren", "zu", ",", "die", "H\u00e4u\u00b7ser", "ganz", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Brunnen rauscht, es kr\u00e4ht ein Hahn von fern \u2013", "tokens": ["Ein", "Brun\u00b7nen", "rauscht", ",", "es", "kr\u00e4ht", "ein", "Hahn", "von", "fern"