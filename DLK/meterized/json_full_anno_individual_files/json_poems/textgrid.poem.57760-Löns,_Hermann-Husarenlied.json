{"textgrid.poem.57760": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Husarenlied", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hei\u00df ist die Liebe,", "tokens": ["Hei\u00df", "ist", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Kalt ist der Schnee, der Schnee;", "tokens": ["Kalt", "ist", "der", "Schnee", ",", "der", "Schnee", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Scheiden und Meiden", "tokens": ["Schei\u00b7den", "und", "Mei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und das tut weh.", "tokens": ["Und", "das", "tut", "weh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Rote Husaren,", "tokens": ["Ro\u00b7te", "Hu\u00b7sa\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Die reiten niemals, niemals Schritt;", "tokens": ["Die", "rei\u00b7ten", "nie\u00b7mals", ",", "nie\u00b7mals", "Schritt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herzliebes M\u00e4dchen", "tokens": ["Herz\u00b7lie\u00b7bes", "M\u00e4d\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Du kannst nicht mit.", "tokens": ["Du", "kannst", "nicht", "mit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wei\u00df ist die Feder", "tokens": ["Wei\u00df", "ist", "die", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "An meinem roten, roten Hut;", "tokens": ["An", "mei\u00b7nem", "ro\u00b7ten", ",", "ro\u00b7ten", "Hut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schwarz ist das Pulver,", "tokens": ["Schwarz", "ist", "das", "Pul\u00b7ver", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "Rot ist das Blut.", "tokens": ["Rot", "ist", "das", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Das gr\u00fcne Gl\u00e4slein", "tokens": ["Das", "gr\u00fc\u00b7ne", "Gl\u00e4s\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+---", "measure": "dactylic.init"}, "line.2": {"text": "Zersprang mir in der, in der Hand;", "tokens": ["Zer\u00b7sprang", "mir", "in", "der", ",", "in", "der", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "$,", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Br\u00fcder, ich sterbe", "tokens": ["Br\u00fc