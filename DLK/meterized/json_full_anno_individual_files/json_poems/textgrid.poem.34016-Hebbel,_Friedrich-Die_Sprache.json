{"textgrid.poem.34016": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sprache", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als h\u00f6chstes Wunder, das der Geist vollbrachte,", "tokens": ["Als", "h\u00f6chs\u00b7tes", "Wun\u00b7der", ",", "das", "der", "Geist", "voll\u00b7brach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Preis' ich die Sprache, die er, sonst verloren", "tokens": ["Preis'", "ich", "die", "Spra\u00b7che", ",", "die", "er", ",", "sonst", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In tiefste Einsamkeit, aus sich geboren,", "tokens": ["In", "tiefs\u00b7te", "Ein\u00b7sam\u00b7keit", ",", "aus", "sich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil sie allein die andern m\u00f6glich machte.", "tokens": ["Weil", "sie", "al\u00b7lein", "die", "an\u00b7dern", "m\u00f6g\u00b7lich", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja, wenn ich sie in Grund und Zweck betrachte,", "tokens": ["Ja", ",", "wenn", "ich", "sie", "in", "Grund", "und", "Zweck", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So hat nur sie den schweren Fluch beschworen,", "tokens": ["So", "hat", "nur", "sie", "den", "schwe\u00b7ren", "Fluch", "be\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem er, zum dumpfen Einzelsein erkoren,", "tokens": ["Dem", "er", ",", "zum", "dum\u00b7pfen", "Ein\u00b7zel\u00b7sein", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erlegen w\u00e4re, eh' er noch erwachte.", "tokens": ["Er\u00b7le\u00b7gen", "w\u00e4\u00b7re", ",", "eh'", "er", "noch", "er\u00b7wach\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn ist das unerforschte Eins und Alles", "tokens": ["Denn", "ist", "das", "un\u00b7er\u00b7forschte", "Eins", "und", "Al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "KON", "PIS"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In nie begriff'nem Selbstzersplitt'rungs-Drange", "tokens": ["In", "nie", "be\u00b7griff'\u00b7nem", "Selbst\u00b7zer\u00b7splitt'\u00b7rungs\u00b7Dran\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu einer Welt von Puncten gleich zerstoben:", "tokens": ["Zu", "ei\u00b7ner", "Welt", "von", "Punc\u00b7ten", "gleich", "zer\u00b7sto\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So wird durch sie, die jedes Wesen-Balles", "tokens": ["So", "wird", "durch", "sie", ",", "die", "je\u00b7des", "We\u00b7sen\u00b7Bal\u00b7les"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geheimstes Sein erscheinen l\u00e4\u00dft im Klange,", "tokens": ["Ge\u00b7heims\u00b7tes", "Sein", "er\u00b7schei\u00b7nen", "l\u00e4\u00dft", "im", "Klan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Trennung v\u00f6llig wieder aufgehoben!", "tokens": ["Die", "Tren\u00b7nung", "v\u00f6l\u00b7lig", "wie\u00b7der", "auf\u00b7ge\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Als h\u00f6chstes Wunder, das der Geist vollbrachte,", "tokens": ["Als", "h\u00f6chs\u00b7tes", "Wun\u00b7der", ",", "das", "der", "Geist", "voll\u00b7brach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure