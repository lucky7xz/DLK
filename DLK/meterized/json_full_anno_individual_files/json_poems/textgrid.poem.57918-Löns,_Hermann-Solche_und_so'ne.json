{"textgrid.poem.57918": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Solche und so'ne", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Naturverehrern gibt's zwei Sorten:", "tokens": ["Von", "Na\u00b7tur\u00b7ver\u00b7eh\u00b7rern", "gibt's", "zwei", "Sor\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "CARD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Erstens solche, denen die Natur", "tokens": ["Ers\u00b7tens", "sol\u00b7che", ",", "de\u00b7nen", "die", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIAT", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie sie ist, am allernettsten d\u00fcnket,", "tokens": ["Wie", "sie", "ist", ",", "am", "al\u00b7ler\u00b7netts\u00b7ten", "d\u00fcn\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ohne Beiwerk, ohne Korrektur.", "tokens": ["Oh\u00b7ne", "Bei\u00b7werk", ",", "oh\u00b7ne", "Kor\u00b7rek\u00b7tur", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Zweitens so'ne, und das sind die mehrsten,", "tokens": ["Zwei\u00b7tens", "so'\u00b7ne", ",", "und", "das", "sind", "die", "mehrs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "PDS", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denen macht dieselbe nur Pl\u00e4sier,", "tokens": ["De\u00b7nen", "macht", "die\u00b7sel\u00b7be", "nur", "Pl\u00e4\u00b7sier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDAT", "ADV", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn dieselbe etwas komfortabel", "tokens": ["Wenn", "die\u00b7sel\u00b7be", "et\u00b7was", "kom\u00b7for\u00b7ta\u00b7bel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist versehn mit Butterbrot und Bier.", "tokens": ["Ist", "ver\u00b7sehn", "mit", "But\u00b7ter\u00b7brot", "und", "Bier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n geebnet m\u00fcssen sein die Wege,", "tokens": ["Sch\u00f6n", "geeb\u00b7net", "m\u00fcs\u00b7sen", "sein", "die", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VMFIN", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ansichtskarten mu\u00df es geben dort,", "tokens": ["An\u00b7sichts\u00b7kar\u00b7ten", "mu\u00df", "es", "ge\u00b7ben", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und ein Aussichtsturm mu\u00df daselbst stehen,", "tokens": ["Und", "ein", "Aus\u00b7sichts\u00b7turm", "mu\u00df", "da\u00b7selbst", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn geeignet daf\u00fcr ist der Ort.", "tokens": ["Wenn", "ge\u00b7eig\u00b7net", "da\u00b7f\u00fcr", "ist", "der", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PAV", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Von dem Aussichtsturm da kann man n\u00e4mlich", "tokens": ["Von", "dem", "Aus\u00b7sichts\u00b7turm", "da", "kann", "man", "n\u00e4m\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VMFIN", "PIS", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die Natur sich voll Gef\u00fchl besehn", "tokens": ["Die", "Na\u00b7tur", "sich", "voll", "Ge\u00b7f\u00fchl", "be\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "ADJD", "NN", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und sch\u00e4rmeerisch zu dem Nachbar sagen:", "tokens": ["Und", "sch\u00e4r\u00b7mee\u00b7risch", "zu", "dem", "Nach\u00b7bar", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbdie Natur, die ist doch faktisch sch\u00f6n!\u00ab", "tokens": ["\u00bb", "die", "Na\u00b7tur", ",", "die", "ist", "doch", "fak\u00b7tisch", "sch\u00f6n", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "ADJD", "ADJD", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Von Naturverehrern gibt's zwei Sorten:", "tokens": ["Von", "Na\u00b7tur\u00b7ver\u00b7eh\u00b7rern", "gibt's", "zwei", "Sor\