{"dta.poem.4010": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Launischen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r' ich ferne nur her, wenn ich f\u00fcr mich geklagt,", "tokens": ["H\u00f6r'", "ich", "fer\u00b7ne", "nur", "her", ",", "wenn", "ich", "f\u00fcr", "mich", "ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Saitenspiel und Gesang, schwingt mir das Herz", "tokens": ["Sai\u00b7ten\u00b7spiel", "und", "Ge\u00b7sang", ",", "schwingt", "mir", "das", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "doch gleich;", "tokens": ["doch", "gleich", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Bald auch bin ich verwandelt,", "tokens": ["Bald", "auch", "bin", "ich", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Blinkst du, purpurner Wein! mich an", "tokens": ["Blinkst", "du", ",", "pur\u00b7pur\u00b7ner", "Wein", "!", "mich", "an"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "PPER", "PTKVZ"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Unter Schatten des Waldes, wo die gewaltige", "tokens": ["Un\u00b7ter", "Schat\u00b7ten", "des", "Wal\u00b7des", ",", "wo", "die", "ge\u00b7wal\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PWAV", "ART", "ADJA"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mittagssonne mir sanft \u00fcber dem Laube gl\u00e4nzt;", "tokens": ["Mit\u00b7tags\u00b7son\u00b7ne", "mir", "sanft", "\u00fc\u00b7ber", "dem", "Lau\u00b7be", "gl\u00e4nzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-++--++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ruhig sitz' ich daselbst, wenn,", "tokens": ["Ru\u00b7hig", "sitz'", "ich", "da\u00b7selbst", ",", "wenn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PAV", "$,", "KOUS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00fcrnend schwerer Beleidigung,", "tokens": ["Z\u00fcr\u00b7nend", "schwe\u00b7rer", "Be\u00b7lei\u00b7di\u00b7gung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich im Felde geirrt \u2014 z\u00fcrnen zu gerne doch", "tokens": ["Ich", "im", "Fel\u00b7de", "ge\u00b7irrt", "z\u00fcr\u00b7nen", "zu", "ger\u00b7ne", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$(", "VVFIN", "APPR", "ADV", "ADV"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Deine Dichter, Natur! trauern und weinen leicht,", "tokens": ["Dei\u00b7ne", "Dich\u00b7ter", ",", "Na\u00b7tur", "!", "trau\u00b7ern", "und", "wei\u00b7nen", "leicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$.", "VVINF", "KON", "VVINF", "ADJD", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die Begl\u00fcckten; wie Kinder,", "tokens": ["Die", "Be\u00b7gl\u00fcck\u00b7ten", ";", "wie", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWAV", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die zu z\u00e4rtlich die Mutter h\u00e4lt,", "tokens": ["Die", "zu", "z\u00e4rt\u00b7lich", "die", "Mut\u00b7ter", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKA", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Sind sie m\u00fcrrisch und voll herrischen Eigensinns;", "tokens": ["Sind", "sie", "m\u00fcr\u00b7risch", "und", "voll", "her\u00b7ri\u00b7schen", "Ei\u00b7gen\u00b7sinns", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Wandeln still sie des Wegs, irret Geringes doch", "tokens": ["Wan\u00b7deln", "still", "sie", "des", "Wegs", ",", "ir\u00b7ret", "Ge\u00b7rin\u00b7ges", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$,", "VVFIN", "NN", "ADV"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Bald sie wieder; sie rei\u00dfen", "tokens": ["Bald", "sie", "wie\u00b7der", ";", "sie", "rei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "ADV", "$.", "PPER", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aus dem Gleise sich str\u00e4ubend Dir.", "tokens": ["Aus", "dem", "Glei\u00b7se", "sich", "str\u00e4u\u00b7bend", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADJD", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Doch Du r\u00fchrest sie kaum, Liebende! freundlich an,", "tokens": ["Doch", "Du", "r\u00fch\u00b7rest", "sie", "kaum", ",", "Lie\u00b7ben\u00b7de", "!", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "NN", "$.", "ADJD", "PTKVZ", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sind sie friedlich und fromm; fr\u00f6hlich gehorchen sie!", "tokens": ["Sind", "sie", "fried\u00b7lich", "und", "fromm", ";", "fr\u00f6h\u00b7lich", "ge\u00b7hor\u00b7chen", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "ADJD", "VVFIN", "PPER", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Du lenkst, Meisterin! sie mit", "tokens": ["Du", "lenkst", ",", "Meis\u00b7te\u00b7rin", "!", "sie", "mit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "PPER", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weichem Z\u00fcgel, wohin Du willst.", "tokens": ["Wei\u00b7chem", "Z\u00fc\u00b7gel", ",", "wo\u00b7hin", "Du", "willst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}