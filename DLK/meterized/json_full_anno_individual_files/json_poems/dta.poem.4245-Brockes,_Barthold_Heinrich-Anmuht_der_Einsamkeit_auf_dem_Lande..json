{"dta.poem.4245": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anmuht der Einsamkeit  \n auf dem Lande.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In dieser angenehmen Ein\u00f6d', in diesen Schatten-reichen", "tokens": ["In", "die\u00b7ser", "an\u00b7ge\u00b7neh\u00b7men", "Ei\u00b7n\u00f6d'", ",", "in", "die\u00b7sen", "Schat\u00b7ten\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "B\u00fcschen", "tokens": ["B\u00fc\u00b7schen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Besch\u00e4ftigt sich mein stiller Geist, sich zu vergn\u00fcgen, zu erfri-", "tokens": ["Be\u00b7sch\u00e4f\u00b7tigt", "sich", "mein", "stil\u00b7ler", "Geist", ",", "sich", "zu", "ver\u00b7gn\u00fc\u00b7gen", ",", "zu", "er\u00b7fri"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$,", "PRF", "PTKZU", "VVINF", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "schen.", "tokens": ["schen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Hier labet mich, in sanfter Unschuld, die ruhige Zufrieden-", "tokens": ["Hier", "la\u00b7bet", "mich", ",", "in", "sanf\u00b7ter", "Un\u00b7schuld", ",", "die", "ru\u00b7hi\u00b7ge", "Zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "heit,", "tokens": ["heit", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Befreyt vom Hof- und Stadt-Get\u00fcmmel, entfernt von", "tokens": ["Be\u00b7freyt", "vom", "Hof", "und", "Stadt\u00b7Ge\u00b7t\u00fcm\u00b7mel", ",", "ent\u00b7fernt", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPRART", "TRUNC", "KON", "NN", "$,", "ADJD", "APPR"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Scheelsucht, Zank und Streit.", "tokens": ["Scheel\u00b7sucht", ",", "Zank", "und", "Streit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Mit dem verha\u00dften Lerm der Welt, dem ich mich hier", "tokens": ["Mit", "dem", "ver\u00b7ha\u00df\u00b7ten", "Lerm", "der", "Welt", ",", "dem", "ich", "mich", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nunmehr entzogen,", "tokens": ["nun\u00b7mehr", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.3": {"text": "Sind auch zugleich die schwarzen Sorgen von mir gewichen,", "tokens": ["Sind", "auch", "zu\u00b7gleich", "die", "schwar\u00b7zen", "Sor\u00b7gen", "von", "mir", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "weggeflogen.", "tokens": ["weg\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Hier, abgesondert von dem Vorwurf, aus welchem ihre", "tokens": ["Hier", ",", "ab\u00b7ge\u00b7son\u00b7dert", "von", "dem", "Vor\u00b7wurf", ",", "aus", "wel\u00b7chem", "ih\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVPP", "APPR", "ART", "NN", "$,", "APPR", "PWAT", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Plagen quillen,", "tokens": ["Pla\u00b7gen", "quil\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "F\u00fchl\u2019 ich, nur sanfte Regungen mein ruhiges Gem\u00fcht", "tokens": ["F\u00fchl'", "ich", ",", "nur", "sanf\u00b7te", "Re\u00b7gun\u00b7gen", "mein", "ru\u00b7hi\u00b7ges", "Ge\u00b7m\u00fcht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "ADV", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+--+----+-+--+", "measure": "dactylic.di.plus"}, "line.8": {"text": "erf\u00fcllen.", "tokens": ["er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Jm Schoo\u00df von einem tiefen Frieden, und sanfter Stille,", "tokens": ["Jm", "Schoo\u00df", "von", "ei\u00b7nem", "tie\u00b7fen", "Frie\u00b7den", ",", "und", "sanf\u00b7ter", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "stellt sich mir", "tokens": ["stellt", "sich", "mir"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Des stolzen Hofes schimmernd Elend entdeckt, und sonder", "tokens": ["Des", "stol\u00b7zen", "Ho\u00b7fes", "schim\u00b7mernd", "E\u00b7lend", "ent\u00b7deckt", ",", "und", "son\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "VVPP", "$,", "KON", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Schminke, f\u00fcr.", "tokens": ["Schmin\u00b7ke", ",", "f\u00fcr", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "APPR", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Ich seh\u2019 das Bild der eitlen Welt, ich seh\u2019 der G\u00f6tzen unsrer", "tokens": ["Ich", "seh'", "das", "Bild", "der", "eit\u00b7len", "Welt", ",", "ich", "seh'", "der", "G\u00f6t\u00b7zen", "uns\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Zeit,", "tokens": ["Zeit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Des Gl\u00fccks, des Reichthums und der Ehre, zerbrechliche", "tokens": ["Des", "Gl\u00fccks", ",", "des", "Reicht\u00b7hums", "und", "der", "Eh\u00b7re", ",", "zer\u00b7brech\u00b7li\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,", "ADJA"], "meter": "-+-++--+--+--", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Beschaffenheit,", "tokens": ["Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Und finde, wenn auch, im Besitz, sie unsern heissen Wunsch", "tokens": ["Und", "fin\u00b7de", ",", "wenn", "auch", ",", "im", "Be\u00b7sitz", ",", "sie", "un\u00b7sern", "heis\u00b7sen", "Wunsch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "$,", "APPRART", "NN", "$,", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "erf\u00fcllen,", "tokens": ["er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Da\u00df sie uns dennoch nicht vergn\u00fcgen, und nimmer die", "tokens": ["Da\u00df", "sie", "uns", "den\u00b7noch", "nicht", "ver\u00b7gn\u00fc\u00b7gen", ",", "und", "nim\u00b7mer", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$,", "KON", "ADV", "ART"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Begierden stillen.", "tokens": ["Be\u00b7gier\u00b7den", "stil\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Hier, wenn ich durch die Wiesen wandre, folgt oft mein", "tokens": ["Hier", ",", "wenn", "ich", "durch", "die", "Wie\u00b7sen", "wand\u00b7re", ",", "folgt", "oft", "mein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Blick dem schnellen Bach", "tokens": ["Blick", "dem", "schnel\u00b7len", "Bach"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In seinem ungehemmten, ew\u2019gen und immer regen Laufe", "tokens": ["In", "sei\u00b7nem", "un\u00b7ge\u00b7hemm\u00b7ten", ",", "ew'\u00b7gen", "und", "im\u00b7mer", "re\u00b7gen", "Lau\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "nach.", "tokens": ["nach", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Sein stets ununterbrochnes Rennen scheint mir von meinem", "tokens": ["Sein", "stets", "un\u00b7un\u00b7ter\u00b7broch\u00b7nes", "Ren\u00b7nen", "scheint", "mir", "von", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "regen Leben", "tokens": ["re\u00b7gen", "Le\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ein recht nat\u00fcrlich lebhaft Bild und wahres Gleichni\u00df", "tokens": ["Ein", "recht", "na\u00b7t\u00fcr\u00b7lich", "leb\u00b7haft", "Bild", "und", "wah\u00b7res", "Gleich\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "ADJD", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "abzugeben.", "tokens": ["ab\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVIZU", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Wie so viel Millionen Wellen auf seiner \u00e4ussern Fl\u00e4ch\u2019", "tokens": ["Wie", "so", "viel", "Mil\u00b7lion\u00b7en", "Wel\u00b7len", "auf", "sei\u00b7ner", "\u00e4us\u00b7sern", "Fl\u00e4ch'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIAT", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "entstanden,", "tokens": ["ent\u00b7stan\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Von welchen, da sie nicht mehr da, nicht die geringste Spuhr", "tokens": ["Von", "wel\u00b7chen", ",", "da", "sie", "nicht", "mehr", "da", ",", "nicht", "die", "ge\u00b7rings\u00b7te", "Spuhr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "ADV", "$,", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "vorhanden;", "tokens": ["vor\u00b7han\u00b7den", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "So sieht man alle Menschen auch entstehen, leben, bald", "tokens": ["So", "sieht", "man", "al\u00b7le", "Men\u00b7schen", "auch", "ent\u00b7ste\u00b7hen", ",", "le\u00b7ben", ",", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN", "ADV", "VVINF", "$,", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "verdrungen,", "tokens": ["ver\u00b7drun\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Und in dem Meer der Ewigkeit unwiederbringlich einge-", "tokens": ["Und", "in", "dem", "Meer", "der", "E\u00b7wig\u00b7keit", "un\u00b7wie\u00b7der\u00b7bring\u00b7lich", "ein\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.16": {"text": "schlungen.", "tokens": ["schlun\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Die Stille, die hier \u00fcberall so Feld als Wald bedeckt und", "tokens": ["Die", "Stil\u00b7le", ",", "die", "hier", "\u00fc\u00b7be\u00b7rall", "so", "Feld", "als", "Wald", "be\u00b7deckt", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADV", "NN", "KOUS", "NN", "VVPP", "KON"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "f\u00fcllt,", "tokens": ["f\u00fcllt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Vergleichet sich mit jener Stille, und scheint ein sanftes", "tokens": ["Ver\u00b7glei\u00b7chet", "sich", "mit", "je\u00b7ner", "Stil\u00b7le", ",", "und", "scheint", "ein", "sanf\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,", "KON", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ebenbild", "tokens": ["E\u00b7ben\u00b7bild"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Von jener ewig- sel\u2019gen Ruh, die, wenn man auf den", "tokens": ["Von", "je\u00b7ner", "e\u00b7wig", "sel'\u00b7gen", "Ruh", ",", "die", ",", "wenn", "man", "auf", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "TRUNC", "ADJA", "NN", "$,", "PRELS", "$,", "KOUS", "PIS", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sch\u00f6pfer denket,", "tokens": ["Sch\u00f6p\u00b7fer", "den\u00b7ket", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Die ewige selbst\u00e4nd\u2019ge Lieb\u2019, uns hier bereits der Glaube", "tokens": ["Die", "e\u00b7wi\u00b7ge", "selb\u00b7st\u00e4n\u00b7d'\u00b7ge", "Lieb'", ",", "uns", "hier", "be\u00b7reits", "der", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "schenket.", "tokens": ["schen\u00b7ket", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Ist denn der Lerm in dieser Welt, ja der Besitz von allen", "tokens": ["Ist", "denn", "der", "Lerm", "in", "die\u00b7ser", "Welt", ",", "ja", "der", "Be\u00b7sitz", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "$,", "ADV", "ART", "NN", "APPR", "PIAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Reichen,", "tokens": ["Rei\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Mit diesem Bilde jener Stille, die k\u00fcnftig ist, wohl zu ver-", "tokens": ["Mit", "die