word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Selbst in das Rohr am See.", "tokens": ["Selbst", "in", "das", "Rohr", "am", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Wir wandelten zufrieden,", "tokens": ["Wir", "wan\u00b7del\u00b7ten", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir glaubten uns zu zwei;", "tokens": ["Wir", "glaub\u00b7ten", "uns", "zu", "zwei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch anders war's beschieden,", "tokens": ["Doch", "an\u00b7ders", "wa\u00b7r's", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und sieh! wir waren drei,", "tokens": ["Und", "sieh", "!", "wir", "wa\u00b7ren", "drei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VAFIN", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und vier und f\u00fcnf und sechse,", "tokens": ["Und", "vier", "und", "f\u00fcnf", "und", "sech\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "KON", "CARD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie sa\u00dfen um den Topf,", "tokens": ["Sie", "sa\u00b7\u00dfen", "um", "den", "Topf", ","], "token_info": ["word", "wor