{"textgrid.poem.55057": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die gl\u00fccklichen Gatten", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach diesem Fr\u00fchlingsregen,", "tokens": ["Nach", "die\u00b7sem", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den wir so warm erfleht,", "tokens": ["Den", "wir", "so", "warm", "er\u00b7fleht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weibchen, o sieh den Segen,", "tokens": ["Weib\u00b7chen", ",", "o", "sieh", "den", "Se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "FM", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der unsre Flur durchweht.", "tokens": ["Der", "uns\u00b7re", "Flur", "durch\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur in der blauen Tr\u00fcbe", "tokens": ["Nur", "in", "der", "blau\u00b7en", "Tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verliert sich fern der Blick;", "tokens": ["Ver\u00b7liert", "sich", "fern", "der", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hier wandelt noch die Liebe,", "tokens": ["Hier", "wan\u00b7delt", "noch", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hier hauset noch das Gl\u00fcck.", "tokens": ["Hier", "hau\u00b7set", "noch", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das P\u00e4rchen wei\u00dfer Tauben,", "tokens": ["Das", "P\u00e4r\u00b7chen", "wei\u00b7\u00dfer", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du siehst, es fliegt dorthin,", "tokens": ["Du", "siehst", ",", "es", "fliegt", "dor\u00b7thin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo um besonnte Lauben", "tokens": ["Wo", "um", "be\u00b7sonn\u00b7te", "Lau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gef\u00fcllte Veilchen bl\u00fchn.", "tokens": ["Ge\u00b7f\u00fcll\u00b7te", "Veil\u00b7chen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dort banden wir zusammen", "tokens": ["Dort", "ban\u00b7den", "wir", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den allerersten Strau\u00df", "tokens": ["Den", "al\u00b7le\u00b7rers\u00b7ten", "Strau\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dort schlugen unsre Flammen", "tokens": ["Dort", "schlu\u00b7gen", "uns\u00b7re", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zuerst gewaltig aus.", "tokens": ["Zu\u00b7erst", "ge\u00b7wal\u00b7tig", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch als uns vom Altare,", "tokens": ["Doch", "als", "uns", "vom", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Nach dem beliebten Ja,", "tokens": ["Nach", "dem", "be\u00b7lieb\u00b7ten", "Ja", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit manchem jungen Paare", "tokens": ["Mit", "man\u00b7chem", "jun\u00b7gen", "Paa\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Pfarrer eilen sah,", "tokens": ["Der", "Pfar\u00b7rer", "ei\u00b7len", "sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da gingen andre Sonnen", "tokens": ["Da", "gin\u00b7gen", "and\u00b7re", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und andre Monden auf,", "tokens": ["Und", "and\u00b7re", "Mon\u00b7den", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da war die Welt gewonnen", "tokens": ["Da", "war", "die", "Welt", "ge\u00b7won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr unsern Lebenslauf.", "tokens": ["F\u00fcr", "un\u00b7sern", "Le\u00b7bens\u00b7lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und hunderttausend Siegel", "tokens": ["Und", "hun\u00b7dert\u00b7tau\u00b7send", "Sie\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["KON", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bekr\u00e4ftigten den Bund,", "tokens": ["Be\u00b7kr\u00e4f\u00b7tig\u00b7ten", "den", "Bund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im W\u00e4ldchen auf dem H\u00fcgel,", "tokens": ["Im", "W\u00e4ld\u00b7chen", "auf", "dem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Busch am Wiesengrund,", "tokens": ["Im", "Busch", "am", "Wie\u00b7sen\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In H\u00f6hlen, im Gem\u00e4uer", "tokens": ["In", "H\u00f6h\u00b7len", ",", "im", "Ge\u00b7m\u00e4u\u00b7er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf des Gekl\u00fcftes H\u00f6h,", "tokens": ["Auf", "des", "Ge\u00b7kl\u00fcf\u00b7tes", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Amor trug das Feuer", "tokens": ["Und", "A\u00b7mor", "trug", "das", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Selbst in das Rohr am See.", "tokens": ["Selbst", "in", "das", "Rohr", "am", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir wandelten zufrieden,", "tokens": ["Wir", "wan\u00b7del\u00b7ten", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir glaubten uns zu zwei;", "tokens": ["Wir", "glaub\u00b7ten", "uns", "zu", "zwei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch anders war's beschieden,", "tokens": ["Doch", "an\u00b7ders", "wa\u00b7r's", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und sieh! wir waren drei,", "tokens": ["Und", "sieh", "!", "wir", "wa\u00b7ren", "drei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VAFIN", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und vier und f\u00fcnf und sechse,", "tokens": ["Und", "vier", "und", "f\u00fcnf", "und", "sech\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "KON", "CARD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie sa\u00dfen um den Topf,", "tokens": ["Sie", "sa\u00b7\u00dfen", "um", "den", "Topf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und nun sind die Gew\u00e4chse", "tokens": ["Und", "nun", "sind", "die", "Ge\u00b7w\u00e4ch\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Fast all uns \u00fcbern Kopf.", "tokens": ["Fast", "all", "uns", "\u00fc\u00b7bern", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und dort in sch\u00f6ner Fl\u00e4che", "tokens": ["Und", "dort", "in", "sch\u00f6\u00b7ner", "Fl\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das neugebaute Haus", "tokens": ["Das", "neu\u00b7ge\u00b7bau\u00b7te", "Haus"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umschlingen Pappelb\u00e4che,", "tokens": ["Um\u00b7schlin\u00b7gen", "Pap\u00b7pel\u00b7b\u00e4\u00b7che", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So freundlich sieht's heraus.", "tokens": ["So", "freund\u00b7lich", "sieht's", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NE", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer schaffte wohl da dr\u00fcben", "tokens": ["Wer", "schaff\u00b7te", "wohl", "da", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich diesen frohen Sitz?", "tokens": ["Sich", "die\u00b7sen", "fro\u00b7hen", "Sitz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist es mit seiner Lieben", "tokens": ["Ist", "es", "mit", "sei\u00b7ner", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht unser braver Fritz?", "tokens": ["Nicht", "un\u00b7ser", "bra\u00b7ver", "Fritz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wo im Felsengrunde", "tokens": ["Und", "wo", "im", "Fel\u00b7sen\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der eingeklemmte Flu\u00df", "tokens": ["Der", "ein\u00b7ge\u00b7klemm\u00b7te", "Flu\u00df"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich sch\u00e4umend aus dem Schlunde", "tokens": ["Sich", "sch\u00e4u\u00b7mend", "aus", "dem", "Schlun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf R\u00e4der st\u00fcrzen mu\u00df:", "tokens": ["Auf", "R\u00e4\u00b7der", "st\u00fcr\u00b7zen", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man spricht von M\u00fcllerinnen", "tokens": ["Man", "spricht", "von", "M\u00fcl\u00b7le\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wie so sch\u00f6n sie sind;", "tokens": ["Und", "wie", "so", "sch\u00f6n", "sie", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch immer wird gewinnen", "tokens": ["Doch", "im\u00b7mer", "wird", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dort hinten unser Kind.", "tokens": ["Dort", "hin\u00b7ten", "un\u00b7ser", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch wo das Gr\u00fcn so dichte", "tokens": ["Doch", "wo", "das", "Gr\u00fcn", "so", "dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um Kirch und Rasen steht,", "tokens": ["Um", "Kirch", "und", "Ra\u00b7sen", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da, wo die alte Fichte", "tokens": ["Da", ",", "wo", "die", "al\u00b7te", "Fich\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allein zum Himmel weht,", "tokens": ["Al\u00b7lein", "zum", "Him\u00b7mel", "weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da ruhet unsrer Toten", "tokens": ["Da", "ru\u00b7het", "uns\u00b7rer", "To\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Fr\u00fchzeitiges Geschick", "tokens": ["Fr\u00fch\u00b7zei\u00b7ti\u00b7ges", "Ge\u00b7schick"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und leitet von dem Boden", "tokens": ["Und", "lei\u00b7tet", "von", "dem", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Himmel unsern Blick.", "tokens": ["Zum", "Him\u00b7mel", "un\u00b7sern", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Es blitzen Waffenwogen", "tokens": ["Es", "blit\u00b7zen", "Waf\u00b7fen\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den H\u00fcgel schwankend ab.", "tokens": ["Den", "H\u00fc\u00b7gel", "schwan\u00b7kend", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Heer, es kommt g