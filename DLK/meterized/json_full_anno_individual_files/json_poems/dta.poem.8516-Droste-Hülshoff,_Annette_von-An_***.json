{"dta.poem.8516": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "An ***", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf hohem Felsen lieg' ich hier,", "tokens": ["Auf", "ho\u00b7hem", "Fel\u00b7sen", "lieg'", "ich", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Krankheit Nebel \u00fcber mir,", "tokens": ["Der", "Krank\u00b7heit", "Ne\u00b7bel", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unter mir der tiefe See", "tokens": ["Und", "un\u00b7ter", "mir", "der", "tie\u00b7fe", "See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seiner n\u00e4cht\u2019gen Klage Weh,", "tokens": ["Mit", "sei\u00b7ner", "n\u00e4cht'\u00b7gen", "Kla\u00b7ge", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit seinem Jubel, seiner Lust,", "tokens": ["Mit", "sei\u00b7nem", "Ju\u00b7bel", ",", "sei\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn buntgeschm\u00fcckte Wimpel fliegen,", "tokens": ["Wenn", "bunt\u00b7ge\u00b7schm\u00fcck\u00b7te", "Wim\u00b7pel", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit seinem Dr\u00e4u\u2019n aus hohler Brust,", "tokens": ["Mit", "sei\u00b7nem", "Dr\u00e4u'n", "aus", "hoh\u00b7ler", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn Sturm und Welle sich bekriegen.", "tokens": ["Wenn", "Sturm", "und", "Wel\u00b7le", "sich", "be\u00b7krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir ist er gar ein trauter Freund,", "tokens": ["Mir", "ist", "er", "gar", "ein", "trau\u00b7ter", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mit mir l\u00e4chelt, mit mir weint,", "tokens": ["Der", "mit", "mir", "l\u00e4\u00b7chelt", ",", "mit", "mir", "weint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist, wenn er gr\u00fcnlich golden ruht,", "tokens": ["Ist", ",", "wenn", "er", "gr\u00fcn\u00b7lich", "gol\u00b7den", "ruht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir eine sanfte Zauberfluth,", "tokens": ["Mir", "ei\u00b7ne", "sanf\u00b7te", "Zau\u00b7ber\u00b7fluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus deren tiefem, klaren Grund", "tokens": ["Aus", "de\u00b7ren", "tie\u00b7fem", ",", "kla\u00b7ren", "Grund"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gestalten meines Lebens steigen,", "tokens": ["Ge\u00b7stal\u00b7ten", "mei\u00b7nes", "Le\u00b7bens", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Geliebte Augen, s\u00fc\u00dfer Mund", "tokens": ["Ge\u00b7lieb\u00b7te", "Au\u00b7gen", ",", "s\u00fc\u00b7\u00dfer", "Mund"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich l\u00e4chelnd tr\u00f6stend zu mir neigen.", "tokens": ["Sich", "l\u00e4\u00b7chelnd", "tr\u00f6s\u00b7tend", "zu", "mir", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie hab\u2019 ich schon so manche Nacht", "tokens": ["Wie", "hab'", "ich", "schon", "so", "man\u00b7che", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Mondes Wiederschein bewacht!", "tokens": ["Des", "Mon\u00b7des", "Wie\u00b7der\u00b7schein", "be\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die klare Bahn auf dunklem Gr\u00fcn,", "tokens": ["Die", "kla\u00b7re", "Bahn", "auf", "dunk\u00b7lem", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo meiner Todten Schatten zieh\u2019n;", "tokens": ["Wo", "mei\u00b7ner", "Tod\u00b7ten", "Schat\u00b7ten", "zieh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie manchen Tag den lichten Hang,", "tokens": ["Wie", "man\u00b7chen", "Tag", "den", "lich\u00b7ten", "Hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bewegt von h\u00fcpfend leichten Schritten,", "tokens": ["Be\u00b7wegt", "von", "h\u00fcp\u00b7fend", "leich\u00b7ten", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf dem mit leisem Geistergang", "tokens": ["Auf", "dem", "mit", "lei\u00b7sem", "Geis\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Meiner Lebend\u2019gen Bilder glitten.", "tokens": ["Mei\u00b7ner", "Le\u00b7ben\u00b7d'\u00b7gen", "Bil\u00b7der", "glit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und als dein Bild vor\u00fcberschwand,", "tokens": ["Und", "als", "dein", "Bild", "vor\u00b7\u00fc\u00b7bersc\u00b7hwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da streckte ich nach dir die Hand,", "tokens": ["Da", "streck\u00b7te", "ich", "nach", "dir", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meiner Seele ward es weh,", "tokens": ["Und", "mei\u00b7ner", "See\u00b7le", "ward", "es", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df dir verborgen ihre N\u00e4h\u2019;", "tokens": ["Da\u00df", "dir", "ver\u00b7bor\u00b7gen", "ih\u00b7re", "N\u00e4h'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So nimm denn meine Lieder nun", "tokens": ["So", "nimm", "denn", "mei\u00b7ne", "Lie\u00b7der", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "KON", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als liebesrothe Flammenzungen,", "tokens": ["Als", "lie\u00b7bes\u00b7ro\u00b7the", "Flam\u00b7men\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df sie in deinem Busen ruh\u2019n", "tokens": ["La\u00df", "sie", "in", "dei\u00b7nem", "Bu\u00b7sen", "ruh'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und denk\u2019 ich hab\u2019 sie dir gesungen.", "tokens": ["Und", "denk'", "ich", "hab'", "sie", "dir", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}