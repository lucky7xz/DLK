{"textgrid.poem.39646": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist die neueste der \u00e4ltesten Geschichten,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist die neueste der \u00e4ltesten Geschichten,", "tokens": ["Was", "ist", "die", "neu\u00b7es\u00b7te", "der", "\u00e4l\u00b7tes\u00b7ten", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worin vom kleinen Rom der gro\u00dfe Niebuhr sprich?", "tokens": ["Wo\u00b7rin", "vom", "klei\u00b7nen", "Rom", "der", "gro\u00b7\u00dfe", "Nie\u00b7buhr", "sprich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NE", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er kann viel Neues noch vom \u00e4ltesten berichten;", "tokens": ["Er", "kann", "viel", "Neu\u00b7es", "noch", "vom", "\u00e4l\u00b7tes\u00b7ten", "be\u00b7rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er ist auf Widerspruch selbst gegen sich erpicht.", "tokens": ["Er", "ist", "auf", "Wi\u00b7der\u00b7spruch", "selbst", "ge\u00b7gen", "sich", "er\u00b7picht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der zweite Druck begann, den ersten zu vernichten:", "tokens": ["Der", "zwei\u00b7te", "Druck", "be\u00b7gann", ",", "den", "ers\u00b7ten", "zu", "ver\u00b7nich\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der dritte macht nunmehr den zweiten schon zunicht.", "tokens": ["Der", "drit\u00b7te", "macht", "nun\u00b7mehr", "den", "zwei\u00b7ten", "schon", "zu\u00b7nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du gleichest dem Saturn, der, wie die Alten dichten,", "tokens": ["Du", "glei\u00b7chest", "dem", "Sa\u00b7turn", ",", "der", ",", "wie", "die", "Al\u00b7ten", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "$,", "PWAV", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sich Kinder nur gezeugt zum leckersten Gericht.", "tokens": ["Sich", "Kin\u00b7der", "nur", "ge\u00b7zeugt", "zum", "le\u00b7ckers\u00b7ten", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "ADV", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Einstweilen solltest du noch auf dein Mahl verzichten,", "tokens": ["Einst\u00b7wei\u00b7len", "soll\u00b7test", "du", "noch", "auf", "dein", "Mahl", "ver\u00b7zich\u00b7ten", ","], "token_info": ["word", "word", "word", "wo