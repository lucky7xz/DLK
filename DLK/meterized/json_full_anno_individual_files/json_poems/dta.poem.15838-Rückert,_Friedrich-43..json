{"dta.poem.15838": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sagst: \u201eNicht \u00fcbel ist der Garten deiner Wahl,", "tokens": ["Du", "sagst", ":", "\u201e", "Nicht", "\u00fc\u00b7bel", "ist", "der", "Gar\u00b7ten", "dei\u00b7ner", "Wahl", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PTKNEG", "ADJD", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch mittendurch der Weg, der Weg ist viel zu schmal.", "tokens": ["Doch", "mit\u00b7ten\u00b7durch", "der", "Weg", ",", "der", "Weg", "ist", "viel", "zu", "schmal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du scheinst am liebsten nur mit dir allein zu schreiten,", "tokens": ["Du", "scheinst", "am", "liebs\u00b7ten", "nur", "mit", "dir", "al\u00b7lein", "zu", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "ADV", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es haben zwei nicht Raum, eins an des andern Seiten.\u201c", "tokens": ["Es", "ha\u00b7ben", "zwei", "nicht", "Raum", ",", "eins", "an", "des", "an\u00b7dern", "Sei\u00b7ten", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "CARD", "PTKNEG", "NN", "$,", "PIS", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mitnichten nur allein! es geht sich wohl zu zwein,", "tokens": ["Mit\u00b7nich\u00b7ten", "nur", "al\u00b7lein", "!", "es", "geht", "sich", "wohl", "zu", "zwein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$.", "PPER", "VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Freund mit dem Freunde, wo sich Arm in Arm schlingt ein.", "tokens": ["Freund", "mit", "dem", "Freun\u00b7de", ",", "wo", "sich", "Arm", "in", "Arm", "schlingt", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PWAV", "PRF", "NN", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Es geht sich wohl zu zwein, oft bin ich so gegangen,", "tokens": ["Es", "geht", "sich", "wohl", "zu", "zwein", ",", "oft", "bin", "ich", "so", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Freundin mir zun\u00e4chst, umfangend und umfangen.", "tokens": ["Die", "Freun\u00b7din", "mir", "zu\u00b7n\u00e4chst", ",", "um\u00b7fan\u00b7gend", "und", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ja, Raum dazwischen hat ein Kleines durchzuschl\u00fcpfen,", "tokens": ["Ja", ",", "Raum", "da\u00b7zwi\u00b7schen", "hat", "ein", "Klei\u00b7nes", "durch\u00b7zu\u00b7schl\u00fcp\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "PAV", "VAFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indessen hinterher und vor die Gr\u00f6\u00dfern h\u00fcpfen.", "tokens": ["In\u00b7des\u00b7sen", "hin\u00b7ter\u00b7her", "und", "vor", "die", "Gr\u00f6\u00b7\u00dfern", "h\u00fcp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und wenn rechts oder links wir an die Hecke streifen,", "tokens": ["Und", "wenn", "rechts", "o\u00b7der", "links", "wir", "an", "die", "He\u00b7cke", "strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "KON", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sind es Rosen nur, die uns im Scherz ergreifen.", "tokens": ["So", "sind", "es", "Ro\u00b7sen", "nur", ",", "die", "uns", "im", "Scherz", "er\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So ist der schmale Gang f\u00fcr mich ja breit genug,", "tokens": ["So", "ist", "der", "schma\u00b7le", "Gang", "f\u00fcr", "mich", "ja", "breit", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist der schmale Weg, den ich zum Gl\u00fcck einschlug.", "tokens": ["Es", "ist", "der", "schma\u00b7le", "Weg", ",", "den", "ich", "zum", "Gl\u00fcck", "ein\u00b7schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der Gang ist nur zu schmal f\u00fcr f\u00f6rmlichen Besuch,", "tokens": ["Der", "Gang", "ist", "nur", "zu", "schmal", "f\u00fcr", "f\u00f6rm\u00b7li\u00b7chen", "Be\u00b7such", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ich entbehre gern dergleichen Stadtzuspruch.", "tokens": ["Und", "ich", "ent\u00b7beh\u00b7re", "gern", "derg\u00b7lei\u00b7chen", "Stadt\u00b7zu\u00b7spruch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}