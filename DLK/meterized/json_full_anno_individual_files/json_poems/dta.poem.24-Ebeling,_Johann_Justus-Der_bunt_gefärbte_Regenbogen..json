{"dta.poem.24": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n bunt gef\u00e4rbte Regenbogen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah nicht ohne innre Lust, im aus-\ngespannten Luftrevieren,", "tokens": ["Ich", "sah", "nicht", "oh\u00b7ne", "inn\u00b7re", "Lust", ",", "im", "aus", "ge\u00b7spann\u00b7ten", "Luf\u00b7tre\u00b7vie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,", "APPRART", "TRUNC", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wie nach den blossen Augenschein,", "tokens": ["Wie", "nach", "den", "blos\u00b7sen", "Au\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wolken dik gewebter Schleier", "tokens": ["Der", "Wol\u00b7ken", "dik", "ge\u00b7web\u00b7ter", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da durch ein tr\u00f6pfelndes Gedr\u00e4n-", "tokens": ["Da", "durch", "ein", "tr\u00f6p\u00b7feln\u00b7des", "Ge\u00b7dr\u00e4n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne die dadurch verh\u00fcllt, und durch die", "tokens": ["Die", "Son\u00b7ne", "die", "da\u00b7durch", "ver\u00b7h\u00fcllt", ",", "und", "durch", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PAV", "VVPP", "$,", "KON", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ging bey den Regnen wieder auf; da sah ich einen", "tokens": ["Ging", "bey", "den", "Reg\u00b7nen", "wie\u00b7der", "auf", ";", "da", "sah", "ich", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich stand gleichsam im Mittelpuncte, an einer Seite", "tokens": ["Ich", "stand", "gleich\u00b7sam", "im", "Mit\u00b7tel\u00b7punc\u00b7te", ",", "an", "ei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und an der anderen die Wolken, die mit den Na\u00df", "tokens": ["Und", "an", "der", "an\u00b7de\u00b7ren", "die", "Wol\u00b7ken", ",", "die", "mit", "den", "Na\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Da denn die Sonn in Regen schien, und sich bey", "tokens": ["Da", "denn", "die", "Sonn", "in", "Re\u00b7gen", "schien", ",", "und", "sich", "bey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,", "KON", "PRF", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "In Tropfen durch den Gegenschlag, so bunt gef\u00e4rb-", "tokens": ["In", "Trop\u00b7fen", "durch", "den", "Ge\u00b7gen\u00b7schlag", ",", "so", "bunt", "ge\u00b7f\u00e4r\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die das Naturreich untersuchen, die haben l\u00e4ngst", "tokens": ["Die", "das", "Na\u00b7tur\u00b7reich", "un\u00b7ter\u00b7su\u00b7chen", ",", "die", "ha\u00b7ben", "l\u00e4ngst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVINF", "$,", "PRELS", "VAFIN", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Da\u00df nicht die bunten Regenbogen, in den erhab-", "tokens": ["Da\u00df", "nicht", "die", "bun\u00b7ten", "Re\u00b7gen\u00b7bo\u00b7gen", ",", "in", "den", "er\u00b7hab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Wie woll die bl\u00f6de Einfalt meint, da\u00df sich am", "tokens": ["Wie", "woll", "die", "bl\u00f6\u00b7de", "Ein\u00b7falt", "meint", ",", "da\u00df", "sich", "am"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PRF", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Dergleichen die aus Dunst bestehn, wie ausgespann-", "tokens": ["Derg\u00b7lei\u00b7chen", "die", "aus", "Dunst", "be\u00b7stehn", ",", "wie", "aus\u00b7ge\u00b7spann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "ART", "APPR", "NN", "VVINF", "$,", "PWAV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wir wissen, da\u00df in Regentropfen, sich das ent-", "tokens": ["Wir", "wis\u00b7sen", ",", "da\u00df", "in", "Re\u00b7gen\u00b7trop\u00b7fen", ",", "sich", "das", "ent"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "NN", "$,", "PRF", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als wie in einer holen Fl\u00e4che, im Ein- und Aus-", "tokens": ["Als", "wie", "in", "ei\u00b7ner", "ho\u00b7len", "Fl\u00e4\u00b7che", ",", "im", "Ein", "und", "Aus"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "APPR", "ART", "ADJA", "NN", "$,", "APPRART", "TRUNC", "KON", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Da zeiget sich der bunte Schein, der wie ein run-", "tokens": ["Da", "zei\u00b7get", "sich", "der", "bun\u00b7te", "Schein", ",", "der", "wie", "ein", "run"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,", "PRELS", "KOKOM", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und sich, wie unserm Augen d\u00fcnkt, in den gew\u00f6lb-", "tokens": ["Und", "sich", ",", "wie", "un\u00b7serm", "Au\u00b7gen", "d\u00fcnkt", ",", "in", "den", "ge\u00b7w\u00f6lb"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PRF", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Farben durchgestreifte Z\u00fcge, sind wenn wir", "tokens": ["Der", "Far\u00b7ben", "durch\u00b7ge\u00b7streif\u00b7te", "Z\u00fc\u00b7ge", ",", "sind", "wenn", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "VAFIN", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als wie am ausgespannten Tuche, in wollgemahl-", "tokens": ["Als", "wie", "am", "aus\u00b7ge\u00b7spann\u00b7ten", "Tu\u00b7che", ",", "in", "woll\u00b7ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "KOKOM", "APPRART", "ADJA", "NN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Der \u00e4usre Rand ist roth gemahlt, woran die gelbe", "tokens": ["Der", "\u00e4us\u00b7re", "Rand", "ist", "roth", "ge\u00b7mahlt", ",", "wo\u00b7ran", "die", "gel\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$,", "PWAV", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dann folgt ein gr\u00fcn gezogner Strich daran die", "tokens": ["Dann", "folgt", "ein", "gr\u00fcn", "ge\u00b7zog\u00b7ner", "Strich", "da\u00b7ran", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "ADJA", "NN", "PAV", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Am Ende strahlt der Purpur Schein in seiner dun-", "tokens": ["Am", "En\u00b7de", "strahlt", "der", "Pur\u00b7pur", "Schein", "in", "sei\u00b7ner", "dun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Als wenn der Bogen so gewirkt, wie eine k\u00fcnstli-", "tokens": ["Als", "wenn", "der", "Bo\u00b7gen", "so", "ge\u00b7wirkt", ",", "wie", "ei\u00b7ne", "k\u00fcnst\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADV", "VVPP", "$,", "PWAV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich hatte dabey die Gedanken: Es m\u00fcsse in dem", "tokens": ["Ich", "hat\u00b7te", "da\u00b7bey", "die", "Ge\u00b7dan\u00b7ken", ":", "Es", "m\u00fcs\u00b7se", "in", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "ART", "NN", "$.", "PPER", "VMFIN", "APPR", "ART"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.26": {"text": "In diesen grossen Wunderlichte die Mischung aller", "tokens": ["In", "die\u00b7sen", "gros\u00b7sen", "Wun\u00b7der\u00b7lich\u00b7te", "die", "Misc\u00b7hung", "al\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "NN", "PIAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Die in den Bogen anzusehn, und in den Gegen-", "tokens": ["Die", "in", "den", "Bo\u00b7gen", "an\u00b7zu\u00b7sehn", ",", "und", "in", "den", "Ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVIZU", "$,", "KON", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn durch der Tropfen rundes Na\u00df die ausgebliz-", "tokens": ["Wenn", "durch", "der", "Trop\u00b7fen", "run\u00b7des", "Na\u00df", "die", "aus\u00b7ge\u00b7bli\u00b7z"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Bei dieser sch\u00f6nen Augenweide, die mir des Re-", "tokens": ["Bei", "die\u00b7ser", "sch\u00f6\u00b7nen", "Au\u00b7gen\u00b7wei\u00b7de", ",", "die", "mir", "des", "Re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.30": {"text": "An einen tr\u00fcben Sommer-Tage, zu meiner See-", "tokens": ["An", "ei\u00b7nen", "tr\u00fc\u00b7ben", "Som\u00b7mer\u00b7Ta\u00b7ge", ",", "zu", "mei\u00b7ner", "See"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Durchbrach ein heller Andachtsstrahl, mein durch", "tokens": ["Durch\u00b7brach", "ein", "hel\u00b7ler", "An\u00b7dachts\u00b7strahl", ",", "mein", "durch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PPOSAT", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Vertrieb in einem Augenblik das Sorgen-Heer mit", "tokens": ["Ver\u00b7trieb", "in", "ei\u00b7nem", "Au\u00b7gen\u00b7blik", "das", "Sor\u00b7gen\u00b7Heer", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es fiel mir aus dem Bibel-Buche, mit innigen", "tokens": ["Es", "fiel", "mir", "aus", "dem", "Bi\u00b7bel\u00b7Bu\u00b7che", ",", "mit", "in\u00b7ni\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df die gef\u00e4rbten Regenbogen, die Zeichen ewger", "tokens": ["Da\u00df", "die", "ge\u00b7f\u00e4rb\u00b7ten", "Re\u00b7gen\u00b7bo\u00b7gen", ",", "die", "Zei\u00b7chen", "ew\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.36": {"text": "Aus Liebe und Barmherzigkeit, zum sichren Denk-", "tokens": ["Aus", "Lie\u00b7be", "und", "Barm\u00b7her\u00b7zig\u00b7keit", ",", "zum", "sich\u00b7ren", "Denk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Da\u00df sein Erbarmen nie aufh\u00f6ret, wenn man da-", "tokens": ["Da\u00df", "sein", "Er\u00b7bar\u00b7men", "nie", "auf\u00b7h\u00f6\u00b7ret", ",", "wenn", "man", "da"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "KOUS", "PIS", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wenn die verdorbne Welt in Lastern, als wie in", "tokens": ["Wenn", "die", "ver\u00b7dorb\u00b7ne", "Welt", "in", "Las\u00b7tern", ",", "als", "wie", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "$,", "KOUS", "KOKOM", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Er ist ein Zeichen da\u00df die Welt, die durch die Bos-", "tokens": ["Er", "ist", "ein", "Zei\u00b7chen", "da\u00df", "die", "Welt", ",", "die", "durch", "die", "Bos"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KOUS", "ART", "NN", "$,", "PRELS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Durch eine S\u00fcndflut abgesp\u00fclt, und wie in einen", "tokens": ["Durch", "ei\u00b7ne", "S\u00fcnd\u00b7flut", "ab\u00b7ge\u00b7sp\u00fclt", ",", "und", "wie", "in", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "KON", "PWAV", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Da ", "tokens": ["Da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.42": {"text": "Und dadurch die verdorbnen Menschen, in ihren", "tokens": ["Und", "da\u00b7durch", "die", "ver\u00b7dorb\u00b7nen", "Men\u00b7schen", ",", "in", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PAV", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Ach! d\u00e4chte jeder noch daran; so w\u00fcrde dieses Gna-", "tokens": ["Ach", "!", "d\u00e4ch\u00b7te", "je\u00b7der", "noch", "da\u00b7ran", ";", "so", "w\u00fcr\u00b7de", "die\u00b7ses", "Gna"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PIS", "ADV", "PAV", "$.", "ADV", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.44": {"text": "Ein Prediger der Busse seyn, und das verstokte", "tokens": ["Ein", "Pre\u00b7di\u00b7ger", "der", "Bus\u00b7se", "seyn", ",", "und", "das", "ver\u00b7stok\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAINF", "$,", "KON", "PDS", "VVFIN"], "meter": "-+-+-+-+-+-+-