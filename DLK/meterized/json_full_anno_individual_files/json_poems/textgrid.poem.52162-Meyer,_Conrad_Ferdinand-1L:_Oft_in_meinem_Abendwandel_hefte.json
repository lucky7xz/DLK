: "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Er ist wahr und rein und ohne Trug,", "tokens": ["Er", "ist", "wahr", "und", "rein", "und", "oh\u00b7ne", "Trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Er beseliget und st\u00e4rkt das Leben", "tokens": ["Er", "be\u00b7se\u00b7li\u00b7get", "und", "st\u00e4rkt", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit der tiefsten Sehnsucht stillem Zug.", "tokens": ["Mit", "der", "tiefs\u00b7ten", "Sehn\u00b7sucht", "stil\u00b7lem", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht versteht er Gottes dunkeln Willen,", "tokens": ["Nicht", "ver\u00b7steht", "er", "Got\u00b7tes", "dun\u00b7keln", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Noch der Dinge letzten ew'gen Grund,", "tokens": ["Noch", "der", "Din\u00b7ge", "letz\u00b7ten", "ew'\u00b7gen", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wunden heilt er, Schmerzen kann er stillen", "tokens": ["Wun\u00b7den", "heilt", "er", ",", "Schmer\u00b7zen", "kann", "er", "stil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie das Wort aus eines Freundes Mund.", "tokens": ["Wie", "das", "Wort", "aus", "ei\u00b7nes", "Freun\u00b7des", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "In die Bangnis, die Bedr\u00e4ngnis funkelt", "tokens": ["In", "die", "Bang\u00b7nis", ",", "die", "Be\u00b7dr\u00e4ng\u00b7nis", "fun\u00b7kelt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Er mit seinem hellsten Strahle gern,", "tokens": ["Er", "mit", "sei\u00b7nem", "hells\u00b7ten", "Strah\u00b7le", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und je mehr die Erde m\u00e4hlich dunkelt,", "tokens": ["Und", "je", "mehr", "die", "Er\u00b7de", "m\u00e4h\u00b7lich", "dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Desto n\u00e4her, st\u00e4rker brennt mein Stern.", "tokens": ["Des\u00b7to", "n\u00e4\u00b7her", ",", "st\u00e4r\u00b7ker", "brennt", "mein", "Stern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Holder, einen Namen wirst du tragen,", "token