{"textgrid.poem.41034": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An ***", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Buch, verworfen von des Markts Gewimmel,", "tokens": ["Ein", "Buch", ",", "ver\u00b7wor\u00b7fen", "von", "des", "Markts", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil's jenen, die hier niedre Lust entz\u00fcndet,", "tokens": ["Weil's", "je\u00b7nen", ",", "die", "hier", "nied\u00b7re", "Lust", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "$,", "PRELS", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erstirbt die H\u00fclle, keinen Sternenhimmel,", "tokens": ["Er\u00b7stirbt", "die", "H\u00fcl\u00b7le", ",", "kei\u00b7nen", "Ster\u00b7nen\u00b7him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nein! lange Nacht zu tiefer Reu' verk\u00fcndet;", "tokens": ["Nein", "!", "lan\u00b7ge", "Nacht", "zu", "tie\u00b7fer", "Reu'", "ver\u00b7k\u00fcn\u00b7det", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Buch, drin eines schwachen Weibes Reden", "tokens": ["Ein", "Buch", ",", "drin", "ei\u00b7nes", "schwa\u00b7chen", "Wei\u00b7bes", "Re\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Starken Witz und weltverst\u00e4nd'ges Wesen,", "tokens": ["Der", "Star\u00b7ken", "Witz", "und", "welt\u00b7ver\u00b7st\u00e4n\u00b7d'\u00b7ges", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Babel, so sie bauen, drohn zu t\u00f6ten,", "tokens": ["Das", "Ba\u00b7bel", ",", "so", "sie", "bau\u00b7en", ",", "drohn", "zu", "t\u00f6\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und daher auch ihr Zorn, als sie's gelesen.", "tokens": ["Und", "da\u00b7her", "auch", "ihr", "Zorn", ",", "als", "sie's", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "PPOSAT", "NN", "$,", "KOUS", "PIS", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Das wag' ich ", "tokens": ["Das", "wag'", "ich"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Dir, dem schon l\u00e4ngst der \u00e4u\u00dfre Schein verschwunden,", "tokens": ["Dir", ",", "dem", "schon", "l\u00e4ngst", "der", "\u00e4u\u00df\u00b7re", "Schein", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir, der du hast im Innersten dagegen", "tokens": ["Dir", ",", "der", "du", "hast", "im", "In\u00b7ners\u00b7ten", "da\u00b7ge\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "VAFIN", "APPRART", "NN", "PAV"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Ein Morgenrot, das nie erl\u00f6scht, gefunden.", "tokens": ["Ein", "Mor\u00b7gen\u00b7rot", ",", "das", "nie", "er\u00b7l\u00f6scht", ",", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O nimm es in dein innres geist'ges Leben", "tokens": ["O", "nimm", "es", "in", "dein", "inn\u00b7res", "geist'\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit all den Schmerzen, Tr\u00e4nen, die's geboren,", "tokens": ["Mit", "all", "den", "Schmer\u00b7zen", ",", "Tr\u00e4\u00b7nen", ",", "die's", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$,", "NN", "$,", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die nicht versteht die Welt in ihrem Streben,", "tokens": ["Die", "nicht", "ver\u00b7steht", "die", "Welt", "in", "ih\u00b7rem", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "