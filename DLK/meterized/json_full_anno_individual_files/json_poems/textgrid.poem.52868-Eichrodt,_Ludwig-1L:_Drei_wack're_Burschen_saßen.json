{"textgrid.poem.52868": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drei wack're Burschen sa\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei wack're Burschen sa\u00dfen", "tokens": ["Drei", "wack'\u00b7re", "Bur\u00b7schen", "sa\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gar ungem\u00fcthlich da", "tokens": ["Gar", "un\u00b7ge\u00b7m\u00fcth\u00b7lich", "da"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf einem k\u00fchlen Rasen", "tokens": ["Auf", "ei\u00b7nem", "k\u00fch\u00b7len", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beim ", "tokens": ["Beim"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Da sch\u00fcttelt das Haupt der Eine", "tokens": ["Da", "sch\u00fct\u00b7telt", "das", "Haupt", "der", "Ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ART"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und hub zu singen an", "tokens": ["Und", "hub", "zu", "sin\u00b7gen", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKZU", "VVINF", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Lied von Heinrich Heine,", "tokens": ["Ein", "Lied", "von", "Hein\u00b7rich", "Hei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Jeder singen kann.", "tokens": ["Das", "Je\u00b7der", "sin\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\"ich wei\u00df nicht, was soll es bedeuten,", "tokens": ["\"", "ich", "wei\u00df", "nicht", ",", "was", "soll", "es", "be\u00b7deu\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da\u00df ich so traurig bin,", "tokens": ["Da\u00df", "ich", "so", "trau\u00b7rig", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Beafsteack aus alten Zeiten,", "tokens": ["Ein", "Be\u00b7afs\u00b7teack", "aus", "al\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Das kommt mir nicht aus dem Sinn.\"", "tokens": ["Das", "kommt", "mir", "nicht", "aus", "dem", "Sinn", ".", "\""], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Darauf versetzt der Zweite,", "tokens": ["Da\u00b7rauf", "ver\u00b7setzt", "der", "Zwei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch ich wei\u00df ein Gericht,", "tokens": ["Auch", "ich", "wei\u00df", "ein", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Auf dieses w\u00e4r ich heute", "tokens": ["Auf", "die\u00b7ses", "w\u00e4r", "ich", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Absonderlich erpicht.", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "er\u00b7picht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mich will es schier bed\u00fcnken", "tokens": ["Mich", "will", "es", "schier", "be\u00b7d\u00fcn\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als th\u00e4t uns Eines Noth,", "tokens": ["Als", "th\u00e4t", "uns", "Ei\u00b7nes", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist ein roher Schinken", "tokens": ["Das", "ist", "ein", "ro\u00b7her", "Schin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit sanftem Butterbrod.\"", "tokens": ["Mit", "sanf\u00b7tem", "But\u00b7ter\u00b7brod", ".", "\""], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da trafen sich die Gedanken", "tokens": ["Da", "tra\u00b7fen", "sich", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und alle stimmten bei,", "tokens": ["Und", "al\u00b7le", "stimm\u00b7ten", "bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Butterbrod ein Ranken", "tokens": ["Da\u00df", "But\u00b7ter\u00b7brod", "ein", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch nicht von Leder sei.", "tokens": ["Auch", "nicht", "von", "Le\u00b7der", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und mit der Zunge schnalzten", "tokens": ["Und", "mit", "der", "Zun\u00b7ge", "schnalz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie Alle Mann f\u00fcr Mann,", "tokens": ["Sie", "Al\u00b7le", "Mann", "f\u00fcr", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Worauf sie sich umhalsten", "tokens": ["Wo\u00b7rauf", "sie", "sich", "um\u00b7hals\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und huben zu singen an:", "tokens": ["Und", "hu\u00b7ben", "zu", "sin\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\"schinken, den ich meine,", "tokens": ["\"", "schin\u00b7ken", ",", "den", "ich", "mei\u00b7ne", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der den Hunger stillt,", "tokens": ["Der", "den", "Hun\u00b7ger", "stillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Komm, mit deinem Scheine", "tokens": ["Komm", ",", "mit", "dei\u00b7nem", "Schei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Rosenrothes Bild!\"", "tokens": ["Ro\u00b7sen\u00b7ro\u00b7thes", "Bild", "!", "\""], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Darauf begann der Dritte:", "tokens": ["Da\u00b7rauf", "be\u00b7gann", "der", "Drit\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch ich wei\u00df einen Fra\u00df,", "tokens": ["Auch", "ich", "wei\u00df", "ei\u00b7nen", "Fra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den ich mir nie verbitte", "tokens": ["Den", "ich", "mir", "nie", "ver\u00b7bit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu einem guten Glas:", "tokens": ["Zu", "ei\u00b7nem", "gu\u00b7ten", "Glas", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ich meine Schweinebraten,", "tokens": ["Ich", "mei\u00b7ne", "Schwei\u00b7ne\u00b7bra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der sich gewaschen hat,", "tokens": ["Der", "sich", "ge\u00b7wa\u00b7schen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich meine Schweinebraten,", "tokens": ["Ich", "mei\u00b7ne", "Schwei\u00b7ne\u00b7bra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit gr\u00fc\u00dfendem Salat.\"", "tokens": ["Mit", "gr\u00fc\u00b7\u00dfen\u00b7dem", "Sa\u00b7lat", ".", "\""], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da war man gut berathen", "tokens": ["Da", "war", "man", "gut", "be\u00b7ra\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Alle stimmten bei,", "tokens": ["Und", "Al\u00b7le", "stimm\u00b7ten", "bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df so ein Schweinebraten", "tokens": ["Da\u00df", "so", "ein", "Schwei\u00b7ne\u00b7bra\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch nicht von Leder sei.", "tokens": ["Auch", "nicht", "von", "Le\u00b7der", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da sprach der Erste wieder", "tokens": ["Da", "sprach", "der", "Ers\u00b7te", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hub zu singen an:", "tokens": ["Und", "hub", "zu", "sin\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\"ihr meine lieben Br\u00fcder", "tokens": ["\"", "ihr", "mei\u00b7ne", "lie\u00b7ben", "Br\u00fc\u00b7der"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ist kein leerer Wahn,", "tokens": ["Es", "ist", "kein", "lee\u00b7rer", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Vortrefflich ist und allen", "tokens": ["Vor\u00b7treff\u00b7lich", "ist", "und", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "KON", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verh\u00e4ltnissen gem\u00e4\u00df", "tokens": ["Ver\u00b7h\u00e4lt\u00b7nis\u00b7sen", "ge\u00b7m\u00e4\u00df"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "In diesen heiligen Hallen", "tokens": ["In", "die\u00b7sen", "hei\u00b7li\u00b7gen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein frischer Schweizerk\u00e4s!\"", "tokens": ["Ein", "fri\u00b7scher", "Schwei\u00b7zer\u00b7k\u00e4s", "!", "\""], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Der Zweite rief: \"o h\u00e4tt' ich", "tokens": ["Der", "Zwei\u00b7te", "rief", ":", "\"", "o", "h\u00e4tt'", "ich"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "FM", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu diesem schlechten Bier,", "tokens": ["Zu", "die\u00b7sem", "schlech\u00b7ten", "Bier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur einen edlen Rettig -", "tokens": ["Nur", "ei\u00b7nen", "ed\u00b7len", "Ret\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NE", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geholfen w\u00e4re mir!\"", "tokens": ["Ge\u00b7hol\u00b7fen", "w\u00e4\u00b7re", "mir", "!", "\""], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Der Dritte sprach nicht wieder;", "tokens": ["Der", "Drit\u00b7te", "sprach", "nicht", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu helfen aus der Noth,", "tokens": ["Zu", "hel\u00b7fen", "aus", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schritt er, statt aller Lieder,", "tokens": ["Schritt", "er", ",", "statt", "al\u00b7ler", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Zur That mit ", "tokens": ["Zur", "That", "mit"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.16": {"line.1": {"text": "Drei wack're Burschen sa\u00dfen", "tokens": ["Drei", "wack'\u00b7re", "Bur\u00b7schen", "sa\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gar ungem\u00fcthlich da", "tokens": ["Gar", "un\u00b7ge\u00b7m\u00fcth\u00b7lich", "da"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf einem k\u00fchlen Rasen", "tokens": ["Auf", "ei\u00b7nem", "k\u00fch\u00b7len", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beim ", "tokens": ["Beim"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}}, "stanza.17": {"line.1": {"text": "Da sch\u00fcttelt das Haupt der Eine", "tokens": ["Da", "sch\u00fct\u00b7telt", "das", "Haupt", "der", "Ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ART"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und hub zu singen an", "tokens": ["Und", "hub", "zu", "sin\u0