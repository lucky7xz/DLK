{"textgrid.poem.59972": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Zukunftslied", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcberm\u00fct'ge Triumphierer,", "tokens": ["\u00dc\u00b7ber\u00b7m\u00fct'\u00b7ge", "Tri\u00b7um\u00b7phie\u00b7rer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weh euch, wenn ihr's noch nicht f\u00fchlt,", "tokens": ["Weh", "euch", ",", "wenn", "ih\u00b7r's", "noch", "nicht", "f\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie der treffliche Minierer", "tokens": ["Wie", "der", "treff\u00b7li\u00b7che", "Mi\u00b7nie\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon den Boden unterw\u00fchlt,", "tokens": ["Schon", "den", "Bo\u00b7den", "un\u00b7ter\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ihr in der Geisterstunde", "tokens": ["Da\u00df", "ihr", "in", "der", "Geis\u00b7ter\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kl\u00e4ffend unser Ohr zerrei\u00dft! \u2013", "tokens": ["Kl\u00e4f\u00b7fend", "un\u00b7ser", "Ohr", "zer\u00b7rei\u00dft", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch wir wissen, ihr seid Hunde,", "tokens": ["Doch", "wir", "wis\u00b7sen", ",", "ihr", "seid", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ihr glaubt an keinen Geist.", "tokens": ["Und", "ihr", "glaubt", "an", "kei\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber kommen wird ein Pfingsten", "tokens": ["A\u00b7ber", "kom\u00b7men", "wird", "ein", "Pfings\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Donnernd \u00fcber euer Haupt", "tokens": ["Don\u00b7nernd", "\u00fc\u00b7ber", "eu\u00b7er", "Haupt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Festtag der Geringsten,", "tokens": ["Und", "ein", "Fest\u00b7tag", "der", "Ge\u00b7rings\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der des Hochmuts Stamm entlaubt.", "tokens": ["Der", "des", "Hoch\u00b7muts", "Stamm", "ent\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der sich lange selbst vergessen,", "tokens": ["Der", "sich", "lan\u00b7ge", "selbst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist am Ziel der Ungl\u00fccksbahn,", "tokens": ["Ist", "am", "Ziel", "der", "Un\u00b7gl\u00fccks\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Mensch, der sie durchmessen,", "tokens": ["Und", "der", "Mensch", ",", "der", "sie", "durch\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kommt beim Menschen endlich an.", "tokens": ["Kommt", "beim", "Men\u00b7schen", "end\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fort mit eurer Ahnenbilder", "tokens": ["Fort", "mit", "eu\u00b7rer", "Ah\u00b7nen\u00b7bil\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcbern\u00e4chtigem Gesicht!", "tokens": ["\u00dc\u00b7bern\u00b7\u00e4ch\u00b7ti\u00b7gem", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht und pflanzt in eure Schilder,", "tokens": ["Geht", "und", "pflanzt", "in", "eu\u00b7re", "Schil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ritter, ein Vergi\u00dfmeinnicht!", "tokens": ["Rit\u00b7ter", ",", "ein", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "F\u00fcr die ganze Welt den Adel!", "tokens": ["F\u00fcr", "die", "gan\u00b7ze", "Welt", "den", "A\u00b7del", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcr die Menschheit Brot und Wein!", "tokens": ["F\u00fcr", "die", "Menschheit", "Brot", "und", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Keine Steuern, keine Z\u00f6lle,", "tokens": ["Kei\u00b7ne", "Steu\u00b7ern", ",", "kei\u00b7ne", "Z\u00f6l\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des Gedankens Freiverkehr!", "tokens": ["Des", "Ge\u00b7dan\u00b7kens", "Frei\u00b7ver\u00b7kehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinen Teufel in der H\u00f6lle,", "tokens": ["Kei\u00b7nen", "Teu\u00b7fel", "in", "der", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keinen Gott im Himmel mehr!", "tokens": ["Kei\u00b7nen", "Gott", "im", "Him\u00b7mel", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nieder mit dem Blutpokale,", "tokens": ["Nie\u00b7der", "mit", "dem", "Blut\u00b7po\u00b7ka\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drin der Kirche Wahnwitz kreist!", "tokens": ["Drin", "der", "Kir\u00b7che", "Wahn\u00b7witz", "kreist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ein Kolumb zerbricht die Schale,", "tokens": ["Ein", "Ko\u00b7lumb", "zer\u00b7bricht", "die", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn er eine Welt beweist.", "tokens": ["Wenn", "er", "ei\u00b7ne", "Welt", "be\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Einmal noch uns aufzuraffen", "tokens": ["Ein\u00b7mal", "noch", "uns", "auf\u00b7zu\u00b7raf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu des Lebens Maienlust,", "tokens": ["Zu", "des", "Le\u00b7bens", "Mai\u00b7en\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rei\u00dfen wir das Schwert der Pfaffen", "tokens": ["Rei\u00b7\u00dfen", "wir", "das", "Schwert", "der", "Pfaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Menschheit wunder Brust!", "tokens": ["Aus", "der", "Menschheit", "wun\u00b7der", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zwischen J\u00e4gern und Gehetzten", "tokens": ["Zwi\u00b7schen", "J\u00e4\u00b7gern", "und", "Ge\u00b7hetz\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sei entbrannt die wilde Schlacht,", "tokens": ["Sei", "ent\u00b7brannt", "die", "wil\u00b7de", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis man Frieden auf dem letzten", "tokens": ["Bis", "man", "Frie\u00b7den", "auf", "dem", "letz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eingest\u00fcrzten Tempel macht.", "tokens": ["Ein\u00b7ge\u00b7st\u00fcrz\u00b7ten", "Tem\u00b7pel", "macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zittert, zittert, bl\u00f6de Toren,", "tokens": ["Zit\u00b7tert", ",", "zit\u00b7tert", ",", "bl\u00f6\u00b7de", "To\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor der Zukunft eh'rnem Tritt \u2013", "tokens": ["Vor", "der", "Zu\u00b7kunft", "eh'r\u00b7nem", "Tritt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, die Zeit ist neu geboren,", "tokens": ["Ja", ",", "die", "Zeit", "ist", "neu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja, und ohne Kaiserschnitt;", "tokens": ["Ja", ",", "und", "oh\u00b7ne", "Kai\u00b7ser\u00b7schnitt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erobert wird das Leben,", "tokens": ["Und", "er\u00b7o\u00b7bert", "wird", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wir jubeln gloria:", "tokens": ["Und", "wir", "ju\u00b7beln", "glo\u00b7ria", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Alle Schulden sind vergeben,", "tokens": ["Al\u00b7le", "Schul\u00b7den", "sind", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn kein Gl\u00e4ubiger ist da.", "tokens": ["Denn", "kein", "Gl\u00e4u\u00b7bi\u00b7ger", "ist", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Durch die Wolken seh ich's tagen,", "tokens": ["Durch", "die", "Wol\u00b7ken", "seh", "ich's", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Nebel, sie verwehn;", "tokens": ["Und", "die", "Ne\u00b7bel", ",", "sie", "ver\u00b7wehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem Pegasus am Wagen", "tokens": ["Mit", "dem", "Pe\u00b7ga\u00b7sus", "am", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df es endlich vorw\u00e4rtsgehn.", "tokens": ["Mu\u00df", "es", "end\u00b7lich", "vor\u00b7w\u00e4rts\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine Phalanx la\u00dft uns schlingen,", "tokens": ["Ei\u00b7ne", "Pha\u00b7lanx", "la\u00dft", "uns", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die kein Henker brechen kann,", "tokens": ["Die", "kein", "Hen\u00b7ker", "bre\u00b7chen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wie jener R\u00f6mer singen,", "tokens": ["Und", "wie", "je\u00b7ner", "R\u00f6\u00b7mer", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur: die Waffen und den Mann!", "tokens": ["Nur", ":", "die", "Waf\u00b7fen", "und", "den", "Mann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ungest\u00fcm in tausend Gliedern,", "tokens": ["Un\u00b7ge\u00b7st\u00fcm", "in", "tau\u00b7send", "Glie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausend Adern gl\u00fcht der Streit,", "tokens": ["Tau\u00b7send", "A\u00b7dern", "gl\u00fcht", "der", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Arsenal von Liedern", "tokens": ["Und", "ein", "Ar\u00b7se\u00b7nal", "von", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt in Deutschland kampfbereit.", "tokens": ["Liegt", "in", "Deutschland", "kampf\u00b7be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Denn wir wissen, die Erh\u00f6rung", "tokens": ["Denn", "wir", "wis\u00b7sen", ",", "die", "Er\u00b7h\u00f6\u00b7rung"], "tok