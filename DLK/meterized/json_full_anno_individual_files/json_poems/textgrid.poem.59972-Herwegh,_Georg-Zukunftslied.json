], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcbern\u00e4chtigem Gesicht!", "tokens": ["\u00dc\u00b7bern\u00b7\u00e4ch\u00b7ti\u00b7gem", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht und pflanzt in eure Schilder,", "tokens": ["Geht", "und", "pflanzt", "in", "eu\u00b7re", "Schil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ritter, ein Vergi\u00dfmeinnicht!", "tokens": ["Rit\u00b7ter", ",", "ein", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "F\u00fcr die ganze Welt den Adel!", "tokens": ["F\u00fcr", "die", "gan\u00b7ze", "Welt", "den", "A\u00b7del", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fcr die Menschheit Brot und Wein!", "tokens": ["F\u00fcr", "die", "Menschheit", "Brot", "und", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Keine Steuern, keine Z\u00f6lle,", "tokens": ["Kei\u00b7ne", "Steu\u00b7e