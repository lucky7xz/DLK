en", "tokens": ["Bis", "man", "Frie\u00b7den", "auf", "dem", "letz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eingest\u00fcrzten Tempel macht.", "tokens": ["Ein\u00b7ge\u00b7st\u00fcrz\u00b7ten", "Tem\u00b7pel", "macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Zittert, zittert, bl\u00f6de Toren,", "tokens": ["Zit\u00b7tert", ",", "zit\u00b7tert", ",", "bl\u00f6\u00b7de", "To\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor der Zukunft eh'rnem Tritt \u2013", "tokens": ["Vor", "der", "Zu\u00b7kunft", "eh'r\u00b7nem", "Tritt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, die Zeit ist neu geboren,", "tokens": ["Ja", ",", "die", "Zeit", "ist", "neu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja, und ohne Kaiserschnitt;", "tokens": ["Ja", ",", "und", "oh\u00b7ne", "Kai\u00b7ser\u00b7schnitt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erobert wird das Leben,", "tokens": ["Und", "er\u00b7o\u00b7bert", "wird", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], 