{"dta.poem.8364": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n mannigfaltige Weisheit GOt-  \n tes welche aus der heiligen  \n Schrift hervorleuchtet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es denkt der Sp\u00f6tter Zunfft mit ihren\nAlbern Wiz,", "tokens": ["Es", "denkt", "der", "Sp\u00f6t\u00b7ter", "Zunfft", "mit", "ih\u00b7ren", "Al\u00b7bern", "Wiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es sey in ", "tokens": ["Es", "sey", "in"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Sie schliest draus \u00fcbereilt das Men-", "tokens": ["Sie", "schliest", "draus", "\u00fc\u00b7be\u00b7reilt", "das", "Men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dieselbe nur erdacht, und vor sich aufgeschrie-", "tokens": ["Die\u00b7sel\u00b7be", "nur", "er\u00b7dacht", ",", "und", "vor", "sich", "auf\u00b7ge\u00b7schrie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "ADV", "VVPP", "$,", "KON", "APPR", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein wer dieses Buch ohn Vorurtheile liest,", "tokens": ["Al\u00b7lein", "wer", "die\u00b7ses", "Buch", "ohn", "Vor\u00b7urt\u00b7hei\u00b7le", "liest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PDAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den Hauptzwek drauf es geht, im Lesen nicht ver-", "tokens": ["Den", "Haupt\u00b7zwek", "drauf", "es", "geht", ",", "im", "Le\u00b7sen", "nicht", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "PPER", "VVFIN", "$,", "APPRART", "NN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der wird in jeden St\u00fck das nie ist zu ergr\u00fcn-", "tokens": ["Der", "wird", "in", "je\u00b7den", "St\u00fck", "das", "nie", "ist", "zu", "er\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN", "ART", "ADV", "VAFIN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Geschichte die uns sind in diesem Buch erz\u00e4hlt,", "tokens": ["Ge\u00b7schich\u00b7te", "die", "uns", "sind", "in", "die\u00b7sem", "Buch", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "VAFIN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die hat die Weisheit woll zu ihren Zwek er-", "tokens": ["Die", "hat", "die", "Weis\u00b7heit", "woll", "zu", "ih\u00b7ren", "Zwek", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sie lehren das was wir bei unsers Glaubens Wis-", "tokens": ["Sie", "leh\u00b7ren", "das", "was", "wir", "bei", "un\u00b7sers", "Glau\u00b7bens", "Wis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zur Lebensbesserung als n\u00f6thig fassen m\u00fcssen.", "tokens": ["Zur", "Le\u00b7bens\u00b7bes\u00b7se\u00b7rung", "als", "n\u00f6\u00b7thig", "fas\u00b7sen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man denkt was hilfft es uns, zu unserm Gl\u00fcklich", "tokens": ["Man", "denkt", "was", "hilfft", "es", "uns", ",", "zu", "un\u00b7serm", "Gl\u00fck\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIS", "VVFIN", "PPER", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df uns drin angezeigt so viel Geschlechter", "tokens": ["Da\u00df", "uns", "drin", "an\u00b7ge\u00b7zeigt", "so", "viel", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Was fragen wir darnach da\u00df wir die Nahmen le-", "tokens": ["Was", "fra\u00b7gen", "wir", "dar\u00b7nach", "da\u00df", "wir", "die", "Nah\u00b7men", "le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PAV", "KOUS", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Was in der Vorderwelt vor V\u00f6lker sind gewe-", "tokens": ["Was", "in", "der", "Vor\u00b7der\u00b7welt", "vor", "V\u00f6l\u00b7ker", "sind", "ge\u00b7we"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "APPR", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "O! blinder Unbedacht! wie n\u00fczlich braucht der", "tokens": ["O", "!", "blin\u00b7der", "Un\u00b7be\u00b7dacht", "!", "wie", "n\u00fcz\u00b7lich", "braucht", "der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADJA", "NN", "$.", "PWAV", "ADJD", "VVFIN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Zu wissen da\u00df das Heil aus Davids Samen", "tokens": ["Zu", "wis\u00b7sen", "da\u00df", "das", "Heil", "aus", "Da\u00b7vids", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KOUS", "ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der JEsus den uns ", "tokens": ["Der", "Je\u00b7sus", "den", "uns"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "ART", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.20": {"text": "Denn dieses Merkmahl hat uns ja die Schrifft er-", "tokens": ["Denn", "die\u00b7ses", "Merk\u00b7mahl", "hat", "uns", "ja", "die", "Schrifft", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "W\u00e4r kein Register da; so k\u00f6nte man nicht sehn,", "tokens": ["W\u00e4r", "kein", "Re\u00b7gis\u00b7ter", "da", ";", "so", "k\u00f6n\u00b7te", "man", "nicht", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PTKVZ", "$.", "ADV", "VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wie aus des Davids Stamm die Zweig hernach", "tokens": ["Wie", "aus", "des", "Da\u00b7vids", "Stamm", "die", "Zweig", "her\u00b7nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NE", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Woraus das Reis gebl\u00fcht dadurch gesegnet wer-", "tokens": ["Wo\u00b7raus", "das", "Reis", "ge\u00b7bl\u00fcht", "da\u00b7durch", "ge\u00b7seg\u00b7net", "wer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PAV", "VVPP", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Als durch ein einzig Heil, die V\u00f6lker dieser Er-", "tokens": ["Als", "durch", "ein", "ein\u00b7zig", "Heil", ",", "die", "V\u00f6l\u00b7ker", "die\u00b7ser", "Er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJD", "NN", "$,", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da die Geschlechter Roll nun in der heilgen", "tokens": ["Da", "die", "Ge\u00b7schlech\u00b7ter", "Roll", "nun", "in", "der", "heil\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NE", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "So sieht man wie genau bei JEsu das ein-", "tokens": ["So", "sieht", "man", "wie", "ge\u00b7nau", "bei", "Je\u00b7su", "das", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "ADJD", "APPR", "NE", "ART", "TRUNC"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Was von dem Stamm gesagt, den ", "tokens": ["Was", "von", "dem", "Stamm", "ge\u00b7sagt", ",", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVPP", "$,", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Daraus auch JEsus ist zur rechten Zeit geboh-", "tokens": ["Da\u00b7raus", "auch", "Je\u00b7sus", "ist", "zur", "rech\u00b7ten", "Zeit", "ge\u00b7boh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "NE", "VAFIN", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer die Geschichte merkt, und nach der Ursach", "tokens": ["Wer", "die", "Ge\u00b7schich\u00b7te", "merkt", ",", "und", "nach", "der", "Ur\u00b7sach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Warum der heilge Geist sie in der Schrifft vor-", "tokens": ["Wa\u00b7rum", "der", "heil\u00b7ge", "Geist", "sie", "in", "der", "Schrifft", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.31": {"text": "Der wird auch allemahl das weiseste Verbinden,", "tokens": ["Der", "wird", "auch", "al\u00b7le\u00b7mahl", "das", "wei\u00b7ses\u00b7te", "Ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mit unsrer Glaubens-Lehr, und Lebenspflichten", "tokens": ["Mit", "uns\u00b7rer", "Glau\u00b7bens\u00b7Lehr", ",", "und", "Le\u00b7bens\u00b7pflich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Was Moses uns erz\u00e4hlt, wie ", "tokens": ["Was", "Mo\u00b7ses", "uns", "er\u00b7z\u00e4hlt", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "NE", "PPER", "VVFIN", "$,", "PWAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Den Menschen als zum Schau auf diese B\u00fchn", "tokens": ["Den", "Men\u00b7schen", "als", "zum", "Schau", "auf", "die\u00b7se", "B\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "APPRART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Wie herlich er gemacht, wie er hernach gefallen;", "tokens": ["Wie", "her\u00b7lich", "er", "ge\u00b7macht", ",", "wie", "er", "her\u00b7nach", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVPP", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wie GOtt die Gl\u00e4ubigen als Pilgrim lies hier", "tokens": ["Wie", "Gott", "die", "Gl\u00e4u\u00b7bi\u00b7gen", "als", "Pil\u00b7grim", "lies", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "KOKOM", "NE", "VVFIN", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Dies zeiget uns zum Theil das Werk der Sch\u00f6p-", "tokens": ["Dies", "zei\u00b7get", "uns", "zum", "Theil", "das", "Werk", "der", "Sch\u00f6p"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.39": {"text": "Dies lehret ferner uns wie sein allm\u00e4chtig Wal-", "tokens": ["Dies", "leh\u00b7ret", "fer\u00b7ner", "uns", "wie", "sein", "all\u00b7m\u00e4ch\u00b7tig", "Wa\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "KOKOM", "PPOSAT", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die Menschen auf der Welt kan wunderbar er-", "tokens": ["Die", "Men\u00b7schen", "auf", "der", "Welt", "kan", "wun\u00b7der\u00b7bar", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VMFIN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Sind diese Lehren nicht als wichtig anzusehn?", "tokens": ["Sind", "die\u00b7se", "Leh\u00b7ren", "nicht", "als", "wich\u00b7tig", "an\u00b7zu\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PTKNEG", "KOUS", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und wenn wir weiter hin in seinen B\u00fcchern gehn,", "tokens": ["Und", "wenn", "wir", "wei\u00b7ter", "hin", "in", "sei\u00b7nen", "B\u00fc\u00b7chern", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wie er den Opferdienst durchs Geistes Trieb ab-", "tokens": ["Wie", "er", "den", "Op\u00b7fer\u00b7dienst", "durchs", "Geis\u00b7tes", "Trieb", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "NN", "TRUNC"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.44": {"text": "So sieht man da\u00df hieraus auch ", "tokens": ["So", "sieht", "man", "da\u00df", "hier\u00b7aus", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "KOUS", "PAV", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.45": {"text": "Wer diese Bilder Schul, wie sichs geb\u00fchrt er-", "tokens": ["Wer", "die\u00b7se", "Bil\u00b7der", "Schul", ",", "wie", "sichs", "ge\u00b7b\u00fchrt", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "NN", "$,", "PWAV", "PIS", "VVPP", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Wie des Erl\u00f6sers Amt darinnen vorgelegt;", "tokens": ["Wie", "des", "Er\u00b7l\u00f6\u00b7sers", "Amt", "da\u00b7rin\u00b7nen", "vor\u00b7ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und wie der Glaube dran im Schatten angese-", "tokens": ["Und", "wie", "der", "Glau\u00b7be", "dran", "im", "Schat\u00b7ten", "an\u00b7ge\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PAV", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Wie die Erl\u00f6sung m\u00fcst durch Christi Blut gesche-", "tokens": ["Wie", "die", "Er\u00b7l\u00f6\u00b7sung", "m\u00fcst", "durch", "Chris\u00b7ti", "Blut", "ge\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "