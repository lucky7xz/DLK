{"dta.poem.21854": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "X.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Rosill\u2019 ist zwar Filandern fest verbunden/", "tokens": ["Ro\u00b7sill'", "ist", "zwar", "Fi\u00b7lan\u00b7dern", "fest", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "doch g\u00f6nnt sie mir die ersten Freuden stun-", "tokens": ["doch", "g\u00f6nnt", "sie", "mir", "die", "ers\u00b7ten", "Freu\u00b7den", "stun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin es/ der die vollen Trauben brach/", "tokens": ["Ich", "bin", "es", "/", "der", "die", "vol\u00b7len", "Trau\u00b7ben", "brach", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$(", "ART", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "hernach/ so mag Filander lesen nach.", "tokens": ["her\u00b7nach", "/", "so", "mag", "Fi\u00b7lan\u00b7der", "le\u00b7sen", "nach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "VMFIN", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}