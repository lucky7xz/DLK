 ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.67": {"text": "Zwischen zweier Hirten Grab,", "tokens": ["Zwi\u00b7schen", "zwei\u00b7er", "Hir\u00b7ten", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.68": {"text": "Da\u00df man seiner ein Gedenken habe.", "tokens": ["Da\u00df", "man", "sei\u00b7ner", "ein", "Ge\u00b7den\u00b7ken", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.69": {"text": "Herr, bei dir allein ist Ruh',", "tokens": ["Herr", ",", "bei", "dir", "al\u00b7lein", "ist", "Ruh'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.70": {"text": "Wie die J\u00fcnger einst zu dir auf Erden", "tokens": ["Wie", "die", "J\u00fcn\u00b7ger", "einst", "zu", "dir", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.71": {"text": "Sagten, sprichst zum Pilger du:", "tokens": ["Sag\u00b7ten", ",", "sprichst", "zum", "Pil\u00b7ger", "du", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.72": {"text": "Bleib bei mir, denn es will Abend werden!", "tokens": ["Bleib", "bei", "mir", ",", "denn", "es", "will", "A\u00b7bend", "wer\u00b7den", "!"], "token_info": ["word", "word", "wor