{"textgrid.poem.46054": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[segen \u00fcber diesen Ort!]", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Segen \u00fcber diesen Ort!", "tokens": ["Se\u00b7gen", "\u00fc\u00b7ber", "die\u00b7sen", "Ort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo so treu der Weinberg wird gebauet,", "tokens": ["Wo", "so", "treu", "der", "Wein\u00b7berg", "wird", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gott vergelt's! sein letztes Wort,", "tokens": ["Gott", "ver\u00b7gelt's", "!", "sein", "letz\u00b7tes", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er nach dem Dom zur\u00fcck noch schauet,", "tokens": ["Wenn", "er", "nach", "dem", "Dom", "zu\u00b7r\u00fcck", "noch", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PTKVZ", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der im Morgenlicht entbrannt,", "tokens": ["Der", "im", "Mor\u00b7gen\u00b7licht", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein Altar voll fr\u00fcher Opferkerzen,", "tokens": ["Ein", "Al\u00b7tar", "voll", "fr\u00fc\u00b7her", "Op\u00b7fer\u00b7ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Mahnend oft gen\u00fcber stand", "tokens": ["Mah\u00b7nend", "oft", "ge\u00b7n\u00fc\u00b7ber", "stand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinem m\u00fcden Auge, lauen Herzen,", "tokens": ["Sei\u00b7nem", "m\u00fc\u00b7den", "Au\u00b7ge", ",", "lau\u00b7en", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Und, von Blumen fromm umkniet,", "tokens": ["Und", ",", "von", "Blu\u00b7men", "fromm", "um\u00b7kniet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo des Herren Bild am \u00d6lberg ringet,", "tokens": ["Wo", "des", "Her\u00b7ren", "Bild", "am", "\u00d6l\u00b7berg", "rin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Mit ihm sang das Morgenlied,", "tokens": ["Mit", "ihm", "sang", "das", "Mor\u00b7gen\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wenn der Bl\u00fctengarten Weihrauch schwinget.", "tokens": ["Wenn", "der", "Bl\u00fc\u00b7ten\u00b7gar\u00b7ten", "Weih\u00b7rauch", "schwin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Dom! der in Gewitternacht", "tokens": ["Dom", "!", "der", "in", "Ge\u00b7wit\u00b7ter\u00b7nacht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Bald der Arche gleicht umdroht von Wogen,", "tokens": ["Bald", "der", "Ar\u00b7che", "gleicht", "um\u00b7droht", "von", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Bald in stiller Opferpracht", "tokens": ["Bald", "in", "stil\u00b7ler", "Op\u00b7fer\u00b7pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Sich erhebt, umspannt vom Friedensbogen,", "tokens": ["Sich", "er\u00b7hebt", ",", "um\u00b7spannt", "vom", "Frie\u00b7dens\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Und, vom Sonnenblick gek\u00fc\u00dft", "tokens": ["Und", ",", "vom", "Son\u00b7nen\u00b7blick", "ge\u00b7k\u00fc\u00dft"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Und von wei\u00dfer Tauben Flug umschwebet,", "tokens": ["Und", "von", "wei\u00b7\u00dfer", "Tau\u00b7ben", "Flug", "um\u00b7schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Jetzt gedenket, jetzt vergi\u00dft,", "tokens": ["Jetzt", "ge\u00b7den\u00b7ket", ",", "jetzt", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Wie sich Schweres leicht zu heben strebet.", "tokens": ["Wie", "sich", "Schwe\u00b7res", "leicht", "zu", "he\u00b7ben", "stre\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Dom! der heut im jungen Tag", "tokens": ["Dom", "!", "der", "heut", "im", "jun\u00b7gen", "Tag"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "\u00dcber treuer Hirten Asche gl\u00fchte,", "tokens": ["\u00dc\u00b7ber", "treu\u00b7er", "Hir\u00b7ten", "A\u00b7sche", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Als der Pilger scheidend sprach:", "tokens": ["Als", "der", "Pil\u00b7ger", "schei\u00b7dend", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Nachbar Dom! leb wohl, Gott dich beh\u00fcte!", "tokens": ["Nach\u00b7bar", "Dom", "!", "leb", "wohl", ",", "Gott", "dich", "be\u00b7h\u00fc\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "VVFIN", "ADV", "$,", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Sorgt ihr Freundesherzen nicht,", "tokens": ["Sorgt", "ihr", "Freun\u00b7des\u00b7her\u00b7zen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Wo ein m\u00fcdes Haupt sich niederlege,", "tokens": ["Wo", "ein", "m\u00fc\u00b7des", "Haupt", "sich", "nie\u00b7der\u00b7le\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "Be\u00dfre Herberg giebt es nicht,", "tokens": ["Be\u00df\u00b7re", "Her\u00b7berg", "giebt", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Als beim Kreuze drau\u00dfen an dem Wege.", "tokens": ["Als", "beim", "Kreu\u00b7ze", "drau\u00b7\u00dfen", "an", "dem", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Gastfrei ist der Herbergsmann,", "tokens": ["Gast\u00b7frei", "ist", "der", "Her\u00b7bergs\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Denn der Dorn, sein Knecht, h\u00e4lt an den Pfaden", "tokens": ["Denn", "der", "Dorn", ",", "sein", "Knecht", ",", "h\u00e4lt", "an", "den", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "Gern des Pilgers Mantel an,", "tokens": ["Gern", "des", "Pil\u00b7gers", "Man\u00b7tel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Dringend ihn zur Einkehr einzuladen;", "tokens": ["Drin\u00b7gend", "ihn", "zur", "Ein\u00b7kehr", "ein\u00b7zu\u00b7la\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Und der Wirt ist auch nicht stolz,", "tokens": ["Und", "der", "Wirt", "ist", "auch", "nicht", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.34": {"text": "Drau\u00dfen, ganz von Wunden \u00fcberhagelt,", "tokens": ["Drau\u00b7\u00dfen", ",", "ganz", "von", "Wun\u00b7den", "\u00fc\u00b7berh\u00b7a\u00b7gelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.35": {"text": "Harrt er, am gekreuzten Holz", "tokens": ["Harrt", "er", ",", "am", "ge\u00b7kreuz\u00b7ten", "Holz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Fest mit H\u00e4nd' und F\u00fc\u00dfen angenagelt.", "tokens": ["Fest", "mit", "H\u00e4nd'", "und", "F\u00fc\u00b7\u00dfen", "an\u00b7ge\u00b7na\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Welcher Wirt hat so Geduld?", "tokens": ["Wel\u00b7cher", "Wirt", "hat", "so", "Ge\u00b7duld", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Wi\u00dft, er ist allein in diesen Qualen,", "tokens": ["Wi\u00dft", ",", "er", "ist", "al\u00b7lein", "in", "die\u00b7sen", "Qua\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.39": {"text": "Aller armen G\u00e4ste Schuld,", "tokens": ["Al\u00b7ler", "ar\u00b7men", "G\u00e4s\u00b7te", "Schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Die zum Kreuze eingehn, zu bezahlen;", "tokens": ["Die", "zum", "Kreu\u00b7ze", "ein\u00b7gehn", ",", "zu", "be\u00b7zah\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.41": {"text": "Und sein Haupt, gekr\u00f6nt mit Dorn,", "tokens": ["Und", "sein", "Haupt", ",", "ge\u00b7kr\u00f6nt", "mit", "Dorn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Gr\u00fc\u00dft den Gast, der bei ihm \u00fcbernachtet,", "tokens": ["Gr\u00fc\u00dft", "den", "Gast", ",", "der", "bei", "ihm", "\u00fc\u00b7bern\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.43": {"text": "Und sein Herz, ein blut'ger Born,", "tokens": ["Und", "sein", "Herz", ",", "ein", "blut'\u00b7ger", "Born", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Labt den Gast, der d\u00fcrstend nach ihm schmachtet,", "tokens": ["Labt", "den", "Gast", ",", "der", "d\u00fcrs\u00b7tend", "nach", "ihm", "schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "Und sein Leib ist selbst das Haus,", "tokens": ["Und", "sein", "Leib", "ist", "selbst", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Und die Pforten sind f\u00fcnf offne Wunden,", "tokens": ["Und", "die", "Pfor\u00b7ten", "sind", "f\u00fcnf", "off\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "CARD", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.47": {"text": "Keinen treibt er je hinaus,", "tokens": ["Kei\u00b7nen", "treibt", "er", "je", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "PPER", "ADV", "APZR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Der da einmal Ruhe hat gefunden,", "tokens": ["Der", "da", "ein\u00b7mal", "Ru\u00b7he", "hat", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.49": {"text": "Und sein Blut ist selbst der Trank,", "tokens": ["Und", "sein", "Blut", "ist", "selbst", "der", "Trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Und sein Fleisch ist wahrlich selbst die Speise,", "tokens": ["Und", "sein", "Fleisch", "ist", "wahr\u00b7lich", "selbst", "die", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.51": {"text": "Da wird alles heil, was krank,", "tokens": ["Da", "wird", "al\u00b7les", "heil", ",", "was", "krank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "Da ist Brot und Wein zur letzten Reise.", "tokens": ["Da", "ist", "Brot", "und", "Wein", "zur", "letz\u00b7ten", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.53": {"text": "Hier ist Willkomm jeder Gast,", "tokens": ["Hier", "ist", "Will\u00b7komm", "je\u00b7der", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Wer m\u00fchselig ist und schwer beladen,", "tokens": ["Wer", "m\u00fch\u00b7se\u00b7lig", "ist", "und", "schwer", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.55": {"text": "Findet Raum f\u00fcr seine Last,", "tokens": ["Fin\u00b7det", "Raum", "f\u00fcr", "sei\u00b7ne", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Er hat alle selbst ja eingeladen.", "tokens": ["Er", "hat", "al\u00b7le", "selbst", "ja", "ein\u00b7ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.57": {"text": "Wahrlich er allein ist gut!", "tokens": ["Wahr\u00b7lich", "er", "al\u00b7lein", "ist", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Er, der keinen Stein gehabt auf Erden,", "tokens": ["Er", ",", "der", "kei\u00b7nen", "Stein", "ge\u00b7habt", "auf", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PIAT", "NN", "VAPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.59": {"text": "Wo sein m\u00fcdes Haupt geruht,", "tokens": ["Wo", "sein", "m\u00fc\u00b7des", "Haupt", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.60": {"text": "Wollte allen Haus und