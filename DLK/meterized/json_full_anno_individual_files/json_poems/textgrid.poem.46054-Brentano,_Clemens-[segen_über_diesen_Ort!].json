", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Sorgt ihr Freundesherzen nicht,", "tokens": ["Sorgt", "ihr", "Freun\u00b7des\u00b7her\u00b7zen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Wo ein m\u00fcdes Haupt sich niederlege,", "tokens": ["Wo", "ein", "m\u00fc\u00b7des", "Haupt", "sich", "nie\u00b7der\u00b7le\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "Be\u00dfre Herberg giebt es nicht,", "tokens": ["Be\u00df\u00b7re", "Her\u00b7berg", "giebt", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Als beim Kreuze drau\u00dfen an dem Wege.", "tokens": ["Als", "beim", "Kreu\u00b7ze", "drau\u00b7\u00dfen", "an", "dem", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Gastfrei ist der Herbergsmann,", "tokens": ["Gast\u00b7frei", "ist", "der", "Her\u00b7bergs\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Denn der Dorn, sein Knecht, h\u00e4lt an den Pfaden", "tokens": ["Denn", "der", "Dorn", ",", "sein", "Knecht", ",", "h\u00e4lt", "an", "den", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "Gern des Pilgers Mantel an,", "tokens": ["Gern", "des", "Pil\u00b7gers", "Man\u00b7tel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Dringend ihn zur Einkehr einzuladen;", "tokens": ["Drin\u00b7gend", "ihn", "zur", "Ein\u00b7kehr", "ein\u00b7zu\u00b7la\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Und der Wirt ist auch nicht stolz,", "tokens": ["Und", "der", "Wirt", "ist", "auch", "nicht", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.34": {"text": "Drau\u00dfen, ganz von Wunden \u00fcberhagelt,", "tokens": ["Drau\u00b7\u00dfen", ",", "ganz", "von", "Wun\u00b7den", "\u00fc\u00b7berh\u00b7a\u00b7gelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.35": {"text": "Harrt er, am gekreuzten Holz", "tokens": ["Harrt", "er", ",", "am", "ge\u00b7kreuz\u00b7ten", "Holz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Fest mit H\u00e4nd' und F\u00fc\u00dfen angenagelt.", "tokens": ["Fest", "mit", "H\u00e4nd'", "und", "F\u00fc\u00b7\u00dfen", "an\u00b7ge\u00b7na\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Welcher Wirt hat so Geduld?", "tokens": ["Wel\u00b7cher", "Wirt", "hat", "so", "Ge\u00b7duld", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Wi\u00dft, er ist allein in diesen Qualen,", "tokens": ["Wi\u00dft", ",", "er", "ist", "al\u00b7lein", "in", "die\u00b7sen", "Qua\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.39": {"text": "Aller armen G\u00e4ste Schuld,", "tokens": ["Al\u00b7ler", "ar\u00b7men", "G\u00e4s\u00b7te", "Schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Die zum Kreuze eingehn, zu bezahlen;", "tokens": ["Die", "zum", "Kreu\u00b7ze", "ein\u00b7gehn", ",", "zu", "be\u00b7zah\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.41": {"text": "Und sein Haupt, gekr\u00f6nt mit Dorn,", "tokens": ["Und", "sein", "Haupt", ",", "ge\u00b7kr\u00f6nt", "mit", "Dorn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Gr\u00fc\u00dft den Gast, der bei ihm \u00fcbernachtet,", "tokens": ["Gr\u00fc\u00dft", "den", "Gast", ",", "der", "bei", "ihm", "\u00fc\u00b7bern\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.43": {"text": "Und sein Herz, ein blut'ger Born,", "tokens": ["Und", "sein", "Herz", ",", "ein", "blut'\u00b7ger", "Born", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Labt den Gast, der d\u00fcrstend nach ihm schmachtet,", "tokens": ["Labt", "den", "Gast", ",", "der", "d\u00fcrs\u00b7tend", "nach", "ihm", "schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "Und sein Leib ist selbst das Haus,", "tokens": ["Und", "sein", "Leib", "ist", "selbst", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Und die Pforten sind f\u00fcnf offne Wunden,", "tokens": ["Und", "die", "Pfor\u00b7ten", "sind", "f\u00fcnf", "off\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "CARD", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.47": {"text": "Keinen treibt er je hinaus,", "tokens": ["Kei\u00b7nen", "treibt", "er", "je", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "PPER", "ADV", "APZR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Der da einmal Ruhe hat gefunden,", "tokens": ["Der", "da", "ein\u00b7mal", "Ru\u00b7he", "hat", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.49": {"text": "Und sein Blut ist selbst der Trank,", "tokens": ["Und", "sein", "Blut", "ist", "selbst", "der", "Trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Und sein Fleisch ist wahrlich selbst die Speise,", "tokens": ["Und", "sein", "Fleisch", "ist", "wahr\u00b7lich", "selbst", "die", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.51": {"text": "Da wird alles heil, was krank,", "tokens": ["Da", "wird", "al\u00b7les", "heil", ",", "was", "krank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "Da ist Brot und Wein zur letzten Reise.", "tokens": ["Da", "ist", "Brot", "und", "Wein", "zur", "letz\u00b7ten", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.53": {"text": "Hier ist Willkomm jeder Gast,", "tokens": ["Hier", "ist", "Will\u00b7komm", "je\u00b7der", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Wer m\u00fchselig ist und schwer beladen,", "tokens": ["Wer", "m\u00fch\u00b7se\