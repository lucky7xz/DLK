{"textgrid.poem.52531": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Vorgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abschied ist genommen,", "tokens": ["Der", "Ab\u00b7schied", "ist", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und T\u00fccher wehen noch,", "tokens": ["Und", "T\u00fc\u00b7cher", "we\u00b7hen", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und keine Thr\u00e4nen kommen,", "tokens": ["Und", "kei\u00b7ne", "Thr\u00e4\u00b7nen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O sagt, was ist es doch?", "tokens": ["O", "sagt", ",", "was", "ist", "es", "doch", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir sparen unsre Thr\u00e4nen", "tokens": ["Wir", "spa\u00b7ren", "uns\u00b7re", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf fr\u00f6hlich Wiedersehn,", "tokens": ["Auf", "fr\u00f6h\u00b7lich", "Wie\u00b7der\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Freude, kann sie weinen,", "tokens":