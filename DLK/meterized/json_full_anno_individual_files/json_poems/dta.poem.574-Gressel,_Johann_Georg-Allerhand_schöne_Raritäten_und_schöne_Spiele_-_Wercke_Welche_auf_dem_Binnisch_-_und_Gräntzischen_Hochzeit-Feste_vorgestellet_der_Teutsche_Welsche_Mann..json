{"dta.poem.574": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Allerhand sch\u00f6ne Rarit\u00e4ten und sch\u00f6ne  \n Spiele - Wercke/  \n Welche auf dem  \n Binnisch - und Gr\u00e4ntzischen  \n Hochzeit-Feste vorgestellet  \n der Teutsche Welsche Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Seht hier einmahl/ das ist die Kammer und das Bett", "tokens": ["Seht", "hier", "ein\u00b7mahl", "/", "das", "ist", "die", "Kam\u00b7mer", "und", "das", "Bett"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "$(", "PDS", "VAFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worinnen ", "tokens": ["Wo\u00b7rin\u00b7nen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dort sitzet ", "tokens": ["Dort", "sit\u00b7zet"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und speiset eine Milch von ", "tokens": ["Und", "spei\u00b7set", "ei\u00b7ne", "Milch", "von"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hier steht ", "tokens": ["Hier", "steht"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Sch\u00f6ne Rarit\u00e4t/ sch\u00f6ne Spiele-Werckerc.", "tokens