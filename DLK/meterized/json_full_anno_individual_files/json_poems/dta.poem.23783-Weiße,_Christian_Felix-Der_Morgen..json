{"dta.poem.23783": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Morgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Willkommen sch\u00f6ner Morgen!", "tokens": ["Will\u00b7kom\u00b7men", "sch\u00f6\u00b7ner", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie gro\u00df ist deine Pracht!", "tokens": ["Wie", "gro\u00df", "ist", "dei\u00b7ne", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie bliebe mir verborgen,", "tokens": ["Sie", "blie\u00b7be", "mir", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "W\u00e4r ich nicht fr\u00fch erwacht:", "tokens": ["W\u00e4r", "ich", "nicht", "fr\u00fch", "er\u00b7wacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lust, Wunder und Entz\u00fccken", "tokens": ["Lust", ",", "Wun\u00b7der", "und", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Begegnen meinen Blicken,", "tokens": ["Be\u00b7geg\u00b7nen", "mei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wohin ich immer seh,", "tokens": ["Wo\u00b7hin", "ich", "im\u00b7mer", "seh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Jm Thal und auf der H\u00f6h.", "tokens": ["Jm", "Thal", "und", "auf", "der", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es gl\u00fchn der Berge Spitzen", "tokens": ["Es", "gl\u00fchn", "der", "Ber\u00b7ge", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von g\u00fcldnem Sonnenstrahl;", "tokens": ["Von", "g\u00fcld\u00b7nem", "Son\u00b7nen\u00b7strahl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Diamanten blitzen", "tokens": ["Von", "Di\u00b7a\u00b7man\u00b7ten", "blit\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Pfl\u00e4nzgen \u00fcberall.", "tokens": ["Die", "Pfl\u00e4nz\u00b7gen", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Luft und auf der Weide", "tokens": ["In", "Luft", "und", "auf", "der", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ert\u00f6nt das Lied der Freude,", "tokens": ["Er\u00b7t\u00f6nt", "das", "Lied", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und weckt in s\u00fc\u00dfem Schall", "tokens": ["Und", "weckt", "in", "s\u00fc\u00b7\u00dfem", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Den dankbarn Wiederhall.", "tokens": ["Den", "dank\u00b7barn", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jhr wi\u00dft nicht, reiche Prasser,", "tokens": ["Ihr", "wi\u00dft", "nicht", ",", "rei\u00b7che", "Pras\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ihr f\u00fcr Gl\u00fcck verschlaft?", "tokens": ["Was", "ihr", "f\u00fcr", "Gl\u00fcck", "ver\u00b7schlaft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seyd eure eignen Hasser,", "tokens": ["Seyd", "eu\u00b7re", "eig\u00b7nen", "Has\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und durch euch selbst bestraft!", "tokens": ["Und", "durch", "euch", "selbst", "be\u00b7straft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verschlaft die sch\u00f6nsten Stunden,", "tokens": ["Ver\u00b7schlaft", "die", "sch\u00f6ns\u00b7ten", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nie sey von euch empfunden,", "tokens": ["Nie", "sey", "von", "euch", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was diese sch\u00f6ne Welt", "tokens": ["Was", "die\u00b7se", "sch\u00f6\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr Wunder in sich h\u00e4lt.", "tokens": ["F\u00fcr", "Wun\u00b7der", "in", "sich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich will es aber f\u00fchlen. &#8212;", "tokens": ["Ich", "will", "es", "a\u00b7ber", "f\u00fch\u00b7len", ".", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "punct", "XML_entity"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Indem die Weste mir", "tokens": ["In\u00b7dem", "die", "Wes\u00b7te", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Locken lieblich spielen,", "tokens": ["In", "Lo\u00b7cken", "lieb\u00b7lich", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sitz und betracht ich hier.", "tokens": ["Sitz", "und", "be\u00b7tracht", "ich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Gott! ist mein irrdisch Leben", "tokens": ["Gott", "!", "ist", "mein", "irr\u00b7disch", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VAFIN", "PPOSAT", "ADJD", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Mit so viel Gl\u00fcck umgeben,", "tokens": ["Mit", "so", "viel", "Gl\u00fcck", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Was wird der Wohnplatz seyn,", "tokens": ["Was", "wird", "der", "Wohn\u00b7platz", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der uns dort soll erfreun!", "tokens": ["Der", "uns", "dort", "soll", "er\u00b7freun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}