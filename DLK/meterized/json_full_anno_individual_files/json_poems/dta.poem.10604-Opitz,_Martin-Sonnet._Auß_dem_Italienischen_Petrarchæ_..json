{"dta.poem.10604": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n  Au\u00df dem Italienischen   Petrarch\u00e6 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ist Liebe lauter nichts/ wie da\u00df sie mich entz\u00fcndet?", "tokens": ["Ist", "Lie\u00b7be", "lau\u00b7ter", "nichts", "/", "wie", "da\u00df", "sie", "mich", "ent\u00b7z\u00fcn\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PIAT", "PIS", "$(", "KOKOM", "KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist sie dann gleichwol was/ wem ist jhr thun bewust?", "tokens": ["Ist", "sie", "dann", "gleich\u00b7wol", "was", "/", "wem", "ist", "jhr", "thun", "be\u00b7wust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PWS", "$(", "PWS", "VAFIN", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ist sie auch recht vnd gut/ wie bringt sie b\u00f6se Lust?", "tokens": ["Ist", "sie", "auch", "recht", "vnd", "gut", "/", "wie", "bringt", "sie", "b\u00f6\u00b7se", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$(", "PWAV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist sie nicht gut/ wie da\u00df man Freudt au\u00df jhr empfindet?", "tokens": ["Ist", "sie", "nicht", "gut", "/", "wie", "da\u00df", "man", "Freudt", "au\u00df", "jhr", "emp\u00b7fin\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "$(", "KOKOM", "KOUS", "PIS", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Lieb ich gar williglich/ wie da\u00df ich Schmertzen trage?", "tokens": ["Lieb", "ich", "gar", "wil\u00b7lig\u00b7lich", "/", "wie", "da\u00df", "ich", "Schmert\u00b7zen", "tra\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "$(", "KOKOM", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.6": {"text": "Mu\u00df ich es thun/ was hilffts/ da\u00df ich solch trawren f\u00fchr?", "tokens": ["Mu\u00df", "ich", "es", "thun", "/", "was", "hilffts", "/", "da\u00df", "ich", "solch", "traw\u00b7ren", "f\u00fchr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$(", "PWS", "VVFIN", "$(", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Thue ichs nicht gern/ wer ists/ der es befihlet mir?", "tokens": ["Thue", "ichs", "nicht", "gern", "/", "wer", "ists", "/", "der", "es", "be\u00b7fih\u00b7let", "mir", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "ADV", "$(", "PWS", "VAFIN", "$(", "PRELS", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Thue ich es gern/ warumb/ da\u00df ich mich dann beklage?", "tokens": ["Thue", "ich", "es", "gern", "/", "wa\u00b7rumb", "/", "da\u00df", "ich", "mich", "dann", "be\u00b7kla\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$(", "PWAV", "$(", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich wancke/ wie das Gras/ so von den k\u00fchlen Winden", "tokens": ["Ich", "wan\u00b7cke", "/", "wie", "das", "Gras", "/", "so", "von", "den", "k\u00fch\u00b7len", "Win\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "ART", "NN", "$(", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vmb Vesperzeit bald hin geneiget wirdt/ bald her.", "tokens": ["Vmb", "Ves\u00b7per\u00b7zeit", "bald", "hin", "ge\u00b7nei\u00b7get", "wirdt", "/", "bald", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$(", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich walle wie ein Schiff/ da\u00df in dem wilden Meer", "tokens": ["Ich", "wal\u00b7le", "wie", "ein", "Schiff", "/", "da\u00df", "in", "dem", "wil\u00b7den", "Meer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$(", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von Wellen vmbgejagt nicht kan zu rande finden.", "tokens": ["Von", "Wel\u00b7len", "vmb\u00b7ge\u00b7jagt", "nicht", "kan", "zu", "ran\u00b7de", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKNEG", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich wei\u00df nicht was ich will/ ich will nicht was ich wei\u00df/", "tokens": ["Ich", "wei\u00df", "nicht", "was", "ich", "will", "/", "ich", "will", "nicht", "was", "ich", "wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VMFIN", "$(", "PPER", "VMFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Im Sommer ist mir kalt/ im Winter ist mir hei\u00df.", "tokens": ["Im", "Som\u00b7mer", "ist", "mir", "kalt", "/", "im", "Win\u00b7ter", "ist", "mir", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "$(", "APPRART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}