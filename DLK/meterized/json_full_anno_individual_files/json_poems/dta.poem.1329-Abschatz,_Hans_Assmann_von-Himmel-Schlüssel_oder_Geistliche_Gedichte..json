{"dta.poem.1329": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "An fremdem Orte schlie\u00df ich dieses alte Jahr/", "tokens": ["An", "frem\u00b7dem", "Or\u00b7te", "schlie\u00df", "ich", "die\u00b7ses", "al\u00b7te", "Jahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herr JEsu/ nimm mein auch zu Hause gn\u00e4dig wahr/", "tokens": ["Herr", "Je\u00b7su", "/", "nimm", "mein", "auch", "zu", "Hau\u00b7se", "gn\u00e4\u00b7dig", "wahr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "VVIMP", "PPOSAT", "ADV", "APPR", "NN", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Beh\u00fctte/ was mir lieb/ f\u00fcr Schaden und Gefahr/", "tokens": ["Be\u00b7h\u00fct\u00b7te", "/", "was", "mir", "lieb", "/", "f\u00fcr", "Scha\u00b7den", "und", "Ge\u00b7fahr", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "PPER", "ADJD", "$(", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hohl mich/ wenn du wilt/ zu deiner Engel-Schaar!", "tokens": ["Und", "hohl", "mich", "/", "wenn", "du", "wilt", "/", "zu", "dei\u00b7ner", "En\u00b7gel\u00b7Schaar", "!"], "token_info": ["word", "word", "word", "punct"