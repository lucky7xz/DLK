{"textgrid.poem.40863": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Entscheidung", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Nordland gibt's, da d\u00e4mmert fahl Zwielicht mondenlang,", "tokens": ["Ein", "Nord\u00b7land", "gibt's", ",", "da", "d\u00e4m\u00b7mert", "fahl", "Zwie\u00b7licht", "mon\u00b7den\u00b7lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "F\u00fcr eine Nacht zu helle, f\u00fcr Tag zu d\u00fcster und bang,", "tokens": ["F\u00fcr", "ei\u00b7ne", "Nacht", "zu", "hel\u00b7le", ",", "f\u00fcr", "Tag", "zu", "d\u00fcs\u00b7ter", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "$,", "APPR", "NN", "PTKA", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und dennoch ist's all' Beides! So auch mit diesem Krieg,", "tokens": ["Und", "den\u00b7noch", "ist's", "all'", "Bei\u00b7des", "!", "So", "auch", "mit", "die\u00b7sem", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "$.", "ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Geschlagen beide Heere, und keines hat den Sieg.", "tokens": ["Ge\u00b7schla\u00b7gen", "bei\u00b7de", "Hee\u00b7re", ",", "und", "kei\u00b7nes", "hat", "den", "Sieg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "KON", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wollte jeden Gefallenen man legen in einen Sarg,", "tokens": ["Und", "woll\u00b7te", "je\u00b7den", "Ge\u00b7fal\u00b7le\u00b7nen", "man", "le\u00b7gen", "in", "ei\u00b7nen", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "NN", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "W\u00fcrd' im Ardennerwalde f\u00fcrwahr das Holz zu karg;", "tokens": ["W\u00fcrd'", "im", "Ar\u00b7den\u00b7ner\u00b7wal\u00b7de", "f\u00fcr\u00b7wahr", "das", "Holz", "zu", "karg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die Thr\u00e4nen, die da flossen, wohl geben einen See,", "tokens": ["Die", "Thr\u00e4\u00b7nen", ",", "die", "da", "flos\u00b7sen", ",", "wohl", "ge\u00b7ben", "ei\u00b7nen", "See", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "ADV", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "In seine Fluthen tauchte trostloses Liebesweh.", "tokens": ["In", "sei\u00b7ne", "Flut\u00b7hen", "tauch\u00b7te", "trost\u00b7lo\u00b7ses", "Lie\u00b7bes\u00b7weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Seht ihr die blanken Mauern, drauf sauset Blitz auf Blitz?", "tokens": ["Seht", "ihr", "die", "blan\u00b7ken", "Mau\u00b7ern", ",", "drauf", "sau\u00b7set", "Blitz", "auf", "Blitz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PAV", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das sind Terouanne's W\u00e4lle und Maxens Donnergesch\u00fctz.", "tokens": ["Das", "sind", "Terouan\u00b7ne's", "W\u00e4l\u00b7le", "und", "Ma\u00b7xens", "Don\u00b7ner\u00b7ge\u00b7sch\u00fctz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "NN", "KON", "NE", "NE", "$."], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was flirrt in blauer Ferne, wie Waffenglanz erregt?", "tokens": ["Was", "flirrt", "in", "blau\u00b7er", "Fer\u00b7ne", ",", "wie", "Waf\u00b7fen\u00b7glanz", "er\u00b7regt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "$,", "PWAV", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das sind des Ludwig Schaaren von Crevecoeur bewegt.", "tokens": ["Das", "sind", "des", "Lud\u00b7wig", "Schaa\u00b7ren", "von", "Cre\u00b7ve\u00b7co\u00b7eur", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NE", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Es dehnt sich eine Ebne, wie ein See so weit und glatt,", "tokens": ["Es", "dehnt", "sich", "ei\u00b7ne", "Eb\u00b7ne", ",", "wie", "ein", "See", "so", "weit", "und", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Von Terouanne's W\u00e4llen bis gegen Guinegat',", "tokens": ["Von", "Terouan\u00b7ne's", "W\u00e4l\u00b7len", "bis", "ge\u00b7gen", "Gu\u00b7i\u00b7ne\u00b7gat'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "APPR", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da reitet Max tiefsinnend, seine Auge schweift ringsum:", "tokens": ["Da", "rei\u00b7tet", "Max", "tief\u00b7sin\u00b7nend", ",", "sei\u00b7ne", "Au\u00b7ge", "schweift", "ring\u00b7sum", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "\u00bbein herrlich Feld zum Kampfe, weit g'nug f\u00fcr Schmach und Ruhm!", "tokens": ["\u00bb", "ein", "herr\u00b7lich", "Feld", "zum", "Kamp\u00b7fe", ",", "weit", "g'\u00b7nug", "f\u00fcr", "Schmach", "und", "Ruhm", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJD", "NN", "APPRART", "NN", "$,", "ADJD", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Fast d\u00fcnk' ein Todtengr\u00e4ber ich mir zu dieser Frist;", "tokens": ["Fast", "d\u00fcnk'", "ein", "Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", "ich", "mir", "zu", "die\u00b7ser", "Frist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Denn vor dem Kampf der Feldherr, wenn er das Schlachtfeld mi\u00dft,", "tokens": ["Denn", "vor", "dem", "Kampf", "der", "Feld\u00b7herr", ",", "wenn", "er", "das", "Schlacht\u00b7feld", "mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Und jener, wenn er schaufelt, sie denken alle zwei:", "tokens": ["Und", "je\u00b7ner", ",", "wenn", "er", "schau\u00b7felt", ",", "sie", "den\u00b7ken", "al\u00b7le", "zwei", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PIAT", "CARD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mu\u00df sehn, ob Raum zur Gn\u00fcge f\u00fcr meine Todten sei!", "tokens": ["Mu\u00df", "sehn", ",", "ob", "Raum", "zur", "Gn\u00fc\u00b7ge", "f\u00fcr", "mei\u00b7ne", "Tod\u00b7ten", "sei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "NN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch seht dort Frankreichs Banner sich ferne gl\u00e4nzend regen,", "tokens": ["Doch", "seht", "dort", "Fran\u00b7kreichs", "Ban\u00b7ner", "sich", "fer\u00b7ne", "gl\u00e4n\u00b7zend", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NE", "NN", "PRF", "ADV", "ADJD", "ADJA", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf, la\u00dft zum Aufbruch blasen, und rasch dem Feind entgegen!", "tokens": ["Auf", ",", "la\u00dft", "zum", "Auf\u00b7bruch", "bla\u00b7sen", ",", "und", "rasch", "dem", "Feind", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "APPRART", "NN", "VVINF", "$,", "KON", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was sitzen wir vor den W\u00e4llen, verpuppt in tr\u00e4ger Schanz',", "tokens": ["Was", "sit\u00b7zen", "wir", "vor", "den", "W\u00e4l\u00b7len", ",", "ver\u00b7puppt", "in", "tr\u00e4\u00b7ger", "Schanz'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Inde\u00df dort frohe Boten uns nahn mit dem Ehrenkranz?\u00ab", "tokens": ["In\u00b7de\u00df", "dort", "fro\u00b7he", "Bo\u00b7ten", "uns", "nahn", "mit", "dem", "Eh\u00b7ren\u00b7kranz", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "PPER", "ADJA", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Max sinkt auf seine Kniee, das ganze Heer ihm nach,", "tokens": ["Max", "sinkt", "auf", "sei\u00b7ne", "Kni\u00b7ee", ",", "das", "gan\u00b7ze", "Heer", "ihm", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf tausend Panzern goldig der Sonne Gluth sich brach,", "tokens": ["Auf", "tau\u00b7send", "Pan\u00b7zern", "gol\u00b7dig", "der", "Son\u00b7ne", "Gluth", "sich", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie'n Strahl des Glaubens, der aufw\u00e4rts aus Menschenherzen f\u00e4hrt,", "tokens": ["Wie'n", "Strahl", "des", "Glau\u00b7bens", ",", "der", "auf\u00b7w\u00e4rts", "aus", "Men\u00b7schen\u00b7her\u00b7zen", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie 'n Strahl der Gnade, der nieder aus Gottes Aug' sich kehrt.", "tokens": ["Wie", "'", "n", "Strahl", "der", "Gna\u00b7de", ",", "der", "nie\u00b7der", "aus", "Got\u00b7tes", "Aug", "'", "sich", "kehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$(", "XY", "NN", "ART", "NN", "$,", "PRELS", "PTKVZ", "APPR", "NN", "NN", "$(", "PRF", "VVFIN", "$."], "meter": "--+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Drommetengeschmetter und Feldruf! Drauf Heer an Heeresmacht!", "tokens": ["Drom\u00b7me\u00b7ten\u00b7ge\u00b7schmet\u00b7ter", "und", "Feld\u00b7ruf", "!", "Drauf", "Heer", "an", "Hee\u00b7res\u00b7macht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "PAV", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Zusammenprallt's, wie st\u00fcrzend sich Berg an Berg zerkracht,", "tokens": ["Zu\u00b7sam\u00b7men\u00b7prallt's", ",", "wie", "st\u00fcr\u00b7zend", "sich", "Berg", "an", "Berg", "zer\u00b7kracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "PRF", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der blasse Tod rief Vivat! und in den L\u00fcften sang", "tokens": ["Der", "blas\u00b7se", "Tod", "rief", "Vi\u00b7vat", "!", "und", "in", "den", "L\u00fcf\u00b7ten", "sang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$.", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein Chor von schwarzen Raben: Viel M\u00fch', sch\u00f6n Dank, sch\u00f6n Dank!", "tokens": ["Ein", "Chor", "von", "schwar\u00b7zen", "Ra\u00b7ben", ":", "Viel", "M\u00fch'", ",", "sch\u00f6n", "Dank", ",", "sch\u00f6n", "Dank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$.", "PIAT", "NN", "$,", "ADJD", "NN", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Hier fliegender Kugeln Sausen, dort donnernder M\u00f6rser Gedr\u00f6hn,", "tokens": ["Hier", "flie\u00b7gen\u00b7der", "Ku\u00b7geln", "Sau\u00b7sen", ",", "dort", "don\u00b7nern\u00b7der", "M\u00f6r\u00b7ser", "Ge\u00b7dr\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hier trunkner Sieger Jubel, dort sterbender Krieger Gest\u00f6hn,", "tokens": ["Hier", "trunk\u00b7ner", "Sie\u00b7ger", "Ju\u00b7bel", ",", "dort", "ster\u00b7ben\u00b7der", "Krie\u00b7ger", "Ge\u00b7st\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$,", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Z\u00e4hnknirschen dort und Fluchen, hier brechender Lippen Gebet, \u2013", "tokens": ["Z\u00e4hn\u00b7knir\u00b7schen", "dort", "und", "Flu\u00b7chen", ",", "hier", "bre\u00b7chen\u00b7der", "Lip\u00b7pen", "Ge\u00b7bet", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "KON", "NN", "$,", "ADV", "ADJA", "NN", "NN", "$,", "$("], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dort Crevecoeur: vorw\u00e4rts Memmen! hier Max: steht, Br\u00fcder, steht!", "tokens": ["Dort", "Cre\u00b7ve\u00b7co\u00b7eur", ":", "vor\u00b7w\u00e4rts", "Mem\u00b7men", "!", "hier", "Max", ":", "steht", ",", "Br\u00fc\u00b7der", ",", "steht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$.", "ADV", "NN", "$.", "ADV", "NE", "$.", "VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.10": {"line.1": {"text": "Dann ward es wieder stiller, nur Schwerter h\u00f6rt man m\u00e4hn,", "tokens": ["Dann", "ward", "es", "wie\u00b7der", "stil\u00b7ler", ",", "nur", "Schwer\u00b7ter", "h\u00f6rt", "man", "m\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Rings Staubgew\u00f6lk, und Niemand kann, ", "tokens": ["Rings", "Staub\u00b7ge\u00b7w\u00f6lk", ",", "und", "Nie\u00b7mand", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Windsto\u00df nur zeigt Einem die Leichen, die er geballt,", "tokens": ["Ein", "Wind\u00b7sto\u00df", "nur", "zeigt", "Ei\u00b7nem", "die", "Lei\u00b7chen", ",", "die", "er", "ge\u00b7ballt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PIS", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch auch dem Feind des Siegers, wo dessen Herzblut wallt.", "tokens": ["Doch", "auch", "dem", "Feind", "des", "Sie\u00b7gers", ",", "wo", "des\u00b7sen", "Herz\u00b7blut", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "$,", "PWAV", "PRELAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Ha, dr\u00fcben wankt's und taumelt's, gel\u00f6st sind Frankreichs Reihn,", "tokens": ["Ha", ",", "dr\u00fc\u00b7ben", "wankt's", "und", "tau\u00b7melt's", ",", "ge\u00b7l\u00f6st", "sind", "Fran\u00b7kreichs", "Reihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "KON", "VVFIN", "$,", "VVPP", "VAFIN", "NE", "NE", "$,"], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schon ruft der Deutsche jauchzend: da