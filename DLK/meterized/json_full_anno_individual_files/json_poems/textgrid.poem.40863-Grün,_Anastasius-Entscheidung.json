.", "tokens": ["Das", "sind", "des", "Lud\u00b7wig", "Schaa\u00b7ren", "von", "Cre\u00b7ve\u00b7co\u00b7eur", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NE", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.17": {"line.1": {"text": "Es dehnt sich eine Ebne, wie ein See so weit und glatt,", "tokens": ["Es", "dehnt", "sich", "ei\u00b7ne", "Eb\u00b7ne", ",", "wie", "ein", "See", "so", "weit", "und", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Von Terouanne's W\u00e4llen bis gegen Guinegat',", "tokens": ["Von", "Terouan\u00b7ne's", "W\u00e4l\u00b7len", "bis", "ge\u00b7gen", "Gu\u00b7i\u00b7ne\u00b7gat'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "APPR", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da reitet Max tiefsinnend, seine Auge schweift ringsum:", "tokens": ["Da", "rei\u00b7tet", "Max", "tief\u00b7sin\u00b7nend", ",", "sei\u00b7ne", "Au\u00b7ge", "schweift", "ring\u00b7sum", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "\u00bbein herrlich Feld zum Kampfe, weit g'nug f\u00fcr Schmach und Ruhm!", "tokens": ["\u00bb", "ein", "herr\u00b7lich", "Feld", "zum", "Kamp\u00b7fe", ",", "weit", "g'\u00b7nug", "f\u00fcr", "Schmach", "und", "Ruhm", "!"], "token_info": ["punct", "word", "word", "word", "word"