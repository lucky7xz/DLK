, "NN", "$,", "ADJD", "NN", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.22": {"line.1": {"text": "Hier fliegender Kugeln Sausen, dort donnernder M\u00f6rser Gedr\u00f6hn,", "tokens": ["Hier", "flie\u00b7gen\u00b7der", "Ku\u00b7geln", "Sau\u00b7sen", ",", "dort", "don\u00b7nern\u00b7der", "M\u00f6r\u00b7ser", "Ge\u00b7dr\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hier trunkner Sieger Jubel, dort sterbender Krieger Gest\u00f6hn,", "tokens": ["Hier", "trunk\u00b7ner", "Sie\u00b7ger", "Ju\u00b7bel", ",", "dort", "ster\u00b7ben\u00b7der", "Krie\u00b7ger", "Ge\u00b7st\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$,", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Z\u00e4hnknirschen dort und Fluchen, hier brechender Lippen Gebet, \u2013", "tokens": ["Z\u00e4hn\u00b7knir\u00b7schen", "dort", "und", "Flu\u00b7chen", ",", "hier", "bre\u00b7chen\u00b7der", "Lip\u00b7pen", "Ge\u00b7bet", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "KON", "NN", "$,", "ADV", "ADJA", "NN", "NN", "$,", "$("], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dort Crevecoeur: vorw\u00e4rts Memmen! hier Max: steht, Br\u00fcder, steht!", "tokens": ["Dort", "Cre\u00b7ve\u00b7co\u00b7eur", ":", "vor\u00b7w\u00e4rts", "Mem\u00b7men", "!", "hier", "Max", ":", "steht", ",", "Br\u00fc\u00b7der", ",", "steht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$.", "ADV", "NN", "$.", "ADV", "NE", "$.", "VVFIN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.23": {"line.1": {"text": "Dann ward es wieder stiller, nur Schwerter h\u00f6rt man m\u00e4hn,", "tokens": ["Dann", "ward", "es", "wie\u00b7der", "stil\u00b7ler", ",", "nur", "Schwer\u00b7ter", "h\u00f6rt", "man", "m\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Rings Staubgew\u00f6lk, und Niemand kann, ", "tokens": ["Rings", "Staub\u00b7ge\u00b7w\u00f6lk", ",", "und", "Nie\u00b7mand", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Windsto\u00df nur zeigt Einem die Leichen, die er geballt,", "tokens": ["Ein", "Wind\u00b7sto\u00df", "nur", "zeigt", "Ei\u00b7nem", "die", "Lei\u00b7chen", ",", "die", "er", "ge\u00b7ballt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PIS", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch auch dem Feind des Siegers, wo dessen Herzblut wallt.", "tokens": ["Doch", "auch", "dem", "Feind", "des", "Sie\u00b7gers", ",", "wo", "des\u00b7sen", "Herz\u00b7blut", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "$,", "PWAV", "PRELAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.24": {"line.1": {"text": "Ha, dr\u00fcben wankt's und taumelt's, gel\u00f6st sind Frankreichs Reihn,", "tokens": ["Ha", ",", "dr\u00fc\u00b7ben", "wankt's", "und", "tau\u00b7melt's", ",", "ge\u00b7l\u00f6st", "sind", "Fran\u00b7kreichs", "Reihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "KON", "VVFIN", "$,", "VVPP", "VAFIN", "NE", "NE", "$,"], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Schon ruft der Deutsche jauchzend: das Schlachtgefild ist mein!", "tokens": ["Schon", "ruft", "der", "Deut\u00b7sche", "jauch\u00b7zend", ":", "das", "Schlacht\u00b7ge\u00b7fild", "ist", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$.", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hui, Frankenmacht ist zerstoben, zersprengt die m\u00e4cht'ge Schaar,", "tokens": ["Hui", ",", "Fran\u00b7ken\u00b7macht", "ist", "zer\u00b7sto\u00b7ben", ",", "zer\u00b7sprengt", "die", "m\u00e4cht'\u00b7ge", "Schaar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "$,", "NN", "VAFIN", "VVPP", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und mit der gepfl\u00fcckten Lilie steigt auf zur Sonne der Aar!", "tokens": ["Und", "mit", "der", "ge\u00b7pfl\u00fcck\u00b7ten", "Li\u00b7lie", "steigt", "auf", "zur", "Son\u00b7ne", "der", "Aar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.25": {"line.1": {"text": "Doch Max denkt, als er Abends durchs w\u00fcste Schlachtfeld reitet:", "tokens": ["Doch", "Max", "denkt", ",", "als", "er", "A\u00b7bends", "durchs", "w\u00fcs\u00b7te", "Schlacht\u00b7feld", "rei\u00b7tet", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+---+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00bbist nicht die Schlacht ein Wetter, das t\u00f6dtend vor\u00fcberschreitet?", "tokens": ["\u00bb", "ist", "nicht", "die", "Schlacht", "ein", "Wet\u00b7ter", ",", "das", "t\u00f6d\u00b7tend", "vor\u00b7\u00fc\u00b7bersc\u00b7hrei\u00b7tet", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zwei Wolken prallen zusammen, Blitz zuckt und Donner schnaubt,", "tokens": ["Zwei", "Wol\u00b7ken", "pral\u00b7len", "zu\u00b7sam\u00b7men", ",", "Blitz", "zuckt", "und", "Don\u00b7ner", "schnaubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PTKVZ", "$,", "NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drauf rasselt Hagel nieder, das Feld steht saatberaubt.", "tokens": ["Drauf", "ras\u00b7selt", "Ha\u00b7gel", "nie\u00b7der", ",", "das", "Feld", "steht", "saat\u00b7be\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+--