, "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "NN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.19": {"line.1": {"text": "Doch seht dort Frankreichs Banner sich ferne gl\u00e4nzend regen,", "tokens": ["Doch", "seht", "dort", "Fran\u00b7kreichs", "Ban\u00b7ner", "sich", "fer\u00b7ne", "gl\u00e4n\u00b7zend", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NE", "NN", "PRF", "ADV", "ADJD", "ADJA", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf, la\u00dft zum Aufbruch blasen, und rasch dem Feind entgegen!", "tokens": ["Auf", ",", "la\u00dft", "zum", "Auf\u00b7bruch", "bla\u00b7sen", ",", "und", "rasch", "dem", "Feind", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "APPRART", "NN", "VVINF", "$,", "KON", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was sitzen wir vor den W\u00e4llen, verpuppt in tr\u00e4ger Schanz',", "tokens": ["Was", "sit\u00b7zen", "wir", "vor", "den", "W\u00e4l\u00b7len", ",", "ver\u00b7puppt", "in", "tr\u00e4\u00b7ger", "Schanz'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Inde\u00df dort frohe Boten uns nahn mit dem Ehrenkranz?\u00ab", "tokens": ["In\u00b7de\u00df", "dort", "fro\u00b7he", "Bo\u00b7ten", "uns", "nahn", "mit", "dem", "Eh\u00b7ren\u00b7kranz", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "PPER", "ADJA", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.20": {"line.1": {"text": "Max sinkt auf seine Kniee, das ganze Heer ihm nach,", "tokens": ["Max", "sinkt", "auf", "sei\u00b7ne", "Kni\u00b7ee", ",", "das", "gan\u00b7ze", "Heer", "ihm", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf tausend Panzern goldig der Sonne Gluth sich brach,", "tokens": ["Auf", "tau\u00b7send", "Pan\u00b7zern", "gol\u00b7dig", "der", "Son\u00b7ne", "Gluth", "sich", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie'n Strahl des Glaubens, der aufw\u00e4rts aus Menschenherzen f\u00e4hrt,", "tokens": ["Wie'n", "Strahl", "des", "Glau\u00b7bens", ",", "der", "auf\u00b7w\u00e4rts", "aus", "Men\u00b7schen\u00b7her\u00b7zen", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie 'n Strahl der Gnade, der nieder aus Gottes Aug' sich kehrt.", "tokens": ["Wie", "'", "n", "Strahl", "der", "Gna\u00b7de", ",", "der", "nie\u00b7der", "aus", "Got\u00b7tes", "Aug", "'", "sich", "kehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", 