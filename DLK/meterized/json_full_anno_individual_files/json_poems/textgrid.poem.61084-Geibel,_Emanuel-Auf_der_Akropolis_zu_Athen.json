{"textgrid.poem.61084": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Akropolis zu Athen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei euch, ihr hohen S\u00e4ulen, la\u00dft mich weilen,", "tokens": ["Bei", "euch", ",", "ihr", "ho\u00b7hen", "S\u00e4u\u00b7len", ",", "la\u00dft", "mich", "wei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr stummen Zeugen wechselvoller Tage,", "tokens": ["Ihr", "stum\u00b7men", "Zeu\u00b7gen", "wech\u00b7sel\u00b7vol\u00b7ler", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und la\u00dft sich mein Gem\u00fct ergehn in Klage,", "tokens": ["Und", "la\u00dft", "sich", "mein", "Ge\u00b7m\u00fct", "er\u00b7gehn", "in", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df nichts entrinnen mag des Schicksals Pfeilen.", "tokens": ["Da\u00df", "nichts", "ent\u00b7rin\u00b7nen", "mag", "des", "Schick\u00b7sals", "Pfei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Zeit des Glanzes saht ihr schnell enteilen,", "tokens": ["Die", "Zeit", "des",