{"textgrid.poem.24286": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Theater-Reformer", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch ein L\u00e4rmen! Welch ein Schrein! \u2013:", "tokens": ["Welch", "ein", "L\u00e4r\u00b7men", "!", "Welch", "ein", "Schrein", "!", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIAT", "ART", "NN", "$.", "PIAT", "ART", "NN", "$.", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gebt uns Schl\u00e4uche! Neue Schl\u00e4uche!", "tokens": ["Gebt", "uns", "Schl\u00e4u\u00b7che", "!", "Neu\u00b7e", "Schl\u00e4u\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seh ich hin, sinds arme G\u00e4uche,", "tokens": ["Seh", "ich", "hin", ",", "sinds", "ar\u00b7me", "G\u00e4u\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie haben keinen Wein.", "tokens": ["Und", "sie", "ha\u00b7ben", "kei\u00b7nen", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Pfui! rufen sie, Guckkasten! Welch ein Stall da!", "tokens": ["Pfui", "!", "ru\u00b7fen", "sie", ",", "Guck\u00b7kas\u00b7ten", "!", "Welch", "ein", "Stall", "da", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "$,", "NN", "$.", "PIAT", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Doch \u00fcber diesem Stalle leuchtete der Stern", "tokens": ["\u2013", "Doch", "\u00fc\u00b7ber", "die\u00b7sem", "Stal\u00b7le", "leuch\u00b7te\u00b7te", "der", "Stern"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "APPR", "PDAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu mancher g\u00f6ttlichen Geburt. \u2013 O, werte Herrn,", "tokens": ["Zu", "man\u00b7cher", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7burt", ".", "\u2013", "O", ",", "wer\u00b7te", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$.", "$(", "NE", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bedenkt den Spruch: Hic Rhodus est, hic salta!", "tokens": ["Be\u00b7denkt", "den", "Spruch", ":", "Hic", "Rho\u00b7dus", "est", ",", "hic", "sal\u00b7ta", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "NE", "NE", "VVFIN", "$,", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Das ist gewi\u00df: Es wird ein sch\u00f6nes Haus,", "tokens": ["Das", "ist", "ge\u00b7wi\u00df", ":", "Es", "wird", "ein", "sch\u00f6\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sieht neu, geschmackvoll, festlich, heiter aus,", "tokens": ["Sieht", "neu", ",", "ge\u00b7schmack\u00b7voll", ",", "fest\u00b7lich", ",", "hei\u00b7ter", "aus", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, was der wohlerfahrene Architekt", "tokens": ["Und", ",", "was", "der", "woh\u00b7ler\u00b7fah\u00b7re\u00b7ne", "Ar\u00b7chi\u00b7tekt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Erstrebt, es wird erreicht, nicht blo\u00df bezweckt:", "tokens": ["Er\u00b7strebt", ",", "es", "wird", "er\u00b7reicht", ",", "nicht", "blo\u00df", "be\u00b7zweckt", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$,", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Man kann von allen Sitzen alles h\u00f6rn und sehn.", "tokens": ["Man", "kann", "von", "al\u00b7len", "Sit\u00b7zen", "al\u00b7les", "h\u00f6rn", "und", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PIAT", "NN", "PIS", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In dies Theater werd ich gerne gehn,", "tokens": ["In", "dies", "The\u00b7a\u00b7ter", "werd", "ich", "ger\u00b7ne", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sofern das H\u00f6rn und Sehn sich auch verlohnt", "tokens": ["So\u00b7fern", "das", "H\u00f6rn", "