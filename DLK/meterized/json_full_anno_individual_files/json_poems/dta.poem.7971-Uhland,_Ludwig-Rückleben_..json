{"dta.poem.7971": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckleben .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "An Ihrem Grabe kniet\u2019 ich, festgebunden,", "tokens": ["An", "Ih\u00b7rem", "Gra\u00b7be", "kniet'", "ich", ",", "fest\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und senkte tief den Geist in\u2019s Todtenreich.", "tokens": ["Und", "senk\u00b7te", "tief", "den", "Geist", "in's", "Tod\u00b7ten\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Himmel reichte nicht mein Blick, es stunden", "tokens": ["Zum", "Him\u00b7mel", "reich\u00b7te", "nicht", "mein", "Blick", ",", "es", "stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Wiedersehens Bilder fern und bleich.", "tokens": ["Des", "Wie\u00b7der\u00b7se\u00b7hens", "Bil\u00b7der", "fern", "und", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da so ich vorw\u00e4rts Grauen nur gefunden,", "tokens": ["Da", "so", "ich", "vor\u00b7w\u00e4rts", "Grau\u00b7en", "nur", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vergangne Tage, fl\u00fcchtet\u2019 ich zu euch;", "tokens": ["Ver\u00b7gang\u00b7ne", "Ta\u00b7ge", ",", "fl\u00fcch\u00b7tet'", "ich", "zu", "euch", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich lie\u00df den Sarg des Grabes Nacht entheben,", "tokens": ["Ich", "lie\u00df", "den", "Sarg", "des", "Gra\u00b7bes", "Nacht", "ent\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zur\u00fcck Sie tragen in das sch\u00f6ne Leben.", "tokens": ["Zu\u00b7r\u00fcck", "Sie", "tra\u00b7gen", "in", "das", "sch\u00f6\u00b7ne", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schon huben sich die bleichen Augenlieder,", "tokens": ["Schon", "hu\u00b7ben", "sich", "die", "blei\u00b7chen", "Au\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Auge schmachtete zu mir empor;", "tokens": ["Ihr", "Au\u00b7ge", "schmach\u00b7te\u00b7te", "zu", "mir", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald strebten auf die frischverj\u00fcngten Glieder,", "tokens": ["Bald", "streb\u00b7ten", "auf", "die", "frischver\u00b7j\u00fcng\u00b7ten", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie schwebte bl\u00fchend in der Schwestern Chor;", "tokens": ["Sie", "schweb\u00b7te", "bl\u00fc\u00b7hend", "in", "der", "Schwes\u00b7tern", "Chor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Liebe goldne Stunden traten wieder,", "tokens": ["Der", "Lie\u00b7be", "gold\u00b7ne", "Stun\u00b7den", "tra\u00b7ten", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Selbst mit des ersten Kusses Lust, hervor:", "tokens": ["Selbst", "mit", "des", "ers\u00b7ten", "Kus\u00b7ses", "Lust", ",", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bis sich verlor Ihr Leben und das meine", "tokens": ["Bis", "sich", "ver\u00b7lor", "Ihr", "Le\u00b7ben", "und", "das", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "VVFIN", "PPOSAT", "NN", "KON", "PDS", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In sel\u2019ger Kindheit Duft und Morgenscheine.", "tokens": ["In", "sel'\u00b7ger", "Kind\u00b7heit", "Duft", "und", "Mor\u00b7gen\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}