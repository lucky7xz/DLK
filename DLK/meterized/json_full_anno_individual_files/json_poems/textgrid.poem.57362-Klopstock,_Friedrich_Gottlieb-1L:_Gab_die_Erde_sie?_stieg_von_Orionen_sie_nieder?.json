{"textgrid.poem.57362": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gab die Erde sie? stieg von Orionen sie nieder?", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gab die Erde sie? stieg von Orionen sie nieder?", "tokens": ["Gab", "die", "Er\u00b7de", "sie", "?", "stieg", "von", "O\u00b7rio\u00b7nen", "sie", "nie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$.", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sie von der sch\u00f6nen Seele beseelt,", "tokens": ["Sie", "von", "der", "sch\u00f6\u00b7nen", "See\u00b7le", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sie, des Gesetzes Mutter, das weiser ist, zu der Wohlfahrt", "tokens": ["Sie", ",", "des", "Ge\u00b7set\u00b7zes", "Mut\u00b7ter", ",", "das", "wei\u00b7ser", "ist", ",", "zu", "der", "Wohl\u00b7fahrt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "NN", "$,", "ART", "ADJA", "VAFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+--+-++-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Stimmender, menschlicher ist!", "tokens": ["Stim\u00b7men\u00b7der", ",", "menschli\u00b7cher", "ist", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ungeweihte h\u00f6ren wich auch; drum sprech' ich der G\u00f6ttin", "tokens": ["Un\u00b7ge\u00b7weih\u00b7te", "h\u00f6\u00b7ren", "wich", "auch", ";", "drum", "sprech'", "ich", "der", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "VVFIN", "ADV", "$.", "PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Namen nicht aus.", "tokens": ["Na\u00b7men", "nicht", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ungeweihter ist keiner, als wer von dem Morde den Wahn w\u00e4hnt,", "tokens": ["Un\u00b7ge\u00b7weih\u00b7ter", "ist", "kei\u00b7ner", ",", "als", "wer", "von", "dem", "Mor\u00b7de", "den", "Wahn", "w\u00e4hnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "$,", "KOUS", "PWS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Ihr zu opfern, zuck' er das Schwert.", "tokens": ["Ihr", "zu", "op\u00b7fern", ",", "zuck'", "er", "das", "Schwert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Opferer ist er am Fuss Tisiphona's; auf dieser Altare", "tokens": ["Op\u00b7fe\u00b7rer", "ist", "er", "am", "Fuss", "Ti\u00b7si\u00b7pho\u00b7na's", ";", "auf", "die\u00b7ser", "Al\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPRART", "NN", "NE", "$.", "APPR", "PDAT", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Raucht das vergossene Blut.", "tokens": ["Raucht", "das", "ver\u00b7gos\u00b7se\u00b7ne", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Gl\u00fccklich war, gl\u00fcckselig das Volk, von erhebender Freuden", "tokens": ["Gl\u00fcck\u00b7lich", "war", ",", "gl\u00fcck\u00b7se\u00b7lig", "das", "Volk", ",", "von", "er\u00b7he\u00b7ben\u00b7der", "Freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "ADJD", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Neuem Gef\u00fchl", "tokens": ["Neu\u00b7em", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Trunken, war benedeit, war selig, zu dem des Gesetzes", "tokens": ["Trun\u00b7ken", ",", "war", "be\u00b7ne\u00b7deit", ",", "war", "se\u00b7lig", ",", "zu", "dem", "des", "Ge\u00b7set\u00b7zes"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "$,", "APPR", "ART", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Mutter von den Unsterblichen kam.", "tokens": ["Mut\u00b7ter", "von", "den", "U\u00b7nsterb\u00b7li\u00b7chen", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}, "line.15": {"text": "Aber sie hatte ihn kaum geboren, selber gehuldigt", "tokens": ["A\u00b7ber", "sie", "hat\u00b7te", "ihn", "kaum", "ge\u00b7bo\u00b7ren", ",", "sel\u00b7ber", "ge\u00b7hul\u00b7digt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "ADV", "VVPP"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Ihrem lieblichen, fr\u00f6hlichen Sohn;", "tokens": ["Ih\u00b7rem", "lieb\u00b7li\u00b7chen", ",", "fr\u00f6h\u00b7li\u00b7chen", "Sohn", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Da entfloh die Wonne, versanken der Gl\u00fccklichen Inseln", "tokens": ["Da", "ent\u00b7floh", "die", "Won\u00b7ne", ",", "ver\u00b7san\u00b7ken", "der", "Gl\u00fcck\u00b7li\u00b7chen", "In\u00b7seln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "In die Tiefe das Meers!", "tokens": ["In", "die", "Tie\u00b7fe", "das", "Meers", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Da entstand ... Gern nent' ichs (den Elendstiftern am liebsten!)", "tokens": ["Da", "ent\u00b7stand", "...", "Gern", "nent'", "ichs", "(", "den", "E\u00b7lend\u00b7stif\u00b7tern", "am", "liebs\u00b7ten", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$(", "NN", "VVFIN", "PIS", "$(", "ART", "NN", "PTKA", "ADJD", "$.", "$("], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Doch der Sprache fehlet das Wort", "tokens": ["Doch", "der", "Spra\u00b7che", "feh\u00b7let", "das", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "F\u00fcr dies Scheussliche. Ha! es beschloss zu verwandeln die G\u00f6ttin:", "tokens": ["F\u00fcr", "dies", "Scheuss\u00b7li\u00b7che", ".", "Ha", "!", "es", "be\u00b7schloss", "zu", "ver\u00b7wan\u00b7deln", "die", "G\u00f6t\u00b7tin", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "$.", "ITJ", "$.", "PPER", "ADJD", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Und die Verwandlung gelang.", "tokens": ["Und", "die", "Ver\u00b7wand\u00b7lung", "ge\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.23": {"text": "Zwillingsh\u00f6hlen dampfen auf einem Erobererschlachtfeld,", "tokens": ["Zwil\u00b7lings\u00b7h\u00f6h\u00b7len", "damp\u00b7fen", "auf", "ei\u00b7nem", "Er\u00b7o\u00b7be\u00b7rer\u00b7schlacht\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Werden bewohnt,", "tokens": ["Wer\u00b7den", "be\u00b7wohnt", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "Die von der Raubsucht, die von der wilderen Wilden, der Herrschsucht.", "tokens": ["Die", "von", "der", "Raub\u00b7sucht", ",", "die", "von", "der", "wil\u00b7de\u00b7ren", "Wil\u00b7den", ",", "der", "Herrschsucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Dreymal heulten sie, sprengten sie Blut,", "tokens": ["Drey\u00b7mal", "heul\u00b7ten", "sie", ",", "spreng\u00b7ten", "sie", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.27": {"text": "Schlugen dreymal auf ein Hohngelach: und das Namen", "tokens": ["Schlu\u00b7gen", "drey\u00b7mal", "auf", "ein", "Hohn\u00b7ge\u00b7lach", ":", "und", "das", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$.", "KON", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Lose war itzt von den Schwestern geweiht,", "tokens": ["Lo\u00b7se", "war", "itzt", "von", "den", "Schwes\u00b7tern", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.29": {"text": "Hatte Beschw\u00f6rung gelernt; die schrien sonst Zauberges\u00e4nge,", "tokens": ["Hat\u00b7te", "Be\u00b7schw\u00f6\u00b7rung", "ge\u00b7lernt", ";", "die", "schri\u00b7en", "sonst", "Zau\u00b7ber\u00b7ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$.", "ART", "VVFIN", "ADV", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Schreyerin war die Beredtsemkeit jetzt;", "tokens": ["Schrey\u00b7e\u00b7rin", "war", "die", "Be\u00b7redt\u00b7sem\u00b7keit", "jetzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Und Es verwandelte: Tagscheu ward der leidenden Auge,", "tokens": ["Und", "Es", "ver\u00b7wan\u00b7del\u00b7te", ":", "Tag\u00b7scheu", "ward", "der", "lei\u00b7den\u00b7den", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "---+--++--+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Taub des B\u00fcrgers Worte das Ohr;", "tokens": ["Taub", "des", "B\u00fcr\u00b7gers", "Wor\u00b7te", "das", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.33": {"text": "Aber dem Luge nicht, dem h\u00f6rte sie leise, vernahm ihn,", "tokens": ["A\u00b7ber", "dem", "Lu\u00b7ge", "nicht", ",", "dem", "h\u00f6r\u00b7te", "sie", "lei\u00b7se", ",", "ver\u00b7nahm", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Murmelt' er auch nur von fern.", "tokens": ["Mur\u00b7melt'", "er", "auch", "nur", "von", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Dolche wurden (Gesang, der Wahrheit treu, du vergehst nicht,", "tokens": ["Dol\u00b7che", "wur\u00b7den", "(", "Ge\u00b7sang", ",", "der", "Wahr\u00b7heit", "treu", ",", "du", "ver\u00b7gehst", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$(", "NN", "$,", "ART", "NN", "ADJD", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Klagst vor dem richtenden Enkel noch an!)", "tokens": ["Klagst", "vor", "dem", "rich\u00b7ten\u00b7den", "En\u00b7kel", "noch", "an", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Dolche wurden ihr die Rosenfinger, und nun auch", "tokens": ["Dol\u00b7che", "wur\u00b7den", "ihr", "die", "Ro\u00b7sen\u00b7fin\u00b7ger", ",", "und", "nun", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "$,", "KON", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.38": {"text": "R\u00f6ther. Der Mund", "tokens": ["R\u00f6\u00b7ther", ".", "Der", "Mund"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.39": {"text": "Konte nur Tod aussprechen. Die Haare wanden sich, zischten,", "tokens": ["Kon\u00b7te", "nur", "Tod", "aus\u00b7spre\u00b7chen", ".", "Die", "Haa\u00b7re", "wan\u00b7den", "sich", ",", "zischten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "VVINF", "$.", "ART", "NN", "VVFIN", "PRF", "$,", "VVFIN", "$,"], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.40": {"text": "Und zu Brande ward das Gehirn!", "tokens": ["Und", "zu", "Bran\u00b7de", "ward", "das", "Ge\u00b7hirn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.41": {"text": "Und ein schreckliches L\u00fcsten durchbebt' ihr das Herz, zu ermorden", "tokens": ["Und", "ein", "schreck\u00b7li\u00b7ches", "L\u00fcs\u00b7ten", "durch\u00b7bebt'", "ihr", "das", "Herz", ",", "zu", "er\u00b7mor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "PTKZU", "VVINF"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.42": {"text": "Wie Medea, den Sohn!", "tokens": ["Wie", "Me\u00b7dea", ",", "den", "Sohn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "ART", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.43": {"text": "(barthelemi erhob das Haupt, und, ich neide die Wandlung!", "tokens": ["(", "bar\u00b7the\u00b7le\u00b7mi", "er\u00b7hob", "das", "Haupt", ",", "und", ",", "ich", "nei\u00b7de", "die", "Wand\u00b7lung", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VVFIN", "ART", "NN", "$,", "KON", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.44": {"text": "Rufte sie, aus der Vergangenheit Nacht.)", "tokens": ["Ruf\u00b7te", "sie", ",", "aus", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "Nacht", ".", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.45": {"text": "Aber Eius misslang: Der Beschwornen wurden die F\u00fclse", "tokens": ["A\u00b7ber", "Ei\u00b7us", "miss\u00b7lang", ":", "Der", "Be\u00b7schwor\u00b7nen", "wur\u00b7den", "die", "F\u00fcl\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "$.", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.46": {"text": "Nicht zu Thou; und so sank sie nicht hin;", "tokens": ["Nicht", "zu", "Thou", ";", "und", "so", "sank", "sie", "nicht", "hin", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NE", "$.", "KON", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.47": {"text": "Steht noch! St\u00fcrzte sie nieder; so war es geschehn, und vergebens", "tokens": ["Steht", "noch", "!", "St\u00fcrz\u00b7te", "sie", "nie\u00b7der", ";", "so", "war", "es", "ge\u00b7schehn", ",", "und", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$,", "KON", "ADV"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.48": {"text": "D\u00fcrstete Wiederverwandlung der Wunsch,", "tokens": ["D\u00fcrs\u00b7te\u00b7te", "Wie\u00b7der\u00b7ver\u00b7wand\u00b7lung", "der", "Wunsch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.49": {"text": "Wiederverwandlung in sie, die sie war vor der Sprengung des Blutes,", "tokens": ["Wie\u00b7der\u00b7ver\u00b7wand\u00b7lung", "in", "sie", ",", "die", "sie", "war", "vor", "der", "Spren\u00b7gung", "des", "Blu\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PRELS", "PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "---+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.50": {"text": "Und der Lache des Hohns,", "tokens": ["Und", "der", "La\u00b7che", "des", "Hohns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.51": {"text": "Wonne! in sie, in die Mutter des tiefgedachten Gesetzes,", "tokens": ["Won\u00b7ne", "!", "in", "sie", ",", "in", "die", "Mut\u00b7ter", "des", "tief\u00b7ge\u00b7dach\u00b7ten", "Ge\u00b7set\u00b7zes", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PPER", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.52": {"text": "Welches menschlicher ist.", "tokens": ["Wel\u00b7ches", "menschli\u00b7cher", "ist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.53": {"text": "Komt, erquickt mich, ihr, die ihr Zukunft wisset, erquickt mich:", "tokens": ["Komt", ",", "er\u00b7quickt", "mich", ",", "ihr", ",", "die", "ihr", "Zu\u00b7kunft", "wis\u00b7set", ",", "er\u00b7quickt", "mich", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+--+-+-++-+-", "measure": "trochaic.septa.relaxed"}, "line.54": {"text": "Werd' ich sehn in der ersten Gestalt,", "tokens": ["Werd'", "ich", "sehn", "in", "der", "ers\u00b7ten", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.55": {"text": "Sehn, wie vom Himmel sie kam, des Gesetzes Mutter? das weiser", "tokens": ["Sehn", ",", "wie", "vom", "Him\u00b7mel", "sie", "kam", ",", "des", "Ge\u00b7set\u00b7zes", "Mut\u00b7ter", "?", "das", "wei\u00b7ser"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "APPRART", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "NN", "$.", "ART", "ADJA"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.56": {"text": "Ist, wohlth\u00e4tiger, menschlicher ist!", "tokens": ["Ist", ",", "wohlt\u00b7h\u00e4\u00b7ti\u00b7ger", ",", "menschli\u00b7cher", "ist", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJD", "$,", "ADJA", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.57": {"text": "Durch das endlich der Traum eintraf, der so lange getr\u00e4umt ward", "tokens": ["Durch", "das", "end\u00b7lich", "der", "Traum", "ein\u00b7traf", ",", "der", "so", "lan\u00b7ge", "ge\u00b7tr\u00e4umt", "ward"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADV", "VVPP", "VAFIN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.58": {"text": "Von der goldenen Zeit!", "tokens": ["Von", "der", "gol\u00b7de\u00b7nen", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.59": {"text": "Ach ihr verstumt mir! strebet umsonst, durch die H\u00fclle der Wolken,", "tokens": ["Ach", "ihr", "ver\u00b7stumt", "mir", "!", "stre\u00b7bet", "um\u00b7sonst", ",", "durch", "die", "H\u00fcl\u00b7le", "der", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "$.", "VVFIN", "ADV", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.60": {"text": "Die stets nachtender w\u00e4lzt der Orkan,", "tokens": ["Die", "stets", "nach\u00b7ten\u00b7der", "w\u00e4lzt", "der", "Or\u00b7kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.61": {"text": "Durch der gehobenen Str\u00f6m Erguss, des h\u00f6heren Weltmeers", "tokens": ["Durch", "der", "ge\u00b7ho\u00b7be\u00b7nen", "Str\u00f6m", "Er\u00b7guss", ",", "des", "h\u00f6\u00b7he\u00b7ren", "Welt\u00b7meers"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.62": {"text": "Wogenberge zu sehn.", "tokens": ["Wo\u00b7gen\u00b7ber\u00b7ge", "zu", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Gab die Erde sie? stieg von Orionen sie nieder?", "tokens": ["Gab", "die", "Er\u00b7de", "sie", "?", "stieg", "von", "O\u00b7rio\u00b7nen", "sie", "nie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$.", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sie von der sch\u00f6nen Seele beseelt,", "tokens": ["Sie", "von", "der", "sch\u00f6\u00b7nen", "See\u00b7le", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sie, des Gesetzes Mutter, das weiser ist, zu der Wohlfahrt", "tokens": ["Sie", ",", "des", "Ge\u00b7set\u00b7zes", "Mut\u00b7ter", ",", "das", "wei\u00b7ser", "ist", ",", "zu", "der", "Wohl\u00b7fahrt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "NN", "$,", "ART", "ADJA", "VAFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+--+-++-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Stimmender, menschlicher ist!", "tokens": ["Stim\u00b7men\u00b7der", ",", "menschli\u00b7cher", "ist", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ungeweihte h\u00f6ren wich auch; drum sprech' ich der G\u00f6ttin", "tokens": ["Un\u00b7ge\u00b7weih\u00b7te", "h\u00f6\u00b7ren", "wich", "auch", ";", "drum", "sprech'", "ich", "der", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "VVFIN", "ADV", "$.", "PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Namen nicht aus.", "tokens": ["Na\u00b7men", "nicht", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ungeweihter ist keiner, als wer von dem Morde den Wahn w\u00e4hnt,", "tokens": ["Un\u00b7ge\u00b7weih\u00b7ter", "ist", "kei\u00b7ner", ",", "als", "wer", "von", "dem", "Mor\u00b7de", "den", "Wahn", "w\u00e4hnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "$,", "KOUS", "PWS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Ihr zu opfern, zuck' er das Schwert.", "tokens": ["Ihr", "zu", "op\u00b7fern", ",", "zuck'", "er", "das", "Schwert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Opferer ist er am Fuss Tisiphona's; auf dieser Altare", "tokens": ["Op\u00b7fe\u00b7rer", "ist", "er", "am", "Fuss", "Ti\u00b7si\u00b7pho\u00b7na's", ";", "auf", "die\u00b7ser", "Al\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPRART", "NN", "NE", "$.", "APPR", "PDAT", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Raucht das vergossene Blut.", "tokens": ["Raucht", "das", "ver\u00b7gos\u00b7se\u00b7ne", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Gl\u00fccklich war, gl\u00fcckselig das Volk, von erhebender Freuden", "tokens": ["Gl\u00fcck\u00b7lich", "war", ",", "gl\u00fcck\u00b7se\u00b7lig", "das", "Volk", ",", "von", "er\u00b7he\u00b7ben\u00b7der", "Freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "ADJD", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Neuem Gef\u00fchl", "tokens": ["Neu\u00b7em", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Trunken, war benedeit, war selig, zu dem des Gesetzes", "tokens": ["Trun\u00b7ken", ",", "war", "be\u00b7ne\u00b7deit", ",", "war", "se\u00b7lig", ",", "zu", "dem", "des", "Ge\u00b7set\u00b7zes"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "$,", "APPR", "ART", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Mutter von den Unsterblichen kam.", "tokens": ["Mut\u00b7ter", "von", "den", "U\u00b7nsterb\u00b7li\u00b7chen", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}, "line.15": {"text": "Aber sie hatte ihn kaum geboren, selber gehuldigt", "tokens": ["A\u00b7ber", "sie", "hat\u00b7te", "ihn", "kaum", "ge\u00b7bo\u00b7ren", ",", "sel\u00b7ber", "ge\u00b7hul\u00b7digt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "ADV", "VVPP"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Ihrem lieblichen, fr\u00f6hlichen Sohn;", "tokens": ["Ih\u00b7rem", "lieb\u00b7li\u00b7chen", ",", "fr\u00f6h\u00b7li\u00b7chen", "Sohn", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Da entfloh die Wonne, versanken der Gl\u00fccklichen Inseln", "tokens": ["Da", "ent\u00b7floh", "die", "Won\u00b7ne", ",", "ver\u00b7san\u00b7ken", "der", "Gl\u00fcck\u00b7li\u00b7chen", "In\u00b7seln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "In die Tiefe das Meers!", "tokens": ["In", "die", "Tie\u00b7fe", "das", "Meers", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Da entstand ... Gern nent' ichs (den Elendstiftern am liebsten!)", "tokens": ["Da