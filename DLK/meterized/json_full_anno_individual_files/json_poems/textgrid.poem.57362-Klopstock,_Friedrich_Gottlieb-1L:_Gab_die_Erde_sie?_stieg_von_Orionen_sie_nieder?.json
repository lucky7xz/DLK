n fern.", "tokens": ["Mur\u00b7melt'", "er", "auch", "nur", "von", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Dolche wurden (Gesang, der Wahrheit treu, du vergehst nicht,", "tokens": ["Dol\u00b7che", "wur\u00b7den", "(", "Ge\u00b7sang", ",", "der", "Wahr\u00b7heit", "treu", ",", "du", "ver\u00b7gehst", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$(", "NN", "$,", "ART", "NN", "ADJD", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Klagst vor dem richtenden Enkel noch an!)", "tokens": ["Klagst", "vor", "dem", "rich\u00b7ten\u00b7den", "En\u00b7kel", "noch", "an", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Dolche wurden ihr die Rosenfinger, und nun auch", "tokens": ["Dol\u00b7che", "wur\u00b7den", "ihr", "die", "Ro\u00b7sen\u00b7fin\u00b7ger", ",", "und", "nun", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "$,", "KON", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.38": {"text": "R\u00f6ther. Der Mund", "tokens": ["R\u00f6\u00b7ther", ".", "Der", "Mund"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.39": {"text": "Konte nur Tod aussprechen. Die Haare wanden sich, zischten,", "tokens": ["Kon\u00b7te", "nur", "Tod", "aus\u00b7spre\u00b7chen", ".", "Die", "Haa\u00b7re", "wan\u00b7den", "sich", ",",