: ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.59": {"text": "Ach ihr verstumt mir! strebet umsonst, durch die H\u00fclle der Wolken,", "tokens": ["Ach", "ihr", "ver\u00b7stumt", "mir", "!", "stre\u00b7bet", "um\u00b7sonst", ",", "durch", "die", "H\u00fcl\u00b7le", "der", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "PPER", "$.", "VVFIN", "ADV", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.60": {"text": "Die stets nachtender w\u00e4lzt der Orkan,", "tokens": ["Die", "stets", "nach\u00b7ten\u00b7der", "w\u00e4lzt", "der", "Or\u00b7kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.61": {"text": "Durch der gehobenen Str\u00f6m Erguss, des h\u00f6heren Weltmeers", "tokens": ["Durch", "der", "ge\u00b7ho\u00b7be\u00b7nen", "Str\u00f6m", "Er\u00b7guss", ",", "des", "h\u00f6\u00b7he\u00b7ren", "Welt\u00b7meers"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.62": {"text": "Wogenberge zu sehn.", "tokens": ["Wo\u00b7gen\u00b7ber\u00b7ge", "zu", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}