{"dta.poem.13027": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.85", "id:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Thalia nahm es auch und sah das Vorblatt an,", "tokens": ["Tha\u00b7lia", "nahm", "es", "auch", "und", "sah", "das", "Vor\u00b7blatt", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Da\u00df ich Gelegenheit, es auch zu sehn, gewann.", "tokens": ["Da\u00df", "ich", "Ge\u00b7le\u00b7gen\u00b7heit", ",", "es", "auch", "zu", "sehn", ",", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich las nur obenhin von Erb-Recht und Gew\u00e4hrung,", "tokens": ["Ich", "las", "nur", "o\u00b7ben\u00b7hin", "von", "Er\u00b7bRecht", "und", "Ge\u00b7w\u00e4h\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von Manns- und Weibs-Geschlecht, von Frieden und Verschw\u00f6rung.", "tokens": ["Von", "Manns", "und", "Weibs\u00b7Ge\u00b7schlecht", ",", "von", "Frie\u00b7den", "und", "Ver\u00b7schw\u00f6\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}