d", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADV", "ADV", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mein gantzer Mensche scheint entseelet und betr\u00fcbet,", "tokens": ["Mein", "gant\u00b7zer", "Men\u00b7sche", "scheint", "ent\u00b7see\u00b7let", "und", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil deine Cynthia nichts \u00fcber dich geliebet,", "tokens": ["Weil", "dei\u00b7ne", "Cyn\u00b7thia", "nichts", "\u00fc\u00b7ber", "dich", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Sie wei\u00df bey den Verlust sich, Sch\u00f6nster, nicht zu fassen,", "tokens": ["Sie", "wei\u00df", "bey", "den", "Ver\u00b7lust", "sich", ",", "Sch\u00f6ns\u00b7ter", ",", "nicht", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PRF", "$,", "NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich soll, bedenck es selbst, dich lieben und doch lassen.", "tokens": ["Ich", "soll", ",", "be\u00b7denck", "es", "selbst", ",", "dich", "lie\u00b7ben", "und", "doch", "las\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch ja! ich liebe dich, so lange Blut und Leben", "tokens": ["Doch", "ja", "!", "ich", "lie\u00b7be", "dich", ",", "so", "lan\u00b7ge", "Blut", "und", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "PPER", "$,", "ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Noch in den Adern ist, verbleibt es dir ergeben.", "tokens": ["Noch", "in", "den", "A\u00b7dern", "ist", ",", "ver\u00b7bleibt", "es", "dir", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sucht das Verh\u00e4ngni\u00df uns vor die\u00dfmahl gleich zu trennen,", "tokens": ["Sucht", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "uns", "vor", "die\u00df\u00b7mahl", "gleich", "zu", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Will ich doch bi\u00df in Tod dein Eigenthum mich nennen.", "tokens": ["Will", "ich", "doch", "bi\u00df", "in", "Tod", "dein", "Ei\u00b7gen\u00b7thum", "mich", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mich s