{"textgrid.poem.54228": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich soll zum letzten mahl an dich, Geliebter, schreiben,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich soll zum letzten mahl an dich, Geliebter, schreiben,", "tokens": ["Ich", "soll", "zum", "letz\u00b7ten", "mahl", "an", "dich", ",", "Ge\u00b7lieb\u00b7ter", ",", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "ADV", "APPR", "PPER", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Hand ist gantz verstarrt, ich kan vor Schmertz nicht bleiben,", "tokens": ["Die", "Hand", "ist", "gantz", "ver\u00b7starrt", ",", "ich", "kan", "vor", "Schmertz", "nicht", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,", "PPER", "VMFIN", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Dinte will nicht so, wie meine Thr\u00e4nen, flie\u00dfen,", "tokens": ["Die", "Din\u00b7te", "will", "nicht", "so", ",", "wie", "mei\u00b7ne", "Thr\u00e4\u00b7nen", ",", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "$,", "PWAV", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sich den Str\u00f6hmen gleich mit auf das Blatt ergie\u00dfen.", "tokens": ["Die", "sich", "den", "Str\u00f6h\u00b7men", "gleich", "mit", "auf", "das", "Blatt", "er\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "ADV", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Schicksals Grausamkeit will, da\u00df ich dich soll missen,", "tokens": ["Des", "Schick\u00b7sals", "Grau\u00b7sam\u00b7keit", "will", ",", "da\u00df", "ich", "dich", "soll", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und nun an deiner Statt den leeren Schatten k\u00fcssen,", "tokens": ["Und", "nun", "an", "dei\u00b7ner", "Statt", "den", "lee\u00b7ren", "Schat\u00b7ten", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein h\u00f6chstbeklemmtes Hertz sucht Rath und Trost zu finden,", "tokens": ["Mein", "h\u00f6chst\u00b7be\u00b7klemm\u00b7tes", "Hertz", "sucht", "Rath", "und", "Trost", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und trifft doch keinen an; mein Fu\u00df durchstreicht die Linden,", "tokens": ["Und", "trifft", "doch", "kei\u00b7nen", "an", ";", "mein", "Fu\u00df", "durch\u00b7streicht", "die", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kein Donner r\u00fchrt so hart die allerst\u00e4rcksten B\u00e4ume,", "tokens": ["Kein", "Don\u00b7ner", "r\u00fchrt", "so", "hart", "die", "al\u00b7ler\u00b7st\u00e4rcks\u00b7ten", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich wei\u00df wahrhafftig nicht, ob ich schon wachend tr\u00e4ume.", "tokens": ["Ich", "wei\u00df", "wahr\u00b7haff\u00b7tig", "nicht", ",", "ob", "ich", "schon", "wa\u00b7chend", "tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kein Sturm kan wohl so starck auf Fluth und Wellen rasen,", "tokens": ["Kein", "Sturm", "kan", "wohl", "so", "starck", "auf", "Fluth", "und", "Wel\u00b7len", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und bey des Aeols Wuth auf Schiffe brausend blasen,", "tokens": ["Und", "bey", "des", "A\u00b7e\u00b7ols", "Wuth", "auf", "Schif\u00b7fe", "brau\u00b7send", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Kein Blitz, kein schneller Pfeil, fliegt, schw\u00f6r ich, so behende,", "tokens": ["Kein", "Blitz", ",", "kein", "schnel\u00b7ler", "Pfeil", ",", "fliegt", ",", "schw\u00f6r", "ich", ",", "so", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$,", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als ich mich \u00e4ngstiglich bald da, bald dorthin wende.", "tokens": ["Als", "ich", "mich", "\u00e4ngs\u00b7tig\u00b7lich", "bald", "da", ",", "bald", "dor\u00b7thin", "wen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADV", "ADV", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mein gantzer Mensche scheint entseelet und betr\u00fcbet,", "tokens": ["Mein", "gant\u00b7zer", "Men\u00b7sche", "scheint", "ent\u00b7see\u00b7let", "und", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil deine Cynthia nichts \u00fcber dich geliebet,", "tokens": ["Weil", "dei\u00b7ne", "Cyn\u00b7thia", "nichts", "\u00fc\u00b7ber", "dich", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Sie wei\u00df bey den Verlust sich, Sch\u00f6nster, nicht zu fassen,", "tokens": ["Sie", "wei\u00df", "bey", "den", "Ver\u00b7lust", "sich", ",", "Sch\u00f6ns\u00b7ter", ",", "nicht", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PRF", "$,", "NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich soll, bedenck es selbst, dich lieben und doch lassen.", "tokens": ["Ich", "soll", ",", "be\u00b7denck", "es", "selbst", ",", "dich", "lie\u00b7ben", "und", "doch", "las\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch ja! ich liebe dich, so lange Blut und Leben", "tokens": ["Doch", "ja", "!", "ich", "lie\u00b7be", "dich", ",", "so", "lan\u00b7ge", "Blut", "und", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "PPER", "$,", "ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Noch in den Adern ist, verbleibt es dir ergeben.", "tokens": ["Noch", "in", "den", "A\u00b7dern", "ist", ",", "ver\u00b7bleibt", "es", "dir", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sucht das Verh\u00e4ngni\u00df uns vor die\u00dfmahl gleich zu trennen,", "tokens": ["Sucht", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "uns", "vor", "die\u00df\u00b7mahl", "gleich", "zu", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Will ich doch bi\u00df in Tod dein Eigenthum mich nennen.", "tokens": ["Will", "ich", "doch", "bi\u00df", "in", "Tod", "dein", "Ei\u00b7gen\u00b7thum", "mich", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mich soll kein frembder Strahl auch in Entfernung blenden,", "tokens": ["Mich", "soll", "kein", "fremb\u00b7der", "Strahl", "auch", "in", "Ent\u00b7fer\u00b7nung", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADJA", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mein Auge wird sich stets von andern C\u00f6rpern wenden.", "tokens": ["Mein", "Au\u00b7ge", "wird", "sich", "stets", "von", "an\u00b7dern", "C\u00f6r\u00b7pern", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wenn ein Narcissus mir die Schlinge wolte legen,", "tokens": ["Wenn", "ein", "Nar\u00b7cis\u00b7sus", "mir", "die", "Schlin\u00b7ge", "wol\u00b7te", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So wird sich doch bey mir kein Trieb noch Neigung regen.", "tokens": ["So", "wird", "sich", "doch", "bey", "mir", "kein", "Trieb", "noch", "Nei\u00b7gung", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "APPR", "PPER", "PIAT", "NN", "ADV", "NN", "ADJA", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "Raubt gleich ein harter Schlu\u00df durch dich mir mein Vergn\u00fcgen,", "tokens": ["Raubt", "gleich", "ein", "har\u00b7ter", "Schlu\u00df", "durch", "dich", "mir", "mein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So will ich durch Gedult den Unstern doch besiegen.", "tokens": ["So", "will", "ich", "durch", "Ge\u00b7dult", "den", "Uns\u00b7tern", "doch", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dein Bild bleibt unterde\u00df in meiner Brust gepr\u00e4get,", "tokens": ["Dein", "Bild", "bleibt", "un\u00b7ter\u00b7de\u00df", "in", "mei\u00b7ner", "Brust", "ge\u00b7pr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So lange, bi\u00df man mich in Sand und Erde leget,", "tokens": ["So", "lan\u00b7ge", ",", "bi\u00df", "man", "mich", "in", "Sand", "und", "Er\u00b7de", "le\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PIS", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und solt ich vor der Zeit in Grufft und Bogen reisen,", "tokens": ["Und", "solt", "ich", "vor", "der", "Zeit", "in", "Grufft", "und", "Bo\u00b7gen", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wird Asch und Moder doch in dich verliebt noch heissen.", "tokens": ["Wird", "Asch", "und", "Mo\u00b7der", "doch", "in", "dich", "ver\u00b7liebt", "noch", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "APPR", "PPER", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich soll zum letzten mahl an dich, Geliebter, schreiben,", "tokens": ["Ich", "soll", "zum", "letz\u00b7ten", "mahl", "an", "dich", ",", "Ge\u00b7lieb\u00b7ter", ",", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "ADV", "APPR", "PPER", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Hand ist gantz verstarrt, ich kan vor Schmertz nicht bleiben,", "tokens": ["Die", "Hand", "ist", "gantz", "ver\u00b7starrt", ",", "ich", "kan", "vor", "Schmertz", "nicht", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,", "PPER", "VMFIN", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Dinte will nicht so, wie meine Thr\u00e4nen, flie\u00dfen,", "tokens": ["Die", "Din\u00b7te", "will", "nicht", "so", ",", "wie", "mei\u00b7ne", "Thr\u00e4\u00b7nen", ",", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "$,", "PWAV", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sich den Str\u00f6hmen gleich mit auf das Blatt ergie\u00dfen.", "tokens": ["Die", "sich", "den", "Str\u00f6h\u00b7men", "gleich", "mit", "auf", "das", "Blatt", "er\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "ADV", "APPR", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Schicksals Grausamkeit will, da\u00df ich dich soll missen,", "tokens": ["Des", "Schick\u00b7sals", "Grau\u00b7sam\u00b7keit", "will", ",", "da\u00df", "ich", "dich", "soll", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und nun an deiner Statt den leeren Schatten k\u00fcssen,", "tokens": ["Und", "nun", "an", "dei\u00b7ner", "Statt", "den", "lee\u00b7ren", "Schat\u00b7ten", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein h\u00f6chstbeklemmtes Hertz sucht Rath und Trost zu finden,", "tokens": ["Mein", "h\u00f6chst\u00b7be\u00b7klemm\u00b7tes", "Hertz", "sucht", "Rath", "und", "Trost", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und trifft doch keinen an; mein Fu\u00df durchstreicht die Linden,", "tokens": ["Und", "trifft", "doch", "kei\u00b7nen", "an", ";", "mein", "Fu\u00df", "durch\u00b7streicht", "die", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kein Donner r\u00fchrt so hart die allerst\u00e4rcksten B\u00e4ume,", "tokens": ["Kein", "Don\u00b7ner", "r\u00fchrt", "so", "hart", "die", "al\u00b7ler\u00b7st\u00e4rcks\u00b7ten", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich wei\u00df wahrhafftig nicht, ob ich schon wachend tr\u00e4ume.", "tokens": ["Ich", "wei\u00df", "wahr\u00b7haff\u00b7tig", "nicht", ",", "ob", "ich", "schon", "wa\u00b7chend", "tr\u