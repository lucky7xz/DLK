{"textgrid.poem.42309": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An Stentor unter der Predigt", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, deine Predigt gleicht dem Heerposaunenschalle,", "tokens": ["Freund", ",", "dei\u00b7ne", "Pre\u00b7digt", "gleicht", "dem", "Heer\u00b7po\u00b7sau\u00b7nen\u00b7schal\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Dem Jericho erlag, durch ihren Wunderlaut.", "tokens": ["Dem", "Je\u00b7ri\u00b7cho", "er\u00b7lag", ",", "durch", "ih\u00b7ren", "Wun\u00b7der\u00b7laut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn bald zerrei\u00dft von ihrem Donnerhalle \u2013", "tokens": ["Denn", "bald", "zer\u00b7rei\u00dft", "von", "ih\u00b7rem", "Don\u00b7ner\u00b7hal\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Gotteskraft! \u2013 des Ohres Trommelhaut.", "tokens": ["O", "Got\u00b7tes\u00b7kraft", "!", "\u2013", "des", "Oh\u00b7res", "Trom\u00b7mel\u00b7haut", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch, soll das End' auch noch H\u00f6rers Beifall lohnen,", "tokens": ["Doch", ",", "soll", "das", "End'", "auch", "noch", "H\u00f6\u00b7rers", "Bei\u00b7fall", "loh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "ART", "NN", "ADV", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So mu\u00dft du seiner Ohren schonen.", "tokens": ["So", "mu\u00dft", "du", "sei\u00b7ner", "Oh\u00b7ren", "scho\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Freund, deine Predigt gleicht dem Heerposaunenschalle,", "tokens": ["Freund", ",", "dei\u00b7ne", "Pre\u00b7digt", "gleicht", "dem", "Heer\u00b7po\u00b7sau\u00b7nen\u00b7schal\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Dem Jericho erlag, durch ihren Wunderlaut.", "tokens": ["Dem", "Je\u00b7ri\u00b7cho", "er\u00b7lag", ",", "durch", "ih\u00b7ren", "Wun\u00b7der\u00b7laut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn bald zerrei\u00dft von ihrem Donnerhalle \u2013", "tokens": ["Denn", "bald", "zer\u00b7rei\u00dft", "von", "ih\u00b7rem", "Don\u00b7ner\u00b7hal\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Gotteskraft! \u2013 des Ohres Trommelhaut.", "tokens": ["O", "Got\u00b7tes\u00b7kraft", "!", "