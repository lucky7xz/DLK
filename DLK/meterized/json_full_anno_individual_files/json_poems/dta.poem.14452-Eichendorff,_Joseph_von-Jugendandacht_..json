{"dta.poem.14452": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl ist des Dichters Seele stumm verbunden", "tokens": ["Wohl", "ist", "des", "Dich\u00b7ters", "See\u00b7le", "stumm", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit M\u00e4chten, die am Volk' vor\u00fcberrauschen;", "tokens": ["Mit", "M\u00e4ch\u00b7ten", ",", "die", "am", "Vol\u00b7k'", "vor\u00b7\u00fc\u00b7berr\u00b7au\u00b7schen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Sehnsucht mu\u00df wachsen an der Tiefe Rauschen", "tokens": ["Sehn\u00b7sucht", "mu\u00df", "wach\u00b7sen", "an", "der", "Tie\u00b7fe", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "VVINF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach hellerm Licht und nach des Himmels Kunden.", "tokens": ["Nach", "hel\u00b7lerm", "Licht", "und", "nach", "des", "Him\u00b7mels", "Kun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}