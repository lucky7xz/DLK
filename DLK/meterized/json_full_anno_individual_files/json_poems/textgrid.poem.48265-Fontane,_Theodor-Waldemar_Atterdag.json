{"textgrid.poem.48265": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Waldemar Atterdag", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Im Dome zu Ringstedt nahm er die Kron',", "tokens": ["Im", "Do\u00b7me", "zu", "Rings\u00b7tedt", "nahm", "er", "die", "Kron'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nun f\u00fchrt er die Herrschaft mit kluger Hand", "tokens": ["Nun", "f\u00fchrt", "er", "die", "Herr\u00b7schaft", "mit", "klu\u00b7ger", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "\u00dcber D\u00e4nemark-Meer und D\u00e4nemark-Land,", "tokens": ["\u00dc\u00b7ber", "D\u00e4\u00b7ne\u00b7mark\u00b7Meer", "und", "D\u00e4\u00b7ne\u00b7mark\u00b7Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Nie fa\u00dft ihn J\u00e4hzorn, nie treibt ihn Eil',", "tokens": ["Nie", "fa\u00dft", "ihn", "J\u00e4h\u00b7zorn", ",", "nie", "treibt", "ihn", "Eil'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "\u00bberst w\u00e4gen, dann wagen\u00ab ... \u00bbEile mit Weil'.\u00ab", "tokens": ["\u00bb", "erst", "w\u00e4\u00b7gen", ",", "dann", "wa\u00b7gen", "\u00ab", "...", "\u00bb", "Ei\u00b7le", "mit", "Weil'", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "$,", "ADV", "VVINF", "$(", "$(", "$(", "NN", "APPR", "NN", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und ob es zur Tat ihn auch dr\u00e4ngen mag,", "tokens": ["Und", "ob", "es", "zur", "Tat", "ihn", "auch", "dr\u00e4n\u00b7gen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Auf den ", "tokens": ["Auf", "den"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Und er f\u00e4hrt gen J\u00fctland. Auf Schlo\u00df Aarhuus", "tokens": ["Und", "er", "f\u00e4hrt", "gen", "J\u00fct\u00b7land", ".", "Auf", "Schlo\u00df", "Aar\u00b7hu\u00b7us"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NE", "$.", "APPR", "NN", "NE"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Harrt er auf Huldigung und Gru\u00df,", "tokens": ["Harrt", "er", "auf", "Hul\u00b7di\u00b7gung", "und", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Auf Gru\u00df des Adels. Der h\u00e4lt sich zur\u00fcck;", "tokens": ["Auf", "Gru\u00df", "des", "A\u00b7dels", ".", "Der", "h\u00e4lt", "sich", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$.", "ART", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbum Gott, K\u00f6nig Waldemar, auf und flieh,", "tokens": ["\u00bb", "um", "Gott", ",", "K\u00f6\u00b7nig", "Wal\u00b7de\u00b7mar", ",", "auf", "und", "flieh", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "NN", "NE", "$,", "APPR", "KON", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "In hellen Haufen kommen sie,", "tokens": ["In", "hel\u00b7len", "Hau\u00b7fen", "kom\u00b7men", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie z\u00fcrnen dir schwer, weil du zubestimmst", "tokens": ["Sie", "z\u00fcr\u00b7nen", "dir", "schwer", ",", "weil", "du", "zu\u00b7be\u00b7stimmst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Dem Bauer all das, was dem Adel du nimmst,", "tokens": ["Dem", "Bau\u00b7er", "all", "das", ",", "was", "dem", "A\u00b7del", "du", "nimmst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "PDS", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Sehstedt f\u00fchrt sie; von Viborg her", "tokens": ["Seh\u00b7stedt", "f\u00fchrt", "sie", ";", "von", "Vi\u00b7borg", "her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "APPR", "NE", "APZR"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Kommen dreihundert oder mehr.", "tokens": ["Kom\u00b7men", "drei\u00b7hun\u00b7dert", "o\u00b7der", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "KON", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "In den Sattel, K\u00f6nig, und flieh und jag'", "tokens": ["In", "den", "Sat\u00b7tel", ",", "K\u00f6\u00b7nig", ",", "und", "flieh", "und", "jag'"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "KON", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Hin \u00fcber die Heide.\u00ab ... \u00bbAtterdag.\u00ab", "tokens": ["Hin", "\u00fc\u00b7ber", "die", "Hei\u00b7de", ".", "\u00ab", "...", "\u00bb", "At\u00b7ter\u00b7dag", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$(", "$(", "$(", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und ein Jahr und ein Tag, und auf Schlo\u00df Helsing\u00f6r", "tokens": ["Und", "ein", "Jahr", "und", "ein", "Tag", ",", "und", "auf", "Schlo\u00df", "Hel\u00b7sin\u00b7g\u00f6r"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "$,", "KON", "APPR", "NN", "NE"], "meter": "--+--+--+---", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Im Landsthing sitzt er und gibt Geh\u00f6r;", "tokens": ["Im", "Land\u00b7sthing", "sitzt", "er", "und", "gibt", "Ge\u00b7h\u00f6r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Um ihn her seine R\u00e4te; da st\u00fcrmt in den Saal", "tokens": ["Um", "ihn", "her", "sei\u00b7ne", "R\u00e4\u00b7te", ";", "da", "st\u00fcrmt", "in", "den", "Saal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "ADV", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Erik Swensen, sein erster Admiral.", "tokens": ["E\u00b7rik", "Swen\u00b7sen", ",", "sein", "ers\u00b7ter", "Ad\u00b7mi\u00b7ral", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "\u00bbeile dich, K\u00f6nig. Zu dieser Stund'", "tokens": ["\u00bb", "ei\u00b7le", "dich", ",", "K\u00f6\u00b7nig", ".", "Zu", "die\u00b7ser", "Stund'"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$.", "APPR", "PDAT", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Fahren die L\u00fcbischen in den Sund,", "tokens": ["Fah\u00b7ren", "die", "L\u00fc\u00b7bi\u00b7schen", "in", "den", "Sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Zwischen Insel Amak und Insel Hveen", "tokens": ["Zwi\u00b7schen", "In\u00b7sel", "A\u00b7mak", "und", "In\u00b7sel", "Hveen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NE", "KON", "NN", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Sind siebenundzwanzig Segel zu sehn,", "tokens": ["Sind", "sie\u00b7ben\u00b7und\u00b7zwan\u00b7zig", "Se\u00b7gel", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "An der Spitze die \u203aSeekuh\u2039, ihr bestes Schiff,", "tokens": ["An", "der", "Spit\u00b7ze", "die", "\u203a", "See\u00b7kuh", "\u2039", ",", "ihr", "bes\u00b7tes", "Schiff", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$(", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Greif zu, wie dein Vater einst sie griff.", "tokens": ["Greif", "zu", ",", "wie", "dein", "Va\u00b7ter", "einst", "sie", "griff", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "PPOSAT", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Sie kommen wie R\u00e4uber. Nach Gut und Blut", "tokens": ["Sie", "kom\u00b7men", "wie", "R\u00e4u\u00b7ber", ".", "Nach", "Gut", "und", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "$.", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "D\u00fcrsten sie. Zertritt ihre Brut,", "tokens": ["D\u00fcrs\u00b7ten", "sie", ".", "Zer\u00b7tritt", "ih\u00b7re", "Brut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Vernichte sie mit einem Schlag.\u00ab", "tokens": ["Ver\u00b7nich\u00b7te", "sie", "mit", "ei\u00b7nem", "Schlag", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "\u00bberst w\u00e4gen, dann wagen ... Atterdag.\u00ab", "tokens": ["\u00bb", "erst", "w\u00e4\u00b7gen", ",", "dann", "wa\u00b7gen", "...", "At\u00b7ter\u00b7dag", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "$,", "ADV", "VVINF", "$(", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und wieder ein Jahr, und auf Schlo\u00df Wordingborg", "tokens": ["Und", "wie\u00b7der", "ein", "Jahr", ",", "und", "auf", "Schlo\u00df", "Wor\u00b7ding\u00b7borg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "KON", "APPR", "NN", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "In Stille sitzt er und doch in Sorg',", "tokens": ["In", "Stil\u00b7le", "sitzt", "er", "und", "doch", "in", "Sor\u00b7g'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In Sorg' um Heilwig. Auf seinem Sinn", "tokens": ["In", "Sor\u00b7g'", "um", "Heil\u00b7wig", ".", "Auf", "sei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NE", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Lastet die sch\u00f6ne K\u00f6nigin.", "tokens": ["Las\u00b7tet", "die", "sch\u00f6\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Es hei\u00dft, sie sei krank, ohne Schlaf ihre Ruh,", "tokens": ["Es", "hei\u00dft", ",", "sie", "sei", "krank", ",", "oh\u00b7ne", "Schlaf", "ih\u00b7re", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "$,", "KOUI", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Aber ein K\u00e4mmerling fl\u00fcstert ihm zu:", "tokens": ["A\u00b7ber", "ein", "K\u00e4m\u00b7mer\u00b7ling", "fl\u00fcs\u00b7tert", "ihm", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "\u00bbder K\u00f6nigin Krankheit ist Lug, ist Schein,", "tokens": ["\u00bb", "der", "K\u00f6\u00b7ni\u00b7gin", "Krank\u00b7heit", "ist", "Lug", ",", "ist", "Schein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "VAFIN", "NE", "$,", "VAFIN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Sten Sture geht lachend aus und ein,", "tokens": ["Sten", "Stu\u00b7re", "geht", "la\u00b7chend", "aus", "und", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Er ist noch ein Knabe, noch halb ein Kind,", "tokens": ["Er", "ist", "noch", "ein", "Kna\u00b7be", ",", "noch", "halb", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Das lieben die Frauen, wie Frauen sind.", "tokens": ["Das", "lie\u00b7ben", "die", "Frau\u00b7en", ",", "wie", "Frau\u00b7en", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PWAV", "NN", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Auf, Waldemar, st\u00f6r ihre Lust, ihre List,", "tokens": ["Auf", ",", "Wal\u00b7de\u00b7mar", ",", "st\u00f6r", "ih\u00b7re", "Lust", ",", "ih\u00b7re", "List", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "NE", "$,", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Zeige, da\u00df du der K\u00f6nig bist,", "tokens": ["Zei\u00b7ge", ",", "da\u00df", "du", "der", "K\u00f6\u00b7nig", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "\u00dcberrasche Sch\u00f6n-Heilwig, erforsche sie, frag.\u00ab", "tokens": ["\u00dc\u00b7berr\u00b7a\u00b7sche", "Sch\u00f6n\u00b7Heil\u00b7wig", ",", "er\u00b7for\u00b7sche", "sie", ",", "frag", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$.", "$("], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "\u00bbes w\u00fcrde sie t\u00f6ten ... Atterdag.\u00ab", "tokens": ["\u00bb", "es", "w\u00fcr\u00b7de", "sie", "t\u00f6\u00b7ten", "...", "At\u00b7ter\u00b7dag", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "p