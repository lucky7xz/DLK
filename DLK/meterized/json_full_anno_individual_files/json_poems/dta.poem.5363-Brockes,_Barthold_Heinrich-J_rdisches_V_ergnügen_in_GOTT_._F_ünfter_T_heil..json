{"dta.poem.5363": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch uns mit allem Ernst bem\u00fchn,", "tokens": ["Auch", "uns", "mit", "al\u00b7lem", "Ernst", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den uns geschenckten freyen Willen,", "tokens": ["Den", "uns", "ge\u00b7schenck\u00b7ten", "frey\u00b7en", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um deinen Willen zu erf\u00fcllen,", "tokens": ["Um", "dei\u00b7nen", "Wil\u00b7len", "zu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von dem, was dir misf\u00e4llt, zu ziehn!", "tokens": ["Von", "dem", ",", "was", "dir", "mis\u00b7f\u00e4llt", ",", "zu", "ziehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wirst du uns, auf dieser Welt", "tokens": ["So", "wirst", "du", "uns", ",", "auf", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht das, wodurch man sich erh\u00e4lt,", "tokens": ["Nicht", "das", ",", "wo\u00b7durch", "man", "sich", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDS", "$,", "PWAV", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zur Lust und Nothdurft nur beschehren;", "tokens": ["Zur", "Lust", "und", "Noth\u00b7durft", "nur", "be\u00b7scheh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du wirst uns auch nach dieser Zeit,", "tokens": ["Du", "wirst", "uns", "auch", "nach", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In jener seel\u2019gen Ewigkeit,", "tokens": ["In", "je\u00b7ner", "seel'\u00b7gen", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit ew\u2019ger Himmels-Speise n\u00e4hren!", "tokens": ["Mit", "ew'\u00b7ger", "Him\u00b7mels\u00b7Spei\u00b7se", "n\u00e4h\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}