{"dta.poem.7500": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "IiI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was er geschaffen, ist ein Edelstein,", "tokens": ["Was", "er", "ge\u00b7schaf\u00b7fen", ",", "ist", "ein", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "D'rin blitzen Strahlen f\u00fcr die Ewigkeit;", "tokens": ["D'\u00b7rin", "blit\u00b7zen", "Strah\u00b7len", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Doch h\u00e4tt' er uns ein Leitstern sollen sein", "tokens": ["Doch", "h\u00e4tt'", "er", "uns", "ein", "Leits\u00b7tern", "sol\u00b7len", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PRF", "ART", "NN", "VMFIN", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In dieser halben, irrgeword'nen Zeit,", "tokens": ["In", "die\u00b7ser", "hal\u00b7ben", ",", "irr\u00b7ge\u00b7word'\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In dieser Zeit, so wetterschw\u00fcl und bang,", "tokens": ["In", "die\u00b7ser", "Zeit", ",", "so", "wet\u00b7ter\u00b7schw\u00fcl", "und", "bang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die noch im Ohr der Kindheit Glockenklang,", "tokens": ["Die", "noch", "im", "Ohr", "der", "Kind\u00b7heit", "Glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und mit der Hand schon nach dem Schwerte zittert,", "tokens": ["Und", "mit", "der", "Hand", "schon", "nach", "dem", "Schwer\u00b7te", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zur H\u00e4lfte tot, zur H\u00e4lfte neugeboren,", "tokens": ["Zur", "H\u00e4lf\u00b7te", "tot", ",", "zur", "H\u00e4lf\u00b7te", "neu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Gleich einer Pflanze, die den Fr\u00fchling wittert", "tokens": ["Gleich", "ei\u00b7ner", "Pflan\u00b7ze", ",", "die", "den", "Fr\u00fch\u00b7ling", "wit\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und ihre alten Bl\u00e4tter nicht verloren.", "tokens": ["Und", "ih\u00b7re", "al\u00b7ten", "Bl\u00e4t\u00b7ter", "nicht", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Er h\u00e4tte \u2014 aber g\u00f6nnt ihm seine Ruh!", "tokens": ["Er", "h\u00e4t\u00b7te", "a\u00b7ber", "g\u00f6nnt", "ihm", "sei\u00b7ne", "Ruh", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Augen fielen einem M\u00fcden zu;", "tokens": ["Die", "Au\u00b7gen", "fie\u00b7len", "ei\u00b7nem", "M\u00fc\u00b7den", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Doch hat er, funkelnd in Begeisterung,", "tokens": ["Doch", "hat", "er", ",", "fun\u00b7kelnd", "in", "Be\u00b7geis\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Vom Himmelslichte trunken, sie geschlossen,", "tokens": ["Vom", "Him\u00b7mels\u00b7lich\u00b7te", "trun\u00b7ken", ",", "sie", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der Dichtung Quelle hat sich voll und jung", "tokens": ["Der", "Dich\u00b7tung", "Quel\u00b7le", "hat", "sich", "voll", "und", "jung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "PRF", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Noch in den stillen Ocean ergossen.", "tokens": ["Noch", "in", "den", "stil\u00b7len", "O\u00b7cean", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Und eine Braut nahm ihn der andern ab;", "tokens": ["Und", "ei\u00b7ne", "Braut", "nahm", "ihn", "der", "an\u00b7dern", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.20": {"text": "Und legt sich bis zum j\u00fcngsten Tag daneben.", "tokens": ["Und", "legt", "sich", "bis", "zum", "j\u00fcng\u00b7sten", "Tag", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPRART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Auch nicht ", "tokens": ["Auch", "nicht"], "token_info": ["word", "word"], "pos": ["ADV", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Zwei Pfeiler unsrer Kirche st\u00fcrzten ein;", "tokens": ["Zwei", "Pfei\u00b7ler", "uns\u00b7rer", "Kir\u00b7che", "st\u00fcrz\u00b7ten", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Erst als den freisten Mann die Gruft empfangen,", "tokens": ["Erst", "als", "den", "freis\u00b7ten", "Mann", "die", "Gruft", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Senkt man auch B\u00fcchner in den Totenschrein.", "tokens": ["Senkt", "man", "auch", "B\u00fcch\u00b7ner", "in", "den", "To\u00b7ten\u00b7schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "B\u00fcchner und B\u00f6rne! \u2014 Deutsche Dioskuren,", "tokens": ["B\u00fcch\u00b7ner", "und", "B\u00f6r\u00b7ne", "!", "Deut\u00b7sche", "Dios\u00b7ku\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$.", "$(", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.26": {"text": "Weh', da\u00df der Lorber nicht auf deutschen Fluren", "tokens": ["Weh'", ",", "da\u00df", "der", "Lor\u00b7ber", "nicht", "auf", "deut\u00b7schen", "Flu\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "F\u00fcr solch geweihte H\u00e4ubter wachsen darf!", "tokens": ["F\u00fcr", "solch", "ge\u00b7weih\u00b7te", "H\u00e4ub\u00b7ter", "wach\u00b7sen", "darf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Der Wind im Norden weht noch rauh und scharf,", "tokens": ["Der", "Wind", "im", "Nor\u00b7den", "weht", "noch", "rauh", "und", "scharf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Der Lorber will im Treibhaus nur gedeihen,", "tokens": ["Der", "Lor\u00b7ber", "will", "im", "Treib\u00b7haus", "nur", "ge\u00b7dei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Ein freier Mann holt sich ihn aus dem Freien!", "tokens": ["Ein", "frei\u00b7er", "Mann", "holt", "sich", "ihn", "aus", "dem", "Frei\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}}}}}