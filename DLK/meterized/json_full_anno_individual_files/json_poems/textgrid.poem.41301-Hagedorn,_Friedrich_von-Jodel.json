{"textgrid.poem.41301": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Jodel", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Jodel, Jodels Sohn, erbla\u00dfte schnell und satt:", "tokens": ["Herr", "Jo\u00b7del", ",", "Jo\u00b7dels", "Sohn", ",", "er\u00b7bla\u00df\u00b7te", "schnell", "und", "satt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NE", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er, dem die Stadt die Welt, sein Kirchspiel eine Stadt,", "tokens": ["Er", ",", "dem", "die", "Stadt", "die", "Welt", ",", "sein", "Kirch\u00b7spiel", "ei\u00b7ne", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Haus das Kirchspiel war: der nie in fremdem Lande", "tokens": ["Sein", "Haus", "das", "Kirch\u00b7spiel", "war", ":", "der", "nie", "in", "frem\u00b7dem", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "VAFIN", "$.", "ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Luft oder Witz gesch\u00f6pft: ein Feind der welschen Bande,", "tokens": ["Luft", "o\u00b7der", "Witz", "ge\u00b7sch\u00f6pft", ":", "ein", "Feind", "der", "wel\u00b7schen", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word