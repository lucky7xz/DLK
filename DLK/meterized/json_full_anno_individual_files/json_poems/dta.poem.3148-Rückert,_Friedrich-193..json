{"dta.poem.3148": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "193.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Manch falsches Wissen auch sollt ihr bei mir nicht missen;", "tokens": ["Manch", "fal\u00b7sches", "Wis\u00b7sen", "auch", "sollt", "ihr", "bei", "mir", "nicht", "mis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "VMFIN", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum? damit ihr seht: es kommt nicht an aufs Wissen.", "tokens": ["Wa\u00b7rum", "?", "da\u00b7mit", "ihr", "seht", ":", "es", "kommt", "nicht", "an", "aufs", "Wis\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Irrthum irret nicht den wahren Drang des Strebens;", "tokens": ["Ein", "Irr\u00b7thum", "ir\u00b7ret", "nicht", "den", "wah\u00b7ren", "Drang", "des", "Stre\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sei mit Gott dies Buch, und so das eures Lebens.", "tokens": ["So", "sei", "mit", "Gott", "dies", "Buch", ",", "und", "so", "das", "eu\u00b7res", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "wor