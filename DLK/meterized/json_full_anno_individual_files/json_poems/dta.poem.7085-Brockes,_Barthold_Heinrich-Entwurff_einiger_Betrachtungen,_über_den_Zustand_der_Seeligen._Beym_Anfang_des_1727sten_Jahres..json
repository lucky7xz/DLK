{"dta.poem.7085": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebte Leser! Stellt mit mir", "tokens": ["Ge\u00b7lieb\u00b7te", "Le\u00b7ser", "!", "Stellt", "mit", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$.", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch doch einst in Gedancken f\u00fcr,", "tokens": ["Euch", "doch", "einst", "in", "Ge\u00b7dan\u00b7cken", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "NN", "APPR", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ob es nicht herrlich sey, als wie ein Strahl", "tokens": ["Ob", "es", "nicht", "herr\u00b7lich", "sey", ",", "als", "wie", "ein", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$,", "KOUS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich auf einmahl,", "tokens": ["Sich", "auf", "ein\u00b7mahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Von einer Welt zur andern hin zu schwingen,", "tokens": ["Von", "ei\u00b7ner", "Welt", "zur", "an\u00b7dern", "hin", "zu", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und da die G\u00f6ttlichen Befehle", "tokens": ["Und", "da", "die", "G\u00f6tt\u00b7li\u00b7chen", "Be\u00b7feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als Sein Gesandter zu vollbringen.", "tokens": ["Als", "Sein", "Ge\u00b7sand\u00b7ter", "zu", "voll\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}