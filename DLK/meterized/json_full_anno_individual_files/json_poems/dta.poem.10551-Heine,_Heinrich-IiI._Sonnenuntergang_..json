{"dta.poem.10551": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Sonnenuntergang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "B\u00f6se, zischelnde Zungen", "tokens": ["B\u00f6\u00b7se", ",", "zi\u00b7scheln\u00b7de", "Zun\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Brachten also Schmerz und Verderben", "tokens": ["Brach\u00b7ten", "al\u00b7so", "Schmerz", "und", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Selbst \u00fcber ewige G\u00f6tter.", "tokens": ["Selbst", "\u00fc\u00b7ber", "e\u00b7wi\u00b7ge", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und die armen G\u00f6tter, oben am Himmel", "tokens": ["Und", "die", "ar\u00b7men", "G\u00f6t\u00b7ter", ",", "o\u00b7ben", "am", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "ADV", "APPRART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Wandeln sie, qualvoll,", "tokens": ["Wan\u00b7deln", "sie", ",", "qual\u00b7voll", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Trostlos unendliche Bahnen,", "tokens": ["Trost\u00b7los", "un\u00b7end\u00b7li\u00b7che", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Und k\u00f6nnen nicht sterben,", "tokens": ["Und", "k\u00f6n\u00b7nen", "nicht", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und schleppen mit sich", "tokens": ["Und", "schlep\u00b7pen", "mit", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PRF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ihr strahlendes Elend.", "tokens": ["Ihr", "strah\u00b7len\u00b7des", "E\u00b7lend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich aber, der Mensch,", "tokens": ["Ich", "a\u00b7ber", ",", "der", "Mensch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der niedriggepflanzte, der Tod-begl\u00fcckte.", "tokens": ["Der", "nied\u00b7rig\u00b7ge\u00b7pflanz\u00b7te", ",", "der", "To\u00b7dbe\u00b7gl\u00fcck\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich klage nicht l\u00e4nger.", "tokens": ["Ich", "kla\u00b7ge", "nicht", "l\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}