{"dta.poem.12584": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Macht euch ja, noch auf dem Lande,", "tokens": ["Macht", "euch", "ja", ",", "noch", "auf", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon zur Fahrt bereit;", "tokens": ["Schon", "zur", "Fahrt", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da seht, ob ihr auch im Stande", "tokens": ["Da", "seht", ",", "ob", "ihr", "auch", "im", "Stan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Fortzuschiffen seyd.", "tokens": ["Fort\u00b7zu\u00b7schif\u00b7fen", "seyd", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Durch die Wellen m\u00fcsst ihr streichen,", "tokens": ["Durch", "die", "Wel\u00b7len", "m\u00fcsst", "ihr", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie der Wind euch f\u00fchrt,", "tokens": ["Wie", "der", "Wind", "euch", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Der denn in den Wasser-Reichen", "tokens": ["Der", "denn", "in", "den", "Was\u00b7ser\u00b7Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unumschr\u00e4nkt regiert.", "tokens": ["Un\u00b7um\u00b7schr\u00e4nkt", "re\u00b7giert", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}