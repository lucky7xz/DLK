{"dta.poem.19553": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Spielzeug.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Liebchen fand ich spielend. Einen Kasten", "tokens": ["Lieb\u00b7chen", "fand", "ich", "spie\u00b7lend", ".", "Ei\u00b7nen", "Kas\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "$.", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hatte sie entdeckt voll l\u00e4ngst verge\u00dfnen", "tokens": ["Hat\u00b7te", "sie", "ent\u00b7deckt", "voll", "l\u00e4ngst", "ver\u00b7ge\u00df\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Staub'gen Kinderspielzeugs: Mauern, Thore,", "tokens": ["Staub'\u00b7gen", "Kin\u00b7der\u00b7spiel\u00b7zeugs", ":", "Mau\u00b7ern", ",", "Tho\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Rathhaus, H\u00e4user, H\u00e4userchen und Kirche ...", "tokens": ["Rath\u00b7haus", ",", "H\u00e4u\u00b7ser", ",", "H\u00e4u\u00b7ser\u00b7chen", "und", "Kir\u00b7che", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Sie erbaut' das St\u00e4dtchen mit gelenken", "tokens": ["Sie", "er\u00b7baut'", "das", "St\u00e4dt\u00b7chen", "mit", "ge\u00b7len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "H\u00e4nden, stellt' den Kirchthurm in die Mitte.", "tokens": ["H\u00e4n\u00b7den", ",", "stellt'", "den", "Kirch\u00b7thurm", "in", "die", "Mit\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Doch ein H\u00e4uschen hatt' sie vorbehalten,", "tokens": ["Doch", "ein", "H\u00e4usc\u00b7hen", "hatt'", "sie", "vor\u00b7be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Vorbehalten sieben gr\u00fcne Pappeln", "tokens": ["Vor\u00b7be\u00b7hal\u00b7ten", "sie\u00b7ben", "gr\u00fc\u00b7ne", "Pap\u00b7peln"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "F\u00fcr ein allerliebstes kleines Landgut.", "tokens": ["F\u00fcr", "ein", "al\u00b7ler\u00b7liebs\u00b7tes", "klei\u00b7nes", "Land\u00b7gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Nicht zu nah! Im St\u00e4dtchen klatscht man s\u00fcndlich.", "tokens": ["Nicht", "zu", "nah", "!", "Im", "St\u00e4dt\u00b7chen", "klatscht", "man", "s\u00fcnd\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJD", "$.", "APPRART", "NN", "VVFIN", "PIS", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Nicht zu ferne! Man bedarf der Menschen.", "tokens": ["Nicht", "zu", "fer\u00b7ne", "!", "Man", "be\u00b7darf", "der", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "ADJA", "$.", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "\u201eeben sind wir eingezogen!\u201c jubelt'", "tokens": ["\u201e", "e\u00b7ben", "sind", "wir", "ein\u00b7ge\u00b7zo\u00b7gen", "!", "\u201c", "ju\u00b7belt'"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["$(", "ADV", "VAFIN", "PPER", "VVPP", "$.", "$(", "VVFIN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Sie und klatscht' in ihre kleinen H\u00e4nde.", "tokens": ["Sie", "und", "klatscht'", "in", "ih\u00b7re", "klei\u00b7nen", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "In der Wonne des erworbnen Heimes", "tokens": ["In", "der", "Won\u00b7ne", "des", "er\u00b7worb\u00b7nen", "Hei\u00b7mes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Ri\u00df ich Liebchen an mich so gewaltsam,", "tokens": ["Ri\u00df", "ich", "Lieb\u00b7chen", "an", "mich", "so", "ge\u00b7walt\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Da\u00df den Arm sie streckte wie ertrinkend ...", "tokens": ["Da\u00df", "den", "Arm", "sie", "streck\u00b7te", "wie", "er\u00b7trin\u00b7kend", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "KOKOM", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Was erwischte sie mit schnellen Fingern,", "tokens": ["Was", "er\u00b7wischte", "sie", "mit", "schnel\u00b7len", "Fin\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Eng an meine Brust gepre\u00dft? Die Kirche,", "tokens": ["Eng", "an", "mei\u00b7ne", "Brust", "ge\u00b7pre\u00dft", "?", "Die", "Kir\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVPP", "$.", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Ja, die Kirche mit dem rothen Dach war's", "tokens": ["Ja", ",", "die", "Kir\u00b7che", "mit", "dem", "ro\u00b7then", "Dach", "wa\u00b7r's"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.20": {"text": "Und sie stellt' sie dicht vor unser Landhaus.", "tokens": ["Und", "sie", "stellt'", "sie", "dicht", "vor", "un\u00b7ser", "Land\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}