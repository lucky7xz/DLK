{"dta.poem.23607": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Diplomatische Klarheit und K\u00fcrze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hinz! sagt Klaus,", "tokens": ["Hinz", "!", "sagt", "Klaus", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ich komm von Haus.", "tokens": ["Ich", "komm", "von", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es schickt mich her,", "tokens": ["Es", "schickt", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ihr wisst schon ", "tokens": ["Ihr", "wisst", "schon"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Wie hei\u00dft denn ", "tokens": ["Wie", "hei\u00dft", "denn"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Ihr wisst schon ", "tokens": ["Ihr", "wisst", "schon"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Hinz sagt: Klaus!", "tokens": ["Hinz", "sagt", ":", "Klaus", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Geh nur nach Haus,", "tokens": ["Geh", "nur", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und sag du ", "tokens": ["Und", "sag", "du"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Du weisst schon ", "tokens": ["Du", "weisst", "schon"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und sag du ", "tokens": ["Und", "sag", "du"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Ich will ihm was", "tokens": ["Ich", "will", "ihm", "was"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PWS"], "meter": "-+-+", "measure": "iambic.di"}}}}}