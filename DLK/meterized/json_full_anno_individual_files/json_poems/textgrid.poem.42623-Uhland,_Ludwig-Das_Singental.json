{"textgrid.poem.42623": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Singental", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herzog tief im Walde", "tokens": ["Der", "Her\u00b7zog", "tief", "im", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Fu\u00df der Eiche sa\u00df,", "tokens": ["Am", "Fu\u00df", "der", "Ei\u00b7che", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als singend an der Halde", "tokens": ["Als", "sin\u00b7gend", "an", "der", "Hal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein M\u00e4gdlein Beeren las.", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "Bee\u00b7ren", "las", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erdbeeren, k\u00fchl und duftig,", "tokens": ["Erd\u00b7bee\u00b7ren", ",", "k\u00fchl", "und", "duf\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Bot sie dem greisen Mann,", "tokens": ["Bot", "sie", "dem", "grei\u00b7sen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch ihn umschwebte luftig", "tokens": ["Doch", "ihn", "um\u00b7schweb\u00b7te", "luf\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch stets der T\u00f6ne Bann.", "tokens": ["Noch", "stets", "der", "T\u00f6\u00b7ne", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbmit deinem hellen Liede\u00ab,", "tokens": ["\u00bb", "mit", "dei\u00b7nem", "hel\u00b7len", "Lie\u00b7de", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PPOSAT", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sprach er, \u00bbfeine Magd!", "tokens": ["So", "sprach", "er", ",", "\u00bb", "fei\u00b7ne", "Magd", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kam \u00fcber mich der Friede", "tokens": ["Kam", "\u00fc\u00b7ber", "mich", "der", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach mancher st\u00fcrm'schen Jagd.", "tokens": ["Nach", "man\u00b7cher", "st\u00fcr\u00b7m'\u00b7schen", "Jagd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Die Beeren, die du bringest,", "tokens": ["Die", "Bee\u00b7ren", ",", "die", "du", "brin\u00b7gest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erfrischen wohl den Gaum,", "tokens": ["Er\u00b7fri\u00b7schen", "wohl", "den", "Gaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch singe mehr! du singest", "tokens": ["Doch", "sin\u00b7ge", "mehr", "!", "du", "sin\u00b7gest"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Seel in heitern Traum.", "tokens": ["Die", "Seel", "in", "hei\u00b7tern", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ert\u00f6nt an dieser Eiche", "tokens": ["Er\u00b7t\u00f6nt", "an", "die\u00b7ser", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Horn von Elfenbein,", "tokens": ["Mein", "Horn", "von", "El\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In seines Schalls Bereiche", "tokens": ["In", "sei\u00b7nes", "Schalls", "Be\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist all das Waldtal mein;", "tokens": ["Ist", "all", "das", "Wald\u00b7tal", "mein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So weit von jener Birke", "tokens": ["So", "weit", "von", "je\u00b7ner", "Bir\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Lied erklingt rundum,", "tokens": ["Dein", "Lied", "er\u00b7klingt", "run\u00b7dum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Geb ich im Talbezirke", "tokens": ["Geb", "ich", "im", "Tal\u00b7be\u00b7zir\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dir Erb und Eigentum.\u00ab", "tokens": ["Dir", "Erb", "und", "Ei\u00b7gen\u00b7tum", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Noch einmal blies der Alte", "tokens": ["Noch", "ein\u00b7mal", "blies", "der", "Al\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Horn ins Tal hinaus,", "tokens": ["Sein", "Horn", "ins", "Tal", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In ferner Felsenspalte", "tokens": ["In", "fer\u00b7ner", "Fel\u00b7sen\u00b7spal\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verklang's wie Sturmgebraus:", "tokens": ["Ver\u00b7klang's", "wie", "Sturm\u00b7ge\u00b7braus", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann sang vom Birkenh\u00fcgel", "tokens": ["Dann", "sang", "vom", "Bir\u00b7ken\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des M\u00e4gdleins s\u00fc\u00dfer Mund,", "tokens": ["Des", "M\u00e4gd\u00b7leins", "s\u00fc\u00b7\u00dfer", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als rauschten Engelfl\u00fcgel", "tokens": ["Als", "rauschten", "En\u00b7gel\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ob all dem stillen Grund.", "tokens": ["Ob", "all", "dem", "stil\u00b7len", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er legt in ihre H\u00e4nde", "tokens": ["Er", "legt", "in", "ih\u00b7re", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Siegelring zum Pfand:", "tokens": ["Den", "Sie\u00b7gel\u00b7ring", "zum", "Pfand", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmein Waidwerk hat ein Ende,", "tokens": ["\u00bb", "mein", "Waid\u00b7werk", "hat", "ein", "En\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vergabt ist dir das Land.\u00ab", "tokens": ["Ver\u00b7gabt", "ist", "dir", "das", "Land", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da nickt ihm Dank die Holde", "tokens": ["Da", "nickt", "ihm", "Dank", "die", "Hol\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und eilet froh waldaus,", "tokens": ["Und", "ei\u00b7let", "froh", "wald\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie tr\u00e4gt im Ring von Golde", "tokens": ["Sie", "tr\u00e4gt", "im", "Ring", "von", "Gol\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den frischen Erdbeerstrau\u00df. \u2013", "tokens": ["Den", "fri\u00b7schen", "Erd\u00b7beer\u00b7strau\u00df", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Als noch des Hornes Brausen", "tokens": ["Als", "noch", "des", "Hor\u00b7nes", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gebot mit finstrer Macht,", "tokens": ["Ge\u00b7bot", "mit", "finst\u00b7rer", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sah man Eber hausen", "tokens": ["Da", "sah", "man", "E\u00b7ber", "hau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In tiefer Waldesnacht;", "tokens": ["In", "tie\u00b7fer", "Wal\u00b7des\u00b7nacht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Laut bellte dort die Meute,", "tokens": ["Laut", "bell\u00b7te", "dort", "die", "Meu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor der die Hindin floh,", "tokens": ["Vor", "der", "die", "Hin\u00b7din", "floh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und fiel die blut'ge Beute,", "tokens": ["Und", "fiel", "die", "blut'\u00b7ge", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Erscholl ein wild Halloh.", "tokens": ["Er\u00b7scholl", "ein", "wild", "Hal\u00b7loh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch seit des M\u00e4gdleins Singen", "tokens": ["Doch", "seit", "des", "M\u00e4gd\u00b7leins", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist ringsum Wiesengr\u00fcn,", "tokens": ["Ist", "ring\u00b7sum", "Wie\u00b7sen\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri