{"dta.poem.20009": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An Gangloffs Geist .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier in diesen \u00fcpp'gen Feldern,", "tokens": ["Hier", "in", "die\u00b7sen", "\u00fcpp'\u00b7gen", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rebenbergen, wolk'gen W\u00e4ldern,", "tokens": ["Re\u00b7ben\u00b7ber\u00b7gen", ",", "wol\u00b7k'\u00b7gen", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Um das Maal der Frauentreu',", "tokens": ["Um", "das", "Maal", "der", "Frau\u00b7en\u00b7treu'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo du giengst in stillem Sinnen, \u2014", "tokens": ["Wo", "du", "giengst", "in", "stil\u00b7lem", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Brennt es mich im Busen innen,", "tokens": ["Brennt", "es", "mich", "im", "Bu\u00b7sen", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werden alte Wunden neu.", "tokens": ["Wer\u00b7den", "al\u00b7te", "Wun\u00b7den", "neu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Berg und Thale h\u00f6r' ich fragen:", "tokens": ["Berg", "und", "Tha\u00b7le", "h\u00f6r'", "ich", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat er nicht auch dich getragen", "tokens": ["Hat", "er", "nicht", "auch", "dich", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einst im Herzen liebewarm?", "tokens": ["Einst", "im", "Her\u00b7zen", "lie\u00b7be\u00b7warm", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kam er mit dir? \u2014 weh! und schauen", "tokens": ["Kam", "er", "mit", "dir", "?", "weh", "!", "und", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["NE", "PPER", "APPR", "PPER", "$.", "$(", "PTKVZ", "$.", "KON", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Mu\u00df ich deiner Tugend Auen,", "tokens": ["Mu\u00df", "ich", "dei\u00b7ner", "Tu\u00b7gend", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann durch Thr\u00e4nen voll von Harm.", "tokens": ["Dann", "durch", "Thr\u00e4\u00b7nen", "voll", "von", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber die dein Geist erdachte,", "tokens": ["A\u00b7ber", "die", "dein", "Geist", "er\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Hand in's Leben brachte", "tokens": ["Dei\u00b7ne", "Hand", "in's", "Le\u00b7ben", "brach\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Wein-bekr\u00e4nzten Thal,", "tokens": ["In", "dem", "Wein\u00b7be\u00b7kr\u00e4nz\u00b7ten", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene Bilder alter Zeiten", "tokens": ["Je\u00b7ne", "Bil\u00b7der", "al\u00b7ter", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seh' ich oft vor\u00fcber gleiten", "tokens": ["Seh'", "ich", "oft", "vor\u00b7\u00fc\u00b7ber", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geistern gleich im Mondenstrahl.", "tokens": ["Geis\u00b7tern", "gleich", "im", "Mon\u00b7dens\u00b7trahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deine Helden, deine Frauen", "tokens": ["Dei\u00b7ne", "Hel\u00b7den", ",", "dei\u00b7ne", "Frau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geh'n mit mir durch diese Auen", "tokens": ["Geh'n", "mit", "mir", "durch", "die\u00b7se", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch im sp\u00e4ten Abendroth.", "tokens": ["Noch", "im", "sp\u00e4\u00b7ten", "A\u00b7ben\u00b7droth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fcstern: ist auch ", "tokens": ["Fl\u00fcs\u00b7tern", ":", "ist", "auch"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "VAFIN", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Was sein Geist, sein Herz erfunden", "tokens": ["Was", "sein", "Geist", ",", "sein", "Herz", "er\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Raubt der Freundes-Brust kein Tod.", "tokens": ["Raubt", "der", "Freun\u00b7des\u00b7Brust", "kein", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}