ter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.22": {"line.1": {"text": "Trug es so viel der H\u00e4upter, der streitenden,", "tokens": ["Trug", "es", "so", "viel", "der", "H\u00e4up\u00b7ter", ",", "der", "strei\u00b7ten\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Als Herzen athmen, flammende Nahrung sog's", "tokens": ["Als", "Her\u00b7zen", "ath\u00b7men", ",", "flam\u00b7men\u00b7de", "Nah\u00b7rung", "so\u00b7g's"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "VVINF", "$,", "ADJA", "NN", "NE"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Osten, Schweif und Drachenfl\u00fcgel", "tokens": ["Im", "Os\u00b7ten", ",", "Schweif", "und", "Dra\u00b7chen\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlug den zertretnen, zermalmten Abend.", "tokens": ["Schlug", "den", "zer\u00b7tret\u00b7nen", ",", "zer\u00b7malm\u00b7ten", "A\u00b7bend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.23": {"line.1": {"text": "Des Rachens uners\u00e4ttlicher Schlund am Strand", "tokens": ["Des", "Ra\u00b7chens", "un\u00b7er\u00b7s\u00e4tt\u00b7li\u00b7cher", "Schlund", "am", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Tibers g\u00e4hnt' er, Throne zertr\u00fcmmert' er,", "tokens": ["Des", "Ti\u00b7bers", "g\u00e4hnt'", "er", ",", "Thro\u00b7ne", "zer\u00b7tr\u00fcm\u00b7mert'", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein gro\u00df Jahrtausend war sein Leben,", "tokens": ["Ein", "gro\u00df", "Jahr\u00b7tau\u00b7send", "war", "sein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "R\u00fchmt' er nicht selbst sich des Himmels W\u00e4chter,", "tokens": ["R\u00fchmt'", "er", "nicht", "selbst", "sich", "des", "Him\u00b7mels", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PRF", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.24": {"line.1": {"text": "Dein Feind, o Friedrich? Gr\u00f6\u00dfern bek\u00e4mpfte nie", "tokens": ["Dein", "Feind", ",", "o", "Fried\u00b7rich", "?", "Gr\u00f6\u00b7\u00dfern", "be\u00b7k\u00e4mpf\u00b7te", "nie"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "FM", "NE", "$.", "NN", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Held, sei's denn der Engel des Schwerts vielleicht,", "tokens": ["Ein", "Held", ",", "sei's", "denn", "der", "En\u00b7gel", "des", "Schwerts", "viel\u00b7leicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Belial schlug. O Staub des Herrschers,", "tokens": ["Der", "Be\u00b7li\u00b7al", "schlug", ".", "O", "Staub", "des", "Herr\u00b7schers", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Betet' ich Irdisches an, du w\u00e4rst es.", "tokens": ["Be\u00b7tet'", "ich", "Ir\u00b7di\u00b7sches", "an", ",", "du", "w\u00e4rst", "es", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.25": {"line.1": {"text": "Des Bannstrahls denk' ich, den aufs gekr\u00f6nte Haupt", "tokens": ["Des", "Bann\u00b7strahls", "denk'", "ich", ",", "den", "aufs", "ge\u00b7kr\u00f6n\u00b7te", "Haupt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Roms frechster Priester schleuderte, Volk und Land", "tokens": ["Roms", "frechs\u00b7ter", "Pries\u00b7ter", "schleu\u00b7der\u00b7te", ",", "Volk", "und", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Fluch beladend und der Menschheit", "tokens": ["Mit", "Fluch", "be\u00b7la\u00b7dend", "und", "der", "Menschheit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heiligste Fesseln, der W\u00fcthrich, sprengend.", "tokens": ["Hei\u00b7ligs\u00b7te", "Fes\u00