ord", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und H\u00e4nde ringend, Tod im Auge,", "tokens": ["Und", "H\u00e4n\u00b7de", "rin\u00b7gend", ",", "Tod", "im", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Riefe der Staufe: Wo ist mein Enkel?", "tokens": ["Rie\u00b7fe", "der", "Stau\u00b7fe", ":", "Wo", "ist", "mein", "En\u00b7kel", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.29": {"line.1": {"text": "Sein Blut komm' \u00fcber euch und den Priesterstuhl,", "tokens": ["Sein", "Blut", "komm'", "\u00fc\u00b7ber", "euch", "und", "den", "Pries\u00b7ter\u00b7stuhl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "KON", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein letztes Blut, mein theuerstes, \u00fcber euch", "tokens": ["Mein", "letz\u00b7tes", "Blut", ",", "mein", "theu\u00b7ers\u00b7tes", ",", "\u00fc\u00b7ber", "euch"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "$,", "APPR", "PPER"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Komm' es! Gerichtet hat die Stimme", "tokens": ["Komm'", "es", "!", "Ge\u00b7rich\u00b7tet", "hat", "die", "Stim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "VVPP", "VAFIN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "L\u00e4ngst schon der Menschheit, und kommen wird er,", "tokens": ["L\u00e4ngst", "schon", "der", "Menschheit", ",", "und", "kom\u00b7men", "wird", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "KON", "VVINF", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.30": {"line.1": {"text": "Der Tag, wo Jener richtet, der mich dem Staub", "tokens": ["Der", "Tag", ",", "wo", "Je\u00b7ner", "rich\u00b7tet", ",", "der", "mich", "dem", "Staub"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PDAT", "VVFIN", "$,", "PRELS", "PRF", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Anheim gab, fordern wird er von euch die Schuld,", "tokens": ["An\u00b7heim", "gab", ",", "for\u00b7dern", "wird", "er", "von", "euch", "die", "Schuld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "VAFIN", "PPER", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ist auch dreifach eure Krone,", "tokens": ["Und", "ist", "auch", "drei\u00b7fach", "eu\u00b7re", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dreifach mit Greueln beladen ist sie!", "tokens": ["Drei\u00b7fach", "mit", "Greu\u00b7eln", "be\u00b7la\u00b7den", "ist", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "VVPP", "VAFIN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.31": {"line.1": {"text": "So d\u00fcnkt mir, spricht weissagend der Geist; doch l\u00e4ngst", "tokens": ["So", "d\u00fcnkt", "mir", ",", "spricht", "weis\u00b7sa\u00b7gend", "der", "Geist", ";", "doch", "l\u00e4ngst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "ADJD", "ART", "NN", "$.", "ADV", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Grollt ihm der Priester, grollt ihm die Mutter selbst,", "tokens": ["Grollt", "ihm", "der", "Pries\u00b7ter", ",", "grollt", "ihm", "die", "Mut\u00b7ter", "selbst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die allbarmherz'ge, nicht mehr; friedlich", "tokens": ["Die", "all\u00b7barm\u00b7her\u00b7z'\u00b7ge", ",", "nicht", "mehr", ";", "fried\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "PTKNEG", "ADV", "$.", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ruhet im Tempel des Kaisers Asche.", "tokens": ["Ru\u00b7het", "im", "Tem\u00b7pel", "des", "Kai\u00b7sers", "A\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.32": {"line.1": {"text": "Und fern vom goldnen Altar erschallt der Chor", "tokens": ["Und", "fern", "vom", "gold\u00b7nen", "Al\u00b7tar", "er\u00b7schallt", "der", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu Friedrichs Einsamkeit und des Vaters Sarg,", "tokens": ["Zu", "Fried\u00b7richs", "Ein\u00b7sam\u00b7keit", "und", "des", "Va\u00b7ters", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als wollt' er ihren Zorn, als wollt' er", "tokens": ["Als", "wollt'", "er", "ih\u00b7ren", "Zorn", ",", "als", "wollt'", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "VMFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Reuig den r\u00e4chenden Gott bes\u00e4nft'gen.", "tokens": ["Reu\u00b7ig", "den", "r\u00e4\u00b7chen\u00b7den", "Gott", "be\u00b7s\u00e4nft'\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}