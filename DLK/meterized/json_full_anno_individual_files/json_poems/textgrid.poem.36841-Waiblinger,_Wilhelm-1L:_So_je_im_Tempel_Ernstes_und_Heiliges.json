{"textgrid.poem.36841": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: So je im Tempel Ernstes und Heiliges", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So je im Tempel Ernstes und Heiliges", "tokens": ["So", "je", "im", "Tem\u00b7pel", "Erns\u00b7tes", "und", "Hei\u00b7li\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "NE", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Herz mir traf in gro\u00dfen Entfaltungen,", "tokens": ["Das", "Herz", "mir", "traf", "in", "gro\u00b7\u00dfen", "Ent\u00b7fal\u00b7tun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Gottes dunkle Offenbarung", "tokens": ["Des", "Got\u00b7tes", "dunk\u00b7le", "Of\u00b7fen\u00b7ba\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schauer mir go\u00df in des Geistes Tiefen,", "tokens": ["Schau\u00b7er", "mir", "go\u00df", "in", "des", "Geis\u00b7tes", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Und w\u00e4r' es Blut und Leib des Erl\u00f6sers selbst,", "tokens": ["Und", "w\u00e4r'", "es", "Blut", "und", "Leib", "des", "Er\u00b7l\u00f6\u00b7sers", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "KON", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da ihn mein Mund ber\u00fchrte zum erstenmal,", "tokens": ["Da", "ihn", "mein", "Mund", "be\u00b7r\u00fchr\u00b7te", "zum", "ers\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "APPRART", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So Unaussprechliches im Herzen", "tokens": ["So", "Un\u00b7aus\u00b7sprech\u00b7li\u00b7ches", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht an der Treppe des Altars f\u00fchlt' ich,", "tokens": ["Nicht", "an", "der", "Trep\u00b7pe", "des", "Al\u00b7tars", "f\u00fchlt'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.3": {"line.1": {"text": "Und z\u00fcrnte mir der Himmel und z\u00fcrnte mir", "tokens": ["Und", "z\u00fcrn\u00b7te", "mir", "der", "Him\u00b7mel", "und", "z\u00fcrn\u00b7te", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Erde, die im Staube Gebete lallt,", "tokens": ["Die", "Er\u00b7de", ",", "die", "im", "Stau\u00b7be", "Ge\u00b7be\u00b7te", "lallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch sei's bekannt, vor deinem Sarge", "tokens": ["Doch", "sei's", "be\u00b7kannt", ",", "vor", "dei\u00b7nem", "Sar\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "PTKVZ", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Feiert' ich gr\u00f6\u00dfere, k\u00fchn're Andacht.", "tokens": ["Fei\u00b7ert'", "ich", "gr\u00f6\u00b7\u00dfe\u00b7re", ",", "k\u00fchn'\u00b7re", "An\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Du hier, o ew'ge Glorie des Vaterlands,", "tokens": ["Du", "hier", ",", "o", "ew'\u00b7ge", "Glo\u00b7rie", "des", "Va\u00b7ter\u00b7lands", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "FM", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des deutschen Scepters herrlichster F\u00fcrst und Held,", "tokens": ["Des", "deut\u00b7schen", "Scep\u00b7ters", "herr\u00b7lichs\u00b7ter", "F\u00fcrst", "und", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du Sch\u00f6pfer nie gewagter Thaten,", "tokens": ["Du", "Sch\u00f6p\u00b7fer", "nie", "ge\u00b7wag\u00b7ter", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00e4mpfer des Lichts und der bessern Wahrheit,", "tokens": ["K\u00e4mp\u00b7fer", "des", "Lichts", "und", "der", "bes\u00b7sern", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Der gr\u00f6\u00dfer als der Sieger der Hydra einst,", "tokens": ["Der", "gr\u00f6\u00b7\u00dfer", "als", "der", "Sie\u00b7ger", "der", "Hyd\u00b7ra", "einst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der sichtbaren, mit schrecklicherm Feinde rang,", "tokens": ["Der", "sicht\u00b7ba\u00b7ren", ",", "mit", "schreck\u00b7li\u00b7cherm", "Fein\u00b7de", "rang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit gift'germ Ungeheur, mit blut'germ;", "tokens": ["Mit", "gift'\u00b7germ", "Un\u00b7ge\u00b7heur", ",", "mit", "blut'\u00b7germ", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ohne Gestalt und verwundbar Wesen", "tokens": ["Oh\u00b7ne", "Ge\u00b7stalt", "und", "ver\u00b7wund\u00b7bar", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJD", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Trug es so viel der H\u00e4upter, der streitenden,", "tokens": ["Trug", "es", "so", "viel", "der", "H\u00e4up\u00b7ter", ",", "der", "strei\u00b7ten\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Als Herzen athmen, flammende Nahrung sog's", "tokens": ["Als", "Her\u00b7zen", "ath\u00b7men", ",", "flam\u00b7men\u00b7de", "Nah\u00b7rung", "so\u00b7g's"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "VVINF", "$,", "ADJA", "NN", "NE"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Osten, Schweif und Drachenfl\u00fcgel", "tokens": ["Im", "Os\u00b7ten", ",", "Schweif", "und", "Dra\u00b7chen\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlug den zertretnen, zermalmten Abend.", "tokens": ["Schlug", "den", "zer\u00b7tret\u00b7nen", ",", "zer\u00b7malm\u00b7ten", "A\u00b7bend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Des Rachens uners\u00e4ttlicher Schlund am Strand", "tokens": ["Des", "Ra\u00b7chens", "un\u00b7er\u00b7s\u00e4tt\u00b7li\u00b7cher", "Schlund", "am", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Tibers g\u00e4hnt' er, Throne zertr\u00fcmmert' er,", "tokens": ["Des", "Ti\u00b7bers", "g\u00e4hnt'", "er", ",", "Thro\u00b7ne", "zer\u00b7tr\u00fcm\u00b7mert'", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein gro\u00df Jahrtausend war sein Leben,", "tokens": ["Ein", "gro\u00df", "Jahr\u00b7tau\u00b7send", "war", "sein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "R\u00fchmt' er nicht selbst sich des Himmels W\u00e4chter,", "tokens": ["R\u00fchmt'", "er", "nicht", "selbst", "sich", "des", "Him\u00b7mels", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PRF", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Dein Feind, o Friedrich? Gr\u00f6\u00dfern bek\u00e4mpfte nie", "tokens": ["Dein", "Feind", ",", "o", "Fried\u00b7rich", "?", "Gr\u00f6\u00b7\u00dfern", "be\u00b7k\u00e4mpf\u00b7te", "nie"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "FM", "NE", "$.", "NN", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Held, sei's denn der Engel des Schwerts vielleicht,", "tokens": ["Ein", "Held", ",", "sei's", "denn", "der", "En\u00b7gel", "des", "Schwerts", "viel\u00b7leicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Belial schlug. O Staub des Herrschers,", "tokens": ["Der", "Be\u00b7li\u00b7al", "schlug", ".", "O", "Staub", "des", "Herr\u00b7schers", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NE", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Betet' ich Irdisches an, du w\u00e4rst es.", "tokens": ["Be\u00b7tet'", "ich", "Ir\u00b7di\u00b7sches", "an", ",", "du", "w\u00e4rst", "es", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Des Bannstrahls denk' ich, den aufs gekr\u00f6nte Haupt", "tokens": ["Des", "Bann\u00b7strahls", "denk'", "ich", ",", "den", "aufs", "ge\u00b7kr\u00f6n\u00b7te", "Haupt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Roms frechster Priester schleuderte, Volk und Land", "tokens": ["Roms", "frechs\u00b7ter", "Pries\u00b7ter", "schleu\u00b7der\u00b7te", ",", "Volk", "und", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Fluch beladend und der Menschheit", "tokens": ["Mit", "Fluch", "be\u00b7la\u00b7dend", "und", "der", "Menschheit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heiligste Fesseln, der W\u00fcthrich, sprengend.", "tokens": ["Hei\u00b7ligs\u00b7te", "Fes\u00b7seln", ",", "der", "W\u00fcth\u00b7rich", ",", "spren\u00b7gend", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "$,", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Du aber, Kaiser, weintest in hohem Zorn", "tokens": ["Du", "a\u00b7ber", ",", "Kai\u00b7ser", ",", "wein\u00b7test", "in", "ho\u00b7hem", "Zorn"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und riefst: Des Reiches Kronen o bringt mir sie!", "tokens": ["Und", "riefst", ":", "Des", "Rei\u00b7ches", "Kro\u00b7nen", "o", "bringt", "mir", "sie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "ADJA", "NN", "FM", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und aufs geweihte Haupt sie setzend", "tokens": ["Und", "aufs", "ge\u00b7weih\u00b7te", "Haupt", "sie", "set\u00b7zend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sprachst du in Flammen gekr\u00e4nkten Herzens:", "tokens": ["Sprachst", "du", "in", "Flam\u00b7men", "ge\u00b7kr\u00e4nk\u00b7ten", "Her\u00b7zens", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Wer n\u00e4hme mir die Krone von diesem Haupt?", "tokens": ["Wer", "n\u00e4h\u00b7me", "mir", "die", "Kro\u00b7ne", "von", "die\u00b7sem", "Haupt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Worte denk' ich, und in der Seele mir", "tokens": ["Der", "Wor\u00b7te", "denk'", "ich", ",", "und", "in", "der", "See\u00b7le", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "APPR", "ART", "NN", "PPER"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Grollt bittrer Zorn; vom Sarge, d\u00fcnkt mir,", "tokens": ["Grollt", "bit\u00b7trer", "Zorn", ";", "vom", "Sar\u00b7ge", ",", "d\u00fcnkt", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$.", "APPRART", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stiegest empor du in deiner Hoheit,", "tokens": ["Stie\u00b7gest", "em\u00b7por", "du", "in", "dei\u00b7ner", "Ho\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Des Domes S\u00e4ulen st\u00fcrzend und fragend: Wer,", "tokens": ["Des", "Do\u00b7mes", "S\u00e4u\u00b7len", "st\u00fcr\u00b7zend", "und", "fra\u00b7gend", ":", "Wer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "KON", "VVPP", "$.", "PWS", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wer n\u00e4hme mir die Krone vom Kaiserhaupt?", "tokens": ["Wer", "n\u00e4h\u00b7me", "mir", "die", "Kro\u00b7ne", "vom", "Kai\u00b7ser\u00b7haupt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und H\u00e4nde ringend, Tod im Auge,", "tokens": ["Und", "H\u00e4n\u00b7de", "rin\u00b7gend", ",", "Tod", "im", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "