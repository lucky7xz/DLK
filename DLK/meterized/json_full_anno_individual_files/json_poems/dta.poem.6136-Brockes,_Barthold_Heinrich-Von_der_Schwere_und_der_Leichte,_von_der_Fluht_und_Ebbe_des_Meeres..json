{"dta.poem.6136": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Sternen, deren Macht und Kr\u00e4ffte wir versp\u00fcren,", "tokens": ["Von", "Ster\u00b7nen", ",", "de\u00b7ren", "Macht", "und", "Kr\u00e4ff\u00b7te", "wir", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELAT", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist uns die Art, wodurch sie uns regiren,", "tokens": ["Ist", "uns", "die", "Art", ",", "wo\u00b7durch", "sie", "uns", "re\u00b7gi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun nicht mehr unbekannt.", "tokens": ["Nun", "nicht", "mehr", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verborgne Eigenschafft, geheimer Einflu\u00df-Tand", "tokens": ["Ver\u00b7borg\u00b7ne", "Ei\u00b7gen\u00b7schafft", ",", "ge\u00b7hei\u00b7mer", "Ein\u00b7flu\u00df\u00b7Tand"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sind Nahmen, die der Jrrthum lehrte,", "tokens": ["Sind", "Nah\u00b7men", ",", "die", "der", "Jrr\u00b7thum", "lehr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch welche man vordem ein albern Wissen ehrte.", "tokens": ["Durch", "wel\u00b7che", "man", "vor\u00b7dem", "ein", "al\u00b7bern", "Wis\u00b7sen", "ehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}