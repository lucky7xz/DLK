{"dta.poem.3137": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "182.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du trugest, da\u00df der Freund verreist war, ohne Klagen;", "tokens": ["Du", "tru\u00b7gest", ",", "da\u00df", "der", "Freund", "ver\u00b7reist", "war", ",", "oh\u00b7ne", "Kla\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun er gestorben ist, scheint es dir nicht zu tragen.", "tokens": ["Nun", "er", "ge\u00b7stor\u00b7ben", "ist", ",", "scheint", "es", "dir", "nicht", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "So denke doch, er sei verreiset immerfort,", "tokens": ["So", "den\u00b7ke", "doch", ",", "er", "sei", "ver\u00b7rei\u00b7set", "im\u00b7mer\u00b7fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tr\u00f6ste wieder dich des Wiedersehns wie dort.", "tokens": ["Und", "tr\u00f6s\u00b7te", "wie\u00b7der", "dich", "des", "Wie\u00b7der\u00b7sehns", "wie", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ART", "NN", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und ist er nicht verreist? Zwar kommt er nie zur\u00fcck,", "tokens": ["Und", "ist", "er", "nicht", "ver\u00b7reist", "?", "Zwar", "kommt", "er", "nie", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVPP", "$.", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du aber kommst ihm nach, und findest ihn im Gl\u00fcck.", "tokens": ["Du", "a\u00b7ber", "kommst", "ihm", "nach", ",", "und", "fin\u00b7dest", "ihn", "im", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}