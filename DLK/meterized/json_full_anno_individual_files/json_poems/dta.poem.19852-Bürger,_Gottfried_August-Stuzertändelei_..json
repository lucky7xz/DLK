{"dta.poem.19852": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Stuzert\u00e4ndelei .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Freund Amor, kanst du machen,               ", "tokens": ["Freund", "A\u00b7mor", ",", "kanst", "du", "ma\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr einen h\u00fcbschen Kus,", "tokens": ["F\u00fcr", "ei\u00b7nen", "h\u00fcb\u00b7schen", "Kus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df mir Agneschen lachen", "tokens": ["Da\u00df", "mir", "Ag\u00b7ne\u00b7schen", "la\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aus frommen Augen mus?", "tokens": ["Aus", "from\u00b7men", "Au\u00b7gen", "mus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O allerliebste Sachen,", "tokens": ["O", "al\u00b7ler\u00b7liebs\u00b7te", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die kaum ich nennen kan,", "tokens": ["Die", "kaum", "ich", "nen\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schenkt\u2019 ich f\u00fcr dieses Lachen,", "tokens": ["Schenkt'", "ich", "f\u00fcr", "die\u00b7ses", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir, lieber kleiner Man!", "tokens": ["Dir", ",", "lie\u00b7ber", "klei\u00b7ner", "Man", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "ADJA", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In manchem Spiel um Pf\u00e4nder", "tokens": ["In", "man\u00b7chem", "Spiel", "um", "Pf\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab\u2019 ich erobert mir", "tokens": ["Hab'", "ich", "er\u00b7o\u00b7bert", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel sch\u00f6ne bunte B\u00e4nder.", "tokens": ["Viel", "sch\u00f6\u00b7ne", "bun\u00b7te", "B\u00e4n\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die alle g\u00e4b\u2019 ich dir.", "tokens": ["Die", "al\u00b7le", "g\u00e4b'", "ich", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja dies geraubte M\u00fcschgen", "tokens": ["Ja", "dies", "ge\u00b7raub\u00b7te", "M\u00fcschgen"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "PDS", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Empfingest du sogar;", "tokens": ["Emp\u00b7fin\u00b7gest", "du", "so\u00b7gar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dieses Federb\u00fcschgen,", "tokens": ["Und", "die\u00b7ses", "Fe\u00b7der\u00b7b\u00fcschgen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus Minna\u2019s blondem Haar.", "tokens": ["Aus", "Min\u00b7na's", "blon\u00b7dem", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und deinen K\u00f6cher schm\u00fckte", "tokens": ["Und", "dei\u00b7nen", "K\u00f6\u00b7cher", "schm\u00fck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von golddurchwirktem Band\u2019", "tokens": ["Von", "gold\u00b7durch\u00b7wirk\u00b7tem", "Band'"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein R\u00f6schen, welches stikte", "tokens": ["Ein", "R\u00f6\u00b7schen", ",", "wel\u00b7ches", "stik\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des sch\u00f6nsten M\u00e4dchens Hand.", "tokens": ["Des", "sch\u00f6ns\u00b7ten", "M\u00e4d\u00b7chens", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wekst du ihr s\u00fcsses Lachen,", "tokens": ["Wekst", "du", "ihr", "s\u00fcs\u00b7ses", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sieh, so verdienst du dir,", "tokens": ["Sieh", ",", "so", "ver\u00b7dienst", "du", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Nymfen nas zu machen,", "tokens": ["Die", "Nym\u00b7fen", "nas", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die kleine Spr\u00fcze hier.", "tokens": ["Die", "klei\u00b7ne", "Spr\u00fc\u00b7ze", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auch sollen dich belonen,", "tokens": ["Auch", "sol\u00b7len", "dich", "be\u00b7lo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bonbon und Marzipan,", "tokens": ["Bon\u00b7bon", "und", "Mar\u00b7zi\u00b7pan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vortrefliche Makronen,", "tokens": ["Vor\u00b7tre\u00b7fli\u00b7che", "Mak\u00b7ro\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und was dir l\u00fcsten kan.", "tokens": ["Und", "was", "dir", "l\u00fcs\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und siehst du dieses Gl\u00e4schen", "tokens": ["Und", "siehst", "du", "die\u00b7ses", "Gl\u00e4s\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vol Syrakuserwein? \u2014", "tokens": ["Vol", "Sy\u00b7ra\u00b7ku\u00b7ser\u00b7wein", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NE", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erdenke mir ein Sp\u00e4schen!", "tokens": ["Er\u00b7den\u00b7ke", "mir", "ein", "Sp\u00e4sc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du bist ja sonst so fein. \u2014", "tokens": ["Du", "bist", "ja", "sonst", "so", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ha! Kleiner, ich erfinde", "tokens": ["Ha", "!", "Klei\u00b7ner", ",", "ich", "er\u00b7fin\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel eher einen Plan!", "tokens": ["Viel", "e\u00b7her", "ei\u00b7nen", "Plan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den h\u00f6re mir geschwinde", "tokens": ["Den", "h\u00f6\u00b7re", "mir", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit beiden Ohren an!", "tokens": ["Mit", "bei\u00b7den", "Oh\u00b7ren", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "In eine kleine Fliege \u2014", "tokens": ["In", "ei\u00b7ne", "klei\u00b7ne", "Flie\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Siehst du, was ich erfand! \u2014", "tokens": ["Siehst", "du", ",", "was", "ich", "er\u00b7fand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verwandle dich und fliege", "tokens": ["Ver\u00b7wand\u00b7le", "dich", "und", "flie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf ihrer Schn\u00fcrbrust Rand;", "tokens": ["Auf", "ih\u00b7rer", "Schn\u00fcr\u00b7brust", "Rand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Dort gleite durch die Falte,", "tokens": ["Dort", "glei\u00b7te", "durch", "die", "Fal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im zarten Musselin,", "tokens": ["Im", "zar\u00b7ten", "Mus\u00b7se\u00b7lin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis zu dem tiefen Spalte", "tokens": ["Bis", "zu", "dem", "tie\u00b7fen", "Spal\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des warmen Busens hin;", "tokens": ["Des", "war\u00b7men", "Bu\u00b7sens", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dort wage mir hernieder,", "tokens": ["Dort", "wa\u00b7ge", "mir", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geschikt, nach Bergmansart,", "tokens": ["Ge\u00b7schikt", ",", "nach", "Berg\u00b7man\u00b7sart", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geschlossen dein Gefieder,", "tokens": ["Ge\u00b7schlos\u00b7sen", "dein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die wollustvolle Fahrt.", "tokens": ["Die", "wol\u00b7lust\u00b7vol\u00b7le", "Fahrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Dann mus es dir gelingen,", "tokens": ["Dann", "mus", "es", "dir", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr, neidenswehrte M\u00fch!", "tokens": ["Ihr", ",", "nei\u00b7dens\u00b7wehr\u00b7te", "M\u00fch", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein L\u00e4cheln abzuzwingen;", "tokens": ["Ein", "L\u00e4\u00b7cheln", "ab\u00b7zu\u00b7zwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da kizle, kizle sie!", "tokens": ["Da", "kiz\u00b7le", ",", "kiz\u00b7le", "sie", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}