{"textgrid.poem.66311": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Leiche eines Regenten", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid ihr, G\u00f6tter dieser Erde,", "tokens": ["Seid", "ihr", ",", "G\u00f6t\u00b7ter", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "$,", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seid ihr Menschenstaub, wie wir?", "tokens": ["Seid", "ihr", "Men\u00b7schen\u00b7staub", ",", "wie", "wir", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "PPOSAT", "NN", "$,", "PWAV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O! so zittert! Der Gef\u00e4hrte", "tokens": ["O", "!", "so", "zit\u00b7tert", "!", "Der", "Ge\u00b7f\u00e4hr\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eurer Gr\u00f6\u00dfe lieget hier.", "tokens": ["Eu\u00b7rer", "Gr\u00f6\u00b7\u00dfe", "lie\u00b7get", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Steigt von goldnen Stufen nieder", "tokens": ["Steigt", "von", "gold\u00b7nen", "Stu\u00b7fen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu den S\u00e4rgen eurer Br\u00fcder;", "tokens": ["Zu", "den", "S\u00e4r\u00b7gen", "eu\u00b7rer", "Br\u00fc\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denkt beim Leichenpompe heut", "tokens": ["Denkt", "beim", "Lei\u00b7chen\u00b7pom\u00b7pe", "heut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch an eure Sterblichkeit.", "tokens": ["Auch", "an", "eu\u00b7re", "Sterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Habt ihr, wenn der junge Waise", "tokens": ["Habt", "ihr", ",", "wenn", "der", "jun\u00b7ge", "Wai\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor euch klagte, auch geh\u00f6rt?", "tokens": ["Vor", "euch", "klag\u00b7te", ",", "auch", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den fetten Bauch vom Schwei\u00dfe", "tokens": ["Und", "den", "fet\u00b7ten", "Bauch", "vom", "Schwei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer Wittwe nie gen\u00e4hrt?", "tokens": ["Ei\u00b7ner", "Witt\u00b7we", "nie", "ge\u00b7n\u00e4hrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seid ihr willig, reiche Sklaven", "tokens": ["Seid", "ihr", "wil\u00b7lig", ",", "rei\u00b7che", "Skla\u00b7ven"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAIMP", "PPER", "ADJD", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwarzer Laster zu bestrafen?", "tokens": ["Schwar\u00b7zer", "Las\u00b7ter", "zu", "be\u00b7stra\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Helft ihr auch dem Tugendfreund,", "tokens": ["Helft", "ihr", "auch", "dem", "Tu\u00b7gend\u00b7freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wann er h\u00fclflos vor euch weint?", "tokens": ["Wann", "er", "h\u00fcl\u00b7flos", "vor", "euch", "weint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fr\u00f6hnt ihr selber nicht den L\u00fcsten,", "tokens": ["Fr\u00f6hnt", "ihr", "sel\u00b7ber", "nicht", "den", "L\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr scharf an andern straft?", "tokens": ["Die", "ihr", "scharf", "an", "an\u00b7dern", "straft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seid ihr B\u00fcrger, seid ihr Christen?", "tokens": ["Seid", "ihr", "B\u00fcr\u00b7ger", ",", "seid", "ihr", "Chris\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPOSAT", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seid ihr weis' und tugendhaft?", "tokens": ["Seid", "ihr", "weis'", "und", "tu\u00b7gend\u00b7haft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieht man nie von stolzen H\u00f6hen", "tokens": ["Sieht", "man", "nie", "von", "stol\u00b7zen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Euch ver\u00e4chtlich niedersehen?", "tokens": ["Euch", "ver\u00b7\u00e4cht\u00b7lich", "nie\u00b7der\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kennt ihr eure Ritterpflicht? \u2013", "tokens": ["Kennt", "ihr", "eu\u00b7re", "Rit\u00b7ter\u00b7pflicht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "O! so kommt, und zittert nicht.", "tokens": ["O", "!", "so", "kommt", ",", "und", "zit\u00b7tert", "nicht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn hier schlummert ein Regente,", "tokens": ["Denn", "hier", "schlum\u00b7mert", "ein", "Re\u00b7gen\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Verla\u00df'nen Gutes that,", "tokens": ["Der", "Ver\u00b7la\u00df'\u00b7nen", "Gu\u00b7tes", "that", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die richterlichen H\u00e4nde", "tokens": ["Und", "die", "rich\u00b7ter\u00b7li\u00b7chen", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie mit Blut gef\u00e4rbet hat,", "tokens": ["Nie", "mit", "Blut", "ge\u00b7f\u00e4r\u00b7bet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der auf Lasterthaten blitzte,", "tokens": ["Der", "auf", "Las\u00b7ter\u00b7tha\u00b7ten", "blitz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Wittwen Recht besch\u00fctzte,", "tokens": ["Und", "der", "Witt\u00b7wen", "Recht", "be\u00b7sch\u00fctz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der dem Waisen und der Noth", "tokens": ["Der", "dem", "Wai\u00b7sen", "und", "der", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Willig seine H\u00e4nde bot.", "tokens": ["Wil\u00b7lig", "sei\u00b7ne", "H\u00e4n\u00b7de", "bot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Unpartheiisch, wie der Sonne", "tokens": ["Un\u00b7part\u00b7hei\u00b7isch", ",", "wie", "der", "Son\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warmer, segenschwangrer Strahl,", "tokens": ["War\u00b7mer", ",", "se\u00b7gen\u00b7schwang\u00b7rer", "Strahl", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der den Eichen str\u00f6met Wonne,", "tokens": ["Der", "den", "Ei\u00b7chen", "str\u00f6\u00b7met", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dem Veilchen in dem Thal,", "tokens": ["Wie", "dem", "Veil\u00b7chen", "in", "dem", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Strahlt' von seines Stuhles H\u00f6hen", "tokens": ["Strahlt'", "von", "sei\u00b7nes", "Stuh\u00b7les", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Allgemeines Wohlergehen", "tokens": ["All\u00b7ge\u00b7mei\u00b7nes", "Woh\u00b7ler\u00b7ge\u00b7hen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In der Reichen Marmorhaus,", "tokens": ["In", "der", "Rei\u00b7chen", "Mar\u00b7mor\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie in arme H\u00fctten aus.", "tokens": ["Wie", "in", "ar\u00b7me", "H\u00fct\u00b7ten", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Noch in halbentnervten H\u00e4nden", "tokens": ["Noch", "in", "hal\u00b7bent\u00b7nerv\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trug er den Regentenstab,", "tokens": ["Trug", "er", "den", "Re\u00b7gen\u00b7ten\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und das Schwert an schlaffen Lenden,", "tokens": ["Und", "das", "Schwert", "an", "schlaf\u00b7fen", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das Gerechtigkeit ihm gab.", "tokens": ["Das", "Ge\u00b7rech\u00b7tig\u00b7keit", "ihm", "gab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und, wie Helden, wenn sie sterben,", "tokens": ["Und", ",", "wie", "Hel\u00b7den", ",", "wenn", "sie", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "NN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sprach er, ohne zu entf\u00e4rben:", "tokens": ["Sprach", "er", ",", "oh\u00b7ne", "zu", "ent\u00b7f\u00e4r\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott, hier ist die schwere Last,", "tokens": ["Gott", ",", "hier", "ist", "die", "schwe\u00b7re", "Last", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die du mir vertrauet hast.", "tokens": ["Die", "du", "mir", "ver\u00b7trau\u00b7et", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aufgel\u00f6st in Thr\u00e4nen schwanken", "tokens": ["Auf\u00b7ge\u00b7l\u00f6st", "in", "Thr\u00e4\u00b7nen", "schwan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Arme hinter seiner Bahr;", "tokens": ["Ar\u00b7me", "hin\u00b7ter", "sei\u00b7ner", "Bahr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stimmen der Verla\u00dfnen danken", "tokens": ["Stim\u00b7men", "der", "Ver\u00b7la\u00df\u00b7nen", "dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm, der ihre St\u00fctze war.", "tokens": ["Ihm", ",", "der", "ih\u00b7re", "St\u00fct\u00b7ze", "war", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Goldne Zierde deines Standes,", "tokens": ["Gold\u00b7ne", "Zier\u00b7de", "dei\u00b7nes", "Stan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vater unsers Vaterlandes,", "tokens": ["Va\u00b7ter", "un\u00b7sers", "Va\u00b7ter\u00b7lan\u00b7des", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unser unerkauftes Ach", "tokens": ["Un\u00b7ser", "un\u00b7er\u00b7kauf\u00b7tes", "Ach"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fliege deiner Seele nach.", "tokens": ["Flie\u00b7ge", "dei\u00b7ner", "See\u00b7le", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Gro\u00dfe, hebt die Angesichter", "tokens": ["Gro\u00b7\u00dfe", ",", "hebt", "die", "An\u00b7ge\u00b7sich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJA", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber jene Sternenbahn!", "tokens": ["Ue\u00b7ber", "je\u00b7ne", "Ster\u00b7nen\u00b7bahn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dorten trefft ihr euren Richter,", "tokens": ["Dor\u00b7ten", "trefft", "ihr", "eu\u00b7ren", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der \u00e4rms