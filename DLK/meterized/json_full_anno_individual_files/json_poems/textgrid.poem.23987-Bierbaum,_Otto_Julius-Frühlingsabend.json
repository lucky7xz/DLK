{"textgrid.poem.23987": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsabend", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das junge Feld vor mir. Es w\u00e4chst in ihm,", "tokens": ["Das", "jun\u00b7ge", "Feld", "vor", "mir", ".", "Es", "w\u00e4chst", "in", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$.", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die S\u00e4fte steigen stetig auf zum Halm,", "tokens": ["Die", "S\u00e4f\u00b7te", "stei\u00b7gen", "ste\u00b7tig", "auf", "zum", "Halm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Wind bewegt die stille, gr\u00fcne Kraft.", "tokens": ["Kein", "Wind", "be\u00b7wegt", "die", "stil\u00b7le", ",", "gr\u00fc\u00b7ne", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Wald dahinter. Starr der Wipfel Wuchs;", "tokens": ["Der", "Wald", "da\u00b7hin\u00b7ter", ".", "Starr", "der", "Wip\u00b7fel", "Wuchs", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$.", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es zeichnet sich ihr Zackenrand am Himmel,", "tokens": ["Es", "zeich\u00b7net", "sich", "ihr", "Za\u00b7cken\u00b7rand", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tiefdunkel, schwarzgr\u00fcn vor gest\u00e4hltem Blau.", "tokens": ["Tief\u00b7dun\u00b7kel", ",", "schwarz\u00b7gr\u00fcn", "vor", "ge\u00b7st\u00e4hl\u00b7tem", "Blau", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein rosagelber Streifen, lang und schmal,", "tokens": ["Ein", "ro\u00b7sa\u00b7gel\u00b7ber", "Strei\u00b7fen", ",", "lang", "und", "schmal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ruht segnend dr\u00fcber, eine Heilandshand.", "tokens": ["Ruht", "seg\u00b7nend", "dr\u00fc\u00b7ber", ",", "ei\u00b7ne", "Hei\u00b7lands\u00b7hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PAV", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das ist der Friede. Fruchten lebt in ihm.", "tokens": ["Das", "ist", "der", "Frie\u00b7de", ".", "Fruch\u00b7ten", "lebt", "in", "ihm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein einziger Vogel singt im tiefen Wald.", "tokens": ["Ein", "ein\u00b7zi\u00b7ger", "Vo\u00b7gel", "singt", "im", "tie\u00b7fen", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos"