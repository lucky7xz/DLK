{"dta.poem.23332": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Al pari.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Eisenbahnen fahren", "tokens": ["Auf", "Ei\u00b7sen\u00b7bah\u00b7nen", "fah\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwar gern die gro\u00dfen Herrn,", "tokens": ["Zwar", "gern", "die", "gro\u00b7\u00dfen", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch da\u00df auch ", "tokens": ["Doch", "da\u00df", "auch"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Das haben sie nicht gern.", "tokens": ["Das", "ha\u00b7ben", "sie", "nicht", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie k\u00f6nnen auch nicht schneller", "tokens": ["Sie", "k\u00f6n\u00b7nen", "auch", "nicht", "schnel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bei allem Gut und Geld,", "tokens": ["Bei", "al\u00b7lem", "Gut", "und", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht schneller als wir andern", "tokens": ["Nicht", "schnel\u00b7ler", "als", "wir", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "KOKOM", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fortkommen durch die Welt.", "tokens": ["Fort\u00b7kom\u00b7men", "durch", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und nebenbei verdrie\u00dft es", "tokens": ["Und", "ne\u00b7ben\u00b7bei", "ver\u00b7drie\u00dft", "es"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die gro\u00dfen Herrn gar sehr:", "tokens": ["Die", "gro\u00b7\u00dfen", "Herrn", "gar", "sehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ist ja von Vorfahren", "tokens": ["Da", "ist", "ja", "von", "Vor\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Rede gar nicht mehr.", "tokens": ["Die", "Re\u00b7de", "gar", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}