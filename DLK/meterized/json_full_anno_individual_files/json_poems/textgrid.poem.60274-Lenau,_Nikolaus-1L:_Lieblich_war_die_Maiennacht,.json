{"textgrid.poem.60274": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lieblich war die Maiennacht,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieblich war die Maiennacht,", "tokens": ["Lieb\u00b7lich", "war", "die", "Mai\u00b7en\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Silberw\u00f6lklein flogen,", "tokens": ["Sil\u00b7ber\u00b7w\u00f6lk\u00b7lein", "flo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ob der holden Fr\u00fchlingspracht", "tokens": ["Ob", "der", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Freudig hingezogen.", "tokens": ["Freu\u00b7dig", "hin\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Schlummernd lagen Wies und Hain,", "tokens": ["Schlum\u00b7mernd", "la\u00b7gen", "Wies", "und", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder Pfad verlassen;", "tokens": ["Je\u00b7der", "Pfad", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Niemand als der Mondenschein", "tokens": ["Nie\u00b7mand", "als", "der", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wachte auf der Stra\u00dfen.", "tokens": ["Wach\u00b7te", "auf", "der", "Stra\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Leise nur das L\u00fcftchen sprach,", "tokens": ["Lei\u00b7se", "nur", "das", "L\u00fcft\u00b7chen", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es zog gelinder", "tokens": ["Und", "es", "zog", "ge\u00b7lin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Durch das stille Schlafgemach", "tokens": ["Durch", "das", "stil\u00b7le", "Schlaf\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "All der Fr\u00fchlingskinder.", "tokens": ["All", "der", "Fr\u00fch\u00b7lings\u00b7kin\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Heimlich nur das B\u00e4chlein schlich,", "tokens": ["Heim\u00b7lich", "nur", "das", "B\u00e4ch\u00b7lein", "schlich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn der Bl\u00fcten Tr\u00e4ume", "tokens": ["Denn", "der", "Bl\u00fc\u00b7ten", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Dufteten gar wonniglich", "tokens": ["Duf\u00b7te\u00b7ten", "gar", "won\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die stillen R\u00e4ume.", "tokens": ["Durch", "die", "stil\u00b7len", "R\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Rauher war mein Postillion,", "tokens": ["Rau\u00b7her", "war", "mein", "Pos\u00b7til\u00b7li\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Lie\u00df die Gei\u00dfel knallen,", "tokens": ["Lie\u00df", "die", "Gei\u00b7\u00dfel", "knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u00dcber Berg und Tal davon", "tokens": ["\u00dc\u00b7ber", "Berg", "und", "Tal", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Frisch sein Horn erschallen.", "tokens": ["Frisch", "sein", "Horn", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Und von flinken Rossen vier", "tokens": ["Und", "von", "flin\u00b7ken", "Ros\u00b7sen", "vier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "CARD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Scholl der Hufe Schlagen,", "tokens": ["Scholl", "der", "Hu\u00b7fe", "Schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die durchs bl\u00fchende Revier", "tokens": ["Die", "durchs", "bl\u00fc\u00b7hen\u00b7de", "Re\u00b7vier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Trabten mit Behagen.", "tokens": ["Trab\u00b7ten", "mit", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Wald und Flur im schnellen Zug", "tokens": ["Wald", "und", "Flur", "im", "schnel\u00b7len", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaum gegr\u00fc\u00dft \u2013 gemieden;", "tokens": ["Kaum", "ge\u00b7gr\u00fc\u00dft", "\u2013", "ge\u00b7mie\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVPP", "$(", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und vorbei, wie Traumesflug,", "tokens": ["Und", "vor\u00b7bei", ",", "wie", "Trau\u00b7mes\u00b7flug", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "PWAV", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schwand der D\u00f6rfer Frieden.", "tokens": ["Schwand", "der", "D\u00f6r\u00b7fer", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Mitten in dem Maiengl\u00fcck", "tokens": ["Mit\u00b7ten", "in", "dem", "Mai\u00b7en\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lag ein Kirchhof innen,", "tokens": ["Lag", "ein", "Kirch\u00b7hof", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der den raschen Wanderblick", "tokens": ["Der", "den", "ra\u00b7schen", "Wan\u00b7der\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hielt zu ernstem Sinnen.", "tokens": ["Hielt", "zu", "erns\u00b7tem", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Hingelehnt an Bergesrand", "tokens": ["Hin\u00b7ge\u00b7lehnt", "an", "Ber\u00b7ges\u00b7rand"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "War die bleiche Mauer,", "tokens": ["War", "die", "blei\u00b7che", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und das Kreuzbild Gottes stand", "tokens": ["Und", "das", "Kreuz\u00b7bild", "Got\u00b7tes", "stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch, in stummer Trauer.", "tokens": ["Hoch", ",", "in", "stum\u00b7mer", "Trau\u00b7er", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Schwager ritt auf seiner Bahn", "tokens": ["Schwa\u00b7ger", "ritt", "auf", "sei\u00b7ner", "Bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stiller jetzt und tr\u00fcber;", "tokens": ["Stil\u00b7ler", "jetzt", "und", "tr\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Rosse hielt es an,", "tokens": ["Und", "die", "Ros\u00b7se", "hielt", "es", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah zum Kreuz hin\u00fcber:", "tokens": ["Sah", "zum", "Kreuz", "hin\u00b7\u00fc\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbhalten mu\u00df hier Ro\u00df und Rad,", "tokens": ["\u00bb", "hal\u00b7ten", "mu\u00df", "hier", "Ro\u00df", "und", "Rad", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VMFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mags euch nicht gef\u00e4hrden:", "tokens": ["Mags", "euch", "nicht", "ge\u00b7f\u00e4hr\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dr\u00fcben liegt mein Kamerad", "tokens": ["Dr\u00fc\u00b7ben", "liegt", "mein", "Ka\u00b7me\u00b7rad"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der k\u00fchlen Erden!", "tokens": ["In", "der", "k\u00fch\u00b7len", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Ein gar herzlieber Gesell!", "tokens": ["Ein", "gar", "herz\u00b7lie\u00b7ber", "Ge\u00b7sell", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Herr, 's ist ewig schade!", "tokens": ["Herr", ",", "'s", "ist", "e\u00b7wig", "scha\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Keiner blies das Horn so hell", "tokens": ["Kei\u00b7ner", "blies", "das", "Horn", "so", "hell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie mein Kamerade!", "tokens": ["Wie", "mein", "Ka\u00b7me\u00b7ra\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Hier ich immer halten mu\u00df,", "tokens": ["Hier", "ich", "im\u00b7mer", "hal\u00b7ten", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem dort unterm Rasen", "tokens": ["Dem", "dort", "un\u00b7term", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zu