, "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ob der tote Postillion", "tokens": ["Ob", "der", "to\u00b7te", "Pos\u00b7til\u00b7li\u00b7on"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Stimmt' in seine Lieder. \u2013", "tokens": ["Stimmt'", "in", "sei\u00b7ne", "Lie\u00b7der", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Weiter gings durch Feld und Hag", "tokens": ["Wei\u00b7ter", "gings", "durch", "Feld", "und", "Hag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit verh\u00e4ngtem Z\u00fcgel;", "tokens": ["Mit", "ver\u00b7h\u00e4ng\u00b7tem", "Z\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Lang mir noch im Ohre lag", "tokens": ["Lang", "mir", "noch", "im", "Oh\u00b7re", "lag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener Klang vom H\u00fcgel.", "tokens": ["Je\u00b7ner", "Klang", "vom", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "Lieblich war die Maiennacht,", "tokens": ["Lieb\u00b7lich", "war", "die", "Mai\u00b7en\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAF