{"dta.poem.9317": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Dritte Handlung.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Du freundliches liebgen vor zwantzig ducaten/", "tokens": ["Du", "freund\u00b7li\u00b7ches", "lieb\u00b7gen", "vor", "zwant\u00b7zig", "du\u00b7ca\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "CARD", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich wei\u00df/ wo dir die liebe sitzt/", "tokens": ["Ich", "wei\u00df", "/", "wo", "dir", "die", "lie\u00b7be", "sitzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "PPER", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe dich warlich am sch\u00f6nsten berathen/", "tokens": ["Ich", "ha\u00b7be", "dich", "war\u00b7lich", "am", "sch\u00f6ns\u00b7ten", "be\u00b7ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "VVINF", "$("], "meter": "-+--+--+---+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mein gutes geld hat dich erhitzt/", "tokens": ["Mein", "gu\u00b7tes", "geld", "hat", "dich", "er\u00b7hitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das hat mir am besten die bahne gebrochen/", "tokens": ["Das", "hat", "mir", "am", "bes\u00b7ten", "die", "bah\u00b7ne", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKA", "ADJD", "ART", "NN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und alle st\u00fcmper abgestochen.", "tokens": ["Und", "al\u00b7le", "st\u00fcm\u00b7per", "ab\u00b7ge\u00b7sto\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Du qvatschlichtes t\u00e4ubgen/ ich komme mit gelde/", "tokens": ["Du", "qvatschlich\u00b7tes", "t\u00e4ub\u00b7gen", "/", "ich", "kom\u00b7me", "mit", "gel\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "VVFIN", "APPR", "ADJA", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wer wolte mir nicht g\u00fcnstig seyn?", "tokens": ["Wer", "wol\u00b7te", "mir", "nicht", "g\u00fcns\u00b7tig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin zwar ein bauer und komme vom felde/", "tokens": ["Ich", "bin", "zwar", "ein", "bau\u00b7er", "und", "kom\u00b7me", "vom", "fel\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch schie\u00df ich g\u00fcldene kugeln drein/", "tokens": ["Doch", "schie\u00df", "ich", "g\u00fcl\u00b7de\u00b7ne", "ku\u00b7geln", "drein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und suche mit meinem erw\u00fcnschten behagen", "tokens": ["Und", "su\u00b7che", "mit", "mei\u00b7nem", "er\u00b7w\u00fcnschten", "be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die stadt Gal\u00e4ne fort zujagen.", "tokens": ["Die", "stadt", "Ga\u00b7l\u00e4\u00b7ne", "fort", "zu\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ich habe die g\u00fcldenen kugeln verschossen/", "tokens": ["Ich", "ha\u00b7be", "die", "g\u00fcl\u00b7de\u00b7nen", "ku\u00b7geln", "ver\u00b7schos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ach scheu\u00df doch wieder her zu mir/", "tokens": ["Ach", "scheu\u00df", "doch", "wie\u00b7der", "her", "zu", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ADV", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst sterb ich gewi\u00dflich mir selber zum possen/", "tokens": ["Sonst", "sterb", "ich", "ge\u00b7wi\u00df\u00b7lich", "mir", "sel\u00b7ber", "zum", "pos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ADV", "APPRART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und bin ich todt/ wo dien\u2019 ich dir?", "tokens": ["Und", "bin", "ich", "todt", "/", "wo", "dien'", "ich", "dir", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$(", "PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach sage/ mein Vulgus komm hinter die th\u00fcre/", "tokens": ["Ach", "sa\u00b7ge", "/", "mein", "Vul\u00b7gus", "komm", "hin\u00b7ter", "die", "th\u00fc\u00b7re", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$(", "PPOSAT", "NE", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Damit ich allen schmertz verliehre.", "tokens": ["Da\u00b7mit", "ich", "al\u00b7len", "schmertz", "ver\u00b7lieh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}