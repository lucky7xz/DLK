{"textgrid.poem.34291": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "A.", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleines Ding mit M\u00fch und Leiden", "tokens": ["Klei\u00b7nes", "Ding", "mit", "M\u00fch", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier in dieser Brust gepflegt,", "tokens": ["Hier", "in", "die\u00b7ser", "Brust", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herz! wenn sich dein Sturm nicht legt,", "tokens": ["Herz", "!", "wenn", "sich", "dein", "Sturm", "nicht", "legt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PRF", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Herz! wo sind denn deine Freuden?", "tokens": ["Herz", "!", "wo", "sind", "denn", "dei\u00b7ne", "Freu\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine Schl\u00e4ge! wie so selten", "tokens": ["Dei\u00b7ne", "Schl\u00e4\u00b7ge", "!", "wie", "so", "sel\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mischt sich Lust in sie hinein!", "tokens": ["Mischt", "sich", "Lust", "in", "sie", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie schnell sind sie, mit Pein", "tokens": ["Und", "wie", "schnell", "sind", "sie", ",", "mit", "Pein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "VAFIN", "PPER", "$,", "APPR", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Jede Lust mir zu vergelten!", "tokens": ["Je\u00b7de", "Lust", "mir", "zu", "ver\u00b7gel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Phillis! ach nur Augenblicke", "tokens": ["Phil\u00b7lis", "!", "ach", "nur", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lacht, was jeden Unmuth stillt,", "tokens": ["Lacht", ",", "was", "je\u00b7den", "Un\u00b7muth", "stillt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4chelt dein geliebtes Bild", "tokens": ["L\u00e4\u00b7chelt", "dein", "ge\u00b7lieb\u00b7tes", "Bild"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Es von ew'gem Gram zur\u00fccke.", "tokens": ["Es", "von", "ew'\u00b7gem", "Gram", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ganz verwandelt, neu geboren", "tokens": ["Ganz", "ver\u00b7wan\u00b7delt", ",", "neu", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchl ich dann mich, G\u00f6ttern gleich:", "tokens": ["F\u00fchl", "ich", "dann", "mich", ",", "G\u00f6t\u00b7tern", "gleich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPER", "$,", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Welt ein Himmelreich,", "tokens": ["Und", "die", "Welt", "ein", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das du dir zum Sitz erkoren.", "tokens": ["Das", "du", "dir", "zum", "Sitz", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja ein Blick von dir zertheilet", "tokens": ["Ja", "ein", "Blick", "von", "dir", "zer\u00b7thei\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Verzweiflung Nacht in mir,", "tokens": ["Der", "Ver\u00b7zwei\u00b7flung", "Nacht", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mit Riesenschritt zu dir", "tokens": ["Da\u00df", "mit", "Rie\u00b7sen\u00b7schritt", "zu", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Hoffnung siegreich eilet.", "tokens": ["Mei\u00b7ne", "Hoff\u00b7nung", "sieg\u00b7reich", "ei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alles sind mir deine Augen", "tokens": ["Al\u00b7les", "sind", "mir", "dei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was der Erde Sonnenschein,", "tokens": ["Was", "der", "Er\u00b7de", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Trauben ihren Wein,", "tokens": ["Wo", "die", "Trau\u00b7ben", "ih\u00b7ren", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Gesch\u00f6pfe Leben saugen.", "tokens": ["Die", "Ge\u00b7sch\u00f6p\u00b7fe", "Le\u00b7ben", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "K\u00f6nnt ich dir zu f\u00fchlen geben,", "tokens": ["K\u00f6nnt", "ich", "dir", "zu", "f\u00fch\u00b7len", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "All' die Wohlthat deines Blicks!", "tokens": ["All'", "die", "Wohlt\u00b7hat", "dei\u00b7nes", "Blicks", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6pfer meines ganzen Gl\u00fccks,", "tokens": ["Sch\u00f6p\u00b7fer", "mei\u00b7nes", "gan\u00b7zen", "Gl\u00fccks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Spricht er \u00fcber Tod und Leben.", "tokens": ["Spricht", "er", "\u00fc\u00b7ber", "Tod", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aber Angst und Furcht und Schr\u00f6cken", "tokens": ["A\u00b7ber", "Angst", "und", "Furcht", "und", "Schr\u00f6\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueberf\u00e4llt im h\u00f6chsten Wohl", "tokens": ["Ue\u00b7ber\u00b7f\u00e4llt", "im", "h\u00f6chs\u00b7ten", "Wohl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich auf einmal: Phillis! soll", "tokens": ["Mich", "auf", "ein\u00b7mal", ":", "Phil\u00b7lis", "!", "soll"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "APPR", "ADV", "$.", "NE", "$.", "VMFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Diesen Blick einst Nacht bedecken?", "tokens": ["Die\u00b7sen", "Blick", "einst", "Nacht", "be\u00b7de\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sollen diese Zaubermienen,", "tokens": ["Sol\u00b7len", "die\u00b7se", "Zau\u00b7ber\u00b7mie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Liebe ganze Macht", "tokens": ["Wo", "der", "Lie\u00b7be", "gan\u00b7ze", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir das Herz hinweg gelacht,", "tokens": ["Mir", "das", "Herz", "hin\u00b7weg", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APZR", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst dem tr\u00fcben Unmuth dienen?", "tokens": ["Einst", "dem", "tr\u00fc\u00b7ben", "Un\u00b7muth", "die\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Dieser Busen, der mir Triebe", "tokens": ["Die\u00b7ser", "Bu\u00b7sen", ",", "der", "mir", "Trie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Banger Lust entgegen schwoll,", "tokens": ["Ban\u00b7ger", "Lust", "ent\u00b7ge\u00b7gen", "schwoll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll er schwinden? Himmel! soll", "tokens": ["Soll", "er", "schwin\u00b7den", "?", "Him\u00b7mel", "!", "soll"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "$.", "NN", "$.", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn kein Wunsch emp\u00f6ren, Liebe?", "tokens": ["Ihn", "kein", "Wunsch", "em\u00b7p\u00f6\u00b7ren", ",", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVINF", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Phillis, soll sogar dein Feuer", "tokens": ["Phil\u00b7lis", ",", "soll", "so\u00b7gar", "dein", "Feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dein sch\u00f6ner Witz dich fliehn?", "tokens": ["Und", "dein", "sch\u00f6\u00b7ner", "Witz", "dich", "fliehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ungetreue \u2013 sieh mich knien,", "tokens": ["Un\u00b7ge\u00b7treu\u00b7e", "\u2013", "sieh", "mich", "kni\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dennoch bleibst du, bleibst mir theuer.", "tokens": ["Den\u00b7noch", "bleibst", "du", ",", "bleibst", "mir", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "F\u00e4hrt dein Herz nur fort zu schlagen,", "tokens": ["F\u00e4hrt", "dein", "Herz", "nur", "fort", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr das Herz das dich verehrt,", "tokens": ["F\u00fcr", "das", "Herz", "das", "dich", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem du diese Glut gelehrt,", "tokens": ["Dem", "du", "die\u00b7se", "Glut", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie bis in sein Grab zu tragen.", "tokens": ["Sie", "bis", "in", "sein", "Grab", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Ach ich will dich mit Entz\u00fccken,", "tokens": ["Ach", "ich", "will", "dich", "mit", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VMFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn dein Herz nur f\u00fchlbar ist,", "tokens": ["Wenn", "dein", "Herz", "nur", "f\u00fchl\u00b7bar", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst wenn du es nicht mehr bist,", "tokens": ["Selbst", "wenn", "du", "es", "nicht", "mehr", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "PTKNEG", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An des Greisen Schneebrust dr\u00fccken.", "tokens": ["An", "des", "Grei\u00b7sen", "Schnee\u00b7brust", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Auf verwelkten Lippen schweben", "tokens": ["Auf", "ver\u00b7welk\u00b7ten", "Lip\u00b7pen", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Seelen noch vereint,", "tokens": ["Uns\u00b7re", "See\u00b7len", "noch", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn das Auge nicht mehr weint,", "tokens": ["Wenn", "das", "Au\u00b7ge", "nicht", "mehr", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll es doch zu weinen streben.", "tokens": ["Soll", "es", "doch", "zu", "wei\u00b7nen", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Zitternd falten wir die H\u00e4nde", "tokens": ["Zit\u00b7ternd", "fal\u00b7ten", "wir", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ineinander, halb vertaubt,", "tokens":