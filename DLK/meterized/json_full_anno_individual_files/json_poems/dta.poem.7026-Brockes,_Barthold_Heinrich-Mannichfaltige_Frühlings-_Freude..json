{"dta.poem.7026": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mannichfaltige Fr\u00fchlings-  \n Freude.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Liebster GOTT! wie bunt, wie sch\u00f6n,", "tokens": ["Liebs\u00b7ter", "GoTT", "!", "wie", "bunt", ",", "wie", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie so lieblich, klar und helle", "tokens": ["Wie", "so", "lieb\u00b7lich", ",", "klar", "und", "hel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist anjetzo jede Stelle", "tokens": ["Ist", "an\u00b7je\u00b7tzo", "je\u00b7de", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem Welt-Krei\u00df anzusehn!", "tokens": ["Auf", "dem", "Welt\u00b7Krei\u00df", "an\u00b7zu\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie so heiter, licht und rein", "tokens": ["Wie", "so", "hei\u00b7ter", ",", "licht", "und", "rein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "$,", "NN", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist des g\u00fcldnen Morgens Schein,", "tokens": ["Ist", "des", "g\u00fcld\u00b7nen", "Mor\u00b7gens", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der das Firmament erf\u00fcllet!", "tokens": ["Der", "das", "Fir\u00b7ma\u00b7ment", "er\u00b7f\u00fcl\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie so eben, glatt und flach", "tokens": ["Wie", "so", "e\u00b7ben", ",", "glatt", "und", "flach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "$,", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ist der Binsen-reiche Bach,", "tokens": ["Ist", "der", "Bin\u00b7sen\u00b7rei\u00b7che", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Welcher dort in Bluhmen quillet,", "tokens": ["Wel\u00b7cher", "dort", "in", "Bluh\u00b7men", "quil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Recht als ein polierter Spiegel!", "tokens": ["Recht", "als", "ein", "po\u00b7lier\u00b7ter", "Spie\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Seht, wie funckeln dort die H\u00fcgel!", "tokens": ["Seht", ",", "wie", "fun\u00b7ckeln", "dort", "die", "H\u00fc\u00b7gel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Schaut, die Segens-reiche Felder,", "tokens": ["Schaut", ",", "die", "Se\u00b7gens\u00b7rei\u00b7che", "Fel\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wie sie dort das Laub der W\u00e4lder", "tokens": ["Wie", "sie", "dort", "das", "Laub", "der", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Als mit einem Krantz bekr\u00e4ntzen!", "tokens": ["Als", "mit", "ei\u00b7nem", "Krantz", "be\u00b7kr\u00b7\u00e4nt\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Seht, wie dorten Kraut und Gras", "tokens": ["Seht", ",", "wie", "dor\u00b7ten", "Kraut", "und", "Gras"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Vom gefallnen Nacht-Thau na\u00df,", "tokens": ["Vom", "ge\u00b7fall\u00b7nen", "Nacht\u00b7\u00b7T\u00b7hau", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Recht als Edelsteine gl\u00e4ntzen!", "tokens": ["Recht", "als", "E\u00b7del\u00b7stei\u00b7ne", "gl\u00e4nt\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Durch der Bluhmen bunte Pracht,", "tokens": ["Durch", "der", "Bluh\u00b7men", "bun\u00b7te", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Wann auf sie die Sonne strahlet,", "tokens": ["Wann", "auf", "sie", "die", "Son\u00b7ne", "strah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Und so tausend-f\u00e4rbig mahlet,", "tokens": ["Und", "so", "tau\u00b7sen\u00b7df\u00e4r\u00b7big", "mah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Werd ich gleichsam angelacht.", "tokens": ["Werd", "ich", "gleich\u00b7sam", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Doch vor allen andern schien,", "tokens": ["Doch", "vor", "al\u00b7len", "an\u00b7dern", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Recht mit Farben von Rubin,", "tokens": ["Recht", "mit", "Far\u00b7ben", "von", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.25": {"text": "Das geschm\u00fcckte Heer der Rosen", "tokens": ["Das", "ge\u00b7schm\u00fcck\u00b7te", "Heer", "der", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Aug\u2019 und Nase liebzukosen.", "tokens": ["Aug'", "und", "Na\u00b7se", "lieb\u00b7zu\u00b7ko\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "In der aufgekl\u00e4rten Lufft", "tokens": ["In", "der", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Lufft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Schwebt ein angew\u00fcrtzter Dufft,", "tokens": ["Schwebt", "ein", "an\u00b7ge\u00b7w\u00fcrtz\u00b7ter", "Dufft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Den wir mit den sch\u00e4rfsten Augen,", "tokens": ["Den", "wir", "mit", "den", "sch\u00e4rfs\u00b7ten", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Nicht zu sehn, nicht zu entdecken,", "tokens": ["Nicht", "zu", "sehn", ",", "nicht", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Doch zu riechen und zu schmecken,", "tokens": ["Doch", "zu", "rie\u00b7chen", "und", "zu", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Und ihn so zu mercken taugen:", "tokens": ["Und", "ihn", "so", "zu", "mer\u00b7cken", "tau\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Tausend, Tausend, Tausend Arten,", "tokens": ["Tau\u00b7send", ",", "Tau\u00b7send", ",", "Tau\u00b7send", "Ar\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "$,", "CARD", "$,", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Aus dem Feld und aus dem Garten,", "tokens": ["Aus", "dem", "Feld", "und", "aus", "dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Die, aus Bluhmen, Kr\u00e4utern, B\u00fcschen,", "tokens": ["Die", ",", "aus", "Bluh\u00b7men", ",", "Kr\u00e4u\u00b7tern", ",", "B\u00fc\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Wunderbarlich sich vermischen,", "tokens": ["Wun\u00b7der\u00b7bar\u00b7lich", "sich", "ver\u00b7mi\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Suchen ein draufmerckend Hertz,", "tokens": ["Su\u00b7chen", "ein", "drauf\u00b7mer\u00b7ckend", "Hertz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Zu erquicken, zu erfrischen.", "tokens": ["Zu", "er\u00b7qui\u00b7cken", ",", "zu", "er\u00b7fri\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Auch die st\u00e4rckste Specerey", "tokens": ["Auch", "die", "st\u00e4rcks\u00b7te", "Spe\u00b7ce\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Kann so holde Lieblichkeiten", "tokens": ["Kann", "so", "hol\u00b7de", "Lieb\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Dem Gehirn nicht zubereiten,", "tokens": ["Dem", "Ge\u00b7hirn", "nicht", "zu\u00b7be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Als das frisch-gemachte Heu.", "tokens": ["Als", "das", "frischge\u00b7mach\u00b7te", "Heu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Wenn desselben S\u00fcssigkeit,", "tokens": ["Wenn", "des\u00b7sel\u00b7ben", "S\u00fcs\u00b7sig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Mit dem Dufft bethauter Bluhmen,", "tokens": ["Mit", "dem", "Dufft", "be\u00b7thau\u00b7ter", "Bluh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "In der Lufft zusammen fliesset,", "tokens": ["In", "der", "Lufft", "zu\u00b7sam\u00b7men", "flies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Und man dies Gemisch geniesset;", "tokens": ["Und", "man", "dies", "Ge\u00b7misch", "ge\u00b7nies\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDS", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Ist der Balsam aus Jdumen", "tokens": ["Ist", "der", "Bal\u00b7sam", "aus", "Jdu\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.48": {"text": "Nicht so lieblich, nicht so sch\u00f6n.", "tokens": ["Nicht", "so", "lieb\u00b7lich", ",", "nicht", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "$,", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "M\u00f6gt uns solche Eigenschafft,", "tokens": ["M\u00f6gt", "uns", "sol\u00b7che", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Und die angenehme Krafft,", "tokens": ["Und", "die", "an\u00b7ge\u00b7neh\u00b7me", "Krafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "GoTT zum Ruhm, zu Hertzen gehn!", "tokens": ["GoTT", "zum", "Ruhm", ",", "zu", "Hert\u00b7zen", "gehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "M\u00f6gten wir uns doch ergetzen", "tokens": ["M\u00f6g\u00b7ten", "wir", "uns", "doch", "er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "An den unsichtbaren Sch\u00e4tzen,", "tokens": ["An", "den", "un\u00b7sicht\u00b7ba\u00b7ren", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Die der L\u00fcffte weiten Krei\u00df", "tokens": ["Die", "der", "L\u00fcff\u00b7te", "wei\u00b7ten", "Krei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Mit so raren Kr\u00e4fften f\u00fcllen!", "tokens": ["Mit", "so", "ra\u00b7ren", "Kr\u00e4ff\u00b7ten", "f\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "M\u00f6gte doch aus unsrer Brust", "tokens": ["M\u00f6g\u00b7te", "doch", "aus", "uns\u00b7rer", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Auch ein Balsam reiner Lust,", "tokens": ["Auch", "ein", "Bal\u00b7sam", "rei\u00b7ner", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Unserm GOTT zu Ehren, quillen!", "tokens": ["Un\u00b7serm", "GoTT", "zu", "Eh\u00b7ren", ",", "quil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "M\u00f6gte doch durch so viel G\u00fcte,", "tokens": ["M\u00f6g\u00b7te", 