{"textgrid.poem.39469": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Nymphe! sprach Narcissus zu der Quelle,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Nymphe! sprach Narcissus zu der Quelle,", "tokens": ["O", "Nym\u00b7phe", "!", "sprach", "Nar\u00b7cis\u00b7sus", "zu", "der", "Quel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Spiegel! Bett des fern und nahen Lieben!", "tokens": ["Du", "Spie\u00b7gel", "!", "Bett", "des", "fern", "und", "na\u00b7hen", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "NN", "ART", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du Tafel, wo sich Sch\u00f6nheit eingeschrieben,", "tokens": ["Du", "Ta\u00b7fel", ",", "wo", "sich", "Sch\u00f6n\u00b7heit", "ein\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PWAV", "PRF", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und meiner W\u00fcnsch' un\u00fcberstiegne Schwelle!", "tokens": ["Und", "mei\u00b7ner", "W\u00fcn\u00b7sch'", "un\u00b7\u00fc\u00b7bers\u00b7ti\u00b7eg\u00b7ne", "Schwel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht th\u00f6richt mehr umarmend deine Welle", "tokens": ["Nicht", "th\u00f6\u0