{"textgrid.poem.57951": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Monumentalit\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch ist das Rathaus erst halb fertig,", "tokens": ["Noch", "ist", "das", "Rat\u00b7haus", "erst", "halb", "fer\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+++-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die Rennbahnbauschuld nicht gedeckt,", "tokens": ["Die", "Renn\u00b7bahn\u00b7bau\u00b7schuld", "nicht", "ge\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da haben wir bereits schon wieder", "tokens": ["Da", "ha\u00b7ben", "wir", "be\u00b7reits", "schon", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein neues Kommunalprojekt.", "tokens": ["Ein", "neu\u00b7es", "Kom\u00b7mu\u00b7nal\u00b7pro\u00b7jekt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf drei Millionen nur veranschlagt", "tokens": ["Auf", "drei", "Mil\u00b7lion\u00b7en", "nur", "ver\u00b7an\u00b7schlagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird Bellawuppdichs stolzer Bau,", "tokens": ["Wird", "Bel\u00b7la\u00b7wupp\u00b7dichs", "stol\u00b7zer", "Bau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch was der Nachtrag einst wird kosten,", "tokens": ["Doch", "was", "der", "Nach\u00b7trag", "einst", "wird", "kos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df man vorl\u00e4ufig nicht genau.", "tokens": ["Wei\u00df", "man", "vor\u00b7l\u00e4u\u00b7fig", "nicht", "ge\u00b7nau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Monumental, das mu\u00df er werden,", "tokens": ["Mo\u00b7nu\u00b7men\u00b7tal", ",", "das", "mu\u00df", "er", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Monumental ist alles hier,", "tokens": ["Mo\u00b7nu\u00b7men\u00b7tal", "ist", "al\u00b7les", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ganz gleich, ob Rathaus, ob ein H\u00e4uschen,", "tokens": ["Ganz", "gleich", ",", "ob", "Rat\u00b7haus", ",", "ob", "ein", "H\u00e4usc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf dem man liest das W\u00f6rtchen: \u00bbHier!\u00ab", "tokens": ["Auf", "dem", "man", "liest", "das", "W\u00f6rt\u00b7chen", ":", "\u00bb", "Hier",