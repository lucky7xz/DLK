{"textgrid.poem.57402": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der du mir gleich bist, den die Unsterblichen", "genre": "verse", "period": "N.A.", "pub_year": 1747, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du mir gleich bist, den die Unsterblichen", "tokens": ["Der", "du", "mir", "gleich", "bist", ",", "den", "die", "U\u00b7nsterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "ADV", "VAFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6hern Ges\u00e4ngen neben mir auferziehn,", "tokens": ["H\u00f6\u00b7hern", "Ge\u00b7s\u00e4n\u00b7gen", "ne\u00b7ben", "mir", "auf\u00b7er\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Schau mit mir, Schmidt, auf unsrer Freundschaft", "tokens": ["Schau", "mit", "mir", ",", "Schmidt", ",", "auf", "uns\u00b7rer", "Freund\u00b7schaft"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Z\u00e4rtliche Jugend zur\u00fcck und f\u00fchle,", "tokens": ["Z\u00e4rt\u00b7li\u00b7che", "Ju\u00b7gend", "zu\u00b7r\u00fcck", "und", "f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Was du da f\u00fchltest, als in Umarmungen,", "tokens": ["Was", "du", "da", "f\u00fchl\u00b7test", ",", "als", "in", "Um\u00b7ar\u00b7mun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die uns zusegnend der im Olympus sah,", "tokens": ["Die", "uns", "zu\u00b7seg\u00b7nend", "der", "im", "O\u00b7lym\u00b7pus", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "ART", "APPRART", "NE", "VVFIN", "$,"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Dein gro\u00dfes Herz mehr deinem Freunde", "tokens": ["Dein", "gro\u00b7\u00dfes", "Herz", "mehr", "dei\u00b7nem", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als nur gesungene Freundschaft weihte.", "tokens": ["Als", "nur", "ge\u00b7sun\u00b7ge\u00b7ne", "Freund\u00b7schaft", "weih\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Eh wir den Menschen kannten, den g\u00f6ttlichen,", "tokens": ["Eh", "wir", "den", "Men\u00b7schen", "kann\u00b7ten", ",", "den", "g\u00f6tt\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wenn er durch Taten den, der ihn schuf, verehrt,", "tokens": ["Wenn", "er", "durch", "Ta\u00b7ten", "den", ",", "der", "ihn", "schuf", ",", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Den tiefsten P\u00f6bel aller Geister,", "tokens": ["Den", "tiefs\u00b7ten", "P\u00f6\u00b7bel", "al\u00b7ler", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er sich selbst, wenn er Gott verkennet;", "tokens": ["Wenn", "er", "sich", "selbst", ",", "wenn", "er", "Gott", "ver\u00b7ken\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Eh noch des Nachruhms lockender Silberton", "tokens": ["Eh", "noch", "des", "Nach\u00b7ruhms", "lo\u00b7cken\u00b7der", "Sil\u00b7ber\u00b7ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dem Ohre s\u00fc\u00df klang, eh er allm\u00e4chtig uns", "tokens": ["Dem", "Oh\u00b7re", "s\u00fc\u00df", "klang", ",", "eh", "er", "all\u00b7m\u00e4ch\u00b7tig", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit sich im Wirbelstrome fortri\u00df:", "tokens": ["Mit", "sich", "im", "Wir\u00b7bel\u00b7stro\u00b7me", "for\u00b7tri\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Liebten wir uns unbemerkt und gl\u00fccklich.", "tokens": ["Lieb\u00b7ten", "wir", "uns", "un\u00b7be\u00b7merkt", "und", "gl\u00fcck\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Zwar horcht auch oft schon unser fr\u00fch waches Ohr", "tokens": ["Zwar", "horcht", "auch", "oft", "schon", "un\u00b7ser", "fr\u00fch", "wa\u00b7ches", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADV", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht ganz unschuldig, ganz nicht unwissend mehr,", "tokens": ["Nicht", "ganz", "un\u00b7schul\u00b7dig", ",", "ganz", "nicht", "un\u00b7wis\u00b7send", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "$,", "ADV", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn von den liedervollen H\u00fcgeln", "tokens": ["Wenn", "von", "den", "lie\u00b7der\u00b7vol\u00b7len", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dichtern die Ewigkeit l\u00e4chelnd zurief.", "tokens": ["Dich\u00b7tern", "die", "E\u00b7wig\u00b7keit", "l\u00e4\u00b7chelnd", "zu\u00b7rief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Noch jung und furchtsam bebte die Ehrbegier", "tokens": ["Noch", "jung", "und", "furcht\u00b7sam", "beb\u00b7te", "die", "Ehr\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durch unser Herz hin. Freund, dann umarmt ich dich,", "tokens": ["Durch", "un\u00b7ser", "Herz", "hin", ".", "Freund", ",", "dann", "um\u00b7armt", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da hast du mir die sch\u00f6nsten Tr\u00e4nen,", "tokens": ["Da", "hast", "du", "mir", "die", "sch\u00f6ns\u00b7ten", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welche mir jemals mein Herz durchdrungen,", "tokens": ["Wel\u00b7che", "mir", "je\u00b7mals", "mein", "Herz", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Auf meine Wangen jugendlich hingeweint:", "tokens": ["Auf", "mei\u00b7ne", "Wan\u00b7gen", "ju\u00b7gend\u00b7lich", "hin\u00b7ge\u00b7weint", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Tr\u00e4nen der Freundschaft, Tr\u00e4nen der Ehrbegier,", "tokens": ["Tr\u00e4\u00b7nen", "der", "Freund\u00b7schaft", ",", "Tr\u00e4\u00b7nen", "der", "Ehr\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wenn du mit seelenvollem Auge", "tokens": ["Wenn", "du", "mit", "see\u00b7len\u00b7vol\u00b7lem", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald mich umarmtest, bald Miltons Schatten", "tokens": ["Bald", "mich", "um\u00b7arm\u00b7test", ",", "bald", "Mil\u00b7tons", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "$,", "ADV", "NE", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Auf heilgern Bergen, als der Parnassus ist,", "tokens": ["Auf", "heil\u00b7gern", "Ber\u00b7gen", ",", "als", "der", "Par\u00b7nas\u00b7sus", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von Seraphinen und von Uranien", "tokens": ["Von", "Se\u00b7ra\u00b7phi\u00b7nen", "und", "von", "U\u00b7ra\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allein besucht, sahst, menscheneinsam", "tokens": ["Al\u00b7lein", "be\u00b7sucht", ",", "sahst", ",", "men\u00b7schen\u00b7ein\u00b7sam"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "$,", "NE"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Unnachgeahmt ohne Nebenbuhler.", "tokens": ["Un\u00b7nach\u00b7ge\u00b7ahmt", "oh\u00b7ne", "Ne\u00b7ben\u00b7buh\u00b7ler", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}}, "stanza.9": {"line.1": {"text": "Ich sah dich still an, und nur Uranien", "tokens": ["Ich", "sah", "dich", "still", "an", ",", "und", "nur", "U\u00b7ra\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KON", "ADV", "NN"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Allein bemerket, dir aber unbemerkt,", "tokens": ["Al\u00b7lein", "be\u00b7mer\u00b7ket", ",", "dir", "a\u00b7ber", "un\u00b7be\u00b7merkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Weissagend, in prophet'schem Geiste,", "tokens": ["Weis\u00b7sa\u00b7gend", ",", "in", "pro\u00b7phet'\u00b7schem", "Geis\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Segnet ich, Schmidt, dich zum heilgen Dichter.", "tokens": ["Seg\u00b7net", "ich", ",", "Schmidt", ",", "dich", "zum", "heil\u00b7gen", "Dich\u00b7ter."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PPER", "$,", "NE", "$,", "PRF", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Der du mir gleich bist, den die Unsterblichen", "tokens": ["Der", "du", "mir", "gleich", "bist", ",", "den", "die", "U\u00b7nsterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "ADV", "VAFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2"