{"dta.poem.1945": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uberschrifften und Sinn-  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Es lies um ihren Mann Mannholde Thr\u00e4nen fl\u00fcssen/", "tokens": ["Es", "lies", "um", "ih\u00b7ren", "Mann", "Mann\u00b7hol\u00b7de", "Thr\u00e4\u00b7nen", "fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schwur/ sie wolte sich nicht weiter ehlich wissen/", "tokens": ["Und", "schwur", "/", "sie", "wol\u00b7te", "sich", "nicht", "wei\u00b7ter", "eh\u00b7lich", "wis\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PPER", "VMFIN", "PRF", "PTKNEG", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "och als die Leinwand sie zum Sterbek\u00fcttel schneid/", "tokens": ["och", "als", "die", "Lein\u00b7wand", "sie", "zum", "Ster\u00b7be\u00b7k\u00fct\u00b7tel", "schneid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "rieff/ auff Befehl der Hand/ die Scheere nicht zu weit.", "tokens": ["rieff", "/", "auff", "Be\u00b7fehl", "der", "Hand", "/", "die", "Schee\u00b7re", "nicht", "zu", "weit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "APPR", "NN", "ART", "NN", "$(", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "ie ward befragt/ warum sie spart in diesen Sachen:", "tokens": ["ie", "ward", "be\u00b7fragt", "/", "wa\u00b7rum", "sie", "spart", "in", "die\u00b7sen", "Sa\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$(", "PWAV", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "amit man \u00fcbers Jahr kan Hochzeit-Hemde machen.", "tokens": ["a\u00b7mit", "man", "\u00fc\u00b7bers", "Jahr", "kan", "Hoch\u00b7zeit\u00b7Hem\u00b7de", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}