{"textgrid.poem.25722": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00dft den Wagen halten oder fahren!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft den Wagen halten oder fahren!", "tokens": ["La\u00dft", "den", "Wa\u00b7gen", "hal\u00b7ten", "o\u00b7der", "fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denn ich bleib' hier sitzen. Dieser Stein", "tokens": ["Denn", "ich", "bleib'", "hier", "sit\u00b7zen", ".", "Die\u00b7ser", "Stein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "VVINF", "$.", "PDAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Soll des Mannes, der an seiner Schaaren", "tokens": ["Soll", "des", "Man\u00b7nes", ",", "der", "an", "sei\u00b7ner", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Spitze hier geblutet, Altar seyn!", "tokens": ["Spit\u00b7ze", "hier", "ge\u00b7blu\u00b7tet", ",", "Al\u00b7tar", "seyn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Thr\u00e4nen will ich opfern. Denn von allen", "tokens": ["Thr\u00e4\u00b7nen", "will", "ich", "op\u00b7fern", ".", "Denn", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$.", "KON", "APPR", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "K\u00f6nigen, die vor ihm, wie nachher,", "tokens": ["K\u00f6\u00b7ni\u00b7gen", ",", "die", "vor", "ihm", ",", "wie", "nach\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "$,", "PWAV", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Unterm Schwert' des Krieges sind gefallen,", "tokens": ["Un\u00b7term", "Schwert'", "des", "Krie\u00b7ges", "sind", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "War nur ", "tokens": ["War", "nur"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Auf die Stelle, die sein Blut getrunken,", "tokens": ["Auf", "die", "Stel\u00b7le", ",", "die", "sein", "Blut", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "W\u00e4lzten Bauren weinend diesen Stein,", "tokens": ["W\u00e4lz\u00b7ten", "Bau\u00b7ren", "wei\u00b7nend", "die\u00b7sen", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "In die Erde halb schon itzt versunken!", "tokens": ["In", "die", "Er\u00b7de", "halb", "schon", "itzt", "ver\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Und ich kann ihm keinen bessern weihn!", "tokens": ["Und", "ich", "kann", "ihm", "kei\u00b7nen", "bes\u00b7sern", "weihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PIAT", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Solch ein Denkmal f\u00fcr das gro\u00dfe Leben", "tokens": ["Solch", "ein", "Denk\u00b7mal", "f\u00fcr", "das", "gro\u00b7\u00dfe", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Dieses Retters einer halben Welt! \u2013", "tokens": ["Die\u00b7ses", "Ret\u00b7ters", "ei\u00b7ner", "hal\u00b7ben", "Welt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Murren m\u00f6cht ich, (mag mir's Gott vergeben!)", "tokens": ["Mur\u00b7ren", "m\u00f6cht", "ich", ",", "(", "mag", "mir's", "Gott", "ver\u00b7ge\u00b7ben", "!", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "$(", "VMFIN", "NE", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Da\u00df die Armuth mich gefesselt h\u00e4lt.", "tokens": ["Da\u00df", "die", "Ar\u00b7muth", "mich", "ge\u00b7fes\u00b7selt", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Zwar bedarf er nicht der Marmors\u00e4ule,", "tokens": ["Zwar", "be\u00b7darf", "er", "nicht", "der", "Mar\u00b7mor\u00b7s\u00e4u\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Die hier stehen sollte: aber dann", "tokens": ["Die", "hier", "ste\u00b7hen", "soll\u00b7te", ":", "a\u00b7ber", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "VVINF", "VMFIN", "$.", "ADV", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Ruhte hier der Wandrer eine Weile,", "tokens": ["Ruh\u00b7te", "hier", "der", "Wand\u00b7rer", "ei\u00b7ne", "Wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "L\u00e4s' und segnete den braven Mann!", "tokens": ["L\u00e4s'", "und", "seg\u00b7ne\u00b7te", "den", "bra\u00b7ven", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "L\u00e4s' und weinte seinen Muth wohl gr\u00f6\u00dfer,", "tokens": ["L\u00e4s'", "und", "wein\u00b7te", "sei\u00b7nen", "Muth", "wohl", "gr\u00f6\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Seinen Stolz geringer durch die Scham!", "tokens": ["Sei\u00b7nen", "Stolz", "ge\u00b7rin\u00b7ger", "durch", "die", "Scham", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"