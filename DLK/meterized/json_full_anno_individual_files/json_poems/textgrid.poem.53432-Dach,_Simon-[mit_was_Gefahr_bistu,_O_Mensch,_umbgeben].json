{"textgrid.poem.53432": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[mit was Gefahr bistu, O Mensch, umbgeben]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit was Gefahr bistu, O Mensch, umbgeben!", "tokens": ["Mit", "was", "Ge\u00b7fahr", "bis\u00b7tu", ",", "O", "Mensch", ",", "umb\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "VVFIN", "$,", "NE", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Augenblick h\u00e4lt dich vom Tode frey,", "tokens": ["Kein", "Au\u00b7gen\u00b7blick", "h\u00e4lt", "dich", "vom", "To\u00b7de", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch sorgest du so wenig f\u00fcr dein Leben,", "tokens": ["Noch", "sor\u00b7gest", "du", "so", "we\u00b7nig", "f\u00fcr", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und da\u00df dein Ziel dir auff dem Nacken sey.", "tokens": ["Und", "da\u00df", "dein", "Ziel", "dir", "auff", "dem", "Na\u00b7cken", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein blosser Dampff, ein ungewisser Tritt,", "tokens": ["Ein", "blos\u00b7ser", "Dampff", ",", "ein", "un\u00b7ge\u00b7wis\u00b7ser", "Tritt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wer gl\u00e4ubt es wol, kan unser Letztes seyn,", "tokens": ["Wer", "gl\u00e4ubt", "es", "wol", ",", "kan", "un\u00b7ser", "Letz\u00b7tes", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "VMFIN", "PPOSAT", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Todt dringt sich bey allen Sachen ein,", "tokens": ["Der", "Todt", "dringt", "sich", "bey", "al\u00b7len", "Sa\u00b7chen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du nimbst ihn stets zu Tisch und Bette mit.", "tokens": ["Du", "nimbst", "ihn", "stets", "zu", "Tisch", "und", "Bet\u00b7te", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Kriegsmann meynt, er mu\u00df im Streit erliegen,", "tokens": ["Ein", "Kriegs\u00b7mann", "meynt", ",", "er", "mu\u00df", "im", "Streit", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Schiffer wo durch Sturm auff wildem Meer,", "tokens": ["Ein", "Schif\u00b7fer", "wo", "durch", "Sturm", "auff", "wil\u00b7dem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Wandersmann, der M\u00f6rder werd' ihn kriegen,", "tokens": ["Ein", "Wan\u00b7ders\u00b7mann", ",", "der", "M\u00f6r\u00b7der", "werd'", "ihn", "krie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein andrer scheut das Ende sonst woher.", "tokens": ["Ein", "an\u00b7drer", "scheut", "das", "En\u00b7de", "sonst", "wo\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich meide Gift, Emp\u00f6rung, Schiffbruch, Schlacht,", "tokens": ["Ich", "mei\u00b7de", "Gift", ",", "Em\u00b7p\u00f6\u00b7rung", ",", "Schiff\u00b7bruch", ",", "Schlacht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Pest, Hunger, Brandt, Verr\u00e4terey und Mord,", "tokens": ["Pest", ",", "Hun\u00b7ger", ",", "Brandt", ",", "Ver\u00b7r\u00e4\u00b7te\u00b7rey", "und", "Mord", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und was ich kan, und endlich mu\u00df ich fort", "tokens": ["Und", "was", "ich", "kan", ",", "und", "end\u00b7lich", "mu\u00df", "ich", "fort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VMFIN", "$,", "KON", "ADV", "VMFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Durch einen Tod, auff den ich nie gedacht.", "tokens": ["Durch", "ei\u00b7nen", "Tod", ",", "auff", "den", "ich", "nie", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der fromme Storch kennt seine Zeit gar eben,", "tokens": ["Der", "from\u00b7me", "Storch", "kennt", "sei\u00b7ne", "Zeit", "gar", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne wei\u00df, wenn nun ihr klares Licht", "tokens": ["Die", "Son\u00b7ne", "wei\u00df", ",", "wenn", "nun", "ihr", "kla\u00b7res", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umb Abendzeit uns gute Nacht sol geben,", "tokens": ["Umb", "A\u00b7bend\u00b7zeit", "uns", "gu\u00b7te", "Nacht", "sol", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PPER", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der arme Mensch wei\u00df umb sein Ende nicht;", "tokens": ["Der", "ar\u00b7me", "Mensch", "wei\u00df", "umb", "sein", "En\u00b7de", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie in dem Netz' ein Vogel sich verstrickt,", "tokens": ["Wie", "in", "dem", "Netz'", "ein", "Vo\u00b7gel", "sich", "ver\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wie ein Fisch ins Garn k\u00f6mmt ohn gefehr,", "tokens": ["Und", "wie", "ein", "Fisch", "ins", "Garn", "k\u00f6mmt", "ohn", "ge\u00b7fehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So werden wir (ach Jammer!) hin und her", "tokens": ["So", "wer\u00b7den", "wir", "(", "ach", "Jam\u00b7mer", "!", ")", "hin", "und", "her"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$(", "XY", "NN", "$.", "$(", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gantz unverhofft zur b\u00f6sen Zeit bedr\u00fcckt.", "tokens": ["Gantz", "un\u00b7ver\u00b7hofft", "zur", "b\u00f6\u00b7sen", "Zeit", "be\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kein einigs Ding kan auff der weiten Erden", "tokens": ["Kein", "ei\u00b7nigs", "Ding", "kan", "auff", "der", "wei\u00b7ten", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewisser seyn als unser aller Tod.", "tokens": ["Ge\u00b7wis\u00b7ser", "seyn", "als", "un\u00b7ser", "al\u00b7ler", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "KOKOM", "PPOSAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So ungewi\u00df kan nichts gefunden werden", "tokens": ["So", "un\u00b7ge\u00b7wi\u00df", "kan", "nichts", "ge\u00b7fun\u00b7den", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "PIS", "VVPP", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als zwar die Zeit und Stunde solcher Noht,", "tokens": ["Als", "zwar", "die", "Zeit", "und", "Stun\u00b7de", "sol\u00b7cher", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "KON", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Blo\u00df da\u00df wir stets zu sterben fertig seyn,", "tokens": ["Blo\u00df", "da\u00df", "wir", "stets", "zu", "ster\u00b7ben", "fer\u00b7tig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PTKZU", "VVINF", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "(erkenn auch hie des H\u00f6chsten Lieb und Rath)", "tokens": ["(", "er\u00b7kenn", "auch", "hie", "des", "H\u00f6chs\u00b7ten", "Lieb", "und", "Rath", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Sicherheit nicht finde bey uns statt,", "tokens": ["Und", "Si\u00b7cher\u00b7heit", "nicht", "fin\u00b7de", "bey", "uns", "statt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die auff uns bringt des andern Todes Pein.", "tokens": ["Die", "auff", "uns", "bringt", "des", "an\u00b7dern", "To\u00b7des", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ach Herr, lehr uns di\u00df alles wol bedencken,", "tokens": ["Ach", "Herr", ",", "lehr", "uns", "di\u00df", "al\u00b7les", "wol", "be\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVIMP", "PPER", "PDS", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bring uns die Flucht des Lebens gr\u00fcndlich bey,", "tokens": ["Bring", "uns", "die", "Flucht", "des", "Le\u00b7bens", "gr\u00fcnd\u00b7lich", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Damit wir uns zur rechten Klugheit lencken", "tokens": ["Da\u00b7mit", "wir", "uns", "zur", "rech\u00b7ten", "Klug\u00b7heit", "len\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch steten Ha\u00df der Welt und wahre Reu,", "tokens": ["Durch", "ste\u00b7ten", "Ha\u00df", "der", "Welt", "und", "