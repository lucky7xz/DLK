{"textgrid.poem.36224": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Farbensinnbild", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df edlen Mut den wei\u00dfen Altar gr\u00fcnden,", "tokens": ["La\u00df", "ed\u00b7len", "Mut", "den", "wei\u00b7\u00dfen", "Al\u00b7tar", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hoch Fantasie in Purpurflammen wehen,", "tokens": ["Hoch", "Fan\u00b7ta\u00b7sie", "in", "Pur\u00b7pur\u00b7flam\u00b7men", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Liebe wirst du bald im Zentrum sehen,", "tokens": ["Und", "Lie\u00b7be", "wirst", "du", "bald", "im", "Zent\u00b7rum", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo gr\u00fcn die Feuers\u00e4ulen sich entz\u00fcnden;", "tokens": ["Wo", "gr\u00fcn", "die", "Feu\u00b7er\u00b7s\u00e4u\u00b7len", "sich", "ent\u00b7z\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Durch braune Locken wird sich Myrthe winden,", "tokens": ["Durch", "brau\u00b7ne", "Lo\u00b7cken", "wird", "sich", "Myr\u00b7the", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Freund mit goldnen Fr\u00fcchten vor dir stehen,", "tokens": ["Der", "Freund", "mit", "gold\u00b7nen", "Fr\u00fcch\u00b7ten", "vor", "dir", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Kinder dann in Blumen zu dir gehen,", "tokens": ["Die", "Kin\u00b7der", "dann", "in", "Blu\u00b7men", "zu", "dir", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Ros' und Lorbeer dich die Schwester binden.", "tokens": ["Mit", "Ros'", "und", "Lor\u00b7beer", "dich", "die", "Schwes\u00b7ter", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es war der alten Maler gute Sitte,", "tokens": ["Es", "war", "der", "al\u00b7ten", "Ma\u00b7ler", "gu\u00b7te", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Bildes Sinn mit einem Strich zu sagen,", "tokens": ["Des", "Bil\u00b7des", "Sinn", "mit", "ei\u00b7nem", "Strich", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der den Akkord der Farben drunter schriebe;", "tokens": ["Der", "den", "Ak\u00b7kord", "der", "Far\u00b7ben", "drun\u00b7ter", "schrie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So mag auch dieses Lied es k\u00fchnlich wagen,", "tokens": ["So", "mag", "auch", "die\u00b7ses", "Lied", "es", "k\u00fchn\u00b7lich", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PDAT", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu deuten auf der Dichtung innre Mitte,", "tokens": ["Zu", "deu\u00b7ten", "auf", "der", "Dich\u00b7tung", "inn\u00b7re", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Farben spielend um die s\u00fc\u00dfe Liebe.", "tokens": ["In", "Far\u00b7ben", "spie\u00b7lend", "um", "die", "s\u00fc\u00b7\u00dfe", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "La\u00df edlen Mut den wei\u00dfen Altar gr\u00fcnden,", "tokens": ["La\u00df", "ed\u00b7len", "Mut", "den", "wei\u00b7\u00dfen", "Al\u00b7tar", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iam