{"textgrid.poem.55456": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "36", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gingst vor\u00fcber? Wie! ich sah dich nicht;", "tokens": ["Du", "gingst", "vor\u00b7\u00fc\u00b7ber", "?", "Wie", "!", "ich", "sah", "dich", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PWAV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du kamst zur\u00fcck, dich hab ich nicht gesehen \u2013", "tokens": ["Du", "kamst", "zu\u00b7r\u00fcck", ",", "dich", "hab", "ich", "nicht", "ge\u00b7se\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verlorner, ungl\u00fccksel'ger Augenblick!", "tokens": ["Ver\u00b7lor\u00b7ner", ",", "un\u00b7gl\u00fcck\u00b7sel'\u00b7ger", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bin ich denn blind? Wie soll mir das geschehen?", "tokens": ["Bin", "ich", "denn", "blind", "?", "Wie", "soll", "mir", "das", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$.", "PWAV", "VMFIN", "PPER", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch tr\u00f6st ich mich, und du verzeihst mir gern,", "tokens": ["Doch", "tr\u00f6st", "ich", "mich", ",", "und", "du", "ver\u00b7zeihst", "mir", "gern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entschuldigung wirst du mit Freude finden;", "tokens": ["Ent\u00b7schul\u00b7di\u00b7gung", "wirst", "du", "mit", "Freu\u00b7de", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sehe dich, bist du auch noch so fern!", "tokens": ["Ich", "se\u00b7he", "dich", ",", "bist", "du", "auch", "noch", "so", "fern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und in der N\u00e4he kannst du mir verschwinden.", "tokens": ["Und", "in", "der", "N\u00e4\u00b7he", "kannst", "du", "mir", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du gingst vor\u00fcber? Wie! ich sah dich nicht;", "tokens": ["Du", "gingst", "vor\u00b7\u00fc\u00b7ber", "?", "Wie", "!", "ich", "sah", "dich", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PWAV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du kamst zur\u0