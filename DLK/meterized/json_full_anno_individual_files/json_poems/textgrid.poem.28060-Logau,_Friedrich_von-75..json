{"textgrid.poem.28060": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Wunderding, der durch zehn, zwantzig Jahre", "tokens": ["Es", "ist", "ein", "Wun\u00b7der\u00b7ding", ",", "der", "durch", "zehn", ",", "zwant\u00b7zig", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "CARD", "$,", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4nger nicht gewust, was rechter Glaube war,", "tokens": ["Und", "l\u00e4n\u00b7ger", "nicht", "ge\u00b7wust", ",", "was", "rech\u00b7ter", "Glau\u00b7be", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "VVPP", "$,", "PWS", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann der vom ersten trit und nimmt den andren an,", "tokens": ["Wann", "der", "vom", "ers\u00b7ten", "trit", "und", "nimmt", "den", "an\u00b7dren", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPRART", "ADJA", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df der bald alles wei\u00df und andre lehren kan.", "tokens": ["Da\u00df", "der", "bald", "al\u00b7les", "wei\u00df", "und", "and\u00b7re", "leh\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "PIS", "VVFIN", "KON", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mich d\u00fcnckt Gunst, Ehre, Macht, Gemach und gute Bissen", "tokens": ["Mich", "d\u00fcnckt", "Gunst", ",", "Eh\u00b7re", ",", "Macht", ",", "Ge\u00b7mach", "und", "gu\u00b7te", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die st\u00e4rcken ihm das Hirn, nicht aber das Gewissen.", "tokens": ["Die", "st\u00e4r\u00b7cken", "ihm", "das", "Hirn", ",", "nicht", "a\u00b7ber", "das", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$,", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist ein Wunderding, der durch zehn, zwantzig Jahre", "tokens": ["Es", "ist", "ein", "Wun\u00b7der\u00b7ding", ",", "der", "durch", "zehn", ",", "zwant\u00b7zig", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "CARD", "$,", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und l\u00e4nger nicht gewust, was rechter Glaube war,", "tokens": ["Und", "l\u00e4n\u00b7ger", "nicht", "ge\u00b7wust", ",", "was", "rech\u00b7ter", "Glau\u00b7be", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKNEG", "VVPP", "$,", "PWS", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann der vom ersten trit und n