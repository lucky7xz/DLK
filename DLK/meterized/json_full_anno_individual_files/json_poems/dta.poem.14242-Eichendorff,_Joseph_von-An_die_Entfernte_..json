{"dta.poem.14242": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Entfernte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Denk ich, Du Stille, an Dein ruhig Walten,", "tokens": ["Denk", "ich", ",", "Du", "Stil\u00b7le", ",", "an", "Dein", "ru\u00b7hig", "Wal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPER", "NN", "$,", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An jenes letzten Abends rothe K\u00fchle,", "tokens": ["An", "je\u00b7nes", "letz\u00b7ten", "A\u00b7bends", "ro\u00b7the", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo ich die theure Hand noch durfte halten:", "tokens": ["Wo", "ich", "die", "theu\u00b7re", "Hand", "noch", "durf\u00b7te", "hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Steh' ich oft sinnend stille im Gew\u00fchle,", "tokens": ["Steh'", "ich", "oft", "sin\u00b7nend", "stil\u00b7le", "im", "Ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und, wie den Schweizer heim'sche Alphornslieder", "tokens": ["Und", ",", "wie", "den", "Schwei\u00b7zer", "heim'\u00b7sche", "Al\u00b7phorns\u00b7lie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf fremden Bergen, fern den Freunden allen,", "tokens": ["Auf", "frem\u00b7den", "Ber\u00b7gen", ",", "fern", "den", "Freun\u00b7den", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Oft unverhofft befallen,", "tokens": ["Oft", "un\u00b7ver\u00b7hofft", "be\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kommt tiefe Sehnsucht pl\u00f6tzlich auf mich nieder.", "tokens": ["Kommt", "tie\u00b7fe", "Sehn\u00b7sucht", "pl\u00f6tz\u00b7lich", "auf", "mich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}