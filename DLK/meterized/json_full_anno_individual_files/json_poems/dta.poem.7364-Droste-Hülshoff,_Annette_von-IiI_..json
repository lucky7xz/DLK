{"dta.poem.7364": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "IiI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Zu C\u00f6ln am Rheine kniet ein Weib", "tokens": ["Zu", "C\u00f6ln", "am", "Rhei\u00b7ne", "kniet", "ein", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Rabensteine unter'm Rade,", "tokens": ["Am", "Ra\u00b7ben\u00b7stei\u00b7ne", "un\u00b7ter'm", "Ra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber'm Rade liegt ein Leib,", "tokens": ["Und", "\u00fc\u00b7ber'm", "Ra\u00b7de", "liegt", "ein", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An dem sich weiden Kr\u00e4h' und Made;", "tokens": ["An", "dem", "sich", "wei\u00b7den", "Kr\u00e4h'", "und", "Ma\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zerbrochen ist sein Wappenschild,", "tokens": ["Zer\u00b7bro\u00b7chen", "ist", "sein", "Wap\u00b7pen\u00b7schild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Tr\u00fcmmern seine Burg gef\u00fcllt,", "tokens": ["Mit", "Tr\u00fcm\u00b7mern", "sei\u00b7ne", "Burg", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Seele steht bei Gottes Gnade.", "tokens": ["Die", "See\u00b7le", "steht", "bei", "Got\u00b7tes", "Gna\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den Leib des F\u00fcrsten h\u00fcllt der Rauch", "tokens": ["Den", "Leib", "des", "F\u00fcrs\u00b7ten", "h\u00fcllt", "der", "Rauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Ampeln und von Weihrauchschwehlen \u2014", "tokens": ["Von", "Am\u00b7peln", "und", "von", "Weih\u00b7rauch\u00b7schweh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um seinen qualmt der Moderhauch", "tokens": ["Um", "sei\u00b7nen", "qualmt", "der", "Mo\u00b7der\u00b7hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Hagel peitscht der Rippen H\u00f6hlen;", "tokens": ["Und", "Ha\u00b7gel", "peitscht", "der", "Rip\u00b7pen", "H\u00f6h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Im Dome steigt ein Trauerchor,", "tokens": ["Im", "Do\u00b7me", "steigt", "ein", "Trau\u00b7er\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ein Tedeum stieg empor", "tokens": ["Und", "ein", "Te\u00b7de\u00b7um", "stieg", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Bei seiner Qual aus tausend Kehlen.", "tokens": ["Bei", "sei\u00b7ner", "Qual", "aus", "tau\u00b7send", "Keh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn das Rad der B\u00fcrger sieht,", "tokens": ["Und", "wenn", "das", "Rad", "der", "B\u00fcr\u00b7ger", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann l\u00e4\u00dft er rasch sein R\u00f6\u00dflein traben,", "tokens": ["Dann", "l\u00e4\u00dft", "er", "rasch", "sein", "R\u00f6\u00df\u00b7lein", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch eine bleiche Frau die kniet,", "tokens": ["Doch", "ei\u00b7ne", "blei\u00b7che", "Frau", "die", "kniet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und scheucht mit ihrem Tuch die Raben:", "tokens": ["Und", "scheucht", "mit", "ih\u00b7rem", "Tuch", "die", "Ra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Um sie mied er die Schlinge nicht,", "tokens": ["Um", "sie", "mied", "er", "die", "Schlin\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Er war ihr Held, er war ihr Licht \u2014", "tokens": ["Er", "war", "ihr", "Held", ",", "er", "war", "ihr", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ach, der Vater ihrer Knaben!", "tokens": ["Und", "ach", ",", "der", "Va\u00b7ter", "ih\u00b7rer", "Kna\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}