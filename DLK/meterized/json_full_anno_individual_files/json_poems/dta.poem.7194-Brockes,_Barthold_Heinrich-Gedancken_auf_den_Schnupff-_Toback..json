{"dta.poem.7194": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken auf den Schnupff-  \n Toback.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, was wunderns wehrt, man nahet ihn zum Hirn, dem", "tokens": ["Ja", ",", "was", "wun\u00b7derns", "wehrt", ",", "man", "na\u00b7het", "ihn", "zum", "Hirn", ",", "dem"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "PWS", "NN", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "APPRART", "NN", "$,", "PRELS"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Sitz der Seelen:", "tokens": ["Sitz", "der", "See\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Necht, als suche man mit ihm den Verstand selbst zu verm\u00e4hlen.", "tokens": ["Necht", ",", "als", "su\u00b7che", "man", "mit", "ihm", "den", "Ver\u00b7stand", "selbst", "zu", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADJA", "PIS", "APPR", "PPER", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Scheint es nicht, als wenn der Trieb uns dadurch erinnern", "tokens": ["Scheint", "es", "nicht", ",", "als", "wenn", "der", "Trieb", "uns", "da\u00b7durch", "e\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOKOM", "KOUS", "ART", "NN", "PPER", "PAV", "VVINF"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "wolle,", "tokens": ["wol\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Da\u00df die Seele diese Wahrheit \u00f6ffters \u00fcberlegen solle:", "tokens": ["Da\u00df", "die", "See\u00b7le", "die\u00b7se", "Wahr\u00b7heit", "\u00f6ff\u00b7ters", "\u00fc\u00b7berl\u00b7e\u00b7gen", "sol\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Lieber Mensch, du selber bist Staub, und stammest aus der\nErden,", "tokens": ["Lie\u00b7ber", "Mensch", ",", "du", "sel\u00b7ber", "bist", "Staub", ",", "und", "stam\u00b7mest", "aus", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPER", "ADV", "VAFIN", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Wirst auch, eh' du dichs versiehst, wieder Staub und Er-\nden werden?", "tokens": ["Wirst", "auch", ",", "eh'", "du", "dichs", "ver\u00b7siehst", ",", "wie\u00b7der", "Staub", "und", "Er", "den", "wer\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,", "ADV", "NN", "KON", "TRUNC", "ART", "VAINF", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}