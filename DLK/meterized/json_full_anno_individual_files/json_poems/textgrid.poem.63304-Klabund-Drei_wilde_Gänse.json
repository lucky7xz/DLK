{"textgrid.poem.63304": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Drei wilde G\u00e4nse", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei wilde G\u00e4nse, die flogen \u00fcber See.", "tokens": ["Drei", "wil\u00b7de", "G\u00e4n\u00b7se", ",", "die", "flo\u00b7gen", "\u00fc\u00b7ber", "See", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da scho\u00df der J\u00e4ger alle drei,", "tokens": ["Da", "scho\u00df", "der", "J\u00e4\u00b7ger", "al\u00b7le", "drei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was einmal ins Wasser fiel,", "tokens": ["Und", "was", "ein\u00b7mal", "ins", "Was\u00b7ser", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt nimmer in die H\u00f6h'.", "tokens": ["Kommt", "nim\u00b7mer", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Drei junge M\u00e4dels, die f\u00fchrte ein Kavalier aus,", "tokens": ["Drei", "jun\u00b7ge", "M\u00e4\u00b7dels", ",", "die", "f\u00fchr\u00b7te", "ein", "Ka\u00b7va\u00b7lier", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und wenn erst ein M\u00e4del mal Sekt genascht,", "tokens": ["Und", "wenn", "erst", "ein", "M\u00e4\u00b7del", "mal", "Sekt", "ge\u00b7nascht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Liebe genascht, Hiebe genascht \u2013", "tokens": ["Lie\u00b7be", "ge\u00b7nascht", ",", "Hie\u00b7be", "ge\u00b7nascht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "VVPP", "$("], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die kommt nicht mehr nach Haus.", "tokens": ["Die", "kommt", "nicht", "mehr", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ich pfeife auf meine Jungfernschaft,", "tokens": ["Und", "ich", "pfei\u00b7fe", "auf", "mei\u00b7ne", "Jung\u00b7fern\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und ich pfeife auf mein Leben.", "tokens": ["Und", "ich", "pfei\u00b7fe", "auf", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Kerl, der sie mir genommen hat,", "tokens": ["Der", "Kerl", ",", "der", "sie", "mir", "ge\u00b7nom\u00b7men", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Um eins und um zwei und um drei bei der Nacht,", "tokens": ["Um", "eins", "und", "um", "zwei", "und", "um", "drei", "bei", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIS", "KON", "APPR", "CARD", "KON", "APPR", "CARD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der kann sie mir nimmer geben.", "tokens": ["Der", "kann", "sie", "mir", "nim\u00b7mer", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Geh, schenk mir doch 'n Fuffzger,", "tokens": ["Geh", ",", "schenk", "mir", "doch", "'n", "Fuffz\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geh, schenk mir doch 'ne Mark.", "tokens": ["Geh", ",", "schenk", "mir", "doch", "'ne", "Mark", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will mich mit Schnaps besaufen,", "tokens": ["Ich", "will", "mich", "mit", "Schnaps", "be\u00b7sau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich will mir eine Villa kaufen", "tokens": ["Ich", "will", "mir", "ei\u00b7ne", "Vil\u0