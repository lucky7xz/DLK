{"textgrid.poem.38541": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Wonne der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer an der Geliebten Augen hangen,", "tokens": ["Wer", "an", "der", "Ge\u00b7lieb\u00b7ten", "Au\u00b7gen", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wer mit Feuerinbrunst sie umfangen,", "tokens": ["Wer", "mit", "Feu\u00b7e\u00b7rin\u00b7brunst", "sie", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Sich in ihrem Ku\u00df berauschen kann,", "tokens": ["Sich", "in", "ih\u00b7rem", "Ku\u00df", "be\u00b7rau\u00b7schen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Welch ein hochbegl\u00fckter Mann!", "tokens": ["Welch", "ein", "hoch\u00b7be\u00b7gl\u00fck\u00b7ter", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Er verlacht das leere Weltget\u00fcmmel,", "tokens": ["Er", "ver\u00b7lacht", "das", "lee\u00b7re", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seinen Blik umschweben tausend Himmel,", "tokens": ["Sei\u00b7nen", "Blik", "um\u00b7schwe\u00b7ben", "tau\u00b7send", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gold und Ehr' ist ihm ein Kinderspiel,", "tokens": ["Gold", "und", "Ehr'", "ist", "ihm", "ein", "Kin\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gro\u00df und hehr ist sein Gef\u00fchl!", "tokens": ["Gro\u00df", "und", "hehr", "ist", "sein", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "K\u00f6nnten Engel Sterbliche beneiden,", "tokens": ["K\u00f6nn\u00b7ten", "En\u00b7gel", "Sterb\u00b7li\u00b7che", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O! sie neideten ihm seine Freuden!", "tokens": ["O", "!", "sie", "nei\u00b7de\u00b7ten", "ihm", "sei\u00b7ne", "Freu\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "O! sie tr\u00e4nken aus der Liebe Meer", "tokens": ["O", "!", "sie", "tr\u00e4n\u00b7ken", "aus", "der", "Lie\u00b7be", "Meer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "