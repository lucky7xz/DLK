{"dta.poem.15864": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch zwischen Klippen hat ein Tr\u00fcpplein B\u00e4um' ihr Heil", "tokens": ["Hoch", "zwi\u00b7schen", "Klip\u00b7pen", "hat", "ein", "Tr\u00fcpp\u00b7lein", "B\u00e4um'", "ihr", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "VAFIN", "ART", "NN", "NN", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Gefunden, wo sie nicht erreichet Axt und Beil.", "tokens": ["Ge\u00b7fun\u00b7den", ",", "wo", "sie", "nicht", "er\u00b7rei\u00b7chet", "Axt", "und", "Beil", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PTKNEG", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie ziehen d\u00fcrftiger vom Fels der Nahrung Saft,", "tokens": ["Sie", "zie\u00b7hen", "d\u00fcrf\u00b7ti\u00b7ger", "vom", "Fels", "der", "Nah\u00b7rung", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch neiden nicht umher die \u00fcpp'ge Nachbarschaft.", "tokens": ["Doch", "nei\u00b7den", "nicht", "um\u00b7her", "die", "\u00fcpp'\u00b7ge", "Nach\u00b7bar\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn all die andern sehn vom Berg' im fernen Thal", "tokens": ["Denn", "all", "die", "an\u00b7dern", "sehn", "vom", "Ber\u00b7g'", "im", "fer\u00b7nen", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ART", "ADJA", "VVFIN", "APPRART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den Tod vor Augen, der hinab sie holt einmal,", "tokens": ["Den", "Tod", "vor", "Au\u00b7gen", ",", "der", "hin\u00b7ab", "sie", "holt", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sei's um als H\u00fcttenrauch, wie dort qualmt, aufzugehn,", "tokens": ["Sei's", "um", "als", "H\u00fct\u00b7ten\u00b7rauch", ",", "wie", "dort", "qualmt", ",", "auf\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPR", "KOUS", "NN", "$,", "PWAV", "ADV", "VVFIN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei's in der M\u00fchle, die dort \u00e4chzt, zers\u00e4gt zu stehn.", "tokens": ["Sei's", "in", "der", "M\u00fch\u00b7le", ",", "die", "dort", "\u00e4chzt", ",", "zer\u00b7s\u00e4gt", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nur jene sind verschont, bis sie zernagt der Wurm", "tokens": ["Nur", "je\u00b7ne", "sind", "ver\u00b7schont", ",", "bis", "sie", "zer\u00b7nagt", "der", "Wurm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Alters, oder wirft von ihrer Klipp' ein Sturm.", "tokens": ["Des", "Al\u00b7ters", ",", "o\u00b7der", "wirft", "von", "ih\u00b7rer", "Klipp'", "ein", "Sturm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}