{"dta.poem.9181": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n Daphnis beklagt sich/ die Fillis habe in einer gegenwart  \n den Strefon gek\u00fcst.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ach mein leben t\u00f6dtet mich!", "tokens": ["Ach", "mein", "le\u00b7ben", "t\u00f6d\u00b7tet", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "VVINF", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fillis will nichts von mir wissen/", "tokens": ["Fil\u00b7lis", "will", "nichts", "von", "mir", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "APPR", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und hingegen l\u00e4sst sie sich/", "tokens": ["Und", "hin\u00b7ge\u00b7gen", "l\u00e4sst", "sie", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weil ich da bin/ andre k\u00fcssen:", "tokens": ["Weil", "ich", "da", "bin", "/", "and\u00b7re", "k\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$(", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann der stoltze Strefon will", "tokens": ["Wann", "der", "stolt\u00b7ze", "Stre\u00b7fon", "will"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich zu ihren lippen legen/", "tokens": ["Sich", "zu", "ih\u00b7ren", "lip\u00b7pen", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kommt sie selbst ihm halb entgegen/", "tokens": ["Kommt", "sie", "selbst", "ihm", "halb", "ent\u00b7ge\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und bef\u00f6rdert ihm das spiel.", "tokens": ["Und", "be\u00b7f\u00f6r\u00b7dert", "ihm", "das", "spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "VVFIN", "$."], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "2. Doch ich armer Daphnis mu\u00df", "tokens": ["Doch", "ich", "ar\u00b7mer", "Daph\u00b7nis", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00f6ses sehn/ und gutes dencken/", "tokens": ["B\u00f6\u00b7ses", "sehn", "/", "und", "gu\u00b7tes", "den\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "$(", "KON", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es will kein feuchter ku\u00df/", "tokens": ["Denn", "es", "will", "kein", "feuch\u00b7ter", "ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine d\u00fcrre lippen tr\u00e4ncken;", "tokens": ["Mei\u00b7ne", "d\u00fcr\u00b7re", "lip\u00b7pen", "tr\u00e4n\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein verh\u00e4ngnis tr\u00f6stet wohl/", "tokens": ["Mein", "ver\u00b7h\u00e4ng\u00b7nis", "tr\u00f6s\u00b7tet", "wohl", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Daphnis sey du nur gedultig/", "tokens": ["Daph\u00b7nis", "sey", "du", "nur", "ge\u00b7dul\u00b7tig", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fillis bleibt dir etwas schuldig/", "tokens": ["Fil\u00b7lis", "bleibt", "dir", "et\u00b7was", "schul\u00b7dig", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das dir endlich werden soll.", "tokens": ["Das", "dir", "end\u00b7lich", "wer\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VAINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Unterdessen steckt mein sinn/", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", "steckt", "mein", "sinn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen eyfer/ scham und liebe/", "tokens": ["Zwi\u00b7schen", "ey\u00b7fer", "/", "scham", "und", "lie\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJD", "KON", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df/ indem ich lustig bin/", "tokens": ["Da\u00df", "/", "in\u00b7dem", "ich", "lus\u00b7tig", "bin", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich mich ebenfalls betr\u00fcbe.", "tokens": ["Ich", "mich", "e\u00b7ben\u00b7falls", "be\u00b7tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stell ich meine freundschafft ein/", "tokens": ["Stell", "ich", "mei\u00b7ne", "freund\u00b7schafft", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder bleib ich ihr ergeben?", "tokens": ["O\u00b7der", "bleib", "ich", "ihr", "er\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll ich auff ihr falsches leben", "tokens": ["Soll", "ich", "auff", "ihr", "fal\u00b7sches", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "G\u00fcnstig oder b\u00f6se seyn?", "tokens": ["G\u00fcns\u00b7tig", "o\u00b7der", "b\u00f6\u00b7se", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Weicht ihr augen/ kehrt euch weg/", "tokens": ["Weicht", "ihr", "au\u00b7gen", "/", "kehrt", "euch", "weg", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolt ihr meine treu verfluchen/", "tokens": ["Wolt", "ihr", "mei\u00b7ne", "treu", "ver\u00b7flu\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder einen frembden fleck", "tokens": ["O\u00b7der", "ei\u00b7nen", "fremb\u00b7den", "fleck"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auff dem falschen m\u00fcndgen suchen?", "tokens": ["Auff", "dem", "fal\u00b7schen", "m\u00fcnd\u00b7gen", "su\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVINF", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schlisst die schwachen lieder zu/", "tokens": ["Schlisst", "die", "schwa\u00b7chen", "lie\u00b7der", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn ich brauch euch nur zur straffe/", "tokens": ["Denn", "ich", "brauch", "euch", "nur", "zur", "straf\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gebt mir lieber als im schlaffe/", "tokens": ["Gebt", "mir", "lie\u00b7ber", "als", "im", "schlaf\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "KOKOM", "APPRART", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Finstre doch gewisse ruh.", "tokens": ["Finst\u00b7re", "doch", "ge\u00b7wis\u00b7se", "ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Aber bleibt nur immer da/", "tokens": ["A\u00b7ber", "bleibt", "nur", "im\u00b7mer", "da", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll ich mich nur \u00e4rger straffen/", "tokens": ["Soll", "ich", "mich", "nur", "\u00e4r\u00b7ger", "straf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jhre zier beliebt mir ja/", "tokens": ["Ih\u00b7re", "zier", "be\u00b7liebt", "mir", "ja", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drum so d\u00f6rfft ihr auch nicht schlaffen/", "tokens": ["Drum", "so", "d\u00f6rfft", "ihr", "auch", "nicht", "schlaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonst verdoppelt ihr die noth/", "tokens": ["Sonst", "ver\u00b7dop\u00b7pelt", "ihr", "die", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Cilt und \u00f6ffnet eure lieder", "tokens": ["Cilt", "und", "\u00f6ff\u00b7net", "eu\u00b7re", "lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und erblickt die sch\u00f6ne wieder/", "tokens": ["Und", "er\u00b7blickt", "die", "sch\u00f6\u00b7ne", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn wo nicht/ so bin ich tod/", "tokens": ["Denn", "wo", "nicht", "/", "so", "bin", "ich", "tod", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PTKNEG", "$(", "ADV", "VAFIN", "PPER", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Ist es ja so weit geschehn/", "tokens": ["Ist", "es", "ja", "so", "weit", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach so wil ich ihr gesichte/", "tokens": ["Ach", "so", "wil", "ich", "ihr", "ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VMFIN", "PPER", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieber falsch als gar nicht sehn/", "tokens": ["Lie\u00b7ber", "falsch", "als", "gar", "nicht", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "ADV", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alldieweil ich mich verpflichte/", "tokens": ["All\u00b7die\u00b7weil", "ich", "mich", "ver\u00b7pflich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df die Fillis ihre stadt/", "tokens": ["Da\u00df", "die", "Fil\u00b7lis", "ih\u00b7re", "stadt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ohne liederliches wancken", "tokens": ["Oh\u00b7ne", "lie\u00b7der\u00b7li\u00b7ches", "wan\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zwar zuf\u00f6rderst die gedancken/", "tokens": ["Zwar", "zu\u00b7f\u00f6r\u00b7derst", "die", "ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch im augen gleichfalls hat.", "tokens": ["Doch", "im", "au\u00b7gen", "gleich\u00b7falls", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Fillis aber dencke nach", "tokens": ["Fil\u00b7lis", "a\u00b7ber", "den\u00b7cke", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Willst du meine noth verlachen", "tokens": ["Willst", "du", "mei\u00b7ne", "noth", "ver\u00b7la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und durch solches ungemach", "tokens": ["Und", "durch", "sol\u00b7ches", "un\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir das leben sauer machen?", "tokens": ["Mir", "das", "le\u00b7ben", "sau\u00b7er", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "VVFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun so thu es immerhin", "tokens": ["Nun", "so", "thu", "es", "im\u00b7mer\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch da\u00df ich nicht alles wisse/", "tokens": ["Doch", "da\u00df", "ich", "nicht", "al\u00b7les", "wis\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "PIS", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So verspahre deine kusse/", "tokens": ["So", "ver\u00b7spah\u00b7re", "dei\u00b7ne", "kus\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bi\u00df ich nicht zugegen bin.", "tokens": ["Bi\u00df", "ich", "nicht", "zu\u00b7ge\u00b7gen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}