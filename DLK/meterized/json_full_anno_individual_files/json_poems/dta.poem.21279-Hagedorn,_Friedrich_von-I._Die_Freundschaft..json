{"dta.poem.21279": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n Die Freundschaft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1752", "urn": "urn:nbn:de:kobv:b4-200905199117", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du Mutter holder Triebe,", "tokens": ["Du", "Mut\u00b7ter", "hol\u00b7der", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Freunschaft! dir zur Ehre,", "tokens": ["O", "Freun\u00b7schaft", "!", "dir", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dir, Freundschaft, nicht der Liebe,", "tokens": ["Dir", ",", "Freund\u00b7schaft", ",", "nicht", "der", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erschallen unsre Ch\u00f6re,", "tokens": ["Er\u00b7schal\u00b7len", "uns\u00b7re", "Ch\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Phyllis stimmt mit ein:", "tokens": ["Und", "Phyl\u00b7lis", "stimmt", "mit", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "ART", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch sollte das Entz\u00fccken", "tokens": ["Doch", "soll\u00b7te", "das", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von Phyllis Ton und Blicken", "tokens": ["Von", "Phyl\u00b7lis", "Ton", "und", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nichts mehr als Freundschaft seyn?", "tokens": ["Nichts", "mehr", "als", "Freund\u00b7schaft", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "KOUS", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}