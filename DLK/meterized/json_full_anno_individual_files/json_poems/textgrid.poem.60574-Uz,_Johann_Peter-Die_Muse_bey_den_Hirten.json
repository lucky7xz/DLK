{"textgrid.poem.60574": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Muse bey den Hirten", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Muse! darf ich trauen?", "tokens": ["O", "Mu\u00b7se", "!", "darf", "ich", "trau\u00b7en", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Willst du auf unsern Auen", "tokens": ["Willst", "du", "auf", "un\u00b7sern", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit schlechten Hirten weiden", "tokens": ["Mit", "schlech\u00b7ten", "Hir\u00b7ten", "wei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und aus den St\u00e4dten scheiden?", "tokens": ["Und", "aus", "den", "St\u00e4d\u00b7ten", "schei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Stadt mag immer prangen!", "tokens": ["Die", "Stadt", "mag", "im\u00b7mer", "pran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich bin aus ihr gegangen.", "tokens": ["Ich", "bin", "aus", "ihr", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da will man Musen dingen:", "tokens": ["Da", "will", "man", "Mu\u00b7sen", "din\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie sollen iedem singen,", "tokens": ["Sie", "sol\u00b7len", "ie\u00b7dem", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bey ieder Hochzeit leyern", "tokens": ["Bey", "ie\u00b7der", "Hoch\u00b7zeit", "ley\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Nahmenstage feyern.", "tokens": ["Und", "Nah\u00b7mens\u00b7ta\u00b7ge", "fey\u00b7ern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie schickt sich die\u00df f\u00fcr Musen", "tokens": ["Wie", "schickt", "sich", "die\u00df", "f\u00fcr", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "PDS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll Himmels in dem Busen?", "tokens": ["Voll", "Him\u00b7mels", "in", "dem", "Bu\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist f\u00fcr Gratulanten,", "tokens": ["Das", "ist", "f\u00fcr", "Gra\u00b7tu\u00b7lan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die wir vom Pindus bannten.", "tokens": ["Die", "wir", "vom", "Pin\u00b7dus", "bann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00df dich, o Muse! wieder", "tokens": ["La\u00df", "dich", ",", "o", "Mu\u00b7se", "!", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$.", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf unsern Triften nieder.", "tokens": ["Auf", "un\u00b7sern", "Trif\u00b7ten", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst bey frohen Ch\u00f6ren", "tokens": ["Du", "wirst", "bey", "fro\u00b7hen", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Freyheit und Cytheren;", "tokens": ["Die", "Frey\u00b7heit", "und", "Cy\u00b7the\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Und in den k\u00fchlen Gr\u00fcnden", "tokens": ["Und", "in", "den", "k\u00fch\u00b7len", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die holde Stille finden.", "tokens": ["Die", "hol\u00b7de", "Stil\u00b7le", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Muse! darf ich trauen?", "tokens": ["O", "Mu\u00b7se", "!", "da