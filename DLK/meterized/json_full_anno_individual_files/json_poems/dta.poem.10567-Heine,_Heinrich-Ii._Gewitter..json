{"dta.poem.10567": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n Gewitter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Dumpf liegt auf dem Meer' das Gewitter,", "tokens": ["Dumpf", "liegt", "auf", "dem", "Meer'", "das", "Ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und durch die schwarze Wolkenwand", "tokens": ["Und", "durch", "die", "schwar\u00b7ze", "Wol\u00b7ken\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zuckt der zackige Wetterstrahl,", "tokens": ["Zuckt", "der", "za\u00b7cki\u00b7ge", "Wet\u00b7ter\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Rasch aufleuchtend und rasch verschwindend,", "tokens": ["Rasch", "auf\u00b7leuch\u00b7tend", "und", "rasch", "ver\u00b7schwin\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wie'n Witz aus dem Haupte Kronions.", "tokens": ["Wie'n", "Witz", "aus", "dem", "Haup\u00b7te", "Kro\u00b7nions", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ueber das w\u00fcste, wogende Wasser", "tokens": ["Ue\u00b7ber", "das", "w\u00fcs\u00b7te", ",", "wo\u00b7gen\u00b7de", "Was\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDS", "VVFIN", "$,", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Weithin rollen die Donner", "tokens": ["Weit\u00b7hin", "rol\u00b7len", "die", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Und springen die wei\u00dfen Wellenrosse,", "tokens": ["Und", "sprin\u00b7gen", "die", "wei\u00b7\u00dfen", "Wel\u00b7len\u00b7ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die Boreas selber gezeugt", "tokens": ["Die", "Bo\u00b7re\u00b7as", "sel\u00b7ber", "ge\u00b7zeugt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "ADV", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Mit des Erichthons reizenden Stuten,", "tokens": ["Mit", "des", "E\u00b7rich\u00b7thons", "rei\u00b7zen\u00b7den", "Stu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Und es flattert \u00e4ngstlich das Seegev\u00f6gel,", "tokens": ["Und", "es", "flat\u00b7tert", "\u00e4ngst\u00b7lich", "das", "See\u00b7ge\u00b7v\u00f6\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Wie Schattenleichen am Styx,", "tokens": ["Wie", "Schat\u00b7ten\u00b7lei\u00b7chen", "am", "Styx", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Die Charon abwies vom n\u00e4chtlichen Kahn.", "tokens": ["Die", "Cha\u00b7ron", "ab\u00b7wies", "vom", "n\u00e4cht\u00b7li\u00b7chen", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Armes, lustiges Schifflein,", "tokens": ["Ar\u00b7mes", ",", "lus\u00b7ti\u00b7ges", "Schif\u00b7flein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Das dort dahintanzt den schlimmsten Tanz!", "tokens": ["Das", "dort", "da\u00b7hin\u00b7tanzt", "den", "schlimms\u00b7ten", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aeolus schickt ihm die flinksten Gesellen,", "tokens": ["A\u00b7e\u00b7o\u00b7lus", "schickt", "ihm", "die", "flinks\u00b7ten", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die wild aufspielen zum fr\u00f6hlichen Reigen;", "tokens": ["Die", "wild", "auf\u00b7spie\u00b7len", "zum", "fr\u00f6h\u00b7li\u00b7chen", "Rei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVIZU", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Eine pfeift, der Andre bl\u00e4st,", "tokens": ["Der", "Ei\u00b7ne", "pfeift", ",", "der", "And\u00b7re", "bl\u00e4st", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Dritte streicht den dumpfen Brummba\u00df \u2014", "tokens": ["Der", "Drit\u00b7te", "streicht", "den", "dum\u00b7pfen", "Brumm\u00b7ba\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und der schwankende Seemann steht, am Steuer,", "tokens": ["Und", "der", "schwan\u00b7ken\u00b7de", "See\u00b7mann", "steht", ",", "am", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und schaut best\u00e4ndig nach der Bussole,", "tokens": ["Und", "schaut", "be\u00b7st\u00e4n\u00b7dig", "nach", "der", "Bus\u00b7so\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der zitternden Seele des Schiffes,", "tokens": ["Der", "zit\u00b7tern\u00b7den", "See\u00b7le", "des", "Schif\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und hebt die H\u00e4nde flehend zum Himmel:", "tokens": ["Und", "hebt", "die", "H\u00e4n\u00b7de", "fle\u00b7hend", "zum", "Him\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "O rette mich, Kastor, reisiger Held,", "tokens": ["O", "ret\u00b7te", "mich", ",", "Kas\u00b7tor", ",", "rei\u00b7si\u00b7ger", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und Du, K\u00e4mpfer der Faust, Polydeukes!", "tokens": ["Und", "Du", ",", "K\u00e4mp\u00b7fer", "der", "Faust", ",", "Po\u00b7ly\u00b7deu\u00b7kes", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "ART", "NN", "$,", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}}}}