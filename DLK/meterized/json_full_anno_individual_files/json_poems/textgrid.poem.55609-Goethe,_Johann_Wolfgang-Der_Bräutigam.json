{"textgrid.poem.55609": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der Br\u00e4utigam", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um Mitternacht \u2013 ich schlief, im Busen wachte", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "\u2013", "ich", "schlief", ",", "im", "Bu\u00b7sen", "wach\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "NN", "$(", "PPER", "VVFIN", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das liebevolle Herz, als w\u00e4r es Tag;", "tokens": ["Das", "lie\u00b7be\u00b7vol\u00b7le", "Herz", ",", "als", "w\u00e4r", "es", "Tag", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOKOM", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Tag erschien, mir war, als ob es nachte \u2013", "tokens": ["Der", "Tag", "er\u00b7schien", ",", "mir", "war", ",", "als", "ob", "es", "nach\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was ist es mir, soviel er bringen mag.", "tokens": ["Was", "ist", "es", "mir", ",", "so\u00b7viel", "er", "brin\u00b7gen", "mag."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie fehlte ja, mein emsig Tun und Streben,", "