{"dta.poem.19369": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "102.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Menschen ganzes Gl\u00fcck besteht in zweierlei,", "tokens": ["Des", "Men\u00b7schen", "gan\u00b7zes", "Gl\u00fcck", "be\u00b7steht", "in", "zwei\u00b7er\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihm gewis und ungewis die Zukunft sei.", "tokens": ["Da\u00df", "ihm", "ge\u00b7wis", "und", "un\u00b7ge\u00b7wis", "die", "Zu\u00b7kunft", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das ist ihm ungewis, wo er wird seyn und wie,", "tokens": ["Das", "ist", "ihm", "un\u00b7ge\u00b7wis", ",", "wo", "er", "wird", "seyn", "und", "wie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "VAINF", "KON", "PWAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewis, da\u00df er wird seyn, derselbe dort und hie.", "tokens": ["Ge\u00b7wis", ",", "da\u00df", "er", "wird", "seyn", ",", "der\u00b7sel\u00b7be", "dort", "und", "hie", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VAFIN", "VAINF", "$,", "PRELS", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Ungewisheit macht ihn froh der Gegenwart,", "tokens": ["Die", "Un\u00b7ge\u00b7wis\u00b7heit", "macht", "ihn", "froh", "der", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Gewisheit gibt ihm Kraft zur Weiterfahrt.", "tokens": ["Und", "die", "Ge\u00b7wis\u00b7heit", "gibt", "ihm", "Kraft", "zur", "Wei\u00b7ter\u00b7fahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer m\u00f6chte leben, w\u00e4r' ihm nicht sein Tod verborgen?", "tokens": ["Wer", "m\u00f6ch\u00b7te", "le\u00b7ben", ",", "w\u00e4r'", "ihm", "nicht", "sein", "Tod", "ver\u00b7bor\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVINF", "$,", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer k\u00f6nnt' heute seyn, wenn er nichts w\u00e4re morgen?", "tokens": ["Und", "wer", "k\u00f6nnt'", "heu\u00b7te", "seyn", ",", "wenn", "er", "nichts", "w\u00e4\u00b7re", "mor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "ADV", "VAINF", "$,", "KOUS", "PPER", "PIS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}