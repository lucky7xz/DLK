{"dta.poem.14415": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Mari\u00e4 Sehnsucht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "af:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nun ist wohl das Brautkleid gewoben gar,", "tokens": ["Nun", "ist", "wohl", "das", "Braut\u00b7kleid", "ge\u00b7wo\u00b7ben", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "ADV", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und goldene Sterne in's dunkele Haar,", "tokens": ["Und", "gol\u00b7de\u00b7ne", "Ster\u00b7ne", "in's", "dun\u00b7ke\u00b7le", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und im Arme die Jungfrau das Kn\u00e4blein h\u00e4lt,", "tokens": ["Und", "im", "Ar\u00b7me", "die", "Jung\u00b7frau", "das", "Kn\u00e4\u00b7blein", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Hoch \u00fcber der dunkelerbrausenden Welt,", "tokens": ["Hoch", "\u00fc\u00b7ber", "der", "dun\u00b7ke\u00b7ler\u00b7brau\u00b7sen\u00b7den", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und vom Kindlein gehet ein Gl\u00e4nzen aus,", "tokens": ["Und", "vom", "Kin\u00b7dlein", "ge\u00b7het", "ein", "Gl\u00e4n\u00b7zen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das ruft uns nur ewig: nach Haus, nach Haus!", "tokens": ["Das", "ruft", "uns", "nur", "e\u00b7wig", ":", "nach", "Haus", ",", "nach", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "$.", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}