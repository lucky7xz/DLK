{"dta.poem.1080": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Also wan meine schuldigkeit", "tokens": ["Al\u00b7so", "wan", "mei\u00b7ne", "schul\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PPOSAT", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Meinen geisttreibet zuerdichten/", "tokens": ["Mei\u00b7nen", "geist\u00b7trei\u00b7bet", "zu\u00b7er\u00b7dich\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Was dir f\u00fcr ein werck aufzurichten/", "tokens": ["Was", "dir", "f\u00fcr", "ein", "werck", "auf\u00b7zu\u00b7rich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welches der Jahren fr\u00e4ssigkeit", "tokens": ["Wel\u00b7ches", "der", "Jah\u00b7ren", "fr\u00e4s\u00b7sig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Mit dem reichen schmuck der warheit", "tokens": ["Mit", "dem", "rei\u00b7chen", "schmuck", "der", "war\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00f6g vnver\u00e4ltlich widerstehen", "tokens": ["M\u00f6g", "vn\u00b7ver\u00b7\u00e4lt\u00b7lich", "wi\u00b7der\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Macht deiner Tugenten klarheit", "tokens": ["Macht", "dei\u00b7ner", "Tu\u00b7gen\u00b7ten", "klar\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Mit wunder mein gesicht vergehen;", "tokens": ["Mit", "wun\u00b7der", "mein", "ge\u00b7sicht", "ver\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vnd jemehr ich bedencke mich", "tokens": ["Vnd", "je\u00b7mehr", "ich", "be\u00b7den\u00b7cke", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Welches lob erstlich anzuwenden/", "tokens": ["Wel\u00b7ches", "lob", "erst\u00b7lich", "an\u00b7zu\u00b7wen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ADJD", "VVIZU", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Je weniger/ forchtsam/ wai\u00df ich/", "tokens": ["Je", "we\u00b7ni\u00b7ger", "/", "forcht\u00b7sam", "/", "wai\u00df", "ich", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "ADJD", "$(", "VVFIN", "PPER", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Wa anzufangen vnd zu enden.", "tokens": ["Wa", "an\u00b7zu\u00b7fan\u00b7gen", "vnd", "zu", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIZU", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}