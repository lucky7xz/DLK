{"textgrid.poem.43345": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "XiI. Das Unendliche.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieb war mir immer dieser kahle H\u00fcgel", "tokens": ["Lieb", "war", "mir", "im\u00b7mer", "die\u00b7ser", "kah\u00b7le", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und diese Hecke, die dem Blick so Viel", "tokens": ["Und", "die\u00b7se", "He\u00b7cke", ",", "die", "dem", "Blick", "so", "Viel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "ART", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom fernsten Horizont zu schau'n verwehrt.", "tokens": ["Vom", "ferns\u00b7ten", "Ho\u00b7ri\u00b7zont", "zu", "schau'n", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn ich sitz' und um mich blicke, tr\u00e4um' ich,", "tokens": ["Und", "wenn", "ich", "sitz'", "und", "um", "mich", "bli\u00b7cke", ",", "tr\u00e4um'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "KON", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Endlose Weiten, \u00fcbermenschlich Schweigen", "tokens": ["End\u00b7lo\u00b7se", "Wei\u00b7ten", ",", "\u00fc\u00b7ber\u00b7menschlich", "Schwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und allertiefste Ruhe herrsche dort", "tokens": ["Und", "al\u00b7ler\u00b7tiefs\u00b7te", "Ru\u00b7he", "herr\u00b7sche", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Jenseits der niedern Schranke, und das Herz", "tokens": ["Jen\u00b7seits", "der", "nie\u00b7dern", "Schran\u00b7ke", ",", "und", "das", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Erschauert mir vor Grau'n. Und h\u00f6r' ich dann", "tokens": ["Er\u00b7schau\u00b7ert", "mir", "vor", "Grau'", "n.", "Und", "h\u00f6r'", "ich", "dann"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NE", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Den Wind erbrausen im Gezweig, vergleich' ich", "tokens": ["Den", "Wind", "er\u00b7brau\u00b7sen", "im", "Ge\u00b7zweig", ",", "ver\u00b7gleich'", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "APPRART", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die grenzenlose Stille dort, und hier", "tokens": ["Die", "gren\u00b7zen\u00b7lo\u00b7se", "Stil\u00b7le", "dort", ",", "und", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die laute Stimme; und des Ew'gen denk'