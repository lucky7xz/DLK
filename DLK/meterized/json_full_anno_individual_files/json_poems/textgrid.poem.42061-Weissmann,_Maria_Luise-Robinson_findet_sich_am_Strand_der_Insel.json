{"textgrid.poem.42061": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Robinson findet sich am Strand der Insel", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und dies war alles, was er fand, erwacht:", "tokens": ["Und", "dies", "war", "al\u00b7les", ",", "was", "er", "fand", ",", "er\u00b7wacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es lag ein Leib, voll Schmerz, an einem Strand.", "tokens": ["Es", "lag", "ein", "Leib", ",", "voll", "Schmerz", ",", "an", "ei\u00b7nem", "Strand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hin flo\u00df ein Meer in hyazinthne Nacht,", "tokens": ["Hin", "flo\u00df", "ein", "Meer", "in", "hy\u00b7a\u00b7zinth\u00b7ne", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aufbrach in Blau ein unergr\u00fcndlich Land.", "tokens": ["Auf\u00b7brach", "in", "Blau", "ein", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Wind lief schnell, die spitzen M\u00f6wen stie\u00dfen", "tokens": ["Der", "Wind", "lief", "s