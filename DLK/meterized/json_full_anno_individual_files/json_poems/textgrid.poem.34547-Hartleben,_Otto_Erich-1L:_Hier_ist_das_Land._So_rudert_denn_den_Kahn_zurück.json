{"textgrid.poem.34547": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier ist das Land. So rudert denn den Kahn zur\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1897, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ist das Land. So rudert denn den Kahn zur\u00fcck", "tokens": ["Hier", "ist", "das", "Land", ".", "So", "ru\u00b7dert", "denn", "den", "Kahn", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und meldet den Gef\u00e4hrten: Ich betrat mein Reich,", "tokens": ["und", "mel\u00b7det", "den", "Ge\u00b7f\u00e4hr\u00b7ten", ":", "Ich", "be\u00b7trat", "mein", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als F\u00fcrsten sehen sie mich wieder, oder nie. \u2013", "tokens": ["als", "F\u00fcrs\u00b7ten", "se\u00b7hen", "sie", "mich", "wie\u00b7der", ",", "o\u00b7der", "nie", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PRF", "ADV", "$,", "KON", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was steht ihr noch und zaudert? Lasst mich nun allein,", "tokens": ["Was", "steht", "ihr", "noch", "und", "zau\u00b7dert", "?", "Lasst", "mich", "nun", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "allein mit meinem guten Schwert und meinem Ross \u2013", "tokens": ["al\u00b7lein", "mit", "mei\u00b7nem", "gu\u00b7ten", "Schwert", "und", "mei\u00b7nem", "Ross", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "nun werb ich in der Fremde mir die eigne Schaar.", "tokens": ["nun", "werb", "ich", "in", "der", "Frem\u00b7de", "mir", "die", "eig\u00b7ne", "Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Lebt wohl! Dem wandelbaren Meere kehr ich heut", "tokens": ["Lebt", "wohl", "!", "Dem", "wan\u00b7del\u00b7ba\u00b7ren", "Mee\u00b7re", "kehr", "ich", "heut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "den R\u00fccken zu, mein Auge sucht die Burgen auf,", "tokens": ["den", "R\u00fc\u00b7cken", "zu", ",", "mein", "Au\u00b7ge", "sucht", "die", "Bur\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "in deren Mauern sich der Feige sicher f\u00fchlt.", "tokens": ["in", "de\u00b7ren", "Mau\u00b7ern", "sich", "der", "Fei\u00b7ge", "si\u00b7cher", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PRF", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mein Auge sucht am Horizonte seinen Feind.", "tokens": ["Mein", "Au\u00b7ge", "sucht", "am", "Ho\u00b7ri\u00b7zon\u00b7te", "sei\u00b7nen", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Huftritt meines Rosses klingt an morsch Gebein,", "tokens": ["Der", "Huf\u00b7tritt", "mei\u00b7nes", "Ros\u00b7ses", "klingt", "an", "morsch", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "an Menschensch\u00e4del \u2013 mich zu schrecken sind sie wohl", "tokens": ["an", "Men\u00b7schen\u00b7sch\u00e4\u00b7del", "\u2013", "mich", "zu", "schre\u00b7cken", "sind", "sie", "wohl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "PPER", "PTKZU", "VVINF", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "vom Schicksal auf des Reiches Schwelle ausgestreut?", "tokens": ["vom", "Schick\u00b7sal", "auf", "des", "Rei\u00b7ches", "Schwel\u00b7le", "aus\u00b7ge\u00b7streut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zerstampfe sie, mein Schwarzer, stampfe \u00fcber sie hinweg:", "tokens": ["Zer\u00b7stamp\u00b7fe", "sie", ",", "mein", "Schwar\u00b7zer", ",", "stamp\u00b7fe", "\u00fc\u00b7ber", "sie", "hin\u00b7weg", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Sie waren nicht, der ich bin \u2013 darum fielen sie.", "tokens": ["Sie", "wa\u00b7ren", "nicht", ",", "der", "ich", "bin", "\u2013", "da\u00b7rum", "fie\u00b7len", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "PRELS", "PPER", "VAFIN", "$(", "PAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier ist das Land. So rudert denn den Kahn zur\u00fcck", "tokens": ["Hier", "ist", "das", "Land", ".", "So", "ru\u00b7dert", "denn", "den", "Kahn", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und meldet den Gef\u00e4hrten: Ich betrat mein Reich,", "tokens": ["und", "mel\u00b7det", "den", "Ge\u00b7f\u00e4hr\u00b7ten", ":", "Ich", "be\u00b7trat", "mein", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als F\u00fcrsten sehen sie mich wieder, oder nie. \u2013", "tokens": ["als", "F\u00fcrs\u00b7ten", "se\u00b7hen", "sie", "mich", "