{"textgrid.poem.48648": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist dieses nun das s\u00fc\u00dfe Wesen,", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist dieses nun das s\u00fc\u00dfe Wesen,", "tokens": ["Ist", "die\u00b7ses", "nun", "das", "s\u00fc\u00b7\u00dfe", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "nach dem mich so verlanget hat?", "tokens": ["nach", "dem", "mich", "so", "ver\u00b7lan\u00b7get", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist dieses der gesunde Rat,", "tokens": ["Ist", "die\u00b7ses", "der", "ge\u00b7sun\u00b7de", "Rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ohn' den ich kunte nicht genesen?", "tokens": ["ohn'", "den", "ich", "kun\u00b7te", "nicht", "ge\u00b7ne\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ist di\u00df meines Wehmuts Frucht,", "tokens": ["Und", "ist", "di\u00df", "mei\u00b7nes", "Weh\u00b7muts", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die ich so emsig aufgesucht?", "tokens": ["die", "ich", "so", "em\u00b7sig", "auf\u00b7ge\u00b7sucht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Feind, o Falscher, o Tyranne,", "tokens": ["O", "Feind", ",", "o", "Fal\u00b7scher", ",", "o", "Ty\u00b7ran\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kupido, das ist deine List!", "tokens": ["Ku\u00b7pi\u00b7do", ",", "das", "ist", "dei\u00b7ne", "List", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.3": {"text": "Der bist du, der du allzeit bist.", "tokens": ["Der", "bist", "du", ",", "der", "du", "all\u00b7zeit", "bist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du hast mich nun in deinem Banne.", "tokens": ["Du", "hast", "mich", "nun", "in", "dei\u00b7nem", "Ban\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Dienst der falschen Ledigkeit", "tokens": ["Der", "Dienst", "der", "fal\u00b7schen", "Le\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "hat meiner Freiheit mich entfreit.", "tokens": ["hat", "mei\u00b7ner", "Frei\u00b7heit", "mich", "ent\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie unverwirrt ist doch ein Herze,", "tokens": ["Wie", "un\u00b7ver\u00b7wirrt", "ist", "doch", "ein", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "das nicht mehr als sich selbsten kennt,", "tokens": ["das", "nicht", "mehr", "als", "sich", "selbs\u00b7ten", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "KOUS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "von keiner fremden Flamme brennt,", "tokens": ["von", "kei\u00b7ner", "frem\u00b7den", "Flam\u00b7me", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "selbst seine Lust und selbst sein Schmerze!", "tokens": ["selbst", "sei\u00b7ne", "Lust", "und", "selbst", "sein", "Schmer\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Seit da\u00df ich nicht mehr meine bin,", "tokens": ["Seit", "da\u00df", "ich", "nicht", "mehr", "mei\u00b7ne", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PTKNEG", "ADV", "PPOSAT", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "so ist mein ganzes Gl\u00fccke hin.", "tokens": ["so", "ist", "mein", "gan\u00b7zes", "Gl\u00fc\u00b7cke", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie, di\u00df Mensch, diese Halbg\u00f6ttinne,", "tokens": ["Sie", ",", "di\u00df", "Mensch", ",", "die\u00b7se", "Halb\u00b7g\u00f6t\u00b7tin\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PDS", "NN", "$,", "PDAT", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "sie, die ists, mein erfreutes Leid,", "tokens": ["sie", ",", "die", "ists", ",", "mein", "er\u00b7freu\u00b7tes", "Leid", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Kraft der starken Trefligkeit,", "tokens": ["die", "Kraft", "der", "star\u00b7ken", "Tre\u00b7flig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "treibt mich aus mir und meinem Sinne,", "tokens": ["treibt", "mich", "aus", "mir", "und", "mei\u00b7nem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "so da\u00df ich sonst nichts um und an", "tokens": ["so", "da\u00df", "ich", "sonst", "nichts", "um", "und", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PIS", "PTKVZ", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "als sie nur achten mu\u00df und kan.", "tokens": ["als", "sie", "nur", "ach\u00b7ten", "mu\u00df", "und", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich schlaf', ich tr\u00e4ume bei dem Wachen,", "tokens": ["Ich", "schlaf'", ",", "ich", "tr\u00e4u\u00b7me", "bei", "dem", "Wa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ich ruh' und habe keine Ruh'',", "tokens": ["ich", "ruh'", "und", "ha\u00b7be", "kei\u00b7ne", "Ruh", "\"", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "PIAT", "NN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich tu' und wei\u00df nicht, was ich tu',", "tokens": ["ich", "tu'", "und", "wei\u00df", "nicht", ",", "was", "ich", "tu'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich weine mitten in dem Lachen,", "tokens": ["ich", "wei\u00b7ne", "mit\u00b7ten", "in", "dem", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ich denk', ich mache di\u00df und das,", "tokens": ["ich", "denk'", ",", "ich", "ma\u00b7che", "di\u00df", "und", "das", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PDS", "KON", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ich schweig', ich red' und wei\u00df nicht was.", "tokens": ["ich", "schweig'", ",", "ich", "red'", "und", "wei\u00df", "nicht", "was", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Sonne scheint f\u00fcr mich nicht helle,", "tokens": ["Die", "Son\u00b7ne", "scheint", "f\u00fcr", "mich", "nicht", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mich k\u00fchlt die Glut, mich brennt das Eis,", "tokens": ["mich", "k\u00fchlt", "die", "Glut", ",", "mich", "brennt", "das", "Eis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich wei\u00df und wei\u00df nicht, was ich wei\u00df.", "tokens": ["ich", "wei\u00df", "und", "wei\u00df", "nicht", ",", "was", "ich", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nacht tritt an des Tages Stelle.", "tokens": ["Die", "Nacht", "tritt", "an", "des", "Ta\u00b7ges", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Itzt bin ich dort, itzt da, itzt hier,", "tokens": ["Itzt", "bin", "ich", "dort", ",", "itzt", "da", ",", "itzt", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ich folg' und fliehe selbst f\u00fcr mir.", "tokens": ["ich", "fol\u00b7g'", "und", "flie\u00b7he", "selbst", "f\u00fcr", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Bald billig' ich mir meinen Handel,", "tokens": ["Bald", "bil\u00b7lig'", "ich", "mir", "mei\u00b7nen", "Han\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "bald drauf verklag' ich mich bei mir.", "tokens": ["bald", "drauf", "ver\u00b7klag'", "ich", "mich", "bei", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin ver\u00e4ndert f\u00fcr und f\u00fcr", "tokens": ["Ich", "bin", "ver\u00b7\u00e4n\u00b7dert", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und standhaft nur in stetem Wandel.", "tokens": ["und", "stand\u00b7haft", "nur", "in", "ste\u00b7tem", "Wan\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich selbst bin mit mir selbst nicht eins.", "tokens": ["Ich", "selbst", "bin", "mit", "mir", "selbst", "nicht", "eins", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PPER", "ADV", "PTKNEG", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald will ich alles, bald gar keins.", "tokens": ["Bald", "will", "ich", "al\u00b7les", ",", "bald", "gar", "keins", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "$,", "ADV", "ADV", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie wird mirs doch noch endlich gehen?", "tokens": ["Wie", "wird", "mirs", "doch", "noch", "end\u00b7lich", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NE", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wohne nunmehr nicht in mir.", "tokens": ["Ich", "woh\u00b7ne", "nun\u00b7mehr", "nicht", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Schein nur ist es, den ihr hier", "tokens": ["Mein", "Schein", "nur", "ist", "es", ",", "den", "ihr", "hier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "PPER", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in meinem Bilde sehet stehen.", "tokens": ["in", "mei\u00b7nem", "Bil\u00b7de", "se\u00b7het", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin nun nicht mehr selber Ich.", "tokens": ["Ich", "bin", "nun", "nicht", "mehr", "sel\u00b7ber", "Ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach Liebe, worzu bringst du mich!", "tokens": ["Ach", "Lie\u00b7be", ",", "wor\u00b7zu", "bringst", "du", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ist dieses nun das s\u00fc\u00dfe Wesen,", "tokens": ["Ist", "die\u00b7ses", "nun", "das", "s\u00fc\u00b7\u00dfe", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "nach dem mich so verlanget hat?", "tokens": ["nach", "dem", "mich", "so", "ver\u00b7lan\u00b7get", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist dieses der gesunde Rat,", "tokens": ["Ist", "die\u00b7ses", "der", "ge\u00b7sun\u00b7de", "Rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ohn' den ich kunte nicht genesen?", "tokens": ["ohn'", "den", "ich", "kun\u00b7te", "nicht", "ge\u00b7ne\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word"