{"textgrid.poem.34408": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[denkst du daran]", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denkst du daran, wie du zum erstenmal", "tokens": ["Denkst", "du", "da\u00b7ran", ",", "wie", "du", "zum", "ers\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "$,", "PWAV", "PPER", "APPRART", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "aus deiner Heimathberge d\u00fcstrem Forst", "tokens": ["aus", "dei\u00b7ner", "Hei\u00b7math\u00b7ber\u00b7ge", "d\u00fcst\u00b7rem", "Forst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "aus dunklem Tannengr\u00fcn des hohen Harzes", "tokens": ["aus", "dunk\u00b7lem", "Tan\u00b7nen\u00b7gr\u00fcn", "des", "ho\u00b7hen", "Har\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "als Knabe niederschautest in die Ebne? \u2013", "tokens": ["als", "Kna\u00b7be", "nie\u00b7der\u00b7schau\u00b7test", "in", "die", "Eb\u00b7ne", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Welt ist bunt! so riefst du jauchzend aus.", "tokens": ["Die", "Welt", "ist", "bunt", "!", "so", "riefst", "du", "jauch\u00b7zend", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "