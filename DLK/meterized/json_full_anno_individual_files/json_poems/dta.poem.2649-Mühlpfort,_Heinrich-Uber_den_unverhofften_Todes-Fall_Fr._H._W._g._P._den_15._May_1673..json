{"dta.poem.2649": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber den unverhofften Todes-Fall  \n Fr. H. W. g. P. den 15. May 1673.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jsts m\u00f6glich/ da\u00df sein Aug und gantz zerschlagnes Hertze/", "tokens": ["Ists", "m\u00f6g\u00b7lich", "/", "da\u00df", "sein", "Aug", "und", "gantz", "zer\u00b7schlag\u00b7nes", "Hert\u00b7ze", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$(", "KOUS", "PPOSAT", "NN", "KON", "ADV", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins Leid versenckter Freund/ siht meine Zeilen an:", "tokens": ["Ins", "Leid", "ver\u00b7senck\u00b7ter", "Freund", "/", "siht", "mei\u00b7ne", "Zei\u00b7len", "an", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So glaub er/ di\u00df Pappier und aufgetragene Schw\u00e4rtze", "tokens": ["So", "glaub", "er", "/", "di\u00df", "Pap\u00b7pier", "und", "auf\u00b7ge\u00b7tra\u00b7ge\u00b7ne", "Schw\u00e4rt\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PDS", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wei\u00df selbst vor Wehmuth nicht/ was man hier melden", "tokens": ["Wei\u00df", "selbst", "vor", "Weh\u00b7muth", "nicht", "/", "was", "man", "hier", "mel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "PTKNEG", "$(", "PWS", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Liebste wil zur Ruh die m\u00fcden Glieder legen/", "tokens": ["Die", "Liebs\u00b7te", "wil", "zur", "Ruh", "die", "m\u00fc\u00b7den", "Glie\u00b7der", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "(kan.", "tokens": ["(", "kan", "."], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VMFIN", "$."], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Und denckt nicht da\u00df ihr Bett auch werd\u2019 ihr Grabmal seyn.", "tokens": ["Und", "denckt", "nicht", "da\u00df", "ihr", "Bett", "auch", "werd'", "ihr", "Grab\u00b7mal", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "KOUS", "PPOSAT", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Fall! der uns Sterblichen vern\u00fcnfftig zu erwegen/", "tokens": ["Fall", "!", "der", "uns", "Sterb\u00b7li\u00b7chen", "ver\u00b7n\u00fcnff\u00b7tig", "zu", "er\u00b7we\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPER", "NN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Und lehret wie der Tod sich \u00fcberall findt ein.", "tokens": ["Und", "leh\u00b7ret", "wie", "der", "Tod", "sich", "\u00fc\u00b7be\u00b7rall", "findt", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "PRF", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er steht bey der Geburt/ er wachet bey der Wiegen;", "tokens": ["Er", "steht", "bey", "der", "Ge\u00b7burt", "/", "er", "wa\u00b7chet", "bey", "der", "Wie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er g\u00e4ngelt unsern Fu\u00df/ schaut ", "tokens": ["Er", "g\u00e4n\u00b7gelt", "un\u00b7sern", "Fu\u00df", "/", "schaut"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wird mitten in dem Spiel der freyen Jugend liegen.", "tokens": ["Wird", "mit\u00b7ten", "in", "dem", "Spiel", "der", "frey\u00b7en", "Ju\u00b7gend", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist/ wenn wir M\u00e4nner sind/ auch neben uns bem\u00fcht.", "tokens": ["Ist", "/", "wenn", "wir", "M\u00e4n\u00b7ner", "sind", "/", "auch", "ne\u00b7ben", "uns", "be\u00b7m\u00fcht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "PPER", "NN", "VAFIN", "$(", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bald geht er mit zu Feld/ bald tritt er vor Gerichte/", "tokens": ["Bald", "geht", "er", "mit", "zu", "Feld", "/", "bald", "tritt", "er", "vor", "Ge\u00b7rich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "NN", "$(", "ADV", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bald sagt er wer er sey/ bald wie man ihn bezwingt/", "tokens": ["Bald", "sagt", "er", "wer", "er", "sey", "/", "bald", "wie", "man", "ihn", "be\u00b7zwingt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PWS", "PPER", "VAFIN", "$(", "ADV", "KOKOM", "PIS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Entlarvt nicht/ wenn er gleich so handelt sein Gesichte/", "tokens": ["Ent\u00b7larvt", "nicht", "/", "wenn", "er", "gleich", "so", "han\u00b7delt", "sein", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wei\u00df da\u00df der sichre Mensch ihn nirgends wo verdringt.", "tokens": ["Wei\u00df", "da\u00df", "der", "sich\u00b7re", "Mensch", "ihn", "nir\u00b7gends", "wo", "ver\u00b7dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "ART", "ADJA", "NN", "PPER", "ADV", "PWAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Er setzet sich zu Tisch/ isst mit von unsren Speisen/", "tokens": ["Er", "set\u00b7zet", "sich", "zu", "Tisch", "/", "isst", "mit", "von", "un\u00b7sren", "Spei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$(", "ADV", "APPR", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Braucht einerley Getr\u00e4nck/ und ist bey Freuden frey;", "tokens": ["Braucht", "ei\u00b7ner\u00b7ley", "Ge\u00b7tr\u00e4nck", "/", "und", "ist", "bey", "Freu\u00b7den", "frey", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$(", "KON", "VAFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wird bey Gesch\u00e4fften so/ wie wir bem\u00fcht/ sich weisen.", "tokens": ["Wird", "bey", "Ge\u00b7sch\u00e4ff\u00b7ten", "so", "/", "wie", "wir", "be\u00b7m\u00fcht", "/", "sich", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADV", "$(", "PWAV", "PPER", "VVFIN", "$(", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wer meynt nun da\u00df er nicht auch bey dem schlaffen sey?", "tokens": ["Wer", "meynt", "nun", "da\u00df", "er", "nicht", "auch", "bey", "dem", "schlaf\u00b7fen", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "KOUS", "PPER", "PTKNEG", "ADV", "APPR", "ART", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der grosse Lipsius/ das Wunder kluger Sinnen/", "tokens": ["Der", "gros\u00b7se", "Lip\u00b7sius", "/", "das", "Wun\u00b7der", "klu\u00b7ger", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Rieff: Ein Tritt geht ins Bett\u2019 der ander in das Grab.", "tokens": ["Rieff", ":", "Ein", "Tritt", "geht", "ins", "Bett'", "der", "an\u00b7der", "in", "das", "Grab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "ART", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ward seiner Sterbligkeit bey jedem Abend innen/", "tokens": ["Ward", "sei\u00b7ner", "Ster\u00b7blig\u00b7keit", "bey", "je\u00b7dem", "A\u00b7bend", "in\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und wie das Lebens-Garn nehm\u2019 allgem\u00e4hlich ab.", "tokens": ["Und", "wie", "das", "Le\u00b7bens\u00b7Garn", "nehm'", "all\u00b7ge\u00b7m\u00e4h\u00b7lich", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Zwar es macht der Natur und Fleisch und Blut ein Schrecken/", "tokens": ["Zwar", "es", "macht", "der", "Na\u00b7tur", "und", "Fleisch", "und", "Blut", "ein", "Schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "KON", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Wenn man der Sonnen Rad siht fr\u00f6lich untergehn/", "tokens": ["Wenn", "man", "der", "Son\u00b7nen", "Rad", "siht", "fr\u00f6\u00b7lich", "un\u00b7ter\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "NN", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und wei\u00df nicht ob sie uns auch morgen auff wird wecken/", "tokens": ["Und", "wei\u00df", "nicht", "ob", "sie", "uns", "auch", "mor\u00b7gen", "auff", "wird", "we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "KOUS", "PPER", "PPER", "ADV", "ADV", "APPR", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und ob wir auch gesund von unserm Bett\u2019 auffstehn.", "tokens": ["Und", "ob", "wir", "auch", "ge\u00b7sund", "von", "un\u00b7serm", "Bett'", "auffs\u00b7tehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df wir wie Attila nicht in dem Blut ersticken/", "tokens": ["Da\u00df", "wir", "wie", "At\u00b7ti\u00b7la", "nicht", "in", "dem", "Blut", "er\u00b7sti\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NE", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df uns der Donner nicht wie Zoroastern trifft/", "tokens": ["Da\u00df", "uns", "der", "Don\u00b7ner", "nicht", "wie", "Zo\u00b7roas\u00b7tern", "trifft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "KOKOM", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.32": {"text": "Da\u00df uns der H\u00e4user Fall nicht gehling m\u00f6cht erdr\u00fccken/", "tokens": ["Da\u00df", "uns", "der", "H\u00e4u\u00b7ser", "Fall", "nicht", "geh\u00b7ling", "m\u00f6cht", "er\u00b7dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PTKNEG", "VVFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Noch wie den Ladislau im Schlaff uns was vergifft.", "tokens": ["Noch", "wie", "den", "La\u00b7dis\u00b7lau", "im", "Schlaff", "uns", "was", "ver\u00b7gifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPRART", "NN", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Alleine diese Furcht ermuntert unsre Geister/", "tokens": ["Al\u00b7lei\u00b7ne", "die\u00b7se", "Furcht", "er\u00b7mun\u00b7tert", "uns\u00b7re", "Geis\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df wir Soldaten gleich stets in Bereitschafft stehn;", "tokens": ["Da\u00df", "wir", "Sol\u00b7da\u00b7ten", "gleich", "stets", "in", "Be\u00b7reit\u00b7schafft", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Damit wenn ja der Tod an uns noch wird zum Meister/", "tokens": ["Da\u00b7mit", "wenn", "ja", "der", "Tod", "an", "uns", "noch", "wird", "zum", "Meis\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "ADV", "ART", "NN", "APPR", "PPER", "ADV", "VAFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wir unerschrocknes Muths ihm in die Augen gehn.", "tokens": ["Wir", "un\u00b7er\u00b7schrock\u00b7nes", "Muths", "ihm", "in", "die", "Au\u00b7gen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Doch wie ein Krieges-Knecht mit Recht ist zu verlachen/", "tokens": ["Doch", "wie", "ein", "Krie\u00b7ge\u00b7sKnecht", "mit", "Recht", "ist", "zu", "ver\u00b7la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der erst nach Waffen fragt/ wenn es schon blitzt und kracht:", "tokens": ["Der", "erst", "nach", "Waf\u00b7fen", "fragt", "/", "wenn", "es", "schon", "blitzt", "und", "kracht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$(", "KOUS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So k\u00f6nnen wir uns leicht auch gleiche Rechnung machen/", "tokens": ["So", "k\u00f6n\u00b7nen", "wir", "uns", "leicht", "auch", "glei\u00b7che", "Rech\u00b7nung", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Da\u00df unser letztes Ziel zuvor mu\u00df seyn bedacht.", "tokens": ["Da\u00df", "un\u00b7ser", "letz\u00b7tes", "Ziel", "zu\u00b7vor", "mu\u00df", "seyn", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "VMFIN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Denn stehts dem Tode frey/ denn kan ein solches Sterben/", "tokens": ["Denn", "stehts", "dem", "To\u00b7de", "frey", "/", "denn", "kan", "ein", "sol\u00b7ches", "Ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "$(", "KON", "VMFIN", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wie schnell es immer k\u00f6mmt/ mit nichten sch\u00e4dlich seyn.", "tokens": ["Wie", "schnell", "es", "im\u00b7mer", "k\u00f6mmt", "/", "mit", "nich\u00b7ten", "sch\u00e4d\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VVFIN", "$(", "APPR", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wir wissen/ unser Leib mu\u00df einmal doch verderben.", "tokens": ["Wir", "wis\u00b7sen", "/", "un\u00b7ser", "Leib", "mu\u00df", "ein\u00b7mal", "doch", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPOSAT", "NN", "VMFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Was hilfft es ob er so/ und wieder so geht ein?", "tokens": ["Was", "hi