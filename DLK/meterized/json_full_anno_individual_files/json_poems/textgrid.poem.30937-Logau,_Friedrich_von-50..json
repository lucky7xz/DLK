{"textgrid.poem.30937": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Handwercks-Leute haben Zunfften, haben Ordnung und Gesetze,", "tokens": ["Handwercks\u00b7Leu\u00b7te", "ha\u00b7ben", "Zunff\u00b7ten", ",", "ha\u00b7ben", "Ord\u00b7nung", "und", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Da\u00df sich Niemand in ihr Mittel, sein Gewerb zu treiben, setze,", "tokens": ["Da\u00df", "sich", "Nie\u00b7mand", "in", "ihr", "Mit\u00b7tel", ",", "sein", "Ge\u00b7werb", "zu", "trei\u00b7ben", ",", "set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PRF", "PIS", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Der nicht ehlich ist geboren, ob er sonsten gleich ist t\u00fcchtig,", "tokens": ["Der", "nicht", "eh\u00b7lich", "ist", "ge\u00b7bo\u00b7ren", ",", "ob", "er", "sons\u00b7ten", "gleich", "ist", "t\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "ADV", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Der auch ausser seiner Ehe nicht gelebet allzu richtig,", "tokens": ["Der", "auch", "aus\u00b7ser", "sei\u00b7ner", "E\u00b7he", "nicht", "ge\u00b7le\u00b7bet", "all\u00b7zu", "rich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "VVPP", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Ob gleich Busse drauff erfolget, welcher einen Hund erschlagen,", "tokens": ["Ob", "gleich", "Bus\u00b7se", "drauff", "er\u00b7fol\u00b7get", ",", "wel\u00b7cher", "ei\u00b7nen", "Hund", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "PAV", "VVFIN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Obs gleich ohngefehr geschehen, der die Kosten nicht zu tragen", "tokens": ["Obs", "gleich", "ohn\u00b7ge\u00b7fehr", "ge\u00b7sche\u00b7hen", ",", "der", "die", "Kos\u00b7ten", "nicht", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "VVPP", "$,", "PRELS", "ART", "NN", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Zum Ges\u00e4uff und zum Gefr\u00e4sse, der nicht Meisterst\u00fccke machet,", "tokens": ["Zum", "Ge\u00b7s\u00e4uff", "und", "zum", "Ge\u00b7fr\u00e4s\u00b7se", ",", "der", "nicht", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke", "ma\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$,", "PRELS", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Macht ihn gleich das Werck zum Meister; mehres ist, darob man lachet.", "tokens": ["Macht", "ihn", "gleich", "das", "Werck", "zum", "Meis\u00b7ter", ";", "meh\u00b7res", "ist", ",", "da\u00b7rob", "man", "la\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$.", "PIS", "VAFIN", "$,", "PAV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Aber da\u00df man Warheit meidet, da\u00df man schindrisch \u00fcbersetzet,", "tokens": ["A\u00b7ber", "da\u00df", "man", "War\u00b7heit", "mei\u00b7det", ",", "da\u00df", "man", "schin\u00b7drisch", "\u00fc\u00b7bers\u00b7et\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "NN", "VVFIN", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Da\u00df man Falsch f\u00fcr Gut gewehret, da\u00df man Treu und Schwur versetzet:", "tokens": ["Da\u00df", "man", "Falsch", "f\u00fcr", "Gut", "ge\u00b7weh\u00b7ret", ",", "da\u00df", "man", "Treu", "und", "Schwur", "ver\u00b7set\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "APPR", "ADJD", "VVPP", "$,", "KOUS", "PIS", "NN", "KON", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Dieses heist, sich kl\u00fcglich n\u00e4hren. Lieber! sind es Handwercks-St\u00fccke,", "tokens": ["Die\u00b7ses", "heist", ",", "sich", "kl\u00fcg\u00b7lich", "n\u00e4h\u00b7ren", ".", "Lie\u00b7ber", "!", "sind", "es", "Handwercks\u00b7St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRF", "ADJD", "VVINF", "$.", "ADJD", "$.", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+