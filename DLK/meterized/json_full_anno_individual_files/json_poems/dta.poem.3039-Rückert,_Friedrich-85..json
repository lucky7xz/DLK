{"dta.poem.3039": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vier Kr\u00e4fte nenn' ich dir am Menschen, mangelhaft", "tokens": ["Vier", "Kr\u00e4f\u00b7te", "nenn'", "ich", "dir", "am", "Men\u00b7schen", ",", "man\u00b7gel\u00b7haft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu nennen sind die vier vor einer f\u00fcnften Kraft.", "tokens": ["Zu", "nen\u00b7nen", "sind", "die", "vier", "vor", "ei\u00b7ner", "f\u00fcnf\u00b7ten", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "ART", "CARD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Trieb im Menschen, wenn er einen Gegenstand", "tokens": ["Der", "Trieb", "im", "Men\u00b7schen", ",", "wenn", "er", "ei\u00b7nen", "Ge\u00b7gen\u00b7stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ergreifen will, streckt er zuerst danach die Hand.", "tokens": ["Er\u00b7grei\u00b7fen", "will", ",", "streckt", "er", "zu\u00b7erst", "da\u00b7nach", "die", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "VVFIN", "PPER", "ADV", "PAV", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und ist der Gegenstand der Hand nicht zu erlangen,", "tokens": ["Und", "ist", "der", "Ge\u00b7gen\u00b7stand", "der", "Hand", "nicht", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist anstatt der Hand der Fu\u00df danach gegangen.", "tokens": ["So", "ist", "an\u00b7statt", "der", "Hand", "der", "Fu\u00df", "da\u00b7nach", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo auch das Flieh'nde dort will deinem Fu\u00df entweichen,", "tokens": ["Wo", "auch", "das", "Flieh'n\u00b7de", "dort", "will", "dei\u00b7nem", "Fu\u00df", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da mag es noch dein Wort, dein Rufen es erreichen.", "tokens": ["Da", "mag", "es", "noch", "dein", "Wort", ",", "dein", "Ru\u00b7fen", "es", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch weiter als dein Wort, als deine Stimme, dringt", "tokens": ["Doch", "wei\u00b7ter", "als", "dein", "Wort", ",", "als", "dei\u00b7ne", "Stim\u00b7me", ",", "dringt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "KOKOM", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Auge, das dir nah heran das fernste bringt.", "tokens": ["Dein", "Au\u00b7ge", ",", "das", "dir", "nah", "he\u00b7ran", "das", "ferns\u00b7te", "bringt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "PTKVZ", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "In Fernen aber, die du mit des Blickes Schweifen", "tokens": ["In", "Fer\u00b7nen", "a\u00b7ber", ",", "die", "du", "mit", "des", "Bli\u00b7ckes", "Schwei\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADV", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht kannst ermessen, kannst du mit Gedanken greifen.", "tokens": ["Nicht", "kannst", "er\u00b7mes\u00b7sen", ",", "kannst", "du", "mit", "Ge\u00b7dan\u00b7ken", "grei\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "VVINF", "$,", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Drum \u00fcbe Hand und Fu\u00df, und Red'- und Sehekraft,", "tokens": ["Drum", "\u00fc\u00b7be", "Hand", "und", "Fu\u00df", ",", "und", "Re\u00b7d'", "und", "Se\u00b7he\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "KON", "NN", "$,", "KON", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vor allem \u00fcbe doch dich in Denkwissenschaft.", "tokens": ["Vor", "al\u00b7lem", "\u00fc\u00b7be", "doch", "dich", "in", "Denk\u00b7wis\u00b7sen\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ADV", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}