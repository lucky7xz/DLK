{"dta.poem.9365": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun hatt mein gem\u00fcthe sich erquicket/", "tokens": ["Nun", "hatt", "mein", "ge\u00b7m\u00fc\u00b7the", "sich", "er\u00b7quic\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "PRF", "VVFIN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "weil das Auge dieses Licht erblicket/", "tokens": ["weil", "das", "Au\u00b7ge", "die\u00b7ses", "Licht", "er\u00b7bli\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "welches durch die sp\u00e4ten N\u00e4chte bricht/", "tokens": ["wel\u00b7ches", "durch", "die", "sp\u00e4\u00b7ten", "N\u00e4ch\u00b7te", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "und mich brennt/ doch ohne flam\u0304 und Licht.", "tokens": ["und", "mich", "brennt", "/", "doch", "oh\u00b7ne", "flam\u0304", "und", "Licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$(", "ADV", "APPR", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Kom\u0303 und lesche diesen brand der Liebe", "tokens": ["Kom\u0303", "und", "le\u00b7sche", "die\u00b7sen", "brand", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KON", "VVFIN", "PDAT", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "kom\u0304 und mich nicht l\u00e4nger so betr\u00fcbe/", "tokens": ["kom\u0304", "und", "mich", "nicht", "l\u00e4n\u00b7ger", "so", "be\u00b7tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PPER", "PTKNEG", "ADJD", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Du mein Auffenthalt und Lebens-zier", "tokens": ["Du", "mein", "Auf\u00b7fent\u00b7halt", "und", "Le\u00b7bens\u00b7zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "lindre diesen schmertzen doch an mier.", "tokens": ["lind\u00b7re", "die\u00b7sen", "schmert\u00b7zen", "doch", "an", "mier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Nahe dich du g\u00fcldnes Licht der Freuden;", "tokens": ["Na\u00b7he", "dich", "du", "g\u00fcld\u00b7nes", "Licht", "der", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wiltu denn so bald von mier abscheiden?", "tokens": ["Wil\u00b7tu", "denn", "so", "bald", "von", "mier", "ab\u00b7schei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und mich la\u00dfen ohne Sonn und Licht/", "tokens": ["und", "mich", "la\u00b7\u00dfen", "oh\u00b7ne", "Sonn", "und", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "weil die trauren-volle-nacht anbricht.", "tokens": ["weil", "die", "trau\u00b7ren\u00b7vol\u00b7le\u00b7nacht", "an\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sihe wie so sch\u00f6n die Sterne lachen/", "tokens": ["Si\u00b7he", "wie", "so", "sch\u00f6n", "die", "Ster\u00b7ne", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOKOM", "ADV", "ADJD", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und du wilt dich schon von hinnen machen?", "tokens": ["und", "du", "wilt", "dich", "schon", "von", "hin\u00b7nen", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sch\u00f6ne/ du mein fr\u00fch- und Abendstern/", "tokens": ["Sch\u00f6\u00b7ne", "/", "du", "mein", "fr\u00fch", "und", "A\u00b7bends\u00b7tern", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "PPOSAT", "TRUNC", "KON", "NN", "$("], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "nahe dich und bleibe nicht so fern.", "tokens": ["na\u00b7he", "dich", "und", "blei\u00b7be", "nicht", "so", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn gleich alle Stern\u2019 am Himmel schienen/", "tokens": ["Wenn", "gleich", "al\u00b7le", "Stern'", "am", "Him\u00b7mel", "schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "k\u00f6nten sie mier dennoch wenig dienen/", "tokens": ["k\u00f6n\u00b7ten", "sie", "mier", "den\u00b7noch", "we\u00b7nig", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PIS", "VVINF", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Du Alleine bist mein bestes Licht/", "tokens": ["Du", "Al\u00b7lei\u00b7ne", "bist", "mein", "bes\u00b7tes", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "bey dem keine Sonne mier gebricht.", "tokens": ["bey", "dem", "kei\u00b7ne", "Son\u00b7ne", "mier", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}