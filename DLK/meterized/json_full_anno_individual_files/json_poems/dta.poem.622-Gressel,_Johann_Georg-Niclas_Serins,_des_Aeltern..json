{"dta.poem.622": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Niclas Serins,   des Aeltern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mich zwung die reine Treu mein Leben zu verliehren/", "tokens": ["Mich", "zwung", "die", "rei\u00b7ne", "Treu", "mein", "Le\u00b7ben", "zu", "ver\u00b7lieh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Blut wird ", "tokens": ["Mein", "Blut", "wird"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Mich hat der wilde T\u00fcrck aufs Ehren-Bett gelegt/", "tokens": ["Mich", "hat", "der", "wil\u00b7de", "T\u00fcrck", "aufs", "Eh\u00b7ren\u00b7Bett", "ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch fiel ich als ein Held der tausend Feinde schl\u00e4gt.", "tokens": ["Doch", "fiel", "ich", "als", "ein", "Held", "der", "tau\u00b7send", "Fein\u00b7de", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "ART", "NN", "ART", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}