{"dta.poem.10445": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, was soll es bedeuten,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "soll", "es", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da\u00df ich so traurig bin;", "tokens": ["Da\u00df", "ich", "so", "trau\u00b7rig", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein M\u00e4hrchen aus alten Zeiten,", "tokens": ["Ein", "M\u00e4hr\u00b7chen", "aus", "al\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das kommt mir nicht aus dem Sinn.", "tokens": ["Das", "kommt", "mir", "nicht", "aus", "dem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Die Luft ist k\u00fchl und es dunkelt,", "tokens": ["Die", "Luft", "ist", "k\u00fchl", "und", "es", "dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und ruhig flie\u00dft der Rhein;", "tokens": ["Und", "ru\u00b7hig", "flie\u00dft", "der", "Rhein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Gipfel des Berges funkelt", "tokens": ["Der", "Gip\u00b7fel", "des", "Ber\u00b7ges", "fun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Abendsonnenschein.", "tokens": ["Im", "A\u00b7bend\u00b7son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die sch\u00f6nste Jungfrau sitzet", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Jung\u00b7frau", "sit\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort oben wunderbar", "tokens": ["Dort", "o\u00b7ben", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr gold'nes Geschmeide blitzet,", "tokens": ["Ihr", "gold'\u00b7nes", "Ge\u00b7schmei\u00b7de", "blit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie k\u00e4mmt ihr gold'nes Haar.", "tokens": ["Sie", "k\u00e4mmt", "ihr", "gold'\u00b7nes", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie k\u00e4mmt es mit gold'nem Kamme,", "tokens": ["Sie", "k\u00e4mmt", "es", "mit", "gold'\u00b7nem", "Kam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und singt ein Lied dabei;", "tokens": ["Und", "singt", "ein", "Lied", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das hat eine wundersame,", "tokens": ["Das", "hat", "ei\u00b7ne", "wun\u00b7der\u00b7sa\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gewaltige Melodei.", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ge", "Me\u00b7lo\u00b7dei", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Den Schiffer im kleinen Schiffe", "tokens": ["Den", "Schif\u00b7fer", "im", "klei\u00b7nen", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ergreift es mit wildem Weh;", "tokens": ["Er\u00b7greift", "es", "mit", "wil\u00b7dem", "Weh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er schaut nicht die Felsenriffe,", "tokens": ["Er", "schaut", "nicht", "die", "Fel\u00b7sen\u00b7rif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er schaut nur hinauf in die H\u00f6h'.", "tokens": ["Er", "schaut", "nur", "hin\u00b7auf", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Ich glaube, die Wellen verschlingen", "tokens": ["Ich", "glau\u00b7be", ",", "die", "Wel\u00b7len", "ver\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Am Ende Schiffer und Kahn;", "tokens": ["Am", "En\u00b7de", "Schif\u00b7fer", "und", "Kahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und das hat mit ihrem Singen", "tokens": ["Und", "das", "hat", "mit", "ih\u00b7rem", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Lore-Ley gethan.", "tokens": ["Die", "Lo\u00b7re\u00b7Ley", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}