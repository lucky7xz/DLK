 {"line.1": {"text": "Wo Pfade sich mit Pfaden wild verschlingen,", "tokens": ["Wo", "Pfa\u00b7de", "sich", "mit", "Pfa\u00b7den", "wild", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Stein die Ferse feindlich ihm ber\u00fchrt,", "tokens": ["Der", "Stein", "die", "Fer\u00b7se", "feind\u00b7lich", "ihm", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Aug das dunkle Dickicht mag durchdringen,", "tokens": ["Kein", "Aug", "das", "dunk\u00b7le", "Di\u00b7ckicht", "mag", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch das der Weg ansteigend aufw\u00e4rts f\u00fchrt.", "tokens": ["Durch", "das", "der", "Weg", "an\u00b7stei\u00b7gend", "auf\u00b7w\u00e4rts", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "Wohl ihm! wenn aus dem Chor verworrner Stimmen", "tokens": ["Wohl", "ihm", "!", "wenn", "aus", "dem", "Chor", "ver\u00b7worr\u00b7ner", "Stim\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm eine wohlbekannte Stimme t\u00f6nt,", "tokens": ["Ihm", "ei\u00b