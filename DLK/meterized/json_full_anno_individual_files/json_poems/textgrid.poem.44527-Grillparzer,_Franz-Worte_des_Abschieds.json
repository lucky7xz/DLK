{"textgrid.poem.44527": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Worte des Abschieds", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Musen Stimme, gleich dem Chor der Sph\u00e4ren,", "tokens": ["Der", "Mu\u00b7sen", "Stim\u00b7me", ",", "gleich", "dem", "Chor", "der", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist nur dem Eingeweihten s\u00fc\u00dfe Melodie,", "tokens": ["Ist", "nur", "dem", "Ein\u00b7ge\u00b7weih\u00b7ten", "s\u00fc\u00b7\u00dfe", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Neuling glaubt des Donners Ruf zu h\u00f6ren,", "tokens": ["Der", "Neu\u00b7ling", "glaubt", "des", "Don\u00b7ners", "Ruf", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Anfang, statt zu schmeicheln, schrecken sie.", "tokens": ["Im", "An\u00b7fang", ",", "statt", "zu", "schmei\u00b7cheln", ",", "schre\u00b7cken", "sie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUI", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wer das Feld des Wissens und des Wahren", "tokens": ["Und", "wer", "das", "Feld", "des", "Wis\u00b7sens", "und", "des", "Wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zuerst betritt \u2013 ihm eine neue Welt \u2013", "tokens": ["Zu\u00b7erst", "be\u00b7tritt", "\u2013", "ihm", "ei\u00b7ne", "neu\u00b7e", "Welt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo er nicht Ziel, nicht Richtung mag gewahren,", "tokens": ["Wo", "er", "nicht", "Ziel", ",", "nicht", "Rich\u00b7tung", "mag", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr eine W\u00fcste h\u00e4lt er das betretne Feld;", "tokens": ["F\u00fcr", "ei\u00b7ne", "W\u00fcs\u00b7te", "h\u00e4lt", "er", "das", "be\u00b7tret\u00b7ne", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo Pfade sich mit Pfaden wild verschlingen,", "tokens": ["Wo", "Pfa\u00b7de", "sich", "mit", "Pfa\u00b7den", "wild", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Stein die Ferse feindlich ihm ber\u00fchrt,", "tokens": ["Der", "Stein", "die", "Fer\u00b7se", "feind\u00b7lich", "ihm", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Aug das dunkle Dickicht mag durchdringen,", "tokens": ["Kein", "Aug", "das", "dunk\u00b7le", "Di\u00b7ckicht", "mag", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch das der Weg ansteigend aufw\u00e4rts f\u00fchrt.", "tokens": ["Durch", "das", "der", "Weg", "an\u00b7stei\u00b7gend", "auf\u00b7w\u00e4rts", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl ihm! wenn aus dem Chor verworrner Stimmen", "tokens": ["Wohl", "ihm", "!", "wenn", "aus", "dem", "Chor", "ver\u00b7worr\u00b7ner", "Stim\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm eine wohlbekannte Stimme t\u00f6nt,", "tokens": ["Ihm", "ei\u00b7ne", "wohl\u00b7be\u00b7kann\u00b7te", "Stim\u00b7me", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ermutigend, nur r\u00fcstig fortzuklimmen,", "tokens": ["Er\u00b7mu\u00b7ti\u00b7gend", ",", "nur", "r\u00fcs\u00b7tig", "fort\u00b7zu\u00b7klim\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis sich der Aussicht Kreis von selbst versch\u00f6nt;", "tokens": ["Bis", "sich", "der", "Aus\u00b7sicht", "Kreis", "von", "selbst", "ver\u00b7sch\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "NN", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn ihm ein Aug, das oft den Zweifel scheuchte,", "tokens": ["Wenn", "ihm", "ein", "Aug", ",", "das", "oft", "den", "Zwei\u00b7fel", "scheuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vorangeht, und bald vor, bald r\u00fcck gewandt,", "tokens": ["Vor\u00b7an\u00b7geht", ",", "und", "bald", "vor", ",", "bald", "r\u00fcck", "ge\u00b7wandt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADV", "PTKVZ", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch Kl\u00fcfte strahlt, wie eine milde Leuchte,", "tokens": ["Durch", "Kl\u00fcf\u00b7te", "strahlt", ",", "wie", "ei\u00b7ne", "mil\u00b7de", "Leuch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Blick begleitend mit der Helferhand.", "tokens": ["Den", "Blick", "be\u00b7glei\u00b7tend", "mit", "der", "Hel\u00b7fer\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Bis nun des Berges erster Hang erklommen,", "tokens": ["Bis", "nun", "des", "Ber\u00b7ges", "ers\u00b7ter", "Hang", "er\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Waller r\u00fcckschaut in das tiefe Tal,", "tokens": ["Der", "Wal\u00b7ler", "r\u00fcck\u00b7schaut", "in", "das", "tie\u00b7fe", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus dem er, ein Verzagender, gekommen,", "tokens": ["Aus", "dem", "er", ",", "ein", "Ver\u00b7za\u00b7gen\u00b7der", ",", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie hebt sich da die Brust mit einemmal.", "tokens": ["Wie", "hebt", "sich", "da", "die", "Brust", "mit", "ei\u00b7nem\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und niederst\u00fcrzend auf die Kniee, breitet", "tokens": ["Und", "nie\u00b7ders\u00b7t\u00fcr\u00b7zend", "auf", "die", "Kni\u00b7ee", ",", "brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVPP", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er aus die Hand, und str\u00f6mt des Dankes viel", "tokens": ["Er", "aus", "die", "Hand", ",", "und", "str\u00f6mt", "des", "Dan\u00b7kes", "viel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Himmel und dem Mann, der ihn geleitet,", "tokens": ["Dem", "Him\u00b7mel", "und", "dem", "Mann", ",", "der", "ihn", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Weg verb\u00fcrgend und im Weg das Ziel.", "tokens": ["Den", "Weg", "ver\u00b7b\u00fcr\u00b7gend", "und", "im", "Weg", "das", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "So stehn wir heut, nicht mehr der Zukunft bange,", "tokens": ["So", "stehn", "wir", "heut", ",", "nicht", "mehr", "der", "Zu\u00b7kunft", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PTKNEG", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch dich gef\u00fchrt, durch deine Glut geweiht,", "tokens": ["Durch", "dich", "ge\u00b7f\u00fchrt", ",", "durch", "dei\u00b7ne", "Glut", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nun auf des steilen Berges erstem Hange,", "tokens": ["Nun", "auf", "des", "stei\u00b7len", "Ber\u00b7ges", "ers\u00b7tem", "Han\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo sich der Knabe an den J\u00fcngling reiht.", "tokens": ["Wo", "sich", "der", "Kna\u00b7be", "an", "den", "J\u00fcng\u00b7ling", "reiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Zwar ist der Weg noch lang, das Ziel noch ferne,", "tokens": ["Zwar", "ist", "der", "Weg", "noch", "lang", ",", "das", "Ziel", "noch", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und rauh und drohend, was noch vor uns liegt,", "tokens": ["Und", "rauh", "und", "dro\u00b7hend", ",", "was", "noch", "vor", "uns", "liegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVPP", "$,", "PRELS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur, wenn sonst z\u00f6gernd, klimmen wir nun gerne,", "tokens": ["Nur", ",", "wenn", "sonst", "z\u00f6\u00b7gernd", ",", "klim\u00b7men", "wir", "nun", "ger\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "ADJD", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir wissen, wie man Schwieriges besiegt.", "tokens": ["Wir", "wis\u00b7sen", ",", "wie", "man", "Schwie\u00b7ri\u00b7ges", "be\u00b7siegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Doch w\u00e4hrend wir an K\u00fcnftigem uns weiden,", "tokens": ["Doch", "w\u00e4h\u00b7rend", "wir", "an", "K\u00fcnf\u00b7ti\u00b7gem", "uns", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchzuckt ein hei\u00dfer Schmerz die bange Brust,", "tokens": ["Durch\u00b7zuckt", "ein", "hei\u00b7\u00dfer", "Schmerz", "die", "ban\u00b7ge", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es gilt zugleich, von ihm, dem Mann, zu scheiden,", "tokens": ["Es", "gilt", "zu\u00b7gleich", ",", "von", "ihm", ",", "dem", "Mann", ",", "zu", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "PPER", "$,", "ART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der uns der Arbeit M\u00fchn verkehrt in Lust,", "tokens": ["Der", "uns", "der", "Ar\u00b7beit", "M\u00fchn", "ver\u00b7kehrt", "in", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und neu beschleicht die Bangigkeit uns wieder;", "tokens": ["Und", "neu", "be\u00b7schleicht", "die", "Ban\u00b7gig\u00b7keit", "uns", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da t\u00f6nts von fern wie leiser Fl\u00fcgel Schwung,", "tokens": ["Da", "t\u00f6nts", "von", "fern", "wie", "lei\u00b7ser", "Fl\u00fc\u00b7gel", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJD", "KOKOM", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Genius schwebt auf leuchtendem Gefieder,", "tokens": ["Ein", "Ge\u00b7nius", "schwebt", "auf", "leuch\u00b7ten\u00b7dem", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der Jetzt und Einst verkn\u00fcpft: Erinnerung.", "tokens": ["Der", "Jetzt", "und", "Einst", "ver\u00b7kn\u00fcpft", ":", "E\u00b7rin\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "KON", "NN", "VVPP", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Uns bleibt dein Bild, da\u00df deiner Augen Sterne", "tokens": ["Uns", "bleibt", "dein", "Bild", ",", "da\u00df", "dei\u00b7ner", "Au\u00b7gen", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch k\u00fcnftig ob uns strahlen mildes Licht,", "tokens": ["Auch", "k\u00fcnf\u00b7tig", "ob", "uns", "strah\u00b7len", "mil\u00b7des", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf da\u00df dein Mund, ob lautlos durch die Ferne,", "tokens": ["Auf", "da\u00df", "dein", "Mund", ",", "ob", "laut\u00b7los", "durch", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "$,", "KOUS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Herzen doch vernehmlich, zu uns spricht.", "tokens": ["Dem", "Her\u00b7zen", "doch", "ver\u00b7nehm\u00b7lich", ",", "zu", "uns", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Und da Gef\u00fchle mitgef\u00fchlt nur heilen,", "tokens": ["Und", "da", "Ge\u00b7f\u00fch\u00b7le", "mit\u00b7ge\u00b7f\u00fchlt", "nur", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergi\u00df auch du uns nicht, die schwach und jung,", "tokens": ["Ver\u00b7gi\u00df", "auch", "du", "uns", "nicht", ",", "die", "schwach", "und", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "PRF", "PTKNEG", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "mea