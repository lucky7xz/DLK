{"textgrid.poem.40163": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Sieg", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun haben wir den schwersten Kampf gerungen", "tokens": ["Nun", "ha\u00b7ben", "wir", "den", "schwers\u00b7ten", "Kampf", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "im heil'gen Krieg um unsres Wesens Einheit,", "tokens": ["im", "heil'\u00b7gen", "Krieg", "um", "uns\u00b7res", "We\u00b7sens", "Ein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "als hei\u00df wir rangen mit der eignen Kleinheit,", "tokens": ["als", "hei\u00df", "wir", "ran\u00b7gen", "mit", "der", "eig\u00b7nen", "Klein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "bis Eines wir im Andern uns bezwungen,", "tokens": ["bis", "Ei\u00b7nes", "wir", "im", "An\u00b7dern", "uns", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "APPRART", "ADJA", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "bis endlich von den Herzen uns gesprungen", "tokens": ["bis", "end\u00b7lich", "von", "den", "Her\u00b7zen", "uns", "ge\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "wor