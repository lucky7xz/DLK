{"dta.poem.1207": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die lange Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr faulen Stunden ihr/ wie w\u00e4hret ihr so lange!", "tokens": ["Ihr", "fau\u00b7len", "Stun\u00b7den", "ihr", "/", "wie", "w\u00e4h\u00b7ret", "ihr", "so", "lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "$(", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sonsten fr\u00fche Tag h\u00e4lt seinen Einzug auff/", "tokens": ["Der", "sons\u00b7ten", "fr\u00fc\u00b7he", "Tag", "h\u00e4lt", "sei\u00b7nen", "Ein\u00b7zug", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Sternen muntre Schaar steht still in vollem Lauff/", "tokens": ["Der", "Ster\u00b7nen", "mun\u00b7tre", "Schaar", "steht", "still", "in", "vol\u00b7lem", "Lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Matuta l\u00e4sset nach von ihrem schnellen Gange.", "tokens": ["Ma\u00b7tu\u00b7ta", "l\u00e4s\u00b7set", "nach", "von", "ih\u00b7rem", "schnel\u00b7len", "Gan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O Himmel/ der mit sich die Himmels-Lichter ziehet/", "tokens": ["O", "Him\u00b7mel", "/", "der", "mit", "sich", "die", "Him\u00b7mels\u00b7Lich\u00b7ter", "zie\u00b7het", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ART", "APPR", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Krei\u00df/ der sonst den Weg weist andern Krei\u00dfen an/", "tokens": ["O", "Krei\u00df", "/", "der", "sonst", "den", "Weg", "weist", "an\u00b7dern", "Krei\u00b7\u00dfen", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "ART", "ADV", "ART", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was hat mein Unschuld doch zuwider dir gethan/", "tokens": ["Was", "hat", "mein", "Un\u00b7schuld", "doch", "zu\u00b7wi\u00b7der", "dir", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df man zur Plage mir dich also langsam siehet.", "tokens": ["Da\u00df", "man", "zur", "Pla\u00b7ge", "mir", "dich", "al\u00b7so", "lang\u00b7sam", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Minuten sind mir Tag/ und Stunden sind mir Jahre/", "tokens": ["Mi\u00b7nu\u00b7ten", "sind", "mir", "Tag", "/", "und", "Stun\u00b7den", "sind", "mir", "Jah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "$(", "KON", "NN", "VAFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zeit geschwinde F\u00fc\u00df und Fl\u00fcgel sind von Bley.", "tokens": ["Der", "Zeit", "ge\u00b7schwin\u00b7de", "F\u00fc\u00df", "und", "Fl\u00fc\u00b7gel", "sind", "von", "Bley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich glaube da\u00df die Nacht der Zimber k\u00fcrtzer sey/", "tokens": ["Ich", "glau\u00b7be", "da\u00df", "die", "Nacht", "der", "Zim\u00b7ber", "k\u00fcrt\u00b7zer", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "NN", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ich f\u00fcr meinem Tod ihr Ende nicht erfahre.", "tokens": ["Und", "ich", "f\u00fcr", "mei\u00b7nem", "Tod", "ihr", "En\u00b7de", "nicht", "er\u00b7fah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Penelope beschwert von vieler Freyer Menge/", "tokens": ["Pe\u00b7ne\u00b7lo\u00b7pe", "be\u00b7schwert", "von", "vie\u00b7ler", "Frey\u00b7er", "Men\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "PIAT", "NN", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "L\u00f6st auff den Abend auff/ was sie den Tag gemacht:", "tokens": ["L\u00f6st", "auff", "den", "A\u00b7bend", "auff", "/", "was", "sie", "den", "Tag", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "$(", "PWS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich schwere/ Ph\u00f6bus geht zur\u00fccke bey der Nacht/", "tokens": ["Ich", "schwe\u00b7re", "/", "Ph\u00f6\u00b7bus", "geht", "zu\u00b7r\u00fc\u00b7cke", "bey", "der", "Nacht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NE", "VVFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit er seinen Weg und meine Pein verl\u00e4nge.", "tokens": ["Da\u00b7mit", "er", "sei\u00b7nen", "Weg", "und", "mei\u00b7ne", "Pein", "ver\u00b7l\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}