{"dta.poem.11190": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ein jeder, was ihm schmeckt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man lacht den Daphnis aus, da\u00df er Lisetten k\u00fc\u00dft,", "tokens": ["Man", "lacht", "den", "Daph\u00b7nis", "aus", ",", "da\u00df", "er", "Li\u00b7set\u00b7ten", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil sie schlecht von geburth, und schlecht von sch\u00f6n-", "tokens": ["Weil", "sie", "schlecht", "von", "ge\u00b7burth", ",", "und", "schlecht", "von", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ADJD", "$,", "KON", "ADJD", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "heit ist.", "tokens": ["heit", "ist", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Gnung, da\u00df sie klugheit hat, wer fraget nach dem schmincken?", "tokens": ["Gnung", ",", "da\u00df", "sie", "klug\u00b7heit", "hat", ",", "wer", "fra\u00b7get", "nach", "dem", "schmin\u00b7cken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "PWS", "VVFIN", "APPR", "ART", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Man kan wohl guten wein auch aus dem done trincken.", "tokens": ["Man", "kan", "wohl", "gu\u00b7ten", "wein", "auch", "aus", "dem", "do\u00b7ne", "trin\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}