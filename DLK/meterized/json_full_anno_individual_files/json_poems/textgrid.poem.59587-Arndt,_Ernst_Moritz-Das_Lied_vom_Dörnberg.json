{"textgrid.poem.59587": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom D\u00f6rnberg", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein Freiherr fromm und gut", "tokens": ["Es", "war", "ein", "Frei\u00b7herr", "fromm", "und", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Kattenland und Kattenblut \u2013", "tokens": ["Vom", "Kat\u00b7ten\u00b7land", "und", "Kat\u00b7ten\u00b7blut", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O tapfres Land der Hessen! \u2013", "tokens": ["O", "tapf\u00b7res", "Land", "der", "Hes\u00b7sen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ha\u00dfte tief den welschen Tand,", "tokens": ["Der", "ha\u00df\u00b7te", "tief", "den", "wel\u00b7schen", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der konnte Ehr' und Vaterland", "tokens": ["Der", "konn\u00b7te", "Ehr'", "und", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Freiheit nicht vergessen.", "tokens": ["Und", "Frei\u00b7heit", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es hatt' auf den Landgrafenthron", "tokens": ["Es", "hatt'", "auf", "den", "Land\u00b7gra\u00b7fent\u00b7hron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Den Bruder sein Napoleon", "tokens": ["Den", "Bru\u00b7der", "sein", "Na\u00b7po\u00b7le\u00b7on"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Kaiserstolz gesetzet;", "tokens": ["Im", "Kai\u00b7ser\u00b7stolz", "ge\u00b7set\u00b7zet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Bruder hie\u00df Hieronymus,", "tokens": ["Der", "Bru\u00b7der", "hie\u00df", "Hie\u00b7ro\u00b7ny\u00b7mus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Weichling, der im Diebsgenu\u00df", "tokens": ["Ein", "Weich\u00b7ling", ",", "der", "im", "Diebs\u00b7ge\u00b7nu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wollust sich ergetzet.", "tokens": ["Der", "Wol\u00b7lust", "sich", "er\u00b7get\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das deucht dem edlen D\u00f6rnberg schlimm,", "tokens": ["Das", "deucht", "dem", "ed\u00b7len", "D\u00f6rn\u00b7berg", "schlimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er r\u00fcstet sich im Heldengrimm,", "tokens": ["Er", "r\u00fcs\u00b7tet", "sich", "im", "Hel\u00b7den\u00b7grimm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Buben will er schlagen,", "tokens": ["Den", "Bu\u00b7ben", "will", "er", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welschen will der Ritter wert", "tokens": ["Die", "Wel\u00b7schen", "will", "der", "Rit\u00b7ter", "wert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Spie\u00df und Stange, Kolb' und Schwert", "tokens": ["Mit", "Spie\u00df", "und", "Stan\u00b7ge", ",", "Kolb'", "und", "Schwert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weit \u00fcbern Rhein verjagen.", "tokens": ["Weit", "\u00fc\u00b7bern", "Rhein", "ver\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schon hat er klug sein Netz gespannt,", "tokens": ["Schon", "hat", "er", "klug", "sein", "Netz", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon h\u00e4lt ers Schwert in tapfrer Hand,", "tokens": ["Schon", "h\u00e4lt", "ers", "Schwert", "in", "tapf\u00b7rer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon warten seine Treuen,", "tokens": ["Schon", "war\u00b7ten", "sei\u00b7ne", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie brennen all von deutschem Mut,", "tokens": ["Sie", "bren\u00b7nen", "all", "von", "deut\u00b7schem", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie d\u00fcrsten all Franzosenblut", "tokens": ["Sie", "d\u00fcrs\u00b7ten", "all", "Fran\u00b7zo\u00b7sen\u00b7blut"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Durst der edlen Leuen.", "tokens": ["Mit", "Durst", "der", "ed\u00b7len", "Leu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das deuchte einem Schelm nicht recht,", "tokens": ["Das", "deuch\u00b7te", "ei\u00b7nem", "Schelm", "nicht", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Ritter, doch von Sinn ein Knecht,", "tokens": ["Ein", "Rit\u00b7ter", ",", "doch", "von", "Sinn", "ein", "Knecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An Ehren mi\u00dfgeboren,", "tokens": ["An", "Eh\u00b7ren", "mi\u00df\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sagt's dem K\u00f6nig alles aus,", "tokens": ["Der", "sagt's", "dem", "K\u00f6\u00b7nig", "al\u00b7les", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der r\u00fcstet sich und schirmt sein Haus", "tokens": ["Der", "r\u00fcs\u00b7tet", "sich", "und", "schirmt", "sein", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Wehr an T\u00fcrmen und Toren.", "tokens": ["Mit", "Wehr", "an", "T\u00fcr\u00b7men", "und", "To\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Da mu\u00df der edle D\u00f6rnberg fliehn,", "tokens": ["Da", "mu\u00df", "der", "ed\u00b7le", "D\u00f6rn\u00b7berg", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verr\u00e4ter sp\u00e4hen hinter ihn,", "tokens": ["Ver\u00b7r\u00e4\u00b7ter", "sp\u00e4\u00b7hen", "hin\u00b7ter", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Leben zu erlauschen;", "tokens": ["Sein", "Le\u00b7ben", "zu", "er\u00b7lau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er auf der Flucht mu\u00df ab und an", "tokens": ["Er", "auf", "der", "Flucht", "mu\u00df", "ab", "und", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VMFIN", "PTKVZ", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit manchem fremden Wandersmann", "tokens": ["Mit", "man\u00b7chem", "frem\u00b7den", "Wan\u00b7ders\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl Kleid und Kappe tauschen.", "tokens": ["Wohl", "Kleid", "und", "Kap\u00b7pe", "tau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bis er den wackern Braunschweig find't,", "tokens": ["Bis", "er", "den", "wa\u00b7ckern", "Braun\u00b7schweig", "find't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Welfen echtgebornes Kind,", "tokens": ["Der", "Wel\u00b7fen", "echt\u00b7ge\u00b7bor\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den treuen deutschen Degen;", "tokens": ["Den", "treu\u00b7en", "deut\u00b7schen", "De\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da mu\u00dft' noch mancher welsche Hund", "tokens": ["Da", "mu\u00dft'", "noch", "man\u00b7cher", "wel\u00b7sche", "Hund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich blutig auf den gr\u00fcnen Grund", "tokens": ["Sich", "blu\u00b7tig", "auf", "den", "gr\u00fc\u00b7nen", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch seinen S\u00e4bel legen.", "tokens": ["Durch", "sei\u00b7nen", "S\u00e4\u00b7bel", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie hauen sich wie M\u00e4nner durch,", "tokens": ["Sie", "hau\u00b7en", "sich", "wie", "M\u00e4n\u00b7ner", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann segeln sie zur Freiheitsburg,", "tokens": ["Dann", "se\u00b7geln", "sie", "zur", "Frei\u00b7heits\u00b7burg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Altengelland mit Namen;", "tokens": ["Al\u00b7ten\u00b7gel\u00b7land", "mit", "Na\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Da ruhen 