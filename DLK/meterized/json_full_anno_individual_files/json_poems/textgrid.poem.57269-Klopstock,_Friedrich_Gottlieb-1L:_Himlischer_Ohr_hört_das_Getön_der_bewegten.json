{"textgrid.poem.57269": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Himlischer Ohr h\u00f6rt das Get\u00f6n der bewegten", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Himlischer Ohr h\u00f6rt das Get\u00f6n der bewegten", "tokens": ["Him\u00b7li\u00b7scher", "Ohr", "h\u00f6rt", "das", "Ge\u00b7t\u00f6n", "der", "be\u00b7weg\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "+--++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sterne; den Gang, den Seleno und Pleione", "tokens": ["Ster\u00b7ne", ";", "den", "Gang", ",", "den", "Se\u00b7le\u00b7no", "und", "Plei\u00b7o\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "$,", "ART", "NE", "KON", "NN"], "meter": "+--+-+-+-+--", "measure": "iambic.penta.invert"}, "line.3": {"text": "Donnern, kent es, und freut hinh\u00f6rend", "tokens": ["Don\u00b7nern", ",", "kent", "es", ",", "und", "freut", "hin\u00b7h\u00f6\u00b7rend"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN", "VVPP"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sich des gefl\u00fcgelten Halls,", "tokens": ["Sich", "des", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "Halls", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Wenn der Planet fliehend sich w\u00e4lzt, und im Kreislauf", "tokens": ["Wenn", "der", "Pla\u00b7net", "flie\u00b7hend", "sich", "w\u00e4lzt", ",", "und", "im", "Kreis\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVPP", "PRF", "VVFIN", "$,", "KON", "APPRART", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eilet, und wenn, die im Glanze sich verbergen,", "tokens": ["Ei\u00b7let", ",", "und", "wenn", ",", "die", "im", "Glan\u00b7ze", "sich", "ver\u00b7ber\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "KOUS", "$,", "PRELS", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "+--++-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Um sich selber sich drehn! Sturmwinde", "tokens": ["Um", "sich", "sel\u00b7ber", "sich", "drehn", "!", "Sturm\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUI", "PRF", "ADV", "PRF", "VVINF", "$.", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Rauschen, und Meere dann her!", "tokens": ["Rau\u00b7schen", ",", "und", "Mee\u00b7re", "dann", "her", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Hesperus Meer, Meere des Monds, und der Erd', ihr", "tokens": ["Hes\u00b7pe\u00b7rus", "Meer", ",", "Mee\u00b7re", "des", "Monds", ",", "und", "der", "Erd'", ",", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "NN", "ART", "NN", "$,", "KON", "ART", "NN", "$,", "PPOSAT"], "meter": "+--++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sanfter, allein wie erhebt sichs im Bootes,", "tokens": ["Sanf\u00b7ter", ",", "al\u00b7lein", "wie", "er\u00b7hebt", "sichs", "im", "Boo\u00b7tes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOKOM", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "O wie th\u00fcrmt es empor! Hochwogig", "tokens": ["O", "wie", "th\u00fcrmt", "es", "em\u00b7por", "!", "Hoch\u00b7wo\u00b7gig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PTKVZ", "$.", "NE"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Donnerte am Felsengestad!", "tokens": ["Don\u00b7ner\u00b7te", "am", "Fel\u00b7sen\u00b7ge\u00b7stad", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Lauter noch schwebt dort der Altar, und die goldne", "tokens": ["Lau\u00b7ter", "noch", "schwebt", "dort", "der", "Al\u00b7tar", ",", "und", "die", "gold\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ADV", "ART", "NN", "$,", "KON", "ART", "ADJA"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "K\u00f6nigin dort, mit dem Palmzweig' in der Rechte!", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "dort", ",", "mit", "dem", "Palm\u00b7zweig'", "in", "der", "Rech\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Lauter schwingt sich der Schwan, und lauter", "tokens": ["Lau\u00b7ter", "schwingt", "sich", "der", "Schwan", ",", "und", "lau\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wehet die Rose daher.", "tokens": ["We\u00b7het", "die", "Ro\u00b7se", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Psalmengesang t\u00f6net darein! Die erhabnen", "tokens": ["Psal\u00b7men\u00b7ge\u00b7sang", "t\u00f6\u00b7net", "da\u00b7rein", "!", "Die", "er\u00b7hab\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PAV", "$.", "ART", "ADJA"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Feyrer am Thron, die Gerechten und Vollkomnen", "tokens": ["Fey\u00b7rer", "am", "Thron", ",", "die", "Ge\u00b7rech\u00b7ten", "und", "Voll\u00b7kom\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Singen Jubel und Preis! Anbetung!", "tokens": ["Sin\u00b7gen", "Ju\u00b7bel", "und", "Preis", "!", "An\u00b7be\u00b7tung", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$.", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Danken, sie k\u00f6nnen es, Gott!", "tokens": ["Dan\u00b7ken", ",", "sie", "k\u00f6n\u00b7nen", "es", ",", "Gott", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER", "$,", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Ahndung in mir, dunkles Gef\u00fchl der Entz\u00fcckung,", "tokens": ["Ahn\u00b7dung", "in", "mir", ",", "dunk\u00b7les", "Ge\u00b7f\u00fchl", "der", "Ent\u00b7z\u00fc\u00b7ckung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Welche den Staub an dem Staub einst unaussprechlich", "tokens": ["Wel\u00b7che", "den", "Staub", "an", "dem", "Staub", "einst", "un\u00b7aus\u00b7sprech\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "ART", "NN", "APPR", "ART", "NN", "ADV", "ADJD"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Tr\u00f6sten soll, o Gef\u00fchl, Weissager", "tokens": ["Tr\u00f6s\u00b7ten", "soll", ",", "o", "Ge\u00b7f\u00fchl", ",", "Weis\u00b7sa\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "VMFIN", "$,", "FM", "NN", "$,", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Inniger ewiger Ruh,", "tokens": ["In\u00b7ni\u00b7ger", "e\u00b7wi\u00b7ger", "Ruh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Lispel, entflohn jenem Gesang der entflamten", "tokens": ["Lis\u00b7pel", ",", "ent\u00b7flohn", "je\u00b7nem", "Ge\u00b7sang", "der", "ent\u00b7flam\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PDAT", "NN", "ART", "ADJA"], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "S\u00f6hne des Heils, o, besuch oft die beladnen", "tokens": ["S\u00f6h\u00b7ne", "des", "Heils", ",", "o", ",", "be\u00b7such", "oft", "die", "be\u00b7lad\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "FM", "$,", "ADV", "ADV", "ART", "ADJA"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Erdewanderer, kom mildth\u00e4tig,", "tokens": ["E\u00b7rde\u00b7wan\u00b7de\u00b7rer", ",", "kom", "mildt\u00b7h\u00e4\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Trockne des Weinenden Blick!", "tokens": ["Trock\u00b7ne", "des", "Wei\u00b7nen\u00b7den", "Blick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Strahlendes Heer, Welten! ist auch ein Erschafner", "tokens": ["Strah\u00b7len\u00b7des", "Heer", ",", "Wel\u00b7ten", "!", "ist", "auch", "ein", "Er\u00b7schaf\u00b7ner"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "$.", "VAFIN", "ADV", "ART", "NN"], "meter": "+--++-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Irgendwo noch, wie der Mensch, schwach? Es erschreckt uns", "tokens": ["Ir\u00b7gend\u00b7wo", "noch", ",", "wie", "der", "Mensch", ",", "schwach", "?", "Es", "er\u00b7schreckt", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "$.", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Unser Retter, der Tod! Sanft komt er,", "tokens": ["Un\u00b7ser", "Ret\u00b7ter", ",", "der", "Tod", "!", "Sanft", "komt", "er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$.", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Leis' im Gew\u00f6lke des Schlafs;", "tokens": ["Leis'", "im", "Ge\u00b7w\u00f6l\u00b7ke", "des", "Schlafs", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Aber er bleibt f\u00fcrchterlich uns, und wir sehn nur", "tokens": ["A\u00b7ber", "er", "bleibt", "f\u00fcrch\u00b7ter\u00b7lich", "uns", ",", "und", "wir", "sehn", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PPER", "$,", "KON", "PPER", "VVFIN", "ADV"], "meter": "+---+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nieder ins Grab, ob er gleich uns zur Vollendung", "tokens": ["Nie\u00b7der", "ins", "Grab", ",", "ob", "er", "gleich", "uns", "zur", "Vol\u00b7len\u00b7dung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "PPER", "APPRART", "NN"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.3": {"text": "F\u00fchrt, aus H\u00fcllen der Nacht hin\u00fcber", "tokens": ["F\u00fchrt", ",", "aus", "H\u00fcl\u00b7len", "der", "Nacht", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "NN", "ART", "NN", "ADV"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "In der Erkentnisse Land!", "tokens": ["In", "der", "Er\u00b7kent\u00b7nis\u00b7se", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Von der Geduld steinigem Pfad' in ein heitres", "tokens": ["Von", "der", "Ge\u00b7duld", "stei\u00b7ni\u00b7gem", "Pfad'", "in", "ein", "heit\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wonnegefild! zur Gesellschaft der Vollkomnen!", "tokens": ["Won\u00b7ne\u00b7ge\u00b7fild", "!", "zur", "Ge\u00b7sell\u00b7schaft", "der", "Voll\u00b7kom\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Aus dem Leben, das bald durch Felsen", "tokens": ["Aus", "dem", "Le\u00b7ben", ",", "das", "bald", "durch", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Z\u00f6gernder fliesset, und bald", "tokens": ["Z\u00f6\u00b7gern\u00b7der", "flies\u00b7set", ",", "und", "bald"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.11": {"line.1": {"text": "Fl\u00fcchtiger da, wo, zu verbl\u00fchn, die bekr\u00e4nzten", "tokens": ["Fl\u00fcch\u00b7ti\u00b7ger", "da", ",", "wo", ",", "zu", "ver\u00b7bl\u00fchn", ",", "die", "be\u00b7kr\u00e4nz\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "$,", "PTKZU", "VVINF", "$,", "ART", "ADJA"], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Fr\u00fchling' ihr Haupt in des Thaus Glanz und Ger\u00fcchen", "tokens": ["Fr\u00fch\u00b7ling'", "ihr", "Haupt", "in", "des", "Thaus", "Glanz", "und", "Ge\u00b7r\u00fc\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Schimmernd heben; es spiel' hinunter,", "tokens": ["Schim\u00b7mernd", "he\u00b7ben", ";", "es", "spiel'", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Oder es s\u00e4ume, Geschw\u00e4tz!", "tokens": ["O\u00b7der", "es", "s\u00e4u\u00b7me", ",", "Ge\u00b7schw\u00e4tz", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.12": {"line.1": {"text": "Himlischer Ohr h\u00f6rt das Get\u00f6n der bewegten", "tokens": ["Him\u00b7li\u00b7scher", "Ohr", "h\u00f6rt", "das", "Ge\u00b7t\u00f6n", "der", "be\u00b7weg\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "+--++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sterne; den Gang, den Seleno und Pleione", "tokens": ["Ster\u00b7ne", ";", "den", "Gang", ",", "den", "Se\u00b7le\u00b7no", "und", "Plei\u00b7o\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "$,", "ART", "NE", "KON", "NN"], "meter": "+--+-+-+-+--", "measure": "iambic.penta.invert"}, "line.3": {"text": "Donnern, kent es, und freut hinh\u00f6rend", "tokens": ["Don\u00b7nern", ",", "kent", "es", ",", "und", "freut", "hin\u00b7h\u00f6\u00b7rend"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN", "VVPP"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sich des gefl\u00fcgelten Halls,", "tokens": ["Sich", "des", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "Halls", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Wenn der Planet fliehend sich w\u00e4lzt, und im Kreislauf", "tokens": ["Wenn", "der", "Pla\u00b7net", "flie\u00b7hend", "sich", "w\u00e4lzt", ",", "und", "im", "Kreis\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVPP", "PRF", "VVFIN", "$,", "KON", "APPRART", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eilet, und wenn, die im Glanze sich verbergen,", "tokens": ["Ei\u00b7let", ",", "und", "wenn", ",", "die", "im", "Glan\u00b7ze", "sich", "ver\u00b7ber\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "KOUS", "$,", "PRELS", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "+--++-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Um sich selber sich drehn! Sturmwinde", "tokens": ["Um", "sich", "sel\u00b7ber", "sich", "drehn", "!", "Sturm\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUI", "PRF", "ADV", "PRF", "VVINF", "$.", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Rauschen, und Meere dann her!", "tokens": ["Rau\u00b7schen", ",", "und", "Mee\u00b7re", "dann", "her", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.14": {"line.1": {"text": "Hesperus Meer, Meere des Monds, und der Erd', ihr", "tokens": ["Hes\u00b7pe\u00b7rus", "Meer", ",", "Mee\u00b7re", "des", "Monds", ",", "und", "der", "Erd'", ",", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "NN", "ART", "NN", "$,", "KON", "ART", "NN", "$,", "PPOSAT"], "meter": "+--++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sanfter, allein wie erhebt sichs im Bootes,", "tokens": ["Sanf\u00b7ter", ",", "al\u00b7lein", "wie", "er\u00b7hebt", "sichs", "im", "Boo\u00b7tes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOKOM", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "O wie th\u00fcrmt es empor! Hochwogig", "tokens": ["O", "wie", "th\u00fcrmt", "es", "em\u00b7por", "!", "Hoch\u00b7wo\u00b7gig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PTKVZ", "$.", "NE"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Donnerte am Felsengestad!", "tokens": ["Don\u00b7ner\u00b7te", "am", "Fel\u00b7sen\u00b7ge\u00b7stad", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.15": {"line.1": {"text": "Lauter noch schwebt dort der Altar, und die goldne", "tokens": ["Lau\u00b7ter", "noch", "schwebt", "dort", "der", "Al\u00b7tar", ",", "und", "die", "gold\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ADV", "ART", "NN", "$,", "KON", "ART", "ADJA"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "K\u00f6nigin dort, mit dem Palmzweig' in der Rechte!", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "dort", ",", "mit", "dem", "Palm\u00b7zweig'", "in", "der", "Rech\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Lauter schwingt sich 