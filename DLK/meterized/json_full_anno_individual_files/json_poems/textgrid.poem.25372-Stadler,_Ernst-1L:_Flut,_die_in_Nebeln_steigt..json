{"textgrid.poem.25372": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Flut, die in Nebeln steigt.", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flut, die in Nebeln steigt.", "tokens": ["Flut", ",", "die", "in", "Ne\u00b7beln", "steigt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Flut, die versinkt.", "tokens": ["Flut", ",", "die", "ver\u00b7sinkt", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "O Gl\u00fcck: das gro\u00dfe Wasser,", "tokens": ["O", "Gl\u00fcck", ":", "das", "gro\u00b7\u00dfe", "Was\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "das mein Leben \u00fcberschwemmte, sinkt, ertrinkt.", "tokens": ["das", "mein", "Le\u00b7ben", "\u00fc\u00b7bersc\u00b7hwemm\u00b7te", ",", "sinkt", ",", "er\u00b7trinkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Schon wollen H\u00fcgel vor. Schon bricht ges\u00e4nftigt", "tokens": ["Schon", "wol\u00b7len", "H\u00fc\u00b7gel", "vor", ".", "Schon", "bricht", "ge\u00b7s\u00e4nf\u00b7tigt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line