{"dta.poem.16292": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich ri\u00df die Lieb' einmal zum Ha\u00df des Hasses hin,", "tokens": ["Mich", "ri\u00df", "die", "Lieb'", "ein\u00b7mal", "zum", "Ha\u00df", "des", "Has\u00b7ses", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Hasses gegen das, des Liebender ich bin.", "tokens": ["Des", "Has\u00b7ses", "ge\u00b7gen", "das", ",", "des", "Lie\u00b7ben\u00b7der", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDS", "$,", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mich reut mein Ha\u00df, nicht weil er nicht ha\u00dfw\u00fcrd'ges tr\u00e4fe,", "tokens": ["Mich", "reut", "mein", "Ha\u00df", ",", "nicht", "weil", "er", "nicht", "ha\u00df\u00b7w\u00fcrd'\u00b7ges", "tr\u00e4\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PTKNEG", "KOUS", "PPER", "PTKNEG", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ziemt die Nessel nicht um reine Liebesschl\u00e4fe.", "tokens": ["Doch", "ziemt", "die", "Nes\u00b7sel", "nicht", "um", "rei\u00b7ne", "Lie\u00b7bes\u00b7schl\u00e4\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "La\u00df hassen wer da will und be\u00dfres nicht vermag,", "tokens": ["La\u00df", "has\u00b7sen", "wer", "da", "will", "und", "be\u00df\u00b7res", "nicht", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PWS", "ADV", "VMFIN", "KON", "ADJA", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Lieb', und liebe du jung bis zum j\u00fcngsten Tag.", "tokens": ["O", "Lieb'", ",", "und", "lie\u00b7be", "du", "jung", "bis", "zum", "j\u00fcng\u00b7sten", "Tag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}