{"textgrid.poem.64897": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fruchtlos hab' in Schmerzenstoben", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fruchtlos hab' in Schmerzenstoben", "tokens": ["Frucht\u00b7los", "hab'", "in", "Schmer\u00b7zen\u00b7sto\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich vor dir geweint, geras't;", "tokens": ["Ich", "vor", "dir", "ge\u00b7weint", ",", "ge\u00b7ras't", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und die Welt, sie wird dich loben,", "tokens": ["Und", "die", "Welt", ",", "sie", "wird", "dich", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du mich verlassen hast.", "tokens": ["Da\u00df", "du", "mich", "ver\u00b7las\u00b7sen", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "R\u00fchmend wird sie zu dir sagen:", "tokens": ["R\u00fch\u00b7mend", "wird", "sie", "zu", "dir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbkluger Mann, der, stark und fest,", "tokens": ["\u00bb", "klu\u00b7ger", "Mann", ",", "der", ",", "stark", "und", "fest", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PRELS", "$,", "ADJD", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch gebrochner Seelen Klagen", "tokens": ["Durch", "ge\u00b7broch\u00b7ner", "See\u00b7len", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer sich beirren l\u00e4\u00dft;", "tokens": ["Nim\u00b7mer", "sich", "be\u00b7ir\u00b7ren", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dessen Wille gleich dem Pfeile", "tokens": ["Des\u00b7sen", "Wil\u00b7le", "gleich", "dem", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rastlos fliegt zum Ziele fort,", "tokens": ["Rast\u00b7los", "fliegt", "zum", "Zie\u00b7le", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob er auch in seiner Eile", "tokens": ["Ob", "er", "auch", "in", "sei\u00b7ner", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein befreundet Herz durchbohrt!", "tokens": ["Ein", "be\u00b7freun\u00b7det", "Herz", "durch\u00b7bohrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "troch