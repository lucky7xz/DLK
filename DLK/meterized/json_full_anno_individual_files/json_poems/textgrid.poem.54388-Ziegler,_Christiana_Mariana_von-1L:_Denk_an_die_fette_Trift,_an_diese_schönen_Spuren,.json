d", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PDAT", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dem will ich in der That durch mein Verfahren zeigen:", "tokens": ["Dem", "will", "ich", "in", "der", "That", "durch", "mein", "Ver\u00b7fah\u00b7ren", "zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich bleibe einerley im Reden und im Schweigen.", "tokens": ["Ich", "blei\u00b7be", "ei\u00b7ner\u00b7ley", "im", "Re\u00b7den", "und", "im", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dein fest gesetzter Geist ahmt mir in allem nach.", "tokens": ["Dein", "fest", "ge\u00b7setz\u00b7ter", "Geist", "ahmt", "mir", "in", "al\u00b7lem", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und also f\u00fcrchtst du dich vor keinem Ungemach.", "tokens": ["Und", "al\u00b7so", "f\u00fcrchtst", "du", "dich", "vor", "kei\u00b7nem", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So gr\u00fcsse wenn du willst die Stadt und unsre Linden.", "tokens": ["So", "gr\u00fcs\u00b7se", "wenn", "du", "willst", "die", "Stadt", "und", "uns\u00b7re", "Lin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "PPER", "VMFIN", "ART", "NN", "KON", "PPOSAT", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du wirst mich allezeit als deine Freundinn finden.", "tokens": ["Du", "wirst", "mich", "al\u00b7le\u00b7zeit", "als", "dei\u00b7ne", "Freun\u00b7dinn", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "KOKOM", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich unterzeichne dies mit meiner eignen Hand,", "tokens": ["Ich", "un\u00b7ter\u00b7zeich\u00b7ne", "dies", "mit", "mei\u00b7ner", "eig\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich kenne mich zuwohl und keinen Unbestand.", "tokens": ["Ich", "ken\u00b7ne", "mich", "zu\u00b7wohl", "und", "kei\u00b7nen", "Un\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}