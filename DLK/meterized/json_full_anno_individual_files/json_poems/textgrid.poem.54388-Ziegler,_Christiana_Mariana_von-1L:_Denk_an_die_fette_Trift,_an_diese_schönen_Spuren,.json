{"textgrid.poem.54388": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Denk an die fette Trift, an diese sch\u00f6nen Spuren,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk an die fette Trift, an diese sch\u00f6nen Spuren,", "tokens": ["Denk", "an", "die", "fet\u00b7te", "Trift", ",", "an", "die\u00b7se", "sch\u00f6\u00b7nen", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denk an die Sch\u00e4ferey, an Auen, Wies und Fluhren,", "tokens": ["Denk", "an", "die", "Sch\u00e4\u00b7fe\u00b7rey", ",", "an", "Au\u00b7en", ",", "Wies", "und", "Fluh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An unsre Lindenstadt, an Scherz, an Lust, und Spiel,", "tokens": ["An", "uns\u00b7re", "Lin\u00b7den\u00b7stadt", ",", "an", "Scherz", ",", "an", "Lust", ",", "und", "Spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An unsrer Lieder Klang, und was dir wohlgefiel:", "tokens": ["An", "uns\u00b7rer", "Lie\u00b7der", "Klang", ",", "und", "was", "dir", "wohl\u00b7ge\u00b7fiel", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "KON", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So weis ich ganz gewi\u00df, du wirst im Bade lachen,", "tokens": ["So", "weis", "ich", "ganz", "ge\u00b7wi\u00df", ",", "du", "wirst", "im", "Ba\u00b7de", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "PPER", "ADV", "ADV", "$,", "PPER", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dir so leichte nicht vergebne Grillen machen.", "tokens": ["Und", "dir", "so", "leich\u00b7te", "nicht", "ver\u00b7geb\u00b7ne", "Gril\u00b7len", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJA", "PTKNEG", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir denken auch an dich, gelehrter, \u2013 \u2013 \u2013", "tokens": ["Wir", "den\u00b7ken", "auch", "an", "dich", ",", "ge\u00b7lehr\u00b7ter", ",", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "ADJA", "$,", "$(", "$(", "$("], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und ich erinnre mich, was ich dir schuldig bin.", "tokens": ["Und", "ich", "e\u00b7rinn\u00b7re", "mich", ",", "was", "ich", "dir", "schul\u00b7dig", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PWS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein werthgesch\u00e4tzter Freund, das Schicksal soll dich sch\u00fctzen;", "tokens": ["Mein", "werth\u00b7ge\u00b7sch\u00e4tz\u00b7ter", "Freund", ",", "das", "Schick\u00b7sal", "soll", "dich", "sch\u00fct\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du sollst dem Gl\u00fccke stets in seinem Schoo\u00dfe sitzen.", "tokens": ["Du", "sollst", "dem", "Gl\u00fc\u00b7cke", "stets", "in", "sei\u00b7nem", "Schoo\u00b7\u00dfe", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Himmel leite dich, bis du den Wunsch erf\u00fcllt,", "tokens": ["Der", "Him\u00b7mel", "lei\u00b7te", "dich", ",", "bis", "du", "den", "Wunsch", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und das dereinst erlangt, was deine Sehnsucht stillt.", "tokens": ["Und", "das", "de\u00b7reinst", "er\u00b7langt", ",", "was", "dei\u00b7ne", "Sehn\u00b7sucht", "stillt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VVPP", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dank und Erkennlichkeit soll unver\u00e4ndert bleiben;", "tokens": ["Dank", "und", "Er\u00b7kenn\u00b7lich\u00b7keit", "soll", "un\u00b7ver\u00b7\u00e4n\u00b7dert", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Ich werde dann und wann ein kleines Briefchen schreiben.", "tokens": ["Ich", "wer\u00b7de", "dann", "und", "wann", "ein", "klei\u00b7nes", "Brief\u00b7chen", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KON", "PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Entfernung trennt f\u00fcrwahr die wahre Freundschaft nicht.", "tokens": ["Ent\u00b7fer\u00b7nung", "trennt", "f\u00fcr\u00b7wahr", "die", "wah\u00b7re", "Freund\u00b7schaft", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und wer mir diesem Satz, aus Vorwitz widerspricht,", "tokens": ["Und", "wer", "mir", "die\u00b7sem", "Satz", ",", "aus", "Vor\u00b7witz", "wi\u00b7der\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PDAT", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dem will ich in der That durch mein Verfahren zeigen:", "tokens": ["Dem", "will", "ich", "in", "der", "That", "durch", "mein", "Ver\u00b7fah\u00b7ren", "zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich bleibe einerley im Reden und im Schweigen.", "tokens": ["Ich", "blei\u00b7be", "ei\u00b7ner\u00b7ley", "im", "Re\u00b7den", "und", "im", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dein fest gesetzter Geist ahmt mir in allem nach.", "tokens": ["Dein", "fest", "ge\u00b7setz\u00b7ter", "Geist", "ahmt", "mir", "in", "al\u00b7lem", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und also f\u00fcrchtst du dich vor keinem Ungemach.", "tokens": ["Und", "al\u00b7so", "f\u00fcrchtst", "du", "dich", "vor", "kei\u00b7nem", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So gr\u00fcsse wenn du willst die Stadt und unsre Linden.", "tokens": ["So", "gr\u00fcs\u00b7se", "wenn", "du", "willst", "die", "Stadt", "und", "uns\u00b7re", "Lin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "PPER", "VMFIN", "ART", "NN", "KON", "PPOSAT", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du wirst mich allezeit als deine Freundinn finden.", "tokens": ["Du", "wirst", "mich", "al\u00b7le\u00b7zeit", "als", "dei\u00b7ne", "Freun\u00b7dinn", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "KOKOM", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich unterzeichne dies mit meiner eignen Hand,", "tokens": ["Ich", "un\u00b7ter\u00b7zeich\u00b7ne", "dies", "mit", "mei\u00b7ner", "eig\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich kenne mich zuwohl und keinen Unbestand.", "tokens": ["Ich", "ken\u00b7ne", "mich", "zu\u00b7wohl", "und", "kei\u00b7nen", "Un\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denk an die fette Trift, an diese sch\u00f6nen Spuren,", "tokens": ["Denk", "an", "die", "fet\u00b7te", "Trift", ",", "an", "die\u00b7se", "sch\u00f6\u00b7nen", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denk an die Sch\u00e4ferey, an Auen, Wies und Fluhren,", "tokens": ["Denk", "an", "die", "Sch\u00e4\u00b7fe\u00b7rey", ",", "an", "Au\u00b7en", ",", "Wies", "und", "Fluh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An unsre Lindenstadt, an Scherz, an Lust, und