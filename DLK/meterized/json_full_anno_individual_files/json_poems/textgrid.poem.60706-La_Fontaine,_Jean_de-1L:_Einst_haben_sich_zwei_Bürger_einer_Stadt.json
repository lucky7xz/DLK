{"textgrid.poem.60706": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst haben sich zwei B\u00fcrger einer Stadt", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst haben sich zwei B\u00fcrger einer Stadt", "tokens": ["Einst", "ha\u00b7ben", "sich", "zwei", "B\u00fcr\u00b7ger", "ei\u00b7ner", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "CARD", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Best\u00e4ndig um den Rang gestritten;", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "um", "den", "Rang", "ge\u00b7strit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der eine war ein reicher Nimmersatt,", "tokens": ["Der", "ei\u00b7ne", "war", "ein", "rei\u00b7cher", "Nim\u00b7mer\u00b7satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unwissend und von groben Sitten,", "tokens": ["Un\u00b7wis\u00b7send", "und", "von", "gro\u00b7ben", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der andre arm, doch gro\u00df an Geistesgaben.", "tokens": ["Der", "and\u00b7re", "arm", ",", "doch", "gro\u00df", "an", "Geis\u00b7tes\u00b7ga\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "$,", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Reiche wollte alle Ehre haben;", "tokens": ["Der", "Rei\u00b7che", "woll\u00b7te", "al\u00b7le", "Eh\u00b7re", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er meinte, jeder sei verpflichtet gar,", "tokens": ["Er", "mein\u00b7te", ",", "je\u00b7der", "sei", "ver\u00b7pflich\u00b7tet", "gar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VAFIN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ob seiner Sch\u00e4tze ihn zu preisen.", "tokens": ["Ob", "sei\u00b7ner", "Sch\u00e4t\u00b7ze", "ihn", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Was soll man G\u00fctern, des Verdienstes bar,", "tokens": ["Was", "soll", "man", "G\u00fc\u00b7tern", ",", "des", "Ver\u00b7diens\u00b7tes", "bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Achtung wie edlem Geistesgut erweisen?", "tokens": ["Ach\u00b7tung", "wie", "ed\u00b7lem", "Geis\u00b7tes\u00b7gut", "er\u00b7wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So frage ich. Der Dumme aber war", "tokens": ["So", "fra\u00b7ge", "ich", ".", "Der", "Dum\u00b7me", "a\u00b7ber", "war"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ART", "NN", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ganz andrer Meinung, und er sprach zum Weisen:", "tokens": ["Ganz", "an\u00b7drer", "Mei\u00b7nung", ",", "und", "er", "sprach", "zum", "Wei\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "\u00bbmein Freund, Ihr haltet Euch f\u00fcr wert,", "tokens": ["\u00bb", "mein", "Freund", ",", "Ihr", "hal\u00b7tet", "Euch", "f\u00fcr", "wert", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Doch kann man je bei Weisen trefflich speisen?", "tokens": ["Doch", "kann", "man", "je", "bei", "Wei\u00b7sen", "treff\u00b7lich", "spei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADV", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Was n\u00fctzt belesen sein und hochgelehrt?", "tokens": ["Was", "n\u00fctzt", "be\u00b7le\u00b7sen", "sein", "und", "hoch\u00b7ge\u00b7lehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "VAINF", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Man mu\u00df zu Euch ins dritte Stockwerk reisen,", "tokens": ["Man", "mu\u00df", "zu", "Euch", "ins", "drit\u00b7te", "Stock\u00b7werk", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ihr kleidet Euch im Winter wie im Mai", "tokens": ["Ihr", "klei\u00b7det", "Euch", "im", "Win\u00b7ter", "wie", "im", "Mai"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und habt nur Euren Schatten als Lakai.", "tokens": ["Und", "habt", "nur", "Eu\u00b7ren", "Schat\u00b7ten", "als", "La\u00b7kai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Wie sollte wohl ein Staat bestehen k\u00f6nnen", "tokens": ["Wie", "soll\u00b7te", "wohl", "ein", "Staat", "be\u00b7ste\u00b7hen", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "NN", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Von Leuten, die sich keinen Luxus g\u00f6nnen?", "tokens": ["Von", "Leu\u00b7ten", ",", "die", "sich", "kei\u00b7nen", "Lu\u00b7xus", "g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Brauchbare B\u00fcrger m\u00fcssen Geld verbreiten!", "tokens": ["Brauch\u00b7ba\u00b7re", "B\u00fcr\u00b7ger", "m\u00fcs\u00b7sen", "Geld", "ver\u00b7brei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.22": {"text": "Seht uns, wie wir durch Lustbarkeiten", "tokens": ["Seht", "uns", ",", "wie", "wir", "durch", "Lust\u00b7bar\u00b7kei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Der ganzen Stadt Verdienst bereiten,", "tokens": ["Der", "gan\u00b7zen", "Stadt", "Ver\u00b7dienst", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Dem K\u00fcnstler wie dem Handelsmann,", "tokens": ["Dem", "K\u00fcnst\u00b7ler", "wie", "dem", "Han\u00b7dels\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Dem, der die R\u00f6cke macht, und denen, die sie tragen!", "tokens": ["Dem", ",", "der", "die", "R\u00f6\u00b7cke", "macht", ",", "und", "de\u00b7nen", ",", "die", "sie", "tra\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "KON", "PDS", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dagegen Ihr? Ihr fertigt B\u00fccher an,", "tokens": ["Da\u00b7ge\u00b7gen", "Ihr", "?", "Ihr", "fer\u00b7tigt", "B\u00fc\u00b7cher", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "$.", "PPER", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Die mit den Namen hoher G\u00f6nner prahlen", "tokens": ["Die", "mit", "den", "Na\u00b7men", "ho\u00b7her", "G\u00f6n\u00b7ner", "prah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und die wahrhaftig sonst nicht viel besagen,", "tokens": ["Und", "die", "wahr\u00b7haf\u00b7tig", "sonst", "nicht", "viel", "be\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Als da\u00df Belobte Schlechtes gut bezahlen.\u00ab", "tokens": ["Als", "da\u00df", "Be\u00b7lob\u00b7te", "Schlech\u00b7tes", "gut", "be\u00b7zah\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "KOUS", "ADJA", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Es fand der unversch\u00e4mte Redeschwall,", "tokens": ["Es", "fand", "der", "un\u00b7ver\u00b7sch\u00e4m\u00b7te", "Re\u00b7de\u00b7schwall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Was er verdiente: keinen Widerhall!", "tokens": ["Was", "er", "ver\u00b7dien\u00b7te", ":", "kei\u00b7nen", "Wi\u00b7der\u00b7hall", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$.", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Der Weise schwieg, er sparte sich sogar", "tokens": ["Der", "Wei\u00b7se", "schwieg", ",", "er", "spar\u00b7te", "sich", "so\u00b7gar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Eine Satire, die gewi\u00df am Platze war.", "tokens": ["Ei\u00b7ne", "Sa\u00b7ti\u00b7re", ",", "die", "ge\u00b7wi\u00df", "am", "Plat\u00b7ze", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "Weit besser r\u00e4chten ihn die Kriegesplagen,", "tokens": ["Weit", "bes\u00b7ser", "r\u00e4ch\u00b7ten", "ihn", "die", "Krie\u00b7ges\u00b7pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.35": {"text": "Die bald darauf der Feind ins Land getragen.", "tokens": ["Die", "bald", "da\u00b7rauf", "der", "Feind", "ins", "Land", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PAV", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Die Stadt wird eingenommen und zerst\u00f6rt,", "tokens": ["Die", "Stadt", "wird", "ein\u00b7ge\u00b7nom\u00b7men", "und", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Schutt ist die Pracht, die jenem Protz geh\u00f6rt,", "tokens": ["Schutt", "ist", "die", "Pracht", ",", "die", "je\u00b7nem", "Protz", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.38": {"text": "Und wie der Arme mu\u00df auch der Verarmte fliehn.", "tokens": ["Und", "wie", "der", "Ar\u00b7me", "mu\u00df", "auch", "der", "Ver\u00b7arm\u00b7te", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Er irrte um und wu\u00dfte nicht wohin.", "tokens": ["Er", "irr\u00b7te", "um", "und", "wu\u00df\u00b7te", "nicht", "wo\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKNEG", "PWAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Man wies Unbildung allerorten ab,", "tokens": ["Man", "wies", "Un\u00b7bil\u00b7dung", "al\u00b7le\u00b7ror\u00b7ten", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Indes dem Weisen gern man Hilfe gab,", "tokens": ["In\u00b7des", "dem", "Wei\u00b7sen", "gern", "man", "Hil\u00b7fe", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Denn Wissen wurde allerorts gesch\u00e4tzt.", "tokens": ["Denn", "Wis\u00b7sen", "wur\u00b7de", "al\u00b7le\u00b7rorts", "ge\u00b7sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Und das entschied den Streit zu guter Letzt.", "tokens": ["Und", "das", "ent\u00b7schied", "den", "Streit", "zu", "gu\u00b7ter", "Letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was auch die Dummheit knurrt und bellt,", "tokens": ["Was", "auch", "die", "Dumm\u00b7heit", "knurrt", "und", "bellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wissenschaft gilt mehr als Geld.", "tokens": ["Die", "Wis\u00b7sen\u00b7schaft", "gilt", "mehr", "als", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Einst haben sich zwei B\u00fcrger einer Stadt", "tokens": ["Einst", "ha\u00b7ben", "sich", "zwei", "B\u00fcr\u00b7ger", "ei\u00b7ner", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "CARD", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Best\u00e4ndig um den Rang gestritten;", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "um", "den", "Rang", "ge\u00b7strit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der eine war ein reicher Nimmersatt,", "tokens": ["Der", "ei\u00b7ne", "war", "ein", "rei\u00b7cher", "Nim\u00b7mer\u00b7satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Unwissend und von groben Sitten,", "tokens": ["Un\u00b7wis\u00b7send", "und", "von", "gro\u00b7ben", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word"