{"textgrid.poem.58331": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "369. Auserw\u00e4hlt, Gott verm\u00e4hlt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denke, Gott, das h\u00f6chste Wesen,", "tokens": ["Den\u00b7ke", ",", "Gott", ",", "das", "h\u00f6chs\u00b7te", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat dich ihm zur Lust erlesen", "tokens": ["Hat", "dich", "ihm", "zur", "Lust", "er\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erkauft zu seiner Braut;", "tokens": ["Und", "er\u00b7kauft", "zu", "sei\u00b7ner", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rein, alleine bist du seine,", "tokens": ["Rein", ",", "al\u00b7lei\u00b7ne", "bist", "du", "sei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihn nur lieb und lauter meine,", "tokens": ["Ihn", "nur", "lieb", "und", "lau\u00b7ter", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stets sein Auge auf dich schaut!", "tokens": ["Stets", "sein", "Au\u00b7ge", "auf", "dich", "schaut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denke, Gott, das h\u00f6chste Wesen,", "tokens": ["Den\u00b7ke", ",", "Gott", ",", "das", "h\u00f6chs\u00b7te", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat dich ihm zur Lust erlesen", "tokens": ["Hat", "dich", "ihm", "zur", "Lust", "er\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erkauft zu seiner Braut;", "tokens": ["Und", "er\u00b7kauft", "zu", "sei\u00b7ner", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rein, alleine bist du seine,", "tokens": ["Rein", ",", "al\u00b7lei\u00b7ne", "bist", "du", "sei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihn nur lieb und lauter meine,", "tokens": ["Ihn", "nur", "lieb", "und", "lau\u00b7ter", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stets sein Auge auf dich schaut!", "tokens": ["Stets", "sein", "Au\u00b7ge", "auf", "dich", "schaut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punc