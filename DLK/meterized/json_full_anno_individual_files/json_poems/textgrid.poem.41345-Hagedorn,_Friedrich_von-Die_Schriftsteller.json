{"textgrid.poem.41345": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Schriftsteller", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was gibt dem, was man schreibt, der Dauer Sicherheit?", "tokens": ["Was", "gibt", "dem", ",", "was", "man", "schreibt", ",", "der", "Dau\u00b7er", "Si\u00b7cher\u00b7heit", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "$,", "PRELS", "PIS", "VVFIN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht Ordnung, nicht Geschmack: nicht Flei\u00df, noch Gr\u00fcndlichkeit.", "tokens": ["Nicht", "Ord\u00b7nung", ",", "nicht", "Ge\u00b7schmack", ":", "nicht", "Flei\u00df", ",", "noch", "Gr\u00fcnd\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$.", "PTKNEG", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht immer ist es g'nug, der Welt durch Wahrheit n\u00fctzen,", "tokens": ["Nicht", "im\u00b7mer", "ist", "es", "g'\u00b7nug", ",", "der", "Welt", "durch", "Wahr\u00b7heit", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "PPER", "ADV", "$,", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nicht g'nug, der Alten Geist, der Neuern Witz besitzen:", "tokens": ["Nicht", "g'\u00b7nug", ",", "der", "Al\u00b7ten", "Geist", ",", "der", "Neu\u00b7ern", "Witz", "be\u00b7sit\u00b7zen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Am wenigsten genug, da\u00df man vor seine Schrift", "tokens": ["Am", "we\u00b7nigs\u00b7ten", "ge\u00b7nug", ",", "da\u00df", "man", "vor", "sei\u00b7ne", "Schrift"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "PIS", "ADV", "$,", "KOUS", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "M\u00e4cenen stellt, sie preist, und sittsam \u00fcbertrifft,", "tokens": ["M\u00e4\u00b7ce\u00b7nen", "stellt", ",", "sie", "preist", ",", "und", "sitt\u00b7sam", "\u00fc\u00b7bert\u00b7rifft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VVFIN", "$,", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df auch von unserm Werth die \u00f6ffentlichen Proben", "tokens": ["Da\u00df", "auch", "von", "un\u00b7serm", "Werth", "die", "\u00f6f\u00b7fent\u00b7li\u00b7chen", "Pro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Kein Kritikus verruft, und zwanzig Vettern loben,", "tokens": ["Kein", "Kri\u00b7ti\u00b7kus", "ver\u00b7ruft", ",", "und", "zwan\u00b7zig", "Vet\u00b7tern", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KON", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df ein beredter Held im sch\u00e4rfsten Vorbericht,", "tokens": ["Da\u00df", "ein", "be\u00b7red\u00b7ter", "Held", "im", "sch\u00e4rfs\u00b7ten", "Vor\u00b7be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "F\u00fcr unsers Namens Ruhm mit allen Tropen ficht.", "tokens": ["F\u00fcr", "un\u00b7sers", "Na\u00b7mens", "Ruhm", "mit", "al\u00b7len", "Tro\u00b7pen", "ficht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Oft wird das beste Buch durch andere begraben!", "tokens": ["Oft", "wird", "das", "bes\u00b7te", "Buch", "durch", "an\u00b7de\u00b7re", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein Buch, das leben soll, mu\u00df seinen Schutzgeist haben.", "tokens": ["Ein", "Buch", ",", "das", "le\u00b7ben", "soll", ",", "mu\u00df", "sei\u00b7nen", "Schutz\u00b7geist", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVINF", "VMFIN", "$,", "VMFIN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was gibt dem, was man schreibt, der Dauer Sicherheit?", "tokens": ["Was", "gibt", "dem", ",", "was", "man", "schreibt", ",", "der", "Dau\u00b7er", "Si\u00b7cher\u00b7heit", "?"], "token_info": ["word", "word", 