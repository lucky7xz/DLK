", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "An solchem Ort f\u00e4ngt an dein Leid,", "tokens": ["An", "sol\u00b7chem", "Ort", "f\u00e4ngt", "an", "dein", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies, Jesu, ist, was uns andeut,", "tokens": ["Dies", ",", "Je\u00b7su", ",", "ist", ",", "was", "uns", "an\u00b7deut", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NE", "$,", "VAFIN", "$,", "PRELS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df uns dein Leiden, deine Pein", "tokens": ["Da\u00df", "uns", "dein", "Lei\u00b7den", ",", "dei\u00b7ne", "Pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Bot' zum Frieden solle seyn", "tokens": ["Ein", "Bot'", "zum", "Frie\u00b7den", "sol\u00b7le", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VMFIN", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df, da du dich wirfst zur Erd,", "tokens": ["Und", "da\u00df", ",", "da", "du", "dich", "wirfst", "zur", "Erd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "KOUS", "PPER", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Vaters Zorn gehoben werd.", "tokens": ["Des", "Va\u00b7ters", "Zorn", "ge\u00b7ho\u00b7ben", "werd", "."], "token_inf