{"dta.poem.887": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "DiE SCH\u00d6NE STADT", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Alte Pl\u00e4tze sonnig schweigen.", "tokens": ["Al\u00b7te", "Pl\u00e4t\u00b7ze", "son\u00b7nig", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief in Blau und Gold versponnen", "tokens": ["Tief", "in", "Blau", "und", "Gold", "ver\u00b7spon\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Traumhaft hasten sanfte Nonnen", "tokens": ["Traum\u00b7haft", "has\u00b7ten", "sanf\u00b7te", "Non\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter schw\u00fcler Buchen Schweigen.", "tokens": ["Un\u00b7ter", "schw\u00fc\u00b7ler", "Bu\u00b7chen", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus den braun erhellten Kirchen", "tokens": ["Aus", "den", "braun", "er\u00b7hell\u00b7ten", "Kir\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaun des Todes reine Bilder,", "tokens": ["Schaun", "des", "To\u00b7des", "rei\u00b7ne", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gro\u00dfer F\u00fcrsten sch\u00f6ne Schilder.", "tokens": ["Gro\u00b7\u00dfer", "F\u00fcrs\u00b7ten", "sch\u00f6\u00b7ne", "Schil\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kronen schimmern in den Kirchen.", "tokens": ["Kro\u00b7nen", "schim\u00b7mern", "in", "den", "Kir\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "R\u00f6sser tauchen aus dem Brunnen.", "tokens": ["R\u00f6s\u00b7ser", "tau\u00b7chen", "aus", "dem", "Brun\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fctenkrallen drohn aus B\u00e4umen.", "tokens": ["Bl\u00fc\u00b7ten\u00b7kral\u00b7len", "drohn", "aus", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Knaben spielen wirr von Tr\u00e4umen", "tokens": ["Kna\u00b7ben", "spie\u00b7len", "wirr", "von", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Abends leise dort am Brunnen.", "tokens": ["A\u00b7bends", "lei\u00b7se", "dort", "am", "Brun\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "M\u00e4dchen stehen an den Toren,", "tokens": ["M\u00e4d\u00b7chen", "ste\u00b7hen", "an", "den", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schauen scheu ins farbige Leben.", "tokens": ["Schau\u00b7en", "scheu", "ins", "far\u00b7bi\u00b7ge", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ihre feuchten Lippen beben", "tokens": ["Ih\u00b7re", "feuch\u00b7ten", "Lip\u00b7pen", "be\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie warten an den Toren.", "tokens": ["Und", "sie", "war\u00b7ten", "an", "den", "To\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zitternd flattern Glockenklange,", "tokens": ["Zit\u00b7ternd", "flat\u00b7tern", "Glo\u00b7cken\u00b7klan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VVINF", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Marschtakt hallt und Wacherufen.", "tokens": ["Marschtakt", "hallt", "und", "Wa\u00b7che\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fremde lauschen auf den Stufen.", "tokens": ["Frem\u00b7de", "lau\u00b7schen", "auf", "den", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch im Blau sind Orgelkl\u00e4nge.", "tokens": ["Hoch", "im", "Blau", "sind", "Or\u00b7gel\u00b7kl\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Helle Instrumente singen.", "tokens": ["Hel\u00b7le", "Inst\u00b7ru\u00b7men\u00b7te", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Durch der Garten Bl\u00e4tterrahmen", "tokens": ["Durch", "der", "Gar\u00b7ten", "Bl\u00e4t\u00b7ter\u00b7rah\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwirrt das Lachen sch\u00f6ner Damen.", "tokens": ["Schwirrt", "das", "La\u00b7chen", "sch\u00f6\u00b7ner", "Da\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leise junge M\u00fctter singen.", "tokens": ["Lei\u00b7se", "jun\u00b7ge", "M\u00fct\u00b7ter", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Heimlich haucht an blumigen Fenstern", "tokens": ["Heim\u00b7lich", "haucht", "an", "blu\u00b7mi\u00b7gen", "Fens\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Duft von Weihrauch, Teer und Flieder.", "tokens": ["Duft", "von", "Weih\u00b7rauch", ",", "Teer", "und", "Flie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Silbern flimmern m\u00fcde Lider", "tokens": ["Sil\u00b7bern", "flim\u00b7mern", "m\u00fc\u00b7de", "Li\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die Blumen an den Fenstern.", "tokens": ["Durch", "die", "Blu\u00b7men", "an", "den", "Fens\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}