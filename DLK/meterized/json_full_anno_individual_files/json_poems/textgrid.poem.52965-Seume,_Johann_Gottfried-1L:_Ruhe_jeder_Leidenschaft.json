{"textgrid.poem.52965": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ruhe jeder Leidenschaft", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ruhe jeder Leidenschaft", "tokens": ["Ru\u00b7he", "je\u00b7der", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word"], "pos": ["NN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4nkt das Herz mit G\u00f6tterkraft;", "tokens": ["Tr\u00e4nkt", "das", "Herz", "mit", "G\u00f6t\u00b7ter\u00b7kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhe st\u00e4hlet Sehn' und Mark,", "tokens": ["Ru\u00b7he", "st\u00e4h\u00b7let", "Sehn'", "und", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht zu jeder B\u00fcrde stark.", "tokens": ["Macht", "zu", "je\u00b7der", "B\u00fcr\u00b7de", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ruhe f\u00fchrt des Sehers Sinn", "tokens": ["Ru\u00b7he", "f\u00fchrt", "des", "Se\u00b7hers", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6her durch die Welten hin,", "tokens": ["H\u00f6\u00b7her", "durch", "die", "Wel\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo er Orionen mi\u00dft", "tokens": ["Wo", "er", "O\u00b7rio\u00b7nen", "mi\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und der Erde Sand vergi\u00dft.", "tokens": ["Und", "der", "Er\u00b7de", "Sand", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ruhe senkt des Weisen Blick", "tokens": ["Ru\u00b7he", "senkt", "des", "Wei\u00b7sen", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefer zu der Br\u00fcder Gl\u00fcck;", "tokens": ["Tie\u00b7fer", "zu", "der", "Br\u00fc\u00b7der", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhe mi\u00dft am Lebensstab", "tokens": ["Ru\u00b7he", "mi\u00dft", "am", "Le\u00b7bens\u00b7stab"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Richtig Zweck und Mittel ab.", "tokens": ["Rich\u00b7tig", "Zweck", "und", "Mit\u00b7tel", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ruhe z\u00fcckt des Kriegers Schwert", "tokens": ["Ru\u00b7he", "z\u00fcckt", "des", "Krie\u00b7gers", "Schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blitzender f\u00fcr Haus und Herd;", "tokens": ["Blit\u00b7zen\u00b7der", "f\u00fcr", "Haus", "und", "Herd", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhe biethet der Gefahr", "tokens": ["Ru\u00b7he", "bie\u00b7thet", "der", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fester Stirn und Busen dar.", "tokens": ["Fes\u00b7ter", "Stirn", "und", "Bu\u00b7sen", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ruhe scheucht wie Sonnenblick", "tokens": ["Ru\u00b7he", "scheucht", "wie", "Son\u00b7nen\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KOKOM", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nebel von dem Pfad zur\u00fcck;", "tokens": ["Ne\u00b7bel", "von", "dem", "Pfad", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhe lehrt, was gut und sch\u00f6n,", "tokens": ["Ru\u00b7he", "lehrt", ",", "was", "gut", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem hellsten Lichte sehn,", "tokens": ["In", "dem", "hells\u00b7ten", "Lich\u00b7te", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ruhe reihet jedes Ding", "tokens": ["Ru\u00b7he", "rei\u00b7het", "je\u00b7des", "Ding"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Kette rechten Ring;", "tokens": ["In", "der", "Ket\u00b7te", "rech\u00b7ten", "Ring", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhe bleibet, immer rein,", "tokens": ["Ru\u00b7he", "blei\u00b7bet", ",", "im\u00b7mer", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder Freude Probestein.", "tokens": ["Je\u00b7der", "Freu\u00b7de", "Pro\u00b7be\u00b7stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ruhe zieht aus Gottes Luft", "tokens": ["Ru\u00b7he", "zieht", "aus", "Got\u00b7tes", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfer seines Lenzes Duft;", "tokens": ["S\u00fc\u00b7\u00dfer", "sei\u00b7nes", "Len\u00b7zes", "Duft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhe schmeckt der Traube Blut", "tokens": ["Ru\u00b7he", "schmeckt", "der", "Trau\u00b7be", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geistiger zu hohem Muth.", "tokens": ["Geis\u00b7ti\u00b7ger", "zu", "ho\u00b7hem", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ruhe trinkt zum zweyten Mahl", "tokens": ["Ru\u00b7he", "trinkt", "zum", "zwey\u00b7ten", "Mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Freude Festpokal;", "tokens": ["Aus", "der", "Freu\u00b7de", "Fest\u00b7po\u00b7kal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhe tr\u00e4gt die Freuden heim,", "tokens": ["Ru\u00b7he", "tr\u00e4gt", "die", "Freu\u00b7den", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Biene Honigseim.", "tokens": ["Wie", "die", "Bie\u00b7ne", "Ho\u00b7ni\u00b7gseim", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ruhe hat bey schwarzem Brot", "tokens": ["Ru\u00b7he", "hat", "bey", "schwar\u00b7zem", "Brot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00f6tterkost im Abendroth;", "tokens": ["G\u00f6t\u00b7ter\u00b7kost", "im", "A\u00b7ben\u00b7droth", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhe sch\u00f6pft zum Nectartrank", "tokens": ["Ru\u00b7he", "sch\u00f6pft", "zum", "Nec\u00b7tar\u00b7trank"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wasser von der Rasenbank.", "tokens": ["Was\u00b7ser", "von", "der", "Ra\u00b7sen\u00b7bank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ruhe trotzt dem nahen Sturm", "tokens": ["Ru\u00b7he", "trotzt", "dem", "na\u00b7hen", "Sturm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Wach' im Felsenthurm;", "tokens": ["Wie", "die", "Wach'", "im", "Fel\u00b7sen\u00b7thurm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhe sieht ins offne Grab", "tokens": ["Ru\u00b7he", "sieht", "ins", "off\u00b7ne", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Herzensangst hinab.", "tokens": ["Oh\u00b7ne", "Her\u00b7zen\u00b7sangst", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ruhe nicht, die ohne Sinn,", "tokens": ["Ru\u00b7he", "nicht", ",", "die", "oh\u00b7ne", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Schaden und Gewinn,", "tokens": ["Oh\u00b7ne", "Scha\u00b7den", "und", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Schlafsucht um sich g\u00e4hnt,", "tokens": ["Wie", "die", "Schlaf\u00b7sucht", "um", "sich", "g\u00e4hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber kaum die Glieder dehnt;", "tokens": ["A\u00b7ber", "kaum", "die", "Glie\u00b7der", "dehnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ruhe nicht, die matt und stumpf", "tokens": ["Ru\u00b7he", "nicht", ",", "die", "matt", "und", "stumpf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "PRELS", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bey dem Menschenelend dumpf,", "tokens": ["Bey", "dem", "Men\u00b7sche\u00b7ne\u00b7lend", "dumpf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Herz und Regung sitzt,", "tokens": ["Oh\u00b7ne", "Herz", "und", "Re\u00b7gung", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Schwei\u00df der Dummheit schwitzt;", "tokens": ["Und", "den", "Schwei\u00df", "der", "Dumm\u00b7heit", "schwitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.