{"dta.poem.19448": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Lieder-Seelen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "In der Nacht, die die B\u00e4ume mit Bl\u00fcten deckt,", "tokens": ["In", "der", "Nacht", ",", "die", "die", "B\u00e4u\u00b7me", "mit", "Bl\u00fc\u00b7ten", "deckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ward ich von s\u00fc\u00dfen Gespenstern erschreckt,", "tokens": ["Ward", "ich", "von", "s\u00fc\u00b7\u00dfen", "Ge\u00b7spens\u00b7tern", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Reigen schwang im Garten sich,", "tokens": ["Ein", "Rei\u00b7gen", "schwang", "im", "Gar\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den ich mit leisem Fu\u00df beschlich;", "tokens": ["Den", "ich", "mit", "lei\u00b7sem", "Fu\u00df", "be\u00b7schlich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie zarter Elfen Chor im Ring", "tokens": ["Wie", "zar\u00b7ter", "El\u00b7fen", "Chor", "im", "Ring"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein wei\u00dfer, lebendiger Schimmer ging.", "tokens": ["Ein", "wei\u00b7\u00dfer", ",", "le\u00b7ben\u00b7di\u00b7ger", "Schim\u00b7mer", "ging", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Die Schemen hab' ich keck befragt:", "tokens": ["Die", "Sche\u00b7men", "hab'", "ich", "keck", "be\u00b7fragt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer seid ihr, luftige Wesen? Sagt!", "tokens": ["Wer", "seid", "ihr", ",", "luf\u00b7ti\u00b7ge", "We\u00b7sen", "?", "Sagt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$.", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201eich bin ein W\u00f6lkchen, gespiegelt im See.\u201c", "tokens": ["\u201e", "ich", "bin", "ein", "W\u00f6lk\u00b7chen", ",", "ge\u00b7spie\u00b7gelt", "im", "See", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "VVPP", "APPRART", "NN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "\u201eich bin eine Reihe von Stapfen im Schnee.\u201c", "tokens": ["\u201e", "ich", "bin", "ei\u00b7ne", "Rei\u00b7he", "von", "Stap\u00b7fen", "im", "Schnee", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "APPR", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u201eich bin ein Seufzer gen Himmel empor!\u201c", "tokens": ["\u201e", "ich", "bin", "ein", "Seuf\u00b7zer", "gen", "Him\u00b7mel", "em\u00b7por", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "\u201eich bin ein Geheimnis, gefl\u00fcstert ins Ohr.\u201c", "tokens": ["\u201e", "ich", "bin", "ein", "Ge\u00b7heim\u00b7nis", ",", "ge\u00b7fl\u00fcs\u00b7tert", "ins", "Ohr", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "VVPP", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "\u201eich bin ein frommes, gestorbenes Kind.\u201c", "tokens": ["\u201e", "ich", "bin", "ein", "from\u00b7mes", ",", "ge\u00b7stor\u00b7be\u00b7nes", "Kind", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "\u201eich bin ein \u00fcppiges Blumengewind \u2014\u201c", "tokens": ["\u201e", "ich", "bin", "ein", "\u00fcp\u00b7pi\u00b7ges", "Blu\u00b7men\u00b7ge\u00b7wind", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "\u201eund die du w\u00e4hlst, und der's beschied", "tokens": ["\u201e", "und", "die", "du", "w\u00e4hlst", ",", "und", "der's", "be\u00b7schied"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "KON", "PRELS", "PPER", "VVFIN", "$,", "KON", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Gunst der Stunde, die wird ein Lied.\u201c", "tokens": ["Die", "Gunst", "der", "Stun\u00b7de", ",", "die", "wird", "ein", "Lied", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}