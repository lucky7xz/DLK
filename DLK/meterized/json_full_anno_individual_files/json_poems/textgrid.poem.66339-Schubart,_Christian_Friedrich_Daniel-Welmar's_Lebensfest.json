{"textgrid.poem.66339": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Welmar's Lebensfest", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Edler ", "tokens": ["Ed\u00b7ler"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Deine Mutter auf die Arme nahm,", "tokens": ["Dei\u00b7ne", "Mut\u00b7ter", "auf", "die", "Ar\u00b7me", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und dein Engel, Himmelsl\u00fcfte f\u00e4chelnd,", "tokens": ["Und", "dein", "En\u00b7gel", ",", "Him\u00b7mel\u00b7sl\u00fcf\u00b7te", "f\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf die Erde dich zu sch\u00fctzen kam;", "tokens": ["Auf", "die", "Er\u00b7de", "dich", "zu", "sch\u00fct\u00b7zen", "kam", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, da hat aus seines Lichtes Klarheit", "tokens": ["Ach", ",", "da", "hat", "aus", "sei\u00b7nes", "Lich\u00b7tes", "Klar\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dich der Wesen Urgeist angeblickt,", "tokens": ["Dich", "der", "We\u00b7sen", "Ur\u00b7geist", "an\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und in deine sanfte Seele ", "tokens": ["Und", "in", "dei\u00b7ne", "sanf\u00b7te", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Darum gl\u00e4nzt von deinem Aug' herunter", "tokens": ["Da\u00b7rum", "gl\u00e4nzt", "von", "dei\u00b7nem", "Aug'", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "APZR"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "So viel Menschenw\u00fcrde. Darum gl\u00fcht", "tokens": ["So", "viel", "Men\u00b7schen\u00b7w\u00fcr\u00b7de", ".", "Da\u00b7rum", "gl\u00fcht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$.", "PAV", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dir der Denkerblick, wenn er die Wunder", "tokens": ["Dir", "der", "Den\u00b7ker\u00b7blick", ",", "wenn", "er", "die", "Wun\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seines gro\u00dfen Bilders strahlen sieht.", "tokens": ["Sei\u00b7nes", "gro\u00b7\u00dfen", "Bil\u00b7ders", "strah\u00b7len", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Darum seufzt dein Herz in seinen Tiefen", "tokens": ["Da\u00b7rum", "seufzt", "dein", "Herz", "in", "sei\u00b7nen", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ach! nach Wahrheit, Wahrheit seufzt es nur.", "tokens": ["Ach", "!", "nach", "Wahr\u00b7heit", ",", "Wahr\u00b7heit", "seufzt", "es", "nur", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "NN", "$,", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und mit Tritten, die die Pfade pr\u00fcfen,", "tokens": ["Und", "mit", "Trit\u00b7ten", ",", "die", "die", "Pfa\u00b7de", "pr\u00fc\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wanderst du auf hoher Weisheit Spur.", "tokens": ["Wan\u00b7derst", "du", "auf", "ho\u00b7her", "Weis\u00b7heit", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Darum schaurst du Wonne, wenn das Sch\u00f6ne", "tokens": ["Da\u00b7rum", "schaurst", "du", "Won\u00b7ne", ",", "wenn", "das", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deine reingestimmte Seele r\u00fchrt,", "tokens": ["Dei\u00b7ne", "rein\u00b7ge\u00b7stimm\u00b7te", "See\u00b7le", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und durch Dichtung, S\u00e4ulen, Farben, T\u00f6ne", "tokens": ["Und", "durch", "Dich\u00b7tung", ",", "S\u00e4u\u00b7len", ",", "Far\u00b7ben", ",", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dich zum Urbild aller Sch\u00f6nheit f\u00fchrt.", "tokens": ["Dich", "zum", "Ur\u00b7bild", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Darum n\u00e4\u00dft das Mitleid deine Wangen,", "tokens": ["Da\u00b7rum", "n\u00e4\u00dft", "das", "Mit\u00b7leid", "dei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn du Menschen um dich elend siehst,", "tokens": ["Wenn", "du", "Men\u00b7schen", "um", "dich", "e\u00b7lend", "siehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Arm, verachtet, elend und gefangen,", "tokens": ["Arm", ",", "ver\u00b7ach\u00b7tet", ",", "e\u00b7lend", "und", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und um sie vergebens dich bem\u00fchst.", "tokens": ["Und", "um", "sie", "ver\u00b7ge\u00b7bens", "dich", "be\u00b7m\u00fchst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wo die Wahrheit k\u00fcmmerlich gedeiht,", "tokens": ["Wo", "die", "Wahr\u00b7heit", "k\u00fcm\u00b7mer\u00b7lich", "ge\u00b7deiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Sch\u00f6nheit himmlische Geberde", "tokens": ["Wo", "der", "Sch\u00f6n\u00b7heit", "himm\u00b7li\u00b7sche", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Jeder Kr\u00fcppel Galliens bespeit;", "tokens": ["Je\u00b7der", "Kr\u00fcp\u00b7pel", "Gal\u00b7li\u00b7ens", "be\u00b7speit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NE", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Ha, zu gut f\u00fcr theurerkaufte Gnaden,", "tokens": ["Ha", ",", "zu", "gut", "f\u00fcr", "theu\u00b7rer\u00b7kauf\u00b7te", "Gna\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PTKA", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Viel zu edel f\u00fcr ein h\u00f6fisch Fest,", "tokens": ["Viel", "zu", "e\u00b7del", "f\u00fcr", "ein", "h\u00f6\u00b7fisch", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo der F\u00fcrst an einem seidnen Faden", "tokens": ["Wo", "der", "F\u00fcrst", "an", "ei\u00b7nem", "seid\u00b7nen", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seine Pantin's vor sich tanzen l\u00e4\u00dft;", "tokens": ["Sei\u00b7ne", "Pan\u00b7tin's", "vor", "sich", "tan\u00b7zen", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PRF", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Auch zu gut f\u00fcr eine Welt, wo Sp\u00f6tter", "tokens": ["Auch", "zu", "gut", "f\u00fcr", "ei\u00b7ne", "Welt", ",", "wo", "Sp\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PTKA", "ADJD", "APPR", "ART", "NN", "$,", "PWAV", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00f6hnisch lachen der Religion,", "tokens": ["H\u00f6h\u00b7nisch", "la\u00b7chen", "der", "Re\u00b7li\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und wo K\u00f6nige zum Gott der G\u00f6tter", "tokens": ["Und", "wo", "K\u00f6\u00b7ni\u00b7ge", "zum", "Gott", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Trotzend sehn von ihrem Leimenthron;", "tokens": ["Trot\u00b7zend", "sehn", "von", "ih\u00b7rem", "Lei\u00b7men\u00b7thron", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Wo die Tonkunst, deine traute Schwester,", "tokens": ["Wo", "die", "Ton\u00b7kunst", ",", "dei\u00b7ne", "trau\u00b7te", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jammert unter welscher Tyrannei,", "tokens": ["Jam\u00b7mert", "un\u00b7ter", "wel\u00b7scher", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PWAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo so manches schallende Orchester", "tokens": ["Wo", "so", "man\u00b7ches", "schal\u00b7len\u00b7de", "Or\u00b7ches\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Harmonie verkennt und Melodei.", "tokens": ["Har\u00b7mo\u00b7nie", "ver\u00b7kennt", "und", "Me\u00b7lo\u00b7dei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Guter Himmel, hast du keine H\u00fctte", "tokens": ["Gu\u00b7ter", "Him\u00b7mel", ",", "hast", "du", "kei\u00b7ne", "H\u00fct\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VAFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcr den theuren, auserw\u00e4hlten Freund,", "tokens": ["F\u00fcr", "den", "theu\u00b7ren", ",", "au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dessen Auge beim Verderb der Sitte", "tokens": ["Des\u00b7sen", "Au\u00b7ge", "beim", "Ver\u00b7derb", "der", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und beim weibischen Geschmacke weint?", "tokens": ["Und", "beim", "wei\u00b7bi\u00b7schen", "Ge\u00b7schma\u00b7cke", "weint", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Keine H\u00fctte, drinn er Wahrheit finden", "tokens": ["Kei\u00b7ne", "H\u00fct\u00b7te", ",", "drinn", "er", "Wahr\u00b7heit", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "ADV", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und getreu der Wahrheit leben kann,", "tokens": ["Und", "ge\u00b7treu", "der", "Wahr\u00b7heit", "le\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und, beweht vom Silberduft der Linden,", "tokens": ["Und", ",", "be\u00b7weht", "vom", "Sil\u00b7ber\u00b7duft", "der", "Lin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "APPRART", "NN", "ART", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wandeln auf der Sch\u00f6nheit Rosenbahn?", "tokens": ["Wan\u00b7deln", "auf", "der", "Sch\u00f6n\u00b7heit", "Ro\u00b7sen\u00b7bahn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Ach, die H\u00fctt' ist, ", "tokens": ["Ach", ",", "die", "H\u00fctt'", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Droben, ", "tokens": ["Dro\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Dorten erst, dort wandeln wir im Frieden", "tokens": ["Dor\u00b7ten", "erst", ",", "dort", "wan\u00b7deln", "wir", "im", "Frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auf der Wahrheit und der Sch\u00f6nheit Spur.", "tokens": ["Auf", "der", "Wahr\u00b7heit", "und", "der", "Sch\u00f6n\u00b7heit", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Edler ", "tokens": ["Ed\u00b7ler"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Deine Mutter auf die Arme nahm,", "tokens": ["Dei\u00b7ne", "Mut\u00b7ter", "auf", "die", "Ar\u00b7me", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und dein Engel, Himmelsl\u00fcfte f\u00e4chelnd,", "tokens": ["Und", "dein", "En\u00b7gel", ",", "Him\u00b7mel\u00b7sl\u00fcf\u00b7te", "f\u00e4\u00b7chelnd", ","], "token_info": ["wor