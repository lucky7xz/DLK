{"textgrid.poem.41426": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Hase und der Dachs", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Hase wird vor Furcht und wachem Kummer grau,", "tokens": ["Ein", "Ha\u00b7se", "wird", "vor", "Furcht", "und", "wa\u00b7chem", "Kum\u00b7mer", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, Eremiten gleich, durch strenges Fasten hager.", "tokens": ["Und", ",", "E\u00b7re\u00b7mi\u00b7ten", "gleich", ",", "durch", "stren\u00b7ges", "Fas\u00b7ten", "ha\u00b7ger", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nichts, als die h\u00f6chste Noth, treibt ihn aus seinem Lager.", "tokens": ["Nichts", ",", "als", "die", "h\u00f6chs\u00b7te", "Noth", ",", "treibt", "ihn", "aus", "sei\u00b7nem", "La\u00b7ger", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein fetter Freund, der Dachs, geht \u00f6fters aus dem Bau,", "tokens": ["Sein", "fet\u00b7ter", "Freund", ",", "der", "Dachs", ",", "geht", "\u00f6f\u00b7ters", "aus", "dem", "Bau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und suchet Luft und Fra\u00df bei jedem Fr\u00fchlingsthau.", "tokens": ["Und", "su\u00b7chet", "Luft", "und", "Fra\u00df", "bei", "je\u00b7dem", "Fr\u00fch\u00b7lingst\u00b7hau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kaum l\u00e4\u00dft sich ein Ger\u00e4usch versp\u00fcren,", "tokens": ["Kaum", "l\u00e4\u00dft", "sich", "ein", "Ge\u00b7r\u00e4usch", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Kaum kann der hohe Storch zum Froschfang ausspazieren,", "tokens": ["Kaum", "kann", "der", "ho\u00b7he", "Storch", "zum", "Froschfang", "aus\u00b7spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Kaum k\u00f6nnen Hasen selbst im Busche haseliren;", "tokens": ["Kaum", "k\u00f6n\u00b7nen", "Ha\u00b7sen", "selbst", "im", "Bu\u00b7sche", "ha\u00b7se\u00b7li\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wird auch jener gleich die L\u00f6ffel \u00e4ngstlich r\u00fchren.", "tokens": ["So", "wird", "auch", "je\u00b7ner", "gleich", "die", "L\u00f6f\u00b