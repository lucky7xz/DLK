{"textgrid.poem.60152": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du, mir aus H\u00e4nden der Freundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, mir aus H\u00e4nden der Freundschaft", "tokens": ["Du", ",", "mir", "aus", "H\u00e4n\u00b7den", "der", "Freund\u00b7schaft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In diese schreibende Hand", "tokens": ["In", "die\u00b7se", "schrei\u00b7ben\u00b7de", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zu langer Dauer gegeben,", "tokens": ["Zu", "lan\u00b7ger", "Dau\u00b7er", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schreib kein unheiliges Lied!", "tokens": ["Schreib", "kein", "un\u00b7hei\u00b7li\u00b7ges", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Dich schuf aus gl\u00e4nzendem Erze", "tokens": ["Dich", "schuf", "aus", "gl\u00e4n\u00b7zen\u00b7dem", "Er\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Schmuck arbeitende Schmid!", "tokens": ["Der", "Schmuck", "ar\u00b7bei\u00b7ten\u00b7de", "Schmid", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zeus gab nicht unter dem Himmel", "tokens": ["Zeus", "gab", "nicht", "un\u00b7ter", "dem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus einem Vogel dich mir!", "tokens": ["Aus", "ei\u00b7nem", "Vo\u00b7gel", "dich", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Der Strau\u00df, die balzenden Hahnen", "tokens": ["Der", "Strau\u00df", ",", "die", "bal\u00b7zen\u00b7den", "Hah\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Am hohen Brocken im Lenz,", "tokens": ["Am", "ho\u00b7hen", "Bro\u00b7cken", "im", "Lenz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Pfau mit pr\u00e4chtigem Rade,", "tokens": ["Der", "Pfau", "mit", "pr\u00e4ch\u00b7ti\u00b7gem", "Ra\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die alle trugen dich nicht.", "tokens": ["Die", "al\u00b7le", "tru\u00b7gen", "dich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "In reichgeseegneter Ader", "tokens": ["In", "reich\u00b7ge\u00b7seeg\u00b7ne\u00b7ter", "A\u