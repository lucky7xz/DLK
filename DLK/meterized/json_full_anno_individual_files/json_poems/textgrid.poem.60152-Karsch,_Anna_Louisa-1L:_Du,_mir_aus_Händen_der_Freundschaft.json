n", ",", "ein", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Dem Tod im Feuer zu trotzen,", "tokens": ["Dem", "Tod", "im", "Feu\u00b7er", "zu", "trot\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "An einem h\u00f6lzernen Spie\u00df", "tokens": ["An", "ei\u00b7nem", "h\u00f6l\u00b7zer\u00b7nen", "Spie\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Noch singen: da\u00df sie gebraten", "tokens": ["Noch", "sin\u00b7gen", ":", "da\u00df", "sie", "ge\u00b7bra\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "KOUS", "PPER", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Feindes Br\u00fcder auch einst!", "tokens": ["Des", "Fein\u00b7des", "Br\u00fc\u00b7der", "auch", "einst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "O, du mir k\u00f6stliche Feder!", "tokens": ["O", ",", "du", "mir", "k\u00f6st\u00b7li\u00b7che", "Fe\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Dich las ein M\u00e4dchen vielleicht", "tokens": ["Dich", "las", "ein", "M\u00e4d\u00b7chen", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Aus einem Bache voll Goldsand,", "tokens": ["Aus", "ei\u00b7nem", "Ba\u00b7che", "voll", "Gold\u00b7sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und sagte seufzend dabey:", "tokens": ["Und", "sag\u00b7te", "seuf\u00b7zend", "da\u00b7bey", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "\u00bbwo bleibt der liebende J\u00fcngling?", "tokens": ["\u00bb", "wo", "bleibt", "der", "lie\u00b7ben\u00b7de", "J\u00fcng\u00b7ling", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "O, mir ver\u00e4chtlicher Staub!", "tokens": ["O", ",", "mir", "ver\u00b7\u00e4cht\u00b7li\u00b7cher", "Staub", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Sein Herz im l\u00e4chelnden Aug", "tokens": ["Sein", "Herz", "im", "l\u00e4\u00b7cheln\u00b7den", "Aug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Gl\u00e4nzt mehr, ist theurer als du!\u00ab", "tokens": ["Gl\u00e4nzt", "mehr", ",", "ist", "theu\u00b7rer", "als", "du", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "$,", "VAFIN", "ADJD", "KOKOM", "PPER", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "So sprach das M\u00e4dchen vielleicht", "tokens": ["So", "sprach", "das", "M\u00e4d\u00b7chen", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Zu dir noch rohem Metall!", "tokens": ["Zu", "dir", "noch", "ro\u00b7hem", "Me\u00b7tall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Izt aber bist du gebildet", "tokens": ["Izt", "a\u00b7ber", "bist", "du", "ge\u00b7bil\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00fcr mich zu hohem Gebrauch!", "tokens": ["F\u00fcr", "mich", "zu", "ho\u00b7hem", "Ge\u00b7brauch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "O nur den G\u00f6ttern und Helden", "tokens": ["O", "nur", "den", "G\u00f6t\u00b7tern", "und", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Zu schreiben diene du mir,", "tokens": ["Zu", "schrei\u00b7ben", "die\u00b7ne", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und g\u00f6ttlich denkenden Freunden", "tokens": ["Und", "g\u00f6tt\u00b7lich", "den\u00b7ken\u00b7den", "Freun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"