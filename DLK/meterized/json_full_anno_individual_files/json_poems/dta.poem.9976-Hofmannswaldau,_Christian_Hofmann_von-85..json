{"dta.poem.9976": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn mein vater mich will machen/ hencket er die mutter an/", "tokens": ["Wenn", "mein", "va\u00b7ter", "mich", "will", "ma\u00b7chen", "/", "hen\u00b7cket", "er", "die", "mut\u00b7ter", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VMFIN", "VVINF", "$(", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Jeder sie an seinem leibe so gut/ als er/ sehen kan.", "tokens": ["Je\u00b7der", "sie", "an", "sei\u00b7nem", "lei\u00b7be", "so", "gut", "/", "als", "er", "/", "se\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "PPER", "APPR", "PPOSAT", "VVFIN", "ADV", "ADJD", "$(", "KOUS", "PPER", "$(", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Es geht zwar sehr langsam zu/ eh die mutter mich gebiehret/", "tokens": ["Es", "geht", "zwar", "sehr", "lang\u00b7sam", "zu", "/", "eh", "die", "mut\u00b7ter", "mich", "ge\u00b7bieh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "PTKZU", "$(", "KOUS", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Denn sie endlich sich in mir selbst/ durch die geburt/ verlieret.", "tokens": ["Denn", "sie", "end\u00b7lich", "sich", "in", "mir", "selbst", "/", "durch", "die", "ge\u00b7burt", "/", "ver\u00b7lie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PRF", "APPR", "PPER", "ADV", "$(", "APPR", "ART", "NN", "$(", "VVFIN", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}}}}}