nd", "je\u00b7der", "Wunsch", "ein", "gl\u00fc\u00b7hend", "Min\u00b7ne\u00b7lied", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Inmitten diesem seligen Gebiet", "tokens": ["In\u00b7mit\u00b7ten", "die\u00b7sem", "se\u00b7li\u00b7gen", "Ge\u00b7biet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ist mir der Liebe Sonne aufgegangen.", "tokens": ["Ist", "mir", "der", "Lie\u00b7be", "Son\u00b7ne", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "So bringt das Herz sich ihr voll Weihe dar.", "tokens": ["So", "bringt", "das", "Herz", "sich", "ihr", "voll", "Wei\u00b7he", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "PPOSAT", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Nach keinem Himmel mag es mehr verlangen", "tokens": ["Nach", "kei\u00b7nem", "Him\u00b7mel", "mag", "es", "mehr", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Als den, der jetzt ihm pl\u00f6tzlich offenbar,", "tokens": ["Als", "den", ",", "der", "jetzt", "ihm", "pl\u00f6tz\u00b7lich", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "PRELS", "ADV", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Denn sch\u00f6n und rein wie heller Sonnenglanz", "tokens": ["Denn", "sch\u00f6n", "und", "rein", "wie", "hel\u00b7ler", "Son\u00b7nen\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Erf\u00fcllt der Liebe Seligkeit es ganz.", "tokens": ["Er\u00b7f\u00fcllt", "der", "Lie\u00b7be", "Se\u00b7lig\u00b7keit", "es", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}