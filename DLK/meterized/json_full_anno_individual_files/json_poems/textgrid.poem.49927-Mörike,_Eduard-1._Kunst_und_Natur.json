{"textgrid.poem.49927": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1. Kunst und Natur", "genre": "verse", "period": "N.A.", "pub_year": 1863, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute dein einsames Tal durchstreifend o trautestes Kloster,", "tokens": ["Heu\u00b7te", "dein", "ein\u00b7sa\u00b7mes", "Tal", "durch\u00b7strei\u00b7fend", "o", "trau\u00b7tes\u00b7tes", "Klos\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVPP", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Fand ich im Walde zun\u00e4chst jenen ver\u00f6deten Grund,", "tokens": ["Fand", "ich", "im", "Wal\u00b7de", "zu\u00b7n\u00e4chst", "je\u00b7nen", "ver\u00b7\u00f6\u00b7de\u00b7ten", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dem du die m\u00e4chtigen Quader verdankst und was dir zum Schmucke", "tokens": ["Dem", "du", "die", "m\u00e4ch\u00b7ti\u00b7gen", "Qua\u00b7der", "ver\u00b7dankst", "und", "was", "dir", "zum", "Schmu\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "KON", "PWS", "PPER", "APPRART", "NN"], "meter": "-+-+--+--+-++-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Deines gegliederten Turms alles der Meister verliehn.", "tokens": ["Dei\u00b7nes", "ge\u00b7glie\u00b7der\u00b7ten", "Turms", "al\u00b7les", "der", "Meis\u00b7ter", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ganz ein Gebild des f\u00fchlenden Geistes verleugnest du dennoch", "tokens": ["Ganz", "ein", "Ge\u00b7bild", "des", "f\u00fch\u00b7len\u00b7den", "Geis\u00b7tes", "ver\u00b7leug\u00b7nest", "du", "den\u00b7noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Nimmer den Mutterscho\u00df dr\u00fcben am felsigen Hang.", "tokens": ["Nim\u00b7mer", "den", "Mut\u00b7ter\u00b7scho\u00df", "dr\u00fc\u00b7ben", "am", "fel\u00b7si\u00b7gen", "Hang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Spielend ahmst du den schlanken Kristall und die rankende Pflanze", "tokens": ["Spie\u00b7lend", "ahmst", "du", "den", "schlan\u00b7ken", "Kris\u00b7tall", "und", "die", "ran\u00b7ken\u00b7de", "Pflan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Nach und so manches Getier, das in den Kl\u00fcften sich birgt.", "tokens": ["Nach", "und", "so", "man\u00b7ches", "Ge\u00b7tier", ",", "das", "in", "den", "Kl\u00fcf\u00b7ten", "sich", "birgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "ADV", "PIAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"l