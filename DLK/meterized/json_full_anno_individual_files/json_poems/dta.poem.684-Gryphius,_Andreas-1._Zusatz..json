{"dta.poem.684": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1. Zusatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.71", "da:0.28"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Dv aller Herren HERR/ den Erdt vnd Himmel ehren- ", "tokens": ["Dv", "al\u00b7ler", "Her\u00b7ren", "HeRR", "/", "den", "Erdt", "vnd", "Him\u00b7mel", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "NN", "NN", "$(", "ART", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr dem die H\u00f6ll erschrickt.", "tokens": ["F\u00fcr", "dem", "die", "H\u00f6ll", "er\u00b7schrickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du du hast mich/ du hast mich wollen h\u00f6ren", "tokens": ["Du", "du", "hast", "mich", "/", "du", "hast", "mich", "wol\u00b7len", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "VAFIN", "PPER", "$(", "PPER", "VAFIN", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da als ich gantz verstrickt/ ", "tokens": ["Da", "als", "ich", "gantz", "ver\u00b7strickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jm ", "tokens": ["Jm"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Vmbsahe/ da mein Heyland hast du mir", "tokens": ["Vmb\u00b7sa\u00b7he", "/", "da", "mein", "Hey\u00b7land", "hast", "du", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "KOUS", "PPOSAT", "NN", "VAFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Geboten h\u00fclff vnd handt/ du hast das netz zutrennet.", "tokens": ["Ge\u00b7bo\u00b7ten", "h\u00fclff", "vnd", "handt", "/", "du", "hast", "das", "netz", "zu\u00b7tren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "$(", "PPER", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich bin dur", "tokens": ["Ich", "bin", "dur"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}