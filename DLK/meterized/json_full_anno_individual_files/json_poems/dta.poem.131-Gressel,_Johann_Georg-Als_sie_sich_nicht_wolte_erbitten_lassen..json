{"dta.poem.131": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich nicht wolte erbitten lassen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mich will die strenge Fluth ins Ungl\u00fccks-Rachen jagen/", "tokens": ["Mich", "will", "die", "stren\u00b7ge", "Fluth", "ins", "Un\u00b7gl\u00fccks\u00b7Ra\u00b7chen", "ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es wird mein Hoffnungs-Schiff im Jammer-Merzer-", "tokens": ["Es", "wird", "mein", "Hoff\u00b7nungs\u00b7Schiff", "im", "Jam\u00b7mer\u00b7Mer\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schlagen/", "tokens": ["schla\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Jedwede Wolcke ist von Blitz und Donner voll", "tokens": ["Jed\u00b7we\u00b7de", "Wol\u00b7cke", "ist", "von", "Blitz", "und", "Don\u00b7ner", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wei\u00df nicht wo ich mich vor Unmuth lassen soll.", "tokens": ["Ich", "wei\u00df", "nicht", "wo", "ich", "mich", "vor", "Un\u00b7muth", "las\u00b7sen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "PRF", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du f\u00fchrst/ ", "tokens": ["Du", "f\u00fchrst", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die deine Freundlichkeit in strengen Zorn verstellen/", "tokens": ["Die", "dei\u00b7ne", "Freund\u00b7lich\u00b7keit", "in", "stren\u00b7gen", "Zorn", "ver\u00b7stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Derselben starcke Fluth so rasend auf mich dringt/", "tokens": ["Der\u00b7sel\u00b7ben", "star\u00b7cke", "Fluth", "so", "ra\u00b7send", "auf", "mich", "dringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df mein erschrockner Geist die Sterbe-Lieder singt.", "tokens": ["Da\u00df", "mein", "er\u00b7schrock\u00b7ner", "Geist", "die", "Ster\u00b7be\u00b7Lie\u00b7der", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mein Weinen kan dein Hertz/ o Schmertze! nicht durch h\u00f6hlen!", "tokens": ["Mein", "Wei\u00b7nen", "kan", "dein", "Hertz", "/", "o", "Schmert\u00b7ze", "!", "nicht", "durch", "h\u00f6h\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "$(", "FM", "NN", "$.", "PTKNEG", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein harter Kiesel-Stein wird deine Brust beseelen/", "tokens": ["Ein", "har\u00b7ter", "Kie\u00b7sel\u00b7Stein", "wird", "dei\u00b7ne", "Brust", "be\u00b7see\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dein Felsen harter Sinn erh\u00f6rt mein Seuffzen nicht", "tokens": ["Dein", "Fel\u00b7sen", "har\u00b7ter", "Sinn", "er\u00b7h\u00f6rt", "mein", "Seuff\u00b7zen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein kranckes Hertze wird durch deinen Ha\u00df gericht.", "tokens": ["Mein", "kran\u00b7ckes", "Hert\u00b7ze", "wird", "durch", "dei\u00b7nen", "Ha\u00df", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich mu\u00df wie ", "tokens": ["Ich", "mu\u00df", "wie"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Mein Ungl\u00fcck qu\u00e4let mich mit lauter Finsternissen/", "tokens": ["Mein", "Un\u00b7gl\u00fcck", "qu\u00e4\u00b7let", "mich", "mit", "lau\u00b7ter", "Fins\u00b7ter\u00b7nis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die krancke Seele ist bis in den Tod betr\u00fcbt/", "tokens": ["Die", "kran\u00b7cke", "See\u00b7le", "ist", "bis", "in", "den", "Tod", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von Unmuth fast entseelt/ und ohne Trost verliebt.", "tokens": ["Von", "Un\u00b7muth", "fast", "ent\u00b7seelt", "/", "und", "oh\u00b7ne", "Trost", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "$(", "KON", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So wenig als das Meer dem festen Felsen schadet/", "tokens": ["So", "we\u00b7nig", "als", "das", "Meer", "dem", "fes\u00b7ten", "Fel\u00b7sen", "scha\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und wenn mein tr\u00fcbes Aug\u2019 in tausend Thr\u00e4nen badet", "tokens": ["Und", "wenn", "mein", "tr\u00fc\u00b7bes", "Aug'", "in", "tau\u00b7send", "Thr\u00e4\u00b7nen", "ba\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "APPR", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das ist in allen gleich; du bist und bleibst ein Stein/", "tokens": ["Das", "ist", "in", "al\u00b7len", "gleich", ";", "du", "bist", "und", "bleibst", "ein", "Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "ADV", "$.", "PPER", "VAFIN", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dein hartes Hertze will mir immer grausahm seyn.", "tokens": ["Dein", "har\u00b7tes", "Hert\u00b7ze", "will", "mir", "im\u00b7mer", "grau\u00b7sahm", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Haven meiner Lust ist mir/ ach Schmertz! verschlossen/", "tokens": ["Der", "Ha\u00b7ven", "mei\u00b7ner", "Lust", "ist", "mir", "/", "ach", "Schmertz", "!", "ver\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "PPER", "$(", "XY", "NN", "$.", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dein Unwill hat mir da die Ketten vorgeschlossen/", "tokens": ["Dein", "Un\u00b7will", "hat", "mir", "da", "die", "Ket\u00b7ten", "vor\u00b7ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mein armes Schifflein mu\u00df in wilder See vergehn/", "tokens": ["Mein", "ar\u00b7mes", "Schif\u00b7flein", "mu\u00df", "in", "wil\u00b7der", "See", "ver\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der tr\u00fcbe Himmel l\u00e4st nichts als ", "tokens": ["Der", "tr\u00fc\u00b7be", "Him\u00b7mel", "l\u00e4st", "nichts", "als"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "KOKOM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ich k\u00f6nte zwar noch wolden frohen Haven finden/", "tokens": ["Ich", "k\u00f6n\u00b7te", "zwar", "noch", "wol\u00b7den", "fro\u00b7hen", "Ha\u00b7ven", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJA", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Allein dein Zorn mu\u00df erst in heitrer Lufft verschwinden/", "tokens": ["Al\u00b7lein", "dein", "Zorn", "mu\u00df", "erst", "in", "hei\u00b7trer", "Lufft", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die schwartze Trauer-Nacht mu\u00df mir ein Mittag seyn/", "tokens": ["Die", "schwart\u00b7ze", "Trau\u00b7e\u00b7rNacht", "mu\u00df", "mir", "ein", "Mit\u00b7tag", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die d\u00fcstre Abend-Lufft ein heller Morgen-Schein.", "tokens": ["Die", "d\u00fcst\u00b7re", "A\u00b7ben\u00b7dLufft", "ein", "hel\u00b7ler", "Mor\u00b7gen\u00b7Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn mich dis Gl\u00fccke k\u00fc\u00dft so kan ich gl\u00fccklich leben/", "tokens": ["Wenn", "mich", "dis", "Gl\u00fc\u00b7cke", "k\u00fc\u00dft", "so", "kan", "ich", "gl\u00fcck\u00b7lich", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "VVFIN", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bis an den Himmel wird die s\u00fcsse Lust mich heben/", "tokens": ["Bis", "an", "den", "Him\u00b7mel", "wird", "die", "s\u00fcs\u00b7se", "Lust", "mich", "he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Denn e\u00df ich Zucker Brod/ und trincke ", "tokens": ["Denn", "e\u00df", "ich", "Zu\u00b7cker", "Brod", "/", "und", "trin\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "NN", "$(", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Denn ist was mich gequ\u00e4hlt gantz fort und weggeschafft.", "tokens": ["Denn", "ist", "was", "mich", "ge\u00b7qu\u00e4hlt", "gantz", "fort", "und", "weg\u00b7ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PWS", "PPER", "VVPP", "ADV", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wird sich dein Z\u00fcrnen nun in holde Gunst verkehren?", "tokens": ["Wird", "sich", "dein", "Z\u00fcr\u00b7nen", "nun", "in", "hol\u00b7de", "Gunst", "ver\u00b7keh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So werde ich dich stets als meine G\u00f6ttin ehren/", "tokens": ["So", "wer\u00b7de", "ich", "dich", "stets", "als", "mei\u00b7ne", "G\u00f6t\u00b7tin", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "KOUS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So bald dein Auge l\u00e4st den Hoffnungs-Stern auffgehn/", "tokens": ["So", "bald", "dein", "Au\u00b7ge", "l\u00e4st", "den", "Hoff\u00b7nungs\u00b7S\u00b7tern", "auff\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "So kan ich meine Lust aus den ", "tokens": ["So", "kan", "ich", "mei\u00b7ne", "Lust", "aus", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "ART"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.38": {"text": "Ich will mich nun/ mein Schatz/ auf deine Huld verlassen/", "tokens": ["Ich", "will", "mich", "nun", "/", "mein", "Schatz", "/", "auf", "dei\u00b7ne", "Huld", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$(", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dein sch\u00f6nes Wesen kan mein treues Hertz nicht hassen/", "tokens": ["Dein", "sch\u00f6\u00b7nes", "We\u00b7sen", "kan", "mein", "treu\u00b7es", "Hertz", "nicht", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn aber deine Gunst ein steter Zorn verstellt/", "tokens": ["Wenn", "a\u00b7ber", "dei\u00b7ne", "Gunst", "ein", "ste\u00b7ter", "Zorn", "ver\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So h\u00f6r ich diesen Spruch: ", "tokens": ["So", "h\u00f6r", "ich", "die\u00b7sen", "Spruch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}