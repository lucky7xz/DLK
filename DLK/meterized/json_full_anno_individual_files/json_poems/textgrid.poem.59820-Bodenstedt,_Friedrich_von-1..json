{"textgrid.poem.59820": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem Feuerquell des Weines,", "tokens": ["Aus", "dem", "Feu\u00b7er\u00b7quell", "des", "Wei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Zaubergrund des Bechers", "tokens": ["Aus", "dem", "Zau\u00b7ber\u00b7grund", "des", "Be\u00b7chers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprudelt Gift und \u2013 s\u00fc\u00dfe Labung,", "tokens": ["Spru\u00b7delt", "Gift", "und", "\u2013", "s\u00fc\u00b7\u00dfe", "La\u00b7bung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprudelt Sch\u00f6nes und \u2013 Gemeines:", "tokens": ["Spru\u00b7delt", "Sch\u00f6\u00b7nes", "und", "\u2013", "Ge\u00b7mei\u00b7nes", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "$(", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach dem eignen Wert des Zechers,", "tokens": ["Nach", "dem", "eig\u00b7nen", "Wert", "des", "Ze\u00b7chers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nach des Trinkenden Begabung!", "tokens": ["Nach", "des", "Trin\u00b7ken\u00b7den", "Be\u00b7ga\u00b7bung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In Gemeinheit tief versunken", "tokens": ["In", "Ge\u00b7mein\u00b7heit", "tief", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt der Tor, vom Rausch bemeistert;", "tokens": ["Liegt", "der", "Tor", ",", "vom", "Rausch", "be\u00b7meis\u00b7tert", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er trinkt \u2013 wird er betrunken,", "tokens": ["Wenn", "er", "trinkt", "\u2013", "wird", "er", "be\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$(", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trinken wir \u2013 sind wir begeistert!", "tokens": ["Trin\u00b7ken", "wir", "\u2013", "sind", "wir", "be\u00b7geis\u00b7tert", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Spr\u00fchen hohe Witzesfunken,", "tokens": ["Spr\u00fc\u00b7hen", "ho\u00b7he", "Wit\u00b7zes\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reden wie mit Engelzungen,", "tokens": ["Re\u00b7den", "wie", "mit", "En\u00b7gel\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und von Glut sind wir durchdrungen,", "tokens": ["Und", "von", "Glut", "sind", "wir", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Und von Sch\u00f6nheit sind wir trunken!", "tokens": ["Und", "von", "Sch\u00f6n\u00b7heit", "sind", "wir", "trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn es gleicht der Wein dem Regen,", "tokens": ["Denn", "es", "gleicht", "der", "Wein", "dem", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der im Schmutze selbst zu Schmutz wird,", "tokens": ["Der", "im", "Schmut\u00b7ze", "selbst", "zu", "Schmutz", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch auf gutem Acker Segen", "tokens": ["Doch", "auf", "gu\u00b7tem", "A\u00b7cker", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringt und jedermann zunutz wird!", "tokens": ["Bringt", "und", "je\u00b7der\u00b7mann", "zu\u00b7nutz", "wird", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PIS", "ADV", "VAFIN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"te