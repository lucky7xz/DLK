{"textgrid.poem.35309": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sp\u00e4therbst. \u2013 Wir schritten langsam durch den Wald", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sp\u00e4therbst. \u2013 Wir schritten langsam durch den Wald", "tokens": ["Sp\u00e4t\u00b7herbst", ".", "\u2013", "Wir", "schrit\u00b7ten", "lang\u00b7sam", "durch", "den", "Wald"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "$(", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zur D\u00e4mmrungszeit, erm\u00fcdet und verdrossen,", "tokens": ["Zur", "D\u00e4mm\u00b7rungs\u00b7zeit", ",", "er\u00b7m\u00fc\u00b7det", "und", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da sprach ich zu dem wackern Jagdgenossen:", "tokens": ["Da", "sprach", "ich", "zu", "dem", "wa\u00b7ckern", "Jagd\u00b7ge\u00b7nos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbfreund, la\u00df uns hier ein Weilchen ruhn!\u00ab \u2013 Und bald", "tokens": ["\u00bb", "freund", ",", "la\u00df", "uns", "hier", "ein", "Weil\u00b7chen", "ruhn", "!", "\u00ab", "\u2013", "Und", "bald"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["$(", "ADJD", "$,", "VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$.", "$(", "$(", "KON", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Erstarben uns die Worte auf den Lippen.", "tokens": ["Er\u00b7star\u00b7ben", "uns", "die", "Wor\u00b7te", "auf", "den", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Busche h\u00f6rten wir den Nachtwind s\u00e4useln,", "tokens": ["Im", "Bu\u00b7sche", "h\u00f6r\u00b7ten", "wir", "den", "Nacht\u00b7wind", "s\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das tote Laub zu unsern F\u00fc\u00dfen kr\u00e4useln,", "tokens": ["Das", "to\u00b7te", "Laub", "zu", "un\u00b7sern", "F\u00fc\u00b7\u00dfen", "kr\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und alte Birken sahn wir, gleich Gerippen,", "tokens": ["Und", "al\u00b7te", "Bir\u00b7ken", "sahn", "wir", ",", "gleich", "Ge\u00b7rip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Im schwarzen Moorgrund; Schatten, riesenhaft,", "tokens": ["Im", "schwar\u00b7zen", "Mo\u00b7or\u00b7grund", ";", "Schat\u00b7ten", ",", "rie\u00b7sen\u00b7haft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "NN", "$,", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Umflogen uns und huschten rasch vor\u00fcber;", "tokens": ["Um\u00b7flo\u00b7gen", "uns", "und", "huschten", "rasch", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Des Tages Nachglanz wurde bleicher, tr\u00fcber.", "tokens": ["Des", "Ta\u00b7ges", "Nach\u00b7glanz", "wur\u00b7de", "blei\u00b7cher", ",", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Unheimlich war es in der Nachbarschaft;", "tokens": ["Un\u00b7heim\u00b7lich", "war", "es", "in", "der", "Nach\u00b7bar\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ein sonderbares Regen in den Zweigen,", "tokens": ["Ein", "son\u00b7der\u00b7ba\u00b7res", "Re\u00b7gen", "in", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sonst alles tiefes Schweigen. \u2013", "tokens": ["Sonst", "al\u00b7les", "tie\u00b7fes", "Schwei\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Ich schlief nicht, tr\u00e4umte nicht; ein Schleier lag", "tokens": ["Ich", "schlief", "nicht", ",", "tr\u00e4um\u00b7te", "nicht", ";", "ein", "Schlei\u00b7er", "lag"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "PTKNEG", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Auf mir, doch blieb ich meiner Sinne m\u00e4chtig \u2013", "tokens": ["Auf", "mir", ",", "doch", "blieb", "ich", "mei\u00b7ner", "Sin\u00b7ne", "m\u00e4ch\u00b7tig", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und da, in meiner N\u00e4he, \u00fcbern\u00e4chtig,", "tokens": ["Und", "da", ",", "in", "mei\u00b7ner", "N\u00e4\u00b7he", ",", "\u00fc\u00b7bern\u00b7\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Erschien mir pl\u00f6tzlich, blendend wie der Tag,", "tokens": ["Er\u00b7schien", "mir", "pl\u00f6tz\u00b7lich", ",", "blen\u00b7dend", "wie", "der", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ein Bild, das schmerzliche Erinnrung weckte.", "tokens": ["Ein", "Bild", ",", "das", "schmerz\u00b7li\u00b7che", "E\u00b7rinn\u00b7rung", "weck\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.20": {"text": "Du warst es, stolze Lady Margaret,", "tokens": ["Du", "warst", "es", ",", "stol\u00b7ze", "La\u00b7dy", "Mar\u00b7ga\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Du, deren Liebe ich umsonst erfleht,", "tokens": ["Du", ",", "de\u00b7ren", "Lie\u00b7be", "ich", "um\u00b7sonst", "er\u00b7fleht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Du, deren Sarg mit Kr\u00e4nzen ich bedeckte \u2013", "tokens": ["Du", ",", "de\u00b7ren", "Sarg", "mit", "Kr\u00e4n\u00b7zen", "ich", "be\u00b7deck\u00b7te", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "APPR", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "O langbeweinte, herrliche Gestalt!", "tokens": ["O", "lang\u00b7be\u00b7wein\u00b7te", ",", "herr\u00b7li\u00b7che", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Du sa\u00dfest wieder auf dem wei\u00dfen Pferde", "tokens": ["Du", "sa\u00b7\u00dfest", "wie\u00b7der", "auf", "dem", "wei\u00b7\u00dfen", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Wie einstmals. \u2013 Lie\u00df der Liebe Allgewalt", "tokens": ["Wie", "einst\u00b7mals", ".", "\u2013", "Lie\u00df", "der", "Lie\u00b7be", "All\u00b7ge\u00b7walt"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$.", "$(", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Dir keine Rast in halberstarrter Erde?", "tokens": ["Dir", "kei\u00b7ne", "Rast", "in", "hal\u00b7ber\u00b7starr\u00b7ter", "Er\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Ich sah dich auf den Hals des Zelters klopfen;", "tokens": ["Ich", "sah", "dich", "auf", "den", "Hals", "des", "Zel\u00b7ters", "klop\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Aus deinen Augen fielen schwere Tropfen", "tokens": ["Aus", "dei\u00b7nen", "Au\u00b7gen", "fie\u00b7len", "schwe\u00b7re", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Auf deine holde, oft gek\u00fc\u00dfte Hand.", "tokens": ["Auf", "dei\u00b7ne", "hol\u00b7de", ",", "oft", "ge\u00b7k\u00fc\u00df\u00b7te", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Vorbei, vorbei! \u2013 Ein Winken mit dem Tuche,", "tokens": ["Vor\u00b7bei", ",", "vor\u00b7bei", "!", "\u2013", "Ein", "Win\u00b7ken", "mit", "dem", "Tu\u00b7che", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$.", "$(", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "O teures Antlitz, das ich ewig suche,", "tokens": ["O", "teu\u00b7res", "Ant\u00b7litz", ",", "das", "ich", "e\u00b7wig", "su\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Ein letzter Blick \u2013 und die Erscheinung schwand.", "tokens": ["Ein", "letz\u00b7ter", "Blick", "\u2013", "und", "die", "Er\u00b7schei\u00b7nung", "schwand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Und sprachlos starrend in des Waldes D\u00fcster", "tokens": ["Und", "sprach\u00b7los", "star\u00b7rend", "in", "des", "Wal\u00b7des", "D\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Vernahm ich jetzt ein Rauschen, ein Gefl\u00fcster \u2013", "tokens": ["Ver\u00b7nahm", "ich", "jetzt", "ein", "Rau\u00b7schen", ",", "ein", "Ge\u00b7fl\u00fcs\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Mir drang es in die Brust wie Grabeshauch.", "tokens": ["Mir", "drang", "es", "in", "die", "Brust", "wie", "Gra\u00b7bes\u00b7hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Lebendig aber wurden Baum und Strauch", "tokens": ["Le\u00b7ben\u00b7dig", "a\u00b7ber", "wur\u00b7den", "Baum", "und", "Strauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VAFIN", "NN", "KON", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.37": {"text": "Und warfen mir, der Geisterwelt Erwachen", "tokens": ["Und", "war\u00b7fen", "mir", ",", "der", "Geis\u00b7ter\u00b7welt", "Er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Begr\u00fc\u00dfend, leise diese Worte zu:", "tokens": ["Be\u00b7gr\u00fc\u00b7\u00dfend", ",", "lei\u00b7se", "die\u00b7se", "Wor\u00b7te", "zu", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "\u00bbgestorben, ja gestorben bist auch du \u2013 \u2013\u00ab", "tokens": ["\u00bb", "ge\u00b7stor\u00b7ben", ",", "ja", "ge\u00b7stor\u00b7ben", "bist", "auch", "du", "\u2013", "\u2013", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVPP", "$,", "ADV", "VVPP", "VAFIN", "ADV", "PPER", "$(", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Und in der Ferne dann ein hohles Lachen.", "tokens": ["Und", "in", "der", "Fer\u00b7ne", "dann", "ein", "hoh\u00b7les", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "War's eitel T\u00e4uschung? Fragt den Dichter nicht!", "tokens": ["Wa\u00b7r's", "ei\u00b7tel", "T\u00e4u\u00b7schung", "?", "Fragt", "den", "Dich\u00b7ter", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "$.", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.42": {"text": "An meiner Seite fand ich den Gef\u00e4hrten,", "tokens": ["An", "mei\u00b7ner", "Sei\u00b7te", "fand", "ich", "den", "Ge\u00b7f\u00e4hr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Den treuen Freund, den starken, vielbew\u00e4hrten;", "tokens": ["Den", "treu\u00b7en", "Freund", ",", "den", "star\u00b7ken", ",", "viel\u00b7be\u00b7w\u00e4hr\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Ein blasser Mondstrahl fiel auf sein Gesicht.", "tokens": ["Ein", "blas\u00b7ser", "Mond\u00b7strahl", "fiel", "auf", "sein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.45": {"text": "Ersch\u00fcttert, wie ich nimmer ihn gesehn,", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", ",", "wie", "ich", "nim\u00b7mer", "ihn", "ge\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Doch die gespannte Flinte unterm Arme,", "tokens": ["Doch", "die", "ge\u00b7spann\u00b7te", "Flin\u00b7te", "un\u00b7term", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Ergriff er meine Hand, die fieberwarme,", "tokens": ["Er\u00b7griff", "er", "mei\u00b7ne", "Hand", ",", "die", "fie\u00b7ber\u00b7war\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Und sagte: \u00bbFreund, wir m\u00fcssen wieder gehn.\u00ab", "tokens": ["Und", "sag\u00b7te", ":", "\u00bb", "Freund", ",", "wir", "m\u00fcs\u00b7sen", "wie\u00b7der", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sp\u00e4therbst. \u2013 Wir schritten langsam durch den Wald", "tokens": ["Sp\u00e4t\u00b7herbst", ".", "\u2013", "Wir", "schrit\u00b7ten", "lang\u00b7sam", "durch", "den", "Wald"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "$(", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zur D\u00e4mmrungszeit, erm\u00fcdet und verdrossen,", "tokens": ["Zur", "D\u00e4mm\u00b7rungs\u00b7zeit", ",", "er\u00b7m\u00fc\u00b7det", "und", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da sprach ich zu dem wackern Jagdgenossen:", "tokens": ["Da", "sprach", "ich", "zu", "dem", "wa\u00b7ckern", "Jagd\u00b7ge\u00b7nos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbfreund, la\u00df uns hier ein Weilchen ruhn!\u00ab \u2013 Und bald", "tokens": ["\u00bb", "freund", ",", "la\u00df", "uns", "hier", "ein", "Weil\u00b7chen", "ruhn", "!", "\u00ab", "\u2013", "Und", "bald"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["$(", "ADJD", "$,", "VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$.", "$(", "$(", "KON", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Erstarben uns die Worte auf den Lippen.", "tokens": ["Er\u00b7star\u00b7ben", "uns", "die", "Wor\u00b7te", "auf", "den", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Busche h\u00f6rten wir den Nachtwind s\u00e4useln,", "tokens": ["Im", "Bu\u00b7sche", "h\u00f6r\u00b7ten", "wir", "den", "Nacht\u00b7wind", "s\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das tote Laub zu unsern F\u00fc\u00dfen kr\u00e4useln,", "tokens": ["Das", "to\u00b7te", "Laub", "zu", "un\u00b7sern", "F\u00fc\u00b7\u00dfen", "kr\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und alte Birken sahn wir, gleich Gerippen,", "tokens": ["Und", "al\u00b7te", "Bir\u00b7ken", "sahn", "wir", ",", "gleich", "Ge\u00b7rip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Im schwarzen Moorgrund; Schatten, riesenhaft,", "tokens": ["Im", "schwar\u00b7zen", "Mo\u00b7or\u00b7grund", ";", "Schat\u00b7ten", ",", "rie\u00b7sen\u00b7haft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "NN", "$,", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Umflogen uns und huschten rasch vor\u00fcber;", "tokens": ["Um\u00b7flo\u00b7gen", "uns", "und", "huschten", "rasch", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Des Tages Nachglanz wurde bleicher, tr\u00fcber.", "tokens": ["Des", "Ta\u00b7ges", "Nach\u00b7glanz", "wur\u00b7de", "blei\u00b7cher", ",", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Unheimlich war es in der Nachbarschaft;", "tokens": ["Un\u00b7heim\u00b7lich", "war", "es", "in", "der", "Nach\u00b7bar\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ein sonderbares Regen in den Zweigen,", "tokens": ["Ein", "son\u00b7der\u00b7ba\u00b7res", "Re\u00b7gen", "in", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sonst alles tiefes Schweigen. \u2013", "tokens": ["Sonst", "al\u00b7les", "tie\u00b7fes", "Schwei\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Ich schlief nicht, tr\u00e4umte nicht; ein Schleier lag", "tokens": ["Ich", "schlief", "nicht", ",", "tr\u00e4um\u00b7te", "nicht", ";", "ein", "Schlei\u00b7er", "lag"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "PTKNEG", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Auf mir, doch blieb ich meiner Sinne m\u00e4chtig \u2013", "tokens": ["Auf", "mir", ",", "doch", "blieb", "ich", "mei\u00b7ner", "Sin\u00b7ne", "m\u00e4ch\u00b7tig", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und da, in meiner N\u00e4he, \u00fcbern\u00e4chtig,", "tokens": ["Und", "da", ",", "in", "mei\u00b7ner", "N\u00e4\u00b7he", ",", "\u00fc\u00b7bern\u00b7\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Erschien mir pl\u00f6tzlich, blendend wie der Tag,", "tokens": ["Er\u00b7schien", "mir", "pl\u00f6tz\u00b7lich", ",", "blen\u00b7dend", "wie", "der", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ein Bild, das schmerzliche Erinnrung weckte.", "tokens": ["Ein", "Bild", ",", "das", "schmerz\u00b7li\u00b7che", "E\u00b7rinn\u00b7rung", "weck\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.20": {"text": "Du warst es, stolze Lady Margaret,", "tokens": ["Du", "warst", "es", ",", "stol\u00b7ze", "La\u00b7dy", "Mar\u00b7ga\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Du, deren Liebe ich umsonst erfleht,", "tokens": ["Du", ",", "de\u00b7ren", "Lie\u00b7be", "ich", "um\u00b7sonst", "er\u00b7fleht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Du, deren Sarg mit Kr\u00e4nzen ich bedeckte \u2013", "tokens": ["Du", ",", "de\u00b7ren", "Sarg", "mit", "Kr\u00e4n\u00b7zen", "ich", "be\u00b7deck\u00b7te", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "APPR", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "O langbeweinte, herrliche Gestalt!", "tokens": ["O", "lang\u00b7be\u00b7wein\u00b7te", ",", "herr\u00b7li\u00b7che", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Du sa\u00dfest wieder auf dem wei\u00dfen Pferde", "tokens": ["Du", "sa\u00b7\u00dfest", "wie\u00b7der", "auf", "dem", "wei\u00b7\u00dfen", "Pfe