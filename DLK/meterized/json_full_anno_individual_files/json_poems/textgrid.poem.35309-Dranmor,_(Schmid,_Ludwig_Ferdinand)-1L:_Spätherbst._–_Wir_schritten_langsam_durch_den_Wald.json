er": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Vorbei, vorbei! \u2013 Ein Winken mit dem Tuche,", "tokens": ["Vor\u00b7bei", ",", "vor\u00b7bei", "!", "\u2013", "Ein", "Win\u00b7ken", "mit", "dem", "Tu\u00b7che", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$.", "$(", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "O teures Antlitz, das ich ewig suche,", "tokens": ["O", "teu\u00b7res", "Ant\u00b7litz", ",", "das", "ich", "e\u00b7wig", "su\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Ein letzter Blick \u2013 und die Erscheinung schwand.", "tokens": ["Ein", "letz\u00b7ter", "Blick", "\u2013", "und", "die", "Er\u00b7schei\u00b7nung", "schwand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Und sprachlos starrend in des Waldes D\u00fcster", "tokens": ["Und", "sprach\u00b7los", "star\u00b7rend", "in", "des", "Wal\u00b7des", "D\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Vernahm ich jetzt ein Rauschen, ein Gefl\u00fcster \u2013", "tokens": ["Ver\u00b7nahm", "ich", "jetzt", "ein", "Rau\u00b7schen", ",", "ein", "Ge\u00b7fl\u00fcs\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "