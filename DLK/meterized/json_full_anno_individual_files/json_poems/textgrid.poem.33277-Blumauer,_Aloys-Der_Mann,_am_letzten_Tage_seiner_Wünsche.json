{"textgrid.poem.33277": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Der Mann, am letzten Tage seiner W\u00fcnsche", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie naht, sie naht die s\u00fcsse Stunde,", "tokens": ["Sie", "naht", ",", "sie", "naht", "die", "s\u00fcs\u00b7se", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom b\u00e4ngsten Wunsch nur langsam hergef\u00fchrt,", "tokens": ["Vom", "b\u00e4ngs\u00b7ten", "Wunsch", "nur", "lang\u00b7sam", "her\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Lieb' und Lust in ihrem sch\u00f6nsten Bunde", "tokens": ["Die", "Lieb'", "und", "Lust", "in", "ih\u00b7rem", "sch\u00f6ns\u00b7ten", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An mir und ihr erblicken wird.", "tokens": ["An", "mir", "und", "ihr", "er\u00b7bli\u00b7cken", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hernieder, Tag, auf deiner Fahrt, hernieder,", "tokens": ["Her\u00b7nie\u00b7der", ",", "Tag", ",", "auf", "dei\u00b7ner", "Fahrt", ",", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der du noch zwischen mir und meinem Gl\u00fccke stehst,", "tokens": ["Der", "du", "noch", "zwi\u00b7schen", "mir", "und", "mei\u00b7nem", "Gl\u00fc\u00b7cke", "stehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und Sonne, du, schlie\u00df deine Augenlieder,", "tokens": ["Und", "Son\u00b7ne", ",", "du", ",", "schlie\u00df", "dei\u00b7ne", "Au\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du bist nur sch\u00f6n, wenn du zu Bette gehst.", "tokens": ["Du", "bist", "nur", "sch\u00f6n", ",", "wenn", "du", "zu", "Bet\u00b7te", "gehst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Herauf, o Nacht, auf deinem Sternenwagen,", "tokens": ["Her\u00b7auf", ",", "o", "Nacht", ",", "auf", "dei\u00b7nem", "Ster\u00b7nen\u00b7wa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Vom stillen Mond herangef\u00fchrt,", "tokens": ["Vom", "stil\u00b7len", "Mond", "her\u00b7an\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ha, w\u00fcrde dir, was mir heut werden wird,", "tokens": ["Ha", ",", "w\u00fcr\u00b7de", "dir", ",", "was", "mir", "heut", "wer\u00b7den", "wird", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VAINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Du w\u00fcrdest deine Rosse ausser Athem jagen.", "tokens": ["Du", "w\u00fcr\u00b7dest", "dei\u00b7ne", "Ros\u00b7se", "aus\u00b7ser", "A\u00b7them", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was alles ich in meinen J\u00fcnglingstagen", "tokens": ["Was", "al\u00b7les", "ich", "in", "mei\u00b7nen", "J\u00fcng\u00b7lings\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir von Genu\u00df und Freude phantasirt,", "tokens": ["Mir", "von", "Ge\u00b7nu\u00df", "und", "Freu\u00b7de", "phan\u00b7ta\u00b7sirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Dies alles und weit mehr noch wird", "tokens": ["Dies", "al\u00b7les", "und", "weit", "mehr", "noch", "wird"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "KON", "ADJD", "ADV", "ADV", "VAFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Heut Wirklichkeit. \u2013 O gauckelt immerhin", "tokens": ["Heut", "Wirk\u00b7lich\u00b7keit", ".", "\u2013", "O", "gau\u00b7ckelt", "im\u00b7mer\u00b7hin"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$.", "$(", "NE", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr Traumideen jener Zeit,", "tokens": ["Ihr", "Trau\u00b7mi\u00b7de\u00b7en", "je\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um meinen heissen, liebetrunk'nen Sinn,", "tokens": ["Um", "mei\u00b7nen", "heis\u00b7sen", ",", "lie\u00b7be\u00b7trunk'\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihr werdet heut zur Wirklichkeit.", "tokens": ["Ihr", "wer\u00b7det", "heut", "zur", "Wirk\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit euch, mit euch gie\u00df' ich den Becher", "tokens": ["Mit", "euch", ",", "mit", "euch", "gie\u00df'", "ich", "den", "Be\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Lust, aus dem mir armen Lecher", "tokens": ["Der", "Lust", ",", "aus", "dem", "mir", "ar\u00b7men", "Le\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst immer Nahrung neues Durstes quoll,", "tokens": ["Sonst", "im\u00b7mer", "Nah\u00b7rung", "neu\u00b7es", "Durs\u00b7tes", "quoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit euch gie\u00df' ich ihn heute sch\u00e4umend voll,", "tokens": ["Mit", "euch", "gie\u00df'", "ich", "ihn", "heu\u00b7te", "sch\u00e4u\u00b7mend", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "ADJD", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und halt' ihn hoch empor, und seh' ihn an,", "tokens": ["Und", "halt'", "ihn", "hoch", "em\u00b7por", ",", "und", "seh'", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bis mir vor Trinkbegier die Lippen gl\u00fchen;", "tokens": ["Bis", "mir", "vor", "Trink\u00b7be\u00b7gier", "die", "Lip\u00b7pen", "gl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und will schon Geist und Seele mir entfliehen,", "tokens": ["Und", "will", "schon", "Geist", "und", "See\u00b7le", "mir", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So h\u00e4ng' ich mich mit beiden Lippen d'ran,", "tokens": ["So", "h\u00e4ng'", "ich", "mich", "mit", "bei\u00b7den", "Lip\u00b7pen", "d'\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Und trink' \u2013 und krachten alle Pole,", "tokens": ["Und", "trink'", "\u2013", "und", "krach\u00b7ten", "al\u00b7le", "Po\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und brennte H\u00f6llenfeu'r mir an der Sohle,", "tokens": ["Und", "brenn\u00b7te", "H\u00f6l\u00b7len\u00b7feu'r", "mir", "an", "der", "Soh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und st\u00fcrzte Erd' und Himmel mit Gebraus \u2013", "tokens": ["Und", "st\u00fcrz\u00b7te", "Erd'", "und", "Him\u00b7mel", "mit", "Ge\u00b7braus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Bei Gott! \u2013 ich tr\u00e4nk' ihn aus!", "tokens": ["Bei", "Gott", "!", "\u2013", "ich", "tr\u00e4nk'", "ihn", "aus", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie naht, sie naht die s\u00fcsse Stunde,", "tokens": ["Sie", "naht", ",", "sie", "naht", "die", "s\u00fcs\u00b7se", "Stun\u00b7de", ","], "toke