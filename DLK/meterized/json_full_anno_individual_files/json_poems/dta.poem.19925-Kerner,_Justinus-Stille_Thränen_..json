{"dta.poem.19925": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Stille Thr\u00e4nen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist vom Schlaf erstanden", "tokens": ["Du", "bist", "vom", "Schlaf", "er\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wandelst durch die Au,", "tokens": ["Und", "wan\u00b7delst", "durch", "die", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da liegt ob allen Landen", "tokens": ["Da", "liegt", "ob", "al\u00b7len", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Himmel ", "tokens": ["Der", "Him\u00b7mel"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Wei\u00dft nicht, da\u00df als ohn' Sorgen", "tokens": ["Wei\u00dft", "nicht", ",", "da\u00df", "als", "ohn'", "Sor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du schliefest schmerzenlos,", "tokens": ["Du", "schlie\u00b7fest", "schmer\u00b7zen\u00b7los", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel bis zum Morgen", "tokens": ["Der", "Him\u00b7mel", "bis", "zum", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel Thr\u00e4nen niedergo\u00df.", "tokens": ["Viel", "Thr\u00e4\u00b7nen", "nie\u00b7der\u00b7go\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In stillen N\u00e4chten weinet", "tokens": ["In", "stil\u00b7len", "N\u00e4ch\u00b7ten", "wei\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Oft mancher aus den Schmerz,", "tokens": ["Oft", "man\u00b7cher", "aus", "den", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Morgens dann man meinet", "tokens": ["Und", "Mor\u00b7gens", "dann", "man", "mei\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ste", "tokens": ["Ste"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}}}}