{"dta.poem.7727": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der edle K\u00f6nig kam an seinem Siegestag", "tokens": ["Der", "ed\u00b7le", "K\u00f6\u00b7nig", "kam", "an", "sei\u00b7nem", "Sie\u00b7ge\u00b7stag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur pr\u00e4cht'gen Gruft, in der sein Widersacher lag.", "tokens": ["Zur", "pr\u00e4cht'\u00b7gen", "Gruft", ",", "in", "der", "sein", "Wi\u00b7der\u00b7sa\u00b7cher", "lag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da sprachen sie: Es ist nach unsres K\u00f6nigs Siege", "tokens": ["Da", "spra\u00b7chen", "sie", ":", "Es", "ist", "nach", "uns\u00b7res", "K\u00f6\u00b7nigs", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht Recht, da\u00df so geehrt sein \u00e4rgster Todfeind liege.", "tokens": ["Nicht", "Recht", ",", "da\u00df", "so", "ge\u00b7ehrt", "sein", "\u00e4rgs\u00b7ter", "Tod\u00b7feind", "lie\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "KOUS", "ADV", "VVPP", "VAINF", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ausgraben soll man ihn und nebenaus ihn legen.", "tokens": ["Aus\u00b7gra\u00b7ben", "soll", "man", "ihn", "und", "ne\u00b7be\u00b7naus", "ihn", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "PPER", "KON", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der K\u00f6nig aber sprach: Es soll ihn Niemand regen.", "tokens": ["Der", "K\u00f6\u00b7nig", "a\u00b7ber", "sprach", ":", "Es", "soll", "ihn", "Nie\u00b7mand", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Im Todfeind gegen uns war Tod und Feind verbunden;", "tokens": ["Im", "Tod\u00b7feind", "ge\u00b7gen", "uns", "war", "Tod", "und", "Feind", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun hat der Tod den Feind, den Tod der Feind gefunden.", "tokens": ["Nun", "hat", "der", "Tod", "den", "Feind", ",", "den", "Tod", "der", "Feind", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "La\u00dft ihn nur liegen so! Was k\u00f6nnt' ich bessers haben,", "tokens": ["La\u00dft", "ihn", "nur", "lie\u00b7gen", "so", "!", "Was", "k\u00f6nnt'", "ich", "bes\u00b7sers", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVFIN", "ADV", "$.", "PWS", "VMFIN", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als l\u00e4ge jeder Feind so pr\u00e4chtig mir begraben!", "tokens": ["Als", "l\u00e4\u00b7ge", "je\u00b7der", "Feind", "so", "pr\u00e4ch\u00b7tig", "mir", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PIAT", "NN", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}