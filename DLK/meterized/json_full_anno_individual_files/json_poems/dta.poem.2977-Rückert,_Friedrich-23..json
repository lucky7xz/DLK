{"dta.poem.2977": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie zu vereinigen ist all der Sekten Heer?", "tokens": ["Wie", "zu", "ver\u00b7ei\u00b7ni\u00b7gen", "ist", "all", "der", "Sek\u00b7ten", "Heer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKZU", "VVINF", "VAFIN", "PIAT", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jeder Versuch dazu gibt eine neue mehr.", "tokens": ["Je\u00b7der", "Ver\u00b7such", "da\u00b7zu", "gibt", "ei\u00b7ne", "neu\u00b7e", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PAV", "VVFIN", "ART", "ADJA", "ADV", "$."], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Wie wenn verschiedene Hundarten sich vermischen;", "tokens": ["Wie", "wenn", "ver\u00b7schie\u00b7de\u00b7ne", "Hund\u00b7ar\u00b7ten", "sich", "ver\u00b7mi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die alten bleiben, und die neue w\u00e4chst dazwischen.", "tokens": ["Die", "al\u00b7ten", "blei\u00b7ben", ",", "und", "die", "neu\u00b7e", "w\u00e4chst", "da\u00b7zwi\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$,", "KON", "ART", "ADJA", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}