, "PPOSAT", "VVINF", "$,", "VVINF", "KON", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.69": {"text": "Das werthe Qvedlinburg, das dich, als ein exempel", "tokens": ["Das", "wert\u00b7he", "Qved\u00b7lin\u00b7burg", ",", "das", "dich", ",", "als", "ein", "ex\u00b7em\u00b7pel"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NE", "$,", "PRELS", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.70": {"text": "Rechtschaffner priester, liebt, gr\u00e4bt diesen schlu\u00df in stein,", "tokens": ["Recht\u00b7schaff\u00b7ner", "pries\u00b7ter", ",", "liebt", ",", "gr\u00e4bt", "die\u00b7sen", "schlu\u00df", "in", "stein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVFIN", "$,", "VVFIN", "PDAT", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Ja selber in das hertz: Der, so in meinem tempel,", "tokens": ["Ja", "sel\u00b7ber", "in", "das", "hertz", ":", "Der", ",", "so", "in", "mei\u00b7nem", "tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "ART", "NN", "$.", "ART", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.72": {"text": "Als Hoherpriester lehrt, mu\u00df so, wie Kettner seyn.", "tokens": ["Als", "Ho\u00b7her\u00b7pries\u00b7ter", "lehrt", ",", "mu\u00df", "so", ",", "wie", "Kett\u00b7ner", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "VMFIN", "ADV", "$,", "PWAV", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hex