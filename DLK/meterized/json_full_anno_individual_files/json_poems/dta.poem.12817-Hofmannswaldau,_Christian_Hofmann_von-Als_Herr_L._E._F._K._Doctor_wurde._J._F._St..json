{"dta.poem.12817": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als  \n Herr  L.  E. F. K.  Doctor  wurde.  \n J. F. St.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebtes Qvedlinburg! umg\u00fcrte dich mit freuden,", "tokens": ["Ge\u00b7lieb\u00b7tes", "Qved\u00b7lin\u00b7burg", "!", "um\u00b7g\u00fcr\u00b7te", "dich", "mit", "freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dieweil dein Kettner itzt den Doctor-hut empf\u00e4ngt:", "tokens": ["Die\u00b7weil", "dein", "Kett\u00b7ner", "itzt", "den", "Doc\u00b7tor\u00b7hut", "emp\u00b7f\u00e4ngt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Kettner, der dich pflegt mit himmels-kost zu weiden,", "tokens": ["Dein", "Kett\u00b7ner", ",", "der", "dich", "pflegt", "mit", "him\u00b7mels\u00b7kost", "zu", "wei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und kein gef\u00e4hrlich gifft in seine lehren mengt.", "tokens": ["Und", "kein", "ge\u00b7f\u00e4hr\u00b7lich", "gifft", "in", "sei\u00b7ne", "leh\u00b7ren", "mengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dergleichen priester sind vor andern hoch zu halten,", "tokens": ["Derg\u00b7lei\u00b7chen", "pries\u00b7ter", "sind", "vor", "an\u00b7dern", "hoch", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VAFIN", "APPR", "PIS", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jemehr das unkraut sich in Zions g\u00e4rten dringt.", "tokens": ["Je\u00b7mehr", "das", "un\u00b7kraut", "sich", "in", "Zi\u00b7ons", "g\u00e4r\u00b7ten", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "PRF", "APPR", "NE", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der andacht feuer will bey manchem gar erkalten,", "tokens": ["Der", "an\u00b7dacht", "feu\u00b7er", "will", "bey", "man\u00b7chem", "gar", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VMFIN", "APPR", "PIAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der mit dem munde doch GOtt t\u00e4glich opffer bringt.", "tokens": ["Der", "mit", "dem", "mun\u00b7de", "doch", "Gott", "t\u00e4g\u00b7lich", "opf\u00b7fer", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "KON", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So ist dein Kettner nicht; von so verkehrten lehrern", "tokens": ["So", "ist", "dein", "Kett\u00b7ner", "nicht", ";", "von", "so", "ver\u00b7kehr\u00b7ten", "leh\u00b7rern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "$.", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist, Werthes Qvedlinburg! dein heiligthum befreyt.", "tokens": ["Ist", ",", "Wert\u00b7hes", "Qved\u00b7lin\u00b7burg", "!", "dein", "hei\u00b7lig\u00b7thum", "be\u00b7freyt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJA", "NN", "$.", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Wo so ein Haupt regiert, da fehlt es an verehrern", "tokens": ["Wo", "so", "ein", "Haupt", "re\u00b7giert", ",", "da", "fehlt", "es", "an", "ver\u00b7eh\u00b7rern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des reinen glaubens nicht, da herrscht die heiligkeit.", "tokens": ["Des", "rei\u00b7nen", "glau\u00b7bens", "nicht", ",", "da", "herrscht", "die", "hei\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wer unsern Kettner kennt, der wird ihn loben m\u00fcssen,", "tokens": ["Wer", "un\u00b7sern", "Kett\u00b7ner", "kennt", ",", "der", "wird", "ihn", "lo\u00b7ben", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "VAFIN", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und den ertheilten ruhm ihm gerne zugestehn.", "tokens": ["Und", "den", "er\u00b7theil\u00b7ten", "ruhm", "ihm", "ger\u00b7ne", "zu\u00b7ge\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "In diesem Lehrer paart sich wissen und gewissen,", "tokens": ["In", "die\u00b7sem", "Leh\u00b7rer", "paart", "sich", "wis\u00b7sen", "und", "ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PRF", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die itzt bey vielen nicht in einem paare gehn.", "tokens": ["Die", "itzt", "bey", "vie\u00b7len", "nicht", "in", "ei\u00b7nem", "paa\u00b7re", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PIS", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wer wei\u00df nicht, wie itzund der schwarm der ignoranten", "tokens": ["Wer", "wei\u00df", "nicht", ",", "wie", "it\u00b7zund", "der", "schwarm", "der", "ig\u00b7no\u00b7ran\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "$,", "PWAV", "ADV", "ART", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ohn\u2019 alle wissenschafft des Geistes amt begehrt;", "tokens": ["Ohn'", "al\u00b7le", "wis\u00b7sen\u00b7schafft", "des", "Geis\u00b7tes", "amt", "be\u00b7gehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie manche lieber gar die fr\u00f6mmigkeit verbannten,", "tokens": ["Wie", "man\u00b7che", "lie\u00b7ber", "gar", "die", "fr\u00f6m\u00b7mig\u00b7keit", "ver\u00b7bann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Weil wohllust, geitz und stoltz ihr glaubens-\u00f6l verzehrt.", "tokens": ["Weil", "wohl\u00b7lust", ",", "geitz", "und", "stoltz", "ihr", "glau\u00b7bens\u00b7\u00f6l", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "VVIMP", "KON", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "In einem priester mu\u00df die fr\u00f6mmigkeit nicht fehlen,", "tokens": ["In", "ei\u00b7nem", "pries\u00b7ter", "mu\u00df", "die", "fr\u00f6m\u00b7mig\u00b7keit", "nicht", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Doch ohne wissenschafft kan sie nicht richtig seyn.", "tokens": ["Doch", "oh\u00b7ne", "wis\u00b7sen\u00b7schafft", "kan", "sie", "nicht", "rich\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wer demnach eine nur von beyden will erwehlen,", "tokens": ["Wer", "dem\u00b7nach", "ei\u00b7ne", "nur", "von", "bey\u00b7den", "will", "er\u00b7weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "ART", "ADV", "APPR", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der stellt sich in das chor beth\u00f6rter lehrer ein.", "tokens": ["Der", "stellt", "sich", "in", "das", "chor", "be\u00b7th\u00f6r\u00b7ter", "leh\u00b7rer", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "In unserm Kettner sieht man beyde sich umfassen:", "tokens": ["In", "un\u00b7serm", "Kett\u00b7ner", "sieht", "man", "bey\u00b7de", "sich", "um\u00b7fas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Er scheut den aberwitz, der rotten geister nicht,", "tokens": ["Er", "scheut", "den", "a\u00b7ber\u00b7witz", ",", "der", "rot\u00b7ten", "geis\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wie die, so die vernunfft als wie den Satan hassen,", "tokens": ["Wie", "die", ",", "so", "die", "ver\u00b7nunfft", "als", "wie", "den", "Sa\u00b7tan", "has\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "$,", "ADV", "ART", "NN", "KOUS", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil sie durch alle macht der falschen schl\u00fcsse bricht.", "tokens": ["Weil", "sie", "durch", "al\u00b7le", "macht", "der", "fal\u00b7schen", "schl\u00fcs\u00b7se", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Er kan des H\u00f6chsten wort in seinen sprachen lesen,", "tokens": ["Er", "kan", "des", "H\u00f6chs\u00b7ten", "wort", "in", "sei\u00b7nen", "spra\u00b7chen", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Er kennet auch zugleich die grentzen der vernunfft,", "tokens": ["Er", "ken\u00b7net", "auch", "zu\u00b7gleich", "die", "grent\u00b7zen", "der", "ver\u00b7nunfft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Spinoza ist vor ihn nicht zu subtil gewesen,", "tokens": ["Spi\u00b7no\u00b7za", "ist", "vor", "ihn", "nicht", "zu", "sub\u00b7til", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPER", "PTKNEG", "PTKA", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Er wei\u00df die falsche kunst, der atheisten zunfft.", "tokens": ["Er", "wei\u00df", "die", "fal\u00b7sche", "kunst", ",", "der", "at\u00b7heis\u00b7ten", "zunfft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Socinens rotte mu\u00df vor seiner feder zittern,", "tokens": ["So\u00b7ci\u00b7nens", "rot\u00b7te", "mu\u00df", "vor", "sei\u00b7ner", "fe\u00b7der", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die wahre lehre bleibt auf festem grunde stehn.", "tokens": ["Die", "wah\u00b7re", "leh\u00b7re", "bleibt", "auf", "fes\u00b7tem", "grun\u00b7de", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es kan kein falscher wahn den glaubens-grund ersch\u00fcttern,", "tokens": ["Es", "kan", "kein", "fal\u00b7scher", "wahn", "den", "glau\u00b7bens\u00b7grund", "er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Er mu\u00df, als wie der schnee bey heisser glut, zergehn.", "tokens": ["Er", "mu\u00df", ",", "als", "wie", "der", "schnee", "bey", "heis\u00b7ser", "glut", ",", "zer\u00b7gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Zu diesem wissen kommt auch endlich das gewissen,", "tokens": ["Zu", "die\u00b7sem", "wis\u00b7sen", "kommt", "auch", "end\u00b7lich", "das", "ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ADV", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das Kettners klugen geist zur gottesfurcht gef\u00fchrt.", "tokens": ["Das", "Kett\u00b7ners", "klu\u00b7gen", "geist", "zur", "got\u00b7tes\u00b7furcht", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wer wei\u00df nicht, wie er sich der schn\u00f6den welt entrissen,", "tokens": ["Wer", "wei\u00df", "nicht", ",", "wie", "er", "sich", "der", "schn\u00f6\u00b7den", "welt", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PRF", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und wie sein wandel stets sein hohes amt geziert?", "tokens": ["Und", "wie", "sein", "wan\u00b7del", "stets", "sein", "ho\u00b7hes", "amt", "ge\u00b7ziert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sein predigen wei\u00df nichts von eitlen k\u00fcnsteleyen,", "tokens": ["Sein", "pre\u00b7di\u00b7gen", "wei\u00df", "nichts", "von", "eit\u00b7len", "k\u00fcns\u00b7te\u00b7le\u00b7yen", ","], "token_inf