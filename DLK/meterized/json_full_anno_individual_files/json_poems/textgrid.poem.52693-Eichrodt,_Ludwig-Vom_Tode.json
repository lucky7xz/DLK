{"textgrid.poem.52693": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Vom Tode", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So nenne mir den allersch\u00f6nsten Tod.", "tokens": ["So", "nen\u00b7ne", "mir", "den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich nenne dir den Tod in hei\u00dfer Schlacht \u2013", "tokens": ["Ich", "nen\u00b7ne", "dir", "den", "Tod", "in", "hei\u00b7\u00dfer", "Schlacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um theure G\u00fcter ist der Streit erwacht,", "tokens": ["Um", "theu\u00b7re", "G\u00fc\u00b7ter", "ist", "der", "Streit", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr Ehre, Freiheit, f\u00fcr dein gutes Recht,", "tokens": ["F\u00fcr", "Eh\u00b7re", ",", "Frei\u00b7heit", ",", "f\u00fcr", "dein", "gu\u00b7tes", "Recht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich denke solcher Tod d\u00fcnkt Niemand schlecht.", "tokens": ["Ich", "den\u00b7ke", "sol\u00b7cher", "Tod", "d\u00fcnkt", "Nie\u00b7mand", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "VVFIN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Heldentod nenn ich den sch\u00f6nsten Tod.", "tokens": ["Den", "Hel\u00b7den\u00b7tod", "nenn", "ich", "den", "sch\u00f6ns\u00b7ten", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So nenne mir den allersch\u00f6nsten Tod.", "tokens": ["So", "nen\u00b7ne", "mir", "den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich nenn den Tod, in den ich freudig geh", "tokens": ["Ich", "nenn", "den", "Tod", ",", "in", "den", "ich", "freu\u00b7dig", "geh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr Ueberzeugung, Wahrheit, die Idee \u2013", "tokens": ["F\u00fcr", "Ue\u00b7ber\u00b7zeu\u00b7gung", ",", "Wahr\u00b7heit", ",", "die", "I\u00b7dee", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Kerker, auf der Walstatt, dem Schaffot,", "tokens": ["Im", "Ker\u00b7ker", ",", "auf", "der", "Wals\u00b7tatt", ",", "dem", "Schaf\u00b7fot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Sieg ist mein, ich sterb f\u00fcr meinen Gott.", "tokens": ["Der", "Sieg", "ist", "mein", ",", "ich", "sterb", "f\u00fcr", "mei\u00b7nen", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Zeugentod nenn ich den sch\u00f6nsten Tod.", "tokens": ["Den", "Zeu\u00b7gen\u00b7tod", "nenn", "ich", "den", "sch\u00f6ns\u00b7ten", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So nenne mir den allersch\u00f6nsten Tod.", "tokens": ["So", "nen\u00b7ne", "mir", "den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich nenne dir den Tod in Manneskraft,", "tokens": ["Ich", "nen\u00b7ne", "dir", "den", "Tod", "in", "Man\u00b7nes\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als Opfer edler That dahingerafft;", "tokens": ["Als", "Op\u00b7fer", "ed\u00b7ler", "That", "da\u00b7hin\u00b7ge\u00b7rafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du stirbst ein Retter, stirbst ein Tugendheld,", "tokens": ["Du", "stirbst", "ein", "Ret\u00b7ter", ",", "stirbst", "ein", "Tu\u00b7gend\u00b7held", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Lohn im Herzen, unterm Dank der Welt.", "tokens": ["Den", "Lohn", "im", "Her\u00b7zen", ",", "un\u00b7term", "Dank", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Opfertod nenn ich den sch\u00f6nsten Tod.", "tokens": ["Den", "Op\u00b7fer\u00b7tod", "nenn", "ich", "den", "sch\u00f6ns\u00b7ten", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So nenne mir den allersch\u00f6nsten Tod.", "tokens": ["So", "nen\u00b7ne", "mir", "den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich nenne dir den Tod durch s\u00fc\u00dfes Gift \u2013", "tokens": ["Ich", "nen\u00b7ne", "dir", "den", "Tod", "durch", "s\u00fc\u00b7\u00dfes", "Gift", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch Blitz, der aus dem heitern Himmel trifft;", "tokens": ["Durch", "Blitz", ",", "der", "aus", "dem", "hei\u00b7tern", "Him\u00b7mel", "trifft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sei es im Elend, das er schnell beschlie\u00dft,", "tokens": ["Sei", "es", "im", "E\u00b7lend", ",", "das", "er", "schnell", "be\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Seis in der Lust, die sorgenlos genie\u00dft.", "tokens": ["Seis", "in", "der", "Lust", ",", "die", "sor\u00b7gen\u00b7los", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.6": {"text": "Der Unerwartete ist sch\u00f6nster Tod.", "tokens": ["Der", "Un\u00b7er\u00b7war\u00b7te\u00b7te", "ist", "sch\u00f6ns\u00b7ter", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So nenne mir den allersch\u00f6nsten Tod.", "tokens": ["So", "nen\u00b7ne", "mir", "den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich nenn den Tod \u2013 wenn lebensm\u00fcd und schwach", "tokens": ["Ich", "nenn", "den", "Tod", "\u2013", "wenn", "le\u00b7bens\u00b7m\u00fcd", "und", "schwach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "KOUS", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Leib entschl\u00e4ft im heimischen Gemach.", "tokens": ["Der", "Leib", "ent\u00b7schl\u00e4ft", "im", "hei\u00b7mi\u00b7schen", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Rings traute Nacht \u2013 die Rechnung schlie\u00df ich ab", "tokens": ["Rings", "trau\u00b7te", "Nacht", "\u2013", "die", "Rech\u00b7nung", "schlie\u00df", "ich", "ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "$(", "ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit dieser Welt, Erl\u00f6sung ist das Grab.", "tokens": ["Mit", "die\u00b7ser", "Welt", ",", "Er\u00b7l\u00f6\u00b7sung", "ist", "das", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein selig Ende ist der sch\u00f6nste Tod.", "tokens": ["Ein", "se\u00b7lig", "En\u00b7de", "ist", "der", "sch\u00f6ns\u00b7te", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "So nenne mir den allersch\u00f6nsten Tod.", "tokens": ["So", "nen\u00b7ne", "mir", "den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bblausch auf! du siehst junge Blumen bl\u00fchn,", "tokens": ["\u00bb", "lausch", "auf", "!", "du", "siehst", "jun\u00b7ge", "Blu\u00b7men", "bl\u00fchn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Die stille Wiese und den Quell im Gr\u00fcn.", "tokens": ["Die", "stil\u00b7le", "Wie\u00b7se", "und", "den", "Quell", "im", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Morgenhauch, ein milder Sonnenstrahl,", "tokens": ["Ein", "Mor\u00b7gen\u00b7hauch", ",", "ein", "mil\u00b7der", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Fr\u00fchling ist gekommen in das Thal \u2013", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "ge\u00b7kom\u00b7men", "in", "das", "Thal", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dort stirbt ein Greis den gleichwillkommnen Tod.\u00ab", "tokens": ["Dort", "stirbt", "ein", "Greis", "den", "gleich\u00b7will\u00b7komm\u00b7nen", "Tod", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Du nennest mir den allersch\u00f6nsten Tod \u2013", "tokens": ["Du", "nen\u00b7nest", "mir", "den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Tod", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbich nenne dir den Tod, den ich erfleh;", "tokens": ["\u00bb", "ich", "nen\u00b7ne", "dir", "den", "Tod", ",", "den", "ich", "er\u00b7fleh", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein war mein Leben, mein war die Idee!", "tokens": ["Mein", "war", "mein", "Le\u00b7ben", ",", "mein", "war", "die", "I\u00b7dee", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "VAFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ich hab gek\u00e4mpft, geduldet, ich geno\u00df,", "tokens": ["Ich", "hab", "ge\u00b7k\u00e4mpft", ",", "ge\u00b7dul\u00b7det", ",", "ich", "ge\u00b7no\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVPP", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich ende \u2013 ohne Prunk, nur still und gro\u00df.", "tokens": ["Ich", "en\u00b7de", "\u2013", "oh\u00b7ne", "Prunk", ",", "nur", "still", "und", "gro\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "APPR", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nicht Arzt, nicht Priester k\u00fcndet mir den Tod.\u00ab", "tokens": ["Nicht", "Arzt", ",", "nicht", "Pries\u00b7ter", "k\u00fcn\u00b7det", "mir", "den", "Tod", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "\u00bbich preise dir den allersch\u00f6nsten Tod:", "tokens": ["\u00bb", "ich", "prei\u00b7se", "dir", "den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Tod", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Lieben all sie trugen mich hinaus,", "tokens": ["Die", "Lie\u00b7ben", "all", "sie", "tru\u00b7gen", "mich", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "PPER", "VVFIN", "PRF", "APZR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vier W\u00e4nde sind dem Sterbenden nur Graus.", "tokens": ["Vier", "W\u00e4n\u00b7de", "sind", "dem", "Ster\u00b7ben\u00b7den", "nur", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es war das Sein so reich so lebensroth \u2013", "tokens": ["Es", "war", "das", "Sein", "so", "reich", "so", "le\u00b7bens\u00b7roth", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nun ist mir Leben lieb und auch 