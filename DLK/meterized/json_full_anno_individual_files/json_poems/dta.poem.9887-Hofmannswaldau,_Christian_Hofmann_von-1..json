{"dta.poem.9887": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht stelle dich/ du engel dieser welt/", "tokens": ["Nicht", "stel\u00b7le", "dich", "/", "du", "en\u00b7gel", "die\u00b7ser", "welt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$(", "PPER", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als w\u00fcst du nicht/ wer dir zu f\u00fcssen f\u00e4llt;", "tokens": ["Als", "w\u00fcst", "du", "nicht", "/", "wer", "dir", "zu", "f\u00fcs\u00b7sen", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PTKNEG", "$(", "PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein mensch/ der dich zu einen abgott macht/", "tokens": ["Ein", "mensch", "/", "der", "dich", "zu", "ei\u00b7nen", "ab\u00b7gott", "macht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat seine brunst genug ans licht gebracht.", "tokens": ["Hat", "sei\u00b7ne", "brunst", "ge\u00b7nug", "ans", "licht", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du sprichst: Es sey dir g\u00e4ntzlich unbekant/", "tokens": ["Du", "sprichst", ":", "Es", "sey", "dir", "g\u00e4ntz\u00b7lich", "un\u00b7be\u00b7kant", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer dir sein hertz als weyrauch angebrant;", "tokens": ["Wer", "dir", "sein", "hertz", "als", "wey\u00b7rauch", "an\u00b7ge\u00b7brant", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und deine hand hat flamm und feur gef\u00fchlt/", "tokens": ["Und", "dei\u00b7ne", "hand", "hat", "flamm", "und", "feur", "ge\u00b7f\u00fchlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als meinen mund ich damit abgek\u00fchlt.", "tokens": ["Als", "mei\u00b7nen", "mund", "ich", "da\u00b7mit", "ab\u00b7ge\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mein auge lad\u2019t dich stets zum opffer ein/", "tokens": ["Mein", "au\u00b7ge", "lad't", "dich", "stets", "zum", "opf\u00b7fer", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kan dir/ was alle sehn/ verborgen feyn?", "tokens": ["Kan", "dir", "/", "was", "al\u00b7le", "sehn", "/", "ver\u00b7bor\u00b7gen", "feyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "PWS", "PIS", "VVINF", "$(", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die seele giebt dir heerd und rauchfa\u00df dar/", "tokens": ["Die", "see\u00b7le", "giebt", "dir", "heerd", "und", "rauch\u00b7fa\u00df", "dar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "KON", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und meine brust ist tempel und altar.", "tokens": ["Und", "mei\u00b7ne", "brust", "ist", "tem\u00b7pel", "und", "al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich drehe mich nach dir als ein magnet/", "tokens": ["Ich", "dre\u00b7he", "mich", "nach", "dir", "als", "ein", "mag\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "KOUS", "ART", "VVFIN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie? siehst du nicht/ wer dir vor augen steht?", "tokens": ["Wie", "?", "siehst", "du", "nicht", "/", "wer", "dir", "vor", "au\u00b7gen", "steht", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PTKNEG", "$(", "PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du meinst/ du l\u00f6schst so den gebrandten grau\u00df/", "tokens": ["Du", "meinst", "/", "du", "l\u00f6schst", "so", "den", "ge\u00b7brand\u00b7ten", "grau\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und giessest \u00f6hl vielmehr auf selben aus.", "tokens": ["Und", "gies\u00b7sest", "\u00f6hl", "viel\u00b7mehr", "auf", "sel\u00b7ben", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dein laulicht-seyn verl\u00f6scht nicht meine glut/", "tokens": ["Dein", "lau\u00b7licht\u00b7seyn", "ver\u00b7l\u00f6scht", "nicht", "mei\u00b7ne", "glut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den kalck entbrennt des wassers kalte fluth/", "tokens": ["Den", "kalck", "ent\u00b7brennt", "des", "was\u00b7sers", "kal\u00b7te", "fluth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil Bathseba sich in den brunnen findt/", "tokens": ["Weil", "Bath\u00b7se\u00b7ba", "sich", "in", "den", "brun\u00b7nen", "findt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wird Davids geist viel sch\u00e4rffer angez\u00fcndt.", "tokens": ["Wird", "Da\u00b7vids", "geist", "viel", "sch\u00e4rf\u00b7fer", "an\u00b7ge\u00b7z\u00fcndt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "VVFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Je mehr du fliehst/ je mehr verfolg ich dich/", "tokens": ["Je", "mehr", "du", "fliehst", "/", "je", "mehr", "ver\u00b7folg", "ich", "dich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$(", "ADV", "ADV", "VVFIN", "PPER", "PRF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch sturm und wind vermehrt das feuer sich/", "tokens": ["Durch", "sturm", "und", "wind", "ver\u00b7mehrt", "das", "feu\u00b7er", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "VAFIN", "ADJD", "ART", "NN", "PRF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stellstu dich noch so fremd und eckel an/", "tokens": ["Stell\u00b7stu", "dich", "noch", "so", "fremd", "und", "ec\u00b7kel", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Liebt doch mein hertz/ so viel es lieben kan.", "tokens": ["Liebt", "doch", "mein", "hertz", "/", "so", "viel", "es", "lie\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$(", "ADV", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "La\u00df diesen schlu\u00df dir dein gebl\u00fcte r\u00fchrn", "tokens": ["La\u00df", "die\u00b7sen", "schlu\u00df", "dir", "dein", "ge\u00b7bl\u00fc\u00b7te", "r\u00fchrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PDAT", "NN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und glut aus glut sich wiederum gebiehrn.", "tokens": ["Und", "glut", "aus", "glut", "sich", "wie\u00b7de\u00b7rum", "ge\u00b7biehrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die lieb allein ist wieder liebe werth/", "tokens": ["Die", "lieb", "al\u00b7lein", "ist", "wie\u00b7der", "lie\u00b7be", "werth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VAFIN", "ADV", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und tempel stehn dem offen der sie ehrt.", "tokens": ["Und", "tem\u00b7pel", "stehn", "dem", "of\u00b7fen", "der", "sie", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJD", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}