{"dta.poem.17280": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "130  Von der Eitelkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wend ab dein Angesicht vom glast der Eitelkeit:", "tokens": ["Wend", "ab", "dein", "An\u00b7ge\u00b7sicht", "vom", "glast", "der", "Ei\u00b7tel\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "APPRART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jemehr man jhn beschaut/ jemehr wird man verleitt.", "tokens": ["Je\u00b7mehr", "man", "jhn", "be\u00b7schaut", "/", "je\u00b7mehr", "wird", "man", "ver\u00b7leitt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$(", "ADV", "VAFIN", "PIS", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jedoch kehrs wider hin: denn wer jhn nicht betracht/", "tokens": ["Je\u00b7doch", "kehrs", "wi\u00b7der", "hin", ":", "denn", "wer", "jhn", "nicht", "be\u00b7tracht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKVZ", "PTKVZ", "$.", "KON", "PWS", "PPER", "PTKNEG", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ist schon halb von jhm gef\u00e4llt und umbgebracht.", "tokens": ["Der", "ist", "schon", "halb", "von", "jhm", "ge\u00b7f\u00e4llt", "und", "umb\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "wo