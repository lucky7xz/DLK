{"textgrid.poem.42537": {"metadata": {"author": {"name": "Cronegk, Johann Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Klagt mit mir, ihr stillen Felder!", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klagt mit mir, ihr stillen Felder!", "tokens": ["Klagt", "mit", "mir", ",", "ihr", "stil\u00b7len", "Fel\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klagt mit mir, der Fr\u00fchling flieht!", "tokens": ["Klagt", "mit", "mir", ",", "der", "Fr\u00fch\u00b7ling", "flieht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den T\u00e4lern, bei den Linden", "tokens": ["In", "den", "T\u00e4\u00b7lern", ",", "bei", "den", "Lin\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann kein Zephir Floren finden,", "tokens": ["Kann", "kein", "Ze\u00b7phir", "Flo\u00b7ren", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Er verl\u00e4\u00dft die Rose, die verbl\u00fcht.", "tokens": ["Er", "ver\u00b7l\u00e4\u00dft", "die", "Ro\u00b7se", ",", "die", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Fl\u00fcchtig, gleich den Fr\u00fchlingstagen,", "tokens": ["Fl\u00fcch\u00b7tig", ",", "gleich", "den", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mein ganzes Gl\u00fcck entflohn.", "tokens": ["Ist", "mein", "gan\u00b7zes", "Gl\u00fcck", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch verstummet, sanfte Triebe!", "tokens": ["Doch", "ver\u00b7stum\u00b7met", ",", "sanf\u00b7te", "Trie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blinde Gottheit! falsche Liebe!", "tokens": ["Blin\u00b7de", "Got\u00b7theit", "!", "fal\u00b7sche", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist dann dies der Treue letzter Lohn?", "tokens": ["Ist", "dann", "dies", "der", "Treu\u00b7e", "letz\u00b7ter", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDS", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Lebet wohl, geliebte Herden!", "tokens": ["Le\u00b7bet", "wohl", ",", "ge\u00b7lieb\u00b7te", "Her\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weg mit Kranz und Hirtenstab!", "tokens": ["Weg", "mit", "Kranz", "und", "Hir\u00b7ten\u00b7stab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebet wohl, vergn\u00fcgte Haine!", "tokens": ["Le\u00b7bet", "wohl", ",", "ver\u00b7gn\u00fcg\u00b7te", "Hai\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trostlos, traurig und alleine,", "tokens": ["Trost\u00b7los", ",", "trau\u00b7rig", "und", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Flieht Mirtill und sucht ein fernes Grab.", "tokens": ["Flieht", "Mir\u00b7till", "und", "sucht", "ein", "fer\u00b7nes", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Nymphen, die mit frischen Rosen", "tokens": ["Nym\u00b7phen", ",", "die", "mit", "fri\u00b7schen", "Ro\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oft sein junges Haupt bekr\u00f6nt!", "tokens": ["Oft", "sein", "jun\u00b7ges", "Haupt", "be\u00b7kr\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur bekr\u00e4nzet von Zypressen,", "tokens": ["Nur", "be\u00b7kr\u00e4n\u00b7zet", "von", "Zyp\u00b7res\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Flieht er jetzt und will vergessen,", "tokens": ["Flieht", "er", "jetzt", "und", "will", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sein Lied von Chloris' Lob ert\u00f6nt.", "tokens": ["Da\u00df", "sein", "Lied", "von", "Chlo\u00b7ris'", "Lob", "er\u00b7t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ist noch einer von den Hirten,", "tokens": ["Ist", "noch", "ei\u00b7ner", "von", "den", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der gleich mir ungl\u00fccklich liebt?", "tokens": ["Der", "gleich", "mir", "un\u00b7gl\u00fcck\u00b7lich", "liebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ADJD", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Denket an mich, eine Z\u00e4hre", "tokens": ["Den\u00b7ket", "an", "mich", ",", "ei\u00b7ne", "Z\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Lohn, den ich begehre,", "tokens": ["Ist", "der", "Lohn", ",", "den", "ich", "be\u00b7geh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den mir frommer Treue Wehmut gibt.", "tokens": ["Den", "mir", "from\u00b7mer", "Treu\u00b7e", "Weh\u00b7mut", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Lebet wohl, ihr Sch\u00e4ferinnen,", "tokens": ["Le\u00b7bet", "wohl", ",", "ihr", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denen sonst mein Lied gefiel!", "tokens": ["De\u00b7nen", "sonst", "mein", "Lied", "ge\u00b7fiel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebet wohl, ihr s\u00fc\u00dfe Stunden!", "tokens": ["Le\u00b7bet", "wohl", ",", "ihr", "s\u00fc\u00b7\u00dfe", "Stun\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenige hab' ich empfunden,", "tokens": ["We\u00b7ni\u00b7ge", "hab'", "ich", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Und der traurigen nur allzuviel.", "tokens": ["Und", "der", "trau\u00b7ri\u00b7gen", "nur", "all\u00b7zu\u00b7viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADV", "PIAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Niemand wird mehr an mich denken,", "tokens": ["Nie\u00b7mand", "wird", "mehr", "an", "mich", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als bei Mitternacht vielleicht,", "tokens": ["Als", "bei", "Mit\u00b7ter\u00b7nacht", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann mein Geist beim Mondenscheine,", "tokens": ["Wann", "mein", "Geist", "beim", "Mon\u00b7den\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tief im schauervollen Haine,", "tokens": ["Tief", "im", "schau\u00b7er\u00b7vol\u00b7len", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bla\u00df und traurig durch die B\u00fcsche schleicht.", "tokens": ["Bla\u00df", "und", "trau\u00b7rig", "durch", "die", "B\u00fc\u00b7sche", "schleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Flie\u00dft indessen, traur'ge Stunden,", "tokens": ["Flie\u00dft", "in\u00b7des\u00b7sen", ",", "traur'\u00b7ge", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flie\u00dft in stiller Schwermut hin!", "tokens": ["Flie\u00dft", "in", "stil\u00b7ler", "Schwer\u00b7mut", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werdet dunkler, \u00f6de Haine!", "tokens": ["Wer\u00b7det", "dunk\u00b7ler", ",", "\u00f6\u00b7de", "Hai\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeigt der Welt nicht, da\u00df ich weine", "tokens": ["Zeigt", "der", "Welt", "nicht", ",", "da\u00df", "ich", "wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und noch selbst im Tode z\u00e4rtlich bin!", "tokens": ["Und", "noch", "selbst", "im", "To\u00b7de", "z\u00e4rt\u00b7lich", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPRART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Klagt mit mir, ihr stillen Felder!", "tokens": ["Klagt", "mit", "mir", ",", "ihr", "stil\u00b7len", "Fel\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klagt mit mir, der Fr\u00fchling flieht!", "tokens": ["Klagt", "mit", "mir", ",", "der", "Fr\u00fch\u00b7ling", "flieht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetr