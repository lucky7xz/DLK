{"dta.poem.1249": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "In dieser tunckel-braunen Nacht/", "tokens": ["In", "die\u00b7ser", "tun\u00b7ckel\u00b7brau\u00b7nen", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Furcht und Schrecken um mich wacht/", "tokens": ["Wo", "Furcht", "und", "Schre\u00b7cken", "um", "mich", "wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Leyd und Trauren mich umfangen/", "tokens": ["Wo", "Leyd", "und", "Trau\u00b7ren", "mich", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Frag\u2019 ich Dianen/ wie vielmahl", "tokens": ["Frag'", "ich", "Di\u00b7a\u00b7nen", "/", "wie", "viel\u00b7mahl"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "NE", "$(", "KOKOM", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie seit dem Anfang meiner Qual", "tokens": ["Sie", "seit", "dem", "An\u00b7fang", "mei\u00b7ner", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr gl\u00e4ntzend Silber ausgehangen.", "tokens": ["Ihr", "gl\u00e4nt\u00b7zend", "Sil\u00b7ber", "aus\u00b7ge\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich frage sie/ wie offt ihr Rad/", "tokens": ["Ich", "fra\u00b7ge", "sie", "/", "wie", "offt", "ihr", "Rad", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOKOM", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Celinde/ deine sch\u00f6ne Stadt", "tokens": ["Ce\u00b7lin\u00b7de", "/", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Stadt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seit meinem Scheiden hat beschienen.", "tokens": ["Seit", "mei\u00b7nem", "Schei\u00b7den", "hat", "be\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie offt der m\u00fcden Augen Licht", "tokens": ["Wie", "offt", "der", "m\u00fc\u00b7den", "Au\u00b7gen", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat ihr erbla\u00dftes Angesicht", "tokens": ["Hat", "ihr", "er\u00b7bla\u00df\u00b7tes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bi\u00dfher zur Fackel m\u00fcssen dienen.", "tokens": ["Bi\u00df\u00b7her", "zur", "Fa\u00b7ckel", "m\u00fcs\u00b7sen", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich frage sie/ ob sie nicht wei\u00df/", "tokens": ["Ich", "fra\u00b7ge", "sie", "/", "ob", "sie", "nicht", "wei\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie offt der heissen Thr\u00e4nen Schwei\u00df", "tokens": ["Wie", "offt", "der", "heis\u00b7sen", "Thr\u00e4\u00b7nen", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat meine Wangen \u00fcbergossen.", "tokens": ["Hat", "mei\u00b7ne", "Wan\u00b7gen", "\u00fc\u00b7ber\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich frage sie/ wo ist die Zeit/", "tokens": ["Ich", "fra\u00b7ge", "sie", "/", "wo", "ist", "die", "Zeit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PWAV", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ich Celindens H\u00f6fligkeit", "tokens": ["Da", "ich", "Ce\u00b7lin\u00b7dens", "H\u00f6f\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In s\u00fcsser Gegenwart genossen.", "tokens": ["In", "s\u00fcs\u00b7ser", "Ge\u00b7gen\u00b7wart", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo sind die sch\u00f6nen Stunden hin/", "tokens": ["Wo", "sind", "die", "sch\u00f6\u00b7nen", "Stun\u00b7den", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ihre Freundschafft meinen Sinn", "tokens": ["Da", "ih\u00b7re", "Freund\u00b7schafft", "mei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit klugen Reden hat vergn\u00fcget/", "tokens": ["Mit", "klu\u00b7gen", "Re\u00b7den", "hat", "ver\u00b7gn\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da wir/ doch sonder Feind zu seyn/", "tokens": ["Da", "wir", "/", "doch", "son\u00b7der", "Feind", "zu", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "ADV", "ADJA", "NN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O s\u00fcsse Quelle meiner Pein!", "tokens": ["O", "s\u00fcs\u00b7se", "Quel\u00b7le", "mei\u00b7ner", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Wort und Karten offt gekrieget.", "tokens": ["Mit", "Wort", "und", "Kar\u00b7ten", "offt", "ge\u00b7krie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie bin ich izt so \u00fcbel auff/", "tokens": ["Wie", "bin", "ich", "izt", "so", "\u00fc\u00b7bel", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun meiner Reise strenger Lauff", "tokens": ["Nun", "mei\u00b7ner", "Rei\u00b7se", "stren\u00b7ger", "Lauff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von meinem Arzte mich vertrieben.", "tokens": ["Von", "mei\u00b7nem", "Arz\u00b7te", "mich", "ver\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der blosse Schatten findt sich hier/", "tokens": ["Der", "blos\u00b7se", "Schat\u00b7ten", "findt", "sich", "hier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Geist/ das beste Theil von mir", "tokens": ["Der", "Geist", "/", "das", "bes\u00b7te", "Theil", "von", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist unvermerckt zur\u00fccke blieben.", "tokens": ["Ist", "un\u00b7ver\u00b7merckt", "zu\u00b7r\u00fc\u00b7cke", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Izt f\u00fchl ich erst/ was Scheiden sey/", "tokens": ["Izt", "f\u00fchl", "ich", "erst", "/", "was", "Schei\u00b7den", "sey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "PWS", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit was f\u00fcr Plag und Tiranney", "tokens": ["Mit", "was", "f\u00fcr", "Plag", "und", "Ti\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich mu\u00df ein Hertz von Hertze trennen/", "tokens": ["Sich", "mu\u00df", "ein", "Hertz", "von", "Hert\u00b7ze", "tren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VMFIN", "ART", "NN", "APPR", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo wahre Freundschafft fasset Grund/", "tokens": ["Wo", "wah\u00b7re", "Freund\u00b7schafft", "fas\u00b7set", "Grund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und selbst die Seelen/ nicht der Mund", "tokens": ["Und", "selbst", "die", "See\u00b7len", "/", "nicht", "der", "Mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$(", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Allein/ von reinen Flammen brennen.", "tokens": ["Al\u00b7lein", "/", "von", "rei\u00b7nen", "Flam\u00b7men", "bren\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach Monden dupple deinen Gang/", "tokens": ["Ach", "Mon\u00b7den", "dupp\u00b7le", "dei\u00b7nen", "Gang", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mach uns die Monat nicht so lang/", "tokens": ["Mach", "uns", "die", "Mo\u00b7nat", "nicht", "so", "lang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bi\u00df das bestimmte Ziel erschienen/", "tokens": ["Bi\u00df", "das", "be\u00b7stimm\u00b7te", "Ziel", "er\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich geneigter Sternen Schlu\u00df/", "tokens": ["Und", "mich", "ge\u00b7neig\u00b7ter", "Ster\u00b7nen", "Schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem ich mich unterwerffen mu\u00df/", "tokens": ["Dem", "ich", "mich", "un\u00b7ter\u00b7werf\u00b7fen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Celinden wieder l\u00e4st bedienen.", "tokens": ["Ce\u00b7lin\u00b7den", "wie\u00b7der", "l\u00e4st", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}