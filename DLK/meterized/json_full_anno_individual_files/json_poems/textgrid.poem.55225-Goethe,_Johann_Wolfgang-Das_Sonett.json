{"textgrid.poem.55225": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Das Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sich in erneutem Kunstgebrauch zu \u00fcben", "tokens": ["Sich", "in", "er\u00b7neu\u00b7tem", "Kunst\u00b7ge\u00b7brauch", "zu", "\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist heil'ge Pflicht, die wir dir auferlegen:", "tokens": ["Ist", "heil'\u00b7ge", "Pflicht", ",", "die", "wir", "dir", "auf\u00b7er\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du kannst dich auch, wie wir, bestimmt bewegen", "tokens": ["Du", "kannst", "dich", "auch", ",", "wie", "wir", ",", "be\u00b7stimmt", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "$,", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach Tritt und Schritt, wie es dir vorgeschrieben.", "tokens": ["Nach", "Tritt", "und", "Schritt", ",", "wie", "es", "dir", "vor\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PWAV", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn eben die Beschr\u00e4nkung l\u00e4\u00dft sich lieben,", "tokens": ["Denn", "e\u00b7ben", "die", 