{"dta.poem.2641": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der unsterbliche F\u00f6nix die Seele/  \n Bey Beerdigung Hn. G. v. S. entworffen/  \n den 10. Octobr. 1672.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo sich das Morgen-Licht in ersten Rosen weiset/", "tokens": ["Wo", "sich", "das", "Mor\u00b7gen\u00b7Licht", "in", "ers\u00b7ten", "Ro\u00b7sen", "wei\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weit hinter Indien bl\u00fcht ein geheiligt Wald/", "tokens": ["Weit", "hin\u00b7ter", "In\u00b7di\u00b7en", "bl\u00fcht", "ein", "ge\u00b7hei\u00b7ligt", "Wald", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "VVFIN", "ART", "VVPP", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das sch\u00f6ne Wunder-Thier so man den F\u00f6nix preiset/", "tokens": ["Das", "sch\u00f6\u00b7ne", "Wun\u00b7der\u00b7Thier", "so", "man", "den", "F\u00f6\u00b7nix", "prei\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PIS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat hier sein Vaterland und steten Aufenthalt.", "tokens": ["Hat", "hier", "sein", "Va\u00b7ter\u00b7land", "und", "ste\u00b7ten", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er wohnet hochbegl\u00fcckt als Nachbar bey den Sternen/", "tokens": ["Er", "woh\u00b7net", "hoch\u00b7be\u00b7gl\u00fcckt", "als", "Nach\u00b7bar", "bey", "den", "Ster\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil ihm verj\u00fcngte Krafft ein ewig Leben gibt/", "tokens": ["Weil", "ihm", "ver\u00b7j\u00fcng\u00b7te", "Krafft", "ein", "e\u00b7wig", "Le\u00b7ben", "gibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "ART", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kennt keinen Hunger nicht/ wird sich vom Trauck entfernen/", "tokens": ["Kennt", "kei\u00b7nen", "Hun\u00b7ger", "nicht", "/", "wird", "sich", "vom", "Trauck", "ent\u00b7fer\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKNEG", "$(", "VAFIN", "PRF", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Indem ihm eintzig nur der Sonnen-Glantz beliebt.", "tokens": ["In\u00b7dem", "ihm", "eint\u00b7zig", "nur", "der", "Son\u00b7nen\u00b7Glantz", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Augen gl\u00fcend Blitz streut gleichsam lichte Funcken/", "tokens": ["Der", "Au\u00b7gen", "gl\u00fcend", "Blitz", "streut", "gleich\u00b7sam", "lich\u00b7te", "Fun\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "ADJD", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Es spitzt sich als ein Stern der Kamm auf seinem Haupt/", "tokens": ["Es", "spitzt", "sich", "als", "ein", "Stern", "der", "Kamm", "auf", "sei\u00b7nem", "Haupt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOUS", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Beine scheinen wie vom Blut der Schnecken truncken/", "tokens": ["Die", "Bei\u00b7ne", "schei\u00b7nen", "wie", "vom", "Blut", "der", "Schne\u00b7cken", "trun\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "APPRART", "NN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Federn Himmel-blau mit Golde reich belaubt.", "tokens": ["Die", "Fe\u00b7dern", "Him\u00b7mel\u00b7blau", "mit", "Gol\u00b7de", "reich", "be\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein g\u00fcldner Zirckel flicht sich umb de\u00df Halses Gr\u00e4ntzen/", "tokens": ["Ein", "g\u00fcld\u00b7ner", "Zir\u00b7ckel", "flicht", "sich", "umb", "de\u00df", "Hal\u00b7ses", "Gr\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Des R\u00fcckens stoltze Pracht sticht auch den Purpur hin/", "tokens": ["Des", "R\u00fc\u00b7ckens", "stolt\u00b7ze", "Pracht", "sticht", "auch", "den", "Pur\u00b7pur", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ja was mehr wunderns werth/ er kan sich seibst erg\u00e4ntzen/", "tokens": ["Ja", "was", "mehr", "wun\u00b7derns", "werth", "/", "er", "kan", "sich", "seibst", "er\u00b7g\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PRELS", "PIAT", "NN", "ADJD", "$(", "PPER", "VMFIN", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und wird au\u00df eigner Asch am sch\u00f6nsten wieder bl\u00fchn.", "tokens": ["Und", "wird", "au\u00df", "eig\u00b7ner", "Asch", "am", "sch\u00f6ns\u00b7ten", "wie\u00b7der", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "APPRART", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der/ wenn nun tausend Jahr in ihrem Lauff verflossen/", "tokens": ["Der", "/", "wenn", "nun", "tau\u00b7send", "Jahr", "in", "ih\u00b7rem", "Lauff", "ver\u00b7flos\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "ADV", "CARD", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nimmt gleich dem Monden ab an Kr\u00e4fften und Gestalt/", "tokens": ["Nimmt", "gleich", "dem", "Mon\u00b7den", "ab", "an", "Kr\u00e4ff\u00b7ten", "und", "Ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Augen hell Crystall starrt als vom Frost geschlossen/", "tokens": ["Der", "Au\u00b7gen", "hell", "Crys\u00b7tall", "starrt", "als", "vom", "Frost", "ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "KOKOM", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es wanckt der gantze Leib wie Fichten die sehr alt.", "tokens": ["Es", "wanckt", "der", "gant\u00b7ze", "Leib", "wie", "Fich\u00b7ten", "die", "sehr", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KOKOM", "NN", "ART", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Denn denckt er auf sein Grab/ und wieder neue Wiegen/", "tokens": ["Denn", "denckt", "er", "auf", "sein", "Grab", "/", "und", "wie\u00b7der", "neu\u00b7e", "Wie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$(", "KON", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er baut ihm selbst die Baar von Zimmet-Stauden auf/", "tokens": ["Er", "baut", "ihm", "selbst", "die", "Baar", "von", "Zim\u00b7met\u00b7Stau\u00b7den", "auf", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er tr\u00e4gt/ in solchem Nest bequem und wol zu liegen", "tokens": ["Er", "tr\u00e4gt", "/", "in", "sol\u00b7chem", "Nest", "be\u00b7quem", "und", "wol", "zu", "lie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "APPR", "PIAT", "NN", "ADJD", "KON", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Amom und Narden zu/ und streut viel Kr\u00e4uter drauff.", "tokens": ["A\u00b7mom", "und", "Nar\u00b7den", "zu", "/", "und", "streut", "viel", "Kr\u00e4u\u00b7ter", "drauff", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "PTKZU", "$(", "KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Denn rufft er mit Gesang der Sonnen heisse Stralen/", "tokens": ["Denn", "rufft", "er", "mit", "Ge\u00b7sang", "der", "Son\u00b7nen", "heis\u00b7se", "Stra\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sich und auch seinen Sarch zu stecken in den Brand/", "tokens": ["Sich", "und", "auch", "sei\u00b7nen", "Sarch", "zu", "ste\u00b7cken", "in", "den", "Brand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wirfft so sein Alter weg/ de\u00df Leibes d\u00fcrre Schalen/", "tokens": ["Wirfft", "so", "sein", "Al\u00b7ter", "weg", "/", "de\u00df", "Lei\u00b7bes", "d\u00fcr\u00b7re", "Scha\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sucht mitten in dem Tod de\u00df Lebens neuen Stand.", "tokens": ["Sucht", "mit\u00b7ten", "in", "dem", "Tod", "de\u00df", "Le\u00b7bens", "neu\u00b7en", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Hier m\u00fcht sich die Natur/ da\u00df nicht ihr Schatz verderbe/", "tokens": ["Hier", "m\u00fcht", "sich", "die", "Na\u00b7tur", "/", "da\u00df", "nicht", "ihr", "Schatz", "ver\u00b7der\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$(", "KOUS", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sie mahnt die Flammen an/ da\u00df sie nicht sch\u00e4dlich seyn/", "tokens": ["Sie", "mahnt", "die", "Flam\u00b7men", "an", "/", "da\u00df", "sie", "nicht", "sch\u00e4d\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$(", "KOUS", "PPER", "PTKNEG", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df nicht ihr Trost und Ruhm der edle Vogel sterbe/", "tokens": ["Da\u00df", "nicht", "ihr", "Trost", "und", "Ruhm", "der", "ed\u00b7le", "Vo\u00b7gel", "ster\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So geust sie neue Krafft den zarten Gliedern ein.", "tokens": ["So", "geust", "sie", "neu\u00b7e", "Krafft", "den", "zar\u00b7ten", "Glie\u00b7dern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Asche f\u00e4ngt sich an wie lebendig zu r\u00fchren/", "tokens": ["Die", "A\u00b7sche", "f\u00e4ngt", "sich", "an", "wie", "le\u00b7ben\u00b7dig", "zu", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "KOKOM", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Das Grab weicht der Geburt/ was vor gestorben/ lebt.", "tokens": ["Das", "Grab", "weicht", "der", "Ge\u00b7burt", "/", "was", "vor", "ge\u00b7stor\u00b7ben", "/", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "PWS", "APPR", "VVPP", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es ist nach kurtzer Zeit der F\u00f6nix neu zu sp\u00fcren/", "tokens": ["Es", "ist", "nach", "kurt\u00b7zer", "Zeit", "der", "F\u00f6\u00b7nix", "neu", "zu", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Den gleicher Farbe Glantz und Herrligkeit erhebt.", "tokens": ["Den", "glei\u00b7cher", "Far\u00b7be", "Glantz", "und", "Herr\u00b7lig\u00b7keit", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Bild der Unsterbligkeit! Und Abri\u00df unsrer Seele!", "tokens": ["Bild", "der", "U\u00b7nster\u00b7blig\u00b7keit", "!", "Und", "Ab\u00b7ri\u00df", "uns\u00b7rer", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.38": {"text": "So sich dem F\u00f6nix gleich in vielen St\u00fccken zeigt/", "tokens": ["So", "sich", "dem", "F\u00f6\u00b7nix", "gleich", "in", "vie\u00b7len", "St\u00fc\u00b7cken", "zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die/ ob sie zwar umbringt noch in des Leibes H\u00f6le/", "tokens": ["Die", "/", "ob", "sie", "zwar", "um\u00b7bringt", "noch", "in", "des", "Lei\u00b7bes", "H\u00f6\u00b7le", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "PPER", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch ihrem Ursprung nach siets zu dem Himmel steigt.", "tokens": ["Doch", "ih\u00b7rem", "Ur\u00b7sprung", "nach", "siets", "zu", "d