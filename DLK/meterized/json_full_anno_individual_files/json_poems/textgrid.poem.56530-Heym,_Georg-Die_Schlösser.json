{"textgrid.poem.56530": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Schl\u00f6sser", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alt von Blute, und manches im toten Munde", "tokens": ["Alt", "von", "Blu\u00b7te", ",", "und", "man\u00b7ches", "im", "to\u00b7ten", "Mun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "$,", "KON", "PIS", "APPRART", "ADJA", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Kauen sie Dunkel. \u2013 Wo gro\u00dfe Schwerter geblitzt.", "tokens": ["Kau\u00b7en", "sie", "Dun\u00b7kel", ".", "\u2013", "Wo", "gro\u00b7\u00dfe", "Schwer\u00b7ter", "ge\u00b7blitzt", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$.", "$(", "PWAV", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Tr\u00fcbe Gelage zur Nacht in der K\u00f6nige Runde \u2013", "tokens": ["Tr\u00fc\u00b7be", "Ge\u00b7la\u00b7ge", "zur", "Nacht", "in", "der", "K\u00f6\u00b7ni\u00b7ge", "Run\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "APPR", "ART", "NN", "NN", "$("], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.4": {"text": "Drau\u00dfen die Sonne die sp\u00e4ten Pfeile noch spritzt.", "tokens": ["Drau\u00b7\u00dfen", "die", "Son\u00b7ne", "die", "sp\u00e4\u00b7ten", "Pfei\u00b7le", "noch", "spritzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza