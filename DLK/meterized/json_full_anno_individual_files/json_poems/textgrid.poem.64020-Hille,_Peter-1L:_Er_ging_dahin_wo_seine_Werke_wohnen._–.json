{"textgrid.poem.64020": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er ging dahin wo seine Werke wohnen. \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ging dahin wo seine Werke wohnen. \u2013", "tokens": ["Er", "ging", "da\u00b7hin", "wo", "sei\u00b7ne", "Wer\u00b7ke", "woh\u00b7nen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PWAV", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit anget\u00fcrmtem Nacken ihm zur Seiten trabt der Eroberer.", "tokens": ["Mit", "an\u00b7ge\u00b7t\u00fcrm\u00b7tem", "Na\u00b7cken", "ihm", "zur", "Sei\u00b7ten", "trabt", "der", "Er\u00b7o\u00b7be\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+--+--", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Aus tiefem Sande grinsen fremde Zeichen:", "tokens": ["Aus", "tie\u00b7fem", "San\u00b7de", "grin\u00b7sen", "frem\u00b7de", "Zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gebeine sind es, die so leuchtend bleichen.", "tokens": ["Ge\u00b7bei\u00b7ne", "sind", "es", ",", "die", "so", "leuch\u00b7tend", "blei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vor rohen Hufen knirscht die hei\u00dfe W\u00fcste;", "tokens": ["Vor", "ro\u00b7hen", "Hu\u00b7fen", "knirscht", "die", "hei\u00b7\u00dfe", "W\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gr\u00fcn steigt ein H\u00fcgel auf und ruht", "tokens": ["Gr\u00fcn", "steigt", "ein", "H\u00fc\u00b7gel", "auf", "und", "ruht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "In Blumenk\u00fchle aus vom hei\u00dfen Glei\u00dfen.", "tokens": ["In", "Blu\u00b7men\u00b7k\u00fch\u00b7le", "aus", "vom", "hei\u00b7\u00dfen", "Glei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In tr\u00e4ger Schr\u00e4ge ruht ein alter Faun", "tokens": ["In", "tr\u00e4\u00b7ger", "Schr\u00e4\u00b7ge", "ruht", "ein", "al\u00b7ter", "Faun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und glotzt in Weiten, die wie bald verloren ihm,", "tokens": ["Und", "glotzt", "in", "Wei\u00b7ten", ",", "die", "wie", "bald", "ver\u00b7lo\u00b7ren", "ihm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "PRELS", "KOKOM", "ADV", "VVPP", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit schwerem Auge, fremdbek\u00fcmmert.", "tokens": ["Mit", "schwe\u00b7rem", "Au\u00b7ge", ",", "fremd\u00b7be\u00b7k\u00fcm\u00b7mert", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ein F\u00e4unlein, goldnes Stroh im roten Nacken,", "tokens": ["Ein", "F\u00e4un\u00b7lein", ",", "gold\u00b7nes", "Stroh", "im", "ro\u00b7ten", "Na\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Reckt tief zum Quell die drallen B\u00e4cklein nieder.", "tokens": ["Reckt", "tief", "zum", "Quell", "die", "dral\u00b7len", "B\u00e4c\u00b7klein", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Genug gesehn! Ich will mir selber lauschen;", "tokens": ["Ge\u00b7nug", "ge\u00b7sehn", "!", "Ich", "will", "mir", "sel\u00b7ber", "lau\u00b7schen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da kommt ein Wald, der soll mir rauschen!", "tokens": ["Da", "kommt", "ein", "Wald", ",", "der", "soll", "mir", "rau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie klopft des Mittags Angst! \u2013", "tokens": ["Wie", "klopft", "des", "Mit\u00b7tags", "Angst", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gescheckt, erschreckt", "tokens": ["Ge\u00b7scheckt", ",", "er\u00b7schreckt"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$,", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die starren, steilen St\u00e4mme.", "tokens": ["Die", "star\u00b7ren", ",", "stei\u00b7len", "St\u00e4m\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": " hoch und t\u00fcckisch,", "tokens": ["hoch", "und", "t\u00fc\u00b7ckisch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Das seltsam b\u00f6sgedrehte Horn voraus:", "tokens": ["Das", "selt\u00b7sam", "b\u00f6s\u00b7ge\u00b7dreh\u00b7te", "Horn", "vo\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das Einhorn ...", "tokens": ["Das", "Ein\u00b7horn", "..."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": " sinnig-wild", "tokens": ["sin\u00b7nig\u00b7wild"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Aufblickt des M\u00e4rchens \u00fcppig-fremdes Auge. \u2013", "tokens": ["Auf\u00b7blickt", "des", "M\u00e4r\u00b7chens", "\u00fcp\u00b7pig\u00b7frem\u00b7des", "Au\u00b7ge", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da von der Rechten schwellend atmet's Raum,", "tokens": ["Da", "von", "der", "Rech\u00b7ten", "schwel\u00b7lend", "at\u00b7met's", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hebt gr\u00fcne Wipfel hoch noch \u00fcber die blauen", "tokens": ["Hebt", "gr\u00fc\u00b7ne", "Wip\u00b7fel", "hoch", "noch", "\u00fc\u00b7ber", "die", "blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "ADJD", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und bietet Erde, bietet Himmel, Str\u00e4u\u00dfe, Schaum", "tokens": ["Und", "bie\u00b7tet", "Er\u00b7de", ",", "bie\u00b7tet", "Him\u00b7mel", ",", "Str\u00e4u\u00b7\u00dfe", ",", "Schaum"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "NN", "$,", "VVFIN", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schl\u00e4gt lustkreisend einen Purzelbaum:", "tokens": ["Und", "schl\u00e4gt", "lust\u00b7krei\u00b7send", "ei\u00b7nen", "Pur\u00b7zel\u00b7baum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und blickt wie Angst, wie Trauer der Unendlichkeit,", "tokens": ["Und", "blickt", "wie", "Angst", ",", "wie", "Trau\u00b7er", "der", "Un\u00b7end\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "NN", "$,", "PWAV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie Irrsinn, wie wehlachend Spotten:", "tokens": ["Wie", "Irr\u00b7sinn", ",", "wie", "weh\u00b7la\u00b7chend", "Spot\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PWAV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das wilde Element! \u2013", "tokens": ["Das", "wil\u00b7de", "E\u00b7le\u00b7ment", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und Abend wird's; das Meer ging ferne schlafen.", "tokens": ["Und", "A\u00b7bend", "wird's", ";", "das", "Meer", "ging", "fer\u00b7ne", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$.", "ART", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein braunes Gl\u00f6ckelh\u00e4uslein.", "tokens": ["Ein", "brau\u00b7nes", "Gl\u00f6\u00b7ckel\u00b7h\u00e4us\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Da steht, geneigt", "tokens": ["Da", "steht", ",", "ge\u00b7neigt"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Das wei\u00dfe, stille Haupt, der braune M\u00f6nch und", "tokens": ["Das", "wei\u00b7\u00dfe", ",", "stil\u00b7le", "Haupt", ",", "der", "brau\u00b7ne", "M\u00f6nch", "und"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Geigt und streut wie Blumen nieder", "tokens": ["Geigt", "und", "streut", "wie", "Blu\u00b7men", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN", "KOKOM", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Zu F\u00fc\u00dfen der Maria sp\u00e4te Glut. \u2013", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "der", "Ma\u00b7ria", "sp\u00e4\u00b7te", "Glut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ART", "NE", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Auf Zehen, seine Wangen voll und fromm,", "tokens": ["Auf", "Ze\u00b7hen", ",", "sei\u00b7ne", "Wan\u00b7gen", "voll", "und", "fromm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "$,", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ein B\u00fcblein lugt; leis zittert seiner Schwinge", "tokens": ["Ein", "B\u00fcb\u00b7lein", "lugt", ";", "leis", "zit\u00b7tert", "sei\u00b7ner", "Schwin\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "blaugr\u00fcner Reif ...", "tokens": ["blau\u00b7gr\u00fc\u00b7ner", "Reif", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Er ging dahin, wo seine Werke wohnen;", "tokens": ["Er", "ging", "da\u00b7hin", ",", "wo", "sei\u00b7ne", "Wer\u00b7ke", "woh\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie leuchten hei\u00dfer auf in ihrer Seele Saft,", "tokens": ["Sie", "leuch\u00b7ten", "hei\u00b7\u00dfer", "auf", "in", "ih\u00b7rer", "See\u00b7le", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Urgeburten dieses gro\u00dfen Lebens!", "tokens": ["Die", "Ur\u00b7ge\u00b7bur\u00b7ten", "die\u00b7ses", "gro\u00b7\u00dfen", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein frohes Tosen wiehert der Stromsturz", "tokens": ["Ein", "fro\u00b7hes", "To\u00b7sen", "wie\u00b7hert", "der", "Strom\u00b7sturz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nieder; die W\u00e4lder \u00f6ffnen atmend", "tokens": ["Nie\u00b7der", ";", "die", "W\u00e4l\u00b7der", "\u00f6ff\u00b7nen", "at\u00b7mend"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ART", "NN", "VVINF", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Befreite Brust.", "tokens": ["Be\u00b7frei\u00b7te", "Brust", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Die gro\u00dfen stummen Seelen bitten", "tokens": ["Die", "gro\u00b7\u00dfen", "stum\u00b7men", "See\u00b7len", "bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der ungeheuren Dinge und der wilden Welt:", "tokens": ["Der", "un\u00b7ge\u00b7heu\u00b7ren", "Din\u00b7ge", "und", "der", "wil\u00b7den", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u00bbdu bist nun da; so l\u00f6se uns die Lippen;", "tokens": ["\u00bb", "du", "bist", "nun", "da", ";", "so", "l\u00f6\u00b7se", "uns", "die", "Lip\u00b7pen", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du wei\u00dft uns alle tr\u00e4umen unser Brausen!", "tokens": ["Du", "wei\u00dft", "uns", "al\u00b7le", "tr\u00e4u\u00b7men", "un\u00b7ser", "Brau\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Lebens Wein in heitrer Andacht trinkst", "tokens": ["Des", "Le\u00b7bens", "Wein", "in", "hei\u00b7trer", "An\u00b7dacht", "trinkst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du pr\u00fcfend und bei hohem L\u00e4cheln neigt", "tokens": ["Du", "pr\u00fc\u00b7fend", "und", "bei", "ho\u00b7hem", "L\u00e4\u00b7cheln", "neigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sich leicht dein Manneshaupt, da dir Freund Hein", "tokens": ["Sich", "leicht", "dein", "Man\u00b7nes\u00b7haupt", ",", "da", "dir", "Freund", "H