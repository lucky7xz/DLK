{"dta.poem.23781": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Schmetterling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "O seht den bunten Schmetterling,", "tokens": ["O", "seht", "den", "bun\u00b7ten", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welch gl\u00e4nzend allerliebstes Ding!", "tokens": ["Welch", "gl\u00e4n\u00b7zend", "al\u00b7ler\u00b7liebs\u00b7tes", "Ding", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ist ihm doch geschehen!", "tokens": ["Wie", "ist", "ihm", "doch", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als ich ihn k\u00fcrzlich noch gesehen,", "tokens": ["Als", "ich", "ihn", "k\u00fcrz\u00b7lich", "noch", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "War es ein kriechend garstges Thier,", "tokens": ["War", "es", "ein", "krie\u00b7chend", "garst\u00b7ges", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur Ekel macht es mir.", "tokens": ["Nur", "E\u00b7kel", "macht", "es", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}