{"dta.poem.23628": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Steuerverweigerungsverfassungsm\u00e4\u00dfig-  \n berechtigte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sprecht von Volks- und Menschenrechten,", "tokens": ["Sprecht", "von", "Volks", "und", "Men\u00b7schen\u00b7rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "'s ist doch eitel was ihr sprecht!", "tokens": ["'s", "ist", "doch", "ei\u00b7tel", "was", "ihr", "sprecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr erlangt mit allem Fechten", "tokens": ["Ihr", "er\u00b7langt", "mit", "al\u00b7lem", "Fech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weder Schreib- noch Rederecht.", "tokens": ["We\u00b7der", "Schreib", "noch", "Re\u00b7de\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sprecht zu hunderttausend Malen", "tokens": ["Sprecht", "zu", "hun\u00b7dert\u00b7tau\u00b7send", "Ma\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer nein, und nein, ja nein:", "tokens": ["Im\u00b7mer", "nein", ",", "und", "nein", ",", "ja", "nein", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKANT", "$,", "KON", "PTKANT", "$,", "ADV", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eure Steuern m\u00fcsst ihr zahlen!", "tokens": ["Eu\u00b7re", "Steu\u00b7ern", "m\u00fcsst", "ihr", "zah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ist euer Recht allein.", "tokens": ["Das", "ist", "eu\u00b7er", "Recht", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}