{"textgrid.poem.53813": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Das alte Vertiko", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Haus, in unsrer guten Stube,", "tokens": ["Zu", "Haus", ",", "in", "uns\u00b7rer", "gu\u00b7ten", "Stu\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da stand, gleich neben dem Tr\u00fcmoh,", "tokens": ["da", "stand", ",", "gleich", "ne\u00b7ben", "dem", "Tr\u00fc\u00b7moh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "mit einem Griff an jedem Schube", "tokens": ["mit", "ei\u00b7nem", "Griff", "an", "je\u00b7dem", "Schu\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ein altes braunes Vertiko.", "tokens": ["ein", "al\u00b7tes", "brau\u00b7nes", "Ver\u00b7ti\u00b7ko", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es war verziert und reich gedrechselt", "tokens": ["Es", "war", "ver\u00b7ziert", "und", "reich", "ge\u00b7drech\u00b7selt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "mit Kn\u00f6pfen, K\u00f6pfen weit und breit;", "tokens": ["mit", "Kn\u00f6p\u00b7fen", ",", "K\u00f6p\u00b7fen", "weit", "und", "breit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "den Stil hat niemand nicht verwechselt:", "tokens": ["den", "Stil", "hat", "nie\u00b7mand", "nicht", "ver\u00b7wech\u00b7selt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Di\u00df war noch aus der Muschelzeit.", "tokens": ["Di\u00df", "war", "noch", "aus", "der", "Mu\u00b7schel\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir schiens ein Sinnbild unsres Lebens.", "tokens": ["Mir", "schiens", "ein", "Sinn\u00b7bild", "uns\u00b7res", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So kam zu mir in jungem Jahr,", "tokens": ["So", "kam", "zu", "mir", "in", "jun\u00b7gem", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "leicht schielend, aber nie vergebens,", "tokens": ["leicht", "schie\u00b7lend", ",", "a\u00b7ber", "nie", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ein M\u00e4dchen sch\u00f6n und wunderbar.", "tokens": ["ein