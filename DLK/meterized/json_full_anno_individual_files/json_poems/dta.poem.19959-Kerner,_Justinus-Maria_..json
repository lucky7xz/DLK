{"dta.poem.19959": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Maria .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da sitzet sie mit andern Blumen spielend", "tokens": ["Da", "sit\u00b7zet", "sie", "mit", "an\u00b7dern", "Blu\u00b7men", "spie\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Knospe der Rose,", "tokens": ["Knos\u00b7pe", "der", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Noch nicht den Strahl der Gottheit in sich f\u00fchlend,", "tokens": ["Noch", "nicht", "den", "Strahl", "der", "Got\u00b7theit", "in", "sich", "f\u00fch\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "ART", "NN", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der bald des Himmels F\u00fcll' ihr weckt im Schoo\u00dfe,", "tokens": ["Der", "bald", "des", "Him\u00b7mels", "F\u00fcll'", "ihr", "weckt", "im", "Schoo\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch ahnet's schon das L\u00e4mmlein, das sie liebt,", "tokens": ["Doch", "ah\u00b7net's", "schon", "das", "L\u00e4mm\u00b7lein", ",", "das", "sie", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Blickt s\u00fc\u00df betr\u00fcbt,", "tokens": ["Blickt", "s\u00fc\u00df", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Blume ahnet's, die sie tr\u00e4gt am Herzen,", "tokens": ["Die", "Blu\u00b7me", "ah\u00b7net's", ",", "die", "sie", "tr\u00e4gt", "am", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verbl\u00fchet schnell in wonniglichen Schmerzen.", "tokens": ["Ver\u00b7bl\u00fc\u00b7het", "schnell", "in", "won\u00b7nig\u00b7li\u00b7chen", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bald aber sinkt auf strahlendem Gefieder", "tokens": ["Bald", "a\u00b7ber", "sinkt", "auf", "strah\u00b7len\u00b7dem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Engel sich herab, o sel'ge Stunde!", "tokens": ["Der", "En\u00b7gel", "sich", "her\u00b7ab", ",", "o", "sel'\u00b7ge", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bringt ihr die Kunde.", "tokens": ["Bringt", "ihr", "die", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und betend sinkt die Gottgeweihte nieder.", "tokens": ["Und", "be\u00b7tend", "sinkt", "die", "Gott\u00b7ge\u00b7weih\u00b7te", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Strahl des Himmels z\u00fckt durch ihre Glieder,", "tokens": ["Ein", "Strahl", "des", "Him\u00b7mels", "z\u00fckt", "durch", "ih\u00b7re", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Knospe reift zur Paradiesesf\u00fclle,", "tokens": ["Die", "Knos\u00b7pe", "reift", "zur", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie doch erhebet sich in Demuth wieder.", "tokens": ["Sie", "doch", "er\u00b7he\u00b7bet", "sich", "in", "De\u00b7muth", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u201eich bin die Magd, Herr! es gescheh' dein Wille!\u201c", "tokens": ["\u201e", "ich", "bin", "die", "Magd", ",", "Herr", "!", "es", "ge\u00b7scheh'", "dein", "Wil\u00b7le", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}}}}}