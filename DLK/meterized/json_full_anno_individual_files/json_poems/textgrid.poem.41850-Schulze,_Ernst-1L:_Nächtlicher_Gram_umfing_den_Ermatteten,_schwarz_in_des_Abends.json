{"textgrid.poem.41850": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: N\u00e4chtlicher Gram umfing den Ermatteten, schwarz in des Abends", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chtlicher Gram umfing den Ermatteten, schwarz in des Abends", "tokens": ["N\u00e4cht\u00b7li\u00b7cher", "Gram", "um\u00b7fing", "den", "Er\u00b7mat\u00b7te\u00b7ten", ",", "schwarz", "in", "des", "A\u00b7bends"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Nebel geh\u00fcllt, und dumpf schwieg das erstorbene Herz.", "tokens": ["Ne\u00b7bel", "ge\u00b7h\u00fcllt", ",", "und", "dumpf", "schwieg", "das", "ers\u00b7tor\u00b7be\u00b7ne", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Thorheit schien und Wahn mir das Heilige, nichtig des Lebens", "tokens": ["Thor\u00b7heit", "schien", "und", "Wahn", "mir", "das", "Hei\u00b7li\u00b7ge", ",", "nich\u00b7tig", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN", "PPER", "ART", "ADJA", "$,", "ADJD", "ART", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Gaukelgefild, und so klagte der d\u00fcstere Geist:", "tokens": ["Gau\u00b7kel\u00b7ge\u00b7fild", ",", "und", "so", "klag\u00b7te", "der", "d\u00fcs\u00b7te\u00b7re", "Geist", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Fruchtlos hast du gespielt und getr\u00e4umt! Stets bl\u00fchte der Hoffnung", "tokens": ["Frucht\u00b7los", "hast", "du", "ge\u00b7spielt", "und", "ge\u00b7tr\u00e4umt", "!", "Stets", "bl\u00fch\u00b7te", "der", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "KON", "VVPP", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Ueppiger Baum: doch nie reifte die labende Frucht.", "tokens": ["Uep\u00b7pi\u00b7ger", "Baum", ":", "doch", "nie", "reif\u00b7te", "die", "la\u00b7ben\u00b7de", "Frucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Gl\u00e4nzend erhob sich ein herrliches Ziel dem begeisterten J\u00fcngling:", "tokens": ["Gl\u00e4n\u00b7zend", "er\u00b7hob", "sich", "ein", "herr\u00b7li\u00b7ches", "Ziel", "dem", "be\u00b7geis\u00b7ter\u00b7ten", "J\u00fcng\u00b7ling", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Aber der Zufall nur lenkt die entg\u00f6tterte Welt.", "tokens": ["A\u00b7ber", "der", "Zu\u00b7fall", "nur", "lenkt", "die", "ent\u00b7g\u00f6t\u00b7ter\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Sehnsucht d\u00e4mmerte dir, und der Lieb' aufstrahlende Sonne", "tokens": ["Sehn\u00b7sucht", "d\u00e4m\u00b7mer\u00b7te", "dir", ",", "und", "der", "Lieb'", "auf\u00b7strah\u00b7len\u00b7de", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Go\u00df jungfr\u00e4ulichen Reiz \u00fcber den Traum des Gef\u00fchls.", "tokens": ["Go\u00df", "jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Reiz", "\u00fc\u00b7ber", "den", "Traum", "des", "Ge\u00b7f\u00fchls", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Doch l\u00e4ngst sank der erloschene Strahl, und schw\u00e4rzere Nacht nur", "tokens": ["Doch", "l\u00e4ngst", "sank", "der", "er\u00b7lo\u00b7sche\u00b7ne", "Strahl", ",", "und", "schw\u00e4r\u00b7ze\u00b7re", "Nacht", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "ADV"], "meter": "-++--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "F\u00fcllt, je heller er einst leuchtete, jetzt dir das Herz.", "tokens": ["F\u00fcllt", ",", "je", "hel\u00b7ler", "er", "einst", "leuch\u00b7te\u00b7te", ",", "jetzt", "dir", "das", "Herz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ADJD", "PPER", "ADV", "VVFIN", "$,", "ADV", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "Was du ges\u00e4't, wird rauben der Sturm! Was k\u00e4mpfst du vergebens", "tokens": ["Was", "du", "ge\u00b7s\u00e4't", ",", "wird", "rau\u00b7ben", "der", "Sturm", "!", "Was", "k\u00e4mpfst", "du", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "VVINF", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Gegen des Schicksals Spott? Ewig ist einzig der Tod!", "tokens": ["Ge\u00b7gen", "des", "Schick\u00b7sals", "Spott", "?", "E\u00b7wig", "ist", "ein\u00b7zig", "der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$.", "ADJD", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "+--+--+-++--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Eins sind Grab und Wiege f\u00fcr dich; in des nichtigen Lebens", "tokens": ["Eins", "sind", "Grab", "und", "Wie\u00b7ge", "f\u00fcr", "dich", ";", "in", "des", "nich\u00b7ti\u00b7gen", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "APPR", "PPER", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Zwecklos t\u00e4ndelndem Spiel ringst du, ein Nichts mit dem Nichts!", "tokens": ["Zweck\u00b7los", "t\u00e4n\u00b7deln\u00b7dem", "Spiel", "ringst", "du", ",", "ein", "Nichts", "mit", "dem", "Nichts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "PPER", "$,", "ART", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Also z\u00fcrnet' ich mir und der Welt; doch kalte Verachtung", "tokens": ["Al\u00b7so", "z\u00fcr\u00b7net'", "ich", "mir", "und", "der", "Welt", ";", "doch", "kal\u00b7te", "Ver\u00b7ach\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KON", "ART", "NN", "$.", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Tilgte den Zorn, und laut lacht' ich im bitteren Hohn.", "tokens": ["Tilg\u00b7te", "den", "Zorn", ",", "und", "laut", "lacht'", "ich", "im", "bit\u00b7te\u00b7ren", "Hohn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Horch, da schwamm, gleich lindem Ged\u00fcft, auf der sinkenden D\u00e4mmrung", "tokens": ["Horch", ",", "da", "schwamm", ",", "gleich", "lin\u00b7dem", "Ge\u00b7d\u00fcft", ",", "auf", "der", "sin\u00b7ken\u00b7den", "D\u00e4mm\u00b7rung"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "$,", "ADV", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Friedlichem Hauch leicht aufwogend ein lieblicher Ton;", "tokens": ["Fried\u00b7li\u00b7chem", "Hauch", "leicht", "auf\u00b7wo\u00b7gend", "ein", "lieb\u00b7li\u00b7cher", "Ton", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.21": {"text": "Schwellend verkettete bald sich das zitternde Gold, bald rann es", "tokens": ["Schwel\u00b7lend", "ver\u00b7ket\u00b7te\u00b7te", "bald", "sich", "das", "zit\u00b7tern\u00b7de", "Gold", ",", "bald", "rann", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "PRF", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.22": {"text": "Sanfthinschmelzend und oft leise verhallend daher,", "tokens": ["Sanft\u00b7hinsc\u00b7hmel\u00b7zend", "und", "oft", "lei\u00b7se", "ver\u00b7hal\u00b7lend", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "ADJD", "VVPP", "PAV", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Und s\u00fc\u00df wallte Gesang auf des Tons leichtflatternder Schwinge", "tokens": ["Und", "s\u00fc\u00df", "wall\u00b7te", "Ge\u00b7sang", "auf", "des", "Tons", "leicht\u00b7flat\u00b7tern\u00b7der", "Schwin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Tr\u00f6stend, gleich dem Gespr\u00e4ch freundlicher Engel, heran.", "tokens": ["Tr\u00f6s\u00b7tend", ",", "gleich", "dem", "Ge\u00b7spr\u00e4ch", "freund\u00b7li\u00b7cher", "En\u00b7gel", ",", "he\u00b7ran", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ART", "NN", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Ach, da regte sich still das erkaltete Herz, von des Wohllauts", "tokens": ["Ach", ",", "da", "reg\u00b7te", "sich", "still", "das", "er\u00b7kal\u00b7te\u00b7te", "Herz", ",", "von", "des", "Wohl\u00b7lauts"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Athem erw\u00e4rmt, und hell tagte die dunkele Welt.", "tokens": ["A\u00b7them", "er\u00b7w\u00e4rmt", ",", "und", "hell", "tag\u00b7te", "die", "dun\u00b7ke\u00b7le", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.27": {"text": "Bilder umgaukelten mich, s\u00fc\u00dfschmeichelnde, zarte Gestalten,", "tokens": ["Bil\u00b7der", "um\u00b7gau\u00b7kel\u00b7ten", "mich", ",", "s\u00fc\u00df\u00b7schmei\u00b7cheln\u00b7de", ",", "zar\u00b7te", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+---++--+--+-", "measure": "dactylic.di.plus"}, "line.28": {"text": "Und in lebende Form schmiegte sich jedes Gef\u00fchl;", "tokens": ["Und", "in", "le\u00b7ben\u00b7de", "Form", "schmieg\u00b7te", "sich", "je\u00b7des", "Ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Dich nur nannte mir jedes Gef\u00fchl, und jeglichem Traum lieh", "tokens": ["Dich", "nur", "nann\u00b7te", "mir", "je\u00b7des", "Ge\u00b7f\u00fchl", ",", "und", "jeg\u00b7li\u00b7chem", "Traum", "lieh"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "KON", "PIAT", "NN", "VVFIN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Dein holdseliger Reiz Wesen und bl\u00fchende Kraft.", "tokens": ["Dein", "hold\u00b7se\u00b7li\u00b7ger", "Reiz", "We\u00b7sen", "und", "bl\u00fc\u00b7hen\u00b7de", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Doch zum Ganzen verkettete bald sich das Einzelne; kunstvoll", "tokens": ["Doch", "zum", "Gan\u00b7zen", "ver\u00b7ket\u00b7te\u00b7te", "bald", "sich", "das", "Ein\u00b7zel\u00b7ne", ";", "kunst\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADV", "PRF", "ART", "ADJA", "$.", "VMFIN"], "meter": "--+--+--+--+---+", "measure": "anapaest.tetra.plus"}, "line.32": {"text": "Trat ", "tokens": ["Trat"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.33": {"text": "Vielfach lebt' in dem bunten Gebild dein wechselnder Liebreiz,", "tokens": ["Viel\u00b7fach", "lebt'", "in", "dem", "bun\u00b7ten", "Ge\u00b7bild", "dein", "wech\u00b7seln\u00b7der", "Lieb\u00b7reiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Neu stets warst du und stets holder in jeglicher Form:", "tokens": ["Neu", "stets", "warst", "du", "und", "stets", "hol\u00b7der", "in", "jeg\u00b7li\u00b7cher", "Form", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "PPER", "KON", "ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "Doch eintr\u00e4chtig erschien in der Sch\u00f6nheit stiller Verkl\u00e4rung", "tokens": ["Doch", "ein\u00b7tr\u00e4ch\u00b7tig", "er\u00b7schien", "in", "der", "Sch\u00f6n\u00b7heit", "stil\u00b7ler", "Ver\u00b7kl\u00e4\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.36": {"text": "Alles verwebt; ", "tokens": ["Al\u00b7les", "ver\u00b7webt", ";"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "Und allm\u00e4chtige Lieb' umwand mit der ewigen Kette,", "tokens": ["Und", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Lieb'", "um\u00b7wand", "mit", "der", "e\u00b7wi\u00b7gen", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.38": {"text": "Da\u00df kein feindliches Bild nahe, den fl\u00fcchtigen Traum.", "tokens": ["Da\u00df", "kein", "feind\u00b7li\u00b7ches", "Bild", "na\u00b7he", ",", "den", "fl\u00fcch\u00b7ti\u00b7gen", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.39": {"text": "Ha, da f\u00fchlt' ich die fr\u00fchere Kraft; k\u00fchn blickt' ich empor, hell", "tokens": ["Ha", ",", "da", "f\u00fchlt'", "ich", "die", "fr\u00fc\u00b7he\u00b7re", "Kraft", ";", "k\u00fchn", "blickt'", "ich", "em\u00b7por", ",", "hell"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD"], "meter": "+-+--+--+-+---+", "measure": "trochaic.hexa.relaxed"}, "line.40": {"text": "Flammte der Geist, hochauf schlug das erweiterte Herz.", "tokens": ["Flamm\u00b7te", "der", "Geist", ",", "hoch\u00b7auf", "schlug", "das", "er\u00b7wei\u00b7ter\u00b7te", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.41": {"text": "Herrlich enth\u00fcllte die Welt mir des Ruhms muthpr\u00fcfende Laufbahn,", "tokens": ["Herr\u00b7lich", "ent\u00b7h\u00fcll\u00b7te", "die", "Welt", "mir", "des", "Ruhms", "muth\u00b7pr\u00fc\u00b7fen\u00b7de", "Lauf\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.42": {"text": "Und wohl schien mir des Kampfs w\u00fcrdig der ewige Kranz,", "tokens": ["Und", "wohl", "schien", "mir", "des", "Kampfs", "w\u00fcr\u00b7dig", "der", "e\u00b7wi\u00b7ge", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.43": {"text": "Und ich empfand, noch leb' in der Brust mir der heilige Lichtstrahl", "tokens": ["Und", "ich", "emp\u00b7fand", ",", "noch", "leb'", "in", "der", "Brust", "mir", "der", "hei\u00b7li\u00b7ge", "Licht\u00b7strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "l