"word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Athem erw\u00e4rmt, und hell tagte die dunkele Welt.", "tokens": ["A\u00b7them", "er\u00b7w\u00e4rmt", ",", "und", "hell", "tag\u00b7te", "die", "dun\u00b7ke\u00b7le", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.27": {"text": "Bilder umgaukelten mich, s\u00fc\u00dfschmeichelnde, zarte Gestalten,", "tokens": ["Bil\u00b7der", "um\u00b7gau\u00b7kel\u00b7ten", "mich", ",", "s\u00fc\u00df\u00b7schmei\u00b7cheln\u00b7de", ",", "zar\u00b7te", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+---++--+--+-", "measure": "dactylic.di.plus"}, "line.28": {"text": "Und in lebende Form schmiegte sich jedes Gef\u00fchl;", "tokens": ["Und", "in", "le\u00b7ben\u00b7de", "Form", "schmieg\u00b7te", "sich", "je\u00b7des", "Ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Dich nur nannte mir jedes Gef\u00fchl, und jeglichem Traum lieh", "tokens": ["Dich", "nur", "nann\u00b7te", "mir", "je\u00b7des", "Ge\u00b7f\u00fchl", ",", "und", "jeg\u00b7li\u00b7chem", "Traum", "lieh"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,", "KON", "PIAT", "NN", "VVFIN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Dein holdseliger Reiz Wesen und bl\u00fchende Kraft.", "tokens": ["Dein", "hold\u00b7se\u00b7li\u00b7ger", "Reiz", "We\u00b7sen", "und", "bl\u00fc\u00b7hen\u00b7de", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Doch zum Ganzen verkettete bald sich das Einzelne; kunstvoll", "tokens": ["Doch", "zum", "Gan\u00b7zen", "ver\u00b7ket\u00b7te\u00b7te", "bald", "sich", "das", "Ein\u00b7zel\u00b7ne", ";", "kunst\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ADV", "PRF", "ART", "ADJA", "$.", "VMFIN"], "meter": "--+--+--+--+---+", "measure": "anapaest.tetra.plus"}, "line.32": {"text": "Trat ", "tokens": ["Trat"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.33": {"text": "Vielfach lebt' in dem bunten Gebild dein wechselnder Liebreiz,", "tokens": ["Viel\u00b7fach", "lebt'", "in", "dem", "bun\u00b7ten", "Ge\u00b7bild", "dein", "wech\u00b7seln\u00b7der", "Lieb\u00b7reiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+---+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Neu stets warst du und stets holder in jeglicher Form:", "tokens": ["Neu", "stets", "warst", "du", "und", "stets", "hol\u00b7der", "in", "jeg\u00b7li\u00b7cher", "Form", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "PPER", "KON", "ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.35": {"te