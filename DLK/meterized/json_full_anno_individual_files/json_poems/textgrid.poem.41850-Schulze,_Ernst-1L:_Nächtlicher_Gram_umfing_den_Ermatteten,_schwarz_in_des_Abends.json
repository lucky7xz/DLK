, "PPER", "ART", "ADJA", "NN", "$.", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD"], "meter": "+-+--+--+-+---+", "measure": "trochaic.hexa.relaxed"}, "line.40": {"text": "Flammte der Geist, hochauf schlug das erweiterte Herz.", "tokens": ["Flamm\u00b7te", "der", "Geist", ",", "hoch\u00b7auf", "schlug", "das", "er\u00b7wei\u00b7ter\u00b7te", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.41": {"text": "Herrlich enth\u00fcllte die Welt mir des Ruhms muthpr\u00fcfende Laufbahn,", "tokens": ["Herr\u00b7lich", "ent\u00b7h\u00fcll\u00b7te", "die", "Welt", "mir", "des", "Ruhms", "muth\u00b7pr\u00fc\u00b7fen\u00b7de", "Lauf\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.42": {"text": "Und wohl schien mir des Kampfs w\u00fcrdig der ewige Kranz,", "tokens": ["Und", "wohl", "schien", "mir", "des", "Kampfs", "w\u00fcr\u00b7dig", "der", "e\u00b7wi\u00b7ge", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.43": {"text": "Und ich empfand, noch leb' in der Brust mir der heilige Lichtstrahl", "tokens": ["Und", "ich", "emp\u00b7fand", ",", "noch", "leb'", "in", "der", "Brust", "mir", "der", "hei\u00b7li\u00b7ge", "Licht\u00b7strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "G\u00f6ttlicher Kraft, und gro\u00df werd' ich und ruhig durch dich.", "tokens": ["G\u00f6tt\u00b7li\u00b7cher", "Kraft", ",", "und", "gro\u00df", "werd'", "ich", "und", "ru\u00b7hig", "durch", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADJD", "VAFIN", "PPER", "KON", "ADJD", "APPR", "PPER", "$."], "meter": "+--+-+-+-++-+", "measure": "iambic.septa.invert"}, "line.45": {"text": "Bannt dein Wille mich auch in den Kreis schmerzvoller Entsagung;", "tokens": ["Bannt", "dein", "Wil\u00b7le", "mich", "auch", "in", "den", "Kreis", "schmerz\u00b7vol\u00b7ler", "Ent\u00b7sa\u00b7gung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+++-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.46": {"text": "Nie doch, ehe du selbst schwandest, entschwindet das Ziel.", "tokens": ["Nie", "doch", ",", "e\u00b7he", "du", "selbst", "schwan\u00b7dest", ",", "ent\u00b7schwin\u00b7det", "das", "Ziel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.47": {"text": "Himmlische Sch\u00f6nheit flieht vor des Sterblichen k\u00fchner Umarmung;", "tokens": ["Himm\u00b7li\u00b7sche", "Sch\u00f6n\u00b7heit", "flieht", "vor", "des", "Sterb\u00b7li\u00b7chen", "k\u00fch\u00b7ner", "Um\u00b7ar\u00b7mung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.48": {"text": "Aber mit ewigem Wunsch lohnt sie dem hoffenden Geist.", "tokens": ["A\u00b7ber", "mit", "e\u00b7wi\u00b7gem", "Wunsch", "lohnt", "sie", "dem", "hof\u00b7fen\u00b7den", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}