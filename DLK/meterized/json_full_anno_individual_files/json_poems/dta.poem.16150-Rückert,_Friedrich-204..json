{"dta.poem.16150": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "204.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie selten ahnt ein Freund, was dein Gem\u00fcth bekriegt;", "tokens": ["Wie", "sel\u00b7ten", "ahnt", "ein", "Freund", ",", "was", "dein", "Ge\u00b7m\u00fcth", "be\u00b7kriegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm steht von weitem, was dir n\u00e4chst am Herzen liegt.", "tokens": ["Ihm", "steht", "von", "wei\u00b7tem", ",", "was", "dir", "n\u00e4chst", "am", "Her\u00b7zen", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "$,", "PWS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auch zwischen Freunden gibts unmittheilbare Sachen,", "tokens": ["Auch", "zwi\u00b7schen", "Freun\u00b7den", "gibts", "un\u00b7mit\u00b7theil\u00b7ba\u00b7re", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die jeder mit sich selbst und Gott hat abzumachen.", "tokens": ["Die", "je\u00b7der", "mit", "sich", "selbst", "und", "Gott", "hat", "ab\u00b7zu\u00b7ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PRF", "ADV", "KON", "NN", "VAFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}