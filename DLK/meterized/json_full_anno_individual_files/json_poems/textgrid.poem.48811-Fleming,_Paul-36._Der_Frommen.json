{"textgrid.poem.48811": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "36. Der Frommen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sch\u00f6nste hei\u00dfest du, wenn Sch\u00f6nheit sch\u00f6ne macht,", "tokens": ["Die", "Sch\u00f6ns\u00b7te", "hei\u00b7\u00dfest", "du", ",", "wenn", "Sch\u00f6n\u00b7heit", "sch\u00f6\u00b7ne", "macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Keuscheste von Zucht. Doch la\u00df' ich mir behagen,", "tokens": ["die", "Keu\u00b7sches\u00b7te", "von", "Zucht", ".", "Doch", "la\u00df'", "ich", "mir", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "KON", "VVIMP", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dir vor der Fr\u00f6mmigkeit den Namen anzutragen,", "tokens": ["dir", "vor", "der", "Fr\u00f6m\u00b7mig\u00b7keit", "den", "Na\u00b7men", "an\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die aus den Augen dir mit kluger Einfalt lacht.", "tokens": ["die", "aus", "den", "Au\u00b7gen", "dir", "mit", "klu\u00b7ger", "Ein\u00b7falt", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mund trifft mit Herzen zu. Der Sch\u00f6nheit sanfte Pracht", "tokens": ["Mund", "trifft", "mit", "Her\u00b7zen", "zu", ".", "Der", "Sch\u00f6n\u00b7heit", "sanf\u00b7te", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "PTKVZ", "$.", "ART", "NN", "ADJA", "NN"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "gibt deiner Demut nach. Es kommen Viel' und fragen,", "tokens": ["gibt", "dei\u00b7ner", "De\u00b7mut", "nach", ".", "Es", "kom\u00b7men", "Viel'", "und", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wie kan ich ihnen doch was mehr und bessers sagen,", "tokens": ["wie", "kan", "ich", "ih\u00b7nen", "doch", "was", "mehr", "und", "bes\u00b7sers", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "ADV", "PWS", "ADV", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "als was sie hatten schon bei sich von dir gedacht?", "tokens": ["als", "was", "sie", "hat\u00b7ten", "schon", "bei", "sich", "von", "dir", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "PPER", "VAFIN", "ADV", "APPR", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dein Ansehn redt f\u00fcr dich, das sittige, das liebe,", "tokens": ["Dein", "An\u00b7sehn", "redt", "f\u00fcr", "dich", ",", "das", "sit\u00b7ti\u00b7ge", ",", "das", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,", "ART", "ADJA", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in welches die Natur die Treflichkeit ganz schriebe,", "tokens": ["in", "wel\u00b7ches", "die", "Na\u00b7tur", "die", "Tref\u00b7lich\u00b7keit", "ganz", "schrie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die in der Seelen liegt und hell ergl\u00e4nzt, wie sehr", "tokens": ["die", "in", "der", "See\u00b7len", "liegt", "und", "hell", "er\u00b7gl\u00e4nzt", ",", "wie", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "KON", "ADJD", "VVPP", "$,", "PWAV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "sie auch sich in sich h\u00e4lt. An Menschen nur sind M\u00e4ngel", "tokens": ["sie", "auch", "sich", "in", "sich", "h\u00e4lt", ".", "An", "Men\u00b7schen", "nur", "sind", "M\u00e4n\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PRF", "APPR", "PRF", "VVFIN", "$.", "APPR", "NN", "ADV", "VAFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und was verwerflich ist. Au dir, du reiner Engel,", "tokens": ["und", "was", "ver\u00b7werf\u00b7lich", "ist", ".", "Au", "dir", ",", "du", "rei\u00b7ner", "En\u00b7gel", ","], "token_info": ["word", "word", "w