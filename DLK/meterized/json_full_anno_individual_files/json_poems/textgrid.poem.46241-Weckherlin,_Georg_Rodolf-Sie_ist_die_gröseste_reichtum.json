{"textgrid.poem.46241": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Sie ist die gr\u00f6seste reichtum", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das pr\u00e4chtigste kriegsschif, dem je das meer war kund,", "tokens": ["Das", "pr\u00e4ch\u00b7tigs\u00b7te", "kriegs\u00b7schif", ",", "dem", "je", "das", "meer", "war", "kund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hat keinen mast so hoch, als hoch ist mein begehren;", "tokens": ["hat", "kei\u00b7nen", "mast", "so", "hoch", ",", "als", "hoch", "ist", "mein", "be\u00b7geh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "ADJD", "$,", "KOUS", "ADJD", "VAFIN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "kein anker halb so stark und bei\u00dfend in den grund,", "tokens": ["kein", "an\u00b7ker", "halb", "so", "stark", "und", "bei\u00b7\u00dfend", "in", "den", "grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "ADV", "ADJD", "KON", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "als meine lieb und treu, die unaufh\u00f6rlich wehren;", "tokens": ["als", "mei\u00b7ne", "lieb", "und", "treu", ",", "die", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "weh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJD", "KON", "ADJD", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So kn\u00fcpfet auch kein seil noch lein ein solchen bund", "tokens": ["So", "kn\u00fcp\u00b7fet", "auch", "kein", "seil", "noch", "lein", "ein", "sol\u00b7chen", "bund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "ADV", "ADJD", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "als die zart krause haar, die meinen geist beschweren;", "tokens": ["als", "die", "zart", "krau\u00b7se", "haar", ",", "die", "mei\u00b7nen", "geist", "be\u00b7schwe\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "--++-+-+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "kein wind blies jemal auf die segel stolz und rund,", "tokens": ["kein", "wind", "blies", "je\u00b7mal", "auf", "die", "se\u00b7gel", "stolz", "und", "rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "als mich die s\u00fc\u00dfe l\u00fcft des roten munds beth\u00f6ren;", "tokens": ["als", "mich", "die", "s\u00fc\u00b7\u00dfe", "l\u00fcft", "des", "ro\u00b7ten", "munds", "be\u00b7th\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kein schifman hat jemahls in einer schwarzen nacht", "tokens": ["Kein", "schi\u00b7fman", "hat", "je\u00b7mahls", "in", "ei\u00b7ner", "schwar\u00b7zen", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PIS", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "ein halb so klares liecht oder gestirn erblicket", "tokens": ["ein", "halb", "so", "kla\u00b7res", "liecht", "o\u00b7der", "ge\u00b7stirn", "er\u00b7bli\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "ADJA", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "als hell seind die augstern, mein trost und Amors pracht;", "tokens": ["als", "hell", "seind", "die", "augs\u00b7tern", ",", "mein", "trost", "und", "A\u00b7mors", "pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "KON", "NE", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "So hat auch noch kein schif, nach langer fahrt begl\u00fccket,", "tokens": ["So", "hat", "auch", "noch", "kein", "schif", ",", "nach", "lan\u00b7ger", "fahrt", "be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PIAT", "ADJD", "$,", "APPR", "ADJA", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "ein kleinot so vil wert zu uns von Ost gebracht", "tokens": ["ein", "klei\u00b7not", "so", "vil", "wert", "zu", "uns", "von", "Ost", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "wor