{"textgrid.poem.24052": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schwand der Fr\u00fchlingstag, der frische Tummel-Junge,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwand der Fr\u00fchlingstag, der frische Tummel-Junge,", "tokens": ["Schwand", "der", "Fr\u00fch\u00b7lings\u00b7tag", ",", "der", "fri\u00b7sche", "Tum\u00b7mel\u00b7Jun\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Floh zum grauen Meer hin \u00fcber die blauen Berge;", "tokens": ["Floh", "zum", "grau\u00b7en", "Meer", "hin", "\u00fc\u00b7ber", "die", "blau\u00b7en", "Ber\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Hei, wie flatterten ihm die gr\u00fcnen Raschelkr\u00e4nze", "tokens": ["Hei", ",", "wie", "flat\u00b7ter\u00b7ten", "ihm", "die", "gr\u00fc\u00b7nen", "Ra\u00b7schel\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Hell im Haar, wie wehten die lichten Locken!", "tokens": ["Hell", "im", "Haar", ",", "wie", "weh\u00b7ten", "die", "lich\u00b7ten", "Lo\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "s