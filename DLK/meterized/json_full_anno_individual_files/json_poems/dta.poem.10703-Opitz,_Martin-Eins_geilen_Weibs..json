{"dta.poem.10703": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Eins geilen Weibs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Hier lieget ein sehr sch\u00f6n/ doch geiles Weib begraben/", "tokens": ["Hier", "lie\u00b7get", "ein", "sehr", "sch\u00f6n", "/", "doch", "gei\u00b7les", "Weib", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "ADJD", "$(", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00fcnscht jhr nicht/ da\u00df sie ruh soll in der Erden haben/", "tokens": ["W\u00fcnscht", "jhr", "nicht", "/", "da\u00df", "sie", "ruh", "soll", "in", "der", "Er\u00b7den", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$(", "KOUS", "PPER", "NN", "VMFIN", "APPR", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie hat dem Himmel gleich zu werden sich ge\u00fcbt/", "tokens": ["Sie", "hat", "dem", "Him\u00b7mel", "gleich", "zu", "wer\u00b7den", "sich", "ge\u00b7\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PTKZU", "VAINF", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd nichts als stetige bewegung mehr gelibt.", "tokens": ["Vnd", "nichts", "als", "ste\u00b7ti\u00b7ge", "be\u00b7we\u00b7gung", "mehr", "ge\u00b7libt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}