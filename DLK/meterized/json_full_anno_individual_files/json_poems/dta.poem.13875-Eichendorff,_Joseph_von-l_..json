{"dta.poem.13875": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "l .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Theure! zierlich, mit drei Fingern,", "tokens": ["Theu\u00b7re", "!", "zier\u00b7lich", ",", "mit", "drei", "Fin\u00b7gern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "$,", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich'rer, mit der ganzen Hand \u2014", "tokens": ["Sich'\u00b7rer", ",", "mit", "der", "gan\u00b7zen", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so f\u00fcllt man aus den Dingern", "tokens": ["Und", "so", "f\u00fcllt", "man", "aus", "den", "Din\u00b7gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "'s Glas nicht halb, nein, bis zum Rand.", "tokens": ["'s", "Glas", "nicht", "halb", ",", "nein", ",", "bis", "zum", "Rand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKNEG", "ADJD", "$,", "PTKANT", "$,", "KOUS", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}