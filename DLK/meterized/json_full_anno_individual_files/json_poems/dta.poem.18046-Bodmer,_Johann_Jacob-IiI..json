{"dta.poem.18046": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein L\u00f6we war an Jahren alt,", "tokens": ["Ein", "L\u00f6\u00b7we", "war", "an", "Jah\u00b7ren", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Tugenden und Kr\u00e4ften kalt,", "tokens": ["An", "Tu\u00b7gen\u00b7den", "und", "Kr\u00e4f\u00b7ten", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ihm von Feinden weh geschah.", "tokens": ["Als", "ihm", "von", "Fein\u00b7den", "weh", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ihn ein Eber liegen sah,", "tokens": ["Da", "ihn", "ein", "E\u00b7ber", "lie\u00b7gen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dacht er an seinen alten Schaden,", "tokens": ["Dacht", "er", "an", "sei\u00b7nen", "al\u00b7ten", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und bi\u00df den L\u00f6wen in die Waden.", "tokens": ["Und", "bi\u00df", "den", "L\u00f6\u00b7wen", "in", "die", "Wa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Ochs kam auch daher gerannt,", "tokens": ["Der", "Ochs", "kam", "auch", "da\u00b7her", "ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo er den alten L\u00f6wen fand", "tokens": ["Wo", "er", "den", "al\u00b7ten", "L\u00f6\u00b7wen", "fand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er \u00fcbt an ihm auch seine Rach\u2019,", "tokens": ["Er", "\u00fcbt", "an", "ihm", "auch", "sei\u00b7ne", "Rach'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Indem er ihn mit beyden H\u00f6rnern stach.", "tokens": ["In\u00b7dem", "er", "ihn", "mit", "bey\u00b7den", "H\u00f6r\u00b7nern", "stach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mit andern Thieren kam zugleich", "tokens": ["Mit", "an\u00b7dern", "Thie\u00b7ren", "kam", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Esel auch hinzu, und gab ihm manchen Streich", "tokens": ["Der", "E\u00b7sel", "auch", "hin\u00b7zu", ",", "und", "gab", "ihm", "man\u00b7chen", "Streich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "An seine krancke Stirn. Das ist f\u00fcr die Untugend,", "tokens": ["An", "sei\u00b7ne", "kran\u00b7cke", "Stirn", ".", "Das", "ist", "f\u00fcr", "die", "Un\u00b7tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "PDS", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die du mir vor der Zeit erzeigt in deiner Jugend;", "tokens": ["Die", "du", "mir", "vor", "der", "Zeit", "er\u00b7zeigt", "in", "dei\u00b7ner", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sprach er. Der L\u00f6we fieng mit einem Seufzer an:", "tokens": ["Sprach", "er", ".", "Der", "L\u00f6\u00b7we", "fi\u00b7eng", "mit", "ei\u00b7nem", "Seuf\u00b7zer", "an", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ART", "NE", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "\u201eich litt es, schl\u00fcge mich ein Mann,", "tokens": ["\u201e", "ich", "litt", "es", ",", "schl\u00fc\u00b7ge", "mich", "ein", "Mann", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "\u201eda\u00df mich ein Esel schl\u00e4gt, schmerzt mehr als alle", "tokens": ["\u201e", "da\u00df", "mich", "ein", "E\u00b7sel", "schl\u00e4gt", ",", "schmerzt", "mehr", "als", "al\u00b7le"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PIS", "KOKOM", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "(noth,", "tokens": ["(", "noth", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "NN", "$,"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "\u201eso weh thut nicht der bittre Tod.", "tokens": ["\u201e", "so", "weh", "thut", "nicht", "der", "bitt\u00b7re", "Tod", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}