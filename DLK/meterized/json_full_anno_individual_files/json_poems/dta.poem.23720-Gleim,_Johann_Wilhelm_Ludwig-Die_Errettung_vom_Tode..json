{"dta.poem.23720": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Errettung vom Tode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das kleine Kind, mit Fl&#252;geln,", "tokens": ["Das", "klei\u00b7ne", "Kind", ",", "mit", "Fl", "&#252;", "geln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "XML_entity", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "$(", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ich erst kennen lernte,", "tokens": ["Das", "ich", "erst", "ken\u00b7nen", "lern\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Garten meines M\u00e4dchen,", "tokens": ["Im", "Gar\u00b7ten", "mei\u00b7nes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lief i\u00fcngst, mit Pfeil und Bogen,", "tokens": ["Lief", "i\u00b7\u00fcngst", ",", "mit", "Pfeil", "und", "Bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "Und mit dem vollen K\u00f6cher,", "tokens": ["Und", "mit", "dem", "vol\u00b7len", "K\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Im Garten meines Prinzen,", "tokens": ["Im", "Gar\u00b7ten", "mei\u00b7nes", "Prin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und scho\u00df nach kleinen V\u00f6geln.", "tokens": ["Und", "scho\u00df", "nach", "klei\u00b7nen", "V\u00f6\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es \u00fcbte sich, ihr Sch\u00f6nen,", "tokens": ["Es", "\u00fcb\u00b7te", "sich", ",", "ihr", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der kleine Gott im Schiessen;", "tokens": ["Der", "klei\u00b7ne", "Gott", "im", "Schies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Denn er will einst, im Garten,", "tokens": ["Denn", "er", "will", "einst", ",", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Euch ohne Fehler treffen.", "tokens": ["Euch", "oh\u00b7ne", "Feh\u00b7ler", "tref\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ich sah, verstekkt im Busche,", "tokens": ["Ich", "sah", ",", "vers\u00b7tekkt", "im", "Bu\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Den kleinen J\u00e4ger wandern.", "tokens": ["Den", "klei\u00b7nen", "J\u00e4\u00b7ger", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Allein, ich muste lachen.", "tokens": ["Al\u00b7lein", ",", "ich", "mus\u00b7te", "la\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Er traf mit zwanzig Pfeilen", "tokens": ["Er", "traf", "mit", "zwan\u00b7zig", "Pfei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Kein V\u00f6gelchen im Garten.", "tokens": ["Kein", "V\u00f6\u00b7gel\u00b7chen", "im", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Er lief mit leisen Tritten,", "tokens": ["Er", "lief", "mit", "lei\u00b7sen", "Trit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und aufgespantem Bogen,", "tokens": ["Und", "auf\u00b7ge\u00b7span\u00b7tem", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Schnell durch die Sommerlauben,", "tokens": ["Schnell", "durch", "die", "Som\u00b7mer\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Und fand mich hinterm Busche.", "tokens": ["Und", "fand", "mich", "hin\u00b7term", "Bu\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Er sch\u00fcttelte den K\u00f6cher,", "tokens": ["Er", "sch\u00fct\u00b7tel\u00b7te", "den", "K\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Es rasselten die Pfeile,", "tokens": ["Es", "ras\u00b7sel\u00b7ten", "die", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.23": {"text": "Er nikkte mit dem Kopfe,", "tokens": ["Er", "nikk\u00b7te", "mit", "dem", "Kop\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}