{"textgrid.poem.24860": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Abendstern", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Stern, der du vom fernen Osten her", "tokens": ["O", "Stern", ",", "der", "du", "vom", "fer\u00b7nen", "Os\u00b7ten", "her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "APPRART", "ADJA", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So einsam kommst, verlassen von den andern,", "tokens": ["So", "ein\u00b7sam", "kommst", ",", "ver\u00b7las\u00b7sen", "von", "den", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "VVPP", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ziehst du ruhelos im steten Wandern,", "tokens": ["Was", "ziehst", "du", "ru\u00b7he\u00b7los", "im", "ste\u00b7ten", "Wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein m\u00fcder Pilger, \u00fcber Land und Meer?", "tokens": ["Ein", "m\u00fc\u00b7der", "Pil\u00b7ger", ",", "\u00fc\u00b7ber", "Land", "und", "Meer", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein Strahl h\u00e4ngt bebend auf der Wellenbucht", "tokens": ["Dein", "Strahl", "h\u00e4ngt", "be\u00b7bend", "auf", "der", "Wel\u00b7len\u00b7bu