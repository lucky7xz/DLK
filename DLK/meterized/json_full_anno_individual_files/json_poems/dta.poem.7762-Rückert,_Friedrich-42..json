{"dta.poem.7762": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ist ganz ein Tyrann? Nicht, wer hat unterjocht", "tokens": ["Wer", "ist", "ganz", "ein", "Ty\u00b7rann", "?", "Nicht", ",", "wer", "hat", "un\u00b7ter\u00b7jocht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "$.", "PTKNEG", "$,", "PWS", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein freies Volk mit Macht; er that was er vermocht.", "tokens": ["Ein", "frei\u00b7es", "Volk", "mit", "Macht", ";", "er", "that", "was", "er", "ver\u00b7mocht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$.", "PPER", "VVFIN", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht, wer sich selber sagt: Weil es die Freiheit liebt,", "tokens": ["Nicht", ",", "wer", "sich", "sel\u00b7ber", "sagt", ":", "Weil", "es", "die", "Frei\u00b7heit", "liebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWS", "PRF", "ADV", "VVFIN", "$.", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df es mich hassen; doch ihm nicht die Freiheit gibt;", "tokens": ["Mu\u00df", "es", "mich", "has\u00b7sen", ";", "doch", "ihm", "nicht", "die", "Frei\u00b7heit", "gibt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$.", "ADV", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er hofft, da\u00df ein Verein von Streng' und Mild' erringe", "tokens": ["Er", "hofft", ",", "da\u00df", "ein", "Ver\u00b7ein", "von", "Streng'", "und", "Mild'", "er\u00b7rin\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "NE", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ziel zuletzt, da\u00df aus Gewohnheit Lieb' entspringe.", "tokens": ["Das", "Ziel", "zu\u00b7letzt", ",", "da\u00df", "aus", "Ge\u00b7wohn\u00b7heit", "Lieb'", "ent\u00b7sprin\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer aber, wenn sich ihm der Nacken sklavisch beugt,", "tokens": ["Wer", "a\u00b7ber", ",", "wenn", "sich", "ihm", "der", "Na\u00b7cken", "skla\u00b7visch", "beugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "KOUS", "PRF", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Unterw\u00fcrfigkeit ihm Hand und Mund bezeugt,", "tokens": ["Und", "Un\u00b7ter\u00b7w\u00fcr\u00b7fig\u00b7keit", "ihm", "Hand", "und", "Mund", "be\u00b7zeugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Zu sagen wagt: Ich wei\u00df, da\u00df euch die Liebe fehle", "tokens": ["Zu", "sa\u00b7gen", "wagt", ":", "Ich", "wei\u00df", ",", "da\u00df", "euch", "die", "Lie\u00b7be", "feh\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu mir, und diese Lieb' ists die ich euch befehle;", "tokens": ["Zu", "mir", ",", "und", "die\u00b7se", "Lieb'", "ists", "die", "ich", "euch", "be\u00b7feh\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "PDAT", "NN", "VAFIN", "ART", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der ist ganz ein Tyrann, der nicht Gehorsam still", "tokens": ["Der", "ist", "ganz", "ein", "Ty\u00b7rann", ",", "der", "nicht", "Ge\u00b7hor\u00b7sam", "still"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PTKNEG", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich l\u00e4\u00dft gen\u00fcgen, und befehlen Liebe will.", "tokens": ["Sich", "l\u00e4\u00dft", "ge\u00b7n\u00fc\u00b7gen", ",", "und", "be\u00b7feh\u00b7len", "Lie\u00b7be", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "VVINF", "$,", "KON", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}