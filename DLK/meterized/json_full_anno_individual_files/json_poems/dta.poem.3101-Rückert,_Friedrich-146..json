{"dta.poem.3101": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "146.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Such' etwas Sch\u00f6nes dir nur immer aus vom Gang", "tokens": ["Such'", "et\u00b7was", "Sch\u00f6\u00b7nes", "dir", "nur", "im\u00b7mer", "aus", "vom", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "ADJA", "PPER", "ADV", "ADV", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu denken der Natur und Weltzusammenhang,", "tokens": ["Zu", "den\u00b7ken", "der", "Na\u00b7tur", "und", "Welt\u00b7zu\u00b7sam\u00b7men\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was du ausdenken magst, es ist ein Traum allein;", "tokens": ["Was", "du", "aus\u00b7den\u00b7ken", "magst", ",", "es", "ist", "ein", "Traum", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df wenigstens den Traum sinnreich und trostreich seyn!", "tokens": ["La\u00df", "we\u00b7nigs\u00b7tens", "den", "Traum", "sinn\u00b7reich", "und", "trost\u00b7reich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}