{"dta.poem.11804": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht unter Gleichen ist die Freundschaft, noch Ungleichen,", "tokens": ["Nicht", "un\u00b7ter", "Glei\u00b7chen", "ist", "die", "Freund\u00b7schaft", ",", "noch", "Un\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VAFIN", "ART", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur zwischen Aehnlichen, die sich Verschiednes reichen.", "tokens": ["Nur", "zwi\u00b7schen", "A\u00b7ehn\u00b7li\u00b7chen", ",", "die", "sich", "Ver\u00b7schied\u00b7nes", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PRELS", "PRF", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wer etwas geben soll, mu\u00df eine F\u00fcll' an Gaben,", "tokens": ["Wer", "et\u00b7was", "ge\u00b7ben", "soll", ",", "mu\u00df", "ei\u00b7ne", "F\u00fcll'", "an", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "VMFIN", "$,", "VMFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer empfangen will, mu\u00df einen Mangel haben.", "tokens": ["Und", "wer", "emp\u00b7fan\u00b7gen", "will", ",", "mu\u00df", "ei\u00b7nen", "Man\u00b7gel", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVINF", "VMFIN", "$,", "VMFIN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und eines Mangel mu\u00df des andern F\u00fclle seyn,", "tokens": ["Und", "ei\u00b7nes", "Man\u00b7gel", "mu\u00df", "des", "an\u00b7dern", "F\u00fcl\u00b7le", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst ist es nicht ein Tausch, nur einer T\u00e4uschung Schein,", "tokens": ["Sonst", "ist", "es", "nicht", "ein", "Tausch", ",", "nur", "ei\u00b7ner", "T\u00e4u\u00b7schung", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn du nicht geben kannst, was ich empfangen kann;", "tokens": ["Wenn", "du", "nicht", "ge\u00b7ben", "kannst", ",", "was", "ich", "emp\u00b7fan\u00b7gen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Wasser nimmt kein Oel, und auch kein Feuer an.", "tokens": ["Das", "Was\u00b7ser", "nimmt", "kein", "O\u00b7el", ",", "und", "auch", "kein", "Feu\u00b7er", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,", "KON", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Doch hast du geist'ges Oel, und du hast geist'ge Flammen,", "tokens": ["Doch", "hast", "du", "geist'\u00b7ges", "O\u00b7el", ",", "und", "du", "hast", "geist'\u00b7ge", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "$,", "KON", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So traget ins Gef\u00e4\u00df der Freundschaft sie zusammen.", "tokens": ["So", "tra\u00b7get", "ins", "Ge\u00b7f\u00e4\u00df", "der", "Freund\u00b7schaft", "sie", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Glutdocht wird im Oel, das Oel am Glutdocht brennen,", "tokens": ["Der", "Glut\u00b7docht", "wird", "im", "O\u00b7el", ",", "das", "O\u00b7el", "am", "Glut\u00b7docht", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und hell im Lampenschein zwei Geister sich erkennen.", "tokens": ["Und", "hell", "im", "Lam\u00b7pen\u00b7schein", "zwei", "Geis\u00b7ter", "sich", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "CARD", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}