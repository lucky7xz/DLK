{"dta.poem.7692": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zur Freundschaft ists genug, des Freundes Freund zu seyn;", "tokens": ["Zur", "Freund\u00b7schaft", "ists", "ge\u00b7nug", ",", "des", "Freun\u00b7des", "Freund", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Freund des Freundes schlie\u00dft der Bund darum nicht ein.", "tokens": ["Den", "Freund", "des", "Freun\u00b7des", "schlie\u00dft", "der", "Bund", "da\u00b7rum", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "PAV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df du an dieser mich, ihn h\u00e4ltst an jener Hand,", "tokens": ["Da\u00df", "du", "an", "die\u00b7ser", "mich", ",", "ihn", "h\u00e4ltst", "an", "je\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "PPER", "$,", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kn\u00fcpft zwischen mir und ihm unmittelbar kein Band.", "tokens": ["Kn\u00fcpft", "zwi\u00b7schen", "mir", "und", "ihm", "un\u00b7mit\u00b7tel\u00b7bar", "kein", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "PPER", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch deines Feindes Freund zu hei\u00dfen, mu\u00df ich lassen,", "tokens": ["Doch", "dei\u00b7nes", "Fein\u00b7des", "Freund", "zu", "hei\u00b7\u00dfen", ",", "mu\u00df", "ich", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil man nicht lieben kann, was man den Freund sieht hassen.", "tokens": ["Weil", "man", "nicht", "lie\u00b7ben", "kann", ",", "was", "man", "den", "Freund", "sieht", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$,", "PRELS", "PIS", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}