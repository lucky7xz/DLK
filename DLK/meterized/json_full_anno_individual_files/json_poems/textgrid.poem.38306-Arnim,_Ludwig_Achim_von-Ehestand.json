{"textgrid.poem.38306": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ehestand", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gieng spazieren in ein Feld", "tokens": ["Ich", "gieng", "spa\u00b7zie\u00b7ren", "in", "ein", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ohne S\u00fcnde;", "tokens": ["Oh\u00b7ne", "S\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mich umzusehen in der Welt,", "tokens": ["Mich", "um\u00b7zu\u00b7se\u00b7hen", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie es st\u00fcnde.", "tokens": ["Wie", "es", "st\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Es war an einem Sonntag gut,", "tokens": ["Es", "war", "an", "ei\u00b7nem", "Sonn\u00b7tag", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach dem Essen;", "tokens": ["Nach", "dem", "Es\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Mein Leid, das mich so qu\u00e4len thut,", "tokens": ["Mein", "Leid", ",", "das", "mich", "so", "qu\u00e4\u00b7len", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu vergessen.", "tokens": ["Zu", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Mit Gedanken th\u00e4t ich zanken,", "tokens": ["Mit", "Ge\u00b7dan\u00b7ken", "th\u00e4t", "ich", "zan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Th\u00e4t ich zanken.", "tokens": ["Th\u00e4t", "ich", "zan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Sehr tief gedacht ich hin und her,", "tokens": ["Sehr", "tief", "ge\u00b7dacht", "ich", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ich auswollt;", "tokens": ["Wo", "ich", "aus\u00b7wollt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mir selbst wu\u00dft nicht zu rathen mehr,", "tokens": ["Mir", "selbst", "wu\u00dft", "nicht", "zu", "ra\u00b7then", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "PTKZU", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was ich thun sollt.", "tokens": ["Was", "ich", "thun", "sollt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Allein zu bleiben mich verdro\u00df,", "tokens": ["Al\u00b7lein", "zu", "blei\u00b7ben", "mich", "ver\u00b7dro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit der Weile;", "tokens": ["Mit", "der", "Wei\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Zum Heurathen die Lust war gro\u00df,", "tokens": ["Zum", "Heu\u00b7ra\u00b7then", "die", "Lust", "war", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In der Eile.", "tokens": ["In", "der", "Ei\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Wollt schier wagen, ja zu sagen,", "tokens": ["Wollt", "schier", "wa\u00b7gen", ",", "ja", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ja zu sagen.", "tokens": ["Ja", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKANT", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Und sieh, ein J\u00fcngling trat herf\u00fcr, Wohlbekleidet;", "tokens": ["Und", "sieh", ",", "ein", "J\u00fcng\u00b7ling", "trat", "her\u00b7f\u00fcr", ",", "Wohl\u00b7be\u00b7klei\u00b7det", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er gr\u00fc\u00dft mich freundlich in Geb\u00fchr,", "tokens": ["Er", "gr\u00fc\u00dft", "mich", "freund\u00b7lich", "in", "Ge\u00b7b\u00fchr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich begleitet.", "tokens": ["Mich", "be\u00b7glei\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "An H\u00e4nden trug er g\u00fcldne Ring,", "tokens": ["An", "H\u00e4n\u00b7den", "trug", "er", "g\u00fcld\u00b7ne", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die ihn zierten;", "tokens": ["Die", "ihn", "zier\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Auch noch mehr andre k\u00f6stlich Ding", "tokens": ["Auch", "noch", "mehr", "and\u00b7re", "k\u00f6st\u00b7lich", "Ding"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "PIS", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihn ber\u00fchrten.", "tokens": ["Ihn", "be\u00b7r\u00fchr\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "An dem allen h\u00e4tt Gefallen,", "tokens": ["An", "dem", "al\u00b7len", "h\u00e4tt", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "H\u00e4tt Gefallen.", "tokens": ["H\u00e4tt", "Ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Bei neben ward ich auch gewahr,", "tokens": ["Bei", "ne\u00b7ben", "ward", "ich", "auch", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Da\u00df der J\u00fcngling", "tokens": ["Da\u00df", "der", "J\u00fcng\u00b7ling"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ein schweres Joch trug immerdar,", "tokens": ["Ein", "schwe\u00b7res", "Joch", "trug", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ihm anhing.", "tokens": ["Das", "ihm", "an\u00b7hing", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "An F\u00fcssen h\u00e4tt er Ketten stark,", "tokens": ["An", "F\u00fcs\u00b7sen", "h\u00e4tt", "er", "Ket\u00b7ten", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stahl und Eisen;", "tokens": ["Stahl", "und", "Ei\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Das schmerzt ihn bis auf Bein und Mark,", "tokens": ["Das", "schmerzt", "ihn", "bis", "auf", "Bein", "und", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Konnt aufreissen.", "tokens": ["Konnt", "auf\u00b7reis\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Ottern, Schlangen auch dran hangen,", "tokens": ["Ot\u00b7tern", ",", "Schlan\u00b7gen", "auch", "dran", "han\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ADV", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Auch dran hangen.", "tokens": ["Auch", "dran", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Da ich nun ward mit ihm bekannt,", "tokens": ["Da", "ich", "nun", "ward", "mit", "ihm", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich ihn fragte:", "tokens": ["Ich", "ihn", "frag\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "J\u00fcngling wer bist? Wie wirst genannt?", "tokens": ["J\u00fcng\u00b7ling", "wer", "bist", "?", "Wie", "wirst", "ge\u00b7nannt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "VAFIN", "$.", "PWAV", "VAFIN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Er mir sagte:", "tokens": ["Er", "mir", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "Ich bin der Ehstand dieser Welt,", "tokens": ["Ich", "bin", "der", "Eh\u00b7stand", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Also hei\u00df ich;", "tokens": ["Al\u00b7so", "hei\u00df", "ich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "So mancher, tapfre k\u00fchne Held", "tokens": ["So", "man\u00b7cher", ",", "tapf\u00b7re", "k\u00fch\u00b7ne", "Held"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "$,", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Um mich rei\u00dft sich.", "tokens": ["Um", "mich", "rei\u00dft", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "PRF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Zum Heurathen thu ich laden,", "tokens": ["Zum", "Heu\u00b7ra\u00b7then", "thu", "ich", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Thu ich laden.", "tokens": ["Thu", "ich", "la\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Dann ich ihn erst recht schaute an,", "tokens": ["Dann", "ich", "ihn", "erst", "recht", "schau\u00b7te", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "ADV", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Verwundern;", "tokens": ["Mit", "Ver\u00b7wun\u00b7dern", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Gedacht: Sollt denn ich freyer Mann", "tokens": ["Ge\u00b7dacht", ":", "Sollt", "denn", "ich", "frey\u00b7er", "Mann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VMFIN", "KON", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich jezunder", "tokens": ["Gleich", "je\u00b7zun\u00b7der"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Beladen mich mit solchem Joch,", "tokens": ["Be\u00b7la\u00b7den", "mich", "mit", "sol\u00b7chem", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und verbinden?", "tokens": ["Und", "ver\u00b7bin\u00b7den", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ich wills wohl lassen bleiben noch,", "tokens": ["Ich", "wills", "wohl", "las\u00b7sen", "blei\u00b7ben", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kanns nicht finden;", "tokens": ["Kanns", "nicht", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Will mich drinnen bas besinnen,", "tokens": ["Will", "mich", "drin\u00b7nen", "bas", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Bas besinnen.", "tokens": ["Bas", "be\u00b7sin\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Ich gieng spazieren in ein Feld", "tokens": ["Ich", "gieng", "spa\u00b7zie\u00b7ren", "in", "ein", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ohne S\u00fcnde;", "tokens": ["Oh\u00b7ne", "S\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mich umzusehen in der Welt,", "tokens": ["Mich", "um\u00b7zu\u00b7se\u00b7hen", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie es st\u00fcnde.", "tokens": ["Wie", "es", "st\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Es war an einem Sonntag gut,", "tokens": ["Es", "war", "an", "ei\u00b7nem", "Sonn\u00b7tag", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach dem Essen;", "tokens": ["Nach", "dem", "Es\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-