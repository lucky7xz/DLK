{"textgrid.poem.26532": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, And're m\u00f6gen deine Galerien", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, And're m\u00f6gen deine Galerien", "tokens": ["Ja", ",", "An\u00b7d'\u00b7re", "m\u00f6\u00b7gen", "dei\u00b7ne", "Ga\u00b7le\u00b7ri\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PIS", "VMFIN", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durcheilen, deine Dome und Pal\u00e4ste", "tokens": ["Durc\u00b7hei\u00b7len", ",", "dei\u00b7ne", "Do\u00b7me", "und", "Pa\u00b7l\u00e4s\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bestaunen jetzt als red'gewandte G\u00e4ste,", "tokens": ["Be\u00b7stau\u00b7nen", "jetzt", "als", "re\u00b7d'\u00b7ge\u00b7wand\u00b7te", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOUS", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die ihrer eig'nen Leere gern entfliehen.", "tokens": ["Die", "ih\u00b7rer", "eig'\u00b7nen", "Lee\u00b7re", "gern", "ent\u00b7flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu jener Reife bin ich l\u00e4ngst gediehen,", "tokens": ["Zu", "je\u00b7ner", "Rei\u00b7fe", "bin", "ich", "l\u00e4ngst", "ge\u00b7die\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": [