{"textgrid.poem.62339": {"metadata": {"author": {"name": "Gessner, Salomon", "birth": "N.A.", "death": "N.A."}, "title": "Erythia", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seys denn; bey dieser schw\u00fclen Hitze sucht jeder schmachtend die K\u00fchlung.", "tokens": ["Seys", "denn", ";", "bey", "die\u00b7ser", "schw\u00fc\u00b7len", "Hit\u00b7ze", "sucht", "je\u00b7der", "schmach\u00b7tend", "die", "K\u00fch\u00b7lung", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "$.", "APPR", "PDAT", "ADJA", "NN", "VVFIN", "PIS", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+--+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Du siehst alles; nur nicht, da\u00df wir da sind wo wir seyn sollen.", "tokens": ["Du", "siehst", "al\u00b7les", ";", "nur", "nicht", ",", "da\u00df", "wir", "da", "sind", "wo", "wir", "seyn", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "VAFIN", "PWAV", "PPER", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Ha ja! Pan! Ihr G\u00f6tter! Welch angenehmer Ort ist das!", "tokens": ["Ha", "ja", "!", "Pan", "!", "Ihr", "G\u00f6t\u00b7ter", "!", "Welch", "an\u00b7ge\u00b7neh\u00b7mer", "Ort", "ist", "das", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "NE", "$.", "PPOSAT", "NN", "$.", "PIAT", "ADJA", "NN", "VAFIN", "PDS", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.4": {"line.1"