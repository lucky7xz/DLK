{"textgrid.poem.34765": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun der Gott mir g\u00fcnstig nicket,", "tokens": ["Nun", "der", "Gott", "mir", "g\u00fcns\u00b7tig", "ni\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll ich schweigen wie ein Stummer,", "tokens": ["Soll", "ich", "schwei\u00b7gen", "wie", "ein", "Stum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich, der, als ich unbegl\u00fccket,", "tokens": ["Ich", ",", "der", ",", "als", "ich", "un\u00b7be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Soviel sang von meinem Kummer,", "tokens": ["So\u00b7viel", "sang", "von", "mei\u00b7nem", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df mir tausend arme Jungen", "tokens": ["Da\u00df", "mir", "tau\u00b7send", "ar\u00b7me", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gar verzweifelt nachgedichtet,", "tokens": ["Gar", "ver\u00b7zwei\u00b7felt", "nach\u00b7ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Leid, das ich besungen,", "tokens": ["Und", "das", "Leid", ",", "das", "ich", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch viel Schlimmres angerichtet!", "tokens": ["Noch", "viel", "Schlimm\u00b7res", "an\u00b7ge\u00b7rich\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oh, ihr Nachtigallench\u00f6re,", "tokens": ["Oh", ",", "ihr", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7ch\u00f6\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ich trage in der Seele,", "tokens": ["Die", "ich", "tra\u00b7ge", "in", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df man eure Wonne h\u00f6re,", "tokens": ["Da\u00df", "man", "eu\u00b7re", "Won\u00b7ne", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jubelt auf mit voller Kehle!", "tokens": ["Ju\u00b7belt", "auf", "mit", "vol\u00b7ler", "Keh\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "meas