{"dta.poem.23320": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caf\u00e9 national.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch ein Fl\u00fcstern, welch ein Summen!", "tokens": ["Welch", "ein", "Fl\u00fcs\u00b7tern", ",", "welch", "ein", "Sum\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$,", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welch ein stiller Leseflei\u00df!", "tokens": ["Welch", "ein", "stil\u00b7ler", "Le\u00b7se\u00b7flei\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur Marqueure schrei'n und brummen:", "tokens": ["Nur", "Mar\u00b7que\u00b7u\u00b7re", "schrei'n", "und", "brum\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tasse schwarz! und Tasse wei\u00df!", "tokens": ["Tas\u00b7se", "schwarz", "!", "und", "Tas\u00b7se", "wei\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die Zeitungsbl\u00e4tter rauschen,", "tokens": ["Und", "die", "Zei\u00b7tungs\u00b7bl\u00e4t\u00b7ter", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und man liest und liest sich satt,", "tokens": ["Und", "man", "liest", "und", "liest", "sich", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um Ideen einzutauschen,", "tokens": ["Um", "I\u00b7deen", "ein\u00b7zu\u00b7tau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil man selbst gar wenig hat.", "tokens": ["Weil", "man", "selbst", "gar", "we\u00b7nig", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "PIS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und sie plaudern, bl\u00e4ttern, suchen,", "tokens": ["Und", "sie", "plau\u00b7dern", ",", "bl\u00e4t\u00b7tern", ",", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich kommt ein Resultat:", "tokens": ["End\u00b7lich", "kommt", "ein", "Re\u00b7sul\u00b7tat", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch ein St\u00fcckchen Aepfelkuchen!", "tokens": ["Noch", "ein", "St\u00fcck\u00b7chen", "A\u00b7e\u00b7pfel\u00b7ku\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Zwar der Cours steht desolat.", "tokens": ["Zwar", "der", "Cours", "steht", "de\u00b7so\u00b7lat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "NE", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Und sie sitzen, gr\u00fcbeln, denken,", "tokens": ["Und", "sie", "sit\u00b7zen", ",", "gr\u00fc\u00b7beln", ",", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie werden hei\u00df und stumm,", "tokens": ["Und", "sie", "wer\u00b7den", "hei\u00df", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit k\u00fchlenden Getr\u00e4nken", "tokens": ["Und", "mit", "k\u00fch\u00b7len\u00b7den", "Ge\u00b7tr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00e4rken sie sich wiederum.", "tokens": ["St\u00e4r\u00b7ken", "sie", "sich", "wie\u00b7de\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So vertreibt man sich die Zeiten", "tokens": ["So", "ver\u00b7treibt", "man", "sich", "die", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach des Tages Hitz' und Last,", "tokens": ["Nach", "des", "Ta\u00b7ges", "Hitz'", "und", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis erf\u00fcllt mit Neuigkeiten", "tokens": ["Bis", "er\u00b7f\u00fcllt", "mit", "Neu\u00b7ig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht nach Haus der letzte Gast.", "tokens": ["Geht", "nach", "Haus", "der", "letz\u00b7te", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch am Morgen sieht sich wieder", "tokens": ["Doch", "am", "Mor\u00b7gen", "sieht", "sich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PRF", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier der alte Lesekreis,", "tokens": ["Hier", "der", "al\u00b7te", "Le\u00b7se\u00b7kreis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und man l\u00e4\u00dft sich h\u00e4uslich nieder:", "tokens": ["Und", "man", "l\u00e4\u00dft", "sich", "h\u00e4us\u00b7lich", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tasse schwarz! und Tasse wei\u00df!", "tokens": ["Tas\u00b7se", "schwarz", "!", "und", "Tas\u00b7se", "wei\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}