{"dta.poem.9561": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An etliche schreib-federn/ die er ihr zuschickte.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr stummen redner ihr/ ihr mahler der gedancken/", "tokens": ["Ihr", "stum\u00b7men", "red\u00b7ner", "ihr", "/", "ihr", "mah\u00b7ler", "der", "ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "$(", "PPOSAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihr die heimligkeit des tieffsten hertzens wi\u00dft/", "tokens": ["Die", "ihr", "die", "heim\u00b7lig\u00b7keit", "des", "tieffs\u00b7ten", "hert\u00b7zens", "wi\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr heuchler/ die ihr steckt voll trug und arger list/", "tokens": ["Ihr", "heuch\u00b7ler", "/", "die", "ihr", "steckt", "voll", "trug", "und", "ar\u00b7ger", "list", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "VVFIN", "ADJD", "VVFIN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ihr bald weint und lacht/ bald z\u00fcrnt und pflegt zu zancken;", "tokens": ["Die", "ihr", "bald", "weint", "und", "lacht", "/", "bald", "z\u00fcrnt", "und", "pflegt", "zu", "zan\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$(", "ADV", "VVPP", "KON", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr silber-r\u00f6hren ihr/ aus deren engen schrancken", "tokens": ["Ihr", "sil\u00b7ber\u00b7r\u00f6h\u00b7ren", "ihr", "/", "aus", "de\u00b7ren", "en\u00b7gen", "schran\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "$(", "APPR", "PRELAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Glut/ seuffzer/ grimm und ha\u00df/ rach/ lieb\u2019 und eifer fliest;", "tokens": ["Glut", "/", "seuff\u00b7zer", "/", "grimm", "und", "ha\u00df", "/", "rach", "/", "lieb'", "und", "ei\u00b7fer", "fliest", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "ADJD", "KON", "VVIMP", "$(", "VVFIN", "$(", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Jhr \u00e4rtzte/ die ihr seyd mit klugheit ausger\u00fcst/", "tokens": ["Ihr", "\u00e4rtz\u00b7te", "/", "die", "ihr", "seyd", "mit", "klug\u00b7heit", "aus\u00b7ge\u00b7r\u00fcst", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PRELS", "PPER", "VAFIN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und keinem treuer dient/ als den verliebten krancken:", "tokens": ["Und", "kei\u00b7nem", "treu\u00b7er", "dient", "/", "als", "den", "ver\u00b7lieb\u00b7ten", "kran\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$(", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Geht zur Oranten hin/ ihr schlauen hexen-meister/", "tokens": ["Geht", "zur", "O\u00b7ran\u00b7ten", "hin", "/", "ihr", "schlau\u00b7en", "he\u00b7xen\u00b7meis\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die ihr gantz unvermerckt hertz/ seele/ blut und geister", "tokens": ["Die", "ihr", "gantz", "un\u00b7ver\u00b7merckt", "hertz", "/", "see\u00b7le", "/", "blut", "und", "geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJD", "NN", "$(", "VVFIN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Durch eure schwartze kunst bezaubert und verstrickt/", "tokens": ["Durch", "eu\u00b7re", "schwart\u00b7ze", "kunst", "be\u00b7zau\u00b7bert", "und", "ver\u00b7strickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Geht/ doch mit dem beding/ da\u00df ihr mir thut zu wissen/", "tokens": ["Geht", "/", "doch", "mit", "dem", "be\u00b7ding", "/", "da\u00df", "ihr", "mir", "thut", "zu", "wis\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADV", "APPR", "ART", "VVFIN", "$(", "KOUS", "PPER", "PPER", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie bald Orante sich zur liebe wird entschliessen/", "tokens": ["Wie", "bald", "O\u00b7ran\u00b7te", "sich", "zur", "lie\u00b7be", "wird", "ent\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PRF", "APPRART", "ADJA", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ob ich ewig dann soll bleiben unbegl\u00fcckt?", "tokens": ["Und", "ob", "ich", "e\u00b7wig", "dann", "soll", "blei\u00b7ben", "un\u00b7be\u00b7gl\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "VMFIN", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}