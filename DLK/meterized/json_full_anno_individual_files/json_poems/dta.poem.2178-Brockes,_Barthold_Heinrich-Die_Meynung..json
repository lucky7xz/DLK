{"dta.poem.2178": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Meynung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist das Praeiudicium autoritatis in der That", "tokens": ["Es", "ist", "das", "Prae\u00b7i\u00b7u\u00b7di\u00b7ci\u00b7um", "au\u00b7to\u00b7ri\u00b7ta\u00b7tis", "in", "der", "That"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Die\u00df: einem andern nach zu meynen, was er vor-", "tokens": ["Die\u00df", ":", "ei\u00b7nem", "an\u00b7dern", "nach", "zu", "mey\u00b7nen", ",", "was", "er", "vor"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$.", "ART", "ADJA", "APPR", "PTKZU", "VVINF", "$,", "PWS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "her gemeynet hat.", "tokens": ["her", "ge\u00b7mey\u00b7net", "hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}