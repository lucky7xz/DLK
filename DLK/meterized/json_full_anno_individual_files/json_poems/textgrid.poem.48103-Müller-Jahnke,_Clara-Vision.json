{"textgrid.poem.48103": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Vision", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur lichtumflossenen Weihnachtszeit", "tokens": ["Zur", "lich\u00b7tum\u00b7flos\u00b7se\u00b7nen", "Weih\u00b7nachts\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "wie doppelt schwer ist Menschenleid!", "tokens": ["wie", "dop\u00b7pelt", "schwer", "ist", "Men\u00b7schen\u00b7leid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie doppelt tief ist des Elends Nacht,", "tokens": ["Wie", "dop\u00b7pelt", "tief", "ist", "des", "E\u00b7lends", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wenn Lichtschein aus Pal\u00e4sten lacht!", "tokens": ["wenn", "Licht\u00b7schein", "aus", "Pa\u00b7l\u00e4s\u00b7ten", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ein Waisenkind im Winterschnee:", "tokens": ["Und", "ein", "Wai\u00b7sen\u00b7kind", "im", "Win\u00b7ter\u00b7schnee", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "das Auge wird feucht, das Herz tut weh . . .", "tokens": ["das", "Au\u00b7ge", "wird", "feucht", ",", "das", "Herz", "tut", "weh", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$.", "$.", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "\u2013 Ich ging in die sinkende Nacht hinaus;", "tokens": ["\u2013", "Ich", "ging", "in", "die", "sin\u00b7ken\u00b7de", "Nacht", "hin\u00b7aus", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "die Glocken klangen vom Gotteshaus.", "tokens": ["die", "Glo\u00b7cken", "klan\u00b7gen", "vom", "Got\u00b7tes\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "In des Himmels blitzendem Diadem", "tokens": ["In", "des", "Him\u00b7mels", "blit\u00b7zen\u00b7dem", "Di\u00b7a\u00b7dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "strahlte der Stern von Bethlehem.", "tokens": ["strahl\u00b7te", "der", "Stern", "von", "Beth\u00b7le\u00b7hem", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Und als ich schritt aus des St\u00e4dtleins Tor,", "tokens": ["Und", "als", "ich", "schritt", "aus", "des", "St\u00e4dt\u00b7leins", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "stiegen die Nebel der Nacht empor.", "tokens": ["stie\u00b7gen", "die", "Ne\u00b7bel", "der", "Nacht", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Sie spannen mich ein \u2013 da\u00df Gott erbarm'! \u2013", "tokens": ["Sie", "span\u00b7nen", "mich", "ein", "\u2013", "da\u00df", "Gott", "er\u00b7barm'", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "$(", "KOUS", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "von Schemen schien es ein bleicher Schwarm:", "tokens": ["von", "Sche\u00b7men", "schien", "es", "ein", "blei\u00b7cher", "Schwarm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "fahle Wangen und welke Gesichter,", "tokens": ["fah\u00b7le", "Wan\u00b7gen", "und", "wel\u00b7ke", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "PWAT", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "liebehungernde Augenlichter,", "tokens": ["lie\u00b7be\u00b7hun\u00b7gern\u00b7de", "Au\u00b7gen\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "tastende, gierende Bettlerh\u00e4nde \u2013", "tokens": ["tas\u00b7ten\u00b7de", ",", "gie\u00b7ren\u00b7de", "Bett\u00b7ler\u00b7h\u00e4n\u00b7de", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "und neue Scharen \u2013 und noch kein Ende . . .", "tokens": ["und", "neu\u00b7e", "Scha\u00b7ren", "\u2013", "und", "noch", "kein", "En\u00b7de", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "KON", "ADV", "PIAT", "NN", "$.", "$.", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Ein endloses Heer von Leidgenossen,", "tokens": ["Ein", "end\u00b7lo\u00b7ses", "Heer", "von", "Leid\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "vom Feste der Liebe ausgeschlossen!", "tokens": ["vom", "Fes\u00b7te", "der", "Lie\u00b7be", "aus\u00b7ge\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Und sieh: aus der Darbenden Reihen tritt", "tokens": ["Und", "sieh", ":", "aus", "der", "Dar\u00b7ben\u00b7den", "Rei\u00b7hen", "tritt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.12": {"line.1": {"text": "Ein K\u00f6nig erscheint er im Bettlergewand.", "tokens": ["Ein", "K\u00f6\u00b7nig", "er\u00b7scheint", "er", "im", "Bett\u00b7ler\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit ruhvollen Augen, mit segnender Hand", "tokens": ["Mit", "ruh\u00b7vol\u00b7len", "Au\u00b7gen", ",", "mit", "seg\u00b7nen\u00b7der", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.13": {"line.1": {"text": "\u2013 einen lichten Schein um das blonde Haar \u2013", "tokens": ["\u2013", "ei\u00b7nen", "lich\u00b7ten", "Schein", "um", "das", "blon\u00b7de", "Haar", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "f\u00fchrt er die blasse, hohl\u00e4ugige Schar", "tokens": ["f\u00fchrt", "er", "die", "blas\u00b7se", ",", "hoh\u00b7l\u00e4u\u00b7gi\u00b7ge", "Schar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.14": {"line.1": {"text": "durch die l\u00e4rmenden Stra\u00dfen, das Festtagsgebraus,", "tokens": ["durch", "die", "l\u00e4r\u00b7men\u00b7den", "Stra\u00b7\u00dfen", ",", "das", "Fest\u00b7tags\u00b7ge\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "vor ein s\u00e4ulengetragenes, f\u00fcrstliches Haus.", "tokens": ["vor", "ein", "s\u00e4u\u00b7len\u00b7ge\u00b7tra\u00b7ge\u00b7nes", ",", "f\u00fcrst\u00b7li\u00b7ches", "Haus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.15": {"line.1": {"text": "Durch die schimmernden Scheiben ins Dunkle bricht", "tokens": ["Durch", "die", "schim\u00b7mern\u00b7den", "Schei\u00b7ben", "ins", "Dunk\u00b7le", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "eine F\u00fclle von Glanz, eine F\u00fclle von Licht, \u2013", "tokens": ["ei\u00b7ne", "F\u00fcl\u00b7le", "von", "Glanz", ",", "ei\u00b7ne", "F\u00fcl\u00b7le", "von", "Licht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ART", "NN", "APPR", "NN", "$,", "$("], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.16": {"line.1": {"text": "und Kinderjubel und Weihnachtslieder", "tokens": ["und", "Kin\u00b7der\u00b7ju\u00b7bel", "und", "Weih\u00b7nachts\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "klingen aus leuchtender H\u00f6he nieder.", "tokens": ["klin\u00b7gen", "aus", "leuch\u00b7ten\u00b7der", "H\u00f6\u00b7he", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.17": {"line.1": {"text": "Vor den T\u00fcren die schenkenden Diener stehn:", "tokens": ["Vor", "den", "T\u00fc\u00b7ren", "die", "schen\u00b7ken\u00b7den", "Die\u00b7ner", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "\u00bbheut soll kein Bettler vor\u00fcbergehn . . .\u00ab", "tokens": ["\u00bb", "heut", "soll", "kein", "Bett\u00b7ler", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", ".", ".", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["$(", "ADV", "VMFIN", "PIAT", "NN", "VVINF", "$.", "$.", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.18": {"line.1": {"text": "Er aber b\u00fcckt sich mit stiller Geb\u00e4rde", "tokens": ["Er", "a\u00b7ber", "b\u00fcckt", "sich", "mit", "stil\u00b7ler", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und sammelt die Brocken von der Erde:", "tokens": ["und", "sam\u00b7melt", "die", "Bro\u00b7cken", "von", "der", "Er\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.19": {"line.1": {"text": "\u00bbihr Herren der Erde, ihr Reichen an Habe,", "tokens": ["\u00bb", "ihr", "Her\u00b7ren", "der", "Er\u00b7de", ",", "ihr", "Rei\u00b7chen", "an", "Ha\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "am Feste der Liebe ist ", "tokens": ["am", "Fes\u00b7te", "der", "Lie\u00b7be", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.20": {"line.1": {"text": "ein christlich Almosen, ein gn\u00e4dig Erbarmen \u2013", "tokens": ["ein", "christ\u00b7lich", "Al\u00b7mo\u00b7sen", ",", "ein", "gn\u00e4\u00b7dig", "Er\u00b7bar\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "ADJD", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "und ich suchte das Recht f\u00fcr die Aermsten der Armen", "tokens": ["und", "ich", "such\u00b7te", "das", "Recht", "f\u00fcr", "die", "A\u00b7erms\u00b7ten", "der", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}}, "stanza.21": {"line.1": {"text": "und die Liebe, die voll aus dem Vollen gibt,", "tokens": ["und", "die", "Lie\u00b7be", ",", "die", "voll", "aus", "dem", "Vol\u00b7len", "gibt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "die nicht w\u00e4gt und nicht rechnet, \u2013 die Liebe, die liebt!\u00ab", "tokens": ["die", "nicht", "w\u00e4gt", "und", "nicht", "rech\u00b7net", ",", "\u2013", "die", "Lie\u00b7be", ",", "die", "liebt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "KON", "PTKNEG", "VVFIN", "$,", "$(", "ART", "NN", "$,", "PRELS", "VVFIN", "$.", "$("], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.22": {"line.1": {"text": "Und wendet sich stumm und weicht von hinnen,", "tokens": ["Und", "wen\u00b7det", "sich", "stumm", "und", "weicht", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "KON", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wie fallende Nebel die Schatten zerrinnen . . .", "tokens": ["wie", "fal\u00b7len\u00b7de", "Ne\u00b7bel", "die", "Schat\u00b7ten", "zer\u00b7rin\u00b7nen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "VVINF", "$.", "$.", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.23": {"line.1": {"text": "Die Luft wird klar. Hoch im Zenit", "tokens": ["Die", "Luft", "wird", "klar", ".", "Hoch", "im", "Ze\u00b7nit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein sch\u00f6nheitschimmerndes Sternbild bl\u00fcht", "tokens": ["ein", "sch\u00f6n\u00b7heit\u00b7schim\u00b7mern\u00b7des", "Stern\u00b7bild", "bl\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.24": {"line.1": {"text": "und gie\u00dft auf das \u00e4rmste, verfallendste Haus", "tokens": ["und", "gie\u00dft", "auf", "das", "\u00e4rms\u00b7te", ",", "ver\u00b7fal\u00b7lends\u00b7te", "Haus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "die F\u00fclle himmlischer Strahlen aus.", "tokens": ["die", "F\u00fcl\u00b7le", "himm\u00b7li\u00b7scher", "Strah\u00b7len", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.25": {"line.1": {"te