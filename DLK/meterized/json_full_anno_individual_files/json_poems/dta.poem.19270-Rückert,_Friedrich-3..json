{"dta.poem.19270": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weltweisheit lehr' ich dich, nicht Weisheit dieser Welt,", "tokens": ["Welt\u00b7weis\u00b7heit", "lehr'", "ich", "dich", ",", "nicht", "Weis\u00b7heit", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "$,", "PTKNEG", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Weisheit, die zu gut nicht f\u00fcr die Welt sich h\u00e4lt;", "tokens": ["Doch", "Weis\u00b7heit", ",", "die", "zu", "gut", "nicht", "f\u00fcr", "die", "Welt", "sich", "h\u00e4lt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PTKA", "ADJD", "PTKNEG", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weltweisheit, die die Welt in allen Lebensweisen", "tokens": ["Welt\u00b7weis\u00b7heit", ",", "die", "die", "Welt", "in", "al\u00b7len", "Le\u00b7bens\u00b7wei\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir zur Belehrung will, zur Unterhaltung weisen;", "tokens": ["Dir", "zur", "Be\u00b7leh\u00b7rung", "will", ",", "zur", "Un\u00b7ter\u00b7hal\u00b7tung", "wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VMFIN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Weltweisheit, die nicht sich beweisen will der Welt,", "tokens": ["Welt\u00b7weis\u00b7heit", ",", "die", "nicht", "sich", "be\u00b7wei\u00b7sen", "will", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "PRF", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch in Beweisen vor der Welt sich wohlgef\u00e4llt;", "tokens": ["Noch", "in", "Be\u00b7wei\u00b7sen", "vor", "der", "Welt", "sich", "wohl\u00b7ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Weltweisheit, in der Welt Wegweiserdienst erweisend,", "tokens": ["Welt\u00b7weis\u00b7heit", ",", "in", "der", "Welt", "Weg\u00b7wei\u00b7ser\u00b7dienst", "er\u00b7wei\u00b7send", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mild unterweisend hier, dort streng, wo's gilt, verweisend.", "tokens": ["Mild", "un\u00b7ter\u00b7wei\u00b7send", "hier", ",", "dort", "streng", ",", "wo's", "gilt", ",", "ver\u00b7wei\u00b7send", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "ADV", "$,", "ADV", "VVFIN", "$,", "PWAV", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}