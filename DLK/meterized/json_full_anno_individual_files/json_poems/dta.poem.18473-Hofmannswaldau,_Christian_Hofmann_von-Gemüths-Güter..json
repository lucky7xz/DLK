{"dta.poem.18473": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gem\u00fcths-G\u00fcter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man kan in einem nun verliehren alle G\u00fcter;", "tokens": ["Man", "kan", "in", "ei\u00b7nem", "nun", "ver\u00b7lieh\u00b7ren", "al\u00b7le", "G\u00fc\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "ART", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts aber raubet mir den reichthum der gem\u00fcther.", "tokens": ["Nichts", "a\u00b7ber", "rau\u00b7bet", "mir", "den", "reicht\u00b7hum", "der", "ge\u00b7m\u00fc\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}