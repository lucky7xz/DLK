{"textgrid.poem.42407": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Erde dr\u00f6hnt von Trappen und Gestampf,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde dr\u00f6hnt von Trappen und Gestampf,", "tokens": ["Die", "Er\u00b7de", "dr\u00f6hnt", "von", "Trap\u00b7pen", "und", "Ge\u00b7stampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Regimenter ziehen in den Kampf.", "tokens": ["Die", "Re\u00b7gi\u00b7men\u00b7ter", "zie\u00b7hen", "in", "den", "Kampf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie ausgetretne Str\u00f6me tosend nahn,", "tokens": ["Wie", "aus\u00b7ge\u00b7tret\u00b7ne", "Str\u00f6\u00b7me", "to\u00b7send", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So \u00fcberfluten sie den weiten Plan.", "tokens": ["So", "\u00fc\u00b7berf\u00b7lu\u00b7ten", "sie", "den", "wei\u00b7ten", "Plan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Feind dringt zu Feind, zu Tausenden entl\u00e4sst", "tokens": ["Feind", "dringt", "zu", "Feind", ",", "zu", "Tau\u00b7sen\u00b7den", "ent\u00b7l\u00e4sst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die S\u00f6hne Nord und S\u00fcd und Ost und West.", "tokens": ["Die", "S\u00f6h\u00b7ne", "Nord", "und", "S\u00fcd", "und", "Ost", "und", "West", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Aufmarsch ohne Ende, \u2013 mehr und mehr \u2013", "tokens": ["Ein", "Auf\u00b7marsch", "oh\u00b7ne", "En\u00b7de", ",", "\u2013", "mehr", "und", "mehr", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "$(", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Mutterboden gibt sie willig her.", "tokens": ["Der", "Mut\u00b7ter\u00b7bo\u00b7den", "gibt", "sie", "wil\u00b7lig", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nun stockt der Fluss, der sich nach r\u00fcckw\u00e4rts ballt,", "tokens": ["Nun", "stockt", "der", "Fluss", ",", "der", "sich", "nach", "r\u00fcck\u00b7w\u00e4rts", "ballt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Ziel ist da, die Vordern machen halt.", "tokens": ["Das", "Ziel", "ist", "da", ",", "die", "Vor\u00b7dern", "ma\u00b7chen", "halt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Noch wogt und wimmelt es am Horizont,", "tokens": ["Noch", "wogt", "und", "wim\u00b7melt", "es", "am", "Ho\u00b7ri\u00b7zont", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch fest in Reih und Glied starrt schon die Front.", "tokens": ["Doch", "fest", "in", "Reih", "und", "Glied", "starrt", "schon", "die", "Front", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Schlacht beginnt, die Gr\u00e4ben sind verschanzt,", "tokens": ["Die", "Schlacht", "be\u00b7ginnt", ",", "die", "Gr\u00e4\u00b7ben", "sind", "ver\u00b7schanzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Feldzeichen und Gesch\u00fctze aufgepflanzt.", "tokens": ["Feld\u00b7zei\u00b7chen", "und", "Ge\u00b7sch\u00fct\u00b7ze", "auf\u00b7ge\u00b7pflanzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.8": {"line.1": {"text": "Dann streicht der Kugelregen durch die Flur", "tokens": ["Dann", "streicht", "der", "Ku\u00b7gel\u00b7re\u00b7gen", "durch", "die", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und streift die M\u00e4nner weg wie eine Schur.", "tokens": ["Und", "streift", "die", "M\u00e4n\u00b7ner", "weg", "wie", "ei\u00b7ne", "Schur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Kommandoruf ergeht von Korps zu Korps", "tokens": ["Kom\u00b7man\u00b7do\u00b7ruf", "er\u00b7geht", "von", "Korps", "zu", "Korps"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und neue Bataillone st\u00fcrmen vor,", "tokens": ["Und", "neu\u00b7e", "Ba\u00b7tail\u00b7lo\u00b7ne", "st\u00fcr\u00b7men", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Stehn einen Augenblick wie eine Wand,", "tokens": ["Stehn", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "wie", "ei\u00b7ne", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schon im n\u00e4chsten liegen sie im Sand.", "tokens": ["Und", "schon", "im", "n\u00e4chs\u00b7ten", "lie\u00b7gen", "sie", "im", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Hernach, wenn sich verzogen Marsch und Ritt", "tokens": ["Her\u00b7nach", ",", "wenn", "sich", "ver\u00b7zo\u00b7gen", "Marsch", "und", "Ritt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PRF", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geht Einer \u00fcbers Feld, wie nach dem Schnitt.", "tokens": ["Geht", "Ei\u00b7ner", "\u00fc\u00b7bers", "Feld", ",", "wie", "nach", "dem", "Schnitt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "$,", "PWAV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Sieht diese Ernte in den Staub gestreckt,", "tokens": ["Sieht", "die\u00b7se", "Ern\u00b7te", "in", "den", "Staub", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die wilden Garbenb\u00fcndel blutbedeckt,", "tokens": ["Die", "wil\u00b7den", "Gar\u00b7ben\u00b7b\u00fcn\u00b7del", "blut\u00b7be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Und wie ein Ackersmann, dem man zertrat", "tokens": ["Und", "wie", "ein", "A\u00b7ckers\u00b7mann", ",", "dem", "man", "zer\u00b7trat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die reifen Halme und zerschlug die Saat,", "tokens": ["Die", "rei\u00b7fen", "Hal\u00b7me", "und", "zer\u00b7schlug", "die", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Hebt er die H\u00e4nde schmerzgeballt und schwer", "tokens": ["Hebt", "er", "die", "H\u00e4n\u00b7de", "schmerz\u00b7ge\u00b7ballt", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie strafend hinter dem Verderber her", "tokens": ["Wie", "stra\u00b7fend", "hin\u00b7ter", "dem", "Ver\u00b7der\u00b7ber", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Und schl\u00e4gt sie vor die Augen dann voll Pein:", "tokens": ["Und", "schl\u00e4gt", "sie", "vor", "die", "Au\u00b7gen", "dann", "voll", "Pein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O Ackerland, wer macht dich wieder rein!", "tokens": ["O", "A\u00b7cker\u00b7land", ",", "wer", "macht", "dich", "wie\u00b7der", "rein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Die Erde dr\u00f6hnt von Trappen und Gestampf,", "tokens": ["Die", "Er\u00b7de", "dr\u00f6hnt", "von", "Trap\u00b7pen", "und", "Ge\u00b7stampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Regimenter ziehen in den Kampf.", "tokens": ["Die", "Re\u00b7gi\u00b7men\u00b7ter", "z