en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Des Reifes Flocken fing sie dann,", "tokens": ["Des", "Rei\u00b7fes", "Flo\u00b7cken", "fing", "sie", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die fallenden, unverdrossen", "tokens": ["Die", "fal\u00b7len\u00b7den", ",", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und bot mir die Juwelen an,", "tokens": ["Und", "bot", "mir", "die", "Ju\u00b7we\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die auf der Hand zerflossen.", "tokens": ["Die", "auf", "der", "Hand", "zer\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Drauf stieg der Nebel aus dem Tal,", "tokens": ["Drauf", "stieg", "der", "Ne\u00b7bel", "aus", "dem", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empor aus Flu\u00df und Weihern,", "tokens": ["Em\u00b7por", "aus", "Flu\u00df", "und", "Wei\u00b7hern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "i