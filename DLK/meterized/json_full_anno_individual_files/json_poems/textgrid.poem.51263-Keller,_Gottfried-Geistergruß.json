{"textgrid.poem.51263": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Geistergru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah ein holdes Weib im Traum", "tokens": ["Ich", "sah", "ein", "hol\u00b7des", "Weib", "im", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf rotem Laube sitzen", "tokens": ["Auf", "ro\u00b7tem", "Lau\u00b7be", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl unter einem bereiften Baum,", "tokens": ["Wohl", "un\u00b7ter", "ei\u00b7nem", "be\u00b7reif\u00b7ten", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der t\u00e4t wie Silber blitzen.", "tokens": ["Der", "t\u00e4t", "wie", "Sil\u00b7ber", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er blitzte wie Silber und Kristall", "tokens": ["Er", "blitz\u00b7te", "wie", "Sil\u00b7ber", "und", "Kris\u00b7tall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In lieblicher Wintersonne;", "tokens": ["In", "lieb\u00b7li\u00b7cher", "Win\u00b7ter\u00b7son\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Leis rauscht' der Wind, wie Demantenfall", "tokens": ["Leis", "rauscht'", "der", "Wind", ",", "wie", "De\u00b7man\u00b7ten\u00b7fall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "PWAV", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Perlt's von des Baumes Krone.", "tokens": ["Perlt's", "von", "des", "Bau\u00b7mes", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Und auch der Sch\u00f6nen wallendes Haar", "tokens": ["Und", "auch", "der", "Sch\u00f6\u00b7nen", "wal\u00b7len\u00b7des", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sah wei\u00df wie Schnee ich prangen;", "tokens": ["Sah", "wei\u00df", "wie", "Schnee", "ich", "pran\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KOKOM", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn ach, wie manches liebe Jahr", "tokens": ["Denn", "ach", ",", "wie", "man\u00b7ches", "lie\u00b7be", "Jahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PWAV", "PIS", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist schon ins Land gegangen!", "tokens": ["Ist", "schon", "ins", "Land", "ge\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch bl\u00fchte noch ihr Antlitz fein", "tokens": ["Doch", "bl\u00fch\u00b7te", "noch", "ihr", "Ant\u00b7litz", "fein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich wei\u00dfen Rosenauen,", "tokens": ["Gleich", "wei\u00b7\u00dfen", "Ro\u00b7se\u00b7nau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Aug der alte Sternenschein", "tokens": ["Im", "Aug", "der", "al\u00b7te", "Ster\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rot der Mund zu schauen.", "tokens": ["Und", "rot", "der", "Mund", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbwo kommst du her, wo gehst du hin?\u00ab", "tokens": ["\u00bb", "wo", "kommst", "du", "her", ",", "wo", "gehst", "du", "hin", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach ich mit sanftem Beben;", "tokens": ["Sprach", "ich", "mit", "sanf\u00b7tem", "Be\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbbist selig? Bist du B\u00fc\u00dferin?", "tokens": ["\u00bb", "bist", "se\u00b7lig", "?", "Bist", "du", "B\u00fc\u00b7\u00dfe\u00b7rin", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "$.", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo lebst du nun dein Leben?\u00ab", "tokens": ["Wo", "lebst", "du", "nun", "dein", "Le\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie l\u00e4chelte mild am selben Ort,", "tokens": ["Sie", "l\u00e4\u00b7chel\u00b7te", "mild", "am", "sel\u00b7ben", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auch hab ich sie nicken sehen;", "tokens": ["Auch", "hab", "ich", "sie", "ni\u00b7cken", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVINF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie sprach ein halb gehauchtes Wort,", "tokens": ["Sie", "sprach", "ein", "halb", "ge\u00b7hauch\u00b7tes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das konnt ich nicht verstehen.", "tokens": ["Das", "konnt", "ich", "nicht", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Des Reifes Flocken fing sie dann,", "tokens": ["Des", "Rei\u00b7fes", "Flo\u00b7cken", "fing", "sie", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die fallenden, unverdrossen", "tokens": ["Die", "fal\u00b7len\u00b7den", ",", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und bot mir die Juwelen an,", "tokens": ["Und", "bot", "mir", "die", "Ju\u00b7we\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die auf der Hand zerflossen.", "tokens": ["Die", "auf", "der", "Hand", "zer\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Drauf stieg der Nebel aus dem Tal,", "tokens": ["Drauf", "stieg", "der", "Ne\u00b7bel", "aus", "dem", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empor aus Flu\u00df und Weihern,", "tokens": ["Em\u00b7por", "aus", "Flu\u00df", "und", "Wei\u00b7hern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verh\u00e4ngend rasch des Waldes Saal", "tokens": ["Ver\u00b7h\u00e4n\u00b7gend", "rasch", "des", "Wal\u00b7des", "Saal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seinen dichten Schleiern.", "tokens": ["Mit", "sei\u00b7nen", "dich\u00b7ten", "Schlei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ich sah sie zwischen die B\u00e4ume hinein", "tokens": ["Ich", "sah", "sie", "zwi\u00b7schen", "die", "B\u00e4u\u00b7me", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APZR"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Tief in den Schatten gehen", "tokens": ["Tief", "in", "den", "Schat\u00b7ten", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und ihres Haares Silberschein", "tokens": ["Und", "ih\u00b7res", "Haa\u00b7res", "Sil\u00b7ber\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In D\u00fcsternis verwehen.", "tokens": ["In", "D\u00fcs\u00b7ter\u00b7nis", "ver\u00b7we\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Noch hat es hier, noch hat es dort", "tokens": ["Noch", "hat", "es", "hier", ",", "noch", "hat", "es", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Augenglanz gefunkelt;", "tokens": ["Wie", "Au\u00b7gen\u00b7glanz", "ge\u00b7fun\u00b7kelt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zuletzt war die Erscheinung fort", "tokens": ["Zu\u00b7letzt", "war", "die", "Er\u00b7schei\u00b7nung", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch der Traum verdunkelt.", "tokens": ["Und", "auch", "der", "Traum", "ver\u00b7dun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ich sah ein holdes Weib im Traum", "tokens": ["Ich", "sah", "ein", "hol\u00b7des", "Weib", "im", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf rotem Laube sitzen", "tokens": ["Auf", "ro\u00b7tem", "Lau\u00b7be", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl unter einem bereiften Baum,", "tokens": ["Wohl", "un\u00b7ter", "ei\u00b7nem", "be\u00b7reif\u00b7ten", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der t\u00e4t wie Silber blitzen.", "tokens": ["Der", "t\u00e4t", "wie", "Sil\u00b7ber", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er blitzte wie Silber und Kristall", "tokens": ["Er", "blitz\u00b7te", "wie", "Sil\u00b7ber", "und", "Kris\u00b7tall"], "token_info": ["word", "word", "word", "word", "word", "wor