{"dta.poem.5910": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn er von diesem Stoff will eine Welt formiren,", "tokens": ["Wenn", "er", "von", "die\u00b7sem", "Stoff", "will", "ei\u00b7ne", "Welt", "for\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Theilt er vermuhtlich ihn in Cubos ein,", "tokens": ["Theilt", "er", "ver\u00b7muht\u00b7lich", "ihn", "in", "Cu\u00b7bos", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die nicht von gleicher Art. Die Theilung zu vollf\u00fchren,", "tokens": ["Die", "nicht", "von", "glei\u00b7cher", "Art", ".", "Die", "Thei\u00b7lung", "zu", "voll\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "ADJA", "NN", "$.", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In lauter runde C\u00f6rperlein,", "tokens": ["In", "lau\u00b7ter", "run\u00b7de", "C\u00f6r\u00b7per\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird unbegreifflich seyn.", "tokens": ["Wird", "un\u00b7be\u00b7greif\u00b7flich", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wenn Kugeln bey einander liegen,", "tokens": ["Wenn", "Ku\u00b7geln", "bey", "ein\u00b7an\u00b7der", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So k\u00f6nnen sie, wie es bekannt,", "tokens": ["So", "k\u00f6n\u00b7nen", "sie", ",", "wie", "es", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PWAV", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich so nicht f\u00fcgen:", "tokens": ["Sich", "so", "nicht", "f\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Es wird der Raum nicht voll, es bleibt ein Zwischen-Stand.", "tokens": ["Es", "wird", "der", "Raum", "nicht", "voll", ",", "es", "bleibt", "ein", "Zwi\u00b7schen\u00b7Stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man stelle sich nun vor, da\u00df diese gantze Last", "tokens": ["Man", "stel\u00b7le", "sich", "nun", "vor", ",", "da\u00df", "die\u00b7se", "gant\u00b7ze", "Last"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "PTKVZ", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In einem Kreyse sich bewege,", "tokens": ["In", "ei\u00b7nem", "Krey\u00b7se", "sich", "be\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df von den Theilchen auch, ohn alle Rast,", "tokens": ["Da\u00df", "von", "den", "Theil\u00b7chen", "auch", ",", "ohn", "al\u00b7le", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "$,", "KOUI", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ein jedes ins besondere sich rege.", "tokens": ["Ein", "je\u00b7des", "ins", "be\u00b7son\u00b7de\u00b7re", "sich", "re\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "APPRART", "ADJA", "PRF", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "So wird, von doppelter Bewegung fort gerissen,", "tokens": ["So", "wird", ",", "von", "dop\u00b7pel\u00b7ter", "Be\u00b7we\u00b7gung", "fort", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "ADJA", "NN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das Gantze ja dem Druck des Circkels folgen m\u00fcssen.", "tokens": ["Das", "Gant\u00b7ze", "ja", "dem", "Druck", "des", "Cir\u00b7ckels", "fol\u00b7gen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ein kleiner Cubus nun bricht an den Ecken:", "tokens": ["Ein", "klei\u00b7ner", "Cu\u00b7bus", "nun", "bricht", "an", "den", "E\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.17": {"text": "Da solche Ecken nun den andern an der Seiten", "tokens": ["Da", "sol\u00b7che", "E\u00b7cken", "nun", "den", "an\u00b7dern", "an", "der", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "ADV", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Best\u00e4ndig hin und wieder gleiten;", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "hin", "und", "wie\u00b7der", "glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "So kans vermuhtlich sich nicht anders finden,", "tokens": ["So", "kans", "ver\u00b7muht\u00b7lich", "sich", "nicht", "an\u00b7ders", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "PRF", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Es mu\u00df der Cubus sich abstossen, \u00e4ndern, r\u00fcnden.", "tokens": ["Es", "mu\u00df", "der", "Cu\u00b7bus", "sich", "abs\u00b7tos\u00b7sen", ",", "\u00e4n\u00b7dern", ",", "r\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wann nun in der Natur", "tokens": ["Wann", "nun", "in", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Sich die geringste Spur", "tokens": ["Sich", "die", "ge\u00b7rings\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.24": {"text": "Wie diese Meinung ja vermuhtlich fest,", "tokens": ["Wie", "die\u00b7se", "Mei\u00b7nung", "ja", "ver\u00b7muht\u00b7lich", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}