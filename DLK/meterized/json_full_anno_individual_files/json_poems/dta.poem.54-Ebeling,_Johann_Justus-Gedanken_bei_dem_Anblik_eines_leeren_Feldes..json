{"dta.poem.54": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken  \n bei  \n dem Anblik eines leeren Feldes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zur holden Fr\u00fchlings-Zeit war hier\nnoch alles gr\u00fcn,", "tokens": ["Zur", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7Zeit", "war", "hier", "noch", "al\u00b7les", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sommer kam heran da ward", "tokens": ["Der", "Som\u00b7mer", "kam", "he\u00b7ran", "da", "ward"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In scheinend Gold verkehrt, das", "tokens": ["In", "schei\u00b7nend", "Gold", "ver\u00b7kehrt", ",", "das"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJD", "NN", "VVPP", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit seinen gelben Schmuk im Reif-", "tokens": ["Mit", "sei\u00b7nen", "gel\u00b7ben", "Schmuk", "im", "Reif"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPRART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So kam der Schnitter an mit seinem scharffen", "tokens": ["So", "kam", "der", "Schnit\u00b7ter", "an", "mit", "sei\u00b7nem", "scharf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und hat den Halm zerhaut und seine Pracht zer-", "tokens": ["Und", "hat", "den", "Halm", "zer\u00b7haut", "und", "sei\u00b7ne", "Pracht", "zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "KON", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Wagen folgten nach, als sie ins Seil gebun-", "tokens": ["Die", "Wa\u00b7gen", "folg\u00b7ten", "nach", ",", "als", "sie", "ins", "Seil", "ge\u00b7bun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das Feld ist nunmehr leer, als wenn die Frucht ver-", "tokens": ["Das", "Feld", "ist", "nun\u00b7mehr", "leer", ",", "als", "wenn", "die", "Frucht", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "KOKOM", "KOUS", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Stoppeln sind noch da, die mit der Zeit ver-", "tokens": ["Die", "Stop\u00b7peln", "sind", "noch", "da", ",", "die", "mit", "der", "Zeit", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "So ist vom Jahres Wuchs im Felde nichts zusehn.", "tokens": ["So", "ist", "vom", "Jah\u00b7res", "Wuchs", "im", "Fel\u00b7de", "nichts", "zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "NN", "APPRART", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es wird mit andern Korn, das Feld als neu bes\u00e4et,", "tokens": ["Es", "wird", "mit", "an\u00b7dern", "Korn", ",", "das", "Feld", "als", "neu", "be\u00b7s\u00e4et", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,", "ART", "NN", "KOKOM", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das aus den vorgen stammt, und wiederum auf-", "tokens": ["Das", "aus", "den", "vor\u00b7gen", "stammt", ",", "und", "wie\u00b7de\u00b7rum", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "VVFIN", "$,", "KON", "ADV", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Mir deucht ich sehe hier an diesem Akkerfeld", "tokens": ["Mir", "deucht", "ich", "se\u00b7he", "hier", "an", "die\u00b7sem", "Ak\u00b7ker\u00b7feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein Bildnis r\u00fchrend an, von der bewohnten Welt.", "tokens": ["Ein", "Bild\u00b7nis", "r\u00fch\u00b7rend", "an", ",", "von", "der", "be\u00b7wohn\u00b7ten", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Menschen die gebohrn, die fangen an zu bl\u00fc-", "tokens": ["Die", "Men\u00b7schen", "die", "ge\u00b7bohrn", ",", "die", "fan\u00b7gen", "an", "zu", "bl\u00fc"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Zeit verl\u00e4uft geschwind, da sie von dannen ziehen,", "tokens": ["Die", "Zeit", "ver\u00b7l\u00e4uft", "ge\u00b7schwind", ",", "da", "sie", "von", "dan\u00b7nen", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nachdem sie reif genug; und ihre Zeit bestimmt,", "tokens": ["Nach\u00b7dem", "sie", "reif", "ge\u00b7nug", ";", "und", "ih\u00b7re", "Zeit", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "$.", "KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da kommt der Todt heran, der sie von hinnen nimmt.", "tokens": ["Da", "kommt", "der", "Todt", "he\u00b7ran", ",", "der", "sie", "von", "hin\u00b7nen", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "In einen Lauf der Zeit, von zweimahl funfzig Jahren,", "tokens": ["In", "ei\u00b7nen", "Lauf", "der", "Zeit", ",", "von", "zwei\u00b7mahl", "funf\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sind keine \u00fcbrig mehr; wir sind schon weggefahren.", "tokens": ["Sind", "kei\u00b7ne", "\u00fcb\u00b7rig", "mehr", ";", "wir", "sind", "schon", "weg\u00b7ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJD", "ADV", "$.", "PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Saame der von uns in dieser Zeit entspriest,", "tokens": ["Der", "Saa\u00b7me", "der", "von", "uns", "in", "die\u00b7ser", "Zeit", "ent\u00b7spriest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der bl\u00fchet wieder auf, und wenn man uns vergi\u00dft,", "tokens": ["Der", "bl\u00fc\u00b7het", "wie\u00b7der", "auf", ",", "und", "wenn", "man", "uns", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So ist an solchen stets doch auf der Welt zu lesen,", "tokens": ["So", "ist", "an", "sol\u00b7chen", "stets", "doch", "auf", "der", "Welt", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "ADV", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df wir hier auch gebl\u00fcht, und einmahl da gewesen.", "tokens": ["Da\u00df", "wir", "hier", "auch", "ge\u00b7bl\u00fcht", ",", "und", "ein\u00b7mahl", "da", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$,", "KON", "ADV", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und solte solcher gleich in seiner Bl\u00fct verbl\u00fchn,", "tokens": ["Und", "sol\u00b7te", "sol\u00b7cher", "gleich", "in", "sei\u00b7ner", "Bl\u00fct", "ver\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Eh er zur Reiffe kommt, auch dieser Welt entfliehn:", "tokens": ["Eh", "er", "zur", "Reif\u00b7fe", "kommt", ",", "auch", "die\u00b7ser", "Welt", "ent\u00b7fliehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADV", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So ist das unser Trost: wir k\u00f6nnen nicht vergehen,", "tokens": ["So", "ist", "das", "un\u00b7ser", "Trost", ":", "wir", "k\u00f6n\u00b7nen", "nicht", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wir werden wiederum dereinsten auferstehen.", "tokens": ["Wir", "wer\u00b7den", "wie\u00b7de\u00b7rum", "de\u00b7reins\u00b7ten", "auf\u00b7er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}