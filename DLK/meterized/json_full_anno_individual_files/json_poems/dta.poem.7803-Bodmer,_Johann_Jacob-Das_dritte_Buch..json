{"dta.poem.7803": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Buch.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der David als er h\u00f6rt was Michal von ihm singet,", "tokens": ["Der", "Da\u00b7vid", "als", "er", "h\u00f6rt", "was", "Mic\u00b7hal", "von", "ihm", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KOUS", "PPER", "VVFIN", "PWS", "NE", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denckt: Ein so grosses Lob man unverdient ihm bringet;", "tokens": ["Denckt", ":", "Ein", "so", "gros\u00b7ses", "Lob", "man", "un\u00b7ver\u00b7di\u00b7ent", "ihm", "brin\u00b7get", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ART", "ADV", "ADJA", "NN", "PIS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er neigt sich auf dem Pferd, schaut ehrerbietig hin,", "tokens": ["Er", "neigt", "sich", "auf", "dem", "Pferd", ",", "schaut", "ehr\u00b7er\u00b7bie\u00b7tig", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allwo er stehen sieht die sch\u00f6ne Printze\u00dfin.", "tokens": ["All\u00b7wo", "er", "ste\u00b7hen", "sieht", "die", "sch\u00f6\u00b7ne", "Print\u00b7ze\u00b7\u00dfin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie gr\u00fcst ihn ebenfalls mit lieblichen Geb\u00e4hrden,", "tokens": ["Sie", "gr\u00fcst", "ihn", "e\u00b7ben\u00b7falls", "mit", "lieb\u00b7li\u00b7chen", "Ge\u00b7b\u00e4hr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von j", "tokens": ["Von", "j"], "token_info": ["word", "word"], "pos": ["APPR", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Sie hatte diesen Tag sehr herrlich sich geziert,", "tokens": ["Sie", "hat\u00b7te", "die\u00b7sen", "Tag", "sehr", "herr\u00b7lich", "sich", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "ADV", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ob sonst wohl die Natur den meisten Wunder f\u00fchrt,", "tokens": ["Ob", "sonst", "wohl", "die", "Na\u00b7tur", "den", "meis\u00b7ten", "Wun\u00b7der", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schien sie doch heute mehr, denn ihr vergn\u00fcgtes Wesen", "tokens": ["Schien", "sie", "doch", "heu\u00b7te", "mehr", ",", "denn", "ihr", "ver\u00b7gn\u00fcg\u00b7tes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hatt alle Lieblichkeit in ihr Gesicht erlesen.", "tokens": ["Hatt", "al\u00b7le", "Lieb\u00b7lich\u00b7keit", "in", "ihr", "Ge\u00b7sicht", "er\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Stirn war heller Schnee, die Wangen Milch und Blut,", "tokens": ["Die", "Stirn", "war", "hel\u00b7ler", "Schnee", ",", "die", "Wan\u00b7gen", "Milch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Augen schwartz Agat, und sahen aus so gut", "tokens": ["Die", "Au\u00b7gen", "schwartz", "A\u00b7gat", ",", "und", "sa\u00b7hen", "aus", "so", "gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NE", "$,", "KON", "VVFIN", "APPR", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "De\u00df man sie ohne Furcht verehren konnt und preisen;", "tokens": ["De\u00df", "man", "sie", "oh\u00b7ne", "Furcht", "ver\u00b7eh\u00b7ren", "konnt", "und", "prei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PPER", "APPR", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Mund war hell Corall, der Hals und Brust sich weisen", "tokens": ["Der", "Mund", "war", "hell", "Co\u00b7rall", ",", "der", "Hals", "und", "Brust", "sich", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$,", "ART", "NN", "KON", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dem Marmor gleich am Schein. Das schwarze Lockenhaar,", "tokens": ["Dem", "Mar\u00b7mor", "gleich", "am", "Schein", ".", "Das", "schwar\u00b7ze", "Lo\u00b7cken\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dem hellen Angesicht zum Absatz dienlich war;", "tokens": ["Dem", "hel\u00b7len", "An\u00b7ge\u00b7sicht", "zum", "Ab\u00b7satz", "dien\u00b7lich", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das sie frey fliegen lie\u00df zum Spiel den sanfften Winden,", "tokens": ["Das", "sie", "frey", "flie\u00b7gen", "lie\u00df", "zum", "Spiel", "den", "sanff\u00b7ten", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VVINF", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die sich mit jeder Lock begunten zu verbinden.", "tokens": ["Die", "sich", "mit", "je\u00b7der", "Lock", "be\u00b7gun\u00b7ten", "zu", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PIAT", "NN", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein reicher Perlenkrantz mit Rosen durchgesteckt,", "tokens": ["Ein", "rei\u00b7cher", "Per\u00b7len\u00b7krantz", "mit", "Ro\u00b7sen", "durch\u00b7ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das Haupt und ihre Stirn zum halben Theil bedeckt.", "tokens": ["Das", "Haupt", "und", "ih\u00b7re", "Stirn", "zum", "hal\u00b7ben", "Theil", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie trug ein weisses Kleid mit Diamanten Sternen,", "tokens": ["Sie", "trug", "ein", "weis\u00b7ses", "Kleid", "mit", "Di\u00b7a\u00b7man\u00b7ten", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Selbst must des Himmels Schmuk hiebey sich bald entfernen;", "tokens": ["Selbst", "must", "des", "Him\u00b7mels", "Schmuk", "hie\u00b7bey", "sich", "bald", "ent\u00b7fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN", "ADV", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein rosinfarb Gewand fiel von den Achseln hin;", "tokens": ["Ein", "ro\u00b7sin\u00b7farb", "Ge\u00b7wand", "fiel", "von", "den", "Ach\u00b7seln", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das Niederkleid war bunt, sie hatte selbst darinn", "tokens": ["Das", "Nie\u00b7der\u00b7kleid", "war", "bunt", ",", "sie", "hat\u00b7te", "selbst", "da\u00b7rinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADV", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Gewirckt mit ihrer hand. So ware sie geschm\u00fccket,", "tokens": ["Ge\u00b7wirckt", "mit", "ih\u00b7rer", "hand", ".", "So", "wa\u00b7re", "sie", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jhr Schein und Lieblichkeit gantz Jsrael entz\u00fccket.", "tokens": ["Ihr", "Schein", "und", "Lieb\u00b7lich\u00b7keit", "gantz", "Js\u00b7rael", "ent\u00b7z\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Maacha war die n\u00e4chst die Michal so begleit,", "tokens": ["Maa\u00b7cha", "war", "die", "n\u00e4chst", "die", "Mic\u00b7hal", "so", "be\u00b7gleit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.28": {"text": "Voll Anmuth gleich wie sie. An ihrer andern Seit", "tokens": ["Voll", "An\u00b7muth", "gleich", "wie", "sie", ".", "An", "ih\u00b7rer", "an\u00b7dern", "Seit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "ADV", "KOKOM", "PPER", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.29": {"text": "Die Rizpa war zu sehn. Drauf aus dem gantzen Lande", "tokens": ["Die", "Riz\u00b7pa", "war", "zu", "sehn", ".", "Drauf", "aus", "dem", "gant\u00b7zen", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKZU", "VVINF", "$.", "PAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Des Stammes Benjamin, nach eines jeden Stande", "tokens": ["Des", "Stam\u00b7mes", "Ben\u00b7ja\u00b7min", ",", "nach", "ei\u00b7nes", "je\u00b7den", "Stan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Erschien der Dirnen Schaar; nur Merob nicht allein", "tokens": ["Er\u00b7schien", "der", "Dir\u00b7nen", "Schaar", ";", "nur", "Me\u00b7rob", "nicht", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "NN", "$.", "ADV", "NE", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die sand sich nirgends nicht und deckte ihren Schein.", "tokens": ["Die", "sand", "sich", "nir\u00b7gends", "nicht", "und", "deck\u00b7te", "ih\u00b7ren", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "PTKNEG", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}