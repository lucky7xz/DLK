{"dta.poem.18617": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n dir.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Die Warheit kan zwar nicht zu meinen Ruhme setzen/", "tokens": ["Die", "War\u00b7heit", "kan", "zwar", "nicht", "zu", "mei\u00b7nen", "Ruh\u00b7me", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ein galanter Sinn hier was galantes liest;", "tokens": ["Da\u00df", "ein", "ga\u00b7lan\u00b7ter", "Sinn", "hier", "was", "ga\u00b7lan\u00b7tes", "liest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "PWS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jedoch ich will es selbst vor unvergleichlich sch\u00e4tzen/", "tokens": ["Je\u00b7doch", "ich", "will", "es", "selbst", "vor", "un\u00b7ver\u00b7gleich\u00b7lich", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "ADV", "APPR", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn es das Gl\u00fccke hat/ und dir gef\u00e4llig ist.", "tokens": ["Wenn", "es", "das", "Gl\u00fc\u00b7cke", "hat", "/", "und", "dir", "ge\u00b7f\u00e4l\u00b7lig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$(", "KON", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}