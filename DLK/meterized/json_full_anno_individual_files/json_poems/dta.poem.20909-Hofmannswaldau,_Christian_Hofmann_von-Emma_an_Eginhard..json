in.", "tokens": ["Wann", "uns", "ent\u00b7zo\u00b7gen", "ist", "der", "klah\u00b7re", "Ta\u00b7ges", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.123": {"text": "Jtzt schmeck ich allbereit die hochgew\u00fcnschten Stun-", "tokens": ["Jtzt", "schmeck", "ich", "all\u00b7be\u00b7reit", "die", "hoch\u00b7ge\u00b7w\u00fcnschten", "Stun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.124": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.125": {"text": "Ach Sonne s\u00e4ume nicht und ende deinen Lauff/", "tokens": ["Ach", "Son\u00b7ne", "s\u00e4u\u00b7me", "nicht", "und", "en\u00b7de", "dei\u00b7nen", "Lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "PTKNEG", "KON", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.126": {"text": "Du weist ja wie mir ist/ du hast es auch empfunden/", "tokens": ["Du", "weist", "ja", "wie", "mir", "ist", "/", "du", "hast", "es", "auch", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "PPER", "VAFIN", "$(", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.127": {"text": "Mein Brieflein schlie\u00df ich zu und meine Cammer", "tokens": ["Mein", "Brie\u00b7flein", "schlie\u00df", "ich", "zu", "und", "mei\u00b7ne", "Cam\u00b7mer"], "token_info": ["word", "wor