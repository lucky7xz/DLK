{"dta.poem.20909": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Emma an Eginhard.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r Ich/ mein Eginhard/ was Ich zuvor ge-", "tokens": ["W\u00e4r", "Ich", "/", "mein", "E\u00b7gin\u00b7hard", "/", "was", "Ich", "zu\u00b7vor", "ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$(", "PPOSAT", "NE", "$(", "PWS", "PPER", "ADV", "TRUNC"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "wesen/", "tokens": ["we\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und m\u00fcst Ich nicht itzund in Brand und", "tokens": ["Und", "m\u00fcst", "Ich", "nicht", "it\u00b7zund", "in", "Brand", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "KON"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Banden stehn/", "tokens": ["Ban\u00b7den", "stehn", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "So soltest du ein Wort von meinen H\u00e4nden lesen/", "tokens": ["So", "sol\u00b7test", "du", "ein", "Wort", "von", "mei\u00b7nen", "H\u00e4n\u00b7den", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das auch dem Donner w\u00fcrd an W\u00fcrckung gleiche", "tokens": ["Das", "auch", "dem", "Don\u00b7ner", "w\u00fcrd", "an", "W\u00fcr\u00b7ckung", "glei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN", "VAFIN", "APPR", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "gehn;", "tokens": ["gehn", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Ich schriebe: kahler Knecht/ dein Hals ist nun ver-", "tokens": ["Ich", "schrie\u00b7be", ":", "kah\u00b7ler", "Knecht", "/", "dein", "Hals", "ist", "nun", "ver"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADJA", "NN", "$(", "PPOSAT", "NN", "VAFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "lohren/", "tokens": ["loh\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Was Purpur fleckigt macht/ das f\u00e4llt de\u0303 Tod anheim/", "tokens": ["Was", "Pur\u00b7pur", "flec\u00b7kigt", "macht", "/", "das", "f\u00e4llt", "d\u1ebd", "Tod", "an\u00b7heim", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "VVFIN", "$(", "PDS", "VVFIN", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es hat des Himmels Schlu\u00df zum Feuer dich erkohren/", "tokens": ["Es", "hat", "des", "Him\u00b7mels", "Schlu\u00df", "zum", "Feu\u00b7er", "dich", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "APPRART", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vor Wespen/ gleich wie du/ ist nicht mein Honigseim;", "tokens": ["Vor", "Wes\u00b7pen", "/", "gleich", "wie", "du", "/", "ist", "nicht", "mein", "Ho\u00b7ni\u00b7gseim", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ADV", "KOKOM", "PPER", "$(", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was aus dem Scepter sprost/ das soll kein Knecht ent-", "tokens": ["Was", "aus", "dem", "Scep\u00b7ter", "sprost", "/", "das", "soll", "kein", "Knecht", "ent"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$(", "PDS", "VMFIN", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "f\u00fchren/", "tokens": ["f\u00fch\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Und Keyser Kronen seyn vor deinen Garten nicht/", "tokens": ["Und", "Key\u00b7ser", "Kro\u00b7nen", "seyn", "vor", "dei\u00b7nen", "Gar\u00b7ten", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VAINF", "APPR", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du solt des Keysers Brief/ doch nicht sein Kind be-", "tokens": ["Du", "solt", "des", "Key\u00b7sers", "Brief", "/", "doch", "nicht", "sein", "Kind", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "$(", "ADV", "PTKNEG", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "r\u00fchren/", "tokens": ["r\u00fch\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Es mu\u00df was h\u00f6hers seyn/ so hier ein Siegel bricht.", "tokens": ["Es", "mu\u00df", "was", "h\u00f6\u00b7hers", "seyn", "/", "so", "hier", "ein", "Sie\u00b7gel", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PWS", "ADJA", "VAINF", "$(", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Auff dieser hohen Bahn wirstu den Todt erjagen/", "tokens": ["Auff", "die\u00b7ser", "ho\u00b7hen", "Bahn", "wirs\u00b7tu", "den", "Todt", "er\u00b7ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn Wachs zur Sonne kompt/ so wird es bald ver-", "tokens": ["Wenn", "Wachs", "zur", "Son\u00b7ne", "kompt", "/", "so", "wird", "es", "bald", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPRART", "NN", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "zehrt/", "tokens": ["zehrt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Die Hoffnung die du hast/ soll dich zu Grabe tragen/", "tokens": ["Die", "Hoff\u00b7nung", "die", "du", "hast", "/", "soll", "dich", "zu", "Gra\u00b7be", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VAFIN", "$(", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Auch nur ein Traum davon ist aller Hencker werth.", "tokens": ["Auch", "nur", "ein", "Traum", "da\u00b7von", "ist", "al\u00b7ler", "Hen\u00b7cker", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PAV", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Des Keysers Schreiber soll des Keysers Tochter", "tokens": ["Des", "Key\u00b7sers", "Schrei\u00b7ber", "soll", "des", "Key\u00b7sers", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "k\u00fcssen/", "tokens": ["k\u00fcs\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Wie/ le\u00df ich? schlaf ich halb? wer irrt? ich oder du?", "tokens": ["Wie", "/", "le\u00df", "ich", "?", "schlaf", "ich", "halb", "?", "wer", "irrt", "?", "ich", "o\u00b7der", "du", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ADJD", "$.", "PWS", "VVFIN", "$.", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Des K\u00f6nigs Farbe soll mit Ru\u00df gemischt seyn m\u00fcs-", "tokens": ["Des", "K\u00f6\u00b7nigs", "Far\u00b7be", "soll", "mit", "Ru\u00df", "ge\u00b7mischt", "seyn", "m\u00fcs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VMFIN", "APPR", "NN", "VVPP", "VAINF", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "sen/", "tokens": ["sen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.29": {"text": "Da\u00df lasse Gott und auch mein Vater doch nicht zu.", "tokens": ["Da\u00df", "las\u00b7se", "Gott", "und", "auch", "mein", "Va\u00b7ter", "doch", "nicht", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "ADV", "PPOSAT", "NN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein Mensch/ der nicht zuwohl darf seinen Anfang nen-", "tokens": ["Ein", "Mensch", "/", "der", "nicht", "zu\u00b7wohl", "darf", "sei\u00b7nen", "An\u00b7fang", "nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "PTKNEG", "ADV", "VMFIN", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.31": {"text": "nen", "tokens": ["nen"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.32": {"text": "Und der mehr Dint\u2019 als Bluth vor uns vergossen hat/", "tokens": ["Und", "der", "mehr", "Dint'", "als", "Bluth", "vor", "uns", "ver\u00b7gos\u00b7sen", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "KOUS", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Soll gegen mich/ O Spott/ in Liebesbrunst entbrennen/", "tokens": ["Soll", "ge\u00b7gen", "mich", "/", "O", "Spott", "/", "in", "Lie\u00b7bes\u00b7brunst", "ent\u00b7bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "$(", "NE", "NN", "$(", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Seyn Folltern auch genug vor solche Frevelthat?", "tokens": ["Seyn", "Foll\u00b7tern", "auch", "ge\u00b7nug", "vor", "sol\u00b7che", "Fre\u00b7vel\u00b7that", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es m\u00fc\u00df ein schn\u00f6des Beil dir deinen Hals zuschmeis-", "tokens": ["Es", "m\u00fc\u00df", "ein", "schn\u00f6\u00b7des", "Beil", "dir", "dei\u00b7nen", "Hals", "zu\u00b7schmeis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PPER", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "sen/", "tokens": ["sen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.37": {"text": "Es rei\u00df ein kalter Stahl den heissen F\u00fcrsatz ein/", "tokens": ["Es", "rei\u00df", "ein", "kal\u00b7ter", "Stahl", "den", "heis\u00b7sen", "F\u00fcr\u00b7satz", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dann wolt\u2019 ich deinen Brief in tausend St\u00fccken reis-", "tokens": ["Dann", "wolt'", "ich", "dei\u00b7nen", "Brief", "in", "tau\u00b7send", "St\u00fc\u00b7cken", "reis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "CARD", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "sen/", "tokens": ["sen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.40": {"text": "Und sagen/ Eginhard mu\u00df auch zurissen seyn.", "tokens": ["Und", "sa\u00b7gen", "/", "E\u00b7gin\u00b7hard", "mu\u00df", "auch", "zu\u00b7ris\u00b7sen", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "NE", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So schrieb ich/ Eginhard/ w\u00e4r ich noch ungebunden/", "tokens": ["So", "schrieb", "ich", "/", "E\u00b7gin\u00b7hard", "/", "w\u00e4r", "ich", "noch", "un\u00b7ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NE", "$(", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Nach dem ich aber Magd/ ja Sclavin worden bin/", "tokens": ["Nach", "dem", "ich", "a\u00b7ber", "Magd", "/", "ja", "Scla\u00b7vin", "wor\u00b7den", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "NN", "$(", "ADV", "NN", "VAPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und mich das s\u00fcsse Garn der Liebe hat \u00fcmbwunden/", "tokens": ["Und", "mich", "das", "s\u00fcs\u00b7se", "Garn", "der", "Lie\u00b7be", "hat", "\u00fcm\u00b7bwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So nimm von meiner Faust die schlechten W\u00f6rter hin.", "tokens": ["So", "nimm", "von", "mei\u00b7ner", "Faust", "die", "schlech\u00b7ten", "W\u00f6r\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ich bin itzt hochbest\u00fcrtzt mein Feuer zu entdecken/", "tokens": ["Ich", "bin", "itzt", "hoch\u00b7be\u00b7st\u00fcrtzt", "mein", "Feu\u00b7er", "zu", "ent\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Doch wahre Liebes Brunst ist voll Verr\u00e4therey/", "tokens": ["Doch", "wah\u00b7re", "Lie\u00b7bes", "Brunst", "ist", "voll", "Ver\u00b7r\u00e4\u00b7the\u00b7rey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "VAFIN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und konte dein Gesicht hier diesen Brand erwecken/", "tokens": ["Und", "kon\u00b7te", "dein", "Ge\u00b7sicht", "hier", "die\u00b7sen", "Brand", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "ADV", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "So wei\u00df ich nicht/ was dir mehr zu verh\u00f6len sey.", "tokens": ["So", "wei\u00df", "ich", "nicht", "/", "was", "dir", "mehr", "zu", "ver\u00b7h\u00f6\u00b7len", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPE