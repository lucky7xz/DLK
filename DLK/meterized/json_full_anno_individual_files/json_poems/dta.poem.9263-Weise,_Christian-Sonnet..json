{"dta.poem.9263": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.85", "id:0.14"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Wo ist dasselbe land/ das gar kein laster kennt/", "tokens": ["Wo", "ist", "das\u00b7sel\u00b7be", "land", "/", "das", "gar", "kein", "las\u00b7ter", "kennt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PDAT", "NN", "$(", "PDS", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Deutsche s\u00e4uft zuviel/ und l\u00e4st sich leicht betr\u00fcgen/", "tokens": ["Der", "Deut\u00b7sche", "s\u00e4uft", "zu\u00b7viel", "/", "und", "l\u00e4st", "sich", "leicht", "be\u00b7tr\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$(", "KON", "VVFIN", "PRF", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der stoltze Spanier mu\u00df stehlen oder l\u00fcgen/", "tokens": ["Der", "stolt\u00b7ze", "Spa\u00b7nier", "mu\u00df", "steh\u00b7len", "o\u00b7der", "l\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "KON", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Welsche kennt sich nicht wenn er vor eifer brennt/", "tokens": ["Der", "Wel\u00b7sche", "kennt", "sich", "nicht", "wenn", "er", "vor", "ei\u00b7fer", "brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "KOUS", "PPER", "APPR", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der leichte Frantzman ist durch eitelkeit verblendt/", "tokens": ["Der", "leich\u00b7te", "Frantz\u00b7man", "ist", "durch", "ei\u00b7tel\u00b7keit", "ver\u00b7blendt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Pohle liebt die pracht und mu\u00df wohl selber pfl\u00fcgen/", "tokens": ["Der", "Poh\u00b7le", "liebt", "die", "pracht", "und", "mu\u00df", "wohl", "sel\u00b7ber", "pfl\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der rauhe T\u00fcrck ist falsch und grausam in den kriegen/", "tokens": ["Der", "rau\u00b7he", "T\u00fcrck", "ist", "falsch", "und", "grau\u00b7sam", "in", "den", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Engelland ist leicht der gottes-dienst getrennt.", "tokens": ["In", "En\u00b7gel\u00b7land", "ist", "leicht", "der", "got\u00b7tes\u00b7dienst", "ge\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Derhalben ziehe sich ein ieder bey der nase/", "tokens": ["Der\u00b7hal\u00b7ben", "zie\u00b7he", "sich", "ein", "ie\u00b7der", "bey", "der", "na\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ART", "PIS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem er an ein volck die l\u00e4ster-zunge setzt/", "tokens": ["In\u00b7dem", "er", "an", "ein", "volck", "die", "l\u00e4s\u00b7ter\u00b7zun\u00b7ge", "setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer wei\u00df wer seinen zahn hingegen wieder wetzt", "tokens": ["Wer", "wei\u00df", "wer", "sei\u00b7nen", "zahn", "hin\u00b7ge\u00b7gen", "wie\u00b7der", "wetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PWS", "PPOSAT", "NN", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der ihn bezahlen kan. In unserm Teutschen glase", "tokens": ["Der", "ihn", "be\u00b7zah\u00b7len", "kan", ".", "In", "un\u00b7serm", "Teut\u00b7schen", "gla\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steht nicht nur bier und wein/ es quillt der tapffre muth", "tokens": ["Steht", "nicht", "nur", "bier", "und", "wein", "/", "es", "quillt", "der", "tapf\u00b7fre", "muth"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJD", "KON", "NN", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der andre schamroth macht/ und rechte thaten thut.", "tokens": ["Der", "and\u00b7re", "scham\u00b7roth", "macht", "/", "und", "rech\u00b7te", "tha\u00b7ten", "thut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "$(", "KON", "ADJA", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}