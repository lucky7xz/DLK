{"textgrid.poem.52661": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Frage nicht", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Roth ist Roth und Lieb ist Liebe nur \u2013", "tokens": ["Roth", "ist", "Roth", "und", "Lieb", "ist", "Lie\u00b7be", "nur", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "NN", "VAFIN", "NN", "ADV", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "St\u00fcrmt ihr auch zu alles Denkens Giebeln,", "tokens": ["St\u00fcrmt", "ihr", "auch", "zu", "al\u00b7les", "Den\u00b7kens", "Gie\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unsagbar, was wollt ihr weiter gr\u00fcbeln,", "tokens": ["Un\u00b7sag\u00b7bar", ",", "was", "wollt", "ihr", "wei\u00b7ter", "gr\u00fc\u00b7beln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Freunde, sind die Thaten der Natur.", "tokens": ["Freun\u00b7de", ",", "sind", "die", "Tha\u00b7ten", "der", "Na\u00b7tur", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Singe von der Liebe Herrlichkeit!", "tokens": ["Sin\u00b7ge", "von", "der", "Lie\u00b7be", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "AR