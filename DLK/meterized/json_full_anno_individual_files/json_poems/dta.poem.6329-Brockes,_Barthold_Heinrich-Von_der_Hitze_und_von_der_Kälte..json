{"dta.poem.6329": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die beyden W\u00fcrckungen, die so verschiedlich seyn,", "tokens": ["Die", "bey\u00b7den", "W\u00fcr\u00b7ckun\u00b7gen", ",", "die", "so", "ver\u00b7schied\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Entstehn jedoch aus einem Grund allein:", "tokens": ["Ent\u00b7stehn", "je\u00b7doch", "aus", "ei\u00b7nem", "Grund", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Feur erzeuget sie nur blos durch das Bewegen.", "tokens": ["Das", "Feur", "er\u00b7zeu\u00b7get", "sie", "nur", "blos", "durch", "das", "Be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Dingen, worauf je ein Geist denckt und gedacht,", "tokens": ["Von", "Din\u00b7gen", ",", "wo\u00b7rauf", "je", "ein", "Geist", "denckt", "und", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ver\u00e4ndert alles sich, wird alles gantz verheeret,", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "al\u00b7les", "sich", ",", "wird", "al\u00b7les", "gantz", "ver\u00b7hee\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "$,", "VAFIN", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch heisser Flammen Macht,", "tokens": ["Durch", "heis\u00b7ser", "Flam\u00b7men", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie dieses Marmor selbst und Stahl vor Augen legen,", "tokens": ["Wie", "die\u00b7ses", "Mar\u00b7mor", "selbst", "und", "Stahl", "vor", "Au\u00b7gen", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADV", "KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja gar der Diamant: als welche, wenn die Gluht", "tokens": ["Ja", "gar", "der", "Di\u00b7a\u00b7mant", ":", "als", "wel\u00b7che", ",", "wenn", "die", "Gluht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "NN", "$.", "KOUS", "PIS", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie gar zu hefftig trifft, durch ihre Wuth,", "tokens": ["Sie", "gar", "zu", "heff\u00b7tig", "trifft", ",", "durch", "ih\u00b7re", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "ADJD", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Zumal wenn sie zu lange w\u00e4hret,", "tokens": ["Zu\u00b7mal", "wenn", "sie", "zu", "lan\u00b7ge", "w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Besieget werden und zerst\u00f6hret.", "tokens": ["Be\u00b7sie\u00b7get", "wer\u00b7den", "und", "zer\u00b7st\u00f6h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}