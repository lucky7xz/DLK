{"dta.poem.15674": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O h\u00e4tt' ich B\u00e4ume doch vor f\u00fcnfundzwanzig Jahren", "tokens": ["O", "h\u00e4tt'", "ich", "B\u00e4u\u00b7me", "doch", "vor", "f\u00fcn\u00b7fund\u00b7zwan\u00b7zig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "NN", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gepflanzt, als r\u00fcstig noch dazu die H\u00e4nde waren!", "tokens": ["Ge\u00b7pflanzt", ",", "als", "r\u00fcs\u00b7tig", "noch", "da\u00b7zu", "die", "H\u00e4n\u00b7de", "wa\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ADJD", "ADV", "PAV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie h\u00e4tten l\u00e4ngst nun schon mit Schatten mich gelabt,", "tokens": ["Sie", "h\u00e4t\u00b7ten", "l\u00e4ngst", "nun", "schon", "mit", "Schat\u00b7ten", "mich", "ge\u00b7labt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit goldner Fr\u00fcchte Lohn auch meinen Flei\u00df begabt.", "tokens": ["Mit", "gold\u00b7ner", "Fr\u00fcch\u00b7te", "Lohn", "auch", "mei\u00b7nen", "Flei\u00df", "be\u00b7gabt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun statt der Obstbaumzucht erzog ich Liederkeime,", "tokens": ["Nun", "statt", "der", "Obst\u00b7baum\u00b7zucht", "er\u00b7zog", "ich", "Lie\u00b7der\u00b7kei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir trugen weder Frucht noch Schatten all die Reime.", "tokens": ["Mir", "tru\u00b7gen", "we\u00b7der", "Frucht", "noch", "Schat\u00b7ten", "all", "die", "Rei\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "ADV", "NN", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}