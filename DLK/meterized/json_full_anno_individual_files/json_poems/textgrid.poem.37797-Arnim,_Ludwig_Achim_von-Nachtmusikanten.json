: "iambic.tri"}, "line.3": {"text": "Und legen unsern Kram,", "tokens": ["Und", "le\u00b7gen", "un\u00b7sern", "Kram", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Papier und Noten aus.", "tokens": ["Pa\u00b7pier", "und", "No\u00b7ten", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der erste gibt den Tackt,", "tokens": ["Der", "ers\u00b7te", "gibt", "den", "Tackt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der andre bl\u00e4\u00dft die Fl\u00f6ten,", "tokens": ["Der", "and\u00b7re", "bl\u00e4\u00dft", "die", "Fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der dritte schl\u00e4gt die Pauck',", "tokens": ["Der", "drit\u00b7te", "schl\u00e4gt", "die", "Pauck'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der viert st\u00f6\u00dft die Trompeten.", "tokens": ["Der", "viert", "st\u00f6\u00dft", "die", "Trom\u00b7pe\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "VVFIN", "ART", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ein andrer aber spielt", "tokens": ["Ein", "an\u00b7drer", "a\u00b7ber", "spielt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Theorb und Galischan", "tokens": ["The\u00b7orb", "und", "Ga\u00b7li\u00b7schan"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit gar besonderm Flei\u00df,", "tokens": ["Mit", "gar", "be\u00b7son\u00b7derm", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So gut er immer kann.", "tokens": ["So", "gut", "er", "im\u00b7mer", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wir pflegen auch so lang", "tokens": ["Wir", "pfle\u00b7gen", "auch", "so", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "An einem Eck zu hocken,", "tokens": ["An", "ei\u00b7nem", "Eck", "zu", "ho\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis wir ein sch\u00f6n Gespenst", "tokens": ["Bis", "wir", "ein", "sch\u00f6n", "Ge\u00b7spenst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hin an das Fenster locken;", "tokens": ["Hin", "an", "das", "Fens\u00b7ter", "lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da f\u00e4ngt man alsbald an", "tokens": ["Da", "f\u00e4ngt", "man", "als\u00b7bald", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vor der Geliebten Th\u00fcr", "tokens": ["Vor", "der", "Ge\u00b7lieb\u00b7ten", "Th\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verliebte Arien", "tokens": ["Ver\u00b7lieb\u00b7te", "A\u00b7rien"], "token_info": ["word", "word"], "pos": ["VVFIN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mit Pausen und Suspir.", "tokens": ["Mit", "Pau\u00b7sen", "und", "Sus\u00b7pir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Und sollten vor der Wacht", "tokens": ["Und", "soll\u00b7ten", "vor", "der", "Wacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir endlich weichen m\u00fcssen,", "tokens": ["Wir", "end\u00b7lich", "wei\u00b7chen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So macht man statt der H\u00e4nd',", "tokens": ["So", "macht", "man", "statt", "der", "H\u00e4nd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die L\u00e4ufe mit den F\u00fc\u00dfen.", "tokens": ["Die", "L\u00e4u\u00b7fe", "mit", "den", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und also treiben wirs", "tokens": ["Und", "al\u00b7so", "trei\u00b7ben", "wirs"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Oft durch die lange Nacht,", "tokens": ["Oft", "durch", "die", "lan\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df selbst die ganze Welt", "tokens": ["Da\u00df", "selbst", "die", "gan\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ob unsrer Narrheit lacht.", "tokens": ["Ob", "uns\u00b7rer", "Nar\u00b7rheit", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ach sch\u00f6nste Phillis h\u00f6r", "tokens": ["Ach", "sch\u00f6ns\u00b7te", "Phil\u00b7lis", "h\u00f6r"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Doch unser Musiciren,", "tokens": ["Doch", "un\u00b7ser", "Mu\u00b7si\u00b7ci\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df uns eine Nacht", "tokens": ["Und", "la\u00df", "uns", "ei\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In deinem Schoos pausiren.", "tokens": ["In", "dei\u00b7nem", "Schoos", "pau\u00b7si\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "N