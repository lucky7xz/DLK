{"textgrid.poem.37797": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtmusikanten", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier sind wir arme Narrn", "tokens": ["Hier", "sind", "wir", "ar\u00b7me", "Narrn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf Pl\u00e4tzen und auf Gassen,", "tokens": ["Auf", "Pl\u00e4t\u00b7zen", "und", "auf", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und thun die ganze Nacht", "tokens": ["Und", "thun", "die", "gan\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit unsrer Musick passen.", "tokens": ["Mit", "uns\u00b7rer", "Mu\u00b7sick", "pas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es giebt uns keine Ruhe", "tokens": ["Es", "giebt", "uns", "kei\u00b7ne", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die starke Liebes-Macht,", "tokens": ["Die", "star\u00b7ke", "Lie\u00b7bes\u00b7Macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir stehen mit dem Bogen", "tokens": ["Wir", "ste\u00b7hen", "mit", "dem", "Bo\u00b7gen"], "toke