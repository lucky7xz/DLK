{"dta.poem.10310": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Mittsommer. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Das wei\u00dfe H\u00e4uschen, das ich flimmern sehe,", "tokens": ["Das", "wei\u00b7\u00dfe", "H\u00e4usc\u00b7hen", ",", "das", "ich", "flim\u00b7mern", "se\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie liegt\u2019s abseits in Sonn\u2019 und Sonntagsruh.", "tokens": ["Wie", "liegt's", "ab\u00b7seits", "in", "Sonn'", "und", "Sonn\u00b7tags\u00b7ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der Rosenstrauch am Dach schwillt im Gewehe,", "tokens": ["Der", "Ro\u00b7sen\u00b7strauch", "am", "Dach", "schwillt", "im", "Ge\u00b7we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als w\u00e4r\u2019s der Kamm von einem Kakadu.", "tokens": ["Als", "w\u00e4r's", "der", "Kamm", "von", "ei\u00b7nem", "Ka\u00b7ka\u00b7du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Heut Nachmittag, wenn ich spazieren gehe,", "tokens": ["Heut", "Nach\u00b7mit\u00b7tag", ",", "wenn", "ich", "spa\u00b7zie\u00b7ren", "ge\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KOUS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kehr\u2019 dort ich ein zu einem Rendezvous.", "tokens": ["Kehr'", "dort", "ich", "ein", "zu", "ei\u00b7nem", "Ren\u00b7dez\u00b7vous", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wir sind allein. Doch ja da\u00df nichts geschehe,", "tokens": ["Wir", "sind", "al\u00b7lein", ".", "Doch", "ja", "da\u00df", "nichts", "ge\u00b7sche\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "KON", "ADV", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Spielt M\u00fctterchen dann mit uns Blindekuh.", "tokens": ["Spielt", "M\u00fct\u00b7ter\u00b7chen", "dann", "mit", "uns", "Blin\u00b7de\u00b7kuh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "PPER", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}}}}}