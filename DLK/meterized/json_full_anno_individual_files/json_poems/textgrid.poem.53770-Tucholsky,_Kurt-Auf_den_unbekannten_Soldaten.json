{"textgrid.poem.53770": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Auf den unbekannten Soldaten", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast die N\u00e4chte nur zum Schlafen,", "tokens": ["Du", "hast", "die", "N\u00e4ch\u00b7te", "nur", "zum", "Schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Soldat! Du wei\u00dft doch, wo du liegst!", "tokens": ["Sol\u00b7dat", "!", "Du", "wei\u00dft", "doch", ",", "wo", "du", "liegst", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der kalte Stein, wo du dich schmiegst,", "tokens": ["Der", "kal\u00b7te", "Stein", ",", "wo", "du", "dich", "schmiegst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ist so ein Ort, wohin man geht,", "tokens": ["ist", "so", "ein", "Ort", ",", "wo\u00b7hin", "man", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wenn man nicht wei\u00df, was anzufangen.", "tokens": ["wenn", "man", "nicht", "wei\u00df", ",", "was", "an\u00b7zu\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVFIN", "$,", "PRELS", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da kommen deine Freunde an in langen", "tokens": ["Da", "kom\u00b7men", "dei\u00b7ne", "Freun\u00b7de", "an", "in", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und dichten Scharen. Regenwolken ziehn \u2013", "tokens": ["und", "dich\u00b7ten", "Scha\u00b7ren", ".", "Re\u00b7gen\u00b7wol\u00b7ken", "ziehn", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "heut ist nichts mit den Tuilerien.", "tokens": ["heut", "ist", "nichts", "mit", "den", "Tu\u00b7i\u00b7le\u00b7ri\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Na, gehn wir hierher . . .", "tokens": ["Na", ",", "gehn", "wir", "hier\u00b7her", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PAV", "$.", "$.", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Ins Kino gingen sie, du armer Junge,", "tokens": ["Ins", "Ki\u00b7no", "gin\u00b7gen", "sie", ",", "du", "ar\u00b7mer", "Jun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "w\u00e4rs da umsonst . . . Sieh! Sie ziehn Trauerfalten.", "tokens": ["w\u00e4rs", "da", "um\u00b7sonst", ".", ".", ".", "Sieh", "!", "Sie", "ziehn", "Trau\u00b7er\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$.", "$.", "$.", "NE", "$.", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr dich. Und weil auf ihrer Zunge", "tokens": ["F\u00fcr", "dich", ".", "Und", "weil", "auf", "ih\u00b7rer", "Zun\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$.", "KON", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sich noch von Mittag her ein schlechter Nachgeschmack erhalten.", "tokens": ["sich", "noch", "von", "Mit\u00b7tag", "her", "ein", "schlech\u00b7ter", "Nach\u00b7ge\u00b7schmack", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "APZR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "An manchen Tagen wirst du sch\u00f6n bepredigt", "tokens": ["An", "man\u00b7chen", "Ta\u00b7gen", "wirst", "du", "sch\u00f6n", "be\u00b7pre\u00b7digt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "von gro\u00dfen Tieren, Herrn vom Parlament \u2013", "tokens": ["von", "gro\u00b7\u00dfen", "Tie\u00b7ren", ",", "Herrn", "vom", "Par\u00b7la\u00b7ment", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "(und alle Welt ist froh, wenn das erledigt).", "tokens": ["(", "und", "al\u00b7le", "Welt", "ist", "froh", ",", "wenn", "das", "er\u00b7le\u00b7digt", ")", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PIAT", "NN", "VAFIN", "ADJD", "$,", "KOUS", "PDS", "VVPP", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Flamme brennt . . .", "tokens": ["Die", "Flam\u00b7me", "brennt", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Du bist der ihre, armer Junge!", "tokens": ["Du", "bist", "der", "ih\u00b7re", ",", "ar\u00b7mer", "Jun\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PPOSAT", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich mit dem einen Bein, mit etwas Lunge,", "tokens": ["Dich", "mit", "dem", "ei\u00b7nen", "Bein", ",", "mit", "et\u00b7was", "Lun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ART", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "dich, Opfer, namenlose Nummer:", "tokens": ["dich", ",", "Op\u00b7fer", ",", "na\u00b7men\u00b7lo\u00b7se", "Num\u00b7mer", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dich brauchen sie \u2013 und desto stummer", "tokens": ["dich", "brau\u00b7chen", "sie", "\u2013", "und", "des\u00b7to", "stum\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KON", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "du bist, je besser ists.", "tokens": ["du", "bist", ",", "je", "bes\u00b7ser", "ists", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr ihr Gesch\u00e4ft, Soldat:", "tokens": ["F\u00fcr", "ihr", "Ge\u00b7sch\u00e4ft", ",", "Sol\u00b7dat", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dich brauchen sie als ein Plakat \u2013!", "tokens": ["Dich", "brau\u00b7chen", "sie", "als", "ein", "Pla\u00b7kat", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "ART", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du bist ihr Mann \u2013 der Mann der Generale!", "tokens": ["Du", "bist", "ihr", "Mann", "\u2013", "der", "Mann", "der", "Ge\u00b7ne\u00b7ra\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist ihr Mann \u2013 der Mann der Finanziers!", "tokens": ["Du", "bist", "ihr", "Mann", "\u2013", "der", "Mann", "der", "Fi\u00b7nan\u00b7ziers", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du bist ihr Mann \u2013 der Mann der Prinzipale,", "tokens": ["Du", "bist", "ihr", "Mann", "\u2013", "der", "Mann", "der", "Prin\u00b7zi\u00b7pa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der Hausbesitzer und der Ehrenkomitees!", "tokens": ["der", "Haus\u00b7be\u00b7sit\u00b7zer", "und", "der", "Eh\u00b7ren\u00b7ko\u00b7mi\u00b7tees", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Sie stehn umher auf deinen Knochen.", "tokens": ["Sie", "stehn", "um\u00b7her", "auf", "dei\u00b7nen", "Kno\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weit \u00f6ffnet sich der Rednermund.", "tokens": ["Weit", "\u00f6ff\u00b7net", "sich", "der", "Red\u00b7ner\u00b7mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tags\u00fcber kommt das angekrochen", "tokens": ["Tags\u00b7\u00fc\u00b7ber", "kommt", "das", "an\u00b7ge\u00b7kro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und schreit sich seine Kehle wund.", "tokens": ["und", "schreit", "sich", "sei\u00b7ne", "Keh\u00b7le", "wund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Minister, Offiziere, Grafen . . .", "tokens": ["Mi\u00b7nis\u00b7ter", ",", "Of\u00b7fi\u00b7zie\u00b7re", ",", "Gra\u00b7fen", ".", ".", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du hast die N\u00e4chte nur zum Schlafen.", "tokens": ["Du", "hast", "die", "N\u00e4ch\u00b7te", "nur", "zum", "Schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und kommt die Nacht, gehn sie mit schnellen Beinen", "tokens": ["Und", "kommt", "die", "Nacht", ",", "gehn", "sie", "mit", "schnel\u00b7len", "Bei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in ihre Kneipe, nehmen einen . . .", "tokens": ["in", "ih\u00b7re", "Knei\u00b7pe", ",", "neh\u00b7men", "ei\u00b7nen", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "ART", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nachtschatten steigt. Du liegst allein.", "tokens": ["Nacht\u00b7schat\u00b7ten", "steigt", ".", "Du", "liegst", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So still ist es hier nie gewesen.", "tokens": ["So", "still", "ist", "es", "hier", "nie", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schon kann man nicht mehr alle Namen lesen:", "tokens": ["Schon", "kann", "man", "nicht", "mehr", "al\u00b7le", "Na\u00b7men", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PTKNEG", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Eylau und Wagram \u2013 da im Stein . . .", "tokens": ["Ey\u00b7lau", "und", "Wag\u00b7ram", "\u2013", "da", "im", "Stein", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "KON", "NE", "$(", "ADV", "APPRART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch da ist so viel Blut geflossen.", "tokens": ["Auch", "da", "ist", "so", "viel", "Blut", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "F\u00fcr wen verstr\u00f6mt? F\u00fcr wen vergossen?", "tokens": ["F\u00fcr", "wen", "ver\u00b7str\u00f6mt", "?", "F\u00fcr", "wen", "ver\u00b7gos\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "$.", "APPR", "PWS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Dunkel um dich. Und endlich hast du Ruh.", "tokens": ["Dun\u00b7kel", "um", "dich", ".", "Und", "end\u00b7lich", "hast", "du", "Ruh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$.", "KON", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Alles ist fort. Die Schw\u00e4rze deckt dich zu.", "tokens": ["Al\u00b7les", "ist", "fort", ".", "Die", "Schw\u00e4r\u00b7ze", "deckt", "dich", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Es ging in blauer D\u00e4mmerung Schwaden", "tokens": ["Es", "ging", "in", "blau\u00b7er", "D\u00e4m\u00b7me\u00b7rung", "Schwa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "der letzte Kunde aus dem Laden . . .", "tokens": ["der", "letz\u00b7te", "Kun\u00b7de", "aus", "dem", "La\u00b7den", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr heute ist dein Leidenstag geendet.", "tokens": ["F\u00fcr", "heu\u00b7te", "ist", "dein", "Lei\u00b7dens\u00b7tag", "ge\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Sternenlicht", "tokens": ["Im", "Ster\u00b7nen\u00b7licht"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "unh\u00f6rbar spricht", "tokens": ["un\u00b7h\u00f6r\u00b7bar", "spricht"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "ein toter Mann:", "tokens": ["ein", "to\u00b7ter", "Mann", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "N