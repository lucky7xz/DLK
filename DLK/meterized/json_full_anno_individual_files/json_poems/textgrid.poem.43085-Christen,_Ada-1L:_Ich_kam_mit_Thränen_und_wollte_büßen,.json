{"textgrid.poem.43085": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kam mit Thr\u00e4nen und wollte b\u00fc\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kam mit Thr\u00e4nen und wollte b\u00fc\u00dfen,", "tokens": ["Ich", "kam", "mit", "Thr\u00e4\u00b7nen", "und", "woll\u00b7te", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was ich und Andere verschuldet;", "tokens": ["Was", "ich", "und", "An\u00b7de\u00b7re", "ver\u00b7schul\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie aber traten stolz mit F\u00fc\u00dfen", "tokens": ["Sie", "a\u00b7ber", "tra\u00b7ten", "stolz", "mit", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz, das schon so viel erduldet.", "tokens": ["Das", "Herz", ",", "das", "schon", "so", "viel", "er\u00b7dul\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und Weiber waren es immer wieder,", "tokens": ["Und", "Wei\u00b7ber", "wa\u00b7ren", "es", "im\u00b7mer", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die mich entr\u00fcstet mit Gei\u00dfelhieben", "tokens": ["Die", "mich", "ent\u00b7r\u00fcs\u00b7tet", "mit", "Gei\u00b7\u00df\u00b7el\u00b7hie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So tugend-dumm und weiblich-nieder", "tokens": ["So", "tu\u00b7gen\u00b7ddumm", "und", "weib\u00b7lich\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von neuem stets in's Elend trieben.", "tokens": ["Von", "neu\u00b7em", "stets", "in's", "E\u00b7lend", "trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich kam mit Thr\u00e4nen und wollte b\u00fc\u00dfen,", "tokens": ["Ich", "kam", "mit", "Thr\u00e4\u00b7nen", "und", "woll\u00b7te", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was ich und Andere verschuldet;", "tokens": ["Was", "ich", "und", "An\u00b7de\u00b7re", "ver\u00b7schul\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie aber traten stolz mit F\u00fc\u00dfen", "tokens": ["Sie", "a\u00b7ber", "tra\u00b7ten", "stolz", "mit", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD",