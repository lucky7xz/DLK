{"textgrid.poem.65216": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Wein", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig, dem ich diene,", "tokens": ["Der", "K\u00f6\u00b7nig", ",", "dem", "ich", "die\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PDS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als treuer, tapfrer Held,", "tokens": ["Als", "treu\u00b7er", ",", "tapf\u00b7rer", "Held", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er ist der gr\u00f6\u00dfte K\u00f6nig", "tokens": ["Er", "ist", "der", "gr\u00f6\u00df\u00b7te", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Gottes weiter Welt.", "tokens": ["In", "Got\u00b7tes", "wei\u00b7ter", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Fahne, der ich folge,", "tokens": ["Die", "Fah\u00b7ne", ",", "der", "ich", "fol\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie ist ein gr\u00fcner Zweig,", "tokens": ["Sie", "ist", "ein", "gr\u00fc\u00b7ner", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der weht vor allen Schenken", "tokens": ["Der", "weht", "vor", "al\u00b7len", "Schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meines K\u00f6nigs Reich.", "tokens": ["In", "mei\u00b7nes", "K\u00f6\u00b7nigs", "Reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich trage seine Farbe", "tokens": ["Ich", "tra\u00b7ge", "sei\u00b7ne", "Far\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In meinem Angesicht:", "tokens": ["In", "mei\u00b7nem", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf Kragen und Rabatten", "tokens": ["Auf", "Kra\u00b7gen", "und", "Ra\u00b7bat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sieht unser K\u00f6nig nicht.", "tokens": ["Sieht", "un\u00b7ser", "K\u00f6\u00b7nig", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hochroth ist seine Farbe,", "tokens": ["Hoch\u00b7roth", "ist", "sei\u00b7ne", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gl\u00e4nzt wie ein Edelstein,", "tokens": ["Gl\u00e4nzt", "wie", "ein", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Farbe unsrer Feinde", "tokens": ["Die", "Far\u00b7be", "uns\u00b7rer", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat matten, bleichen Schein.", "tokens": ["Hat", "mat\u00b7ten", ",", "blei\u00b7chen", "Schein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr General und K\u00f6nig", "tokens": ["Ihr", "Ge\u00b7ne\u00b7ral", "und", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird ", "tokens": ["Wird"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Zieht sengend und verbrennend", "tokens": ["Zieht", "sen\u00b7gend", "und", "ver\u00b7bren\u00b7nend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "VVPP"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Durch unsres K\u00f6nigs Land.", "tokens": ["Durch", "uns\u00b7res", "K\u00f6\u00b7nigs", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ist unser Feldgesang,", "tokens": ["Ist", "un\u00b7ser", "Feld\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und unsre Schlachttrompete", "tokens": ["Und", "uns\u00b7re", "Schlacht\u00b7trom\u00b7pe\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist voller Gl\u00e4ser Klang.", "tokens": ["Ist", "vol\u00b7ler", "Gl\u00e4\u00b7ser", "Klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auch fehlen nicht die Trommeln,", "tokens": ["Auch", "feh\u00b7len", "nicht", "die", "Trom\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch donnert mancher Schu\u00df:", "tokens": ["Auch", "don\u00b7nert", "man\u00b7cher", "Schu\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir schlagen auf die Tische,", "tokens": ["Wir", "schla\u00b7gen", "auf", "die", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir stampfen mit dem Fu\u00df.", "tokens": ["Wir", "stamp\u00b7fen", "mit", "dem", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wir haben scharf geladen,", "tokens": ["Wir", "ha\u00b7ben", "scharf", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir f\u00fchren gut Gewehr:", "tokens": ["Wir", "f\u00fch\u00b7ren", "gut", "Ge\u00b7wehr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kanonen sind die Flaschen,", "tokens": ["Ka\u00b7no\u00b7nen", "sind", "die", "Fla\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von edlem Safte schwer.", "tokens": ["Von", "ed\u00b7lem", "Saf\u00b7te", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wohlauf, wohlauf zum Siege!", "tokens": ["Wohl\u00b7auf", ",", "wohl\u00b7auf", "zum", "Sie\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Nase und der Bart", "tokens": ["Die", "Na\u00b7se", "und", "der", "Bart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sind besser, als im Helme,", "tokens": ["Sind", "bes\u00b7ser", ",", "als", "im", "Hel\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In einem Glas bewahrt.", "tokens": ["In", "ei\u00b7nem", "Glas", "be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und wirft ein Hieb mich nieder", "tokens": ["Und", "wirft", "ein", "Hieb", "mich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": 