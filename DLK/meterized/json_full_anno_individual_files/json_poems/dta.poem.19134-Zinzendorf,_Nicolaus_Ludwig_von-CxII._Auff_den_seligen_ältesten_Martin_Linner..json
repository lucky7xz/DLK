{"dta.poem.19134": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CxII.  Auff den seligen \u00e4ltesten Martin  \n Linner.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.85", "en:0.14"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Und wo nehm ich Worte her,", "tokens": ["Und", "wo", "nehm", "ich", "Wor\u00b7te", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Worte, die den Grund der Seelen", "tokens": ["Wor\u00b7te", ",", "die", "den", "Grund", "der", "See\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht verheelen,", "tokens": ["Nicht", "ver\u00b7hee\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Worte, welche jederman", "tokens": ["Wor\u00b7te", ",", "wel\u00b7che", "je\u00b7der\u00b7man"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fassen kan?", "tokens": ["Fas\u00b7sen", "kan", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Geist der Gnaden! deine Regung,", "tokens": ["Geist", "der", "Gna\u00b7den", "!", "dei\u00b7ne", "Re\u00b7gung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine dringende Bewegung,", "tokens": ["Dei\u00b7ne", "drin\u00b7gen\u00b7de", "Be\u00b7we\u00b7gung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "N\u00e4hm ich ietzt mit Freuden an.", "tokens": ["N\u00e4hm", "ich", "ietzt", "mit", "Freu\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O du auserw\u00e4hltes Glied", "tokens": ["O", "du", "au\u00b7ser\u00b7w\u00e4hl\u00b7tes", "Glied"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Des verkl\u00e4rten Creutzes-Stammes,", "tokens": ["Des", "ver\u00b7kl\u00e4r\u00b7ten", "Creut\u00b7zes\u00b7Stam\u00b7mes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsers Lammes;", "tokens": ["Un\u00b7sers", "Lam\u00b7mes", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Dessen Geist der Aeltsten Schaar", "tokens": ["Des\u00b7sen", "Geist", "der", "A\u00b7elts\u00b7ten", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Nahe war,", "tokens": ["Na\u00b7he", "war", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und den ", "tokens": ["Und", "den"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Welche Tag und Nacht nicht schweigen,", "tokens": ["Wel\u00b7che", "Tag", "und", "Nacht", "nicht", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sondern rufen: ", "tokens": ["Son\u00b7dern", "ru\u00b7fen", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Unsre Seele segnet dich.", "tokens": ["Uns\u00b7re", "See\u00b7le", "seg\u00b7net", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwar das Be\u00dfre solte Segen", "tokens": ["Zwar", "das", "Be\u00df\u00b7re", "sol\u00b7te", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VMFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf uns legen,", "tokens": ["Auf", "uns", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Und du segnest uns auch gern", "tokens": ["Und", "du", "seg\u00b7nest", "uns", "auch", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem HErrn;", "tokens": ["In", "dem", "Herrn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Aber deine tiefe Demuth", "tokens": ["A\u00b7ber", "dei\u00b7ne", "tie\u00b7fe", "De\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Ubersieht es unsrer Wehmuth,", "tokens": ["Ub\u00b7er\u00b7sieht", "es", "uns\u00b7rer", "Weh\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df sie sich vom Ziel entfern.", "tokens": ["Da\u00df", "sie", "sich", "vom", "Ziel", "ent\u00b7fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schweigen wird das Beste seyn:", "tokens": ["Schwei\u00b7gen", "wird", "das", "Bes\u00b7te", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn gewi\u00df, die Worte fehlen", "tokens": ["Denn", "ge\u00b7wi\u00df", ",", "die", "Wor\u00b7te", "feh\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu erzehlen,", "tokens": ["Zu", "er\u00b7zeh\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Was du uns gewesen bist;", "tokens": ["Was", "du", "uns", "ge\u00b7we\u00b7sen", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VAPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jesus Christ", "tokens": ["Je\u00b7sus", "Christ"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wei\u00df dich nur allein zu sch\u00e4tzen,", "tokens": ["Wei\u00df", "dich", "nur", "al\u00b7lein", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und dein Bildni\u00df einzu\u00e4tzen,", "tokens": ["Und", "dein", "Bild\u00b7ni\u00df", "ein\u00b7zu\u00b7\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil es seine Arbeit ist.", "tokens": ["Weil", "es", "sei\u00b7ne", "Ar\u00b7beit", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Recht zu klagen hab ich nicht:", "tokens": ["Recht", "zu", "kla\u00b7gen", "hab", "ich", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn es eilten ", "tokens": ["Denn", "es", "eil\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Aus der H\u00fctte,", "tokens": ["Aus", "der", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Eh er unser Aeltster hie\u00df,", "tokens": ["Eh", "er", "un\u00b7ser", "A\u00b7elts\u00b7ter", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Und ich prie\u00df", "tokens": ["Und", "ich", "prie\u00df"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Unsers Bischoffs kr\u00e4fftge Gnade,", "tokens": ["Un\u00b7sers", "Bi\u00b7schoffs", "kr\u00e4fft\u00b7ge", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die ihn mitten in dem Pfade", "tokens": ["Die", "ihn", "mit\u00b7ten", "in", "dem", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Drey Jahr Halte machen lie\u00df.", "tokens": ["Drey", "Jahr", "Hal\u00b7te", "ma\u00b7chen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun, wo ist die Blumen-Streu?", "tokens": ["Nun", ",", "wo", "ist", "die", "Blu\u00b7men\u00b7Streu", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VAFIN", "ART", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Die Gebeine zu bedienen,", "tokens": ["Die", "Ge\u00b7bei\u00b7ne", "zu", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die nun gr\u00fcnen?", "tokens": ["Die", "nun", "gr\u00fc\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Meine soll in dem bestehn:", "tokens": ["Mei\u00b7ne", "soll", "in", "dem", "be\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VMFIN", "APPR", "ART", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich will flehn,", "tokens": ["Ich", "will", "flehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Jesus soll, nach deinem Triebe,", "tokens": ["Je\u00b7sus", "soll", ",", "nach", "dei\u00b7nem", "Trie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine allzu herbe ", "tokens": ["Mei\u00b7ne", "all\u00b7zu", "her\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "PTKA", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "In die linde Lieb erh\u00f6hn.", "tokens": ["In", "die", "lin\u00b7de", "Lieb", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}