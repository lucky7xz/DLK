{"dta.poem.14523": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Umkehrende .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Du sollst mich doch nicht fangen,", "tokens": ["Du", "sollst", "mich", "doch", "nicht", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Duftschw\u00fcle Zaubernacht!", "tokens": ["Duft\u00b7schw\u00fc\u00b7le", "Zau\u00b7ber\u00b7nacht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es steh'n mit goldnem Prangen", "tokens": ["Es", "steh'n", "mit", "gold\u00b7nem", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Stern' auf stiller Wacht,", "tokens": ["Die", "Stern'", "auf", "stil\u00b7ler", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und machen \u00fcber'm Grunde,", "tokens": ["Und", "ma\u00b7chen", "\u00fc\u00b7ber'm", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo Du verirret bist,", "tokens": ["Wo", "Du", "ver\u00b7ir\u00b7ret", "bist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Getreu die alte Runde \u2014", "tokens": ["Ge\u00b7treu", "die", "al\u00b7te", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gelobt sei Jesus Christ!", "tokens": ["Ge\u00b7lobt", "sei", "Je\u00b7sus", "Christ", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}