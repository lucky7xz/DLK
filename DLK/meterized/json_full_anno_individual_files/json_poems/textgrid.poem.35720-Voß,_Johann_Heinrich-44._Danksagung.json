{"textgrid.poem.35720": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "44. Danksagung", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcllt an die Gl\u00e4ser, f\u00fcllt bis oben,", "tokens": ["F\u00fcllt", "an", "die", "Gl\u00e4\u00b7ser", ",", "f\u00fcllt", "bis", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und leert auf Einen Zug sie aus!", "tokens": ["Und", "leert", "auf", "Ei\u00b7nen", "Zug", "sie", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollen unsre Wirtin loben", "tokens": ["Wir", "wol\u00b7len", "uns\u00b7re", "Wir\u00b7tin", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr ihren sch\u00f6nen Abendschmaus!", "tokens": ["F\u00fcr", "ih\u00b7ren", "sch\u00f6\u00b7nen", "A\u00b7bendsc\u00b7hmaus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hat, durch kluge Sorg' und M\u00fche,", "tokens": ["Sie", "hat", ",", "durch", "klu\u00b7ge", "Sor\u00b7g'", "und", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra