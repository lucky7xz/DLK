{"textgrid.poem.60363": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Doppelheimweh", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwiefaches Heimweh h\u00e4lt das Herz befangen,", "tokens": ["Zwie\u00b7fa\u00b7ches", "Heim\u00b7weh", "h\u00e4lt", "das", "Herz", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn wir am Rand des steilen Abgrunds stehn", "tokens": ["Wenn", "wir", "am", "Rand", "des", "stei\u00b7len", "Ab\u00b7grunds", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und in die Grabesnacht hinuntersehn,", "tokens": ["Und", "in", "die", "Gra\u00b7bes\u00b7nacht", "hin\u00b7un\u00b7ter\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit tr\u00fcben Augen, todeshohlen Wangen.", "tokens": ["Mit", "tr\u00fc\u00b7ben", "Au\u00b7gen", ",", "to\u00b7des\u00b7hoh\u00b7len", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Erdenheimweh l\u00e4\u00dft uns trauern, bangen,", "tokens": ["Das", "Er\u00b7den\u00b7heim\u00b7weh", "l\u00e4\u00dft", "uns", "trau\u00b7ern", ",", "ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos"