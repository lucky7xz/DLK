{"dta.poem.14672": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00fchne .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Dort zwischen den Zinnen und Spitzen", "tokens": ["Dort", "zwi\u00b7schen", "den", "Zin\u00b7nen", "und", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von wilden Nelken umbl\u00fcht,", "tokens": ["Von", "wil\u00b7den", "Nel\u00b7ken", "um\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die sch\u00f6nen Waldfrauen sitzen", "tokens": ["Die", "sch\u00f6\u00b7nen", "Wald\u00b7frau\u00b7en", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und singen im Wind ihr Lied.", "tokens": ["Und", "sin\u00b7gen", "im", "Wind", "ihr", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}