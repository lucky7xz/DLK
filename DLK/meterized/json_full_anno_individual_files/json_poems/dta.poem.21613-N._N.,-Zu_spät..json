{"dta.poem.21613": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Zu sp\u00e4t.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.71", "af:0.28"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Nie, arme kleine Knospe, wird", "tokens": ["Nie", ",", "ar\u00b7me", "klei\u00b7ne", "Knos\u00b7pe", ",", "wird"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "ADJA", "ADJA", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Kelch der Sonne sich erschlie\u00dfen,", "tokens": ["Dein", "Kelch", "der", "Son\u00b7ne", "sich", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du hast dich in die Welt verirrt", "tokens": ["Du", "hast", "dich", "in", "die", "Welt", "ver\u00b7irrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Zeit, da Blumen nicht mehr sprie\u00dfen.", "tokens": ["Zur", "Zeit", ",", "da", "Blu\u00b7men", "nicht", "mehr", "sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum hast du so lang verweilt?", "tokens": ["Wa\u00b7rum", "hast", "du", "so", "lang", "ver\u00b7weilt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sommer war ja l\u00e4ngst gekommen.", "tokens": ["Der", "Som\u00b7mer", "war", "ja", "l\u00e4ngst", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn dich der Winter nun ereilt,", "tokens": ["Wenn", "dich", "der", "Win\u00b7ter", "nun", "er\u00b7eilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich ist dir jede Lust genommen.", "tokens": ["Gleich", "ist", "dir", "je\u00b7de", "Lust", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, ich beneide deinen Traum,", "tokens": ["Ach", ",", "ich", "be\u00b7nei\u00b7de", "dei\u00b7nen", "Traum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den du im Erdenscho\u00df getr\u00e4umet.", "tokens": ["Den", "du", "im", "Er\u00b7den\u00b7scho\u00df", "ge\u00b7tr\u00e4u\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dich weckte all der Jubel kaum", "tokens": ["Dich", "weck\u00b7te", "all", "der", "Ju\u00b7bel", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und immer hast du noch ges\u00e4umet.", "tokens": ["Und", "im\u00b7mer", "hast", "du", "noch", "ge\u00b7s\u00e4u\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh um dich her die Schwestern weich", "tokens": ["Sieh", "um", "dich", "her", "die", "Schwes\u00b7tern", "weich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PRF", "APZR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Strahl des Tages schnell getroffen,", "tokens": ["Vom", "Strahl", "des", "Ta\u00b7ges", "schnell", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie neigten sich der Liebe gleich;", "tokens": ["Sie", "neig\u00b7ten", "sich", "der", "Lie\u00b7be", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald waren ihre Kelche offen.", "tokens": ["Bald", "wa\u00b7ren", "ih\u00b7re", "Kel\u00b7che", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie hauchten ihre D\u00fcfte aus,", "tokens": ["Sie", "hauch\u00b7ten", "ih\u00b7re", "D\u00fcf\u00b7te", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Lieb und Demuth hold bezwungen,", "tokens": ["Von", "Lieb", "und", "De\u00b7muth", "hold", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dir haben in der Mutter Haus", "tokens": ["Dir", "ha\u00b7ben", "in", "der", "Mut\u00b7ter", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umsonst die V\u00f6gelein gesungen.", "tokens": ["Um\u00b7sonst", "die", "V\u00f6\u00b7ge\u00b7lein", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie gaben ihre Bl\u00fcthen hin \u2014", "tokens": ["Sie", "ga\u00b7ben", "ih\u00b7re", "Bl\u00fc\u00b7then", "hin"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wind entf\u00fchrte ihre Bl\u00fcthen;", "tokens": ["Der", "Wind", "ent\u00b7f\u00fchr\u00b7te", "ih\u00b7re", "Bl\u00fc\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du thatest wohl in herbem Sinn", "tokens": ["Du", "tha\u00b7test", "wohl", "in", "her\u00b7bem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der eignen Bl\u00fcthe neidisch h\u00fcten.", "tokens": ["Der", "eig\u00b7nen", "Bl\u00fc\u00b7the", "nei\u00b7disch", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun stehen sie entbl\u00e4ttert da,", "tokens": ["Nun", "ste\u00b7hen", "sie", "ent\u00b7bl\u00e4t\u00b7tert", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Get\u00f6dtet durch zu hei\u00dfes Lieben,", "tokens": ["Ge\u00b7t\u00f6d\u00b7tet", "durch", "zu", "hei\u00b7\u00dfes", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "APPR", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur dir kam nie die Liebe nah,", "tokens": ["Nur", "dir", "kam", "nie", "die", "Lie\u00b7be", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur du bist ungek\u00fc\u00dft geblieben.", "tokens": ["Nur", "du", "bist", "un\u00b7ge\u00b7k\u00fc\u00dft", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und sieh! es lockte dich im Hag", "tokens": ["Und", "sieh", "!", "es", "lock\u00b7te", "dich", "im", "Hag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch alle Tage gleiche Wonne,", "tokens": ["Doch", "al\u00b7le", "Ta\u00b7ge", "glei\u00b7che", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die V\u00f6glein sangen jeden Tag", "tokens": ["Die", "V\u00f6\u00b7glein", "san\u00b7gen", "je\u00b7den", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeden Tag ging auf die Sonne.", "tokens": ["Und", "je\u00b7den", "Tag", "ging", "auf", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}