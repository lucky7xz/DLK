{"dta.poem.10716": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Liedt/ im thon:  Ma belle je vous prie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Liebste la\u00df vns eilen", "tokens": ["Ach", "Liebs\u00b7te", "la\u00df", "vns", "ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "VVIMP", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es schadet das verweilen", "tokens": ["Es", "scha\u00b7det", "das", "ver\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der sch\u00f6nen Sch\u00f6nheit gaben", "tokens": ["Der", "sch\u00f6\u00b7nen", "Sch\u00f6n\u00b7heit", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df alles/ was wir haben/", "tokens": ["Da\u00df", "al\u00b7les", "/", "was", "wir", "ha\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$(", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Wangen zier verbleichet/", "tokens": ["Der", "Wan\u00b7gen", "zier", "ver\u00b7blei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der \u00e4uglein fewer weichet/", "tokens": ["Der", "\u00e4ug\u00b7lein", "fe\u00b7wer", "wei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das M\u00fcndlein von Corallen", "tokens": ["Das", "M\u00fcnd\u00b7lein", "von", "Co\u00b7ral\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die H\u00e4nd/ al\u00df Schnee verfallen/", "tokens": ["Die", "H\u00e4nd", "/", "al\u00df", "Schnee", "ver\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Drumb la\u00df vns jetz geniessen", "tokens": ["Drumb", "la\u00df", "vns", "jetz", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Eh dann wir folgen m\u00fcssen", "tokens": ["Eh", "dann", "wir", "fol\u00b7gen", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "VVINF", "VMFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.11": {"text": "Wo du dich selber liebest/", "tokens": ["Wo", "du", "dich", "sel\u00b7ber", "lie\u00b7best", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Gib mir/ da\u00df/ wan\u0303 du gibest/", "tokens": ["Gib", "mir", "/", "da\u00df", "/", "wa\u00f1", "du", "gi\u00b7best", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "KOUS", "$(", "PWAV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wir haben Zeit:", "tokens": ["Wir", "ha\u00b7ben", "Zeit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Vns beider seit.", "tokens": ["Vns", "bei\u00b7der", "seit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Fliehn fu\u00df f\u00fcr fu\u00df", "tokens": ["Fliehn", "fu\u00df", "f\u00fcr", "fu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "APPR", "PTKVZ"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Verschwinden mu\u00df/", "tokens": ["Ver\u00b7schwin\u00b7den", "mu\u00df", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Das Haar wird grei\u00df/", "tokens": ["Das", "Haar", "wird", "grei\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Die flamm wird Ei\u00df.", "tokens": ["Die", "flamm", "wird", "Ei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Wird vngestallt.", "tokens": ["Wird", "vn\u00b7ge\u00b7stallt", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Vnd du wirst Alt.", "tokens": ["Vnd", "du", "wirst", "Alt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Der Jugent frucht/", "tokens": ["Der", "Ju\u00b7gent", "frucht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Der Jahre flucht.", "tokens": ["Der", "Jah\u00b7re", "flucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "So liebe mich/", "tokens": ["So", "lie\u00b7be", "mich", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Verlier auch ich.", "tokens": ["Ver\u00b7lier", "auch", "ich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}