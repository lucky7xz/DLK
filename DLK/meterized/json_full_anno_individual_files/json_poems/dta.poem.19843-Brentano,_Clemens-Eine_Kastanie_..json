{"dta.poem.19843": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Eine Kastanie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jockel. Guck, Bastel, wa\u00df ich funden han,               ", "tokens": ["Jo\u00b7ckel", ".", "Guck", ",", "Bas\u00b7tel", ",", "wa\u00df", "ich", "fun\u00b7den", "han", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$,", "NE", "$,", "KOUS", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Es hat ein Igels Pelzlein an,", "tokens": ["Es", "hat", "ein", "I\u00b7gels", "Pelz\u00b7lein", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein braunes ledern Koller drunter,", "tokens": ["Ein", "brau\u00b7nes", "le\u00b7dern", "Kol\u00b7ler", "drun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Woll gef\u00fcttert, guck! lug! Wunder!", "tokens": ["Mit", "Woll", "ge\u00b7f\u00fct\u00b7tert", ",", "guck", "!", "lug", "!", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVIMP", "$.", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie glatt liegt ihm an Hos' und Hemd.", "tokens": ["Wie", "glatt", "liegt", "ihm", "an", "Hos'", "und", "Hemd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "APPR", "NE", "KON", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Au, Au! mich d\u00fcnkt, s' schmeckt unversch\u00e4mt,", "tokens": ["Au", ",", "Au", "!", "mich", "d\u00fcnkt", ",", "s'", "schmeckt", "un\u00b7ver\u00b7sch\u00e4mt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PPER", "VVFIN", "$,", "NE", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Doch lug! indem ich sch\u00e4l' den Kern,", "tokens": ["Doch", "lug", "!", "in\u00b7dem", "ich", "sch\u00e4l'", "den", "Kern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So schmeckt es s\u00fc\u00df, das e\u00df ich gern!", "tokens": ["So", "schmeckt", "es", "s\u00fc\u00df", ",", "das", "e\u00df", "ich", "gern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bastel. Mein Jockel, dieses hei\u00dft ein K\u00e4sten,               ", "tokens": ["Bas\u00b7tel", ".", "Mein", "Jo\u00b7ckel", ",", "die\u00b7ses", "hei\u00dft", "ein", "K\u00e4s\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wir Schwaben wissens doch am besten.", "tokens": ["Wir", "Schwa\u00b7ben", "wis\u00b7sens", "doch", "am", "bes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drum nennen wir dies Iglein fein,", "tokens": ["Drum", "nen\u00b7nen", "wir", "dies", "Ig\u00b7lein", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PDS", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenns gesch\u00e4hlt ist aus dem Pelzlein sein,", "tokens": ["Wenns", "ge\u00b7sch\u00e4hlt", "ist", "aus", "dem", "Pelz\u00b7lein", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "VAFIN", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ein N\u00fc\u00dflein in eim Lederlein.", "tokens": ["Ein", "N\u00fc\u00df\u00b7lein", "in", "eim", "Le\u00b7der\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Jockel. Ei das war mir ein Schneiderlein,               ", "tokens": ["Jo\u00b7ckel", ".", "Ei", "das", "war", "mir", "ein", "Schnei\u00b7derl\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "PDS", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ders n\u00e4hen konnte also fein!", "tokens": ["Ders", "n\u00e4\u00b7hen", "konn\u00b7te", "al\u00b7so", "fein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}