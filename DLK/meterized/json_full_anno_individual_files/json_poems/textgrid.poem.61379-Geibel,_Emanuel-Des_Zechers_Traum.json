 {"text": "Waren eitel S\u00fc\u00dfigkeit und schmolzen", "tokens": ["Wa\u00b7ren", "ei\u00b7tel", "S\u00fc\u00b7\u00dfig\u00b7keit", "und", "schmol\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "NN", "KON", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "In der hei\u00dfen Flut; des Abends Strahlen", "tokens": ["In", "der", "hei\u00b7\u00dfen", "Flut", ";", "des", "A\u00b7bends", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Schossen als ein goldner Strom herunter", "tokens": ["Schos\u00b7sen", "als", "ein", "gold\u00b7ner", "Strom", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Edlen Geists und f\u00e4rbten bis zum Rande", "tokens": ["Ed\u00b7len", "Geists", "und", "f\u00e4rb\u00b7ten", "bis", "zum", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "VVFIN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Nun die Mischung, da\u00df sie zitternd gl\u00e4nzte.", "tokens": ["Nun", "die", "Misc\u00b7hung", ",", "da\u00df", "sie", "zit\u00b7ternd", "gl\u00e4nz\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Doch zuletzt als Riesenpomeranze", "tokens": ["Doch", "zu\u00b7letzt", "als", "Rie\u00b7sen\u00b7po\u00b7me\u00b7ran\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Sank die Sonn' herab und wogte schwimmend", "tokens": ["Sank", "die", "Sonn'", "her\u00b7ab", "und", "wog\u00b7te", "schwim\u00b7mend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "KON", "VVFIN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Auf dem Trank dahin, die Schale kr\u00f6nend.", "tokens": ["Auf", "dem", "Trank", "da\u00b7hin", ",", "die", "Scha\u00b7le", "kr\u00f6\u00b7nend", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und begierig mit den trocknen Lippen", "tokens": ["Und", "be\u00b7gie\u00b7rig", "mit", "den", "trock\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schl\u00fcrfend setzt' ich an, und schon ber\u00fchrte", "tokens": ["Schl\u00fcr\u00b7fend", "setzt'", "ich", "an", ",", "und", "schon", "be\u00b7r\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mir das seltne Na\u00df den Mund \u2013 da weckte", "tokens": ["Mir", "das", "selt\u00b7ne", "Na\u00df", "den", "Mund", "\u2013", "da", "weck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "ART", "NN", "$(", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mich der Schlag der Uhr; vom Lager fuhr ich", "tokens": ["Mich", "der", "Schlag"