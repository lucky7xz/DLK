{"textgrid.poem.61379": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Des Zechers Traum", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit den Freunden bei der m\u00e4cht'gen Bowle", "tokens": ["Mit", "den", "Freun\u00b7den", "bei", "der", "m\u00e4cht'\u00b7gen", "Bow\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hatt' ich tief bis in die Nacht gesessen;", "tokens": ["Hatt'", "ich", "tief", "bis", "in", "die", "Nacht", "ge\u00b7ses\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sieh, da kam im Schlaf ein seltner Traum mir.", "tokens": ["Sieh", ",", "da", "kam", "im", "Schlaf", "ein", "selt\u00b7ner", "Traum", "mir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "An dem Strand des unfruchtbaren Meeres", "tokens": ["An", "dem", "Strand", "des", "un\u00b7frucht\u00b7ba\u00b7ren", "Mee\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Irrt' ich von gewalt'gem Durst gepeinigt", "tokens": ["Irrt'", "ich", "von", "ge\u00b7walt'\u00b7gem", "Durst", "ge\u00b7pei\u00b7nigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hin und her zur Zeit der Sonnenr\u00fcste;", "tokens": ["Hin", "und", "her", "zur", "Zeit", "der", "Son\u00b7nen\u00b7r\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Eine Quelle sucht' ich, einen Brunnen,", "tokens": ["Ei\u00b7ne", "Quel\u00b7le", "sucht'", "ich", ",", "ei\u00b7nen", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mich zu laben, doch umsonst! Da rief ich", "tokens": ["Mich", "zu", "la\u00b7ben", ",", "doch", "um\u00b7sonst", "!", "Da", "rief", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADV", "ADV", "$.", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Sehnsuchtsvoll umher mit heisrer Stimme:", "tokens": ["Sehn\u00b7suchts\u00b7voll", "um\u00b7her", "mit", "heis\u00b7rer", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "O wer schafft zu trinken mir, zu trinken,", "tokens": ["O", "wer", "schafft", "zu", "trin\u00b7ken", "mir", ",", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VVFIN", "PTKZU", "VVFIN", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Aber nicht zu wenig \u2013 ich vers