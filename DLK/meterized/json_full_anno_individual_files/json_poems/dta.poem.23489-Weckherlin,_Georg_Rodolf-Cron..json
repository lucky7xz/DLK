{"dta.poem.23489": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cron.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Volcks wolfahrt soll das h\u00f6chst", "tokens": ["Des", "Volcks", "wol\u00b7fahrt", "soll", "das", "h\u00f6chst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VMFIN", "PDS", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "gesatz sein", "tokens": ["ge\u00b7satz", "sein"], "token_info": ["word", "word"], "pos": ["ADJD", "VAINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Das thun all seine Sitten lehren.", "tokens": ["Das", "thun", "all", "sei\u00b7ne", "Sit\u00b7ten", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seines gesichts Mayestetischer schein/", "tokens": ["Sei\u00b7nes", "ge\u00b7sichts", "Ma\u00b7ye\u00b7ste\u00b7ti\u00b7scher", "schein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Seiner augen ernstliche lehren/", "tokens": ["Sei\u00b7ner", "au\u00b7gen", "ernst\u00b7li\u00b7che", "leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "VVINF", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Seiner person gantz herrliche gestalt/", "tokens": ["Sei\u00b7ner", "per\u00b7son", "gantz", "herr\u00b7li\u00b7che", "ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJA", "NN", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Seines versta\u0303ds stehts-from\u0303ender gewalt/", "tokens": ["Sei\u00b7nes", "ver\u00b7st\u00e3ds", "stehts\u00b7from\u0303en\u00b7der", "ge\u00b7walt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Darab sich die menschen entsetzen/", "tokens": ["Da\u00b7rab", "sich", "die", "men\u00b7schen", "ent\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Damit sich die G\u00f6tter erg\u00f6tzen/", "tokens": ["Da\u00b7mit", "sich", "die", "G\u00f6t\u00b7ter", "er\u00b7g\u00f6t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Seines munds wahrhaftige frucht/", "tokens": ["Sei\u00b7nes", "munds", "wahr\u00b7haf\u00b7ti\u00b7ge", "frucht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Dadurch die sehl sich bessert vnd erfrewet/", "tokens": ["Da\u00b7durch", "die", "sehl", "sich", "bes\u00b7sert", "vnd", "er\u00b7fre\u00b7wet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "VVPP", "KON", "VVFIN", "$("], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.12": {"text": "Seiner geberden kh\u00fcne zucht/", "tokens": ["Sei\u00b7ner", "ge\u00b7ber\u00b7den", "kh\u00fc\u00b7ne", "zucht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Welche mit ernst schand vn\u0303laster zustr\u00f6wet/", "tokens": ["Wel\u00b7che", "mit", "ernst", "schand", "v\u00f1las\u00b7ter", "zu\u00b7str\u00f6\u00b7wet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ADJD", "VVFIN", "ADJD", "VVFIN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Vnd sein Vrthail/ der K\u00fcnsten lohn/", "tokens": ["Vnd", "sein", "Vrt\u00b7hail", "/", "der", "K\u00fcns\u00b7ten", "lohn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Ja was Er ist/ alles macht vns bekennen/", "tokens": ["Ja", "was", "Er", "ist", "/", "al\u00b7les", "macht", "vns", "be\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWS", "PPER", "VAFIN", "$(", "PIS", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Das Wir Jhn m\u00fcssen billich nennen", "tokens": ["Das", "Wir", "Jhn", "m\u00fcs\u00b7sen", "bil\u00b7lich", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der Musen trost/ vn\u0303 der Tugenten wohn.", "tokens": ["Der", "Mu\u00b7sen", "trost", "/", "v\u00f1", "der", "Tu\u00b7gen\u00b7ten", "wohn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}