{"dta.poem.14009": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsmarsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wir zieh'n durch die Provinzen,", "tokens": ["Wir", "zieh'n", "durch", "die", "Pro\u00b7vin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da funkelt manches Schlo\u00df,", "tokens": ["Da", "fun\u00b7kelt", "man\u00b7ches", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00f6n Lieb, hol' Dich vom Zwinger", "tokens": ["Sch\u00f6n", "Lieb", ",", "hol'", "Dich", "vom", "Zwin\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schwing' Dich mit auf's Ro\u00df!", "tokens": ["Und", "schwing'", "Dich", "mit", "auf's", "Ro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}