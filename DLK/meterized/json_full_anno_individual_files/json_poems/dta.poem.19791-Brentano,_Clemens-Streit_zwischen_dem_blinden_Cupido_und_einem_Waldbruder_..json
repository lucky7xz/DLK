{"dta.poem.19791": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Streit zwischen dem blinden Cupido und  \n einem Waldbruder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Cupido. Willkomm mein lieber Eremit!               ", "tokens": ["Cu\u00b7pi\u00b7do", ".", "Will\u00b7komm", "mein", "lie\u00b7ber", "E\u00b7re\u00b7mit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Was machst in dieser finstern H\u00fctt?", "tokens": ["Was", "machst", "in", "die\u00b7ser", "fins\u00b7tern", "H\u00fctt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie kommts, da\u00df der verdrieslich Wald", "tokens": ["Wie", "kommts", ",", "da\u00df", "der", "ver\u00b7dries\u00b7lich", "Wald"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir besser als die Stadt gefallt?", "tokens": ["Dir", "bes\u00b7ser", "als", "die", "Stadt", "ge\u00b7fallt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Soll dann ein so betr\u00fcbter Stand", "tokens": ["Soll", "dann", "ein", "so", "be\u00b7tr\u00fcb\u00b7ter", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADV", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Das grob und rauhe Klausnerg'wand", "tokens": ["Das", "grob", "und", "rau\u00b7he", "Klaus\u00b7ner\u00b7g'\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Den sch\u00f6nsten Kleidern von Drador", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "Klei\u00b7dern", "von", "Dra\u00b7dor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE"], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.8": {"text": "Und Silber gehen vor.", "tokens": ["Und", "Sil\u00b7ber", "ge\u00b7hen", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Eremit. Ein G'm\u00fcth, so nach dem Himmel tracht,               ", "tokens": ["E\u00b7re\u00b7mit", ".", "Ein", "G'\u00b7m\u00fcth", ",", "so", "nach", "dem", "Him\u00b7mel", "tracht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Acht' kein Geschmuck noch Kleiderpracht,", "tokens": ["Acht'", "kein", "Ge\u00b7schmuck", "noch", "Klei\u00b7der\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein H\u00fctt so mich bedecken kann,", "tokens": ["Ein", "H\u00fctt", "so", "mich", "be\u00b7de\u00b7cken", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist stattlich gnug f\u00fcr mein Person:", "tokens": ["Ist", "statt\u00b7lich", "gnug", "f\u00fcr", "mein", "Per\u00b7son", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dazu wo findt man gr\u00f6\u00dfre Freud,", "tokens": ["Da\u00b7zu", "wo", "findt", "man", "gr\u00f6\u00df\u00b7re", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWAV", "VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als in der s\u00fc\u00dfen Einsamkeit?", "tokens": ["Als", "in", "der", "s\u00fc\u00b7\u00dfen", "Ein\u00b7sam\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da kann man in vergn\u00fcgter Ruh", "tokens": ["Da", "kann", "man", "in", "ver\u00b7gn\u00fcg\u00b7ter", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Leben bringen zu.", "tokens": ["Sein", "Le\u00b7ben", "brin\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Cupido. Ja, ja, hast recht, ich stimm dir bey               ", "tokens": ["Cu\u00b7pi\u00b7do", ".", "Ja", ",", "ja", ",", "hast", "recht", ",", "ich", "stimm", "dir", "bey"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PTKANT", "$,", "PTKANT", "$,", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "PPER", "APPR"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Da\u00df es kein gemeiner Wollust sey,", "tokens": ["Da\u00df", "es", "kein", "ge\u00b7mei\u00b7ner", "Wol\u00b7lust", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zubringen seine Lebenszeit", "tokens": ["Zu\u00b7brin\u00b7gen", "sei\u00b7ne", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In W\u00e4ldern mit der Jagdbarkeit,", "tokens": ["In", "W\u00e4l\u00b7dern", "mit", "der", "Jagd\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo man die Hirschen und die Reh", "tokens": ["Wo", "man", "die", "Hir\u00b7schen", "und", "die", "Reh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sieht lustig springen in die H\u00f6h,", "tokens": ["Sieht", "lus\u00b7tig", "sprin\u00b7gen", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch aber so verschlossen sein,", "tokens": ["Doch", "a\u00b7ber", "so", "ver\u00b7schlos\u00b7sen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das geht mir gar nicht ein.", "tokens": ["Das", "geht", "mir", "gar", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Eremit. Ist nur ein schn\u00f6de Eitelkeit               ", "tokens": ["E\u00b7re\u00b7mit", ".", "Ist", "nur", "ein", "schn\u00f6\u00b7de", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Das irdisch Gesch\u00fctz und Jagdbarkeit,", "tokens": ["Das", "ir\u00b7disch", "Ge\u00b7sch\u00fctz", "und", "Jagd\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein rein anm\u00fcthig Klausnerg'm\u00fcth,", "tokens": ["Ein", "rein", "an\u00b7m\u00fct\u00b7hig", "Klaus\u00b7ner\u00b7g'\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das ist allein mein Jagdgebieth,", "tokens": ["Das", "ist", "al\u00b7lein", "mein", "Jagd\u00b7ge\u00b7bieth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit dem Brevier so mein Gescho\u00df,", "tokens": ["Mit", "dem", "Bre\u00b7vier", "so", "mein", "Ge\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Geh ich auf gutes Waidwerk los,", "tokens": ["Geh", "ich", "auf", "gu\u00b7tes", "Waid\u00b7werk", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bring meiner Seele einen Schmaus", "tokens": ["Bring", "mei\u00b7ner", "See\u00b7le", "ei\u00b7nen", "Schmaus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von dieser Jagd nach Haus.", "tokens": ["Von", "die\u00b7ser", "Jagd", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Cupido. Hast du Lust zu dem Brevier,               ", "tokens": ["Cu\u00b7pi\u00b7do", ".", "Hast", "du", "Lust", "zu", "dem", "Bre\u00b7vier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie gefallt dir das? hab eins bei mir,", "tokens": ["Wie", "ge\u00b7fallt", "dir", "das", "?", "hab", "eins", "bei", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "PPER", "PDS", "$.", "VAFIN", "PIS", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das braucht so viel Durchbl\u00e4ttern nicht.", "tokens": ["Das", "braucht", "so", "viel", "Durch\u00b7bl\u00e4t\u00b7tern", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verla\u00df den Wald und gehe mit,", "tokens": ["Ver\u00b7la\u00df", "den", "Wald", "und", "ge\u00b7he", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will dich f\u00fchren in die Stadt,", "tokens": ["Ich", "will", "dich", "f\u00fch\u00b7ren", "in", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So sch\u00f6ne Pl\u00e4z und H\u00e4user hat,", "tokens": ["So", "sch\u00f6\u00b7ne", "Pl\u00e4z", "und", "H\u00e4u\u00b7ser", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dort leben kannst in guter Ruh,", "tokens": ["Dort", "le\u00b7ben", "kannst", "in", "gu\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Komm! schlag dein H\u00fctte zu.", "tokens": ["Komm", "!", "schlag", "dein", "H\u00fct\u00b7te", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Eremit. Wer Gott recht liebt, ihm dienen will,               ", "tokens": ["E\u00b7re\u00b7mit", ".", "Wer", "Gott", "recht", "liebt", ",", "ihm", "die\u00b7nen", "will", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "NN", "ADJD", "VVFIN", "$,", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Dem ist das Beten nicht zu viel,", "tokens": ["Dem", "ist", "das", "Be\u00b7ten", "nicht", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PTKNEG", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Fasten und die Geisselstreich,", "tokens": ["Das", "Fas\u00b7ten", "und", "die", "Geis\u00b7sel\u00b7streich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die bringen mich ins Himmelreich;", "tokens": ["Die", "brin\u00b7gen", "mich", "ins", "Him\u00b7mel\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum geh nur fort verf\u00fchrisch Kind,", "tokens": ["Drum", "geh", "nur", "fort", "ver\u00b7f\u00fch\u00b7risch", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PTKVZ", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Rath ist nichts als ungesinnt,", "tokens": ["Dein", "Rath", "ist", "nichts", "als", "un\u00b7ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df mich in meiner Klausnerey", "tokens": ["La\u00df", "mich", "in", "mei\u00b7ner", "Klaus\u00b7ne\u00b7rey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Andacht wohnen bey.", "tokens": ["Der", "An\u00b7dacht", "woh\u00b7nen", "bey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Cupido. Du bist der erst mein Eremit!               ", "tokens": ["Cu\u00b7pi\u00b7do", ".", "Du", "bist", "der", "erst", "mein", "E\u00b7re\u00b7mit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "ART", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Der mich verst\u00f6\u00dft aus seiner H\u00fctt,", "tokens": ["Der", "mich", "ver\u00b7st\u00f6\u00dft", "aus", "sei\u00b7ner", "H\u00fctt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist da wie im Himmel drein,", "tokens": ["Du", "bist", "da", "wie", "im", "Him\u00b7mel", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOKOM", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Qu\u00e4l dich einmal ein Gott zu seyn,", "tokens": ["Qu\u00e4l", "dich", "ein\u00b7mal", "ein", "Gott", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast wohl nicht dazu den Muth,", "tokens": ["Du", "hast", "wohl", "nicht", "da\u00b7zu", "den", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin ein armes, junges Blut,", "tokens": ["Ich", "bin", "ein", "ar\u00b7mes", ",", "jun\u00b7ges", "Blut", ","], "token_info": ["wo