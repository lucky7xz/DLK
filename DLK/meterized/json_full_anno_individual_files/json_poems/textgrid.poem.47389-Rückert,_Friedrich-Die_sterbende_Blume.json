{"textgrid.poem.47389": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die sterbende Blume", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoffe! du erlebst es noch,", "tokens": ["Hof\u00b7fe", "!", "du", "er\u00b7lebst", "es", "noch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df der Fr\u00fchling wiederkehrt.", "tokens": ["Da\u00df", "der", "Fr\u00fch\u00b7ling", "wie\u00b7der\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoffen alle B\u00e4ume doch,", "tokens": ["Hof\u00b7fen", "al\u00b7le", "B\u00e4u\u00b7me", "doch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die des Herbstes Wind verheert,", "tokens": ["Die", "des", "Herbs\u00b7tes", "Wind", "ver\u00b7heert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoffen mit der stillen Kraft", "tokens": ["Hof\u00b7fen", "mit", "der", "stil\u00b7len", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihrer Knospen winterlang,", "tokens": ["Ih\u00b7rer", "Knos\u00b7pen", "win\u00b7ter\u00b7lang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis sich wieder regt der Saft", "tokens": ["Bis", "sich", "wie\u00b7der", "regt", "der", "Saft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ein neues Gr\u00fcn entsprang. \u2013", "tokens": ["Und", "ein", "neu\u00b7es", "Gr\u00fcn", "ent\u00b7sprang", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbach, ich bin kein starker Baum,", "tokens": ["\u00bb", "ach", ",", "ich", "bin", "kein", "star\u00b7ker", "Baum", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ein Sommertausend lebt,", "tokens": ["Der", "ein", "Som\u00b7mer\u00b7tau\u00b7send", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach vertr\u00e4umtem Wintertraum", "tokens": ["Nach", "ver\u00b7tr\u00e4um\u00b7tem", "Win\u00b7ter\u00b7traum"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Neue Lenzgedichte webt.", "tokens": ["Neu\u00b7e", "Lenz\u00b7ge\u00b7dich\u00b7te", "webt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, ich bin die Blume nur,", "tokens": ["Ach", ",", "ich", "bin", "die", "Blu\u00b7me", "nur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die des Maies Ku\u00df geweckt", "tokens": ["Die", "des", "Mai\u00b7es", "Ku\u00df", "ge\u00b7weckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und von der nicht bleibt die Spur,", "tokens": ["Und", "von", "der", "nicht", "bleibt", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie das wei\u00dfe Grab sie deckt.\u00ab \u2013", "tokens": ["Wie", "das", "wei\u00b7\u00dfe", "Grab", "sie", "deckt", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn du denn die Blume bist,", "tokens": ["Wenn", "du", "denn", "die", "Blu\u00b7me", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O bescheidenes Gem\u00fct,", "tokens": ["O", "be\u00b7schei\u00b7de\u00b7nes", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00f6ste dich, beschieden ist", "tokens": ["Tr\u00f6s\u00b7te", "dich", ",", "be\u00b7schie\u00b7den", "ist"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Samen allem, was da bl\u00fcht.", "tokens": ["Sa\u00b7men", "al\u00b7lem", ",", "was", "da", "bl\u00fcht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df den Sturm des Todes doch", "tokens": ["La\u00df", "den", "Sturm", "des", "To\u00b7des", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deinen Lebensstaub verstreun,", "tokens": ["Dei\u00b7nen", "Le\u00b7bens\u00b7staub", "ver\u00b7streun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aus dem Staube wirst du noch", "tokens": ["Aus", "dem", "Stau\u00b7be", "wirst", "du", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hundertmal dich selbst erneun. \u2013", "tokens": ["Hun\u00b7dert\u00b7mal", "dich", "selbst", "er\u00b7neun", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbja, es werden nach mir bl\u00fchn", "tokens": ["\u00bb", "ja", ",", "es", "wer\u00b7den", "nach", "mir", "bl\u00fchn"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Andre, die mir \u00e4hnlich sind;", "tokens": ["And\u00b7re", ",", "die", "mir", "\u00e4hn\u00b7lich", "sind", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig ist das ganze Gr\u00fcn,", "tokens": ["E\u00b7wig", "ist", "das", "gan\u00b7ze", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur das einzle welkt geschwind.", "tokens": ["Nur", "das", "einz\u00b7le", "welkt", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber, sind sie, was ich war,", "tokens": ["A\u00b7ber", ",", "sind", "sie", ",", "was", "ich", "war", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bin ich selber es nicht mehr;", "tokens": ["Bin", "ich", "sel\u00b7ber", "es", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jetzt nur bin ich ganz und gar,", "tokens": ["Jetzt", "nur", "bin", "ich", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht zuvor und nicht nachher.", "tokens": ["Nicht", "zu\u00b7vor", "und", "nicht", "nach\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KON", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn einst sie der Sonne Blick", "tokens": ["Wenn", "einst", "sie", "der", "Son\u00b7ne", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "W\u00e4rmt, der jetzt noch mich durchflammt,", "tokens": ["W\u00e4rmt", ",", "der", "jetzt", "noch", "mich", "durch\u00b7flammt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lindert das nicht mein Geschick,", "tokens": ["Lin\u00b7dert", "das", "nicht", "mein", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Das mich nun zur Nacht verdammt.", "tokens": ["Das", "mich", "nun", "zur", "Nacht", "ver\u00b7dammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonne, ja du \u00e4ugelst schon", "tokens": ["Son\u00b7ne", ",", "ja", "du", "\u00e4u\u00b7gelst", "schon"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihnen in die Fernen zu;", "tokens": ["Ih\u00b7nen", "in", "die", "Fer\u00b7nen", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Warum noch mit frost'gem Hohn", "tokens": ["Wa\u00b7rum", "noch", "mit", "frost'\u00b7gem", "Hohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.8": {"text": "Mir aus Wolken l\u00e4chelst du?", "tokens": ["Mir", "aus", "Wol\u00b7ken", "l\u00e4\u00b7chelst", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weh mir, da\u00df ich dir vertraut,", "tokens": ["Weh", "mir", ",", "da\u00df", "ich", "dir", "ver\u00b7traut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als mich wach gek\u00fc\u00dft dein Strahl;", "tokens": ["Als", "mich", "wach", "ge\u00b7k\u00fc\u00dft", "dein", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ins Aug' ich dir geschaut,", "tokens": ["Da\u00df", "ins", "Aug'", "ich", "dir", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis es mir das Leben stahl!", "tokens": ["Bis", "es", "mir", "das", "Le\u00b7ben", "stahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Lebens armen Rest", "tokens": ["Die\u00b7ses", "Le\u00b7bens", "ar\u00b7men", "Rest"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deinem Mitleid zu entziehn,", "tokens": ["Dei\u00b7nem", "Mit\u00b7leid", "zu", "ent\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schlie\u00dfen will ich krankhaft fest", "tokens": ["Schlie\u00b7\u00dfen", "will", "ich", "krank\u00b7haft", "fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich in mich und dir entfliehn.", "tokens": ["Mich", "in", "mich", "und", "dir", "ent\u00b7fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "KON", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch du schmelzest meines Grimms", "tokens": ["Doch", "du", "schmel\u00b7zest", "mei\u00b7nes", "Grimms"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Starres Eis in Thr\u00e4nen auf;", "tokens": ["Star\u00b7res", "Eis", "in", "Thr\u00e4\u00b7nen", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm mein fliehend Leben, nimm's,", "tokens": ["Nimm", "mein", "flie\u00b7hend", "Le\u00b7ben", ",", "nim\u00b7m's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJD", "NN", "$,", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ewige, zu dir hinauf!", "tokens": ["E\u00b7wi\u00b7ge", ",", "zu", "dir", "hin\u00b7auf", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, du sonnest noch den Gram", "tokens": ["Ja", ",", "du", "son\u00b7nest", "noch", "den", "Gram"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der Seele mir zuletzt;", "tokens": ["Aus", "der", "See\u00b7le", "mir", "zu\u00b7letzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles, was von dir mir kam,", "tokens": ["Al\u00b7les", ",", "was", "von", "dir", "mir", "kam", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sterbend dank' ich dir es jetzt:", "tokens": ["Ster\u00b7bend", "dank'", "ich", "dir", "es", "jetzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aller L\u00fcfte Morgenzug,", "tokens": ["Al\u00b7ler", "L\u00fcf\u00b7te", "Mor\u00b7gen\u00b7zug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem ich sommerlang gebebt,", "tokens": ["Dem", "ich", "som\u00b7mer\u00b7lang", "ge\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Schmetterlinge Flug,", "tokens": ["Al\u00b7ler", "Schmet\u00b7ter\u00b7lin\u00b7ge", "Flug", ","], "token_info": ["word", "word", "