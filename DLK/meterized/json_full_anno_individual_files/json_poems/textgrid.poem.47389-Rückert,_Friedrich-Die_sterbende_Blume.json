tra"}, "line.8": {"text": "Und zur sch\u00f6nen Welt hinein.", "tokens": ["Und", "zur", "sch\u00f6\u00b7nen", "Welt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Ew'ges Flammenherz der Welt,", "tokens": ["Ew'\u00b7ges", "Flam\u00b7men\u00b7herz", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df verglimmen mich an dir!", "tokens": ["La\u00df", "ver\u00b7glim\u00b7men", "mich", "an", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Himmel, spann' dein blaues Zelt,", "tokens": ["Him\u00b7mel", ",", "spann'", "dein", "blau\u00b7es", "Zelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein vergr\u00fcntes sinket hier.", "tokens": ["Mein", "ver\u00b7gr\u00fcn\u00b7tes", "sin\u00b7ket", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heil, o Fr\u00fchling, deinem Schein!", "tokens": ["Heil", ",", "o", "Fr\u00fch\u00b7ling", ",", "dei\u00b7nem", "Schein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Morgenluft, Heil deinem Weh'n!", "tokens": ["Mor\u00b7gen\u00b7luft", ",", "Heil", "dei\u00b7nem", "Weh'n", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne Kummer schlaf' ich ein,", "tokens": ["Oh\u00b7ne", "Kum\u00b7mer", "schlaf'", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ohne Hoffnung aufzustehn.\u00ab", "tokens": ["Oh\u00b7ne", "Hoff\u00b7nung", "auf\u00b7zu\u00b7stehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}