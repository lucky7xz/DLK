{"textgrid.poem.36484": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "6. Ein Sperling und eine Taube", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein loser Sperling sprach zu einer frommen Taube,", "tokens": ["Ein", "lo\u00b7ser", "Sper\u00b7ling", "sprach", "zu", "ei\u00b7ner", "from\u00b7men", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als w\u00e4r's sein rechter Ernst: Frau Nachbarin, ich glaube,", "tokens": ["Als", "w\u00e4r's", "sein", "rech\u00b7ter", "Ernst", ":", "Frau", "Nach\u00b7ba\u00b7rin", ",", "ich", "glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "NN", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott schuf den Menschen uns zu unserm Dienst \u2013 Er streut,", "tokens": ["Gott", "schuf", "den", "Men\u00b7schen", "uns", "zu", "un\u00b7serm", "Dienst", "\u2013", "Er", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zwar mit aller Freundlichkeit,", "tokens": ["Und", "zwar", "mit", "al\u00b7ler", "Freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uns unser Futter alle Morgen", "tokens": ["Uns", "un\u00b7ser", "Fut\u00b7ter", "al\u00b7le", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor unsern Schnabel! \u2013 Nahrungssorgen", "tokens": ["Vor", "un\u00b7sern", "Schna\u00b7bel", "!", "\u2013", "Nah\u00b7rungs\u00b7sor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$(", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hat Er: und wir, wir Kinder der Natur,", "tokens": ["Hat", "Er", ":", "und", "wir", ",", "wir", "Kin\u00b7der", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$.", "KON", "PPER", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wir essen, und wir lieben nur!", "tokens": ["Wir", "es\u00b7sen", ",", "und", "wir", "lie\u00b7ben", "nur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit nichten, sagte da die Taube,", "tokens": ["Mit", "nich\u00b7ten", ",", "sag\u00b7te", "da", "die", "Tau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Futter streut der Mensch nicht dir;", "tokens": ["Das", "Fut\u00b7ter", "streut", "der", "Mensch", "nicht", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er streut es meinem Mann und mir!", "tokens": ["Er", "streut", "es", "mei\u00b7nem", "Mann", "und", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, mir allein, und meinem Mann!", "tokens": ["Ja", ",", "mir", "al\u00b7lein", ",", "und", "mei\u00b7nem", "Mann", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "ADV", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist ein Dieb, du lebst vom Raube.", "tokens": ["Du", "bist", "ein", "Dieb", ",", "du", "lebst", "vom", "Rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Glaub' ist falscher Glaube,", "tokens": ["Dein", "Glaub'", "ist", "fal\u00b7scher", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den ich an dir nicht leiden kann!", "tokens": ["Den", "ich", "an", "dir", "nicht", "lei\u00b7den", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein loser Sperling sprach zu einer frommen Taube,", "tokens": ["Ein", "lo\u00b7ser", "Sper\u00b7ling", "sprach", "zu", "ei\u00b7ner", "from\u00b7men", "Tau\u00b7be", ","], "tok