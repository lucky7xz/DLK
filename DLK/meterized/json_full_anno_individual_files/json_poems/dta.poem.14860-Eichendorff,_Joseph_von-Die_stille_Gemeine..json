{"dta.poem.14860": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die stille Gemeine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eer sah so wunderbar auf mich,", "tokens": ["\u201e", "er", "sah", "so", "wun\u00b7der\u00b7bar", "auf", "mich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich konnt' ihn nicht ermorden \u2014", "tokens": ["Ich", "konnt'", "ihn", "nicht", "er\u00b7mor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sank die Burg, er wandte sich", "tokens": ["Da", "sank", "die", "Burg", ",", "er", "wand\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist ein Pfaff geworden.\u201c", "tokens": ["Und", "ist", "ein", "Pfaff", "ge\u00b7wor\u00b7den", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ART", "NE", "VAPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}