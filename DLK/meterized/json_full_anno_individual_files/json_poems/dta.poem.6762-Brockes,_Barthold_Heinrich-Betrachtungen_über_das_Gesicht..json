{"dta.poem.6762": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und weil man auch durch den Gebrauch gelernt,", "tokens": ["Und", "weil", "man", "auch", "durch", "den", "Ge\u00b7brauch", "ge\u00b7lernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Da\u00df C\u00f6rper, die von uns entfernt,", "tokens": ["Da\u00df", "C\u00f6r\u00b7per", ",", "die", "von", "uns", "ent\u00b7fernt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "PRELS", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So deutlich nicht, und nicht so gro\u00df erscheinen,", "tokens": ["So", "deut\u00b7lich", "nicht", ",", "und", "nicht", "so", "gro\u00df", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "$,", "KON", "PTKNEG", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als wenn die Blicke sich mit ihnen nah vereinen:", "tokens": ["Als", "wenn", "die", "Bli\u00b7cke", "sich", "mit", "ih\u00b7nen", "nah", "ver\u00b7ei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PRF", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dadurch, da\u00df wir den Stand von ihrer Gr\u00f6sse kennen,", "tokens": ["Da\u00b7durch", ",", "da\u00df", "wir", "den", "Stand", "von", "ih\u00b7rer", "Gr\u00f6s\u00b7se", "ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und da\u00df man ihre Z\u00fcg\u2019 und Farben schwinden sieht;", "tokens": ["Und", "da\u00df", "man", "ih\u00b7re", "Z\u00fcg'", "und", "Far\u00b7ben", "schwin\u00b7den", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPOSAT", "NN", "KON", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Beurtheilt ein Gem\u00fcth,", "tokens": ["Beurt\u00b7heilt", "ein", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Durch die erkannte Gr\u00f6ss\u2019 auch ihrer Weite Stand,", "tokens": ["Durch", "die", "er\u00b7kann\u00b7te", "Gr\u00f6ss'", "auch", "ih\u00b7rer", "Wei\u00b7te", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wie durch ihre Fern die Gr\u00f6sse wird erkannt.", "tokens": ["So", "wie", "durch", "ih\u00b7re", "Fern", "die", "Gr\u00f6s\u00b7se", "wird", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "PPOSAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}