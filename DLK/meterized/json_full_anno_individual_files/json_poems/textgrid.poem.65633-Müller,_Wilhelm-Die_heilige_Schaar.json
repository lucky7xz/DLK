{"textgrid.poem.65633": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die heilige Schaar", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundes Herz an Freundes Herzen, Freundes Hand in Freundes Hand,", "tokens": ["Freun\u00b7des", "Herz", "an", "Freun\u00b7des", "Her\u00b7zen", ",", "Freun\u00b7des", "Hand", "in", "Freun\u00b7des", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "NN", "$,", "NN", "NN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Unverr\u00fcckt in Glied und Reihe, hielten wir dem Tode Stand,", "tokens": ["Un\u00b7ver\u00b7r\u00fcckt", "in", "Glied", "und", "Rei\u00b7he", ",", "hiel\u00b7ten", "wir", "dem", "To\u00b7de", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Liegen alle auf dem R\u00fccken, himmelw\u00e4rts den Blick gekehrt,", "tokens": ["Lie\u00b7gen", "al\u00b7le", "auf", "dem", "R\u00fc\u00b7cken", ",", "him\u00b7mel\u00b7w\u00e4rts", "den", "Blick", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "In der Brust die Todeswunden, in der Faust das rothe Schwert.", "tokens": ["In", "der", "Brust", "die", "To\u00b7des\u00b7wun\u00b7den", ",", "in", "der", "Faust", "das", "ro\u00b7the", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Nennt uns nicht die letzten Griechen \u2013 Sollen wir die letzten sein?", "tokens": ["Nennt", "uns", "nicht", "die", "letz\u00b7ten", "Grie\u00b7chen", "\u2013", "Sol\u00b7len", "wir", "die", "letz\u00b7ten", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$(", "VMFIN", "PPER", "ART", "ADJA", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Die dem Vaterlande freudig Blut und Leib und Leben weihn?", "tokens": ["Die", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "freu\u00b7dig", "Blut", "und", "Leib", "und", "Le\u00b7ben", "weihn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Nennt uns nicht die letzten Griechen \u2013 Rei\u00dfender als Stahl und Erz", "tokens": ["Nennt", "uns", "nicht", "die", "letz\u00b7ten", "Grie\u00b7chen", "\u2013", "Rei\u00b7\u00dfen\u00b7der", "als", "Stahl", "und", "Erz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$(", "NN", "KOUS", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Dringt der schn\u00f6de Ehrentitel ein in unser wundes Herz.", "tokens": ["Dringt", "der", "schn\u00f6\u00b7de", "Eh\u00b7ren\u00b7ti\u00b7tel", "ein", "in", "un\u00