{"dta.poem.23485": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cron.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje Er gantz gleich an m\u00fch/ flei\u00df vnd", "tokens": ["Wie", "Er", "gantz", "gleich", "an", "m\u00fch", "/", "flei\u00df", "vnd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ADJD", "$(", "NN", "KON"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "verstand/", "tokens": ["ver\u00b7stand", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Seine Freyg\u00e4bigkeit erkl\u00e4ret/", "tokens": ["Sei\u00b7ne", "Frey\u00b7g\u00e4\u00b7big\u00b7keit", "er\u00b7kl\u00e4\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Durch die Er nicht seinen pracht/ ruhm", "tokens": ["Durch", "die", "Er", "nicht", "sei\u00b7nen", "pracht", "/", "ruhm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PRELS", "PPER", "PTKNEG", "PPOSAT", "NN", "$(", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "vnd stand/", "tokens": ["vnd", "stand", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Sondern des H\u00f6chsten lob vermehret:", "tokens": ["Son\u00b7dern", "des", "H\u00f6chs\u00b7ten", "lob", "ver\u00b7meh\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Indem niemand zu gro\u00dfen \u00fcberflu\u00df", "tokens": ["In\u00b7dem", "nie\u00b7mand", "zu", "gro\u00b7\u00dfen", "\u00fc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PTKZU", "ADJA", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "(der gemeinglich einen spahten verdru\u00df", "tokens": ["(", "der", "ge\u00b7mein\u00b7glich", "ei\u00b7nen", "spah\u00b7ten", "ver\u00b7dru\u00df"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Vnd widerwillen verursachet)", "tokens": ["Vnd", "wi\u00b7der\u00b7wil\u00b7len", "ver\u00b7ur\u00b7sa\u00b7chet", ")"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$("], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Bey Jhm falsch-lobet noch verlachet:", "tokens": ["Bey", "Jhm", "falschlo\u00b7bet", "noch", "ver\u00b7la\u00b7chet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Vnd zu\u2014freyhe Freyg\u00e4bigkeit", "tokens": ["Vnd", "zu", "frey\u00b7he", "Frey\u00b7g\u00e4\u00b7big\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "PTKZU", "$(", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Bey Jhm niemand zuverliere\u0303 erschr\u00f6cket;", "tokens": ["Bey", "Jhm", "nie\u00b7mand", "zu\u00b7ver\u00b7lier\u1ebd", "er\u00b7schr\u00f6\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Sondern sich seine Miltigkeit", "tokens": ["Son\u00b7dern", "sich", "sei\u00b7ne", "Mil\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PRF", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Auf den verdienst vnd die nohtdurft erstr\u00f6cket.", "tokens": ["Auf", "den", "ver\u00b7dienst", "vnd", "die", "noht\u00b7durft", "er\u00b7str\u00f6\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Dan ja sein hertz vnd hand recht frey/", "tokens": ["Dan", "ja", "sein", "hertz", "vnd", "hand", "recht", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "KON", "NN", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die wissen wol des bodens zuverschonen/", "tokens": ["Die", "wis\u00b7sen", "wol", "des", "bo\u00b7dens", "zu\u00b7ver\u00b7scho\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Vnd auch nach geb\u00fchr wol-zu-lohnen/", "tokens": ["Vnd", "auch", "nach", "ge\u00b7b\u00fchr", "wol\u00b7zu\u00b7loh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Auf das Sein lob w\u00fcrdig vnd wehrhaft", "tokens": ["Auf", "das", "Sein", "lob", "w\u00fcr\u00b7dig", "vnd", "wehr\u00b7haft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ADJD", "KON", "NN"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.19": {"text": "sey.", "tokens": ["sey", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}}}}}