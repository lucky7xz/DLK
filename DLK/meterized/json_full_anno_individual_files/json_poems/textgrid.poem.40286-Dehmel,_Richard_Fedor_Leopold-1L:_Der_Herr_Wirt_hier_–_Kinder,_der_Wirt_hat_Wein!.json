{"textgrid.poem.40286": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Herr Wirt hier \u2013 Kinder, der Wirt hat Wein!", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr Wirt hier \u2013 Kinder, der Wirt hat Wein!", "tokens": ["Der", "Herr", "Wirt", "hier", "\u2013", "Kin\u00b7der", ",", "der", "Wirt", "hat", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$(", "NN", "$,", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-++-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "aber la\u00dft noch, stille noch, schenkt nicht ein:", "tokens": ["a\u00b7ber", "la\u00dft", "noch", ",", "stil\u00b7le", "noch", ",", "schenkt", "nicht", "ein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "$,", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "ich mu\u00df euch mein Lied vom Kummer erst singen!", "tokens": ["ich", "mu\u00df", "euch", "mein", "Lied", "vom", "Kum\u00b7mer", "erst", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "++--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wenn der Kummer kommt, wenn die Saiten klagen,", "tokens": ["Wenn", "der", "Kum\u00b7mer", "kommt", ",", "wenn", "die", "Sai\u00b7ten", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "wenn die graue Stunde beginnt zu schlagen,", "tokens": ["wenn", "die", "grau\u00b7e", "Stun\u00b7de", "be\u00b7ginnt", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.6": {"text": "wo mein Mund sein Lied und sein Lachen vergi\u00dft,", "tokens": ["wo", "mein", "Mund", "sein", "Lied", "und", "sein", "La\u00b7chen", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "dann wei\u00df Keiner, wie mir ums Herz dann ist,", "tokens": ["dann", "wei\u00df", "Kei\u00b7ner", ",", "wie", "mir", "ums", "Herz", "dann", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "PPER", "APPRART", "NN", "ADV", "VAFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "dann woll'n wir die Kannen schwingen \u2013", "tokens": ["dann", "woll'n", "wir", "die", "Kan\u00b7nen", "schwin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "die Stunde der Verzweiflung naht.", "tokens": ["die", "Stun\u00b7de", "der", "Ver\u00b7zwei\u00b7flung", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr Wirt, dein Keller voll Wein ist dein,", "tokens": ["Herr", "Wirt", ",", "dein", "Kel\u00b7ler", "voll", "Wein", "ist", "dein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN", "ADJD", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "meine lange Laute, die ist mein,", "tokens": ["mei\u00b7ne", "lan\u00b7ge", "Lau\u00b7te", ",", "die", "ist", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "VAFIN", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "ich wei\u00df zwei lustige Dinge:", "tokens": ["ich", "wei\u00df", "zwei", "lus\u00b7ti\u00b7ge", "Din\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "zwei Dinge, die sich gut vertragen:", "tokens": ["zwei", "Din\u00b7ge", ",", "die", "sich", "gut", "ver\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wein trinken und die Laute schlagen!", "tokens": ["Wein", "trin\u00b7ken", "und", "die", "Lau\u00b7te", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "eine Kanne Wein zu ihrer Zeit", "tokens": ["ei\u00b7ne", "Kan\u00b7ne", "Wein", "zu", "ih\u00b7rer", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "ist mehr wert als die Ewigkeit", "tokens": ["ist", "mehr", "wert", "als", "die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "und tausend Silberlinge!", "tokens": ["und", "tau\u00b7send", "Sil\u00b7ber\u00b7lin\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die Stunde der Verzweiflung naht.", "tokens": ["Die", "Stun\u00b7de", "der", "Ver\u00b7zwei\u00b7flung", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn der Himmel auch ewig steht", "tokens": ["Und", "wenn", "der", "Him\u00b7mel", "auch", "e\u00b7wig", "steht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und die Erde noch lange nicht untergeht:", "tokens": ["und", "die", "Er\u00b7de", "noch", "lan\u00b7ge", "nicht", "un\u00b7ter\u00b7geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "wie lange, du, wirst ", "tokens": ["wie", "lan\u00b7ge", ",", "du", ",", "wirst"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "ADV", "$,", "PPER", "$,", "VAFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "du mitsamt deinem Silber-und-Goldklingklange?", "tokens": ["du", "mit\u00b7samt", "dei\u00b7nem", "Sil\u00b7ber\u00b7un\u00b7dGold\u00b7kling\u00b7klan\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "---+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "kaum hundert Jahre \u2013 das ist schon lange!", "tokens": ["kaum", "hun\u00b7dert", "Jah\u00b7re", "\u2013", "das", "ist", "schon", "lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$(", "PDS", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ja: leben und dann mal sterben, wi\u00dft,", "tokens": ["Ja", ":", "le\u00b7ben", "und", "dann", "mal", "ster\u00b7ben", ",", "wi\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$.", "VVINF", "KON", "ADV", "ADV", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "ist Alles, was uns sicher ist;", "tokens": ["ist", "Al\u00b7les", ",", "was", "uns", "si\u00b7cher", "ist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mensch, ist es nicht zum Lachen?!", "tokens": ["Mensch", ",", "ist", "es", "nicht", "zum", "La\u00b7chen", "?!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die Stunde der Verzweiflung naht.", "tokens": ["Die", "Stun\u00b7de", "der", "Ver\u00b7zwei\u00b7flung", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Seht ihr ihn? seht doch, da sitzt er und weint!", "tokens": ["Seht", "ihr", "ihn", "?", "seht", "doch", ",", "da", "sitzt", "er", "und", "weint", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$.", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Seht ihr den Affen? da hockt er und greint,", "tokens": ["Seht", "ihr", "den", "Af\u00b7fen", "?", "da", "hockt", "er", "und", "greint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "im Tamarindenbaum \u2013 h\u00f6rt ihr ihn pl\u00e4rren?", "tokens": ["im", "Ta\u00b7ma\u00b7rin\u00b7den\u00b7baum", "\u2013", "h\u00f6rt", "ihr", "ihn", "pl\u00e4r\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00fcber den Gr\u00e4bern, ganz alleine,", "tokens": ["\u00fc\u00b7ber", "den", "Gr\u00e4\u00b7bern", ",", "ganz", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADV", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "den armen Affen im Mondenscheine? \u2013", "tokens": ["den", "ar\u00b7men", "Af\u00b7fen", "im", "Mon\u00b7den\u00b7schei\u00b7ne", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und jetzt, Herr Wirt, die Kanne zum Spund!", "tokens": ["Und", "jetzt", ",", "Herr", "Wirt", ",", "die", "Kan\u00b7ne", "zum", "Spund", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "NN", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "jetzt ist es Zeit, sie bis zum Grund", "tokens": ["jetzt", "ist", "es", "Zeit", ",", "sie", "bis", "zum", "Grund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "auf Einen Zug zu leeren \u2013 \u2013", "tokens": ["auf", "Ei\u00b7nen", "Zug", "zu", "lee\u00b7ren", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "die Stunde der Verzweiflung naht.", "tokens": ["die", "Stun\u00b7de", "der", "Ver\u00b7zwei\u00b7flung", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Herr Wirt hier \u2013 Kinder, der Wirt hat Wein!", "tokens": ["Der", "Herr", "Wirt", "hier", "\u2013", "Kin\u00b7der", ",", "der", "Wirt", "hat", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$(", "NN", "$,", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-++-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "aber la\u00dft noch, stille noch, schenkt nicht ein:", "tokens": ["a\u00b7ber", "la\u00dft", "noch", ",", "stil\u00b7le", "noch", ",", "schenkt", "nicht", "