{"dta.poem.7257": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Vor vierzig Jahren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Da gab es doch ein Sehnen,", "tokens": ["Da", "gab", "es", "doch", "ein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Hoffen und ein Gl\u00fchn,", "tokens": ["Ein", "Hof\u00b7fen", "und", "ein", "Gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als noch der Mond \u201edurch Thr\u00e4nen", "tokens": ["Als", "noch", "der", "Mond", "\u201e", "durch", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "$(", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Fliederlauben\u201c schien,", "tokens": ["In", "Flie\u00b7der\u00b7lau\u00b7ben", "\u201c", "schien", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$(", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als man dem \u201emilden Sterne\u201c", "tokens": ["Als", "man", "dem", "\u201e", "mil\u00b7den", "Ster\u00b7ne", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gesellte was da lieb,", "tokens": ["Ge\u00b7sell\u00b7te", "was", "da", "lieb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und \u201eLieder in die Ferne\u201c", "tokens": ["Und", "\u201e", "Lie\u00b7der", "in", "die", "Fer\u00b7ne", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf sieben Meilen schrieb!", "tokens": ["Auf", "sie\u00b7ben", "Mei\u00b7len", "schrieb", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ob d\u00fcrftig das Erkennen,", "tokens": ["Ob", "d\u00fcrf\u00b7tig", "das", "Er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Dichtung Flamme schwach,", "tokens": ["Der", "Dich\u00b7tung", "Flam\u00b7me", "schwach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur tief und tiefer brennen", "tokens": ["Nur", "tief", "und", "tie\u00b7fer", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verdeckte Gluten nach.", "tokens": ["Ver\u00b7deck\u00b7te", "Glu\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da lachte nicht der leere,", "tokens": ["Da", "lach\u00b7te", "nicht", "der", "lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der \u00fcbersatte Spott,", "tokens": ["Der", "\u00fc\u00b7bers\u00b7at\u00b7te", "Spott", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Man baute die Alt\u00e4re", "tokens": ["Man", "bau\u00b7te", "die", "Al\u00b7t\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem unbekannten Gott.", "tokens": ["Dem", "un\u00b7be\u00b7kann\u00b7ten", "Gott", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und dr\u00fcber man den Brodem", "tokens": ["Und", "dr\u00fc\u00b7ber", "man", "den", "Bro\u00b7dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "PIS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des liebsten Weihrauchs trug,", "tokens": ["Des", "liebs\u00b7ten", "Weih\u00b7rauchs", "trug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lebend'gen Herzens Odem,", "tokens": ["Le\u00b7ben\u00b7d'\u00b7gen", "Her\u00b7zens", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das frisch und kr\u00e4ftig schlug,", "tokens": ["Das", "frisch", "und", "kr\u00e4f\u00b7tig", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das schamhaft, wie im Tode,", "tokens": ["Das", "scham\u00b7haft", ",", "wie", "im", "To\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Traumes Wundersarg", "tokens": ["In", "Trau\u00b7mes", "Wun\u00b7der\u00b7sarg"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Noch der Begeistrung Ode", "tokens": ["Noch", "der", "Be\u00b7geis\u00b7trung", "O\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NE"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Der Lieb' Ekloge barg.", "tokens": ["Der", "Lieb'", "Ek\u00b7lo\u00b7ge", "barg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir h\u00f6hnen oft und lachen", "tokens": ["Wir", "h\u00f6h\u00b7nen", "oft", "und", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der kaum vergangnen Zeit,", "tokens": ["Der", "kaum", "ver\u00b7gang\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in der W\u00fcste machen", "tokens": ["Und", "in", "der", "W\u00fcs\u00b7te", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Strau\u00dfe wir uns breit.", "tokens": ["Wie", "Strau\u00b7\u00dfe", "wir", "uns", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist Wissen denn Besitzen?", "tokens": ["Ist", "Wis\u00b7sen", "denn", "Be\u00b7sit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist denn Genie\u00dfen Gl\u00fcck?", "tokens": ["Ist", "denn", "Ge\u00b7nie\u00b7\u00dfen", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auch Eises Gletscher blitzen", "tokens": ["Auch", "Ei\u00b7ses", "Glet\u00b7scher", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Basiliskenblick.", "tokens": ["Und", "Ba\u00b7si\u00b7lis\u00b7ken\u00b7blick", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr Greise, die gesunken", "tokens": ["Ihr", "Grei\u00b7se", ",", "die", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Kinder in die Gruft,", "tokens": ["Wie", "Kin\u00b7der", "in", "die", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im letzten Hauche trunken", "tokens": ["Im", "letz\u00b7ten", "Hau\u00b7che", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Lieb' und Aetherduft,", "tokens": ["Von", "Lieb'", "und", "A\u00b7e\u00b7ther\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ihr habt am Lebensbaume", "tokens": ["Ihr", "habt", "am", "Le\u00b7bens\u00b7bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die reinste Frucht gepflegt,", "tokens": ["Die", "reins\u00b7te", "Frucht", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In karger Spannen Raume", "tokens": ["In", "kar\u00b7ger", "Span\u00b7nen", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Eden euch gehegt.", "tokens": ["Ein", "E\u00b7den", "euch", "ge\u00b7hegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun aber sind die Zeiten,", "tokens": ["Nun", "a\u00b7ber", "sind", "die", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die \u00fcberwerthen, da,", "tokens": ["Die", "\u00fc\u00b7ber\u00b7wert\u00b7hen", ",", "da", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo offen alle Weiten,", "tokens": ["Wo", "of\u00b7fen", "al\u00b7le", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und jede Ferne nah.", "tokens": ["Und", "je\u00b7de", "Fer\u00b7ne", "nah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir w\u00fchlen in den Sch\u00e4tzen,", "tokens": ["Wir", "w\u00fch\u00b7len", "in", "den", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir schmettern in den Kampf,", "tokens": ["Wir", "schmet\u00b7tern", "in", "den", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Windsbr\u00e4uten gleich versetzen", "tokens": ["Winds\u00b7br\u00e4u\u00b7ten", "gleich", "ver\u00b7set\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Uns Geistesflug und Dampf.", "tokens": ["Uns", "Geis\u00b7tes\u00b7flug", "und", "Dampf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mit unsres Spottes Gerten", "tokens": ["Mit", "uns\u00b7res", "Spot\u00b7tes", "Ger\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zerhaun wir was nicht Stahl,", "tokens": ["Zer\u00b7haun", "wir", "was", "nicht", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWS", "PTKNEG", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie Morgana's G\u00e4rten", "tokens": ["Und", "wie", "Mor\u00b7ga\u00b7na's", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "NE", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zerrinnt das Ideal;", "tokens": ["Zer\u00b7rinnt", "das", "I\u00b7deal", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Was wir daheim gelassen", "tokens": ["Was", "wir", "da\u00b7heim", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das wird uns arm und klein,", "tokens": ["Das", "wird", "uns", "arm", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was Fremdes wir erfassen", "tokens": ["Was", "Frem\u00b7des", "wir", "er\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wird in der Hand zu Stein.", "tokens": ["Wird", "in", "der", "Hand", "zu", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es wogt von End' zu Ende,", "tokens": ["Es", "wogt", "von", "End'", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es gr\u00fc\u00dft im Fluge her,", "tokens": ["Es", "gr\u00fc\u00dft", "im", "Flu\u00b7ge", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir reichen unsre H\u00e4nde,", "tokens": ["Wir", "rei\u00b7chen", "uns\u00b7re", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u2014 Sie bleiben kalt und leer. \u2014", "tokens": ["Sie", "blei\u00b7ben", "kalt", "und", "leer", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nichts liebend, achtend Wen'ge", "tokens": ["Nichts", "lie\u00b7bend", ",", "ach\u00b7tend", "Wen'\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "ADJD", "$,", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird Herz und Wange bleich,", "tokens": ["Wird", "Herz", "und", "Wan\u00b7ge", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und bettelhafte K\u00f6n'ge", "tokens": ["Und", "bet\u00b7tel\u00b7haf\u00b7te", "K\u00f6n'\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Stehn wir im Steppenreich.", "tokens": ["Stehn", "wir", "im", "Step\u00b7pen\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}