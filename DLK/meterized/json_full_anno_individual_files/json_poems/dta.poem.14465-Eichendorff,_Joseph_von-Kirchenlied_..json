{"dta.poem.14465": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Kirchenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wo Verlass'ne einsam weinen,", "tokens": ["Wo", "Ver\u00b7lass'\u00b7ne", "ein\u00b7sam", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sorgenvoll in stiller Nacht,", "tokens": ["Sor\u00b7gen\u00b7voll", "in", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den'n vor allen l\u00e4\u00dft Du scheinen", "tokens": ["Den'n", "vor", "al\u00b7len", "l\u00e4\u00dft", "Du", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PIS", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Liebe milde Pracht,", "tokens": ["Dei\u00b7ner", "Lie\u00b7be", "mil\u00b7de", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ein tr\u00f6stend Himmelslicht", "tokens": ["Da\u00df", "ein", "tr\u00f6s\u00b7tend", "Him\u00b7mels\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In die dunk'len Herzen bricht.", "tokens": ["In", "die", "dunk'\u00b7len", "Her\u00b7zen", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}