{"textgrid.poem.45944": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[dein Lied erklang, ich habe es geh\u00f6ret]", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Lied erklang, ich habe es geh\u00f6ret,", "tokens": ["Dein", "Lied", "er\u00b7klang", ",", "ich", "ha\u00b7be", "es", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie durch die Rosen es zum Monde zog;", "tokens": ["Wie", "durch", "die", "Ro\u00b7sen", "es", "zum", "Mon\u00b7de", "zog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "APPRART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Schmetterling, der bunt im Fr\u00fchling flog,", "tokens": ["Den", "Schmet\u00b7ter\u00b7ling", ",", "der", "bunt", "im", "Fr\u00fch\u00b7ling", "flog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hast du zur frommen Biene dir bekehret,", "tokens": ["Hast", "du", "zur", "from\u00b7men", "Bie\u00b7ne", "dir", "be\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zur Rose ist mein Drang,", "tokens": ["Zur", "Ro\u00b7se", "ist", "mein", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Seit mir dein Lied erklang!", "tokens": ["Seit", "mir", "dein", "Lied", "er\u00b7klang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dein Lied erklang, die Nacht hat's hingetragen,", "tokens": ["Dein", "Lied", "er\u00b7klang", ",", "die", "Nacht", "hat's", "hin\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach, meiner Ruhe s\u00fc\u00dfes Schwanenlied!", "tokens": ["Ach", ",", "mei\u00b7ner", "Ru\u00b7he", "s\u00fc\u00b7\u00dfes", "Schwa\u00b7nen\u00b7lied", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Mond, der lauschend von dem Himmel sieht,", "tokens": ["Dem", "Mond", ",", "der", "lau\u00b7schend", "von", "dem", "Him\u00b7mel", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Sternen und den Rosen mu\u00df ich's klagen,", "tokens": ["Den", "Ster\u00b7nen", "und", "den", "Ro\u00b7sen", "mu\u00df", "ich's", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {