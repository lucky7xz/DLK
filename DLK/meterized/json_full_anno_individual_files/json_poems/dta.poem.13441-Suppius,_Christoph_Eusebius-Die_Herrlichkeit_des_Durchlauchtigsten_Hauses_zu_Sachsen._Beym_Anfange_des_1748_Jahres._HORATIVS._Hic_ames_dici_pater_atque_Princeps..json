{"dta.poem.13441": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Die Herrlichkeit  \n des  \n  Durchlauchtigsten Hauses  \n zu Sachsen.  \n Beym Anfange des 1748 Jahres.  \n   \n  HORATIVS.  \n  Hic ames dici pater atque Princeps.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Muse! die sonsten G\u00f6tter oder Helden", "tokens": ["Mu\u00b7se", "!", "die", "sons\u00b7ten", "G\u00f6t\u00b7ter", "o\u00b7der", "Hel\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "KON", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Pr\u00e4chtig besinget, wovon soll ich melden?", "tokens": ["Pr\u00e4ch\u00b7tig", "be\u00b7sin\u00b7get", ",", "wo\u00b7von", "soll", "ich", "mel\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Weil ich anitzo deines Geistes St\u00e4rke", "tokens": ["Weil", "ich", "a\u00b7nit\u00b7zo", "dei\u00b7nes", "Geis\u00b7tes", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mich voller K\u00fchnheit von der Erde schwingen,", "tokens": ["Mich", "vol\u00b7ler", "K\u00fchn\u00b7heit", "von", "der", "Er\u00b7de", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und die Bewegung der Gestirne singen,", "tokens": ["Und", "die", "Be\u00b7we\u00b7gung", "der", "Ge\u00b7stir\u00b7ne", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mochte dem Geiste manchen Jrrweg zeigen,", "tokens": ["Moch\u00b7te", "dem", "Geis\u00b7te", "man\u00b7chen", "Jrr\u00b7weg", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Oder gef\u00e4llt dir von verschiednen Zonen,", "tokens": ["O\u00b7der", "ge\u00b7f\u00e4llt", "dir", "von", "ver\u00b7schied\u00b7nen", "Zo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der L\u00e4nder Schicksal, grosser Potentaten", "tokens": ["Der", "L\u00e4n\u00b7der", "Schick\u00b7sal", ",", "gros\u00b7ser", "Po\u00b7ten\u00b7ta\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Soll ich den Perser, seine k\u00fchne Jugend,", "tokens": ["Soll", "ich", "den", "Per\u00b7ser", ",", "sei\u00b7ne", "k\u00fch\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das seltne Beyspiel einer hohen Tugend", "tokens": ["Das", "selt\u00b7ne", "Bey\u00b7spiel", "ei\u00b7ner", "ho\u00b7hen", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Oder den Griechen, der zuerst den Sparter", "tokens": ["O\u00b7der", "den", "Grie\u00b7chen", ",", "der", "zu\u00b7erst", "den", "Spar\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat angew\u00f6hnet zu Geduld und Marter,", "tokens": ["Hat", "an\u00b7ge\u00b7w\u00f6h\u00b7net", "zu", "Ge\u00b7duld", "und", "Mar\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu armer Tugend, Tapferkeit und M\u00fche", "tokens": ["Zu", "ar\u00b7mer", "Tu\u00b7gend", ",", "Tap\u00b7fer\u00b7keit", "und", "M\u00fc\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Hast du vielleicht wohl jenen Stillen lieber,", "tokens": ["Hast", "du", "viel\u00b7leicht", "wohl", "je\u00b7nen", "Stil\u00b7len", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der wilde Geister an der sanften Tiber", "tokens": ["Der", "wil\u00b7de", "Geis\u00b7ter", "an", "der", "sanf\u00b7ten", "Ti\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So klug beruhiget durch die Egerja,", "tokens": ["So", "klug", "be\u00b7ru\u00b7hi\u00b7get", "durch", "die", "E\u00b7ger\u00b7ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Wahr ist, sie zeigten durch so k\u00fchne Werke", "tokens": ["Wahr", "ist", ",", "sie", "zeig\u00b7ten", "durch", "so", "k\u00fch\u00b7ne", "Wer\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "PPER", "VVFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Viel Menschenliebe und Verstandes-St\u00e4rke,", "tokens": ["Viel", "Men\u00b7schen\u00b7lie\u00b7be", "und", "Ver\u00b7stan\u00b7des\u00b7St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So seltne M\u00e4nner von Gem\u00fcthes-Gaben", "tokens": ["So", "selt\u00b7ne", "M\u00e4n\u00b7ner", "von", "Ge\u00b7m\u00fc\u00b7thes\u00b7Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und ihren Nachruhm, nach viel tausend Jahren,", "tokens": ["Und", "ih\u00b7ren", "Nach\u00b7ruhm", ",", "nach", "viel", "tau\u00b7send", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPR", "PIAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Lobger\u00fcchte f\u00fchret ihn behende", "tokens": ["Das", "Lob\u00b7ge\u00b7r\u00fcch\u00b7te", "f\u00fch\u00b7ret", "ihn", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Doch alte Zeiten sind zu weit entfernet,", "tokens": ["Doch", "al\u00b7te", "Zei\u00b7ten", "sind", "zu", "weit", "ent\u00b7fer\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihre Reiche l\u00e4ngstens ausgelernet,", "tokens": ["Und", "ih\u00b7re", "Rei\u00b7che", "l\u00e4ngs\u00b7tens", "aus\u00b7ge\u00b7ler\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wie? was erblick ich? Menschen oder G\u00f6tter?", "tokens": ["Wie", "?", "was", "er\u00b7blick", "ich", "?", "Men\u00b7schen", "o\u00b7der", "G\u00f6t\u00b7ter", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PWS", "VVFIN", "PPER", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer ist der Held dort? Th\u00fcringens Erretter?", "tokens": ["Wer", "ist", "der", "Held", "dort", "?", "Th\u00fc\u00b7rin\u00b7gens", "Er\u00b7ret\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "$.", "NN", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Adler fliehet, der schon in der Raute", "tokens": ["Der", "Ad\u00b7ler", "flie\u00b7het", ",", "der", "schon", "in", "der", "Rau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wie unerschrocken ficht er mit dem Schwerde,", "tokens": ["Wie", "un\u00b7er\u00b7schro\u00b7cken", "ficht", "er", "mit", "dem", "Schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich Hirten gegen W\u00f6lfe f\u00fcr die Heerde,", "tokens": ["Gleich", "Hir\u00b7ten", "ge\u00b7gen", "W\u00f6l\u00b7fe", "f\u00fcr", "die", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er, dieser Halbgott, den allein von weiten", "tokens": ["Er", ",", "die\u00b7ser", "Halb\u00b7gott", ",", "den", "al\u00b7lein", "von", "wei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PDAT", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "V\u00f6lker der Erden! ihr m\u00fc\u00dft von ihm wissen,", "tokens": ["V\u00f6l\u00b7ker", "der", "Er\u00b7den", "!", "ihr", "m\u00fc\u00dft", "von", "ihm", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Denn Mutterliebe hat ihn einst gebissen,", "tokens": ["Denn", "Mut\u00b7ter\u00b7lie\u00b7be", "hat", "ihn", "einst", "ge\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ihn gezeichnet, der voll Vatertriebe", "tokens": ["Und", "ihn", "ge\u00b7zeich\u00b7net", ",", "der", "voll", "Va\u00b7ter\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVPP", "$,", "PRELS", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Von jenem Schutzgott, der so k\u00fchn gehandelt,", "tokens": ["Von", "je\u00b7nem", "Schutz\u00b7gott", ",", "der", "so", "k\u00fchn", "ge\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als zu Attigny er nach Blut und Morden", "tokens": ["Als", "zu", "At\u00b7tig\u00b7ny", "er", "nach", "Blut", "und", "Mor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "PPER", "APPR", "NN", "KON", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Von dem entsprie\u00dft er. Helden zeugen Helden!", "tokens": ["Von", "dem", "ent\u00b7sprie\u00dft", "er", ".", "Hel\u00b7den", "zeu\u00b7gen", "Hel\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PPER", "$.", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was kann man mehr von ihm f\u00fcr Dinge melden!", "tokens": ["Was", "kann", "man", "mehr", "von", "ihm", "f\u00fcr", "Din\u00b7ge", "mel\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "ADV", "APPR", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Seht nur von ihm ein ernsthaft Rei\u00df erwachsen,", "tokens": ["Seht", "nur", "von", "ihm", "ein", "ernst\u00b7haft", "Rei\u00df", "er\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In solchem bl\u00fcht der ganze Stamm von Sachsen,", "tokens": ["In", "sol\u00b7chem", "bl\u00fcht", "der", "gan\u00b7ze", "Stamm", "von", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VVFIN", "ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Britten Sieger mu\u00df ihm wider Franken", "tokens": ["Der", "Brit\u00b7ten", "Sie\u00b7ger", "mu\u00df", "ihm", "wi\u00b7der", "Fran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Ein Strenger folgt ihm, der die Bosheit strafet,", "tokens": ["Ein", "Stren\u00b7ger", "folgt", "ihm", ",", "der", "die", "Bos\u00b7heit", "stra\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und durch die Henne g\u00fcldne Eyer schaffet,", "tokens": ["Und", "durch", "die", "Hen\u00b7ne", "g\u00fcld\u00b7ne", "Ey\u00b7er", "schaf\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die werden seinen Erben nach zwo Zeiten", "tokens": ["Die", "wer\u00b7den", "sei\u00b7nen", "Er\u00b7ben", "nach", "zwo", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "Woher! du Held von ungemeinen Thaten,", "tokens": ["Wo\u00b7her", "!", "du", "Held", "von", "un\u00b7ge\u00b7mei\u00b7nen", "Tha\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sehe schon den Wachsthum deiner Staaten,", "tokens": ["Ich", "se\u00b7he", "schon", "den", "Wach\u00b7sthum", "dei\u00b7ner", "Staa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sobald dein Ruhm mit Hoheit, Sieg u. Schwerdte", "tokens": ["So\u00b7bald", "dein", "Ruhm", "mit", "Ho\u00b7heit", ",", "Sieg", "u.", "Schwerd\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "abbreviation", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "$,", "NN", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.18": {"line.1": {"text": "Glorw\u00fcrdigster! Du deines Hauses Krone!", "tokens": ["Glor\u00b7w\u00fcr\u00b7digs\u00b7ter", "!", "Du", "dei\u00b7nes", "Hau\u00b7ses", "Kro\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Held! Streitbar! Churf\u00fcrst! Musenfreund! o Pleisse!", "tokens": ["Held", "!", "Streit\u00b7bar", "!",