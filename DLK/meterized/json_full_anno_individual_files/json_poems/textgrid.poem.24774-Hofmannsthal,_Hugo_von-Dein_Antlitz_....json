{"textgrid.poem.24774": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Dein Antlitz ...", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Antlitz war mit Tr\u00e4umen ganz beladen.", "tokens": ["Dein", "Ant\u00b7litz", "war", "mit", "Tr\u00e4u\u00b7men", "ganz", "be\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich schwieg und sah dich an mit stummem Beben.", "tokens": ["Ich", "schwieg", "und", "sah", "dich", "an", "mit", "stum\u00b7mem", "Be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie stieg das auf! Da\u00df ich mich einmal schon", "tokens": ["Wie", "stieg", "das", "auf", "!", "Da\u00df", "ich", "mich", "ein\u00b7mal", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PDS", "PTKVZ", "$.", "KOUS", "PPER", "PRF", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In fr\u00fchern N\u00e4chten v\u00f6llig hingegeben", "tokens": ["In", "fr\u00fc\u00b7hern", "N\u00e4ch\u00b7ten", "v\u00f6l\u00b7lig", "hin\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem Mond und dem zuviel geliebten Tal,", "tokens": ["Dem", "Mond", "und", "dem", "zu\u00b7viel", "ge\u00b7lieb\u00b7ten", "Tal", ","]