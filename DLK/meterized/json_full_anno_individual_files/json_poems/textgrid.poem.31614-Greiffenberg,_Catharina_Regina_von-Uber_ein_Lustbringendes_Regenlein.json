{"textgrid.poem.31614": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "Uber ein Lustbringendes Regenlein", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Regen schadet nichts / als da\u00df er uns die Lust", "tokens": ["Der", "Re\u00b7gen", "scha\u00b7det", "nichts", "/", "als", "da\u00df", "er", "uns", "die", "Lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "$(", "KOKOM", "KOUS", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nur tausendmal versch\u00f6nt / und angenemer machet.", "tokens": ["nur", "tau\u00b7send\u00b7mal", "ver\u00b7sch\u00f6nt", "/", "und", "an\u00b7ge\u00b7ne\u00b7mer", "ma\u00b7chet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$(", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sonn / nach hartem Strau\u00df / mit klaren Strahlen lachet.", "tokens": ["Die", "Sonn", "/", "nach", "har\u00b7tem", "Strau\u00df", "/", "mit", "kla\u00b7ren", "Strah\u00b7len", "la\u00b7chet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "ADJA", "NE", "$(", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der Himmel seuget nur die Erd mit seiner Brust.", "tokens": ["der", "Him\u00b7mel", "seu\u00b7get", "nur", "die", "Erd", "mit", "sei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVF