{"textgrid.poem.42618": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Nothemd", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich mu\u00df zu Feld, mein T\u00f6chterlein,", "tokens": ["\u00bb", "ich", "mu\u00df", "zu", "Feld", ",", "mein", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "APPR", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und B\u00f6ses dr\u00e4ut der Sterne Schein,", "tokens": ["Und", "B\u00f6\u00b7ses", "dr\u00e4ut", "der", "Ster\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum schaff du mir ein Notgewand,", "tokens": ["Drum", "schaff", "du", "mir", "ein", "Not\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du Jungfrau, mit der zarten Hand!\u00ab", "tokens": ["Du", "Jung\u00b7frau", ",", "mit", "der", "zar\u00b7ten", "Hand", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-----+-+", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "\u00bbmein Vater! willst du Schlachtgewand", "tokens": ["\u00bb", "mein", "Va\u00b7ter", "!", "willst", "du", "Schlacht\u00b7ge\u00b7wand"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$.", "VMFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von eines M\u00e4gdleins schwacher Hand?", "tokens": ["Von", "ei\u00b7nes", "M\u00e4gd\u00b7leins", "schwa\u00b7cher", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch schlug ich nie den harten Stahl,", "tokens": ["Noch", "schlug", "ich", "nie", "den", "har\u00b7ten", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich spinn und web im Frauensaal.\u00ab", "tokens": ["Ich", "spinn", "und", "web", "im", "Frau\u00b7en\u00b7saal", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbja, spinne, Kind, in heil'ger Nacht,", "tokens": ["\u00bb", "ja", ",", "spin\u00b7ne", ",", "Kind", ",", "in", "heil'\u00b7ger", "Nacht", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "VVFIN", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Faden weih der h\u00f6llischen Macht!", "tokens": ["Den", "Fa\u00b7den", "weih", "der", "h\u00f6l\u00b7li\u00b7schen", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Draus web ein Hemde, lang und weit!", "tokens": ["Draus", "web", "ein", "Hem\u00b7de", ",", "lang", "und", "weit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das wahret mich im blut'gen Streit.\u00ab", "tokens": ["Das", "wah\u00b7ret", "mich", "im", "blut'\u00b7gen", "Streit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In heil'ger Nacht, im Vollmondschein,", "tokens": ["In", "heil'\u00b7ger", "Nacht", ",", "im", "Voll\u00b7mond\u00b7schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da spinnt die Maid im Saal allein.", "tokens": ["Da", "spinnt", "die", "Maid", "im", "Saal", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbin der H\u00f6lle Namen!\u00ab spricht sie leis,", "tokens": ["\u00bb", "in", "der", "H\u00f6l\u00b7le", "Na\u00b7men", "!", "\u00ab", "spricht", "sie", "leis", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "NN", "$.", "$(", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die Spindel rollt in feurigem Kreis.", "tokens": ["Die", "Spin\u00b7del", "rollt", "in", "feu\u00b7ri\u00b7gem", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Dann tritt sie an den Webestuhl", "tokens": ["Dann", "tritt", "sie", "an", "den", "We\u00b7be\u00b7stuhl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wirft mit zager Hand die Spul;", "tokens": ["Und", "wirft", "mit", "za\u00b7ger", "Hand", "die", "Spul", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es rauscht und saust in wilder Hast,", "tokens": ["Es", "rauscht", "und", "saust", "in", "wil\u00b7der", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00f6ben Geisterh\u00e4nde zu Gast.", "tokens": ["Als", "w\u00f6\u00b7ben", "Geis\u00b7ter\u00b7h\u00e4n\u00b7de", "zu", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Als nun das Heer ausritt zur Schlacht,", "tokens": ["Als", "nun", "das", "Heer", "aus\u00b7ritt", "zur", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da tr\u00e4gt der Herzog sondre Tracht:", "tokens": ["Da", "tr\u00e4gt", "der", "Her\u00b7zog", "sond\u00b7re", "Tracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Bildern, Zeichen, schaurig, fremd,", "tokens": ["Mit", "Bil\u00b7dern", ",", "Zei\u00b7chen", ",", "schau\u00b7rig", ",", "fremd", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein wei\u00dfes, weites, wallendes Hemd.", "tokens": ["Ein", "wei\u00b7\u00dfes", ",", "wei\u00b7tes", ",", "wal\u00b7len\u00b7des", "Hemd", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Ihm weicht der Feind wie einem Geist:", "tokens": ["Ihm", "weicht", "der", "Feind", "wie", "ei\u00b7nem", "Geist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer b\u00f6t es ihm, wer stellt' ihn dreist,", "tokens": ["Wer", "b\u00f6t", "es", "ihm", ",", "wer", "stellt'", "ihn", "dreist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "PWS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An dem das h\u00e4rteste Schwert zerschellt,", "tokens": ["An", "dem", "das", "h\u00e4r\u00b7tes\u00b7te", "Schwert", "zer\u00b7schellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von dem der Pfeil auf den Sch\u00fctzen prellt!", "tokens": ["Von", "dem", "der", "Pfeil", "auf", "den", "Sch\u00fct\u00b7zen", "prellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Ein J\u00fcngling sprengt ihm vors Gesicht:", "tokens": ["Ein", "J\u00fcng\u00b7ling", "sprengt", "ihm", "vors", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbhalt, W\u00fcrger, halt! mich schreckst du nicht.", "tokens": ["\u00bb", "halt", ",", "W\u00fcr\u00b7ger", ",", "halt", "!", "mich", "schreckst", "du", "nicht", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht rettet dich die H\u00f6llenkunst,", "tokens": ["Nicht", "ret\u00b7tet", "dich", "die", "H\u00f6l\u00b7len\u00b7kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Werk ist tot, dein Zauber Dunst.\u00ab", "tokens": ["Dein", "Werk", "ist", "tot", ",", "dein", "Zau\u00b7ber", "Dunst", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Sie treffen sich und treffen gut,", "tokens": ["Sie", "tref\u00b7fen", "sich", "und", "tref\u00b7fen", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Herzogs Nothemd trieft von Blut;", "tokens": ["Des", "Her\u00b7zogs", "No\u00b7themd", "trieft", "von", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie haun und haun sich in den Sand,", "tokens": ["Sie", "haun", "und", "haun", "sich", "in", "den", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeder flucht des andern Hand.", "tokens": ["Und", "je\u00b7der", "flucht", "des", "an\u00b7dern", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Tochter steigt hinab ins Feld:", "tokens": ["Die", "Toch\u00b7ter", "steigt", "hin\u00b7ab", "ins", "Feld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwo liegt der herzogliche Held?\u00ab", "tokens": ["\u00bb", "wo", "liegt", "der", "her\u00b7zog\u00b7li\u00b7che", "Held", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie find't die todeswunden zwei,", "tokens": ["Sie", "find't", "die", "to\u00b7des\u00b7wun\u00b7den", "zwei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da hebt sie wildes Klaggeschrei.", "tokens": ["Da", "hebt", "sie", "wil\u00b7des", "Klag\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbbist du's, mein Kind? Unsel'ge Maid!", "tokens": ["\u00bb", "bist", "du's", ",", "mein", "Kind", "?", "Un\u00b7sel'\u00b7ge", "Maid", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PIS", "$,", "PPOSAT", "NN", "$.", "ADJA", "NN", "$."], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wie spannest du das falsche Kleid?", "tokens": ["Wie", "span\u00b7nest", "du", "das", "fal\u00b7sche", "Kleid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hast du die H\u00f6lle nicht genannt?", "tokens": ["Hast", "du", "die", "H\u00f6l\u00b7le", "nicht", "ge\u00b7nannt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War nicht jungfr\u00e4ulich deine Hand?\u00ab", "tokens": ["War", "nicht", "jung\u00b7fr\u00e4u\u00b7lich", "dei\u00b7ne", "Hand", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00bbdie H\u00f6lle hab ich wohl genannt,", "tokens": ["\u00bb", "die", "H\u00f6l\u00b7le", "hab", "ich", "wohl", "ge\u00b7nannt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch nicht jungfr\u00e4ulich war die Hand,", "tokens": ["Doch", "nicht", "jung\u00b7fr\u00e4u\u00b7lich", "war", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der dich erschlug, ist mir nicht fremd,", "tokens": ["Der", "dich", "er\u00b7schlug", ",", "ist", "mir", "nicht", "fremd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So spannt ich, weh! dein Totenhemd.\u00ab", "tokens": ["So", "spannt", "ich", ",", "weh", "!", "dein", "To\u00b7ten\u00b7hemd", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PTKVZ", "$.", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "\u00bbich mu\u00df zu Feld, mein T\u00f6chterlein,", "tokens": ["\u00bb", "ich", "mu\u00df", "zu", "Feld", ",", "mein", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "APPR", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und B\u00f6ses dr\u00e4ut der Sterne Schein,", "tokens": ["Und", "B\u00f6\u00b7ses", "dr\u00e4ut", "der", "Ster\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "