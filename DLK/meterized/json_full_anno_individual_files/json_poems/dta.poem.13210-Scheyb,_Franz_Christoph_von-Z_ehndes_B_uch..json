{"dta.poem.13210": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6niginn Gesicht gieng ihrem ", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7ginn", "Ge\u00b7sicht", "gieng", "ih\u00b7rem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu welchem sie, da\u00df er den Rath umarme, sprach.", "tokens": ["Zu", "wel\u00b7chem", "sie", ",", "da\u00df", "er", "den", "Rath", "um\u00b7ar\u00b7me", ",", "sprach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "335Der Grei\u00df verweilte nicht, nach selbigem zu langen,", "tokens": ["Grei\u00df", "ver\u00b7weil\u00b7te", "nicht", ",", "nach", "sel\u00b7bi\u00b7gem", "zu", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "$,", "APPR", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Als wollt er ihn, bevor er ihn erreicht\u2019, umfangen.", "tokens": ["Als", "wollt", "er", "ihn", ",", "be\u00b7vor", "er", "ihn", "er\u00b7reicht'", ",", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}