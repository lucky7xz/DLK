{"dta.poem.21384": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich jammerte lange \u2013 da kr\u00e4hte der Hahn,", "tokens": ["Ich", "jam\u00b7mer\u00b7te", "lan\u00b7ge", "\u2013", "da", "kr\u00e4h\u00b7te", "der", "Hahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und der Fiebertraum erbla\u00dfte.", "tokens": ["Und", "der", "Fie\u00b7ber\u00b7traum", "er\u00b7bla\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Ich lag zu Minden im schwitzenden Bett,", "tokens": ["Ich", "lag", "zu", "Min\u00b7den", "im", "schwit\u00b7zen\u00b7den", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Adler ward wieder zum Quaste.", "tokens": ["Der", "Ad\u00b7ler", "ward", "wie\u00b7der", "zum", "Quas\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ich reiste fort mit Extrapost,", "tokens": ["Ich", "reis\u00b7te", "fort", "mit", "Ext\u00b7ra\u00b7post", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sch\u00f6pfte freyen Odem", "tokens": ["Und", "sch\u00f6pf\u00b7te", "frey\u00b7en", "O\u00b7dem"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erst drau\u00dfen in der freien Natur,", "tokens": ["Erst", "drau\u00b7\u00dfen", "in", "der", "frei\u00b7en", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Auf B\u00fckkeburgschem Boden.", "tokens": ["Auf", "B\u00fck\u00b7ke\u00b7burg\u00b7schem", "Bo\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}