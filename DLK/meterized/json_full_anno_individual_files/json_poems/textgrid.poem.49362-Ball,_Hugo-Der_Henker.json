{"textgrid.poem.49362": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "Der Henker", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kugle Dich auf Deiner roten Decke.", "tokens": ["Ich", "kug\u00b7le", "Dich", "auf", "Dei\u00b7ner", "ro\u00b7ten", "De\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin am Werk: blank wie ein Metzgermeister.", "tokens": ["Ich", "bin", "am", "Werk", ":", "blank", "wie", "ein", "Metz\u00b7ger\u00b7meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$.", "PTKVZ", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tische und B\u00e4nke stehen wie blitzende Messer", "tokens": ["Ti\u00b7sche", "und", "B\u00e4n\u00b7ke", "ste\u00b7hen", "wie", "blit\u00b7zen\u00b7de", "Mes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "der Syphiliszwerg stochert in T\u00f6pfen voll Gallert und Kleister.", "tokens": ["der", "Sy\u00b7phi\u00b7lis\u00b7zwerg", "sto\u00b7chert", "in", "T\u00f6p\u00b7fen", "voll", "Gal\u00b7lert", "und", "Kleis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+--++-+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Dein Leib ist gekr\u00fcmmt und blendend und gl\u00e4nzt wie der gelbe Mond", "tokens": ["Dein", "Leib", "ist", "ge\u00b7kr\u00fcmmt", "und", "blen\u00b7dend", "und", "gl\u00e4nzt", "wie", "der", "gel\u00b7be", "Mond"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "KON", "ADJD", "KON", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "deine Augen sind kleine l\u00fcsterne Monde", "tokens": ["dei\u00b7ne", "Au\u00b7gen", "sind", "klei\u00b7ne", "l\u00fcs\u00b7ter\u00b7ne", "Mon\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "dein Mund ist geborsten in Wollust und in der J\u00fcdinnen Not", "tokens": ["dein", "Mund", "ist", "ge\u00b7bors\u00b7ten", "in", "Wol\u00b7lust", "und", "in", "der", "J\u00fc\u00b7din\u00b7nen", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "APPR", "NN", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-+--+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "deine Hand eine Schnecke, die in den blutroten G\u00e4rten voll Weintrauben und Rosen wohnte.", "tokens": ["dei\u00b7ne", "Hand", "ei\u00b7ne", "Schne\u00b7cke", ",", "die", "in", "den", "blut\u00b7ro\u00b7ten", "G\u00e4r\u00b7ten", "voll", "Wein\u00b7trau\u00b7ben", "und", "Ro\u00b7sen", "wohn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+--+-+--+--+--++--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Hilf, heilige Maria! Dir sprang die Frucht aus dem Leibe", "tokens": ["Hilf", ",", "hei\u00b7li\u00b7ge", "Ma\u00b7ria", "!", "Dir", "sprang", "die", "Frucht", "aus", "dem", "Lei\u00b7be"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NE", "$.", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "sei gebenedeit! Mir rinnt geiler Brand an den Beinen herunter.", "tokens": ["sei", "ge\u00b7be\u00b7ne\u00b7deit", "!", "Mir", "rinnt", "gei\u00b7ler", "Brand", "an", "den", "Bei\u00b7nen", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "PPER", "VVFIN", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+---+-++-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Mein Haar ein Sturm, mein Gehirn ein Zunder", "tokens": ["Mein", "Haar", "ein", "Sturm", ",", "mein", "Ge\u00b7hirn", "ein", "Zun\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "meine Finger zehn gierige Zimmermannsn\u00e4gel", "tokens": ["mei\u00b7ne", "Fin\u00b7ger", "zehn", "gie\u00b7ri\u00b7ge", "Zim\u00b7mer\u00b7manns\u00b7n\u00e4\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "CARD", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "die schlage ich in der Christenheit G\u00f6tzenplunder.", "tokens": ["die", "schla\u00b7ge", "ich", "in", "der", "Chris\u00b7ten\u00b7heit", "G\u00f6t\u00b7zen\u00b7plun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Als dein Wehgeschrei dir die Z\u00e4hne aus den Kiefern sprengte", "tokens": ["Als", "dein", "Weh\u00b7ge\u00b7schrei", "dir", "die", "Z\u00e4h\u00b7ne", "aus", "den", "Kie\u00b7fern", "spreng\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "da brach auch ein Goldprasseln durch die Himmelssparren nieder.", "tokens": ["da", "brach", "auch", "ein", "Gold\u00b7pras\u00b7seln", "durch", "die", "Him\u00b7mels\u00b7spar\u00b7ren", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--++-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Eine gigantische Hostie gerann und blieb zwischen Rosabergen stehen", "tokens": ["Ei\u00b7ne", "gi\u00b7gan\u00b7ti\u00b7sche", "Hos\u00b7tie", "ge\u00b7rann", "und", "blieb", "zwi\u00b7schen", "Ro\u00b7sa\u00b7ber\u00b7gen", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "KON", "VVFIN", "APPR", "NN", "VVINF"], "meter": "+--+--++-+-+--++--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "ein Hallelujah gurgelte durch Apostel- und Hirtenglieder.", "tokens": ["ein", "Hal\u00b7le\u00b7lu\u00b7jah", "gur\u00b7gel\u00b7te", "durch", "A\u00b7po\u00b7stel", "und", "Hir\u00b7ten\u00b7glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Da tanzten nackichte M\u00e4nner und Huren in verr\u00fcckter Ekstase", "tokens": ["Da", "tanz\u00b7ten", "na\u00b7ckich\u00b7te", "M\u00e4n\u00b7ner", "und", "Hu\u00b7ren", "in", "ver\u00b7r\u00fcck\u00b7ter", "Eks\u00b7ta\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+-+-+-+--", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Heiden, T\u00fcrken, Kaffern und Muhammedaner zumal", "tokens": ["Hei\u00b7den", ",", "T\u00fcr\u00b7ken", ",", "Kaf\u00b7fern", "und", "Mu\u00b7ham\u00b7me\u00b7da\u00b7ner", "zu\u00b7mal"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "KOUS"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Da stoben die Engel den Erdkreis hinunter", "tokens": ["Da", "sto\u00b7ben", "die", "En\u00b7gel", "den", "Erd\u00b7kreis", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPO"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und brachten auf feurigem Teller die Finsternis und die Qual.", "tokens": ["Und", "brach\u00b7ten", "auf", "feu\u00b7ri\u00b7gem", "Tel\u00b7ler", "die", "Fins\u00b7ter\u00b7nis", "und", "die", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+--+-++-+", "measure": "amphibrach.tetra.plus"}, "line.5": {"text": "Da war keine Mutterknospe, kein Auge mehr blutunterlaufen und ohne Hoffen", "tokens": ["Da", "war", "kei\u00b7ne", "Mut\u00b7ter\u00b7knos\u00b7pe", ",", "kein", "Au\u00b7ge",