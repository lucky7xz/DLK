rd", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Obgleich sie nur im Nachtgewand,", "tokens": ["Ob\u00b7gleich", "sie", "nur", "im", "Nacht\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Besen, der am Ofen stand;", "tokens": ["Den", "Be\u00b7sen", ",", "der", "am", "O\u00b7fen", "stand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Indes der Jochen leise flucht", "tokens": ["In\u00b7des", "der", "Jo\u00b7chen", "lei\u00b7se", "flucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und erst mal Licht zu machen sucht.", "tokens": ["Und", "erst", "mal", "Licht", "zu", "ma\u00b7chen", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Sie ging voran, er hinterdrein.", "tokens": ["Sie", "ging", "vo\u00b7ran", ",", "er", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es pfeift der Wind, die H\u00fchner schrein.", "tokens": ["Es", "pfeift", "der", "Wind", ",", "die", "H\u00fch\u00b7ner", "schrein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Nur zu, mahnt Jochen, sei nur dreist", "tokens": ["Nur", "zu", ",", "mahnt", "Jo\u00b7chen", ",", "sei", "nur", "dreist"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "VVFIN", "NE", "$,", "VAFIN", "ADV", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sag Bescheid, wenn er dich bei\u00dft.", "tokens": ["Und", "sag", "Be\u00b7scheid", ",", "wenn", "er", "dich", "bei\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Umsonst sucht sich der Dieb zu dr\u00fccken", "tokens": ["Um\u00b7sonst", "sucht", "sich", "der", "Dieb", "zu", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Madam Dralles Geierblicken.", "tokens": ["Vor", "Ma\u00b7dam", "Dral\u00b7les", "Gei\u00b7er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schl\u00e4gt ihm unaussprechlich schnelle", "tokens": ["Sie", "schl\u00e4gt", "ihm", "un\u00b7aus\u00b7sprech\u00b7lich", "schnel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zwei-dreimal an derselben Stelle", "tokens": ["Zwei\u00b7drei\u00b7mal", "an", "der\u00b7sel\u00b7ben", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit ihres Besens hartem Stiel", "tokens": ["Mit", "ih\u00b7res", "Be\u00b7sens", "har\u00b7tem", "Stiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aufs Nasenbein. Das war zuviel. \u2013", "tokens": ["Aufs", "Na\u00b7sen\u00b7bein", ".", "Das", "war", "zu\u00b7viel", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$.", "PDS", "VAFIN", "PIS", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Ein jeder kriegt, ein jeder nimmt", "tokens": ["Ein", "je\u00b7der", "kriegt", ",", "ein", "je\u00b7der", "nimmt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "$,", "ART", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser Welt, was ihm bestimmt.", "tokens": ["In", "die\u00b7ser", "Welt", ",", "was", "ihm", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Der Fuchs, nachdem der Balg herab,", "tokens": ["Der", "Fuchs", ",", "nach\u00b7dem", "der", "Balg", "her\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "KOUS", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bekommt ein Armes\u00fcndergrab.", "tokens": ["Be\u00b7kommt", "ein", "Ar\u00b7me\u00b7s\u00fcn\u00b7der\u00b7grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frau Dralle, weil sie leichtgesinnt", "tokens": ["Frau", "Dral\u00b7le", ",", "weil", "sie", "leicht\u00b7ge\u00b7sinnt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich ausgesetzt dem Winterwind", "tokens": ["Sich", "aus\u00b7ge\u00b7setzt", "dem", "Win\u00b7ter\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Trotz der Selbsterhaltungspflicht,", "tokens": ["Zum", "Trotz", "der", "Selbs\u00b7ter\u00b7hal\u00b7tungs\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kriegt zu der Grippe noch die Gicht.", "tokens": ["Kriegt", "zu", "der", "Grip\u00b7pe", "noch", "die", "Gicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.21": {"line.1": {"text": "Doch Jochen kriegte hocherfreut", "tokens": ["Doch", "Jo\u00b7chen", "krieg\u00b7te", "ho\u00b7cher\u00b7freut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Infolge der Gelegenheit", "tokens": ["In\u00b7fol\u00b7ge", "der", "Ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Pelzwerk eine warme Kappe", "tokens": ["Von", "Pelz\u00b7werk", "ei\u00b7ne", "war\u00b7me", "Kap\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART",