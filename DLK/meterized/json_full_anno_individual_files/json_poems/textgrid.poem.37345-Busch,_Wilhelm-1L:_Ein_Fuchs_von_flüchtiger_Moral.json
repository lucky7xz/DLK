{"textgrid.poem.37345": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Fuchs von fl\u00fcchtiger Moral", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fuchs von fl\u00fcchtiger Moral", "tokens": ["Ein", "Fuchs", "von", "fl\u00fcch\u00b7ti\u00b7ger", "Mo\u00b7ral"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unbedenklich, wenn er stahl,", "tokens": ["Und", "un\u00b7be\u00b7denk\u00b7lich", ",", "wenn", "er", "stahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schlich sich bei Nacht zum H\u00fchnerstalle", "tokens": ["Schlich", "sich", "bei", "Nacht", "zum", "H\u00fch\u00b7ner\u00b7stal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PRF", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von einem namens Jochen Dralle,", "tokens": ["Von", "ei\u00b7nem", "na\u00b7mens", "Jo\u00b7chen", "Dral\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der, weil die M\u00fche ihn verdro\u00df,", "tokens": ["Der", ",", "weil", "die", "M\u00fc\u00b7he", "ihn", "ver\u00b7dro\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die T\u00fcr mal wieder nicht verschlo\u00df.", "tokens": ["Die", "T\u00fcr", "mal", "wie\u00b7der", "nicht", "ver\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er hat sich, wie er immer pflegt,", "tokens": ["Er", "hat", "sich", ",", "wie", "er", "im\u00b7mer", "pflegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wie er war zu Bett gelegt.", "tokens": ["So", "wie", "er", "war", "zu", "Bett", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schlief und schnarchte auch bereits.", "tokens": ["Er", "schlief", "und", "schnarch\u00b7te", "auch", "be\u00b7reits", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Frau Dralle, welche ihrerseits", "tokens": ["Frau", "Dral\u00b7le", ",", "wel\u00b7che", "ih\u00b7rer\u00b7seits"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch wachte, denn sie hat die Grippe,", "tokens": ["Noch", "wach\u00b7te", ",", "denn", "sie", "hat", "die", "Grip\u00b7pe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stie\u00df Jochen an die kurze Rippe.", "tokens": ["Stie\u00df", "Jo\u00b7chen", "an", "die", "kur\u00b7ze", "Rip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du, rief sie fl\u00fcsternd, h\u00f6r doch blo\u00df,", "tokens": ["Du", ",", "rief", "sie", "fl\u00fcs\u00b7ternd", ",", "h\u00f6r", "doch", "blo\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "PPER", "VVPP", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im H\u00fchnerstall da ist was los;", "tokens": ["Im", "H\u00fch\u00b7ner\u00b7stall", "da", "ist", "was", "los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist der Fuchs, der alte Racker.", "tokens": ["Das", "ist", "der", "Fuchs", ",", "der", "al\u00b7te", "Ra\u00b7cker", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und schon ergriff sie k\u00fchn und wacker,", "tokens": ["Und", "schon", "er\u00b7griff", "sie", "k\u00fchn", "und", "wa\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Obgleich sie nur im Nachtgewand,", "tokens": ["Ob\u00b7gleich", "sie", "nur", "im", "Nacht\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Besen, der am Ofen stand;", "tokens": ["Den", "Be\u00b7sen", ",", "der", "am", "O\u00b7fen", "stand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Indes der Jochen leise flucht", "tokens": ["In\u00b7des", "der", "Jo\u00b7chen", "lei\u00b7se", "flucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und erst mal Licht zu machen sucht.", "tokens": ["Und", "erst", "mal", "Licht", "zu", "ma\u00b7chen", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie ging voran, er hinterdrein.", "tokens": ["Sie", "ging", "vo\u00b7ran", ",", "er", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es pfeift der Wind, die H\u00fchner schrein.", "tokens": ["Es", "pfeift", "der", "Wind", ",", "die", "H\u00fch\u00b7ner", "schrein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nur zu, mahnt Jochen, sei nur dreist", "tokens": ["Nur", "zu", ",", "mahnt", "Jo\u00b7chen", ",", "sei", "nur", "dreist"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "VVFIN", "NE", "$,", "VAFIN", "ADV", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sag Bescheid, wenn er dich bei\u00dft.", "tokens": ["Und", "sag", "Be\u00b7scheid", ",", "wenn", "er", "dich", "bei\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Umsonst sucht sich der Dieb zu dr\u00fccken", "tokens": ["Um\u00b7sonst", "sucht", "sich", "der", "Dieb", "zu", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Madam Dralles Geierblicken.", "tokens": ["Vor", "Ma\u00b7dam", "Dral\u00b7les", "Gei\u00b7er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schl\u00e4gt ihm unaussprechlich schnelle", "tokens": ["Sie", "schl\u00e4gt", "ihm", "un\u00b7aus\u00b7sprech\u00b7lich", "schnel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zwei-dreimal an derselben Stelle", "tokens": ["Zwei\u00b7drei\u00b7mal", "an", "der\u00b7sel\u00b7ben", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit ihres Besens hartem Stiel", "tokens": ["Mit", "ih\u00b7res", "Be\u00b7sens", "har\u00b7tem", "Stiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aufs Nasenbein. Das war zuviel. \u2013", "tokens": ["Aufs", "Na\u00b7sen\u00b7bein", ".", "Das", "war", "zu\u00b7viel", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$.", "PDS", "VAFIN", "PIS", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein jeder kriegt, ein jeder nimmt", "tokens": ["Ein", "je\u00b7der", "kriegt", ",", "ein", "je\u00b7der", "nimmt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "$,", "ART", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser Welt, was ihm bestimmt.", "tokens": ["In", "die\u00b7ser", "Welt", ",", "was", "ihm", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Fuchs, nachdem der Balg herab,", "tokens": ["Der", "Fuchs", ",", "nach\u00b7dem", "der", "Balg", "her\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "KOUS", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bekommt ein Armes\u00fcndergrab.", "tokens": ["Be\u00b7kommt", "ein", "Ar\u00b7me\u00b7s\u00fcn\u00b7der\u00b7grab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frau Dralle, weil sie leichtgesinnt", "tokens": ["Frau", "Dral\u00b7le", ",", "weil", "sie", "leicht\u00b7ge\u00b7sinnt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich ausgesetzt dem Winterwind", "tokens": ["Sich", "aus\u00b7ge\u00b7setzt", "dem", "Win\u00b7ter\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Trotz der Selbsterhaltungspflicht,", "tokens": ["Zum", "Trotz", "der", "Selbs\u00b7ter\u00b7hal\u00b7tungs\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kriegt zu der Grippe noch die Gicht.", "tokens": ["Kriegt", "zu", "der", "Grip\u00b7pe", "noch", "die", "Gicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Doch Jochen kriegte hocherfreut", "tokens": ["Doch", "Jo\u00b7chen", "krieg\u00b7te", "ho\u00b7cher\u00b7freut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Infolge der Gelegenheit", "tokens": ["In\u00b7fol\u00b7ge", "der", "Ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Pelzwerk eine warme Kappe", "tokens": ["Von", "Pelz\u00b7werk", "ei\u00b7ne", "war\u00b7me", "Kap\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Vorder- und mit Hinterklappe.", "tokens": ["Mit", "Vor\u00b7der", "und", "mit", "Hin\u00b7ter\u00b7klap\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Stets hie\u00df es dann, wenn er sie trug:", "tokens": ["Stets", "hie\u00df", "es", "dann", ",", "wenn", "er", "sie", "trug", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ist es, der den Fuchs erschlug.", "tokens": ["Der", "ist", "es", ",", "der", "den", "Fuchs", "er\u00b7schlug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ein Fuchs von fl\u00fcchtiger Moral", "tokens": ["Ein", "Fuchs", "von", "fl\u00fcch\u00b7ti\u00b7ger", "Mo\u00b7ral"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und unbedenklich, wenn er stahl,", "tokens": ["Und", "un\u00b7be\u00b7denk\u00b7lich", ",", "wenn", "er", "stahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schlich sich bei Nacht zum H\u00fchnerstalle", "tokens": ["Schlich", "sich", "bei", "Nacht", "zum", "H\u00fch\u00b7ner\u00b7stal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PRF", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von einem namens Jochen Dralle,", "tokens": ["Von", "ei\u00b7nem", "na\u00b7mens", "Jo\u00b7chen", "Dral\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der, we