{"dta.poem.18904": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Vber den Tod  \n  &c.   Herren/ Johan Ernsten Hertzogen  \n zu Sachsen   &c.  \n  Vnd  \n Herren Ernsten Graven von Man\u00dffeld.   &c.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Warumb hat man die/ welche seind", "tokens": ["Wa\u00b7rumb", "hat", "man", "die", "/", "wel\u00b7che", "seind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "PIS", "ART", "$(", "PWS", "VAFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Freyheit vnd der Warheit feind", "tokens": ["Der", "Frey\u00b7heit", "vnd", "der", "War\u00b7heit", "feind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So rasend/ doll vnd fro gesehen", "tokens": ["So", "ra\u00b7send", "/", "doll", "vnd", "fro", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$(", "ADJD", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Pfaffen stat vnd anderstwa?", "tokens": ["Zu", "Pfaf\u00b7fen", "stat", "vnd", "an\u00b7derst\u00b7wa", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil jhnen nu zu widerstehen", "tokens": ["Weil", "jh\u00b7nen", "nu", "zu", "wi\u00b7der\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-",