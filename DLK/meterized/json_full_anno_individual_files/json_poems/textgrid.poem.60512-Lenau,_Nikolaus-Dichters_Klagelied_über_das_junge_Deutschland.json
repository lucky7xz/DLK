{"textgrid.poem.60512": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Dichters Klagelied \u00fcber das junge Deutschland", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da droben auf jenem Berge,", "tokens": ["Da", "dro\u00b7ben", "auf", "je\u00b7nem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da steh ich tausendmal", "tokens": ["Da", "steh", "ich", "tau\u00b7send\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An meinem Stabe gebogen", "tokens": ["An", "mei\u00b7nem", "Sta\u00b7be", "ge\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und schaue hinab ins Tal;", "tokens": ["Und", "schau\u00b7e", "hin\u00b7ab", "ins", "Tal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Folg meiner Gedankenherde,", "tokens": ["Folg", "mei\u00b7ner", "Ge\u00b7dan\u00b7ken\u00b7her\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Herz bewahret mir sie;", "tokens": ["Mein", "Herz", "be\u00b7wah\u00b7ret", "mir", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Kunst ist herabgekommen", "tokens": ["Die", "Kunst", "ist", "her\u00b7ab\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wei\u00df wohl selber nicht wie.", "tokens": ["Und", "wei\u00df", "wohl", "sel\u00b7ber", "nicht", "wie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKNEG", "PWAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Da stehet von sch\u00f6nen Blumen", "tokens": ["Da", "ste\u00b7het", "von", "sch\u00f6\u00b7nen", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die ganze Wiese so voll,", "tokens": ["Die", "gan\u00b7ze", "Wie\u00b7se", "so", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich breche sie, ohne zu wissen,", "tokens": ["Ich", "bre\u00b7che", "sie", ",", "oh\u00b7ne", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wem ich sie geben soll.", "tokens": ["Wem", "ich", "sie", "ge\u00b7ben", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und Regen, Sturm und Gewitter", "tokens": ["Und", "Re\u00b7gen", ",", "Sturm", "und", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"]