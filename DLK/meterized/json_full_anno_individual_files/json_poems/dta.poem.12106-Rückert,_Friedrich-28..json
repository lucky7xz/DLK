{"dta.poem.12106": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Kirch' hat an den Weg ihr Gottesbild gestellt,", "tokens": ["Die", "Kirch'", "hat", "an", "den", "Weg", "ihr", "Got\u00b7tes\u00b7bild", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Davor anbetend, wer vorbeigeht, niederf\u00e4llt.", "tokens": ["Da\u00b7vor", "an\u00b7be\u00b7tend", ",", "wer", "vor\u00b7bei\u00b7geht", ",", "nie\u00b7der\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVPP", "$,", "PWS", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Dahinter hat der F\u00fcrst gestellt sein eignes Bild,", "tokens": ["Da\u00b7hin\u00b7ter", "hat", "der", "F\u00fcrst", "ge\u00b7stellt", "sein", "eig\u00b7nes", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "VVPP", "VAINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nimmt nun seinen Theil von dem, was jenem gilt.", "tokens": ["Das", "nimmt", "nun", "sei\u00b7nen", "Theil", "von", "dem", ",", "was", "je\u00b7nem", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "$,", "PWS", "PDAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn jeder wer nun f\u00e4llt vorm Gottesbilde nieder,", "tokens": ["Denn", "je\u00b7der", "wer", "nun", "f\u00e4llt", "vorm", "Got\u00b7tes\u00b7bil\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu beugen scheint er auch vorm F\u00fcrsten seine Glieder.", "tokens": ["Zu", "beu\u00b7gen", "scheint", "er", "auch", "vorm", "F\u00fcrs\u00b7ten", "sei\u00b7ne", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihr F\u00fcrsten, wenn ihr wollt geehret euern Thron,", "tokens": ["Ihr", "F\u00fcrs\u00b7ten", ",", "wenn", "ihr", "wollt", "ge\u00b7eh\u00b7ret", "eu\u00b7ern", "Thron", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VMFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verb\u00fcnden m\u00fc\u00dft ihr euch mit der Religion.", "tokens": ["Ver\u00b7b\u00fcn\u00b7den", "m\u00fc\u00dft", "ihr", "euch", "mit", "der", "Re\u00b7li\u00b7gi\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}