{"textgrid.poem.48665": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "28. Heimliches Einverst\u00e4ndni\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00df sie gleich sich itzund stellen,", "tokens": ["Mu\u00df", "sie", "gleich", "sich", "it\u00b7zund", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "als w\u00e4r' ich ihr unbekant,", "tokens": ["als", "w\u00e4r'", "ich", "ihr", "un\u00b7be\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "meint drum nicht, ihr Mitgesellen,", "tokens": ["meint", "drum", "nicht", ",", "ihr", "Mit\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PTKNEG", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df ihr Sinn sei umgewant.", "tokens": ["da\u00df", "ihr", "Sinn", "sei", "um\u00b7ge\u00b7want", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre Treu' in unsrem Handel,", "tokens": ["Ih\u00b7re", "Treu'", "in", "uns\u00b7rem", "Han\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die wei\u00df ganz von keinem Wandel.", "tokens": ["die", "wei\u00df", "ganz", "von", "kei\u00b7nem", "Wan\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Amor liebet solche Herzen,", "tokens": ["A\u00b7mor", "lie\u00b7bet", "sol\u00b7che", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die des Mundes Meister sein,", "tokens": ["die", "des", "Mun\u00b7des", "Meis\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "die bei Trauren k\u00f6nnen scherzen", "tokens": ["die", "bei", "Trau\u00b7ren", "k\u00f6n\u00b7nen", "scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und erfreuet sein in Pein.", "tokens": ["und", "er\u00b7freu\u00b7et", "sein", "in", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer will pa\u00dffrei sein im Lieben,", "tokens": ["Wer", "will", "pa\u00df\u00b7frei", "sein", "im", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADJD", "VAINF", "APPRART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "der mu\u00df sich im Bergen \u00fcben.", "tokens": ["der", "mu\u00df", "sich", "im", "Ber\u00b7gen", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also wenig sie sich hassen", "tokens": ["Al\u00b7so", "we\u00b7nig", "sie", "sich", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und nicht selber sie sein mag,", "tokens": ["und", "nicht", "sel\u00b7ber", "sie", "sein", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "PPER", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "also wenig wird sie lassen", "tokens": ["al\u00b7so", "we\u00b7nig", "wird", "sie", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "den, der sie zu sein stets pflag.", "tokens": ["den", ",", "der", "sie", "zu", "sein", "stets", "pflag", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eins, das sich dem andern giebet,", "tokens": ["Eins", ",", "das", "sich", "dem", "an\u00b7dern", "gie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "liebt es, wie sichs selten liebet.", "tokens": ["liebt", "es", ",", "wie", "sichs", "sel\u00b7ten", "lie\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PIS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dennoch hat sie mich im Sinne,", "tokens": ["Den\u00b7noch", "hat", "sie", "mich", "im", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hat sie mich im Auge nicht.", "tokens": ["hat", "sie", "mich", "im", "Au\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPRART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht ists au\u00dfen, sondern drinne,", "tokens": ["Nicht", "ists", "au\u00b7\u00dfen", ",", "son\u00b7dern", "drin\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "ADV", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "was mir ihre Gunst verspricht.", "tokens": ["was", "mir", "ih\u00b7re", "Gunst", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fcssen schon die Lippen schweigen,", "tokens": ["M\u00fcs\u00b7sen", "schon", "die", "Lip\u00b7pen", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sie denkt doch: der bleibt mein eigen.", "tokens": ["sie", "denkt", "doch", ":", "der", "bleibt", "mein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ART", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Recht so, Schwester, la\u00df nicht merken,", "tokens": ["Recht", "so", ",", "Schwes\u00b7ter", ",", "la\u00df", "nicht", "mer\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$,", "VVIMP", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was dich heimlich labt und kr\u00e4nkt.", "tokens": ["was", "dich", "heim\u00b7lich", "labt", "und", "kr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Man verr\u00e4t sich mit den Werken,", "tokens": ["Man", "ver\u00b7r\u00e4t", "sich", "mit", "den", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "der bleibt sicher, der viel denkt.", "tokens": ["der", "bleibt", "si\u00b7cher", ",", "der", "viel", "denkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADJD", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "La\u00df sie sagen, was sie wollen,", "tokens": ["La\u00df", "sie", "sa\u00b7gen", ",", "was", "sie", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "PRELS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wir nur wissen, was wir sollen.", "tokens": ["wir", "nur", "wis\u00b7sen", ",", "was", "wir", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "PRELS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sei dir \u00e4hnlich und verbleibe,", "tokens": ["Sei", "dir", "\u00e4hn\u00b7lich", "und", "ver\u00b7blei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die du vor warst und noch bist,", "tokens": ["die", "du", "vor", "warst", "und", "noch", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "VAFIN", "KON", "ADV", "VAFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "und denk nicht, weil ich nichts schreibe,", "tokens": ["und", "denk", "nicht", ",", "weil", "ich", "nichts", "schrei\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "da\u00df mein Denken dich vergi\u00dft.", "tokens": ["da\u00df", "mein", "Den\u00b7ken", "dich", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So gedenk' ich stetigs deiner,", "tokens": ["So", "ge\u00b7denk'", "ich", "ste\u00b7tigs", "dei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df ich auch vergesse meiner.", "tokens": ["da\u00df", "ich", "auch", "ver\u00b7ges\u00b7se", "mei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mu\u00df sie gleich sich itzund stellen,", "tokens": ["Mu\u00df", "sie", "gleich", "sich", "it\u00b7zund", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "als w\u00e4r' ich ihr unbekant,", "tokens": ["als", "w\u00e4r'", "ich", "ihr", "un\u00b7be\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "meint drum nicht, ihr Mitgesellen,", "tokens": ["meint", "drum", "nicht", ",", "ihr", "Mit\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PTKNEG", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df ihr Sinn sei umgewant.", "tokens": ["da\u00df", "ihr", "Sinn", "sei", "um\u00b7ge\u00b7want", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre Treu' in unsrem Handel,", "tokens": ["Ih\u00b7re", "Treu'", "in", "uns\u00b7rem", "Han\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die wei\u00df ganz von keinem Wandel.", "tokens": ["die", "wei\u00df", "ganz", "von", "kei\u00b7nem", "Wan\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Amor liebet solche Herzen,", "tokens": ["A\u00b7mor", "lie\u00b7bet", "sol\u00b7che", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die des Mundes Meister sein,", "tokens": ["die", "des", "Mun\u00b7des", "Meis\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "die bei Trauren k\u00f6nnen scherzen", "tokens": ["die", "bei", "Trau\u00b7ren", "k\u00f6n\u00b7nen", "scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und erfreuet sein in Pein.", "tokens": ["und", "er\u00b7freu\u00b7et", "sein", "in", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer will pa\u00dffrei sein im Lieben,", "tokens": ["Wer", "will", "pa\u00df\u00b7frei", "sein", "im", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADJD", "VAINF", "APPRART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "der mu\u00df sich im Bergen \u00fcben.", "tokens": ["der", "mu\u00df", "sich", "im", "Ber\u00b7gen", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Also wenig sie sich hassen", "tokens": ["Al\u00b7so", "we\u00b7nig", "sie", "sich", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und nicht selber sie sein mag,", "tokens": ["und", "nicht", "sel\u00b7ber", "sie", "sein", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "PPER", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "also wenig wird sie lassen", "tokens": ["al\u00b7so", "we\u00b7nig", "wird", "sie", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "den, der sie zu sein stets pflag.", "tokens": ["den", ",", "der", "sie", "zu", "sein", "stets", "pflag", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eins, das sich dem andern giebet,", "tokens": ["Eins", ",", "das", "sich", "dem", "an\u00b7dern", "gie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "liebt es, wie sichs selten liebet.", "tokens": ["liebt", "es", ",", "wie", "sichs", "sel\u00b7ten", "lie\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PIS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Dennoch hat sie mich im Sinne,", "tokens": ["Den\u00b7noch", "hat", "sie", "mich", "im", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hat sie mich im Auge nicht.", "tokens": ["hat", "sie", "mich", "im", "Au\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPRART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht ists au\u00dfen, sondern drinne,", "tokens": ["Nicht", "ists", "au\u00b7\u00dfen", ",", "son\u00b7dern", "drin\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "ADV", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "was mir ihre Gunst verspricht.", "tokens": ["was", "mir", "ih\u00b7re", "Gunst", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fcssen schon die Lippen schweigen,", "tokens": ["M\u00fcs\u00b7sen", "schon", "die", "Lip\u00b7pen", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sie denkt doch: der bleibt mein eigen.", "tokens": ["sie", "denkt", "doch", ":", "der", "bleibt", "mein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ART", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Recht so, Schwester, la\u00df nicht merken,", "tokens": ["Recht", "so", ",", "Schwes\u00b7ter", ",", "la\u00df", "nicht", "mer\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$,", "VVIMP", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was dich heimlich labt und kr\u00e4nkt.", "tokens": ["was", "dich", "heim\u00b7lich", "labt", "und", "kr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Man verr\u00e4t sich mit den Werken,", "tokens": ["Man", "ver\u00b7r\u00e4t", "sich", "mit", "den", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "der bleibt sicher, der viel denkt.", "tokens": ["der", "bleibt", "si\u00b7cher", ",", "der", "viel", "denkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADJD", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "La\u00df sie sagen, was sie wollen,", "tokens": ["La\u00df", "sie", "sa\u00b7gen", ",", "was", "sie", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "PRELS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wir nur wissen, was wir sollen.", "tokens": ["wir", "nur", "wis\u00b7sen", ",", "was", "wir", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "PRELS", "PPER", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Sei dir \u00e4hnlich und verbleibe,", "tokens": ["Sei", "dir", "\u00e4hn\u00b7lich", "und", "ver\u00b7blei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die du vor warst und noch bist,", "tokens": ["die", "du", "vor", "warst", "und", "noch", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "VAFIN", "KON", "ADV", "VAFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "und denk nicht, weil ich nichts schreibe,", "tokens": ["und", "denk", "nicht", ",", "weil", "ich", "nichts", "schrei\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "da\u00df mein Denken dich vergi\u00dft.", "tokens": ["da\u00df", "mein", "Den\u00b7ken", "dich", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So gedenk' ich stetigs deiner,", "tokens": ["So", "ge\u00b7denk'", "ich", "ste\u00b7tigs", "dei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df ich auch vergesse meiner.", "tokens": ["da\u00df", "ich", "auch", "ver\u00b7ges\u00b7se", "mei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}