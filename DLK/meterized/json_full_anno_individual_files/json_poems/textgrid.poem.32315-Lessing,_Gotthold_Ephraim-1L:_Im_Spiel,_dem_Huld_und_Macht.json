{"textgrid.poem.32315": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Spiel, dem Huld und Macht", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Spiel, dem Huld und Macht", "tokens": ["Im", "Spiel", ",", "dem", "Huld", "und", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Welt zur B\u00fchne gab, das Weisheit ausgedacht,", "tokens": ["Die", "Welt", "zur", "B\u00fch\u00b7ne", "gab", ",", "das", "Weis\u00b7heit", "aus\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In diesem Spiel zur kurzen Szen' erlesen,", "tokens": ["In", "die\u00b7sem", "Spiel", "zur", "kur\u00b7zen", "Szen'", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jahr! Zeit, f\u00fcr Sterbliche gewesen!", "tokens": ["Jahr", "!", "Zeit", ",", "f\u00fcr", "Sterb\u00b7li\u00b7che", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$,", "APPR", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr ihn, der eh du kamst, dich als gekommen sah,", "tokens": ["F\u00fcr", "ihn", ",", "der", "eh", "du", "kamst", ",", "dich", "als", "ge\u00b7kom\u00b7men", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "KOUS", "PPER", "VVFIN", "$,", "PPER", "KOUS", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "F\u00fcr Gott noch da!", "tokens": ["F\u00fcr", "Gott", "noch", "da", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "So wie ein Strom, der aus der Erde bricht,", "tokens": ["So", "wie", "ein", "Strom", ",", "der", "aus", "der", "Er\u00b7de", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wenig Meilen rollt, und wieder sich verkriecht,", "tokens": ["Und", "we\u00b7nig", "Mei\u00b7len", "rollt", ",", "und", "wie\u00b7der", "sich", "ver\u00b7kriecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "KON", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bist du, aus der du dich ergossen,", "tokens": ["Bist", "du", ",", "aus", "der", "du", "dich", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "PRELS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Ewigkeit, \u2013 die Gott, mit aller Welten Last,", "tokens": ["Zur", "E\u00b7wig\u00b7keit", ",", "\u2013", "die", "Gott", ",", "mit", "al\u00b7ler", "Wel\u00b7ten", "Last", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "$(", "ART", "NN", "$,", "APPR", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Im Zipfel seines Kleides fa\u00dft, \u2013", "tokens": ["Im", "Zip\u00b7fel", "sei\u00b7nes", "Klei\u00b7des", "fa\u00dft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Ewigkeit zur\u00fcck geflossen.", "tokens": ["Zur", "E\u00b7wig\u00b7keit", "zu\u00b7r\u00fcck", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vom D\u00fcrftigen verseufzt, mit tr\u00e4nenvollen Blicken", "tokens": ["Vom", "D\u00fcrf\u00b7ti\u00b7gen", "ver\u00b7seufzt", ",", "mit", "tr\u00e4\u00b7nen\u00b7vol\u00b7len", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Reuenden verfolgt, zur\u00fcck gew\u00fcnscht vom Tor,", "tokens": ["Des", "Reu\u00b7en\u00b7den", "ver\u00b7folgt", ",", "zu\u00b7r\u00fcck", "ge\u00b7w\u00fcnscht", "vom", "Tor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vom Gl\u00fccklichen erw\u00e4hnt mit trunkenem Entz\u00fccken:", "tokens": ["Vom", "Gl\u00fcck\u00b7li\u00b7chen", "er\u00b7w\u00e4hnt", "mit", "trun\u00b7ke\u00b7nem", "Ent\u00b7z\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jahr, welche Botschaft von der Erde, \u2013", "tokens": ["Jahr", ",", "wel\u00b7che", "Bot\u00b7schaft", "von", "der", "Er\u00b7de", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PWAT", "NN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jetzt unwert jenes Rufs: Sie werde! \u2013", "tokens": ["Jetzt", "un\u00b7wert", "je\u00b7nes", "Rufs", ":", "Sie", "wer\u00b7de", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PDAT", "NN", "$.", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bringst du dem Himmel vor?", "tokens": ["Bringst", "du", "dem", "Him\u00b7mel", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Botschaft ach! vom Triumph des Lasters \u00fcber Tugend,", "tokens": ["Bot\u00b7schaft", "ach", "!", "vom", "Tri\u00b7umph", "des", "Las\u00b7ters", "\u00fc\u00b7ber", "Tu\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ITJ", "$.", "APPRART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Hier vordem ihrem liebsten Sitz;", "tokens": ["Hier", "vor\u00b7dem", "ih\u00b7rem", "liebs\u00b7ten", "Sitz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von V\u00e4tern b\u00f6ser Art; Botschaft von schlimmrer Jugend;", "tokens": ["Von", "V\u00e4\u00b7tern", "b\u00f6\u00b7ser", "Art", ";", "Bot\u00b7schaft", "von", "schlimm\u00b7rer", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$.", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Feinden Gottes, stolz auf Witz;", "tokens": ["Von", "Fein\u00b7den", "Got\u00b7tes", ",", "stolz", "auf", "Witz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Botschaft von feiler Ehr, womit die Schmach sich schm\u00fccket;", "tokens": ["Bot\u00b7schaft", "von", "fei\u00b7ler", "Ehr", ",", "wo\u00b7mit", "die", "Schmach", "sich", "schm\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Von ungerechtem Recht, das arme Fromme dr\u00fccket.", "tokens": ["Von", "un\u00b7ge\u00b7rech\u00b7tem", "Recht", ",", "das", "ar\u00b7me", "From\u00b7me", "dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Botschaft, da\u00df die Natur l\u00e4ngst unsrer m\u00fcde worden,", "tokens": ["Bot\u00b7schaft", ",", "da\u00df", "die", "Na\u00b7tur", "l\u00e4ngst", "uns\u00b7rer", "m\u00fc\u00b7de", "wor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ADV", "PPOSAT", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dort mit Fl\u00fcssen Feuers schreckt,", "tokens": ["Die", "dort", "mit", "Fl\u00fcs\u00b7sen", "Feu\u00b7ers", "schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das paradiesische Gefilde \u00fcberdeckt,", "tokens": ["Das", "pa\u00b7ra\u00b7die\u00b7si\u00b7sche", "Ge\u00b7fil\u00b7de", "\u00fc\u00b7berd\u00b7eckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dort, gesch\u00e4ftig im Ermorden,", "tokens": ["Und", "dort", ",", "ge\u00b7sch\u00e4f\u00b7tig", "im", "Er\u00b7mor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der aufgebotnen Pest", "tokens": ["Der", "auf\u00b7ge\u00b7bot\u00b7nen", "Pest"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die gift'gen Schwingen sch\u00fctteln l\u00e4\u00dft.", "tokens": ["Die", "gift'\u00b7gen", "Schwin\u00b7gen", "sch\u00fct\u00b7teln", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Botschaft von hingeri\u00dfnen G\u00f6ttern", "tokens": ["Bot\u00b7schaft", "von", "hin\u00b7ge\u00b7ri\u00df\u00b7nen", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der einst durch sie regierten Welt;", "tokens": ["Der", "einst", "durch", "sie", "re\u00b7gier\u00b7ten", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Botschaft von finstern Kriegeswettern,", "tokens": ["Bot\u00b7schaft", "von", "fins\u00b7tern", "Krie\u00b7ges\u00b7wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die hier ein Gott zur\u00fccke h\u00e4lt,", "tokens": ["Die", "hier", "ein", "Gott", "zu\u00b7r\u00fc\u00b7cke", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dort ein Gott, der grausamer verf\u00e4hrt,", "tokens": ["Und", "dort", "ein", "Gott", ",", "der", "grau\u00b7sa\u00b7mer", "ver\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit immer neuen Blitzen n\u00e4hrt.", "tokens": ["Mit", "im\u00b7mer", "neu\u00b7en", "Blit\u00b7zen", "n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch Botschaft auch von einem Lande,", "tokens": ["Doch", "Bot\u00b7schaft", "auch", "von", "ei\u00b7nem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und Ruh und Gl\u00fcck, im schwesterlichen Bande,", "tokens": ["Und", "Ruh", "und", "Gl\u00fcck", ",", "im", "schwes\u00b7ter\u00b7li\u00b7chen", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schwellen seines Thrones ziert;", "tokens": ["Die", "Schwel\u00b7len", "sei\u00b7nes", "Thro\u00b7nes", "ziert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Thrones, ungewi\u00df, ob ihn mehr Vorsicht sch\u00fctzt,", "tokens": ["Des", "Thro\u00b7nes", ",", "un\u00b7ge\u00b7wi\u00df", ",", "ob", "ihn", "mehr", "Vor\u00b7sicht", "sch\u00fctzt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als Liebe st\u00fctzt.", "tokens": ["Als", "Lie\u00b7be", "st\u00fctzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "O ihr, die ", "tokens": ["O", "ihr", ",", "die"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "PPOSAT", "$,", "PRELS"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Ihr V\u00f6lker jauchzt ihm zu! Der Himmel stimmet ein.", "tokens": ["Ihr", "V\u00f6l\u00b7ker", "jauchzt", "ihm", "zu", "!", "Der", "Him\u00b7mel", "stim\u00b7met", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf! strebt, da\u00df er mit diesem Jahre,", "tokens": ["Auf", "!", "strebt", ",", "da\u00df", "er", "mit", "die\u00b7sem", "Jah\u00b7re", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er sie jetzt nicht schon erf\u00e4hrt,", "tokens": ["Wenn", "er", "sie", "jetzt", "nicht", "schon", "er\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die wicht'ge Botschaft froh erfahre:", "tokens": ["Die", "wicht'\u00b7ge", "Bot\u00b7schaft", "froh", "er\u00b7fah\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr w\u00e4ret eures ", "tokens": ["Ihr", "w\u00e4\u00b7ret", "eu\u00b7res"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Im Spiel, dem Huld und Macht", "tokens": ["Im", "Spiel", ",", "dem", "Huld", "und", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Welt zur B\u00fchne gab, das Weisheit ausgedacht,", "tokens": ["Die", "Welt", "