{"dta.poem.18059": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "XvI.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Frosch war einst mit seinem Jungen", "tokens": ["Ein", "Frosch", "war", "einst", "mit", "sei\u00b7nem", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus seinem Sumpf ans Land gesprungen;", "tokens": ["Aus", "sei\u00b7nem", "Sumpf", "ans", "Land", "ge\u00b7sprun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo er ein grosses Rindvieh fand;", "tokens": ["Wo", "er", "ein", "gros\u00b7ses", "Rind\u00b7vieh", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das hielt er sich vor eine Schand.", "tokens": ["Das", "hielt", "er", "sich", "vor", "ei\u00b7ne", "Schand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er sprach: O Himmel, hab ich dir", "tokens": ["Er", "sprach", ":", "O", "Him\u00b7mel", ",", "hab", "ich", "dir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NE", "NN", "$,", "VAFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darum zu dancken, da\u00df du mir", "tokens": ["Da\u00b7rum", "zu", "dan\u00b7cken", ",", "da\u00df", "du", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Solch einen kleinen Leib gegeben?", "tokens": ["Solch", "ei\u00b7nen", "klei\u00b7nen", "Leib", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie gantz verschm\u00e4het ist mein Leben", "tokens": ["Wie", "gantz", "ver\u00b7schm\u00e4\u00b7het", "ist", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vor vielen, welche gr\u00f6sser sind,", "tokens": ["Vor", "vie\u00b7len", ",", "wel\u00b7che", "gr\u00f6s\u00b7ser", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie gegenw\u00e4rtig dieses Rind!", "tokens": ["Wie", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "die\u00b7ses", "Rind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Er wollte gern dem Ochsen gleichen,", "tokens": ["Er", "woll\u00b7te", "gern", "dem", "Och\u00b7sen", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Er bl\u00e4ht sich sehr, ihn zu erreichen.", "tokens": ["Er", "bl\u00e4ht", "sich", "sehr", ",", "ihn", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sein Sohn sieht das, und spricht: o nein!", "tokens": ["Sein", "Sohn", "sieht", "das", ",", "und", "spricht", ":", "o", "nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PDS", "$,", "KON", "VVFIN", "$.", "FM", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Es hilft nicht: La\u00df dein bl\u00e4hen seyn.", "tokens": ["Es", "hilft", "nicht", ":", "La\u00df", "dein", "bl\u00e4\u00b7hen", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "VVIMP", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Du magst dem nicht wohl widerstreben,", "tokens": ["Du", "magst", "dem", "nicht", "wohl", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Was die Natur dir hat gegeben.", "tokens": ["Was", "die", "Na\u00b7tur", "dir", "hat", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der Alte folgte nicht dem Knaben,", "tokens": ["Der", "Al\u00b7te", "folg\u00b7te", "nicht", "dem", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Die Hoffart wollt er f\u00fcr sich haben.", "tokens": ["Die", "Hof\u00b7fart", "wollt", "er", "f\u00fcr", "sich", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "PRF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Zum andern mahl nun, bl\u00e4ht er sich,", "tokens": ["Zum", "an\u00b7dern", "mahl", "nun", ",", "bl\u00e4ht", "er", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "ADV", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und sprach zn seinem Sohn: M\u00f6gt ich", "tokens": ["Und", "sprach", "zn", "sei\u00b7nem", "Sohn", ":", "M\u00f6gt", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "NE", "PPOSAT", "NN", "$.", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Dem grossen Ochsen gleicher werden,", "tokens": ["Dem", "gros\u00b7sen", "Och\u00b7sen", "glei\u00b7cher", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Gesch\u00e4he mir auf dieser Erden", "tokens": ["Ge\u00b7sch\u00e4\u00b7he", "mir", "auf", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Nichts liebers, Sohn, auf meinen Eid.", "tokens": ["Nichts", "lie\u00b7bers", ",", "Sohn", ",", "auf", "mei\u00b7nen", "Eid", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der Sohn sprach: Vater, mir ist leid,", "tokens": ["Der", "Sohn", "sprach", ":", "Va\u00b7ter", ",", "mir", "ist", "leid", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Da\u00df du dich marterst ohne Noth", "tokens": ["Da\u00df", "du", "dich", "mar\u00b7terst", "oh\u00b7ne", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ich f\u00fcrchte sehr, du bl\u00e4hst dich todt,", "tokens": ["Ich", "f\u00fcrch\u00b7te", "sehr", ",", "du", "bl\u00e4hst", "dich", "todt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Folg mir und sey mit Willen klein,", "tokens": ["Folg", "mir", "und", "sey", "mit", "Wil\u00b7len", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und la\u00df dein \u00fcppig Bl\u00e4hen seyn.", "tokens": ["Und", "la\u00df", "dein", "\u00fcp\u00b7pig", "Bl\u00e4\u00b7hen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Der Alte sprach: Es w\u00e4r ein Spott,", "tokens": ["Der", "Al\u00b7te", "sprach", ":", "Es", "w\u00e4r", "ein", "Spott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Ich thu es nicht; mir helffe Gott;", "tokens": ["Ich", "thu", "es", "nicht", ";", "mir", "helf\u00b7fe", "Gott", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Ich mu\u00df gro\u00df werden, wie das Rind,", "tokens": ["Ich", "mu\u00df", "gro\u00df", "wer\u00b7den", ",", "wie", "das", "Rind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "De\u00df hast du Ehre, werthes Kind.", "tokens": ["De\u00df", "hast", "du", "Eh\u00b7re", ",", "wert\u00b7hes", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Er bl\u00e4hte sich, indem ers sprach,", "tokens": ["Er", "bl\u00e4h\u00b7te", "sich", ",", "in\u00b7dem", "ers", "sprach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "So heftig auf, da\u00df er zerbrach.", "tokens": ["So", "hef\u00b7tig", "auf", ",", "da\u00df", "er", "zer\u00b7brach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}