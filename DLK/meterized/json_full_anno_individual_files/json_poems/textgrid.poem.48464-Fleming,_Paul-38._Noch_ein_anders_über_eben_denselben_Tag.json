{"textgrid.poem.48464": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "38. Noch ein anders \u00fcber eben denselben Tag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer uns di\u00df vor gesagt und auf die h\u00f6chste Wette,", "tokens": ["Wer", "uns", "di\u00df", "vor", "ge\u00b7sagt", "und", "auf", "die", "h\u00f6chs\u00b7te", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PDS", "APPR", "VVPP", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die nimmer mag geschehn, mit uns gestritten h\u00e4tte,", "tokens": ["die", "nim\u00b7mer", "mag", "ge\u00b7schehn", ",", "mit", "uns", "ge\u00b7strit\u00b7ten", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "VVINF", "$,", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df uns die liebe Stadt, ich meine, ", "tokens": ["da\u00df", "uns", "die", "lie\u00b7be", "Stadt", ",", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "bis auf den sch\u00f6nen Tag solt' halten noch in sich", "tokens": ["bis", "auf", "den", "sch\u00f6\u00b7nen", "Tag", "solt'", "hal\u00b7ten", "noch", "in", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VMFIN", "VVINF", "ADV", "APPR", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "da man euch, Edle, pflegt mit W\u00fcndschen anzubinden,", "tokens": ["da", "man", "euch", ",", "Ed\u00b7le", ",", "pflegt", "mit", "W\u00fcnd\u00b7schen", "an\u00b7zu\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "$,", "ADJA", "$,", "VVFIN", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wir h\u00e4ttens drauf gewagt. Wir sind noch hier zu finden,", "tokens": ["wir", "h\u00e4t\u00b7tens", "drauf", "ge\u00b7wagt", ".", "Wir", "sind", "noch", "hier", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$.", "PPER", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "gedenken fast nicht weg, begehren nur allein,", "tokens": ["ge\u00b7den\u00b7ken", "fast", "nicht", "weg", ",", "be\u00b7geh\u00b7ren", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "da\u00df um euch, sch\u00f6nes Volk, wir lange m\u00f6gen sein.", "tokens": ["da\u00df", "um", "euch", ",", "sch\u00f6\u00b7nes", "Volk", ",", "wir", "lan\u00b7ge", "m\u00f6\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "$,", "ADJA", "NN", "$,", "PPER", "ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu Jahre war es nichts, als wir euch halb kaum kauten.", "tokens": ["Zu", "Jah\u00b7re", "war", "es", "nichts", ",", "als", "wir", "euch", "halb", "kaum", "kau\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PIS", "$,", "KOUS", "PPER", "PRF", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und gleichwol auf Begehr des edlen Abgesandten,", "tokens": ["Und", "gleich\u00b7wol", "auf", "Be\u00b7gehr", "des", "ed\u00b7len", "Ab\u00b7ge\u00b7sand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "den ihr f\u00fcr Oheimb ehrt, so bunden wir euch an,", "tokens": ["den", "ihr", "f\u00fcr", "O\u00b7heimb", "ehrt", ",", "so", "bun\u00b7den", "wir", "euch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,", "ADV", "ADJA", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "der denn sein L\u00f6segeld hat redlich gut getan.", "tokens": ["der", "denn", "sein", "L\u00f6\u00b7se\u00b7geld", "hat", "red\u00b7lich", "gut", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der angenehme Tag ward ganz in Lust verschlissen", "tokens": ["Der", "an\u00b7ge\u00b7neh\u00b7me", "Tag", "ward", "ganz", "in", "Lust", "ver\u00b7schlis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "bei gutem Trank' und Kost. Itzt, ob wir gleich sehn m\u00fcssen,", "tokens": ["bei", "gu\u00b7tem", "Trank'", "und", "Kost", ".", "Itzt", ",", "ob", "wir", "gleich", "sehn", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$.", "ADV", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "da\u00df euch das liebe Dorf f\u00fcr unsre Stadt gef\u00e4llt", "tokens": ["da\u00df", "euch", "das", "lie\u00b7be", "Dorf", "f\u00fcr", "uns\u00b7re", "Stadt", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "und da\u00df euch ", "tokens": ["und", "da\u00df", "euch"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "iedennoch wollen wir nicht unterbleiben lassen,", "tokens": ["ie\u00b7den\u00b7noch", "wol\u00b7len", "wir", "nicht", "un\u00b7ter\u00b7blei\u00b7ben", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "euch in dem Absein auch zu binden gleicher Maa\u00dfen,", "tokens": ["euch", "in", "dem", "Ab\u00b7sein", "auch", "zu", "bin\u00b7den", "glei\u00b7cher", "Maa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "als wie vorhin beschehn. Und da\u00df ihr ja nicht klagt,", "tokens": ["als", "wie", "vor\u00b7hin", "be\u00b7schehn", ".", "Und", "da\u00df", "ihr", "ja", "nicht", "klagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ADV", "VVINF", "$.", "KON", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "man hab' euch euren Tag nicht redlich angesagt,", "tokens": ["man", "hab'", "euch", "eu\u00b7ren", "Tag", "nicht", "red\u00b7lich", "an\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "so seht: wir kommen selbst mit unserm Angebinde", "tokens": ["so", "seht", ":", "wir", "kom\u00b7men", "selbst", "mit", "un\u00b7serm", "An\u00b7ge\u00b7bin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "durch Weile und Verdreu\u00df, durch Regen und durch Winde.", "tokens": ["durch", "Wei\u00b7le", "und", "Ver\u00b7dreu\u00df", ",", "durch", "Re\u00b7gen", "und", "durch", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Gebt her die zarte Hand! Sie mu\u00df gefangen sein.", "tokens": ["Gebt", "her", "die", "zar\u00b7te", "Hand", "!", "Sie", "mu\u00df", "ge\u00b7fan\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Nach diesem wollen wir auch f\u00fchren einen Reihn", "tokens": ["Nach", "die\u00b7sem", "wol\u00b7len", "wir", "auch", "f\u00fch\u00b7ren", "ei\u00b7nen", "Reihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VMFIN", "PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "um euch, ihr sch\u00f6nes Kind, und unter solchem Tanze", "tokens": ["um", "euch", ",", "ihr", "sch\u00f6\u00b7nes", "Kind", ",", "und", "un\u00b7ter", "sol\u00b7chem", "Tan\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "euch w\u00fcndschen, da\u00df der Tag mit seinem Gl\u00fcck' und Glanze", "tokens": ["euch", "w\u00fcnd\u00b7schen", ",", "da\u00df", "der", "Tag", "mit", "sei\u00b7nem", "Gl\u00fcck", "und", "Glan\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "euch vielmal so erfreu'. Es kan nicht m\u00fcglich sein,", "tokens": ["euch", "viel\u00b7mal", "so", "er\u00b7freu'", ".", "Es", "kan", "nicht", "m\u00fcg\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "$.", "PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "da\u00df nicht die G\u00f6tter selbst den Willen g\u00e4ben drein.", "tokens": ["da\u00df", "nicht", "die", "G\u00f6t\u00b7ter", "selbst", "den", "Wil\u00b7len", "g\u00e4\u00b7ben", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sie sind euch alle gut. Wolan! so la\u00dft denn schauen,", "tokens": ["Sie", "sind", "euch", "al\u00b7le", "gut", ".", "Wo\u00b7lan", "!", "so", "la\u00dft", "denn", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADJD", "$.", "ADV", "$.", "ADV", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "wie fr\u00f6lich ihr k\u00f6nnt sein und euer' zwei Jungfrauen!", "tokens": ["wie", "fr\u00f6\u00b7lich", "ihr", "k\u00f6nnt", "sein", "und", "eu\u00b7er'", "zwei", "Jung\u00b7frau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "VAINF", "KON", "PPOSAT", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Vertauschet Gunst mit Lust! Viel L\u00f6sens darf es nicht,", "tokens": ["Ver\u00b7tau\u00b7schet", "Gunst", "mit", "Lust", "!", "Viel", "L\u00f6\u00b7sens", "darf", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$.", "PIAT", "NN", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "nur da\u00df uns heute nichts an s\u00fc\u00dfer Zier gebricht.", "tokens": ["nur", "da\u00df", "uns", "heu\u00b7te", "nichts", "an", "s\u00fc\u00b7\u00dfer", "Zier", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "La\u00dft den Tag euer sein und denket hierbeineben,", "tokens": ["La\u00dft", "den", "Tag", "eu\u00b7er", "sein", "und", "den\u00b7ket", "hier\u00b7bei\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "VAINF", "KON", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "was man ihm anderweit itzt wird f\u00fcr Ehre geben,", "tokens": ["was", "man", "ihm", "an\u00b7der\u00b7weit", "itzt", "wird", "f\u00fcr", "Eh\u00b7re", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "ADV", "ADV", "VAFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "wie man ihn wird begehn! Werd't ihr sein Recht ihm tun,", "tokens": ["wie", "man", "ihn", "wird", "be\u00b7gehn", "!", "Werd't", "ihr", "sein", "Recht", "ihm", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VAFIN", "VVINF", "$.", "VAFIN", "PPER", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "so sollt ihr allemal ihn sehen gleich wie nun.", "tokens": ["so", "sollt", "ihr", "al\u00b7le\u00b7mal", "ihn", "se\u00b7hen", "gleich", "wie", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "VVFIN", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer uns di\u00df vor gesagt und auf die h\u00f6chste Wette,", "tokens": ["Wer", "uns", "di\u00df", "vor", "ge\u00b7sagt", "und", "auf", "die", "h\u00f6chs\u00b7te", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PDS", "APPR", "VVPP", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die nimmer mag geschehn, mit uns gestritten h\u00e4tte,", "tokens": ["die", "nim\u00b7mer", "mag", "ge\u00b7schehn", ",", "mit", "uns", "ge\u00b7strit\u00b7ten", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "VVINF", "$,", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df uns die liebe Stadt, ich meine, ", "tokens": ["da\u00df", "uns", "die", "lie\u00b7be", "Stadt", ",", "ich", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "bis auf den sch\u00f6nen Tag solt' halten noch in sich", "tokens": ["bis", "auf", "den", "sch\u00f6\u00b7nen", "Tag", "solt'", "hal\u00b7ten", "noch", "in", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VMFIN", "VVINF", "ADV", "APPR", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "da man euch, Edle, pflegt mit W\u00fcndschen anzubinden,", "tokens": ["da", "man", "euch", ",", "Ed\u00b7le", ",", "pflegt", "mit", "W\u00fcnd\u00b7schen", "an\u00b7zu\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "$,", "ADJA", "$,", "VVFIN", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wir h\u00e4ttens drauf gewagt. Wir sind noch hier zu finden,", "tokens": ["wir", "h\u00e4t\u00b7tens", "drauf", "ge\u00b7wagt", ".", "Wir", "sind", "noch", "hier", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$.", "PPER", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "gedenken fast nicht weg, begehren nur allein,", "tokens": ["ge\u00b7den\u00b7ken", "fast", "nicht", "weg", ",", "be\u00b7geh\u00b7ren", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "da\u00df um euch, sch\u00f6nes Volk, wir lange m\u00f6gen sein.", "tokens": ["da\u00df", "um", "euch", ",", "sch\u00f6\u00b7nes", "Volk", ",", "wir", "lan\u00b7ge", "m\u00f6\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "$,", "ADJA", "NN", "$,", "PPER", "ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu Jahre war es ni