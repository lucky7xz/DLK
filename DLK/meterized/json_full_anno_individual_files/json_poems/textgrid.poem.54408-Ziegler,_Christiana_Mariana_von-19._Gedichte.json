{"textgrid.poem.54408": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "19. Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Thyrsis! k\u00f6nnt ich dich auf diesen sch\u00f6nen Auen,", "tokens": ["Mein", "Thyr\u00b7sis", "!", "k\u00f6nnt", "ich", "dich", "auf", "die\u00b7sen", "sch\u00f6\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich in dem Augenblick an meiner Seite schauen,", "tokens": ["Gleich", "in", "dem", "Au\u00b7gen\u00b7blick", "an", "mei\u00b7ner", "Sei\u00b7te", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie z\u00e4rtlich wollt ich nicht mit dir im Gr\u00fcnen scherzen,", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "wollt", "ich", "nicht", "mit", "dir", "im", "Gr\u00fc\u00b7nen", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir theilten Wort und Ku\u00df, und zwey verliebte Herzen.", "tokens": ["Wir", "theil\u00b7ten", "Wort", "und", "Ku\u00df", ",", "und", "zwey", "ver\u00b7lieb\u00b7te", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "KON", "CARD", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich b\u00e4nde dir gewi\u00df den aller sch\u00f6nsten Strau\u00df,", "tokens": ["Ich", "b\u00e4n\u00b7de", "dir", "ge\u00b7wi\u00df", "den", "al\u00b7ler", "sch\u00f6ns\u00b7ten", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "PIAT", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und n\u00f6thigte dich darauf mit in mein Hirtenhaus,", "tokens": ["Und", "n\u00f6\u00b7thig\u00b7te", "dich", "da\u00b7rauf", "mit", "in", "mein", "Hir\u00b7ten\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Da wollten wir mit Lust der Liebe Blumen pfl\u00fccken,", "tokens": ["Da", "woll\u00b7ten", "wir", "mit", "Lust", "der", "Lie\u00b7be", "Blu\u00b7men", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und bey dem Lesen uns sanft in die Wangen zwicken.", "tokens": ["Und", "bey", "dem", "Le\u00b7sen", "uns", "sanft", "in", "die", "Wan\u00b7gen", "zwi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Hier k\u00fc\u00dft sich Blum und Halm durch Zephyrs s\u00fcsse Triebe,", "tokens": ["Hier", "k\u00fc\u00dft", "sich", "Blum", "und", "Halm", "durch", "Ze\u00b7phyrs", "s\u00fcs\u00b7se", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NE", "KON", "NN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dies sollt ein Vorbild seyn von unsrer reinen Liebe,", "tokens": ["Dies", "sollt", "ein", "Vor\u00b7bild", "seyn", "von", "uns\u00b7rer", "rei\u00b7nen", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VAINF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Allein die Einsamkeit macht mich hier stum und bleich,", "tokens": ["Al\u00b7lein", "die", "Ein\u00b7sam\u00b7keit", "macht", "mich", "hier", "stum", "und", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "---+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Wo nicht mein Thyrsis ist da gilt mir alles gleich.", "tokens": ["Wo", "nicht", "mein", "Thyr\u00b7sis", "ist", "da", "gilt", "mir", "al\u00b7les", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "PPOSAT", "NN", "VAFIN", "ADV", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Thyrsis! k\u00f6nnt ich dich auf diesen sch\u00f6nen Auen,", "tokens": ["Mein", "Thyr\u00b7sis", "!", "k\u00f6nnt", "ich", "dich", "auf", "die\u00b7sen", "sch\u00f6\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "w