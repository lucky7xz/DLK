{"dta.poem.7262": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Lerche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du der Nacht gespornten W\u00e4chter nicht?", "tokens": ["H\u00f6rst", "du", "der", "Nacht", "ge\u00b7sporn\u00b7ten", "W\u00e4ch\u00b7ter", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein Schrei verzittert mit dem D\u00e4mmerlicht,", "tokens": ["Sein", "Schrei", "ver\u00b7zit\u00b7tert", "mit", "dem", "D\u00e4m\u00b7mer\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und schlummertrunken hebt aus Purpurdecken", "tokens": ["Und", "schlum\u00b7mer\u00b7trun\u00b7ken", "hebt", "aus", "Pur\u00b7pur\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Haupt die Sonne; in das Aetherbecken", "tokens": ["Ihr", "Haupt", "die", "Son\u00b7ne", ";", "in", "das", "A\u00b7e\u00b7ther\u00b7be\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.5": {"text": "Taucht sie die Stirn, man sieht es nicht genau,", "tokens": ["Taucht", "sie", "die", "Stirn", ",", "man", "sieht", "es", "nicht", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PIS", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ob Licht sie z\u00fcnde, oder trink' im Blau.", "tokens": ["Ob", "Licht", "sie", "z\u00fcn\u00b7de", ",", "o\u00b7der", "trink'", "im", "Blau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gl\u00fchrothe Pfeile zucken auf und nieder,", "tokens": ["Gl\u00fch\u00b7ro\u00b7the", "Pfei\u00b7le", "zu\u00b7cken", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und wecken Thaues Blitze, wenn im Flug", "tokens": ["Und", "we\u00b7cken", "Thau\u00b7es", "Blit\u00b7ze", ",", "wenn", "im", "Flug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Sie streifen durch der Haide braunen Zug.", "tokens": ["Sie", "strei\u00b7fen", "durch", "der", "Hai\u00b7de", "brau\u00b7nen", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da sch\u00fcttelt auch die Lerche ihr Gefieder,", "tokens": ["Da", "sch\u00fct\u00b7telt", "auch", "die", "Ler\u00b7che", "ihr", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Des Tages Herold seine Liverei;", "tokens": ["Des", "Ta\u00b7ges", "He\u00b7rold", "sei\u00b7ne", "Li\u00b7ve\u00b7rei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ihr K\u00f6pfchen streckt sie aus dem Ginster scheu,", "tokens": ["Ihr", "K\u00f6pf\u00b7chen", "streckt", "sie", "aus", "dem", "Gins\u00b7ter", "scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Blinzt nun mit diesem, nun mit jenem Aug';", "tokens": ["Blinzt", "nun", "mit", "die\u00b7sem", ",", "nun", "mit", "je\u00b7nem", "Aug'", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "$,", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Dann leise schwankt, es spaltet sich der Strauch,", "tokens": ["Dann", "lei\u00b7se", "schwankt", ",", "es", "spal\u00b7tet", "sich", "der", "Strauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und wirbelnd des Mandates erste Note", "tokens": ["Und", "wir\u00b7belnd", "des", "Man\u00b7da\u00b7tes", "ers\u00b7te", "No\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Schie\u00dft in das feuchte Blau des Tages Bote.", "tokens": ["Schie\u00dft", "in", "das", "feuch\u00b7te", "Blau", "des", "Ta\u00b7ges", "Bo\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u201eauf! auf! die junge F\u00fcrstin ist erwacht!", "tokens": ["\u201e", "auf", "!", "auf", "!", "die", "jun\u00b7ge", "F\u00fcrs\u00b7tin", "ist", "er\u00b7wacht", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "PTKVZ", "$.", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u201eschlaftrunkne K\u00e4mm'rer, habt des Amtes Acht;", "tokens": ["\u201e", "schlaf\u00b7trunk\u00b7ne", "K\u00e4m\u00b7m'\u00b7rer", ",", "habt", "des", "Am\u00b7tes", "Acht", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "VAFIN", "ART", "NN", "CARD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u201edu mit dem Saphirbecken Genziane,", "tokens": ["\u201e", "du", "mit", "dem", "Sa\u00b7phir\u00b7be\u00b7cken", "Gen\u00b7zi\u00b7a\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u201ezwergweide du mit deiner Seidenfahne,", "tokens": ["\u201e", "zwerg\u00b7wei\u00b7de", "du", "mit", "dei\u00b7ner", "Sei\u00b7den\u00b7fah\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u201edas Amt, das Amt, ihr Blumen allzumal,", "tokens": ["\u201e", "das", "Amt", ",", "das", "Amt", ",", "ihr", "Blu\u00b7men", "all\u00b7zu\u00b7mal", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u201edie F\u00fcrstin wacht, bald tritt sie in den Saal!\u201c", "tokens": ["\u201e", "die", "F\u00fcrs\u00b7tin", "wacht", ",", "bald", "tritt", "sie", "in", "den", "Saal", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da regen tausend Wimpern sich zugleich,", "tokens": ["Da", "re\u00b7gen", "tau\u00b7send", "Wim\u00b7pern", "sich", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "CARD", "NN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Masliebchen h\u00e4lt das klare Auge offen,", "tokens": ["Mas\u00b7lieb\u00b7chen", "h\u00e4lt", "das", "kla\u00b7re", "Au\u00b7ge", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wasserlilie sieht ein wenig bleich,", "tokens": ["Die", "Was\u00b7ser\u00b7li\u00b7lie", "sieht", "ein", "we\u00b7nig", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erschrocken, da\u00df im Bade sie betroffen;", "tokens": ["Er\u00b7schro\u00b7cken", ",", "da\u00df", "im", "Ba\u00b7de", "sie", "be\u00b7trof\u00b7fen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie steht der Zitterhalm versch\u00e4mt und zage!", "tokens": ["Wie", "steht", "der", "Zit\u00b7ter\u00b7halm", "ver\u00b7sch\u00e4mt", "und", "za\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die kleine Weide pudert sich geschwind", "tokens": ["Die", "klei\u00b7ne", "Wei\u00b7de", "pu\u00b7dert", "sich", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und reicht dem West ihr Seident\u00fcchlein lind,", "tokens": ["Und", "reicht", "dem", "West", "ihr", "Sei\u00b7den\u00b7t\u00fcch\u00b7lein", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df zu der Hoheit H\u00e4nden er es trage.", "tokens": ["Da\u00df", "zu", "der", "Ho\u00b7heit", "H\u00e4n\u00b7den", "er", "es", "tra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ehrf\u00fcrchtig beut den thauigen Pokal", "tokens": ["Ehr\u00b7f\u00fcrch\u00b7tig", "beut", "den", "thau\u00b7i\u00b7gen", "Po\u00b7kal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das Genzian, und nieder langt der Stral;", "tokens": ["Das", "Gen\u00b7zi\u00b7an", ",", "und", "nie\u00b7der", "langt", "der", "Stral", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PTKVZ", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Prinz von Gebl\u00fcte hat die erste St\u00e4tte", "tokens": ["Prinz", "von", "Ge\u00b7bl\u00fc\u00b7te", "hat", "die", "ers\u00b7te", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.12": {"text": "Er immer dienend an der F\u00fcrstin Bette.", "tokens": ["Er", "im\u00b7mer", "die\u00b7nend", "an", "der", "F\u00fcrs\u00b7tin", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Purpur lischt gemach im Rosenlicht,", "tokens": ["Der", "Pur\u00b7pur", "lischt", "ge\u00b7mach", "im", "Ro\u00b7sen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Horizont ein zuckend Leuchten bricht", "tokens": ["Am", "Ho\u00b7ri\u00b7zont", "ein", "zu\u00b7ckend", "Leuch\u00b7ten", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "VVPP", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Vorhangs Falten, und aufs neue singt", "tokens": ["Des", "Vor\u00b7hangs", "Fal\u00b7ten", ",", "und", "aufs", "neu\u00b7e", "singt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "KON", "APPRART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Lerche, da\u00df es durch den Aether klingt:", "tokens": ["Die", "Ler\u00b7che", ",", "da\u00df", "es", "durch", "den", "A\u00b7e\u00b7ther", "klingt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "\u201edie F\u00fcrstin k\u00f6mmt, die F\u00fcrstin steht am Thor!", "tokens": ["\u201e", "die", "F\u00fcrs\u00b7tin", "k\u00f6mmt", ",", "die", "F\u00fcrs\u00b7tin", "steht", "am", "Thor", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u201efrischauf ihr Musikanten in den Hallen,", "tokens": ["\u201e", "frisc\u00b7hauf", "ihr", "Mu\u00b7si\u00b7kan\u00b7ten", "in", "den", "Hal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u201ela\u00dft euer zartes Saitenspiel erschallen,", "tokens": ["\u201e", "la\u00dft", "eu\u00b7er", "zar\u00b7tes", "Sai\u00b7ten\u00b7spiel", "er\u00b7schal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201eund, florbefl\u00fcgelt Volk, heb' an den Chor,", "tokens": ["\u201e", "und", ",", "flor\u00b7be\u00b7fl\u00fc\u00b7gelt", "Volk", ",", "heb'", "an", "den", "Chor", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "$,", "VVFIN", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u201edie F\u00fcrstin kommt, die F\u00fcrstin steht am Thor!\u201c", "tokens": ["\u201e", "die", "F\u00fcrs\u00b7tin", "kommt", ",", "die", "F\u00fcrs\u00b7tin", "steht", "am", "Thor", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da krimmelt, wimmelt es im Haidgezweige,", "tokens": ["Da", "krim\u00b7melt", ",", "wim\u00b7melt", "es", "im", "Haid\u00b7ge\u00b7zwei\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Grille dreht geschwind das Beinchen um,", "tokens": ["Die", "Gril\u00b7le", "dreht", "ge\u00b7schwind", "das", "Be\u00b7in\u00b7chen", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Streicht an des Thaues Kolophonium,", "tokens": ["Streicht", "an", "des", "Thau\u00b7es", "Ko\u00b7lo\u00b7pho\u00b7ni\u00b7um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und spielt so sch\u00e4ferlich di