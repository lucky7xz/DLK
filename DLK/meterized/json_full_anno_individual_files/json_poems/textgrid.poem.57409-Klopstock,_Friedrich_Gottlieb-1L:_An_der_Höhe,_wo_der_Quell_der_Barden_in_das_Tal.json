{"textgrid.poem.57409": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: An der H\u00f6he, wo der Quell der Barden in das Tal", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An der H\u00f6he, wo der Quell der Barden in das Tal", "tokens": ["An", "der", "H\u00f6\u00b7he", ",", "wo", "der", "Quell", "der", "Bar\u00b7den", "in", "das", "Tal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Sein fliegendes Get\u00f6ne, mit Silber bew\u00f6lkt,", "tokens": ["Sein", "flie\u00b7gen\u00b7des", "Ge\u00b7t\u00f6\u00b7ne", ",", "mit", "Sil\u00b7ber", "be\u00b7w\u00f6lkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "St\u00fcrzet, da erblickt' ich, zeug' es, Hain!", "tokens": ["St\u00fcr\u00b7zet", ",", "da", "er\u00b7blickt'", "ich", ",", "zeug'", "es", ",", "Hain", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die G\u00f6ttin! sie kam zu dem Sterblichen herab!", "tokens": ["Die", "G\u00f6t\u00b7tin", "!", "sie", "kam", "zu", "dem", "Sterb\u00b7li\u00b7chen", "her\u00b7ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Und mit Hoheit in der Miene stand sie! und ich sah", "tokens": ["Und", "mit", "Ho\u00b7heit", "in", "der", "Mie\u00b7ne", "stand", "sie", "!", "und", "ich", "sah"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "$.", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Die Geister um sie her, die, den Liedern entlockt,", "tokens": ["Die", "Geis\u00b7ter", "um", "sie", "her", ",", "die", ",", "den", "Lie\u00b7dern", "ent\u00b7lockt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$,", "PRELS", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "T\u00e4uschen, ihr Gebild. Die Wurdi's Dolch", "tokens": ["T\u00e4u\u00b7schen", ",", "ihr", "Ge\u00b7bild", ".", "Die", "Wur\u00b7di's", "Dolch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Unschuldige traf, die begleiteten sie fern,", "tokens": ["Un\u00b7schul\u00b7di\u00b7ge", "traf", ",", "die", "be\u00b7glei\u00b7te\u00b7ten", "sie", "fern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Wie in D\u00e4mmrung; und die Skulda's m\u00e4chtigerer Stab", "tokens": ["Wie", "in", "D\u00e4mm\u00b7rung", ";", "und", "die", "Skulda's", "m\u00e4ch\u00b7ti\u00b7ge\u00b7rer", "Stab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "$.", "KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-+-++-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Errettete, die schwebten umher in Triumph,", "tokens": ["Er\u00b7ret\u00b7te\u00b7te", ",", "die", "schweb\u00b7ten", "um\u00b7her", "in", "Tri\u00b7umph", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "VVFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schimmernd, um die G\u00f6ttin, hatten stolz", "tokens": ["Schim\u00b7mernd", ",", "um", "die", "G\u00f6t\u00b7tin", ",", "hat\u00b7ten", "stolz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "$,", "KOUI", "ART", "NN", "$,", "VAFIN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mit Laube der Eiche die Schl\u00e4fe sich bekr\u00e4nzt!", "tokens": ["Mit", "Lau\u00b7be", "der", "Ei\u00b7che", "die", "Schl\u00e4\u00b7fe", "sich", "be\u00b7kr\u00e4nzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Den Gedanken, die Empfindung, treffend, und mit Kraft,", "tokens": ["Den", "Ge\u00b7dan\u00b7ken", ",", "die", "Emp\u00b7fin\u00b7dung", ",", "tref\u00b7fend", ",", "und", "mit", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ADJD", "$,", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Mit Wendungen der K\u00fchnheit, zu sagen! das ist,", "tokens": ["Mit", "Wen\u00b7dun\u00b7gen", "der", "K\u00fchn\u00b7heit", ",", "zu", "sa\u00b7gen", "!", "das", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PTKZU", "VVINF", "$.", "PDS", "VAFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Sprache des Thuiskon, G\u00f6ttin, dir,", "tokens": ["Spra\u00b7che", "des", "Thu\u00b7is\u00b7kon", ",", "G\u00f6t\u00b7tin", ",", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "$,", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie unseren Helden Eroberung, ein Spiel!", "tokens": ["Wie", "un\u00b7se\u00b7ren", "Hel\u00b7den", "Er\u00b7o\u00b7be\u00b7rung", ",", "ein", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "O Begeistrung! Sie erhebt sich! Feurigeren Blicks", "tokens": ["O", "Be\u00b7geis\u00b7trung", "!", "Sie", "er\u00b7hebt", "sich", "!", "Feu\u00b7ri\u00b7ge\u00b7ren", "Blicks"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "PRF", "$.", "NN", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Ergie\u00dfet sich ihr Auge, die Seel' in der Glut!", "tokens": ["Er\u00b7gie\u00b7\u00dfet", "sich", "ihr", "Au\u00b7ge", ",", "die", "Seel'", "in", "der", "Glut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Str\u00f6me! denn du schonest des umsonst,", "tokens": ["Str\u00f6\u00b7me", "!", "denn", "du", "scho\u00b7nest", "des", "um\u00b7sonst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PPER", "VVFIN", "ART", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der, leer des Gef\u00fchls, den Gedanken nicht erreicht!", "tokens": ["Der", ",", "leer", "des", "Ge\u00b7f\u00fchls", ",", "den", "Ge\u00b7dan\u00b7ken", "nicht", "er\u00b7reicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "ART", "NN", "$,", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Wie sie herschwebt an des Quells Fall! M\u00e4chtiges Get\u00f6n,", "tokens": ["Wie", "sie", "her\u00b7schwebt", "an", "des", "Quells", "Fall", "!", "M\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Wie Rauschen in den N\u00e4chten des Walds ist ihr Schwung!", "tokens": ["Wie", "Rau\u00b7schen", "in", "den", "N\u00e4ch\u00b7ten", "des", "Walds", "ist", "ihr", "Schwung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Drau\u00dfen im Gefilde braust der Sturm!", "tokens": ["Drau\u00b7\u00dfen", "im", "Ge\u00b7fil\u00b7de", "braust", "der", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gern h\u00f6ret der Wandrer das Rauschen in dem Wald!", "tokens": ["Gern", "h\u00f6\u00b7ret", "der", "Wand\u00b7rer", "das", "Rau\u00b7schen", "in", "dem", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Wie sie schwebet an der Quelle! Sanfteres Get\u00f6n,", "tokens": ["Wie", "sie", "schwe\u00b7bet", "an", "der", "Quel\u00b7le", "!", "Sanf\u00b7te\u00b7res", "Ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Wie Wehen in dem tieferen Wald ist ihr Schwung.", "tokens": ["Wie", "We\u00b7hen", "in", "dem", "tie\u00b7fe\u00b7ren", "Wald", "ist", "ihr", "Schwung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Drau\u00dfen im Gefilde braust der Sturm!", "tokens": ["Drau\u00b7\u00dfen", "im", "Ge\u00b7fil\u00b7de", "braust", "der", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gern h\u00f6ret im Walde der Wanderer das Wehn.", "tokens": ["Gern", "h\u00f6\u00b7ret", "im", "Wal\u00b7de", "der", "Wan\u00b7de\u00b7rer", "das", "Wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "So erscholl mir's von der Telyn Saite, wie im Flug.", "tokens": ["So", "er\u00b7scholl", "mir's", "von", "der", "Te\u00b7lyn", "Sai\u00b7te", ",", "wie", "im", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NE", "APPR", "ART", "NN", "NN", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Mich dauchte, da\u00df die G\u00f6ttin mit L\u00e4cheln auf mich", "tokens": ["Mich", "dauch\u00b7te", ",", "da\u00df", "die", "G\u00f6t\u00b7tin", "mit", "L\u00e4\u00b7cheln", "auf", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "NN", "APPR", "PPER"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Blickte: da durchstr\u00f6mt' es all mein Blut", "tokens": ["Blick\u00b7te", ":", "da", "durch\u00b7str\u00f6mt'", "es", "all", "mein", "Blut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mit Feuer, und R\u00f6te, wie jugendlicher Tanz,", "tokens": ["Mit", "Feu\u00b7er", ",", "und", "R\u00f6\u00b7te", ",", "wie", "ju\u00b7gend\u00b7li\u00b7cher", "Tanz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "NN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "In dem Fr\u00fchlinge getanzt gl\u00fchte mir herauf", "tokens": ["In", "dem", "Fr\u00fch\u00b7lin\u00b7ge", "ge\u00b7tanzt", "gl\u00fch\u00b7te", "mir", "her\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "VVFIN", "PPER", "PTKVZ"], "meter": "+-++--++-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Die Wange! Ihr Begleiter! ihr Geister! so rief", "tokens": ["Die", "Wan\u00b7ge", "!", "Ihr", "Be\u00b7glei\u00b7ter", "!", "ihr", "Geis\u00b7ter", "!", "so", "rief"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Eiliger ich aus, ihr saht den Blick", "tokens": ["Ei\u00b7li\u00b7ger", "ich", "aus", ",", "ihr", "saht", "den", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der G\u00f6ttin, sie l\u00e4chelt! Ihr Genien, ihr sahts!", "tokens": ["Der", "G\u00f6t\u00b7tin", ",", "sie", "l\u00e4\u00b7chelt", "!", "Ihr", "Ge\u00b7ni\u00b7en", ",", "ihr", "sahts", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$.", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "O des Zaubers, den sie jetzo zaubert! Er gebeut;", "tokens": ["O", "des", "Zau\u00b7bers", ",", "den", "sie", "jet\u00b7zo", "zau\u00b7bert", "!", "Er", "ge\u00b7beut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Die Geister der Ges\u00e4nge gesungen durch mich", "tokens": ["Die", "Geis\u00b7ter", "der", "Ge\u00b7s\u00e4n\u00b7ge", "ge\u00b7sun\u00b7gen", "durch", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "APPR", "PPER"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kommen, ihr Gebild, und haben stolz", "tokens": ["Kom\u00b7men", ",", "ihr", "Ge\u00b7bild", ",", "und", "ha\u00b7ben", "stolz"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "KON", "VAFIN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mit heiligem Laube die Schl\u00e4fe sich bekr\u00e4nzt,", "tokens": ["Mit", "hei\u00b7li\u00b7gem", "Lau\u00b7be", "die", "Schl\u00e4\u00b7fe", "sich", "be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Mit dem j\u00fcngsten aus den Hainen! Hebe doch der Dolch", "tokens": ["Mit", "dem", "j\u00fcng\u00b7sten", "aus", "den", "Hai\u00b7nen", "!", "He\u00b7be", "doch", "der", "Dolch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "APPR", "ART", "NN", "$.", "NE", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Der Norne sich! Er fehlt sie! Die G\u00f6ttin hat sie", "tokens": ["Der", "Nor\u00b7ne", "sich", "!", "Er", "fehlt", "sie", "!", "Die", "G\u00f6t\u00b7tin", "hat", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "$.", "PPER", "VVFIN", "PPER", "$.", "ART", "NN", "VAFIN", "PPER"], "meter": "-+-+-+--+-++", "me