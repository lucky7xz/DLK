{"textgrid.poem.57836": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der sch\u00f6ne Fisch", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe ein Wasser blinken,", "tokens": ["Ich", "se\u00b7he", "ein", "Was\u00b7ser", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Wasser das ist frisch;", "tokens": ["Das", "Was\u00b7ser", "das", "ist", "frisch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sehe etwas schimmern,", "tokens": ["Ich", "se\u00b7he", "et\u00b7was", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist f\u00fcrwahr kein Fisch.", "tokens": ["Das", "ist", "f\u00fcr\u00b7wahr", "kein", "Fisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Fisch hat keine Haare", "tokens": ["Ein", "Fisch", "hat", "kei\u00b7ne", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und nicht zwei Arme rund;", "tokens": ["Und", "nicht", "zwei", "Ar\u00b7me", "rund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat keine blauen Augen", "tokens": ["Hat", "kei\u00b7ne", "blau\u00b7en", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und keinen roten Mund.", "tokens": ["Und", "kei\u00b7nen", "ro\u00b7ten", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich will das Fischlein fangen", "tokens": ["Ich", "will", "das", "Fisc\u00b7hlein", "fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit einem Haselstock;", "tokens": ["Mit", "ei\u00b7nem", "Ha\u00b7sel\u00b7stock", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was seh' ich in dem Grase,", "tokens": ["Was", "seh'", "ich", "in", "dem", "Gra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Hemdlein und ein Rock?", "tokens": ["Ein", "Hemd\u00b7lein", "und", "ein", "Rock", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Hemdlein sollst du haben,", "tokens": ["Das", "Hemd\u00b7lein", "sollst", "du", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Rock behalt ich hier;", "tokens": ["Den", "Rock", "be\u00b7halt", "ich", "hier", ";"], "token_info"