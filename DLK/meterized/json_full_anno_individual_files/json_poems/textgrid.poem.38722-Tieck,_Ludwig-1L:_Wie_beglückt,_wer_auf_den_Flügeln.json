{"textgrid.poem.38722": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie begl\u00fcckt, wer auf den Fl\u00fcgeln", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie begl\u00fcckt, wer auf den Fl\u00fcgeln", "tokens": ["Wie", "be\u00b7gl\u00fcckt", ",", "wer", "auf", "den", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "$,", "PWS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seiner Phantasieen wandelt,", "tokens": ["Sei\u00b7ner", "Phan\u00b7ta\u00b7si\u00b7e\u00b7en", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Erde, Wasser, Luft und Himmel", "tokens": ["Er\u00b7de", ",", "Was\u00b7ser", ",", "Luft", "und", "Him\u00b7mel"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht er in dem hohen Gange.", "tokens": ["Sieht", "er", "in", "dem", "ho\u00b7hen", "Gan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aufgeschlossen sind die Reiche", "tokens": ["Auf\u00b7ge\u00b7schlos\u00b7sen", "sind", "die", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo das Gold, die Erze wachsen,", "tokens": ["Wo", "das", "Gold", ",", "die", "Er\u00b7ze", "wach\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo Demant, Rubinen keimen,", "tokens": ["Wo", "De\u00b7mant", ",", "Ru\u00b7bi\u00b7nen", "kei\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ruhig sprie\u00dfen in den Schaalen.", "tokens": ["Ru\u00b7hig", "sprie\u00b7\u00dfen", "in", "den", "Schaa\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Also sieht er auch der Herzen", "tokens": ["Al\u00b7so", "sieht", "er", "auch", "der", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Geister, welche Rathschlag halten,", "tokens": ["Geis\u00b7ter", ",", "wel\u00b7che", "Rath\u00b7schlag", "hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "In der Morgen-Abendr\u00f6the", "tokens": ["In", "der", "Mor\u00b7gen\u00b7A\u00b7bendr\u00f6\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Lieblich bl\u00fchende Gestalten.", "tokens": ["Lieb\u00b7lich", "bl\u00fc\u00b7hen\u00b7de", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Phantasie im goldnen Meere", "tokens": ["Phan\u00b7ta\u00b7sie", "im", "gold\u00b7nen", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wirft, wo sie nur kann, den Anker,", "tokens": ["Wirft", ",", "wo", "sie", "nur", "kann", ",", "den", "An\u00b7ker", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ADV", "VMFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und aus gr\u00fcnen Wogen steigen", "tokens": ["Und", "aus", "gr\u00fc\u00b7nen", "Wo\u00b7gen", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Blumenvolle Wunder-Lande.", "tokens": ["Blu\u00b7men\u00b7vol\u00b7le", "Wun\u00b7der\u00b7Lan\u00b7de", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Nirgend ruht sie, wer ihr folget", "tokens": ["Nir\u00b7gend", "ruht", "sie", ",", "wer", "ihr", "fol\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "An dem sch\u00f6nen Zauberbande,", "tokens": ["An", "dem", "sch\u00f6\u00b7nen", "Zau\u00b7berb\u00b7an\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Steigt in 's Innre, schaut die Kr\u00e4fte", "tokens": ["Steigt", "in", "'s", "Inn\u00b7re", ",", "schaut", "die", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Der regierenden Gewalten:", "tokens": ["Der", "re\u00b7gie\u00b7ren\u00b7den", "Ge\u00b7wal\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wie aus Wasser alle Welten", "tokens": ["Wie", "aus", "Was\u00b7ser", "al\u00b7le", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Hat der ew'ge Trieb erschaffen,", "tokens": ["Hat", "der", "ew'\u00b7ge", "Trieb", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Wie das Feuer ihre Wurzel,", "tokens": ["Wie", "das", "Feu\u00b7er", "ih\u00b7re", "Wur\u00b7zel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Die in ihren Kindern pranget;", "tokens": ["Die", "in", "ih\u00b7ren", "Kin\u00b7dern", "pran\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Und das Licht die h\u00f6chste Bl\u00fchte,", "tokens": ["Und", "das", "Licht", "die", "h\u00f6chs\u00b7te", "Bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "In dem Menschen Lieb' ihr Nahme,", "tokens": ["In", "dem", "Men\u00b7schen", "Lieb'", "ihr", "Nah\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wie sich alles dahin st\u00fcrzet,", "tokens": ["Wie", "sich", "al\u00b7les", "da\u00b7hin", "st\u00fcr\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIS", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Eilt im br\u00fcnstigen Verlangen.", "tokens": ["Eilt", "im", "br\u00fcns\u00b7ti\u00b7gen", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Immer will die Erde aufw\u00e4rts", "tokens": ["Im\u00b7mer", "will", "die", "Er\u00b7de", "auf\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Liebend an der Sonne hangen,", "tokens": ["Lie\u00b7bend", "an", "der", "Son\u00b7ne", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Und das Feuer h\u00e4lt sie innen", "tokens": ["Und", "das", "Feu\u00b7er", "h\u00e4lt", "sie", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "In sich selber eingefangen;", "tokens": ["In", "sich", "sel\u00b7ber", "ein\u00b7ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "So erbiert sie aus den Sehnen", "tokens": ["So", "er\u00b7biert", "sie", "aus", "den", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Liebelechzend reine Wasser,", "tokens": ["Lie\u00b7be\u00b7lech\u00b7zend", "rei\u00b7ne", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Diese sind die Mutter-Thr\u00e4nen,", "tokens": ["Die\u00b7se", "sind", "die", "Mut\u00b7ter\u00b7Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Die ihr flie\u00dfen von den Wangen:", "tokens": ["Die", "ihr", "flie\u00b7\u00dfen", "von", "den", "Wan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Und sie l\u00e4\u00dft die Blumen gr\u00fcnen,", "tokens": ["Und", "sie", "l\u00e4\u00dft", "die", "Blu\u00b7men", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Keimen l\u00e4\u00dft sie sch\u00f6ne Pflanzen,", "tokens": ["Kei\u00b7men", "l\u00e4\u00dft", "sie", "sch\u00f6\u00b7ne", "Pflan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Berge, W\u00e4lder, Flur sind trunken", "tokens": ["Ber\u00b7ge", ",", "W\u00e4l\u00b7der", ",", "Flur", "sind", "trun\u00b7ken"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "In der Wonn', im Liebes-Glanze.", "tokens": ["In", "der", "Wonn'", ",", "im", "Lie\u00b7bes\u00b7Glanze", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.41": {"text": "D\u00fcrstend lechzt der Menschenbusen,", "tokens": ["D\u00fcrs\u00b7tend", "lechzt", "der", "Men\u00b7schen\u00b7bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Seele will hinauf gelangen,", "tokens": ["See\u00b7le", "will", "hin\u00b7auf", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Und in tiefster Inbrunst leise", "tokens": ["Und", "in", "tiefs\u00b7ter", "In\u00b7brunst", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Wird des Schaffens Trieb empfangen:", "tokens": ["Wird", "des", "Schaf\u00b7fens", "Trieb", "emp\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Denn das Feuer f\u00e4ngt die Liebe,", "tokens": ["Denn", "das", "Feu\u00b7er", "f\u00e4ngt", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Und nun kann sie nicht von dannen,", "tokens": ["Und", "nun", "kann", "sie", "nicht", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKNEG", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Worauf manche tiefe Meister", "tokens": ["Wo\u00b7rauf", "man\u00b7che", "tie\u00b7fe", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PIAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.48": {"text": "Wissenschaft und Kunst ersannen:", "tokens": ["Wis\u00b7sen\u00b7schaft", "und", "Kunst", "er\u00b7san\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Und am herrlichsten, am freisten", "tokens": ["Und", "am", "herr\u00b7lichs\u00b7ten", ",", "am", "freis\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "$,", "APPRART", "ADJA"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.50": {"text": "Die kristallnen Brunnen sprangen,", "tokens": ["Die", "kris\u00b7tall\u00b7nen", "Brun\u00b7nen", "spran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.51": {"text": "Die in Reimen, die in T\u00f6nen", "tokens": ["Die", "in", "Rei\u00b7men", ",", "die", "in", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Dichtender Begeistrung klangen.", "tokens": ["Dich\u00b7ten\u00b7der", "Be\u00b7geis\u00b7trung", "klan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Wieder sind es Mutter-Thr\u00e4nen,", "tokens": ["Wie\u00b7der", "sind", "es", "Mut\u00b7ter\u00b7Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Da\u00df die Kinder ihr entschwanden,", "tokens": ["Da\u00df", "die", "Kin\u00b7der", "ihr", "ent\u00b7schwan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Da\u00df der lieben s\u00fc\u00dfes Leben", "tokens": ["Da\u00df", "der", "lie\u00b7ben", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Um sie in den Steinen starret:", "tokens": ["Um", "sie", "in", "den", "Stei\u00b7nen", "star\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Aber drinn steht man das Herze,", "tokens": ["A\u00b7ber", "drinn", "steht", "man", "das", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PDS", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.58": {"text": "Das die ganze Welt erlebet,", "tokens": ["Das", "die", "gan\u00b7ze", "Welt", "er\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Und der Liebesgeist die Fl\u00fcgel",