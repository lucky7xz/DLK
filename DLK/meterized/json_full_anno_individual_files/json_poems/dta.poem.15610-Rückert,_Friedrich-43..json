{"dta.poem.15610": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Grund, auf welchem ruht dein Daseyn, umzuw\u00fchlen,", "tokens": ["Den", "Grund", ",", "auf", "wel\u00b7chem", "ruht", "dein", "Da\u00b7seyn", ",", "um\u00b7zu\u00b7w\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PWAT", "VVFIN", "PPOSAT", "NN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kann dir nicht helfen um dich seyender zu f\u00fchlen.", "tokens": ["Kann", "dir", "nicht", "hel\u00b7fen", "um", "dich", "sey\u00b7en\u00b7der", "zu", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "KOUI", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+---+-+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Vielmehr am seyendsten hast du dich dann gef\u00fchlt,", "tokens": ["Viel\u00b7mehr", "am", "sey\u00b7end\u00b7sten", "hast", "du", "dich", "dann", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du am wenigsten dich selber umgew\u00fchlt.", "tokens": ["Wenn", "du", "am", "we\u00b7nigs\u00b7ten", "dich", "sel\u00b7ber", "um\u00b7ge\u00b7w\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Zwar nicht als rieth' ich dir, gedankenlos zu starren,", "tokens": ["Zwar", "nicht", "als", "ri\u00b7e\u00b7th'", "ich", "dir", ",", "ge\u00b7dan\u00b7ken\u00b7los", "zu", "star\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "KOKOM", "VVFIN", "PPER", "PPER", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Doch sicher im Gef\u00fchl des Lebens zu verharren:", "tokens": ["Doch", "si\u00b7cher", "im", "Ge\u00b7f\u00fchl", "des", "Le\u00b7bens", "zu", "ver\u00b7har\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du bist so wie du bist, und freust dich so zu seyn", "tokens": ["Du", "bist", "so", "wie", "du", "bist", ",", "und", "freust", "dich", "so", "zu", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "KOKOM", "PPER", "VAFIN", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKZU", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so zu bleiben, weil du seyn kannst so allein.", "tokens": ["Und", "so", "zu", "blei\u00b7ben", ",", "weil", "du", "seyn", "kannst", "so", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "VAINF", "VMFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}