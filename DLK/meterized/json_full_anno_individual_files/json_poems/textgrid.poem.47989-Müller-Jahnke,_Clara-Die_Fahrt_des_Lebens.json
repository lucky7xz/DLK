an", "tokens": ["wird", "die", "Nacht", "den", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "finstern Blicks umschweben,", "tokens": ["fins\u00b7tern", "Blicks", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "wird des Meeres glatte Bahn", "tokens": ["wird", "des", "Mee\u00b7res", "glat\u00b7te", "Bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "wogend sich erheben. \u2013 \u2013 \u2013", "tokens": ["wo\u00b7gend", "sich", "er\u00b7he\u00b7ben", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADJD", "PRF", "VVINF", "$.", "$(", "$(", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Dunkel wird's und schauerlich,", "tokens": ["Dun\u00b7kel", "wird's", "und", "schau\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "nun die Farben blassen,", "tokens": ["nun", "die", "Far\u00b7ben", "blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.27": {"text": "nun der Jugend Tr\u00e4ume dich,", "tokens": ["nun", "der", "Ju\u00b7gend", "Tr\u00e4u\u00b7me", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "treulos dich verlassen.", "tokens": ["treu\u00b7los", "dich", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.29": {"text": "Kalt und einsam stehst du noch,", "tokens": ["Kalt", "und", "ein\u00b7sam", "stehst", "du", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "ob die Winde st\u00fcrmen,", "tokens": ["ob", "die", "Win\u00b7de", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.31": {"text": "ob die Wogen bergehoch", "tokens": ["ob", "die", "Wo\u00b7gen", "ber\u00b7ge\u00b7hoch"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "um dein Schiff sich t\u00fcrmen.", "tokens": ["um", "dein", "Schiff", "sich", "t\u00fcr\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.33": {"text": "Traumhaft wird dir manchmal sein,", "tokens": ["Traum\u00b7haft", "wird", "dir", "manch\u00b7mal", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "ob aus Meerestiefen,", "tokens": ["ob", "aus", "Mee\u00b7res\u00b7tie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.35": {"text": "matt begl\u00e4nzt von Sternenschein,", "tokens": ["matt", "be\u00b7gl\u00e4nzt", "von", "Ster\u00b7nen\u00b7schein", ","], "token_info": ["word", 