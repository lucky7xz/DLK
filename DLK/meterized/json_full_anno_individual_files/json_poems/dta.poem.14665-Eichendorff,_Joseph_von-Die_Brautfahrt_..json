{"dta.poem.14665": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Brautfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und er sinkt zu ihr vor Schmerzen,", "tokens": ["Und", "er", "sinkt", "zu", "ihr", "vor", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam in dem fremden Thal,", "tokens": ["Ein\u00b7sam", "in", "dem", "frem\u00b7den", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Thr\u00e4nen aus dem wilden Herzen", "tokens": ["Thr\u00e4\u00b7nen", "aus", "dem", "wil\u00b7den", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brechen da zum Erstenmal,", "tokens": ["Bre\u00b7chen", "da", "zum", "Ers\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und vor diesem Todesbilde", "tokens": ["Und", "vor", "die\u00b7sem", "To\u00b7des\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird die ganze Seele milde.", "tokens": ["Wird", "die", "gan\u00b7ze", "See\u00b7le", "mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["