{"textgrid.poem.40419": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Wanderlied", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlauf, die Luft geht frisch und rein,", "tokens": ["Wohl\u00b7auf", ",", "die", "Luft", "geht", "frisch", "und", "rein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer lange sitzt, mu\u00df rosten;", "tokens": ["Wer", "lan\u00b7ge", "sitzt", ",", "mu\u00df", "ros\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den allersonnigsten Sonnenschein", "tokens": ["Den", "al\u00b7ler\u00b7son\u00b7nigs\u00b7ten", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "L\u00e4\u00dft uns der Himmel kosten.", "tokens": ["L\u00e4\u00dft", "uns", "der", "Him\u00b7mel", "kos\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt reicht mir Stab und Ordenskleid", "tokens": ["Jetzt", "reicht", "mir", "Stab", "und", "Or\u00b7dens\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der fahrenden Scholaren,", "tokens": ["Der", "fah\u00b7ren\u00b7den", "Scho\u00b7la\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich will zu guter Sommerzeit", "tokens": ["Ich", "will", "zu", "gu\u00b7ter", "Som\u00b7mer\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ins Land der Franken fahren!", "tokens": ["Ins", "Land", "der", "Fran\u00b7ken", "fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Wald steht gr\u00fcn, die Jagd geht gut,", "tokens": ["Der", "Wald", "steht", "gr\u00fcn", ",", "die", "Jagd", "geht", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwer ist das Korn geraten;", "tokens": ["Schwer", "ist", "das", "Korn", "ge\u00b7ra\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie k\u00f6nnen auf des Maines Flut", "tokens": ["Sie", "k\u00f6n\u00b7nen", "auf", "des", "Mai\u00b7nes", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schiffe kaum verladen.", "tokens": ["Die", "Schif\u00b7fe", "kaum", "ver\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bald hebt sich auch das Herbsten an,", "tokens": ["Bald", "hebt", "sich", "auch", "das", "Herbs\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Kelter harrt des Weines;", "tokens": ["Die", "Kel\u00b7ter", "harrt", "des", "Wei\u00b7nes", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Winzer Schutzherr Kilian", "tokens": ["Der", "Win\u00b7zer", "Schutz\u00b7herr", "Ki\u00b7li\u00b7an"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Beschert uns etwas Feines.", "tokens": ["Be\u00b7schert", "uns", "et\u00b7was", "Fei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wallfahrer ziehen durch das Tal", "tokens": ["Wall\u00b7fah\u00b7rer", "zie\u00b7hen", "durch", "das", "Tal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit fliegenden Standarten,", "tokens": ["Mit", "flie\u00b7gen\u00b7den", "Stand\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Hell gr\u00fc\u00dft ihr doppelter Choral", "tokens": ["Hell", "gr\u00fc\u00dft", "ihr", "dop\u00b7pel\u00b7ter", "Cho\u00b7ral"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den weiten Gottesgarten.", "tokens": ["Den", "wei\u00b7ten", "Got\u00b7tes\u00b7gar\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie gerne w\u00e4r' ich mitgewallt,", "tokens": ["Wie", "ger\u00b7ne", "w\u00e4r'", "ich", "mit\u00b7ge\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Pfarr' wollt mich nicht haben!", "tokens": ["Ihr", "Pfarr'", "wollt", "mich", "nicht", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So mu\u00df ich seitw\u00e4rts durch den Wald", "tokens": ["So", "mu\u00df", "ich", "seit\u00b7w\u00e4rts", "durch", "den", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als r\u00e4udig Sch\u00e4flein traben.", "tokens": ["Als", "r\u00e4u\u00b7dig", "Sch\u00e4f\u00b7lein", "tra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zum heiligen Veit von Staffelstein", "tokens": ["Zum", "hei\u00b7li\u00b7gen", "Veit", "von", "Staf\u00b7fel\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Komm' ich emporgestiegen", "tokens": ["Komm'", "ich", "em\u00b7por\u00b7ge\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und seh' die Lande um den Main", "tokens": ["Und", "seh'", "die", "Lan\u00b7de", "um", "den", "Main"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meinen F\u00fc\u00dfen liegen:", "tokens": ["Zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von Bamberg bis zum Grabfeldgau", "tokens": ["Von", "Bam\u00b7berg", "bis", "zum", "Grab\u00b7feld\u00b7gau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umrahmen Berg und H\u00fcgel", "tokens": ["Um\u00b7rah\u00b7men", "Berg", "und", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die breite, stromdurchgl\u00e4nzte Au \u2013", "tokens": ["Die", "brei\u00b7te", ",", "strom\u00b7durch\u00b7gl\u00e4nz\u00b7te", "Au", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich wollt', mir w\u00fcchsen Fl\u00fcgel.", "tokens": ["Ich", "wollt'", ",", "mir", "w\u00fcch\u00b7sen", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Einsiedelmann ist nicht zu Haus,", "tokens": ["Ein\u00b7sie\u00b7del\u00b7mann", "ist", "nicht", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dieweil es Zeit zu m\u00e4hen;", "tokens": ["Die\u00b7weil", "es", "Zeit", "zu", "m\u00e4\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh' ihn an der Halde draus", "tokens": ["Ich", "seh'", "ihn", "an", "der", "Hal\u00b7de", "draus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei einer Schnitt'rin stehen.", "tokens": ["Bei", "ei\u00b7ner", "Schnitt'\u00b7rin", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verfahrner Sch\u00fcler Sto\u00dfgebet", "tokens": ["Ver\u00b7fahr\u00b7ner", "Sch\u00fc\u00b7ler", "Sto\u00df\u00b7ge\u00b7bet"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hei\u00dft: \u00bbHerr, gib uns zu trinken!\u00ab", "tokens": ["Hei\u00dft", ":", "\u00bb", "Herr", ",", "gib", "uns", "zu", "trin\u00b7ken", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$.", "$(", "NN", "$,", "VVIMP", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch wer bei sch\u00f6ner Schnitt'rin steht,", "tokens": ["Doch", "wer", "bei", "sch\u00f6\u00b7ner", "Schnitt'\u00b7rin", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem mag man lange winken.", "tokens": ["Dem", "mag", "man", "lan\u00b7ge", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Einsiedel, das war mi\u00dfgetan,", "tokens": ["Ein\u00b7sie\u00b7del", ",", "das", "war", "mi\u00df\u00b7ge\u00b7tan", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Da\u00df du dich hubst von hinnen!", "tokens": ["Da\u00df", "du", "dich", "hubst", "von", "hin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es liegt, ich seh's dem Keller an,", "tokens": ["Es", "liegt", ",", "ich", "seh's", "dem", "Kel\u00b7ler", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein guter Jahrgang drinnen.", "tokens": ["Ein", "gu\u00b7ter", "Jahr\u00b7gang", "drin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hoiho! die Pforten brech' ich ein", "tokens": ["Hoi\u00b7ho", "!", "die", "Pfor\u00b7ten", "brech'", "ich", "ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "NN", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und trinke, was ich finde ...", "tokens": ["Und", "trin\u00b7ke", ",", "was", "ich", "fin\u00b7de", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du heiliger Veit von Staffelstein,", "tokens": ["Du", "hei\u00b7li\u00b7ger", "Veit", "von", "Staf\u00b7fel\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Verzeih' mir Durst und S\u00fcnde!", "tokens": ["Ver\u00b7zeih'", "mir", "Durst", "und", "S\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wohlauf, die Luft geht frisch und rein,", "tokens": ["Wohl\u00b7auf", ",", "die", "Luft", "geht", "frisch", "und", "rein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer lange sitzt, mu\u00df rosten;", "tokens": ["Wer", "lan\u00b7ge", "sitzt", ",", "mu\u00df", "ros\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den allersonnigsten Sonnenschein", "tokens": ["Den", "al\u00b7ler\u00b7son\u00b7nigs\u00b7ten", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "L\u00e4\u00dft uns der Himmel kosten.", "tokens": ["L\u00e4\u00dft", "uns", "der", "Him\u00b7mel", "kos\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt reicht mir Stab und Ordenskleid", "tokens": ["Jetzt", "reicht", "mir", "Stab", "und", "Or\u00b7dens\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der fahrenden Scholaren,", "tokens": ["Der", "fah\u00b7ren\u00b7den", "Scho\u00b7la\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich will zu guter Sommerzeit", "tokens": ["Ich", "will", "zu", "gu\u00b7ter", "Som\u00b7mer\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ins Land der Franken fahren!", "tokens": ["Ins", "Land", "der", "Fran\u00b7ken", "fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Wald steht gr\u00fcn, die Jagd geht gut,", "tokens": ["Der", "Wald", "steht", "gr\u00fcn", ",", "die", "Jagd", "geht", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwer ist das Korn geraten;", "tokens": ["Schwer", "ist", "das", "Korn", "ge\u00b7ra\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie k\u00f6nnen auf des Maines Flut", "tokens": ["Sie", "k\u0