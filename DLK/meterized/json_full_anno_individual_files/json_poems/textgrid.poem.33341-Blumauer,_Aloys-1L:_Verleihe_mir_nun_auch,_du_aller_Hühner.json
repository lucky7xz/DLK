{"textgrid.poem.33341": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verleihe mir nun auch, du aller H\u00fchner", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verleihe mir nun auch, du aller H\u00fchner", "tokens": ["Ver\u00b7lei\u00b7he", "mir", "nun", "auch", ",", "du", "al\u00b7ler", "H\u00fch\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "$,", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erlauchter Gro\u00dfsultan", "tokens": ["Er\u00b7lauch\u00b7ter", "Gro\u00df\u00b7sul\u00b7tan"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein g\u00fctig Ohr, und h\u00f6re deinen Diener", "tokens": ["Ein", "g\u00fc\u00b7tig", "Ohr", ",", "und", "h\u00f6\u00b7re", "dei\u00b7nen", "Die\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In hohen Gnaden an!", "tokens": ["In", "ho\u00b7hen", "Gna\u00b7den", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In deinen starken, ungeschw\u00e4chten Lenden", "tokens": ["In", "dei\u00b7nen", "star\u00b7ken", ",", "un\u00b7ge\u00b7schw\u00e4ch\u00b7ten", "Len\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zeigt noch die Mannheit sich", "tokens": ["Zeigt", "noch", "die", "Mann\u00b7heit", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die, ach, entnervt von buhlerischen H\u00e4nden,", "tokens": ["Die", ",", "ach", ",", "ent\u00b7nervt", "von", "buh\u00b7le\u00b7ri\u00b7schen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ITJ", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Hermanns Enkeln wich.", "tokens": ["Von", "Her\u00b7manns", "En\u00b7keln", "wich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "D'rum sieht auch manches Weibchen, dessen Gatte", "tokens": ["D'\u00b7rum", "sieht", "auch", "man\u00b7ches", "Weib\u00b7chen", ",", "des\u00b7sen", "Gat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "PIAT", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Im Bett nur schlafen kann,", "tokens": ["Im", "Bett", "nur", "schla\u00b7fen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der stolzen Henne Gl\u00fcck auf ihrer Latte", "tokens": ["Der", "stol\u00b7zen", "Hen\u00b7ne", "Gl\u00fcck", "auf", "ih\u00b7rer", "Lat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit neid'schen Augen an.", "tokens": ["Mit", "nei\u00b7d'\u00b7schen", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Selbst die Natur hat schon dich, wie ich glaube,", "tokens": ["Selbst", "die", "Na\u00b7tur", "hat", "schon", "dich", ",", "wie", "ich", "glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Ritter auserkohr'n;", "tokens": ["Zum", "Rit\u00b7ter", "aus\u00b7er\u00b7kohr'n", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie gab dir einen Kamm als Bickelhaube,", "tokens": ["Sie", "gab", "dir", "ei\u00b7nen", "Kamm", "als", "Bi\u00b7ckel\u00b7hau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Federbusch und Sporn.", "tokens": ["Und", "Fe\u00b7der\u00b7busch", "und", "Sporn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du k\u00fcndigst Muth und \u00e4chte Rittersitte", "tokens": ["Du", "k\u00fcn\u00b7digst", "Muth", "und", "\u00e4ch\u00b7te", "Rit\u00b7ter\u00b7sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In jeder Miene an,", "tokens": ["In", "je\u00b7der", "Mie\u00b7ne", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Gang ist stolz, und jeder deiner Schritte", "tokens": ["Dein", "Gang", "ist", "stolz", ",", "und", "je\u00b7der", "dei\u00b7ner", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verr\u00e4th den braven Mann.", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "den", "bra\u00b7ven", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Du scheust, wenn du ergrimmst, im Duelliren", "tokens": ["Du", "scheust", ",", "wenn", "du", "er\u00b7grimmst", ",", "im", "Du\u00b7el\u00b7li\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht Wunden und nicht Blut:", "tokens": ["Nicht", "Wun\u00b7den", "und", "nicht", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ganz Engelland bewundert in Turnieren", "tokens": ["Ganz", "En\u00b7gel\u00b7land", "be\u00b7wun\u00b7dert", "in", "Tur\u00b7nie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch immer deinen Muth.", "tokens": ["Noch", "im\u00b7mer", "dei\u00b7nen", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Allein die grossen Herr'n der Sch\u00f6pfung sch\u00e4men", "tokens": ["Al\u00b7lein", "die", "gros\u00b7sen", "Herr'n", "der", "Sch\u00f6p\u00b7fung", "sch\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob deiner Mannheit sich:", "tokens": ["Ob", "dei\u00b7ner", "Mann\u00b7heit", "sich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie suchen dir den Ritterschmuck zu nehmen,", "tokens": ["Sie", "su\u00b7chen", "dir", "den", "Rit\u00b7ter\u00b7schmuck", "zu", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und degradiren dich;", "tokens": ["Und", "de\u00b7gra\u00b7di\u00b7ren", "dich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Damit du so, wie sie, dich auf der B\u00fchne", "tokens": ["Da\u00b7mit", "du", "so", ",", "wie", "sie", ",", "dich", "auf", "der", "B\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "PWAV", "PPER", "$,", "PRF", "APPR", "ART", "NN"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Welt nur m\u00e4sten l\u00e4\u00dft,", "tokens": ["Der", "Welt", "nur", "m\u00e4s\u00b7ten", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und so, wie sie, fr\u00fch hinter der Gardine", "tokens": ["Und", "so", ",", "wie", "sie", ",", "fr\u00fch", "hin\u00b7ter", "der", "Gar\u00b7di\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "$,", "ADJD", "APPR", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Kastratenartig kr\u00e4hst.", "tokens": ["Kas\u00b7tra\u00b7ten\u00b7ar\u00b7tig", "kr\u00e4hst", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "D'rum denket, h\u00f6rt er dich den Tag verk\u00fcnden,", "tokens": ["D'\u00b7rum", "den\u00b7ket", ",", "h\u00f6rt", "er", "dich", "den", "Tag", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Jetzt mancher Ehemann,", "tokens": ["Jetzt", "man\u00b7cher", "E\u00b7he\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Wie Petrus einst, an seine Jugends\u00fcnden,", "tokens": ["Wie", "Pet\u00b7rus", "einst", ",", "an", "sei\u00b7ne", "Ju\u00b7gend\u00b7s\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "FM", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seufzt: W\u00e4r' ich ein Hahn!", "tokens": ["Und", "seufzt", ":", "W\u00e4r'", "ich", "ein", "Hahn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN",