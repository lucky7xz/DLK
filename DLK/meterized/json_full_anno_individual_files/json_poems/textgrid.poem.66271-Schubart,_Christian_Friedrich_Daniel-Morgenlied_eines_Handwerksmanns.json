{"textgrid.poem.66271": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied eines Handwerksmanns", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfer Gott, mit diesem Morgen", "tokens": ["Gro\u00b7\u00dfer", "Gott", ",", "mit", "die\u00b7sem", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tret' ich vor dein Angesicht;", "tokens": ["Tret'", "ich", "vor", "dein", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du kennst ja meine Sorgen", "tokens": ["Denn", "du", "kennst", "ja", "mei\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Lied verschm\u00e4hst du nicht.", "tokens": ["Und", "mein", "Lied", "ver\u00b7schm\u00e4hst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine Sonnenblicke dringen", "tokens": ["Dei\u00b7ne", "Son\u00b7nen\u00b7bli\u00b7cke", "drin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch in meine Werkstatt ein.", "tokens": ["Auch", "in", "mei\u00b7ne", "Werk\u00b7statt", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollte dir mein Beten, Singen,", "tokens": ["Soll\u00b7te", "dir", "mein", "Be\u00b7ten", ",", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vater, nicht gef\u00e4llig sein?", "tokens": ["Va\u00b7ter", ",", "nicht", "ge\u00b7f\u00e4l\u00b7lig", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja du h\u00f6rst es, was ich bete", "tokens": ["Ja", "du", "h\u00f6rst", "es", ",", "was", "ich", "be\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und du segnest meinen Flei\u00df,", "tokens": ["Und", "du", "seg\u00b7nest", "mei\u00b7nen", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst hier meine Arbeitsst\u00e4tte", "tokens": ["Siehst", "hier", "mei\u00b7ne", "Ar\u00b7beits\u00b7st\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets benetzt von meinem Schwei\u00df.", "tokens": ["Stets", "be\u00b7netzt", "von", "mei\u00b7nem", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gerne eil' ich zum Gesch\u00e4fte,", "tokens": ["Ger\u00b7ne", "eil'", "ich", "zum", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Arbeit ist der Menschen Pflicht,", "tokens": ["Ar\u00b7beit", "ist", "der", "Men\u00b7schen", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gib nur meinen Armen Kr\u00e4fte,", "tokens": ["Gib", "nur", "mei\u00b7nen", "Ar\u00b7men", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wisch den Schwei\u00df vom Angesicht.", "tokens": ["Wisch", "den", "Schwei\u00df", "vom", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Alle Lasten, die ich trage,", "tokens": ["Al\u00b7le", "Las\u00b7ten", ",", "die", "ich", "tra\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Guter Gott, die kennst du ja;", "tokens": ["Gu\u00b7ter", "Gott", ",", "die", "kennst", "du", "ja", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem ersten Morgenschlage", "tokens": ["Mit", "dem", "ers\u00b7ten", "Mor\u00b7