{"dta.poem.3909": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 5.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Bringt sie die Brief herf\u00fcr, die der und der ge-", "tokens": ["Bringt", "sie", "die", "Brief", "her\u00b7f\u00fcr", ",", "die", "der", "und", "der", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "PRELS", "ART", "KON", "ART", "TRUNC"], "meter": "+--++-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "schrieben,", "tokens": ["schrie\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "So heist es zwar gar viel, doch werden nach Beliebe\u0303", "tokens": ["So", "heist", "es", "zwar", "gar", "viel", ",", "doch", "wer\u00b7den", "nach", "Be\u00b7lie\u00b7b\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$,", "ADV", "VAFIN", "APPR", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Lenden sanfft geklopfft,", "tokens": ["Die", "Len\u00b7den", "sanfft", "ge\u00b7klopfft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wer ver\u00e4ndern will, die Backen zart gezopfft.", "tokens": ["Und", "wer", "ver\u00b7\u00e4n\u00b7dern", "will", ",", "die", "Ba\u00b7cken", "zart", "ge\u00b7zopfft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVINF", "VMFIN", "$,", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}