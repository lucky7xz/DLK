{"dta.poem.17216": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "66.  Von Maria Magdalene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Was d\u00e4nkt doch Magdalen da\u00df sie so offentlich", "tokens": ["Was", "d\u00e4nkt", "doch", "Mag\u00b7da\u00b7len", "da\u00df", "sie", "so", "of\u00b7fent\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "NN", "KOUS", "PPER", "ADV", "ADJD"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dem HErrn zu Fusse f\u00e4llt/ und schuldig giebet sich?", "tokens": ["Dem", "Herrn", "zu", "Fus\u00b7se", "f\u00e4llt", "/", "und", "schul\u00b7dig", "gie\u00b7bet", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$(", "KON", "ADJD", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach frage doch nicht erst: schau wie die Augen funken:", "tokens": ["Ach", "fra\u00b7ge", "doch", "nicht", "erst", ":", "schau", "wie", "die", "Au\u00b7gen", "fun\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "PTKNEG", "ADV", "$.", "ADJD", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Du sihst wol da\u00df sie ist von grosser Liebe trunken.", "tokens": ["Du", "sihst", "wol", "da\u00df", "sie", "ist", "von", "gros\u00b7ser", "Lie\u00b7be", "trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word