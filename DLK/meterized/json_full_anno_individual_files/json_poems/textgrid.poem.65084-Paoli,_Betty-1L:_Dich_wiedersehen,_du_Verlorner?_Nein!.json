{"textgrid.poem.65084": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dich wiedersehen, du Verlorner? Nein!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich wiedersehen, du Verlorner? Nein!", "tokens": ["Dich", "wie\u00b7der\u00b7se\u00b7hen", ",", "du", "Ver\u00b7lor\u00b7ner", "?", "Nein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "NN", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich fa\u00dft davor ein namenloses Zagen.", "tokens": ["Mich", "fa\u00dft", "da\u00b7vor", "ein", "na\u00b7men\u00b7lo\u00b7ses", "Za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stark trug ich uns'rer Trennung herbe Pein,", "tokens": ["Stark", "trug", "ich", "un\u00b7s'\u00b7rer", "Tren\u00b7nung", "her\u00b7be", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die\u00df Wiedersehen w\u00fc\u00dft' ich nicht zu tragen.", "tokens": ["Die\u00df", "Wie\u00b7der\u00b7se\u00b7hen", "w\u00fc\u00dft'", "ich", "nicht", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und kehrtest du auch liebend mir zur\u00fcck,", "tokens": ["Und", "kehr\u00b7test", "du", "auch", "lie\u00b7bend", "mir", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erinnerung zu bringen uns'rem Bunde,", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "zu", "brin\u00b7gen", "un\u00b7s'\u00b7rem", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du wecktest nicht das hingeschied'ne Gl\u00fcck \u2013", "tokens": ["Du", "weck\u00b7test", "nicht", "das", "hin\u00b7ge\u00b7schie\u00b7d'\u00b7ne", "Gl\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Es sitzt der Dolch zu tief in meiner Wunde.", "tokens": ["Es", "sitzt", "der", "Dolch", "zu", "tief", "in", "mei\u00b7ner", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und z\u00f6g'st aus dieser Wunde du den Stahl,", "tokens": ["Und", "z\u00f6g'st", "aus", "die\u00b7ser", "Wun\u00b7de", "du", "den", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Blut verstr\u00f6mte rasch in hei\u00dfen G\u00fcssen!", "tokens": ["Mein", "Blut", "ver\u00b7str\u00f6m\u00b7te", "rasch", "in", "hei\u00b7\u00dfen", "G\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du gabst mich frevelnd auf, aus eig'ner Wahl \u2013", "tokens": ["Du", "gabst", "mich", "fre\u00b7velnd", "auf", ",", "aus", "eig'\u00b7ner", "Wahl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt ist's ein schaurig unabweisbar M\u00fcssen.", "tokens": ["Jetzt", "ist's", "ein", "schau\u00b7rig", "un\u00b7ab\u00b7weis\u00b7bar", "M\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bescheide dich mit dem, was du gewannst,", "tokens": ["Be\u00b7schei\u00b7de", "dich", "mit", "dem", ",", "was", "du", "ge\u00b7wannst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als du mein Herz verrathen und zertreten.", "tokens": ["Als", "du", "mein", "Herz", "ver\u00b7ra\u00b7then", "und", "zer\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vergi\u00df mich, o vergi\u00df mich, wenn du kannst,", "tokens": ["Ver\u00b7gi\u00df", "mich", ",", "o", "ver\u00b7gi\u00df", "mich", ",", "wenn", "du", "kannst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "