{"dta.poem.5258": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fche Fr\u00fchlings-Vorw\u00fcrffe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm vier und drei\u00dfigsten, auf siebzehn hundert Jahr,", "tokens": ["Jm", "vier", "und", "drei\u00b7\u00dfigs\u00b7ten", ",", "auf", "sieb\u00b7zehn", "hun\u00b7dert", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "CARD", "KON", "VVFIN", "$,", "APPR", "CARD", "CARD", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Hab ich bereits im Februar,", "tokens": ["Hab", "ich", "be\u00b7reits", "im", "Feb\u00b7ru\u00b7ar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nachdem wir wenig Frost, kaum Eis, und keinen Schnee", "tokens": ["Nach\u00b7dem", "wir", "we\u00b7nig", "Frost", ",", "kaum", "Eis", ",", "und", "kei\u00b7nen", "Schnee"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "$,", "ADV", "NN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den gantzen Winter durch gesehn,", "tokens": ["Den", "gant\u00b7zen", "Win\u00b7ter", "durch", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nebst andern Blumen auch, ein sch\u00f6nes Fr\u00fchlings-Kind,", "tokens": ["Nebst", "an\u00b7dern", "Blu\u00b7men", "auch", ",", "ein", "sch\u00f6\u00b7nes", "Fr\u00fch\u00b7lings\u00b7Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein\u2019 aufgebl\u00fchte Hyacinth,", "tokens": ["Ein'", "auf\u00b7ge\u00b7bl\u00fch\u00b7te", "Hya\u00b7cin\u00b7th", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So aus dem Lande schon, ohn alle Kunst, gestiegen,", "tokens": ["So", "aus", "dem", "Lan\u00b7de", "schon", ",", "ohn", "al\u00b7le", "Kunst", ",", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$,", "KOUI", "PIAT", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit lieblichem Geruch erf\u00fcllt, und sch\u00f6n geschm\u00fcckt,", "tokens": ["Mit", "lieb\u00b7li\u00b7chem", "Ge\u00b7ruch", "er\u00b7f\u00fcllt", ",", "und", "sch\u00f6n", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In weislich-blauer Pracht, mit innigem Vergn\u00fcgen,", "tokens": ["In", "weis\u00b7lich\u00b7blau\u00b7er", "Pracht", ",", "mit", "in\u00b7ni\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Fast f\u00fcr Verwundrung stumm, erblickt.", "tokens": ["Fast", "f\u00fcr", "Ver\u00b7wund\u00b7rung", "stumm", ",", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es war ein aufgekl\u00e4rt- und sch\u00f6ner Tag;", "tokens": ["Es", "war", "ein", "auf\u00b7ge\u00b7kl\u00e4r\u00b7t", "und", "sch\u00f6\u00b7ner", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Solch eine Heiterkeit,", "tokens": ["Solch", "ei\u00b7ne", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wie man, in voller Fr\u00fchlings-Zeit,", "tokens": ["Wie", "man", ",", "in", "vol\u00b7ler", "Fr\u00fch\u00b7lings\u00b7Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Am Firmament zu sehen pflag,", "tokens": ["Am", "Fir\u00b7ma\u00b7ment", "zu", "se\u00b7hen", "pflag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Schien mit der Erde sich zu f\u00fcgen,", "tokens": ["Schien", "mit", "der", "Er\u00b7de", "sich", "zu", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Schien \u00fcberall auf Gras und Kraut zu liegen.", "tokens": ["Schien", "\u00fc\u00b7be\u00b7rall", "auf", "Gras", "und", "Kraut", "zu", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wenn hie und da, durch das noch zarte gr\u00fcn", "tokens": ["Wenn", "hie", "und", "da", ",", "durch", "das", "noch", "zar\u00b7te", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "KON", "ADV", "$,", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Des jungen Krauts, das Licht der Sonnen schien;", "tokens": ["Des", "jun\u00b7gen", "Krauts", ",", "das", "Licht", "der", "Son\u00b7nen", "schien", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "War jedes Blatt durchl\u00e4uchtig, und der Grund,", "tokens": ["War", "je\u00b7des", "Blatt", "durch\u00b7l\u00e4uch\u00b7tig", ",", "und", "der", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Worauf die gr\u00fcne Sch\u00f6nheit stund,", "tokens": ["Wo\u00b7rauf", "die", "gr\u00fc\u00b7ne", "Sch\u00f6n\u00b7heit", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Vermehrte, durch die Dunckelheit,", "tokens": ["Ver\u00b7mehr\u00b7te", ",", "durch", "die", "Dun\u00b7ckel\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die fast schmaragdne Lieblichkeit.", "tokens": ["Die", "fast", "schma\u00b7ragd\u00b7ne", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Es zeigten der Narciss- und Tulpen Bl\u00e4tter", "tokens": ["Es", "zeig\u00b7ten", "der", "Na\u00b7rciss", "und", "Tul\u00b7pen", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "TRUNC", "KON", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Nur halb annoch gesehne Spitzen sich:", "tokens": ["Nur", "halb", "an\u00b7noch", "ge\u00b7seh\u00b7ne", "Spit\u00b7zen", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADJA", "NN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Jhr neues Gr\u00fcn vergn\u00fcgte mich", "tokens": ["Ihr", "neu\u00b7es", "Gr\u00fcn", "ver\u00b7gn\u00fcg\u00b7te", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Recht inniglich,", "tokens": ["Recht", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+---", "measure": "dactylic.init"}, "line.27": {"text": "Zumahl bey dem so angenehmen Wetter.", "tokens": ["Zu\u00b7mahl", "bey", "dem", "so", "an\u00b7ge\u00b7neh\u00b7men", "Wet\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}