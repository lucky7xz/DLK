{"dta.poem.19507": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Kaiser und das Fr\u00e4ulein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch am Septimer, dem Kaiserpasse,", "tokens": ["Hoch", "am", "Sep\u00b7ti\u00b7mer", ",", "dem", "Kai\u00b7ser\u00b7pas\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "(denn die Kaiser pflegten nach Italien", "tokens": ["(", "denn", "die", "Kai\u00b7ser", "pfleg\u00b7ten", "nach", "I\u00b7ta\u00b7li\u00b7en"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "ART", "NN", "VVFIN", "APPR", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ueber dieses Bergesjoch zu reiten)", "tokens": ["Ue\u00b7ber", "die\u00b7ses", "Ber\u00b7ges\u00b7joch", "zu", "rei\u00b7ten", ")"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKZU", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hielt ich unter steilen Sonnenstrahlen", "tokens": ["Hielt", "ich", "un\u00b7ter", "stei\u00b7len", "Son\u00b7nen\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mittagsrast. Mir gegen\u00fcber wand sich", "tokens": ["Mit\u00b7tags\u00b7rast", ".", "Mir", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "wand", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "NE", "APPO", "VVFIN", "PRF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Um den Felsen noch ein St\u00fcck des alten", "tokens": ["Um", "den", "Fel\u00b7sen", "noch", "ein", "St\u00fcck", "des", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ADV", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Saumwegs schwebend \u00fcber j\u00e4hem Abgrund.", "tokens": ["Saum\u00b7wegs", "schwe\u00b7bend", "\u00fc\u00b7ber", "j\u00e4\u00b7hem", "Ab\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mittag ist des Berges Geisterstunde.", "tokens": ["Mit\u00b7tag", "ist", "des", "Ber\u00b7ges", "Geis\u00b7ter\u00b7stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "In die Sonne blinzelt' ich. Ein Hornruf!", "tokens": ["In", "die", "Son\u00b7ne", "blin\u00b7zelt'", "ich", ".", "Ein", "Horn\u00b7ruf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$.", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Banner flattern. Schwert und B\u00fcgel klirren.", "tokens": ["Ban\u00b7ner", "flat\u00b7tern", ".", "Schwert", "und", "B\u00fc\u00b7gel", "klir\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Frau'n und Ritter gleiten aus den S\u00e4tteln.", "tokens": ["Frau'n", "und", "Rit\u00b7ter", "glei\u00b7ten", "aus", "den", "S\u00e4t\u00b7teln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Sorglich leiten S\u00e4umer scheue Rosse.", "tokens": ["Sorg\u00b7lich", "lei\u00b7ten", "S\u00e4u\u00b7mer", "scheu\u00b7e", "Ros\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Die gestrenge Kais'rin seh' ich schreiten,", "tokens": ["Die", "ge\u00b7stren\u00b7ge", "Kais'\u00b7rin", "seh'", "ich", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Ein versteinert Weib mit harten Z\u00fcgen.", "tokens": ["Ein", "ver\u00b7stei\u00b7nert", "Weib", "mit", "har\u00b7ten", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Hinter ihr die Fr\u00e4ulein. Einer Zarten", "tokens": ["Hin\u00b7ter", "ihr", "die", "Fr\u00e4u\u00b7lein", ".", "Ei\u00b7ner", "Zar\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Schwindelt pl\u00f6tzlich. Ihre Kniee wanken.", "tokens": ["Schwin\u00b7delt", "pl\u00f6tz\u00b7lich", ".", "Ih\u00b7re", "Kni\u00b7ee", "wan\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Sich entf\u00e4rbend lehnt sie an die Bergwand ...", "tokens": ["Sich", "ent\u00b7f\u00e4r\u00b7bend", "lehnt", "sie", "an", "die", "Berg\u00b7wand", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Rasch ein Held \u2014 er tr\u00e4gt das Kaiserkr\u00f6nlein", "tokens": ["Rasch", "ein", "Held", "er", "tr\u00e4gt", "das", "Kai\u00b7ser\u00b7kr\u00f6n\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$(", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Um die Kappe \u2014 f\u00e4ngt in seinen m\u00e4cht'gen", "tokens": ["Um", "die", "Kap\u00b7pe", "f\u00e4ngt", "in", "sei\u00b7nen", "m\u00e4cht'\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "$(", "VVFIN", "APPR", "PPOSAT", "ADJA"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "Armen auf das wanke Kind und tr\u00e4gt es", "tokens": ["Ar\u00b7men", "auf", "das", "wan\u00b7ke", "Kind", "und", "tr\u00e4gt", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "An die Brust gedr\u00fcckt. Das M\u00e4dchen schwebte", "tokens": ["An", "die", "Brust", "ge\u00b7dr\u00fcckt", ".", "Das", "M\u00e4d\u00b7chen", "schweb\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Sicher \u00fcberm Abgrund und er raubt' ihr", "tokens": ["Si\u00b7cher", "\u00fc\u00b7berm", "Ab\u00b7grund", "und", "er", "raubt'", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "KON", "PPER", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Einen fl\u00fccht'gen Ku\u00df. Da schwand das Blendwerk.", "tokens": ["Ei\u00b7nen", "fl\u00fccht'\u00b7gen", "Ku\u00df", ".", "Da", "schwand", "das", "Blend\u00b7werk", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Weiter pilgernd r\u00e4thselt' ich ein Weilchen:", "tokens": ["Wei\u00b7ter", "pil\u00b7gernd", "r\u00e4th\u00b7selt'", "ich", "ein", "Weil\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "War es einer der Ottonen oder", "tokens": ["War", "es", "ei\u00b7ner", "der", "Ot\u00b7to\u00b7nen", "o\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ART", "NN", "KON"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "War's ein Heinrich oder war's ein Friedrich,", "tokens": ["Wa\u00b7r's", "ein", "Hein\u00b7rich", "o\u00b7der", "wa\u00b7r's", "ein", "Fried\u00b7rich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NE", "KON", "VAFIN", "ART", "NE", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Der die wehrlos Schwebende gek\u00fc\u00dft hat?", "tokens": ["Der", "die", "wehr\u00b7los", "Schwe\u00b7ben\u00b7de", "ge\u00b7k\u00fc\u00dft", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}