{"dta.poem.10541": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Prolog.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Schwarze R\u00f6cke, seid'ne Str\u00fcmpfe,", "tokens": ["Schwar\u00b7ze", "R\u00f6\u00b7cke", ",", "sei\u00b7d'\u00b7ne", "Str\u00fcmp\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wei\u00dfe, h\u00f6fliche Manschetten,", "tokens": ["Wei\u00b7\u00dfe", ",", "h\u00f6f\u00b7li\u00b7che", "Man\u00b7schet\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanfte Reden, Embrassiren \u2014", "tokens": ["Sanf\u00b7te", "Re\u00b7den", ",", "E\u00b7mbras\u00b7si\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$("], "meter": "+-+-----", "measure": "unknown.measure.di"}, "line.4": {"text": "Ach, wenn sie nur Herzen h\u00e4tten!", "tokens": ["Ach", ",", "wenn", "sie", "nur", "Her\u00b7zen", "h\u00e4t\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Herzen in der Brust, und Liebe,", "tokens": ["Her\u00b7zen", "in", "der", "Brust", ",", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warme Liebe in dem Herzen \u2014", "tokens": ["War\u00b7me", "Lie\u00b7be", "in", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, mich t\u00f6dtet ihr Gesinge", "tokens": ["Ach", ",", "mich", "t\u00f6d\u00b7tet", "ihr", "Ge\u00b7sin\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von erlog'nen Liebesschmerzen.", "tokens": ["Von", "er\u00b7lo\u00b7g'\u00b7nen", "Lie\u00b7bes\u00b7schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf die Berge will ich steigen,", "tokens": ["Auf", "die", "Ber\u00b7ge", "will", "ich", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die frommen H\u00fctten stehen,", "tokens": ["Wo", "die", "from\u00b7men", "H\u00fct\u00b7ten", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Brust sich frei erschlie\u00dfet,", "tokens": ["Wo", "die", "Brust", "sich", "frei", "er\u00b7schlie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die freien L\u00fcfte wehen.", "tokens": ["Und", "die", "frei\u00b7en", "L\u00fcf\u00b7te", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf die Berge will ich steigen,", "tokens": ["Auf", "die", "Ber\u00b7ge", "will", "ich", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die dunkeln Tannen ragen,", "tokens": ["Wo", "die", "dun\u00b7keln", "Tan\u00b7nen", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00e4che rauschen, V\u00f6gel singen,", "tokens": ["B\u00e4\u00b7che", "rau\u00b7schen", ",", "V\u00f6\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die stolzen Wolken jagen.", "tokens": ["Und", "die", "stol\u00b7zen", "Wol\u00b7ken", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lebet wohl, ihr glatten S\u00e4le!", "tokens": ["Le\u00b7bet", "wohl", ",", "ihr", "glat\u00b7ten", "S\u00e4\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glatte Herren! glatte Frauen!", "tokens": ["Glat\u00b7te", "Her\u00b7ren", "!", "glat\u00b7te", "Frau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf die Berge will ich steigen,", "tokens": ["Auf", "die", "Ber\u00b7ge", "will", "ich", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lachend auf Euch niederschauen.", "tokens": ["La\u00b7chend", "auf", "Euch", "nie\u00b7der\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}