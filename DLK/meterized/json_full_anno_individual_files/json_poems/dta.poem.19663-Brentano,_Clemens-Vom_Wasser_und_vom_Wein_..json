{"dta.poem.19663": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Vom Wasser und vom Wein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df mir ein Liedlein, h\u00fcbsch und fein,               ", "tokens": ["Ich", "wei\u00df", "mir", "ein", "Lied\u00b7lein", ",", "h\u00fcbsch", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohl von dem Wasser, wohl von dem Wein,", "tokens": ["Wohl", "von", "dem", "Was\u00b7ser", ",", "wohl", "von", "dem", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Der Wein kanns Wasser nit leiden,", "tokens": ["Der", "Wein", "kanns", "Was\u00b7ser", "nit", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie wollen wohl alleweg streiten.", "tokens": ["Sie", "wol\u00b7len", "wohl", "al\u00b7le\u00b7weg", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Da sprach der Wein: Bin ich so fein,", "tokens": ["Da", "sprach", "der", "Wein", ":", "Bin", "ich", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man f\u00fchrt mich in alle die L\u00e4nder hinein,", "tokens": ["Man", "f\u00fchrt", "mich", "in", "al\u00b7le", "die", "L\u00e4n\u00b7der", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Man f\u00fchrt mich vor's Wirth sein Keller,", "tokens": ["Man", "f\u00fchrt", "mich", "vor's", "Wirth", "sein", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und trinkt mich f\u00fcr Muskateller.", "tokens": ["Und", "trinkt", "mich", "f\u00fcr", "Mus\u00b7ka\u00b7tel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Da sprach das Wasser: Bin ich so fein,", "tokens": ["Da", "sprach", "das", "Was\u00b7ser", ":", "Bin", "ich", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ich laufe in alle die L\u00e4nder hinein,", "tokens": ["Ich", "lau\u00b7fe", "in", "al\u00b7le", "die", "L\u00e4n\u00b7der", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich laufe dem M\u00fcller ums Hau\u00dfe,", "tokens": ["Ich", "lau\u00b7fe", "dem", "M\u00fcl\u00b7ler", "ums", "Hau\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und treibe das R\u00e4dlein mit Brau\u00dfe.", "tokens": ["Und", "trei\u00b7be", "das", "R\u00e4d\u00b7lein", "mit", "Brau\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Da sprach der Wein: Bin ich so fein,", "tokens": ["Da", "sprach", "der", "Wein", ":", "Bin", "ich", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man schenkt mich in Gl\u00e4ser und Becherlein,", "tokens": ["Man", "schenkt", "mich", "in", "Gl\u00e4\u00b7ser", "und", "Be\u00b7cher\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und trinkt mich f\u00fcr s\u00fc\u00df und f\u00fcr sauer,", "tokens": ["Und", "trinkt", "mich", "f\u00fcr", "s\u00fc\u00df", "und", "f\u00fcr", "sau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJD", "KON", "APPR", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Herr als gleich, wie der Bauer.", "tokens": ["Der", "Herr", "als", "gleich", ",", "wie", "der", "Bau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADV", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Da sprach das Wasser: Bin ich so fein,", "tokens": ["Da", "sprach", "das", "Was\u00b7ser", ":", "Bin", "ich", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Man tr\u00e4gt mich in die K\u00fcche hinein,", "tokens": ["Man", "tr\u00e4gt", "mich", "in", "die", "K\u00fc\u00b7che", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Man braucht mich die ganze Wochen,", "tokens": ["Man", "braucht", "mich", "die", "gan\u00b7ze", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Zum Waschen, zum Backen, zum Kochen.", "tokens": ["Zum", "Wa\u00b7schen", ",", "zum", "Ba\u00b7cken", ",", "zum", "Ko\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Da sprach der Wein: Bin ich so fein,", "tokens": ["Da", "sprach", "der", "Wein", ":", "Bin", "ich", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man tr\u00e4gt mich in die Schlacht hinein,", "tokens": ["Man", "tr\u00e4gt", "mich", "in", "die", "Schlacht", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu K\u00f6nigen und auch F\u00fcrsten,", "tokens": ["Zu", "K\u00f6\u00b7ni\u00b7gen", "und", "auch", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df sie nicht m\u00f6gen verd\u00fcrsten.", "tokens": ["Da\u00df", "sie", "nicht", "m\u00f6\u00b7gen", "ver\u00b7d\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Da sprach das Wasser: Bin ich so fein,", "tokens": ["Da", "sprach", "das", "Was\u00b7ser", ":", "Bin", "ich", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Man braucht mich in den Badst\u00fcblein,", "tokens": ["Man", "braucht", "mich", "in", "den", "Bad\u00b7st\u00fcb\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darin manch sch\u00f6ne Jungfraue", "tokens": ["Da\u00b7rin", "manch", "sch\u00f6\u00b7ne", "Jung\u00b7frau\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich badet k\u00fchl und auch laue.", "tokens": ["Sich", "ba\u00b7det", "k\u00fchl", "und", "auch", "lau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADJD", "KON", "ADV", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Da sprach der Wein: Bin ich so fein,", "tokens": ["Da", "sprach", "der", "Wein", ":", "Bin", "ich", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "B\u00fcrgermeister und Rath insgemein", "tokens": ["B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "und", "Rath", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADV"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Den Hut vor mir abnehmen,", "tokens": ["Den", "Hut", "vor", "mir", "ab\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Rathskeller zu Bremen.", "tokens": ["Im", "Raths\u00b7kel\u00b7ler", "zu", "Bre\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da sprach das Wasser: Bin ich so fein,", "tokens": ["Da", "sprach", "das", "Was\u00b7ser", ":", "Bin", "ich", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Man gie\u00dft mich in die Flamm hinein,", "tokens": ["Man", "gie\u00dft", "mich", "in", "die", "Flamm", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Spritz und Eimer man rennet,", "tokens": ["Mit", "Spritz", "und", "Ei\u00b7mer", "man", "ren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df Schlo\u00df und Haus nicht verbrennet.", "tokens": ["Da\u00df", "Schlo\u00df", "und", "Haus", "nicht", "ver\u00b7bren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Da sprach der Wein: Bin ich so fein,", "tokens": ["Da", "sprach", "der", "Wein", ":", "Bin", "ich", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man schenkt mich den Doktoren ein,", "tokens": ["Man", "schenkt", "mich", "den", "Dok\u00b7to\u00b7ren", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenns Lichtlein nit will leuchten,", "tokens": ["Wenns", "Licht\u00b7lein", "nit", "will", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", 