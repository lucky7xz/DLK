{"textgrid.poem.53978": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Kinderstube", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein neues Spiel ist aufgetan \u2013", "tokens": ["Ein", "neu\u00b7es", "Spiel", "ist", "auf\u00b7ge\u00b7tan", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das spielen die Kinder so gerne;", "tokens": ["das", "spie\u00b7len", "die", "Kin\u00b7der", "so", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "sie spielens im Alt und Tenor und Sopran,", "tokens": ["sie", "spie\u00b7lens", "im", "Alt", "und", "Te\u00b7nor", "und", "Sop\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "KON", "NE", "KON", "NE", "$,"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "in Berlin und der bayrischen Ferne.", "tokens": ["in", "Ber\u00b7lin", "und", "der", "bay\u00b7ri\u00b7schen", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Krise!", "tokens": ["Kri\u00b7se", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Krise der Intendanz!", "tokens": ["Kri\u00b7se", "der", "In\u00b7ten\u00b7danz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "und es umschlingt dich ein H\u00f6llentanz:", "tokens": ["und", "es", "um\u00b7schlingt", "dich", "ein", "H\u00f6l\u00b7len\u00b7tanz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Der Kleiber und der Klemperer,", "tokens": ["Der", "Klei\u00b7ber", "und", "der", "Klem\u00b7pe\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.9": {"text": "der Walter und die Kemperer;", "tokens": ["der", "Wal\u00b7ter", "und", "die", "Kem\u00b7pe\u00b7rer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.10": {"text": "der Knappertsbusch und der Richard Strauss,", "tokens": ["der", "Knap\u00b7perts\u00b7busch", "und", "der", "Ric\u00b7hard", "Strauss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NE", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "der Je\u00dfner, der Reinhardt, das Opernhaus \u2013", "tokens": ["der", "Je\u00df\u00b7ner", ",", "der", "Rein\u00b7hardt", ",", "das", "O\u00b7pern\u00b7haus", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "gestern, morgen und heute \u2013", "tokens": ["ge\u00b7stern", ",", "mor\u00b7gen", "und", "heu\u00b7te", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "KON", "ADV", "$("], "meter": "-+---+-", "measure": "dactylic.init"}, "line.13": {"text": "sehr prominente Leute.", "tokens": ["sehr", "pro\u00b7mi\u00b7nen\u00b7te", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Bleibt Legal? Geht Tietjen? Ist Ziegel bereit?", "tokens": ["Bleibt", "Le\u00b7gal", "?", "Geht", "Tiet\u00b7jen", "?", "Ist", "Zie\u00b7gel", "be\u00b7reit", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "VVFIN", "NE", "$.", "VAFIN", "NN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wer inszeniert die ", "tokens": ["Wer", "in\u00b7sze\u00b7niert", "die"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "In Deutschland gibt es um diese Zeit", "tokens": ["In", "Deutschland", "gibt", "es", "um", "die\u00b7se", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "zwei Millionen Arbeitslose.", "tokens": ["zwei", "Mil\u00b7lion\u00b7en", "Ar\u00b7beits\u00b7lo\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Krise?", "tokens": ["Kri\u00b7se", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Es wirtschaftet um dich her", "tokens": ["Es", "wirt\u00b7schaf\u00b7tet", "um", "dich", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "APZR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "der \u00fcbersch\u00e4tzte Kommission\u00e4r:", "tokens": ["der", "\u00fc\u00b7bersc\u00b7h\u00e4tz\u00b7te", "Kom\u00b7mis\u00b7si\u00b7o\u00b7n\u00e4r", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Klemperer und der Kleiberer", "tokens": ["Der", "Klem\u00b7pe\u00b7rer", "und", "der", "Klei\u00b7be\u00b7rer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "und mindere Zeitvertreiberer;", "tokens": ["und", "min\u00b7de\u00b7re", "Zeit\u00b7ver\u00b7trei\u00b7be\u00b7rer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "der Walterer und der Doktor Klein \u2013", "tokens": ["der", "Wal\u00b7te\u00b7rer", "und", "der", "Dok\u00b7tor", "Klein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NE", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Gott gr\u00fc\u00df die Kunst! Eine Krise mu\u00df sein!", "tokens": ["Gott", "gr\u00fc\u00df", "die", "Kunst", "!", "Ei\u00b7ne", "Kri\u00b7se", "mu\u00df", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VMFIN", "VAINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.12": {"text": "Gestern, morgen und heute \u2013:", "tokens": ["Ge\u00b7stern", ",", "mor\u00b7gen", "und", "heu\u00b7te", "\u2013", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ADV", "KON", "ADV", "$(", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.13": {"text": "sehr prominente Leute.", "tokens": ["sehr", "pro\u00b7mi\u00b7nen\u00b7te", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Das alles war schon einmal da \u2013", "tokens": ["Das", "al\u00b7les", "war", "schon", "ein\u00b7mal", "da", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "im M\u00e4rze, im vorigen M\u00e4rze.", "tokens": ["im", "M\u00e4r\u00b7ze", ",", "im", "vo\u00b7ri\u00b7gen", "M\u00e4r\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da tanzte die El\u00dfler den zierlichsten Pas,", "tokens": ["Da", "tanz\u00b7te", "die", "El\u00df\u00b7ler", "den", "zier\u00b7lichs\u00b7ten", "Pas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "und es sch\u00e4umte die Druckerschw\u00e4rze.", "tokens": ["und", "es", "sch\u00e4um\u00b7te", "die", "Dru\u00b7cker\u00b7schw\u00e4r\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Der Horizont war lieblich verengt,", "tokens": ["Der", "Ho\u00b7ri\u00b7zont", "war", "lieb\u00b7lich", "ver\u00b7engt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "das Theater hat alle h\u00fcbsch abgelenkt . 