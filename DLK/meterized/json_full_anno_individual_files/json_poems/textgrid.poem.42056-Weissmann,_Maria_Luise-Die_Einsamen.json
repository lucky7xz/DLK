{"textgrid.poem.42056": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die Einsamen", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Uns sind der H\u00e4user viele umgestellt.", "tokens": ["Uns", "sind", "der", "H\u00e4u\u00b7ser", "vie\u00b7le", "um\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die goldenen Gemache blenden manchmal", "tokens": ["Die", "gol\u00b7de\u00b7nen", "Ge\u00b7ma\u00b7che", "blen\u00b7den", "manch\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der gro\u00dfen Feste, manchmal verweilt", "tokens": ["Der", "gro\u00b7\u00dfen", "Fes\u00b7te", ",", "manch\u00b7mal", "ver\u00b7weilt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Um uns die Zelle, die Klause des Eremiten,", "tokens": ["Um", "uns", "die", "Zel\u00b7le", ",", "die", "Klau\u00b7se", "des", "E\u00b7re\u00b7mi\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Leise bei Nacht.", "tokens": ["Lei\u00b7se", "bei", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wir stehen oft", "tokens": ["Wir", "ste\u00b7hen", "oft"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Aufgang ist unserm Blick das Gestirn,", "tokens": ["Auf\u00b7gang", "ist", "un\u00b7serm", "Blick", "das", "Ge\u00b7stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Weite herbergt uns k\u00fchl. Schlie\u00dft dann der Tag", "tokens": ["Wei\u00b7te", "her\u00b7bergt", "uns", "k\u00fchl", ".", "Schlie\u00dft", "dann", "der", "Tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$.", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Brennend in Liebe uns ein, wir bestehn", "tokens": ["Bren\u00b7nend", "in", "Lie\u00b7be", "uns", "ein", ",", "wir", "be\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "NN", "PPER", "PTKVZ", "$,", "PPER", "VVINF"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Schweigend. \u00d6fters im Abend doch furchen", "tokens": ["Schwei\u00b7gend", ".", "\u00d6f\u00b7ters", "im", "A\u00b7bend", "doch", "fur\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "ADV", "APPRART", "NN", "ADV", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Angst\u00fcberschreckt unsre kindlichen Stirnen sich tiefer.", "tokens": ["Ang\u00b7st\u00fc\u00b7bersc\u00b7hreckt", "uns\u00b7re", "kind\u00b7li\u00b7chen", "Stir\u00b7nen", "sich", "tie\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PRF", "ADJD", "$."], "meter": "-+-+--+--+--+-", "measu