{"dta.poem.10470": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Was will die einsame Thr\u00e4ne?", "tokens": ["Was", "will", "die", "ein\u00b7sa\u00b7me", "Thr\u00e4\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie tr\u00fcbt mir ja den Blick.", "tokens": ["Sie", "tr\u00fcbt", "mir", "ja", "den", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie blieb aus alten Zeiten", "tokens": ["Sie", "blieb", "aus", "al\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meinem Auge zur\u00fcck.", "tokens": ["In", "mei\u00b7nem", "Au\u00b7ge", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sie hatte viel leuchtende Schwestern,", "tokens": ["Sie", "hat\u00b7te", "viel", "leuch\u00b7ten\u00b7de", "Schwes\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die alle zerflossen sind,", "tokens": ["Die", "al\u00b7le", "zer\u00b7flos\u00b7sen", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit meinen Qualen und Freuden,", "tokens": ["Mit", "mei\u00b7nen", "Qua\u00b7len", "und", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zerflossen in Nacht und Wind.", "tokens": ["Zer\u00b7flos\u00b7sen", "in", "Nacht", "und", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie Nebel sind auch zerflossen", "tokens": ["Wie", "Ne\u00b7bel", "sind", "auch", "zer\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VAFIN", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die blauen Sternelein,", "tokens": ["Die", "blau\u00b7en", "Ster\u00b7ne\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die mir jene Freuden und Qualen", "tokens": ["Die", "mir", "je\u00b7ne", "Freu\u00b7den", "und", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PDAT", "NN", "KON", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gel\u00e4chelt in's Herz hinein.", "tokens": ["Ge\u00b7l\u00e4\u00b7chelt", "in's", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ach, meine Liebe selber", "tokens": ["Ach", ",", "mei\u00b7ne", "Lie\u00b7be", "sel\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zerflo\u00df wie eitel Hauch!", "tokens": ["Zer\u00b7flo\u00df", "wie", "ei\u00b7tel", "Hauch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du alte, einsame Thr\u00e4ne,", "tokens": ["Du", "al\u00b7te", ",", "ein\u00b7sa\u00b7me", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zerflie\u00dfe jetzunde", "tokens": ["Zer\u00b7flie\u00b7\u00dfe", "je\u00b7tzun\u00b7de"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}