{"textgrid.poem.55788": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verdunkelt war Dein weites Schlafgemach,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verdunkelt war Dein weites Schlafgemach,", "tokens": ["Ver\u00b7dun\u00b7kelt", "war", "Dein", "wei\u00b7tes", "Schlaf\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So ganz wie damals, und ein schwerer Hauch", "tokens": ["So", "ganz", "wie", "da\u00b7mals", ",", "und", "ein", "schwe\u00b7rer", "Hauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOKOM", "ADV", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von welkem Lorbeer, Veilchen und Lavendel,", "tokens": ["Von", "wel\u00b7kem", "Lor\u00b7beer", ",", "Veil\u00b7chen", "und", "La\u00b7ven\u00b7del", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erstickend, s\u00fc\u00df-bet\u00e4ubend koste, ganz", "tokens": ["Er\u00b7sti\u00b7ckend", ",", "s\u00fc\u00df\u00b7be\u00b7t\u00e4u\u00b7bend", "kos\u00b7te", ",", "ganz"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VVPP", "$,", "ADJD", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie damals, um die wonnig m\u00fcden Nerven.", "tokens": ["Wie", "da\u00b7mals", ",", "um", "die", "won\u00b7nig", "m\u00fc\u00b7den", "Ner\u00b7ven", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUI", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit schwanken Schritten trat ich ein bei Dir.", "tokens": ["Mit", "schwan\u00b7ken", "Schrit\u00b7ten", "trat", "ich", "ein", "bei", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein schlankes Wachslicht sch\u00fcttet fahlen Schein", "tokens": ["Ein", "schlan\u00b7kes", "Wachs\u00b7licht", "sch\u00fct\u00b7tet", "fah\u00b7len", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus bronce-geschnittnem Riesengirandol", "tokens": ["Aus", "bron\u00b7ce\u00b7ge\u00b7schnitt\u00b7nem", "Rie\u00b7sen\u00b7gi\u00b7ran\u00b7dol"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und spielt und huscht und t\u00e4nzelt launentoll", "tokens": ["Und", "spielt", "und", "huscht", "und", "t\u00e4n\u00b7zelt", "lau\u00b7nen\u00b7toll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um deines breiten Lagers \u00fcppig-wei\u00dfe", "tokens": ["Um", "dei\u00b7nes", "brei\u00b7ten", "La\u00b7gers", "\u00fcp\u00b7pig\u00b7wei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Geraffte Schillerseide \u2013 um Dich selbst,", "toke