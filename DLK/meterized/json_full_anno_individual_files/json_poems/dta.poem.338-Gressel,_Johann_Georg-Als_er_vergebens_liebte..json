{"dta.poem.338": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er vergebens liebte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Ja! es ist gewi\u00df genug", "tokens": ["Ach", "Ja", "!", "es", "ist", "ge\u00b7wi\u00df", "ge\u00b7nug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKANT", "$.", "PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat mich nun verstossen/", "tokens": ["Sie", "hat", "mich", "nun", "ver\u00b7stos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Hoffen ist ein leerer Krug/", "tokens": ["Mein", "Hof\u00b7fen", "ist", "ein", "lee\u00b7rer", "Krug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es schl\u00e4get einen Blossen.", "tokens": ["Es", "schl\u00e4\u00b7get", "ei\u00b7nen", "Blos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie ist noch h\u00e4rter als ein Stein", "tokens": ["Sie", "ist", "noch", "h\u00e4r\u00b7ter", "als", "ein", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und will nicht zu bewegen seyn.", "tokens": ["Und", "will", "nicht", "zu", "be\u00b7we\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sagt was habe ich verbrochen", "tokens": ["Sagt", "was", "ha\u00b7be", "ich", "ver\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PWS", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das der Straffe w\u00fcrdig ist?", "tokens": ["Das", "der", "Straf\u00b7fe", "w\u00fcr\u00b7dig", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles was ich noch erduldet", "tokens": ["Al\u00b7les", "was", "ich", "noch", "er\u00b7dul\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "PWS", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat mein Hertze nicht verschuldet.", "tokens": ["Hat", "mein", "Hert\u00b7ze", "nicht", "ver\u00b7schul\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "VVFIN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhr seyd ohne Fug entr\u00fcst.", "tokens": ["Ihr", "seyd", "oh\u00b7ne", "Fug", "ent\u00b7r\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sagt was habe ich verbrochen/", "tokens": ["Sagt", "was", "ha\u00b7be", "ich", "ver\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWS", "VAFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das der Straffe w\u00fcrdig ist.", "tokens": ["Das", "der", "Straf\u00b7fe", "w\u00fcr\u00b7dig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jhr seyd kein Mensch! ihr seyd ein Tyger-Thier/", "tokens": ["Ihr", "seyd", "kein", "Mensch", "!", "ihr", "seyd", "ein", "Ty\u00b7ger\u00b7Thier", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jhr geht an Grimm den B\u00e4ren f\u00fcr/", "tokens": ["Ihr", "geht", "an", "Grimm", "den", "B\u00e4\u00b7ren", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr z\u00fcrnet/ da\u00df ich liebe/", "tokens": ["Ihr", "z\u00fcr\u00b7net", "/", "da\u00df", "ich", "lie\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ha\u00dft die reinen Triebe/", "tokens": ["Und", "ha\u00dft", "die", "rei\u00b7nen", "Trie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die meine Seele nehrt.", "tokens": ["Die", "mei\u00b7ne", "See\u00b7le", "nehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Stein und Stahl wird endlich abgezehrt/", "tokens": ["Der", "Stein", "und", "Stahl", "wird", "end\u00b7lich", "ab\u00b7ge\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Allein ihr seyd noch mehr als steinern", "tokens": ["Al\u00b7lein", "ihr", "seyd", "noch", "mehr", "als", "stei\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "PIAT", "KOKOM", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Eur Hertz will sich verhelffenbeinern.", "tokens": ["Eur", "Hertz", "will", "sich", "ver\u00b7hel\u00b7ffen\u00b7bei\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Hertze halt nur ein!", "tokens": ["Mein", "Hert\u00b7ze", "halt", "nur", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie wird nicht zu bewegen seyn.", "tokens": ["Sie", "wird", "nicht", "zu", "be\u00b7we\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jhr meynt vielleicht", "tokens": ["Ihr", "meynt", "viel\u00b7leicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich m\u00fcste sterben/", "tokens": ["Ich", "m\u00fcs\u00b7te", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nein! eure Meynung treugt/", "tokens": ["Nein", "!", "eu\u00b7re", "Mey\u00b7nung", "treugt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich will des wegen nicht verderben.", "tokens": ["Ich", "will", "des", "we\u00b7gen", "nicht", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "APPR", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Seyd ihr mein Leitstern nicht/", "tokens": ["Seyd", "ihr", "mein", "Leits\u00b7tern", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So scheinet mir noch wol ein ander Licht.", "tokens": ["So", "schei\u00b7net", "mir", "noch", "wol", "ein", "an\u00b7der", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wende dich zu einen Hertzen/", "tokens": ["Wen\u00b7de", "dich", "zu", "ei\u00b7nen", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das mit hellen Liebes-Kertzen", "tokens": ["Das", "mit", "hel\u00b7len", "Lie\u00b7bes\u00b7Kert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in gleichen Flammen brennt.", "tokens": ["Und", "in", "glei\u00b7chen", "Flam\u00b7men", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein Hertze halt nur ein", "tokens": ["Mein", "Hert\u00b7ze", "halt", "nur", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "ADV", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie wird nicht zu bewegen seyn.", "tokens": ["Sie", "wird", "nicht", "zu", "be\u00b7we\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bem\u00fche dich nicht ferner/", "tokens": ["Be\u00b7m\u00fc\u00b7he", "dich", "nicht", "fer\u00b7ner", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und pflantze deine Hoffnungs-K\u00f6rner", "tokens": ["Und", "pflant\u00b7ze", "dei\u00b7ne", "Hoff\u00b7nungs\u00b7K\u00f6r\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So ungewi\u00df nicht hin.", "tokens": ["So", "un\u00b7ge\u00b7wi\u00df", "nicht", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es giebt noch mehr der Sch\u00f6nen/", "tokens": ["Es", "giebt", "noch", "mehr", "der", "Sch\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vielleichte kriegt dein Sehnen", "tokens": ["Viel\u00b7leich\u00b7te", "kriegt", "dein", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Hertze zum Gewinn/", "tokens": ["Ein", "Hert\u00b7ze", "zum", "Ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das nicht so unbarmhertzig ist.", "tokens": ["Das", "nicht", "so", "un\u00b7barm\u00b7hert\u00b7zig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Gl\u00fccke und die Sternen/", "tokens": ["Das", "Gl\u00fc\u00b7cke", "und", "die", "Ster\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die werden sich auf ewig nicht entfernen.", "tokens": ["Die", "wer\u00b7den", "sich", "auf", "e\u00b7wig", "nicht", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "APPR", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sey wohlgemuth/ verzage nicht/", "tokens": ["Sey", "wohl\u00b7ge\u00b7muth", "/", "ver\u00b7za\u00b7ge", "nicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$(", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auf den Abend-voller Sorgen", "tokens": ["Auf", "den", "A\u00b7ben\u00b7dvol\u00b7ler", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Folgt ein Licht/", "tokens": ["Folgt", "ein", "Licht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Das durch einen hellen Morgen", "tokens": ["Das", "durch", "ei\u00b7nen", "hel\u00b7len", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Deine Traurigkeit vers\u00fc\u00dft.", "tokens": ["Dei\u00b7ne", "Trau\u00b7rig\u00b7keit", "ver\u00b7s\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}