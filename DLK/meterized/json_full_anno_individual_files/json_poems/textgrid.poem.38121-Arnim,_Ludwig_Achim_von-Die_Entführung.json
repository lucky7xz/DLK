{"textgrid.poem.38121": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Entf\u00fchrung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin durch Frauen Willen", "tokens": ["Ich", "bin", "durch", "Frau\u00b7en", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geritten in fremde Land,", "tokens": ["Ge\u00b7rit\u00b7ten", "in", "frem\u00b7de", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mich hat ein edler Ritter", "tokens": ["Mich", "hat", "ein", "ed\u00b7ler", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Boten hergesandt.", "tokens": ["Zu", "Bo\u00b7ten", "her\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der entbeut euch sein viel werthen Gru\u00df,", "tokens": ["Der", "ent\u00b7beut", "euch", "sein", "viel", "wert\u00b7hen", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Nun entbiet't ihm was ihr w\u00f6llet,", "tokens": ["Nun", "ent\u00b7biet't", "ihm", "was", "ihr", "w\u00f6l\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von euch, so hat er Freuden g'nug.", "tokens": ["Von", "euch", ",", "so", "hat", "er", "Freu\u00b7den", "g'\u00b7nug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADV", "VAFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Was soll ich ihm entbieten?", "tokens": ["Was", "soll", "ich", "ihm", "ent\u00b7bie\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Redt als das M\u00e4gdlein rein,", "tokens": ["Redt", "als", "das", "M\u00e4gd\u00b7lein", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "S\u00e4h ich den Held mit Augen,", "tokens": ["S\u00e4h", "ich", "den", "Held", "mit", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Das erfreuet das Herze mein.", "tokens": ["Das", "er\u00b7freu\u00b7et", "das", "Her\u00b7ze", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDS", "VVFIN", "PPOSAT", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Und siehst du dort die Linden,", "tokens": ["Und", "siehst", "du", "dort", "die", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wohl vor der Burge stahn,", "tokens": ["Wohl", "vor", "der", "Bur\u00b7ge", "stahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Da hei\u00df dann deinen Herren", "tokens": ["Da", "hei\u00df", "dann", "dei\u00b7nen", "Her\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Des Abends sp\u00e4t darunter gahn.", "tokens": ["Des", "A\u00b7bends", "sp\u00e4t", "da\u00b7run\u00b7ter", "gahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da will ich mit ihm kosen,", "tokens": ["Da", "will", "ich", "mit", "ihm", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und sagen meinen Muth;", "tokens": ["Und", "sa\u00b7gen", "mei\u00b7nen", "Muth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Ich bin vor gro\u00dfen Sorgen", "tokens": ["Ich", "bin", "vor", "gro\u00b7\u00dfen", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Sicher wol behut't.", "tokens": ["Si\u00b7cher", "wol", "be\u00b7hut'", "t."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ADV", "ADV", "VVFIN", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da der edel Ritter", "tokens": ["Da", "der", "e\u00b7del", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Da unter die Linden kam,", "tokens": ["Da", "un\u00b7ter", "die", "Lin\u00b7den", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was fand er unter der Linden?", "tokens": ["Was", "fand", "er", "un\u00b7ter", "der", "Lin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein M\u00e4gdlein die war wolgethan.", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "die", "war", "wol\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ab zog er den Mantel sein,", "tokens": ["Ab", "zog", "er", "den", "Man\u00b7tel", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Er warf ihn in das Gras.", "tokens": ["Er", "warf", "ihn", "in", "das", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da lagen die zwey die lange Nacht,", "tokens": ["Da", "la\u00b7gen", "die", "zwey", "die", "lan\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "CARD", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Bis an den lichten Tag.", "tokens": ["Bis", "an", "den", "lich\u00b7ten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Er halst, er k\u00fc\u00dft, er dr\u00fccket,", "tokens": ["Er", "halst", ",", "er", "k\u00fc\u00dft", ",", "er", "dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Sie lieblich an sein Leib;", "tokens": ["Sie", "lieb\u00b7lich", "an", "sein", "Leib", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Du bist auf meine Treue,", "tokens": ["Du", "bist", "auf", "mei\u00b7ne", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Das allerliebste Weib.", "tokens": ["Das", "al\u00b7ler\u00b7liebs\u00b7te", "Weib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun ist dir dein Will an mir zergangen,", "tokens": ["Nun", "ist", "dir", "dein", "Will", "an", "mir", "zer\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Redt als das M\u00e4gdlein rein,", "tokens": ["Redt", "als", "das", "M\u00e4gd\u00b7lein", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So thust du wol dem geleiche,", "tokens": ["So", "thust", "du", "wol", "dem", "ge\u00b7lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sam du mir treu wollst sein.", "tokens": ["Sam", "du", "mir", "treu", "wollst", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "ADJD", "VMFIN", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und kehrst mir bald den R\u00fccken", "tokens": ["Und", "kehrst", "mir", "bald", "den", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und reist dahin von mir.", "tokens": ["Und", "reist", "da\u00b7hin", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So thu ich als ein kleines Kind,", "tokens": ["So", "thu", "ich", "als", "ein", "klei\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wein, ach edler Herr! nach dir.", "tokens": ["Und", "wein", ",", "ach", "ed\u00b7ler", "Herr", "!", "nach", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "ADV", "ADJA", "NN", "$.", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So verbiet ich euren Augen", "tokens": ["So", "ver\u00b7biet", "ich", "eu\u00b7ren", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr wunder sch\u00f6nes Weib!", "tokens": ["Ihr", "wun\u00b7der", "sch\u00f6\u00b7nes", "Weib", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie nach mir nicht weinen,", "tokens": ["Da\u00df", "sie", "nach", "mir", "nicht", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich komm her wieder in kurzer Zeit.", "tokens": ["Ich", "komm", "her", "wie\u00b7der", "in", "kur\u00b7zer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und siehst du dort mein R\u00f6\u00dflein", "tokens": ["Und", "siehst", "du", "dort", "mein", "R\u00f6\u00df\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nach dem Z\u00fcgel schlagen,", "tokens": ["Nach", "dem", "Z\u00fc\u00b7gel", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Das soll uns, mein allerliebstes Lieb!", "tokens": ["Das", "soll", "uns", ",", "mein", "al\u00b7ler\u00b7liebs\u00b7tes", "Lieb", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Aus gr\u00f6\u00dften N\u00f6then tragen.", "tokens": ["Aus", "gr\u00f6\u00df\u00b7ten", "N\u00f6\u00b7then", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da hub sich in der Burge,", "tokens": ["Da", "hub", "sich", "in", "der", "Bur\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wol wunder gro\u00dfer Schall,", "tokens": ["Wol", "wun\u00b7der", "gro\u00b7\u00dfer", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der W\u00e4chter an der Zinne,", "tokens": ["Der", "W\u00e4ch\u00b7ter", "an", "der", "Zin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sang: die Burg ist aufgethan!", "tokens": ["Der", "sang", ":", "die", "Burg", "ist", "auf\u00b7ge\u00b7than", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat jemand hier verloren,", "tokens": ["Hat", "je\u00b7mand", "hier", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der soll sein nehmen wahr.", "tokens": ["Der", "soll", "sein", "neh\u00b7men", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da sprach der Edel von Kerenstein:", "tokens": ["Da", "sprach", "der", "E\u00b7del", "von", "Ke\u00b7ren\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ich hab mein' sch\u00f6ne Tochter verloren,", "tokens": ["Ich", "hab", "mein'", "sch\u00f6\u00b7ne", "Toch\u00b7ter", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Darum so hast du W\u00e4chter genommen das rote Gold,", "tokens": ["Da\u00b7rum", "so", "hast", "du", "W\u00e4ch\u00b7ter", "ge\u00b7nom\u00b7men", "das", "ro\u00b7te", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VAFIN", "PPER", "NE", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Darum so must du leiden den bittern Tod.", "tokens": ["Da\u00b7rum", "so", "must", "du", "lei\u00b7den", "den", "bit\u00b7tern", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Nun wei\u00df es Christ vom Himmel wol", "tokens": ["Nun", "wei\u00df", "es", "Christ", "vom", "Him\u00b7mel", "wol"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-