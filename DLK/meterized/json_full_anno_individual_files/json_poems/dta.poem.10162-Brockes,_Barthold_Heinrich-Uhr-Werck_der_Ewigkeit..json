{"dta.poem.10162": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uhr-Werck der Ewigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unl\u00e4ngst, als ein guter Freund, der noch offt an mich\ngedencket,", "tokens": ["Un\u00b7l\u00e4ngst", ",", "als", "ein", "gu\u00b7ter", "Freund", ",", "der", "noch", "offt", "an", "mich", "ge\u00b7den\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der die Ehrlichkeit fast selbst, mich mit einer Uhr beschencket,", "tokens": ["Der", "die", "Ehr\u00b7lich\u00b7keit", "fast", "selbst", ",", "mich", "mit", "ei\u00b7ner", "Uhr", "be\u00b7schen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADV", "$,", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Die so k\u00fcnstlich ist, als kostbar, ja ein rechtes Meister-St\u00fcck,", "tokens": ["Die", "so", "k\u00fcnst\u00b7lich", "ist", ",", "als", "kost\u00b7bar", ",", "ja", "ein", "rech\u00b7tes", "Meis\u00b7ter\u00b7St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "ADJD", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und ich meinen frohen Blick", "tokens": ["Und", "ich", "mei\u00b7nen", "fro\u00b7hen", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aemsig, voll Verwundrung, lenckte", "tokens": ["A\u00b7em\u00b7sig", ",", "voll", "Ver\u00b7wund\u00b7rung", ",", "lenck\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "ADJD", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf die unterschiedne Scheiben, (denn nicht nur die gan-", "tokens": ["Auf", "die", "un\u00b7ter\u00b7schied\u00b7ne", "Schei\u00b7ben", ",", "(", "denn", "nicht", "nur", "die", "gan"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "$(", "ADV", "PTKNEG", "ADV", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Sondern, nebst dem Monats-Tag, auch Minuten und", "tokens": ["Son\u00b7dern", ",", "nebst", "dem", "Mo\u00b7nats\u00b7Tag", ",", "auch", "Mi\u00b7nu\u00b7ten", "und"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "NN", "$,", "ADV", "NN", "KON"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Sind besonders vorgestellt) ward ich sonderlich ger\u00fchrt", "tokens": ["Sind", "be\u00b7son\u00b7ders", "vor\u00b7ge\u00b7stellt", ")", "ward", "ich", "son\u00b7der\u00b7lich", "ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP", "$(", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Durch die Scheibe, deren Zeiger sich so schnell im Circkel f\u00fchrt,", "tokens": ["Durch", "die", "Schei\u00b7be", ",", "de\u00b7ren", "Zei\u00b7ger", "sich", "so", "schnell", "im", "Cir\u00b7ckel", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELAT", "NN", "PRF", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Da\u00df, eh der Minuten-Zeiger einen einzgen Schritt gemacht,", "tokens": ["Da\u00df", ",", "eh", "der", "Mi\u00b7nu\u00b7ten\u00b7Zei\u00b7ger", "ei\u00b7nen", "einz\u00b7gen", "Schritt", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Er bereits mit sechszig Schritten seinen Kreis-Lauff gantz", "tokens": ["Er", "be\u00b7reits", "mit", "sechs\u00b7zig", "Schrit\u00b7ten", "sei\u00b7nen", "Kreis\u00b7Lauff", "gantz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "CARD", "NN", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Ich bewunderte die Wei\u00dfheit, die der Sch\u00f6pfer uns", "tokens": ["Ich", "be\u00b7wun\u00b7der\u00b7te", "die", "Wei\u00df\u00b7heit", ",", "die", "der", "Sch\u00f6p\u00b7fer", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Da die Menschheit solche Wercke macht, erfindet und er-", "tokens": ["Da", "die", "Menschheit", "sol\u00b7che", "Wer\u00b7cke", "macht", ",", "er\u00b7fin\u00b7det", "und", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN", "VVFIN", "$,", "VVFIN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ich danckte GOtt daf\u00fcr. Sonderlich da man die Zeit", "tokens": ["Und", "ich", "danck\u00b7te", "Gott", "da\u00b7f\u00fcr", ".", "Son\u00b7der\u00b7lich", "da", "man", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN", "PAV", "$.", "ADV", "KOUS", "PIS", "ART", "NN"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und derselben stillen Lauff (mit so fester Richtigkeit", "tokens": ["Und", "der\u00b7sel\u00b7ben", "stil\u00b7len", "Lauff", "(", "mit", "so", "fes\u00b7ter", "Rich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "$(", "APPR", "ADV", "ADJA", "NN"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Wunderw\u00fcrdig eingetheilt, sichtbar dargestellet findet,", "tokens": ["Wun\u00b7der\u00b7w\u00fcr\u00b7dig", "ein\u00b7ge\u00b7theilt", ",", "sicht\u00b7bar", "dar\u00b7ge\u00b7stel\u00b7let", "fin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Welche sonst, in schneller Stille, kommt, vergeht, entsteht,", "tokens": ["Wel\u00b7che", "sonst", ",", "in", "schnel\u00b7ler", "Stil\u00b7le", ",", "kommt", ",", "ver\u00b7geht", ",", "ent\u00b7steht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Wie es scheint, und wie man meint. Aber eben diese Scheibe,", "tokens": ["Wie", "es", "scheint", ",", "und", "wie", "man", "meint", ".", "A\u00b7ber", "e\u00b7ben", "die\u00b7se", "Schei\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "KON", "PWAV", "PIS", "VVFIN", "$.", "KON", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Welche die Secunden zeigt, zeigt, selbst in der Fl\u00fcchtigkeit,", "tokens": ["Wel\u00b7che", "die", "Se\u00b7cun\u00b7den", "zeigt", ",", "zeigt", ",", "selbst", "in", "der", "Fl\u00fcch\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+---+-+-++-+-+", "measure": "dactylic.init"}, "line.9": {"text": "Ein best\u00e4ndigs Bild der Zeit,", "tokens": ["Ein", "be\u00b7st\u00e4n\u00b7digs", "Bild", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Und da\u00df wir mit Recht nicht k\u00f6nnen", "tokens": ["Und", "da\u00df", "wir", "mit", "Recht", "nicht", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "PTKNEG", "VMFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Das vergangene vergangen, und die Zeiten fl\u00fcchtig nennen.", "tokens": ["Das", "ver\u00b7gan\u00b7ge\u00b7ne", "ver\u00b7gan\u00b7gen", ",", "und", "die", "Zei\u00b7ten", "fl\u00fcch\u00b7tig", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "$,", "KON", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Denn ist der Secunden Kreis gleich vollendet und", "tokens": ["Denn", "ist", "der", "Se\u00b7cun\u00b7den", "Kreis", "gleich", "voll\u00b7en\u00b7det", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ADV", "VVPP", "KON"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sieht man, am Minuten-Zeiger, da\u00df sie gegenw\u00e4rtig sey:", "tokens": ["Sieht", "man", ",", "am", "Mi\u00b7nu\u00b7ten\u00b7Zei\u00b7ger", ",", "da\u00df", "sie", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "sey", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "APPRART", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Folglich sieht man, recht mit Augen, wie es m\u00f6glich sey,", "tokens": ["Folg\u00b7lich", "sieht", "man", ",", "recht", "mit", "Au\u00b7gen", ",", "wie", "es", "m\u00f6g\u00b7lich", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "ADV", "APPR", "NN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Und dennoch, als nicht vergangen, ohne Wandelung bestehen.", "tokens": ["Und", "den\u00b7noch", ",", "als", "nicht", "ver\u00b7gan\u00b7gen", ",", "oh\u00b7ne", "Wan\u00b7de\u00b7lung", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PTKNEG", "VVPP", "$,", "KOUI", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Zeigt uns dieses nicht ein Bild, zu des grossen Sch\u00f6pf-", "tokens": ["Zeigt", "uns", "die\u00b7ses", "nicht", "ein", "Bild", ",", "zu", "des", "gros\u00b7sen", "Sch\u00f6pf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDS", "PTKNEG", "ART", "NN", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Da\u00df, ja gar auf welche Weise,", "tokens": ["Da\u00df", ",", "ja", "gar", "auf", "wel\u00b7che", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "ADV", "APPR", "PWAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir von unsern Lebens-Zeiten (so, dem Schein nach nur,", "tokens": ["Wir", "von", "un\u00b7sern", "Le\u00b7bens\u00b7Zei\u00b7ten", "(", "so", ",", "dem", "Schein", "nach", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$(", "ADV", "$,", "ART", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Die verfiogenen Seeunden alle werden wieder sehn", "tokens": ["Die", "ver\u00b7fio\u00b7ge\u00b7nen", "See\u00b7un\u00b7den", "al\u00b7le", "wer\u00b7den", "wie\u00b7der", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIS", "VAFIN", "ADV", "VVINF"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Dort an dem Minuten-Zeiger, welchen wir, nach dieser Zeit,", "tokens": ["Dort", "an", "dem", "Mi\u00b7nu\u00b7ten\u00b7Zei\u00b7ger", ",", "wel\u00b7chen", "wir", ",", "nach", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PWAT", "PPER", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "In der langen Ewigkeit,", "tokens": ["In", "der", "lan\u00b7gen", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sonder Zweifel finden d\u00fcrften. Da wir denn, was hier", "tokens": ["Son\u00b7der", "Zwei\u00b7fel", "fin\u00b7den", "d\u00fcrf\u00b7ten", ".", "Da", "wir", "denn", ",", "was", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$.", "KOUS", "PPER", "ADV", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "Mit Vergn\u00fcgen oder Zittern, qu\u00e4len oder Fr\u00f6hlichkeit,", "tokens": ["Mit", "Ver\u00b7gn\u00fc\u00b7gen", "o\u00b7der", "Zit\u00b7tern", ",", "qu\u00e4\u00b7len", "o\u00b7der", "Fr\u00f6h\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVINF", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Wie wir unsern Wandel hier angestellt gehabt auf Erden,", "tokens": ["Wie", "wir", "un\u00b7sern", "Wan\u00b7del", "hier", "an\u00b7ge\u00b7stellt", "ge\u00b7habt", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "VAPP", "APPR", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Sehen und betrachten werden.", "tokens": ["Se\u00b7hen", "und", "be\u00b7trach\u00b7ten", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hiemit stimmet \u00fcberein das, was dort Johannes", "tokens": ["Hie\u00b7mit", "stim\u00b7met", "\u00fc\u00b7be\u00b7re\u00b7in", "das", ",", "was", "dort", "Jo\u00b7han\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PDS", "$,", "PRELS", "ADV", "NE"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Wann es heisset: Jhre Wercke folgen ihnen dorten nach.", "tokens": ["Wann", "es", "heis\u00b7set", ":", "Ih\u00b7re", "Wer\u00b7cke", "fol\u00b7gen", "ih\u00b7nen", "dor\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Denn, wo alles, wie es hier zu vergehen scheint, verginge,", "tokens": ["Denn", ",", "wo", "al\u00b7les", ",", "wie", "es", "hier", "zu", "ver\u00b7ge\u00b7hen", "scheint", ",", "ver\u00b7gin\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PIS", "$,", "PWAV", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Alles w\u00e4re wie ein Traum, ja wie ein Geschrey, vorbey;", "tokens": ["Al\u00b7les", "w\u00e4\u00b7re", "wie", "ein", "Traum", ",", "ja", "wie", "ein", "Ge\u00b7schrey", ",", "vor\u00b7bey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VAFIN", "KOKOM", "ART", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Schien\u2019 es, als ob in der That alles Wesen aller Dinge", "tokens": ["Schien'", "es", ",", "als", "ob", "in", "der", "That", "al\u00b7les", "We\u00b7sen", "al\u00b7ler", "Din\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOKOM", "KOUS", "APPR", "ART", "NN", "PIAT", "NN", "PIAT", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Kaum der M\u00fche wehrt gewesen, da\u00df es einst erschaffen sey.", "tokens": ["Kaum", "der", "M\u00fc\u00b7he", "wehrt", "ge\u00b7we\u00b7sen", ",", "da\u00df", "es", "einst", "er\u00b7schaf\u00b7fen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "VAPP", "$,", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Aber ach! was f\u00e4llt mir ein! das Gewercke dieser Uhr", "tokens": ["A\u00b7ber", "ach", "!", "was", "f\u00e4llt", "mir", "ein", "!", "das", "Ge\u00b7wer\u00b7cke", "die\u00b7ser", "Uhr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PWS", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Zeiget mir ein mehres noch. Selbst das Uhr-Werck der", "tokens": ["Zei\u00b7get", "mir", "ein", "meh\u00b7res", "noch", ".", "Selbst", "das", "Uhr\u00b7\u00b7Werck", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "PIS", "ADV", "$.", "ADV", "ART", "NN", "ART"], "meter": "+---+-