{"textgrid.poem.63009": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Thiere lebten viele Jahre", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Thiere lebten viele Jahre", "tokens": ["Die", "Thie\u00b7re", "leb\u00b7ten", "vie\u00b7le", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In friedlicher Demokratie;", "tokens": ["In", "fried\u00b7li\u00b7cher", "De\u00b7mo\u00b7kra\u00b7tie", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch endlich kamen sie einander in die Haare,", "tokens": ["Doch", "end\u00b7lich", "ka\u00b7men", "sie", "ein\u00b7an\u00b7der", "in", "die", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihre Republik versank in Anarchie.", "tokens": ["Und", "ih\u00b7re", "Re\u00b7pub\u00b7lik", "ver\u00b7sank", "in", "An\u00b7ar\u00b7chie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der L\u00f6we machte sich den innern Streit zu Nutze", "tokens": ["Der", "L\u00f6\u00b7we", "mach\u00b7te", "sich", "den", "in\u00b7nern", "Streit", "zu", "Nut\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKZU", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und bot sich ohne Sold dem kleinern Vieh,", "tokens": ["Und", "bot", "sich", "oh\u00b7ne", "Sold", "dem", "klei\u00b7nern", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als dem gedr\u00fcckten Theil, zum Schutze,", "tokens": ["Als", "dem", "ge\u00b7dr\u00fcck\u00b7ten", "Theil", ",", "zum", "Schut\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Retter seiner Freyheit an.", "tokens": ["Zum", "Ret\u00b7ter", "sei\u00b7ner", "Frey\u00b7heit", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er wollte blos des Volkes Diener hei\u00dfen,", "tokens": ["Er", "woll\u00b7te", "blos", "des", "Vol\u00b7kes", "Die\u00b7ner", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und brauchte weislich seinen Zahn", "tokens": ["Und", "brauch\u00b7te", "weis\u00b7lich", "sei\u00b7nen", "Zahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Im Anfang nur die R\u00e4uber zu zerrei\u00dfen.", "tokens": ["Im", "An\u00b7fang", "nur", "die", "R\u00e4u\u00b7ber", "zu", "zer\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Als dies die frohen B\u00fcrger sahn,", "tokens": ["Als", "dies", "die", "fro\u00b7hen", "B\u00fcr\u00b7ger", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ernannten sie zum wohlverdienten Lohne", "tokens": ["Er\u00b7nann\u00b7ten", "sie", "zum", "wohl\u00b7ver\u00b7dien\u00b7ten", "Loh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Den Diener feyerlich zum Chan,", "tokens": ["Den", "Die\u00b7ner", "fey\u00b7er\u00b7lich", "zum", "Chan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Versicherten die W\u00fcrde seinem Sohne,", "tokens": ["Ver\u00b7si\u00b7cher\u00b7ten", "die", "W\u00fcr\u00b7de", "sei\u00b7nem", "Soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Und gaben ihm die Macht die Aemter zu verleihn,", "tokens": ["Und", "ga\u00b7ben", "ihm", "die", "Macht", "die", "A\u00b7em\u00b7ter", "zu", "ver\u00b7leihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Um kr\u00e4ftiger besch\u00fctzt zu seyn.", "tokens": ["Um", "kr\u00e4f\u00b7ti\u00b7ger", "be\u00b7sch\u00fctzt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Nun sprach der neue F\u00fcrst aus einem andern Tone:", "tokens": ["Nun", "sprach", "der", "neu\u00b7e", "F\u00fcrst", "aus", "ei\u00b7nem", "an\u00b7dern", "To\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er g\u00fcrtete sein Haupt mit einer Eichenkrone,", "tokens": ["Er", "g\u00fcr\u00b7te\u00b7te", "sein", "Haupt", "mit", "ei\u00b7ner", "Ei\u00b7chen\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Enthob Tribut, und wer ihm widerstand,", "tokens": ["Ent\u00b7hob", "Tri\u00b7but", ",", "und", "wer", "ihm", "wi\u00b7der\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Fiel als Rebell in seine Pranke.", "tokens": ["Fiel", "als", "Re\u00b7bell", "in", "sei\u00b7ne", "Pran\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Der Tiger und der Fuchs, der Wolf, der Elephant", "tokens": ["Der", "Ti\u00b7ger", "und", "der", "Fuchs", ",", "der", "Wolf", ",", "der", "E\u00b7le\u00b7phant"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NE", "$,", "ART", "NE", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ergaben sich aus List, und jeder ward zum Danke", "tokens": ["Er\u00b7ga\u00b7ben", "sich", "aus", "List", ",", "und", "je\u00b7der", "ward", "zum", "Dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NE", "$,", "KON", "PIS", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Zum k\u00f6niglichen Rath ernannt.", "tokens": ["Zum", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Rath", "er\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Itzt halfen sie dem Chan die schw\u00e4chern Thiere hetzen,", "tokens": ["Itzt", "hal\u00b7fen", "sie", "dem", "Chan", "die", "schw\u00e4\u00b7chern", "Thie\u00b7re", "het\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Bekamen ihren Theil an den erpre\u00dften Sch\u00e4tzen,", "tokens": ["Be\u00b7ka\u00b7men", "ih\u00b7ren", "Theil", "an", "den", "er\u00b7pre\u00df\u00b7ten", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und raubten endlich trotz dem Chan.", "tokens": ["Und", "raub\u00b7ten", "end\u00b7lich", "trotz", "dem", "Chan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Ha, rief das arme Volk mit tiefgesenkten Ohren", "tokens": ["Ha", ",", "rief", "das", "ar\u00b7me", "Volk", "mit", "tief\u00b7ge\u00b7senk\u00b7ten", "Oh\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und mit geschundner Haut, was haben wir gethan! \u2013", "tokens": ["Und", "mit", "ge\u00b7schund\u00b7ner", "Haut", ",", "was", "ha\u00b7ben", "wir", "ge\u00b7than", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Allein der Freyheit Kranz war nun einmal verloren,", "tokens": ["Al\u00b7lein", "der", "Frey\u00b7heit", "Kranz", "war", "nun", "ein\u00b7mal", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der L\u00f6we war und blieb Tyrann;", "tokens": ["Der", "L\u00f6\u00b7we", "war", "und", "blieb", "Ty\u00b7rann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "KON", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Er lie\u00df von jedem Thier sich stolz die Pfote lecken,", "tokens": ["Er", "lie\u00df", "von", "je\u00b7dem", "Thier", "sich", "stolz", "die", "Pfo\u00b7te", "le\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "PRF", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und wer nicht kroch, der mu\u00dfte sich verstecken.", "tokens": ["Und", "wer", "nicht", "kroch", ",", "der", "mu\u00df\u00b7te", "sich", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVFIN", "$,", "PRELS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Thiere lebten viele Jahre", "tokens": ["Die", "Thie\u00b7re", "leb\u00b7ten", "vie\u00b7le", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In friedlicher Demokratie;", "tokens": ["In", "fried\u00b7li\u00b7cher", "De\u00b7mo\u00b7kra\u00b7tie", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch endlich kamen sie einander in die Haare,", "tokens": ["Doch", "end\u00b7lich", "ka\u00b7men", "sie", "ein\u00b7an\u00b7der", "in", "die", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihre Republik versank in Anarchie.", "tokens": ["Und", "ih\u00b7re", "Re\u00b7pub\u00b7lik", "ver\u00b7sank", "in", "An\u00b7ar\u00b7chie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "