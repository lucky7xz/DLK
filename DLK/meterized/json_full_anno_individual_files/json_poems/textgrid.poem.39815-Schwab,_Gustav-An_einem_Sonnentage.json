{"textgrid.poem.39815": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "An einem Sonnentage", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gewischt vom Himmel ist der tr\u00fcbe Flor,", "tokens": ["Ge\u00b7wischt", "vom", "Him\u00b7mel", "ist", "der", "tr\u00fc\u00b7be", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Heer der Regenwolken ist verstoben,", "tokens": ["Das", "Heer", "der", "Re\u00b7gen\u00b7wol\u00b7ken", "ist", "ver\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Smaragden steigt der Berg ins Blau empor,", "tokens": ["Sma\u00b7rag\u00b7den", "steigt", "der", "Berg", "ins", "Blau", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit einem Netz von Sonnenglanz umwoben,", "tokens": ["Mit", "ei\u00b7nem", "Netz", "von", "Son\u00b7nen\u00b7glanz", "um\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von goldnen Wipfeln schallt der V\u00f6gel Chor,", "tokens": ["Von", "gold\u00b7nen", "Wip\u00b7feln", "schallt", "der", "V\u00f6\u00b7gel", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Klar sind die B\u00e4che, wie der Himmel droben;", "tokens": ["Klar", "sind", "die", "B\u00e4\u00b7che", ",", "wie", "der", "Him\u00b7mel", "dro\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "ADV", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Durch alle Sch\u00f6pfung ging ein bl\u00fchend: Werde!", "tokens": ["Durch", "al\u00b7le", "Sch\u00f6p\u00b7fung", "ging", "ein", "bl\u00fc\u00b7hend", ":", "Wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "VVPP", "$.", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du seliger Planet \u2013 bist du die Erde?", "tokens": ["Du", "se\u00b7li\u00b7ger", "Pla\u00b7net", "\u2013", "bist", "du", "die", "Er\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Solch Wunder thut noch immer die Natur,", "tokens": ["Solch", "Wun\u00b7der", "thut", "noch", "im\u00b7mer", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verkl\u00e4rt sich mitten in den tr\u00fcbsten Tagen,", "tokens": ["Ver\u00b7kl\u00e4rt", "sich", "mit\u00b7ten", "in", "den", "tr\u00fcbs\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Welt der Geister, der verdumpften, nur", "tokens": ["Der", "Welt", "der", "Geis\u00b7ter", ",", "der", "ver\u00b7dumpf\u00b7ten", ",", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Soll keine Stunde der Erl\u00f6sung schlagen?", "tokens": ["Soll", "kei\u00b7ne", "Stun\u00b7de", "der", "Er\u00b7l\u00f6\u00b7sung", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vom Blitz gezeichnet, von des Hagels Spur,", "tokens": ["Vom", "Blitz", "ge\u00b7zeich\u00b7net", ",", "von", "des", "Ha\u00b7gels", "Spur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Soll stets die\u00df Reich in schwarze L\u00fcfte ragen?", "tokens": ["Soll", "stets", "die\u00df", "Reich", "in", "schwar\u00b7ze", "L\u00fcf\u00b7te", "ra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PDS", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "H\u00f6rt unsre Zeit, noch taub von Einem Wetter,", "tokens": ["H\u00f6rt", "uns\u00b7re", "Zeit", ",", "noch", "taub", "von", "Ei\u00b7nem", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Schon wieder eines nahenden Geschmetter?", "tokens": ["Schon", "wie\u00b7der", "ei\u00b7nes", "na\u00b7hen\u00b7den", "Ge\u00b7schmet\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo ihr ihn sucht, da findet ihr ihn nie,", "tokens": ["Wo", "ihr", "ihn", "sucht", ",", "da", "fin\u00b7det", "ihr", "ihn", "nie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den reinen Himmel und den heitern Frieden.", "tokens": ["Den", "rei\u00b7nen", "Him\u00b7mel", "und", "den", "hei\u00b7tern", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sucht nicht bei'm Leben! Nacht und Kampf ist hie,", "tokens": ["Sucht", "nicht", "bei'm", "Le\u00b7ben", "!", "Nacht", "und", "Kampf", "ist", "hie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "$.", "NN", "KON", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nie wird die Finsterni\u00df vom Licht geschieden!", "tokens": ["Nie", "wird", "die", "Fins\u00b7ter\u00b7ni\u00df", "vom", "Licht", "ge\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch wendet einmal euch zur ", "tokens": ["Doch", "wen\u00b7det", "ein\u00b7mal", "euch", "zur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPER", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die ihr, im Drang nach Tag, so lang gemieden:", "tokens": ["Die", "ihr", ",", "im", "Drang", "nach", "Tag", ",", "so", "lang", "ge\u00b7mie\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPRART", "NN", "APPR", "NN", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vertraut euch ihrer D\u00e4mmernacht und tr\u00e4umet,", "tokens": ["Ver\u00b7traut", "euch", "ih\u00b7rer", "D\u00e4m\u00b7mer\u00b7nacht", "und", "tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dort wohnt die Klarheit, die hier immer s\u00e4umet.", "tokens": ["Dort", "wohnt", "die", "Klar\u00b7heit", ",", "die", "hier", "im\u00b7mer", "s\u00e4u\u00b7met", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Quell der Dichtung wird euch viel bescheert,", "tokens": ["Im", "Quell", "der", "Dich\u00b7tung", "wird", "euch", "viel", "be\u00b7scheert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da sprudelt Freiheit, Liebe, Gl\u00fcck und Jugend;", "tokens": ["Da", "spru\u00b7delt", "Frei\u00b7heit", ",", "Lie\u00b7be", ",", "Gl\u00fcck", "und", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Becher ohne Hefe wird geleert,", "tokens": ["Ein", "Be\u00b7cher", "oh\u00b7ne", "He\u00b7fe", "wird", "ge\u00b7leert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein lautrer Trank hat seltne Kraft und Tugend;", "tokens": ["Sein", "laut\u00b7rer", "Trank", "hat", "selt\u00b7ne", "Kraft", "und", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Blick ins Leben selber wird verkl\u00e4rt:", "tokens": ["Der", "Blick", "ins", "Le\u00b7ben", "sel\u00b7ber", "wird", "ver\u00b7kl\u00e4rt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nicht mehr mit hohlem Aug' ins Schwarze lugend,", "tokens": ["Nicht", "mehr", "mit", "hoh\u00b7lem", "Aug'", "ins", "Schwar\u00b7ze", "lu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bringt einen Stral ihr von ertr\u00e4umter Sonne", "tokens": ["Bringt", "ei\u00b7nen", "Stral", "ihr", "von", "er\u00b7tr\u00e4um\u00b7ter", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ins Erdendunkel aus des Liedes Bronne.", "tokens": ["Ins", "Er\u00b7den\u00b7dun\u00b7kel", "aus", "des", "Lie\u00b7des", "Bron\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und dieser Stral durchschimmert alle Welt,", "tokens": ["Und", "die\u00b7ser", "Stral", "durch\u00b7schim\u00b7mert", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dieser Stral durchleuchtet die Geschichte.", "tokens": ["Und", "die\u00b7ser", "Stral", "durch\u00b7leuch\u00b7tet", "die", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohin ein Streiflicht seines Glanzes f\u00e4llt,", "tokens": ["Wo\u00b7hin", "ein", "Streif\u00b7licht", "sei\u00b7nes", "Glan\u00b7zes", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wird alles Grau der Schatten schnell zum Lichte.", "tokens": ["Wird", "al\u00b7les", "Grau", "der", "Schat\u00b7ten", "schnell", "zum", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.