{"textgrid.poem.62345": {"metadata": {"author": {"name": "Gessner, Salomon", "birth": "N.A.", "death": "N.A."}, "title": "[fragment einer Satire]", "genre": "verse", "period": "N.A.", "pub_year": 1746, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonn ist albereit von uns hinweg gewichen;", "tokens": ["Die", "Sonn", "ist", "al\u00b7be\u00b7reit", "von", "uns", "hin\u00b7weg", "ge\u00b7wi\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PRF", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist im Westen schon, das Abend-Roth verblichen.", "tokens": ["Es", "ist", "im", "Wes\u00b7ten", "schon", ",", "das", "A\u00b7ben\u00b7dRoth", "ver\u00b7bli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Nacht hat jez mit schwarz, den Himmel \u00fcbermahlet,", "tokens": ["Die", "Nacht", "hat", "jez", "mit", "schwarz", ",", "den", "Him\u00b7mel", "\u00fc\u00b7ber\u00b7mah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus dem ein zwitzernd Heer, zerstreuter Sternen strahlet.", "tokens": ["Aus", "dem", "ein", "zwit\u00b7zernd", "Heer", ",", "zer\u00b7streu\u00b7ter", "Ster\u00b7nen", "strah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJD", "NN", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Mond erhellt die Erd, mit seinem schw\u00e4chern Licht,", "tokens": ["Der", "Mond", "er\u00b7hellt", "die", "Erd", ",", "mit", "sei\u00b7nem", "schw\u00e4\u00b7chern", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df man die Gegen-St\u00e4nd, schwach und betr\u00fcglich sicht.", "tokens": ["Da\u00df", "man", "die", "Ge\u00b7gen\u00b7St\u00e4nd", ",", "schwach", "und", "be\u00b7tr\u00fcg\u00b7lich", "sicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Grillen, W\u00fcnsch, verliebte Stutzer, Eulen, Forcht und Nacht-Gesp\u00e4nster,", "tokens": ["Gril\u00b7len", ",", "W\u00fcnsch", ",", "ver\u00b7lieb\u00b7te", "Stut\u00b7zer", ",", "Eu\u00b7len", ",", "Forcht", "und", "Nacht\u00b7Ge\u00b7sp\u00e4ns\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "NN", "$,", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Schwermen jetz durch alle Gassen, und umflattern th\u00fcr und Fenster;", "tokens": ["Schwer\u00b7men", "jetz", "durch", "al\u00b7le", "Gas\u00b7sen", ",", "und", "um\u00b7flat\u00b7tern", "th\u00fcr", "und", "Fens\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PIAT", "NN", "$,", "KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Man h\u00f6rt nichts mehr, als wacher Hunde b\u00e4llen etc.", "tokens": ["Man", "h\u00f6rt", "nichts", "mehr", ",", "als", "wa\u00b7cher", "Hun\u00b7de", "b\u00e4l\u00b7len", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["PIS", "VVFIN", "PIS", "ADV", "$,", "KOUS", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "[ ...]", "tokens": ["..."], "token_info": ["punct", "punct", "punct"], "pos": ["$(", "$(", "$("]}, "line.11": {"text": "So macht sich Harpax selbst, ein marterliches Leben,", "tokens": ["So", "macht", "sich", "Har\u00b7pax", "selbst", ",", "ein", "mar\u00b7ter\u00b7li\u00b7ches", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NE", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er hat sich seinem Geld, als einem Gott ergeben;", "tokens": ["Er", "hat", "sich", "sei\u00b7nem", "Geld", ",", "als", "ei\u00b7nem", "Gott", "er\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was er zur Kinderzucht, nothwendig sollt verwenden,", "tokens": ["Was", "er", "zur", "Kin\u00b7der\u00b7zucht", ",", "noth\u00b7wen\u00b7dig", "sollt", "ver\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "$,", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Di\u00df hei\u00dft der karge Filz, unn\u00f6thiges verschwenden:", "tokens": ["Di\u00df", "hei\u00dft", "der", "kar\u00b7ge", "Filz", ",", "un\u00b7n\u00f6\u00b7thi\u00b7ges", "ver\u00b7schwen\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,", "ADJA", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Die um ein st\u00fcckgen Brod, um Gottes willen flehen,", "tokens": ["Die", "um", "ein", "st\u00fcck\u00b7gen", "Brod", ",", "um", "Got\u00b7tes", "wil\u00b7len", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,", "KOUI", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "L\u00e4\u00dft er mit einem Fluch, weil der nichts kostet gehen.", "tokens": ["L\u00e4\u00dft", "er", "mit", "ei\u00b7nem", "Fluch", ",", "weil", "der", "nichts", "kos\u00b7tet", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "ART", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er hoffet nur auf Gott, wann Krankheit ihn bef\u00e4llt,", "tokens": ["Er", "hof\u00b7fet", "nur", "auf", "Gott", ",", "wann", "Krank\u00b7heit", "ihn", "be\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,", "PWAV", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Warum auf ihn allein? die \u00c4rzte fordern Geld.", "tokens": ["Wa\u00b7rum", "auf", "ihn", "al\u00b7lein", "?", "die", "\u00c4rz\u00b7te", "for\u00b7dern", "Geld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ADV", "$.", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zur Predigt geht er nicht, vor jeder Kirchen-Th\u00fcr,", "tokens": ["Zur", "Pre\u00b7digt", "geht", "er", "nicht", ",", "vor", "je\u00b7der", "Kir\u00b7chen\u00b7T\u00b7h\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Streckt man ihm ohnversch\u00e4mt, den Armen-Seckel f\u00fcr etc.", "tokens": ["Streckt", "man", "ihm", "ohn\u00b7ver\u00b7sch\u00e4mt", ",", "den", "Ar\u00b7men\u00b7Se\u00b7ckel", "f\u00fcr", "et\u00b7c."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PIS", "PPER", "ADJD", "$,", "ART", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sonn ist albereit von uns hinweg gewichen;", "tokens": ["Die", "Sonn", "ist", "al\u00b7be\u00b7reit", "von", "uns", "hin\u00b7weg", "ge\u00b7wi\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PRF", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist im Westen schon, das Abend-Roth verblichen.", "tokens": ["Es", "ist", "im", "Wes\u00b7ten", "schon", ",", "das", "A\u00b7ben\u00b7dRoth", "ver\u00b7bli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFI