t": "Als niemahls unter uns geschehn.", "tokens": ["Als", "nie\u00b7mahls", "un\u00b7ter", "uns", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "So wird dein Wandel auf der Erden", "tokens": ["So", "wird", "dein", "Wan\u00b7del", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gott und der Welt gef\u00e4llig seyn.", "tokens": ["Gott", "und", "der", "Welt", "ge\u00b7f\u00e4l\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was nie genug gelernt kan werden,", "tokens": ["Was", "nie", "ge\u00b7nug", "ge\u00b7lernt", "kan", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVPP", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das pr\u00e4gt man nie zu h\u00e4ufig ein;", "tokens": ["Das", "pr\u00e4gt", "man", "nie", "zu", "h\u00e4u\u00b7fig", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "PTKA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Darum ermahnt dich meine Liebe:", "tokens": ["Da\u00b7rum", "er\u00b7mahnt", "dich", "mei\u00b7ne", "Lie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gedencke flei\u00dfig an den Tod,", "tokens": ["Ge\u00b7den\u00b7cke", "flei\u00b7\u00dfig", "an", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Empfang ihn mit gela\u00dfnem Triebe", "tokens": ["Emp\u00b7fang", "ihn", "mit", "ge\u00b7la\u00df\u00b7nem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und seufze dies in lezter Noth:", "tokens": ["Und", "seuf\u00b7ze", "dies", "in", "lez\u00b7ter", "Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Hier lieg ich, gro\u00dfer Gott, und schwize", "tokens": ["Hier", "lieg", "ich", ",", "gro\u00b7\u00dfer", "Gott", ",", "und", "schwi\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Wa\u00dfer meines Unrechts aus;", "tokens": ["Das", "Wa\u00b7\u00dfer", "mei\u00b7nes", "Un\u00b7rechts", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchle deines Eifers Hize,", "tokens": ["Ich", "f\u00fch\u00b7le", "dei\u00b7nes", "Ei\u00b7fers", "Hi\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie kehrt den Leib in Asch und Graus.", "tokens": ["Sie", "kehrt", "den", "Leib", "in", "Asch", "und", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es plagen Satan und Gewi\u00dfen;", "tokens": ["Es", "pla\u00b7gen", "Sa\u00b7tan", "und", "Ge\u00b7wi\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Herr, geh nicht zornig ins Gericht;", "tokens": ["Herr", ",", "geh", "nicht", "zor\u00b7nig", "ins", "Ge\u00b7richt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du thatest mir dein Wort zu wi\u00dfen,", "tokens": ["Du", "tha\u00b7test", "mir", "dein", "Wort", "zu", "wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich glaubte, mehr vermocht ich nicht.", "tokens": ["Ich", "glaub\u00b7te", ",", "mehr", "ver\u00b7mocht", "ich", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Ich habe nach dem kleinen Maa\u00dfe", "tokens": ["Ich", "ha\u00b7be", "nach", "dem", "klei\u00b7nen", "Maa\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Geist, Erfahrung und Verstand", "tokens": ["Von", "Geist", ",", "Er\u00b7fah\u00b7rung", "und", "Ver\u00b7stand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Weg der engen Himmelsstra\u00dfe", "tokens": ["Den", "Weg", "der", "en\u00b7gen", "Him\u00b7mels\u00b7stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So weit beschritten als erkand.", "tokens": ["So", "weit", "be\u00b7schrit\u00b7ten", "als", "er\u00b7kand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verdien ich keine Gnadenblicke,", "tokens": ["Ver\u00b7di\u00b7en", "ich", "kei\u00b7ne", "Gna\u00b7den\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "So sieh doch, eh du mich verbannst,", "tokens": ["So", "sieh", "doch", ",", "eh", "du", "mich", "ver\u00b7bannst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vorher auf Golgatha zur\u00fccke", "tokens": ["Vor\u00b7her", "auf", "Gol\u00b7ga\u00b7tha", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "VVFIN"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "Und dann versto\u00df mich, wenn du kanst.", "tokens": ["Und", "dann", "ver\u00b7sto\u00df", "mich", ",", "wenn", "du", "kanst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}