{"textgrid.poem.43965": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Begehre nicht so viel zu h\u00f6ren;", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begehre nicht so viel zu h\u00f6ren;", "tokens": ["Be\u00b7geh\u00b7re", "nicht", "so", "viel", "zu", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wenig weis, der s\u00fcndigt schlecht,", "tokens": ["Wer", "we\u00b7nig", "weis", ",", "der", "s\u00fcn\u00b7digt", "schlecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKVZ", "$,", "PRELS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Umfang unsrer Wei\u00dfheitslehren", "tokens": ["Der", "Um\u00b7fang", "uns\u00b7rer", "Wei\u00df\u00b7heits\u00b7leh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nicht vor jeden Kopf gerecht.", "tokens": ["Ist", "nicht", "vor", "je\u00b7den", "Kopf", "ge\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Warheit schadet viel Gem\u00fcthern", "tokens": ["Die", "War\u00b7heit", "scha\u00b7det", "viel", "Ge\u00b7m\u00fc\u00b7thern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie bl\u00f6den Augen scharfes Licht;", "tokens": ["Wie", "bl\u00f6\u00b7den", "Au\u00b7gen", "schar\u00b7fes", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Behilf dich mit geringern G\u00fctern,", "tokens": ["Be\u00b7hilf", "dich", "mit", "ge\u00b7rin\u00b7gern", "G\u00fc\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu diesem Schaze kommst du nicht.", "tokens": ["Zu", "die\u00b7sem", "Scha\u00b7ze", "kommst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du kanst gleichwohl zufrieden leben", "tokens": ["Du", "kanst", "gleich\u00b7wohl", "zu\u00b7frie\u00b7den", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und einmahl froh zu Grabe gehn", "tokens": ["Und", "ein\u00b7mahl", "froh", "zu", "Gra\u00b7be", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und brauchst, ach glaube doch, nicht eben", "tokens": ["Und", "brauchst", ",", "ach", "glau\u00b7be", "doch", ",", "nicht", "e\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ITJ", "VVFIN", "ADV", "$,", "PTKNEG", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den hohen Leibniz zu verstehn.", "tokens": ["Den", "ho\u00b7hen", "Leib\u00b7niz", "zu", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast genung vor dein Geschlechte,", "tokens": ["Du", "hast", "ge\u00b7nung", "vor", "dein", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nachdem dein lobenswerther Flei\u00df", "tokens": ["Nach\u00b7dem", "dein", "lo\u00b7bens\u00b7wert\u00b7her", "Flei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Wirthschaft und des H\u00f6chsten Rechte", "tokens": ["Die", "Wirth\u00b7schaft", "und", "des", "H\u00f6chs\u00b7ten", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So wie des Umgangs Regeln weis.", "tokens": ["So", "wie", "des", "Um\u00b7gangs", "Re\u00b7geln", "weis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verrichte nur dein Amt mit Freuden,", "tokens": ["Ver\u00b7rich\u00b7te", "nur", "dein", "Amt", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Zuversicht auf Gottes Schuz;", "tokens": ["Mit", "Zu\u00b7ver\u00b7sicht", "auf", "Got\u00b7tes", "Schuz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kommt ohngefehr ein schweres Leiden,", "tokens": ["Kommt", "ohn\u00b7ge\u00b7fehr", "ein", "schwe\u00b7res", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bieth ihm mit der Hofnung Truz.", "tokens": ["So", "bieth", "ihm", "mit", "der", "Hof\u00b7nung", "Truz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verliere nie den wahren Glauben,", "tokens": ["Ver\u00b7lie\u00b7re", "nie", "den", "wah\u00b7ren", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er dient dir zur Gerechtigkeit,", "tokens": ["Er", "dient", "dir", "zur", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn dich lose M\u00e4uler schrauben,", "tokens": ["Und", "wenn", "dich", "lo\u00b7se", "M\u00e4u\u00b7ler", "schrau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So siege mit Gela\u00dfenheit.", "tokens": ["So", "sie\u00b7ge", "mit", "Ge\u00b7la\u00b7\u00dfen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein klug- und th\u00e4tiges Erbarmen", "tokens": ["Ein", "klug", "und", "th\u00e4\u00b7ti\u00b7ges", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kan wider S\u00fcnd und Fluch bestehn;", "tokens": ["Kan", "wi\u00b7der", "S\u00fcnd", "und", "Fluch", "be\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Las, wenn du kanst, nicht einen Armen", "tokens": ["Las", ",", "wenn", "du", "kanst", ",", "nicht", "ei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "VMFIN", "$,", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Betr\u00fcbt und h\u00fclflos von dir gehn.", "tokens": ["Be\u00b7tr\u00fcbt", "und", "h\u00fcl\u00b7flos", "von", "dir", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vergieb und habe mit den Schwachen", "tokens": ["Ver\u00b7gieb", "und", "ha\u00b7be", "mit", "den", "Schwa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KON", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So viel als mit dir selbst Gedult;", "tokens": ["So", "viel", "als", "mit", "dir", "selbst", "Ge\u00b7dult", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "KOKOM", "APPR", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Will Gl\u00fcck und Wetter gar nicht lachen,", "tokens": ["Will", "Gl\u00fcck", "und", "Wet\u00b7ter", "gar", "nicht", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So sey dein Trost: Ich bin nicht schuld.", "tokens": ["So", "sey", "dein", "Trost", ":", "Ich", "bin", "nicht", "schuld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Erg\u00f6ze dich mit Hofnungsblicken", "tokens": ["Er\u00b7g\u00f6\u00b7ze", "dich", "mit", "Hof\u00b7nungs\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An jenes Lebens Lust und Pracht;", "tokens": ["An", "je\u00b7nes", "Le\u00b7bens", "Lust", "und", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort wird dich andre Sch\u00f6nheit schm\u00fccken", "tokens": ["Dort", "wird", "dich", "and\u00b7re", "Sch\u00f6n\u00b7heit", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als die, so hier dich lieblich macht.", "tokens": ["Als", "die", ",", "so", "hier", "dich", "lieb\u00b7lich", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "ADV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort wirstu nicht mehr St\u00fcckwerck wi\u00dfen,", "tokens": ["Dort", "wirs\u00b7tu", "nicht", "mehr", "St\u00fcck\u00b7werck", "wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du wirst der Wunder Ursprung sehn,", "tokens": ["Du", "wirst", "der", "Wun\u00b7der", "Ur\u00b7sprung", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dort werd ich dich noch reiner k\u00fc\u00dfen,", "tokens": ["Dort", "werd", "ich", "dich", "noch", "rei\u00b7ner", "k\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als niemahls unter uns geschehn.", "tokens": ["Als", "nie\u00b7mahls", "un\u00b7ter", "uns", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So wird dein Wandel auf der Erden", "tokens": ["So", "wird", "dein", "Wan\u00b7del", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gott und der Welt gef\u00e4llig seyn.", "tokens": ["Gott", "und", "der", "Welt", "ge\u00b7f\u00e4l\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was nie genug gelernt kan werden,", "tokens": ["Was", "nie", "ge\u00b7nug", "ge\u00b7lernt", "kan", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVPP", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das pr\u00e4gt man nie zu h\u00e4ufig ein;", "tokens": ["Das", "pr\u00e4gt", "man", "nie", "zu", "h\u00e4u\u00b7fig", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "PTKA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Darum ermahnt dich meine Liebe:", "tokens": ["Da\u00b7rum", "er\u00b7mahnt", "dich", "mei\u00b7ne", "Lie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gedencke flei\u00dfig an den Tod,", "tokens": ["Ge\u00b7den\u00b7cke", "flei\u00b7\u00dfig", "an", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Empfang ihn mit gela\u00dfnem Triebe", "tokens": ["Emp\u00b7fang", "ihn", "mit", "ge\u00b7la\u00df\u00b7nem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und seufze dies in lezter Noth:", "tokens": ["Und", "seuf\u00b7ze", "dies", "in", "lez\u00b7ter", "Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Hier lieg ich, gro\u00dfer Gott, und schwize", "tokens": ["Hier", "lieg", "ich", ",", "gro\u00b7\u00dfer", "Gott", ",", "und", "schwi\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Wa\u00dfer meines Unrechts aus;", "tokens": ["Das", "Wa\u00b7\u00dfer", "mei\u00b7nes", "Un\u00b7rechts", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchle deines Eifers Hize,", "tokens": ["Ich", "f\u00fch\u00b7le", "dei\u00b7nes", "Ei\u00b7fers", "Hi\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie kehrt den Leib in Asch und Graus.", "tokens": ["Sie", "kehrt", "den", "Leib", "in", "Asch", "und", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es plagen Satan und Gewi\u00dfen;", "tokens": ["Es", "pla\u00b7gen", "Sa\u00b7tan", "und", "Ge\u00b7wi\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Herr, geh nicht zornig ins Gericht;", "tokens": ["Herr", ",", "geh", "nicht", "zor\u00b7nig", "ins", "Ge\u00b7richt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du thatest mir dein Wort zu wi\u00dfen,", "tokens": ["Du", "tha\u00b7test", "mir", "dein", "Wort", "zu", "wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich glaubte, mehr vermocht ich nicht.", "tokens": ["Ich", "glaub\u00b7te", ",", "mehr", "ver\u00b7mocht", "ich", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich habe nach dem kleinen Maa\u00dfe", "tokens": ["Ich", "ha\u00b7be", "nach", "dem", "klei\u00b7nen", "Maa\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Geist, Erfahrung und Verstand", "tokens": ["Von", "Geist", ",", "Er\u00b7fah\u00b7rung", "und", "Ver\u00b7stand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Weg der engen Himmelsstra\u00dfe", "tokens": ["Den", "Weg", "der", "en\u00b7gen", "Him\u00b7mels\u00b7stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So weit beschritten als erkand.", "tokens": ["So", "weit", "be\u00b7schrit\u00b7ten", "als", "er\u00b7kand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verdien ich keine Gnadenblicke,", "tokens": ["Ver\u00b7di\u00b7en", "ich", "kei\u00b7ne", "Gna\u00b7den\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "So sieh doch, eh du mich verbannst,", "tokens": ["So", "sieh", "doch", ",", "eh", "du", "mich", "ver\u00b7bannst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vorher auf Golgatha zur\u00fccke", "tokens": ["Vor\u00b7her", "auf", "Gol\u00b7ga\u00b7tha", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "VVFIN"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "Und dann versto\u00df mich, wenn du kanst.", "tokens": ["Und", "dann", "ver\u00b7sto\u00df", "mich", ",", "wenn", "du", "kanst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Begehre nicht so viel zu h\u00f6ren;", "tokens": ["Be\u00b7geh\u00b7re", "nicht", "so", "viel", "zu", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wenig weis, der s\u00fcndigt schlecht,", "tokens": ["Wer", "we\u00b7nig", "weis", ",", "der", "s\u00fcn\u00b7digt", "schlecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKVZ", "$,", "PRELS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Umfang unsrer Wei\u00dfheitslehren", "tokens": ["Der", "Um\u00b7fang", "uns\u00b7rer", "Wei\u00df\u00b7heits\u00b7leh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist nicht vor jeden Kopf gerecht.", "tokens": ["Ist", "nicht", "vor", "je\u00b7den", "Kopf", "ge\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Warheit schadet viel Gem\u00fcthern", "tokens": ["Die", "War\u00b7heit", "scha\u00b7det", "viel", "Ge\u00b7m\u00fc\u00b7thern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie bl\u00f6den Augen scharfes Licht;", "tokens": ["Wie", "bl\u00f6\u00b7den", "Au\u00b7gen", "schar\u00b7fes", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Behilf dich mit geringern G\u00fctern,", "tokens": ["Be\u00b7hilf", "dich", "mit", "ge\u00b7rin\u00b7gern", "G\u00fc\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu diesem Schaze kommst du nicht.", "tokens": ["Zu", "die\u00b7sem", "Scha\u00b7ze", "kommst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du kanst gleichwohl zufrieden leben", "tokens": ["Du", "kanst", "gleich\u00b7wohl", "zu\u00b7frie\u00b7den", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und einmahl froh zu Grabe gehn", "tokens": ["Und", "ein\u00b7mahl", "froh", "zu", "Gra\u00b7be", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und brauchst, ach glaube doch, nicht eben", "tokens": ["Und", "brauchst", ",", "ach", "glau\u00b7be", "doch", ",", "nicht", "e\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ITJ", "VVFIN", "ADV", "$,", "PTKNEG", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den hohen Leibniz zu verstehn.", "tokens": ["Den", "ho\u00b7hen", "Leib\u00b7niz", "zu", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast genung vor dein Geschlechte,", "tokens": ["Du", "hast", "ge\u00b7nung", "vor", "dein", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nachdem dein lobenswerther Flei\u00df", "tokens": ["Nach\u00b7dem", "dein", "lo\u00b7bens\u00b7wert\u00b7her", "Flei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Wirthschaft und des H\u00f6chsten Rechte", "tokens": ["Die", "Wirth\u00b7schaft", "und", "des", "H\u00f6chs\u00b7ten", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So wie des Umgangs Regeln weis.", "tokens": ["So", "wie", "des", "Um\u00b7gangs", "Re\u00b7geln", "weis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Verrichte nur dein Amt mit Freuden,", "tokens": ["Ver\u00b7rich\u00b7te", "nur", "dein", "Amt", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Zuversicht auf Gottes Schuz;", "tokens": ["Mit", "Zu\u00b7ver\u00b7sicht", "auf", "Got\u00b7tes", "Schuz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kommt ohngefehr ein schweres Leiden,", "tokens": ["Kommt", "ohn\u00b7ge\u00b7fehr", "ein", "schwe\u00b7res", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bieth ihm mit der Hofnung Truz.", "tokens": ["So", "bieth", "ihm", "mit", "der", "Hof\u00b7nung", "Truz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verliere nie den wahren Glauben,", "tokens": ["Ver\u00b7lie\u00b7re", "nie", "den", "wah\u00b7ren", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er dient dir zur Gerechtigkeit,", "tokens": ["Er", "dient", "dir", "zur", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn dich lose M\u00e4uler schrauben,", "tokens": ["Und", "wenn", "dich", "lo\u00b7se", "M\u00e4u\u00b7ler", "schrau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So siege mit Gela\u00dfenheit.", "tokens": ["So", "sie\u00b7ge", "mit", "Ge\u00b7la\u00b7\u00dfen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ein klug- und th\u00e4tiges Erbarmen", "tokens": ["Ein", "klug", "und", "th\u00e4\u00b7ti\u00b7ges", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kan wider S\u00fcnd und Fluch bestehn;", "tokens": ["Kan", "wi\u00b7der", "S\u00fcnd", "und", "Fluch", "be\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Las, wenn du kanst, nicht einen Armen", "tokens": ["Las", ",", "wenn", "du", "kanst", ",", "nicht", "ei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "VMFIN", "$,", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Betr\u00fcbt und h\u00fclflos von dir gehn.", "tokens": ["Be\u00b7tr\u00fcbt", "und", "h\u00fcl\u00b7flos", "von", "dir", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vergieb und habe mit den Schwachen", "tokens": ["Ver\u00b7gieb", "und", "ha\u00b7be", "mit", "den", "Schwa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KON", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So viel als mit dir selbst Gedult;", "tokens": ["So", "viel", "als", "mit", "dir", "selbst", "Ge\u00b7dult", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "KOKOM", "APPR", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Will Gl\u00fcck und Wetter gar nicht lachen,", "tokens": ["Will", "Gl\u00fcck", "und", "Wet\u00b7ter", "gar", "nicht", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So sey dein Trost: Ich bin nicht schuld.", "tokens": ["So", "sey", "dein", "Trost", ":", "Ich", "bin", "nicht", "schuld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Erg\u00f6ze dich mit Hofnungsblicken", "tokens": ["Er\u00b7g\u00f6\u00b7ze", "dich", "mit", "Hof\u00b7nungs\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An jenes Lebens Lust und Pracht;", "tokens": ["An", "je\u00