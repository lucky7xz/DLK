{"textgrid.poem.54374": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "6. Brief", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? Schwester? doch mag ich dich wohl mit Recht so nennen,", "tokens": ["Wie", "?", "Schwes\u00b7ter", "?", "doch", "mag", "ich", "dich", "wohl", "mit", "Recht", "so", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NN", "$.", "ADV", "VMFIN", "PPER", "PRF", "ADV", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der Geilheit Hitz und Brunst, Vernunft, und Menschheit raubt,", "tokens": ["Der", "Geil\u00b7heit", "Hitz", "und", "Brunst", ",", "Ver\u00b7nunft", ",", "und", "Menschheit", "raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,", "NN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Unthier, das man sieht von solcher Liebe brennen,", "tokens": ["Ein", "Un\u00b7thier", ",", "das", "man", "sieht", "von", "sol\u00b7cher", "Lie\u00b7be", "bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die aus der H\u00f6lle stammt, und kaum die Nachwelt glaubt.", "tokens": ["Die", "aus", "der", "H\u00f6l\u00b7le", "stammt", ",", "und", "kaum", "die", "Nach\u00b7welt", "glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vermaledeyte Brunst! verfluchenswerthe Liebe,", "tokens": ["Ver\u00b7ma\u00b7le\u00b7dey\u00b7te", "Brunst", "!", "ver\u00b7flu\u00b7chens\u00b7wert\u00b7he", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Entmenschte Buhlerinn! du Scheusal der Natur!", "tokens": ["Ent\u00b7menschte", "Buh\u00b7le\u00b7rinn", "!", "du", "Scheu\u00b7sal", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Du hast, vom wildem Vieh gewi\u00df dergleichen Triebe", "tokens": ["Du", "hast", ",", "vom", "wil\u00b7dem", "Vieh", "ge\u00b7wi\u00df", "derg\u00b7lei\u00b7chen", "Trie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "APPRART", "ADJA", "NN", "ADV", "PIS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erlernt und abgesehn; dies zeigte dir die Spur;", "tokens": ["Er\u00b7lernt", "und", "ab\u00b7ge\u00b7sehn", ";", "dies", "zeig\u00b7te", "dir", "die", "Spur", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$.", "PDS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sprich, welche Furie hie\u00df dich den Kiel ergreifen,", "tokens": ["Sprich", ",", "wel\u00b7che", "Fu\u00b7rie", "hie\u00df", "dich", "den", "Kiel", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAT", "NN", "VVFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Den du selbst in den Schlamm der Geilheit eingetaucht?", "tokens": ["Den", "du", "selbst", "in", "den", "Schlamm", "der", "Geil\u00b7heit", "ein\u00b7ge\u00b7taucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer f\u00fchrte dir die Hand, so s\u00fc\u00df mir vorzupfeifen?", "tokens": ["Wer", "f\u00fchr\u00b7te", "dir", "die", "Hand", ",", "so", "s\u00fc\u00df", "mir", "vor\u00b7zu\u00b7pfei\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hat dir nicht Pluto Wort und Einfall eingehaucht?", "tokens": ["Hat", "dir", "nicht", "Plu\u00b7to", "Wort", "und", "Ein\u00b7fall", "ein\u00b7ge\u00b7haucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ists m\u00f6glich, da\u00df du mich, Sirene, suchst zu kirren?", "tokens": ["Ists", "m\u00f6g\u00b7lich", ",", "da\u00df", "du", "mich", ",", "Si\u00b7re\u00b7ne", ",", "suchst", "zu", "kir\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "$,", "NN", "$,", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "Nein wilde Biblide, dein glattes Buhlerlied", "tokens": ["Nein", "wil\u00b7de", "Bib\u00b7li\u00b7de", ",", "dein", "glat\u00b7tes", "Buh\u00b7ler\u00b7lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Wird mich nicht in das Netz, so du mir stellst, verwirren,", "tokens": ["Wird", "mich", "nicht", "in", "das", "Netz", ",", "so", "du", "mir", "stellst", ",", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,", "ADV", "PPER", "PPER", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil Caunus dich weit mehr als eine Schlange flieht.", "tokens": ["Weil", "Cau\u00b7nus", "dich", "weit", "mehr", "als", "ei\u00b7ne", "Schlan\u00b7ge", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADJD", "PIAT", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+++-+-+-+", "measure": "unknown.measure.septa"}, "line.17": {"text": "Ihn d\u00fcnkt, da du so frech und unversch\u00e4mt gehandelt,", "tokens": ["Ihn", "d\u00fcnkt", ",", "da", "du", "so", "frech", "und", "un\u00b7ver\u00b7sch\u00e4mt", "ge\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als h\u00e4tte dich nunmehr dein unversch\u00e4mter Trieb,", "tokens": ["Als", "h\u00e4t\u00b7te", "dich", "nun\u00b7mehr", "dein", "un\u00b7ver\u00b7sch\u00e4m\u00b7ter", "Trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "In einen Basilisk und Drachenbrut verwandelt,", "tokens": ["In", "ei\u00b7nen", "Ba\u00b7si\u00b7lisk", "und", "Dra\u00b7chen\u00b7brut", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als ob nichts menschliches von dir mehr \u00fcberblieb.", "tokens": ["Als", "ob", "nichts", "menschli\u00b7ches", "von", "dir", "mehr", "\u00fc\u00b7berb\u00b7lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "ADJA", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Wie? hast du auf einmal, Barbarin, gleich vergessen", "tokens": ["Wie", "?", "hast", "du", "auf", "ein\u00b7mal", ",", "Bar\u00b7ba\u00b7rin", ",", "gleich", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "$.", "VAFIN", "PPER", "APPR", "ADV", "$,", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Da\u00df ich und du zu gleich, kann wohl was n\u00e4her seyn?", "tokens": ["Da\u00df", "ich", "und", "du", "zu", "gleich", ",", "kann", "wohl", "was", "n\u00e4\u00b7her", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPER", "APPR", "ADV", "$,", "VMFIN", "ADV", "PWS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "In einer Mutter Schoo\u00df so fest verkn\u00fcpft gesessen?", "tokens": ["In", "ei\u00b7ner", "Mut\u00b7ter", "Schoo\u00df", "so", "fest", "ver\u00b7kn\u00fcpft", "ge\u00b7ses\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "ADJD", "VVPP", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wir sind ja, Biblide, von einem Fleisch und Bein,", "tokens": ["Wir", "sind", "ja", ",", "Bib\u00b7li\u00b7de", ",", "von", "ei\u00b7nem", "Fleisch", "und", "Bein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "NN", "$,", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es hat uns eine Brust ges\u00e4uget und genehret,", "tokens": ["Es", "hat", "uns", "ei\u00b7ne", "Brust", "ge\u00b7s\u00e4u\u00b7get", "und", "ge\u00b7neh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und doch sieht mich dein Aug, als einen Fremdling an.", "tokens": ["Und", "doch", "sieht", "mich", "dein", "Aug", ",", "als", "ei\u00b7nen", "Fremd\u00b7ling", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Unsinnige! die Brunst so leider dich beth\u00f6ret,", "tokens": ["Un\u00b7sin\u00b7ni\u00b7ge", "!", "die", "Brunst", "so", "lei\u00b7der", "dich", "be\u00b7th\u00f6\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dr\u00fcckt dir ein Brandmal ein, das sich nicht bergen kann.", "tokens": ["Dr\u00fcckt", "dir", "ein", "Brand\u00b7mal", "ein", ",", "das", "sich", "nicht", "ber\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "PRELS", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wie magst du mit der Macht der Liebe dich wohl sch\u00fctzen?", "tokens": ["Wie", "magst", "du", "mit", "der", "Macht", "der", "Lie\u00b7be", "dich", "wohl", "sch\u00fct\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Rechtfertige diesmal ja dein Begehren nicht,", "tokens": ["Recht\u00b7fer\u00b7ti\u00b7ge", "dies\u00b7mal", "ja", "dein", "Be\u00b7geh\u00b7ren", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dein Vorwand gr\u00fcndet sich auf nichts als m\u00fcrben St\u00fctzen,", "tokens": ["Dein", "Vor\u00b7wand", "gr\u00fcn\u00b7det", "sich", "auf", "nichts", "als", "m\u00fcr\u00b7ben", "St\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PIS", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da dir Vernunft und Recht in allem widerspricht.", "tokens": ["Da", "dir", "Ver\u00b7nunft", "und", "Recht", "in", "al\u00b7lem", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sie hat uns beyderseits zwar durch ein Band verstricket;", "tokens": ["Sie", "hat", "uns", "bey\u00b7der\u00b7seits", "zwar", "durch", "ein", "Band", "ver\u00b7stri\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Doch auf verbothne Glut den strengsten Fluch gelegt,", "tokens": ["Doch", "auf", "ver\u00b7both\u00b7ne", "Glut", "den", "strengs\u00b7ten", "Fluch", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Den deine wilde Faust doch nimmermehr verr\u00fccket,", "tokens": ["Den", "dei\u00b7ne", "wil\u00b7de", "Faust", "doch", "nim\u00b7mer\u00b7mehr", "ver\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So stark das heisse Blut sich auch in Adern regt.", "tokens": ["So", "stark", "das", "heis\u00b7se", "Blut", "sich", "auch", "in", "A\u00b7dern", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "PRF", "ADV", "APPR", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Sie hat uns Aug und Mund, so fest sie kann versiegelt,", "tokens": ["Sie", "hat", "uns", "Aug", "und", "Mund", ",", "so", "fest", "sie", "kann", "ver\u00b7sie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN", "$,", "ADV", "ADJD", "PPER", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Damit nicht Blick und Ku\u00df ihr hohes Recht entweyht,", "tokens": ["Da\u00b7mit", "nicht", "Blick", "und", "Ku\u00df", "ihr", "ho\u00b7hes", "Recht", "ent\u00b7weyht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "NN", "KON", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und der Geschwister Herz in solchem Fall verriegelt;", "tokens": ["Und", "der", "Ge\u00b7schwis\u00b7ter", "Herz", "in", "sol\u00b7chem", "Fall", "ver\u00b7rie\u00b7gelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch Schaam und Furcht entfernt von dir sich allzuweit.", "tokens": ["Doch", "Schaam", "und", "Furcht", "ent\u00b7fernt", "von", "dir", "sich", "all\u00b7zu\u00b7weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN",