": ["Fleuch", "Cau\u00b7nus", ",", "weil", "du", "kannst", ",", "dies", "Scheu\u00b7sal", "zu", "ver\u00b7mei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "KOUS", "PPER", "VMFIN", "$,", "PDS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Verla\u00df dein Vaterland, damit du sicher bist.", "tokens": ["Ver\u00b7la\u00df", "dein", "Va\u00b7ter\u00b7land", ",", "da\u00b7mit", "du", "si\u00b7cher", "bist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Die Schlange kann dich nicht in fremder Luft vergiften.", "tokens": ["Die", "Schlan\u00b7ge", "kann", "dich", "nicht", "in", "frem\u00b7der", "Luft", "ver\u00b7gif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Verhel ihr Weg und Farth, und scheu kein Ungemach,", "tokens": ["Ver\u00b7hel", "ihr", "Weg", "und", "Farth", ",", "und", "scheu", "kein", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "$,", "KON", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "So kann dir Biblis doch kein weitres Schrecken stiften;", "tokens": ["So", "kann", "dir", "Bib\u00b7lis", "doch", "kein", "weit\u00b7res", "Schre\u00b7cken", "stif\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "wo