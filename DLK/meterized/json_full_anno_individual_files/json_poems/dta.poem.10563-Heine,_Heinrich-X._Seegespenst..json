{"dta.poem.10563": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n  Seegespenst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich aber lag am Rande des Schiffes,", "tokens": ["Ich", "a\u00b7ber", "lag", "am", "Ran\u00b7de", "des", "Schif\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schaute, tr\u00e4umenden Auges,", "tokens": ["Und", "schau\u00b7te", ",", "tr\u00e4u\u00b7men\u00b7den", "Au\u00b7ges", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hinab in das spiegelklare Wasser,", "tokens": ["Hin\u00b7ab", "in", "das", "spie\u00b7gel\u00b7kla\u00b7re", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und schaute tiefer und tiefer \u2014", "tokens": ["Und", "schau\u00b7te", "tie\u00b7fer", "und", "tie\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Bis tief, im Meeresgrunde,", "tokens": ["Bis", "tief", ",", "im", "Mee\u00b7res\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Anfangs wie d\u00e4mmernde Nebel,", "tokens": ["An\u00b7fangs", "wie", "d\u00e4m\u00b7mern\u00b7de", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Jedoch allm\u00e4hlig farbenbestimmter,", "tokens": ["Je\u00b7doch", "all\u00b7m\u00e4h\u00b7lig", "far\u00b7ben\u00b7be\u00b7stimm\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Kirchenkuppel und Th\u00fcrme sich zeigten", "tokens": ["Kir\u00b7chen\u00b7kup\u00b7pel", "und", "Th\u00fcr\u00b7me", "sich", "zeig\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "PRF", "VVFIN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Und endlich, sonnenklar, eine ganze Stadt,", "tokens": ["Und", "end\u00b7lich", ",", "son\u00b7nen\u00b7klar", ",", "ei\u00b7ne", "gan\u00b7ze", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Alterth\u00fcmlich niederl\u00e4ndisch,", "tokens": ["Al\u00b7tert\u00b7h\u00fcm\u00b7lich", "nie\u00b7der\u00b7l\u00e4n\u00b7disch", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und menschenbelebt.", "tokens": ["Und", "men\u00b7schen\u00b7be\u00b7lebt", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Bed\u00e4chtige M\u00e4nner, schwarzbem\u00e4ntelt,", "tokens": ["Be\u00b7d\u00e4ch\u00b7ti\u00b7ge", "M\u00e4n\u00b7ner", ",", "schwarz\u00b7be\u00b7m\u00e4n\u00b7telt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Mit wei\u00dfen Halskrausen und Ehrenketten", "tokens": ["Mit", "wei\u00b7\u00dfen", "Hals\u00b7krau\u00b7sen", "und", "Eh\u00b7ren\u00b7ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.14": {"text": "Und langen Degen und langen Gesichtern,", "tokens": ["Und", "lan\u00b7gen", "De\u00b7gen", "und", "lan\u00b7gen", "Ge\u00b7sich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Schreiten \u00fcber den wimmelnden Marktplatz", "tokens": ["Schrei\u00b7ten", "\u00fc\u00b7ber", "den", "wim\u00b7meln\u00b7den", "Markt\u00b7platz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}