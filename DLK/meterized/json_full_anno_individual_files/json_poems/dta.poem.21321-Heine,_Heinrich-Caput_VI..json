{"dta.poem.21321": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Blieb stehen, als wartete er auf was,", "tokens": ["Blieb", "ste\u00b7hen", ",", "als", "war\u00b7te\u00b7te", "er", "auf", "was", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "APPR", "PRELS", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und f\u00f6rderte ich die Schritte,", "tokens": ["Und", "f\u00f6r\u00b7der\u00b7te", "ich", "die", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dann folgte er wieder. So kamen wir", "tokens": ["Dann", "folg\u00b7te", "er", "wie\u00b7der", ".", "So", "ka\u00b7men", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Bis auf des Domplatz Mitte.", "tokens": ["Bis", "auf", "des", "Dom\u00b7platz", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es ward mir unleidlich, ich drehte mich um", "tokens": ["Es", "ward", "mir", "un\u00b7leid\u00b7lich", ",", "ich", "dreh\u00b7te", "mich", "um"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "PRF", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und sprach: Jetzt steh\u2019 mir Rede,", "tokens": ["Und", "sprach", ":", "Jetzt", "steh'", "mir", "Re\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was folgst du mir auf Weg und Steg,", "tokens": ["Was", "folgst", "du", "mir", "auf", "Weg", "und", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier in der n\u00e4chtlichen Oede?", "tokens": ["Hier", "in", "der", "n\u00e4cht\u00b7li\u00b7chen", "O\u00b7e\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich treffe dich immer in der Stund,", "tokens": ["Ich", "tref\u00b7fe", "dich", "im\u00b7mer", "in", "der", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo Weltgef\u00fchle sprie\u00dfen", "tokens": ["Wo", "Welt\u00b7ge\u00b7f\u00fch\u00b7le", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In meiner Brust und durch das Hirn", "tokens": ["In", "mei\u00b7ner", "Brust", "und", "durch", "das", "Hirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Geistesblitze schie\u00dfen.", "tokens": ["Die", "Geis\u00b7tes\u00b7blit\u00b7ze", "schie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du siehst mich an so stier und fest \u2013", "tokens": ["Du", "siehst", "mich", "an", "so", "stier", "und", "fest", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steh\u2019 Rede: was verh\u00fcllst du", "tokens": ["Steh'", "Re\u00b7de", ":", "was", "ver\u00b7h\u00fcllst", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWS", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier unter dem Mantel, das heimlich blinkt?", "tokens": ["Hier", "un\u00b7ter", "dem", "Man\u00b7tel", ",", "das", "heim\u00b7lich", "blinkt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wer bist du und was willst du?", "tokens": ["Wer", "bist", "du", "und", "was", "willst", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "KON", "PWS", "VMFIN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}