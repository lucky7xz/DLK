{"dta.poem.20811": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An den geneigten  \n Leser.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der geschwinde Ludewig aller hohen Wei\u00dfheit voll/", "tokens": ["Der", "ge\u00b7schwin\u00b7de", "Lu\u00b7de\u00b7wig", "al\u00b7ler", "ho\u00b7hen", "Wei\u00df\u00b7heit", "voll", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "PIAT", "ADJA", "NN", "ADJD", "$("], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der gantz Oesterreich regiert/ wie ein Fr\u00e4nkscher K\u00f6nig sol.", "tokens": ["Der", "gantz", "O\u00b7es\u00b7ter\u00b7reich", "re\u00b7giert", "/", "wie", "ein", "Fr\u00e4nk\u00b7scher", "K\u00f6\u00b7nig", "sol", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NE", "VVFIN", "$(", "KOKOM", "ART", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Uber dieses Fr\u00e4nksche Land gehet alle seine Macht/", "tokens": ["U\u00b7ber", "die\u00b7ses", "Fr\u00e4nk\u00b7sche", "Land", "ge\u00b7het", "al\u00b7le", "sei\u00b7ne", "Macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "VVFIN", "PIS", "PPOSAT", "NN", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Die Regierung/ wie ich melde/ hat er in den schwung gebracht.", "tokens": ["Die", "Re\u00b7gie\u00b7rung", "/", "wie", "ich", "mel\u00b7de", "/", "hat", "er", "in", "den", "schwung", "ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PPER", "VVFIN", "$(", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Gott verst\u00e4rck jhm seine G\u00fcte und erfreu jhm seinen Muth/", "tokens": ["Gott", "ver\u00b7st\u00e4rck", "jhm", "sei\u00b7ne", "G\u00fc\u00b7te", "und", "er\u00b7freu", "jhm", "sei\u00b7nen", "Muth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Er erleucht\u2019 jhm sein Gem\u00fcte und gewehr jhm reiches Gut.", "tokens": ["Er", "er\u00b7leucht'", "jhm", "sein", "Ge\u00b7m\u00fc\u00b7te", "und", "ge\u00b7wehr", "jhm", "rei\u00b7ches", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "ADV", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}