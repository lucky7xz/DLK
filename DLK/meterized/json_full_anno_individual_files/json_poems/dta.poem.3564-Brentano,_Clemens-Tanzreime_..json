{"dta.poem.3564": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "S mein seyn und 's dein seyn,               ", "tokens": ["S", "mein", "seyn", "und", "'s", "dein", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "VAINF", "KON", "PPER", "PPOSAT", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und's zu dir liego,", "tokens": ["Un\u00b7d's", "zu", "dir", "lie\u00b7go", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das bringt mich sechs Jahr lang", "tokens": ["Das", "bringt", "mich", "sechs", "Jahr", "lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "CARD", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ins Soldatelebo.", "tokens": ["Ins", "Sol\u00b7da\u00b7te\u00b7le\u00b7bo", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Sechs Jahr und drei Monat,", "tokens": ["Sechs", "Jahr", "und", "drei", "Mo\u00b7nat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dann ist mein Zeit aus;", "tokens": ["Dann", "ist", "mein", "Zeit", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Nach k\u00f6mmt es mein Sch\u00e4tzle,", "tokens": ["Nach", "k\u00f6mmt", "es", "mein", "Sch\u00e4tz\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und f\u00fchrt mich nach Haus.", "tokens": ["Und", "f\u00fchrt", "mich", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}