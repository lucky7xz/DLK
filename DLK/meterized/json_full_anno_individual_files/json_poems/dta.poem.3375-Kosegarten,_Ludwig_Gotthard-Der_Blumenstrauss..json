{"dta.poem.3375": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Blumenstrauss.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bl&#252;mchen, die ihr lieblich bl&#252;htet,", "tokens": ["Bl", "&#252;", "mchen", ",", "die", "ihr", "lieb\u00b7lich", "bl", "&#252;", "htet", ","], "token_info": ["word", "XML_entity", "word", "punct", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NE", "$(", "VVINF", "$,", "PRELS", "PPER", "ADJD", "NE", "$(", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "D\u00fcfte hauchtet, Strahlen spr\u00fchtet,", "tokens": ["D\u00fcf\u00b7te", "hauch\u00b7tet", ",", "Strah\u00b7len", "spr\u00fch\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fcmchen, ach, ihr seyd verbl\u00fcht!", "tokens": ["Bl\u00fcm\u00b7chen", ",", "ach", ",", "ihr", "seyd", "ver\u00b7bl\u00fcht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure Reize sind entwichen,", "tokens": ["Eu\u00b7re", "Rei\u00b7ze", "sind", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eure Sch\u00f6nheit ist verblichen,", "tokens": ["Eu\u00b7re", "Sch\u00f6n\u00b7heit", "ist", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eure Strahlen sind verspr\u00fcht.", "tokens": ["Eu\u00b7re", "Strah\u00b7len", "sind", "ver\u00b7spr\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eures Kelches s\u00fcsse D\u00fcfte,", "tokens": ["Eu\u00b7res", "Kel\u00b7ches", "s\u00fcs\u00b7se", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ausgegossen durch die L\u00fcfte,", "tokens": ["Aus\u00b7ge\u00b7gos\u00b7sen", "durch", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmeicheln keinem Wandrer mehr", "tokens": ["Schmei\u00b7cheln", "kei\u00b7nem", "Wand\u00b7rer", "mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure goldbesprengten Glocken", "tokens": ["Eu\u00b7re", "gold\u00b7be\u00b7spreng\u00b7ten", "Glo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kr\u00e4nzen keines M\u00e4dchens Locken,", "tokens": ["Kr\u00e4n\u00b7zen", "kei\u00b7nes", "M\u00e4d\u00b7chens", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schm\u00fccken keinen Busen mehr.", "tokens": ["Schm\u00fc\u00b7cken", "kei\u00b7nen", "Bu\u00b7sen", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Erdent\u00f6chter, Erdens\u00f6hne,", "tokens": ["Er\u00b7den\u00b7t\u00f6ch\u00b7ter", ",", "Er\u00b7den\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fchmet euch nicht eurer Sch\u00f6ne,", "tokens": ["R\u00fch\u00b7met", "euch", "nicht", "eu\u00b7rer", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trotzet nicht auf eure Kraft.", "tokens": ["Trot\u00b7zet", "nicht", "auf", "eu\u00b7re", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes Daseyns Quell versieget,", "tokens": ["Je\u00b7des", "Da\u00b7seyns", "Quell", "ver\u00b7sie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedes Athems Hauch verflieget,", "tokens": ["Je\u00b7des", "A\u00b7thems", "Hauch", "ver\u00b7flie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeder St\u00e4rke Nerv' erschlafft.", "tokens": ["Je\u00b7der", "St\u00e4r\u00b7ke", "Ner\u00b7v'", "er\u00b7schlafft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "J\u00fcngling, dein gewaltig Leben,", "tokens": ["J\u00fcng\u00b7ling", ",", "dein", "ge\u00b7wal\u00b7tig", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Kr\u00e4fte rastlos Streben,", "tokens": ["Dei\u00b7ner", "Kr\u00e4f\u00b7te", "rast\u00b7los", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deines Stolzes Herrlichkeit", "tokens": ["Dei\u00b7nes", "Stol\u00b7zes", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird zerflattern. Starrend liegen", "tokens": ["Wird", "zer\u00b7flat\u00b7tern", ".", "Star\u00b7rend", "lie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "VVINF", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wirst du, f\u00fchllos f\u00fcr Vergn\u00fcgen,", "tokens": ["Wirst", "du", ",", "f\u00fchl\u00b7los", "f\u00fcr", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchllos selbst f\u00fcr Lieb' und Leid.", "tokens": ["F\u00fchl\u00b7los", "selbst", "f\u00fcr", "Lieb'", "und", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "M\u00e4dchen, deiner Wangen Rosen", "tokens": ["M\u00e4d\u00b7chen", ",", "dei\u00b7ner", "Wan\u00b7gen", "Ro\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welken, und das s\u00fcsse Kosen", "tokens": ["Wel\u00b7ken", ",", "und", "das", "s\u00fcs\u00b7se", "Ko\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner Purpurlippen schweigt.", "tokens": ["Dei\u00b7ner", "Pur\u00b7pur\u00b7lip\u00b7pen", "schweigt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Trittes Rehesschnelle", "tokens": ["Dei\u00b7nes", "Trit\u00b7tes", "Re\u00b7hes\u00b7schnel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4hmt die Zeit. Die Frisch' und Helle", "tokens": ["L\u00e4hmt", "die", "Zeit", ".", "Die", "Frisch'", "und", "Hel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deines Angesichts verbleicht.", "tokens": ["Dei\u00b7nes", "An\u00b7ge\u00b7sichts", "ver\u00b7bleicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zeder, rausche nicht so pr\u00e4chtig", "tokens": ["Ze\u00b7der", ",", "rau\u00b7sche", "nicht", "so", "pr\u00e4ch\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PTKNEG", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die L\u00fcfte. Stark und m\u00e4chtig", "tokens": ["Durch", "die", "L\u00fcf\u00b7te", ".", "Stark", "und", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "NN", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt der Sturm, der dich zerbricht.", "tokens": ["Kommt", "der", "Sturm", ",", "der", "dich", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Steig', o Sonne! Sinke nieder!", "tokens": ["Steig'", ",", "o", "Son\u00b7ne", "!", "Sin\u00b7ke", "nie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$.", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einstens sinkst du, steigst nicht wieder,", "tokens": ["Eins\u00b7tens", "sinkst", "du", ",", "steigst", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kerzen gleich erlosch dein Licht.", "tokens": ["Ker\u00b7zen", "gleich", "er\u00b7losch", "dein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mond, du wirst nicht ewig schimmern;", "tokens": ["Mond", ",", "du", "wirst", "nicht", "e\u00b7wig", "schim\u00b7mern", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blaue Feste, du wirst tr\u00fcmmern;", "tokens": ["Blau\u00b7e", "Fes\u00b7te", ",", "du", "wirst", "tr\u00fcm\u00b7mern", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sternensaat, du wirst verwehn.", "tokens": ["Ster\u00b7nen\u00b7saat", ",", "du", "wirst", "ver\u00b7wehn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was aus Moder spross, muss modern,", "tokens": ["Was", "aus", "Mo\u00b7der", "spross", ",", "muss", "mo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was der Asch' entglomm, verlodern,", "tokens": ["Was", "der", "Asch'", "ent\u00b7glomm", ",", "ver\u00b7lo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was begann, muss untergehn.", "tokens": ["Was", "be\u00b7gann", ",", "muss", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Untergehn? . . . Nein, untergehen,", "tokens": ["Un\u00b7ter\u00b7gehn", "?", ".", ".", ".", "Nein", ",", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "punct", "punct", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "$.", "$.", "$.", "PTKANT", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gar verflattern, gar verwehen", "tokens": ["Gar", "ver\u00b7flat\u00b7tern", ",", "gar", "ver\u00b7we\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag aus Gottes Schoosse nichts.", "tokens": ["Mag", "aus", "Got\u00b7tes", "Schoos\u00b7se", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "NN", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Altern, kr\u00e4nkeln, welken, sterben", "tokens": ["Al\u00b7tern", ",", "kr\u00e4n\u00b7keln", ",", "wel\u00b7ken", ",", "ster\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVINF", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mag es wohl; doch gar verderben,", "tokens": ["Mag", "es", "wohl", ";", "doch", "gar", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$.", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gar verlieren mag sich nichts:", "tokens": ["Gar", "ver\u00b7lie\u00b7ren", "mag", "sich", "nichts", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PRF", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nicht die Asch' erloschner Sonnen,", "tokens": ["Nicht", "die", "Asch'", "er\u00b7lo\u00b7schner", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht Gespinnst, vom Wurm gesponnen,", "tokens": ["Nicht", "Ge\u00b7spinnst", ",", "vom", "Wurm", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht des Baumes fallend Laub,", "tokens": ["Nicht", "des", "Bau\u00b7mes", "fal\u00b7lend", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht zerborstner Welten Tr\u00fcmmer,", "tokens": ["Nicht", "zer\u00b7borst\u00b7ner", "Wel\u00b7ten", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht verblichner Wangen Schimmer,", "tokens": ["Nicht", "ver\u00b7blich\u00b7ner", "Wan\u00b7gen", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht verflogner Blumenstaub.", "tokens": ["Nicht", "ver\u00b7flog\u00b7ner", "Blu\u00b7men\u00b7staub", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Unverloren ruhet Alles,", "tokens": ["Un\u00b7ver\u00b7lo\u00b7ren", "ru\u00b7het", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stoff des Blatts, des Sonnenballes,", "tokens": ["Stoff", "des", "Blatts", ",", "des", "Son\u00b7nen\u00b7bal\u00b7les", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Ewgen sicherm Schooss;", "tokens": ["In", "des", "Ew\u00b7gen", "si\u00b7cherm", "Scho\u00b7oss", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Windet einstens aus dem Staube \u2014", "tokens": ["Win\u00b7det", "eins\u00b7tens", "aus", "dem", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00fcsse Hoffnung! sch\u00f6ner Glaube! \u2014", "tokens": ["S\u00fcs\u00b7se", "Hoff\u00b7nung", "!", "sch\u00f6\u00b7ner", "Glau\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "$.", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Herrlicher sich wieder los. \u2014", "tokens": ["Herr\u00b7li\u00b7cher", "sich", "wie\u00b7der", "los", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PRF", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Bl\u00fcmchen, die ihr hold und lieblich", "tokens": ["Bl\u00fcm\u00b7chen", ",", "die", "ihr", "hold", "und", "lieb\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gestern, bl\u00fchtet, still und tr\u00fcblich", "tokens": ["Ge\u00b7stern", ",", "bl\u00fch\u00b7tet", ",", "still", "und", "tr\u00fcb\u00b7lich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Heute welket \u2014 trauert nicht!", "tokens": ["Heu\u00b7te", "wel\u00b7ket", "trau\u00b7ert", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "VVFIN", "PTKNEG", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Eurer Asch' entspriessen Keime,", "tokens": ["Eu\u00b7rer", "Asch'", "ent\u00b7spries\u00b7sen", "Kei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmelsblumen, Lebensb\u00e4ume,", "tokens": ["Him\u00b7mels\u00b7blu\u00b7men", ",", "Le\u00b7bens\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die kein Herbststurm knickt noch bricht!", "tokens": ["Die", "kein", "Herbst\u00b7sturm", "knickt", "noch", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Edler J\u00fcngling, sey nicht traurig,", "tokens": ["Ed\u00b7ler", "J\u00fcng\u00b7ling", ",", "sey", "nicht", "trau\u00b7rig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn so fr\u00fch, so dumpf und schaurig", "tokens": ["Wenn", "so", "fr\u00fch", ",", "so", "dumpf", "und", "schau\u00b7rig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir die ernste Stimme ruft.", "tokens": ["Dir", "die", "erns\u00b7te", "Stim\u00b7me", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Edler, kr\u00e4ftiger, verkl\u00e4rter,", "tokens": ["Ed\u00b7ler", ",", "kr\u00e4f\u00b7ti\u00b7ger", ",", "ver\u00b7kl\u00e4r\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebevoller, liebenswerther,", "tokens": ["Lie\u00b7be\u00b7vol\u00b7ler", ",", "lie\u00b7bens\u00b7wert\u00b7her", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcberstrahlst du einst die Gruft.", "tokens": ["\u00dc\u00b7bers\u00b7trahlst", "du", "einst", "die", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "M\u00e4dchen mit der sanften Seele,", "tokens": ["M\u00e4d\u00b7chen", "mit", "der", "sanf\u00b7ten", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zittre nicht, die enge H\u00f6hle", "tokens": ["Zitt\u00b7re", "nicht", ",", "die", "en\u00b7ge", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Langer Ruh' hinab zu sehn.", "tokens": ["Lan\u00b7ger", "Ruh'", "hin\u00b7ab", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber Grab und Grabestr\u00fcmmer", "tokens": ["\u00dc\u00b7ber", "Grab", "und", "Gra\u00b7be\u00b7str\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wirst du einst im Sonnenschimmer", "tokens": ["Wirst", "du", "einst", "im", "Son\u00b7nen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Himmlischer Verkl\u00e4rung stehn.", "tokens": ["Himm\u00b7li\u00b7scher", "Ver\u00b7kl\u00e4\u00b7rung", "stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Englisch wird dein Antlitz gl\u00e4nzen", "tokens": ["Eng\u00b7lisch", "wird", "dein", "Ant\u00b7litz", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kr\u00e4nzen wirst du dich mit Kr\u00e4nzen,", "tokens": ["Kr\u00e4n\u00b7zen", "wirst", "du", "dich", "mit", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren Rose nie erblasst.", "tokens": ["De\u00b7ren", "Ro\u00b7se", "nie", "er\u00b7blasst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den selgen Myrtenthalen,", "tokens": ["In", "den", "sel\u00b7gen", "Myr\u00b7ten\u00b7tha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Horchend ewgen Nachtigalen", "tokens": ["Hor\u00b7chend", "ew\u00b7gen", "Nach\u00b7ti\u00b7ga\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wirst du pflegen s\u00fcsser Rast!", "tokens": ["Wirst", "du", "pfle\u00b7gen", "s\u00fcs\u00b7ser", "Rast", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Leibessch\u00f6nheit bleibt dahinden;", "tokens": ["Lei\u00b7bes\u00b7sch\u00f6n\u00b7heit", "bleibt", "da\u00b7hin\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PAV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seelensch\u00f6nheit kann nicht schwinden,", "tokens": ["See\u00b7len\u00b7sch\u00f6n\u00b7heit", "kann", "nicht", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00fcnt und bl\u00fcht in Ewigkeit.", "tokens": ["Gr\u00fcnt", "und", "bl\u00fcht", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanftmuth, Demuth, Unschuld, Tugend", "tokens": ["Sanft\u00b7muth", ",", "De\u00b7muth", ",", "Un\u00b7schuld", ",", "Tu\u00b7gend"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kr\u00e4nzt mit immergr\u00fcner Jugend,", "tokens": ["Kr\u00e4nzt", "mit", "im\u00b7mer\u00b7gr\u00fc\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"tex