?", "tokens": ["Hier", "in", "Angst", "ver\u00b7rau\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sieh das blaue Sternenfeld", "tokens": ["Sieh", "das", "blau\u00b7e", "Ster\u00b7nen\u00b7feld"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wogig um dich flie\u00dfen;", "tokens": ["Wo\u00b7gig", "um", "dich", "flie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sieh den Mond, und sieh die Welt", "tokens": ["Sieh", "den", "Mond", ",", "und", "sieh", "die", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "KON", "VVIMP", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter deinen F\u00fc\u00dfen.", "tokens": ["Un\u00b7ter", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sieh das Licht!", "tokens": ["Sieh", "das", "Licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Funkeln nicht", "tokens": ["Fun\u00b7keln", "nicht"], "token_info": ["word", "word"], "pos": ["NN", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Deines Gottes Wunder", "tokens": ["Dei\u00b7nes", "Got\u00b7tes", "Wun\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Ueberall herunter?", "tokens": ["Ue\u0