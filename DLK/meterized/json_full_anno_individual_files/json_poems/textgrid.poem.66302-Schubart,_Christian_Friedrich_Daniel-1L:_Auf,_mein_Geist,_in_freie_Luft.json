"word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Schwache Seele, willst du nur", "tokens": ["Schwa\u00b7che", "See\u00b7le", ",", "willst", "du", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Sch\u00f6pfer zanken?", "tokens": ["Mit", "dem", "Sch\u00f6p\u00b7fer", "zan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Heb dich \u00fcber die Natur,", "tokens": ["Heb", "dich", "\u00fc\u00b7ber", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lern f\u00fcr's Elend danken,", "tokens": ["Lern", "f\u00fcr's", "E\u00b7lend", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Unter Zucht", "tokens": ["Un\u00b7ter", "Zucht"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "W\u00e4chst die Frucht,", "tokens": ["W\u00e4chst", "die", "Frucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Reift der Geist zu Freuden", "tokens": ["Reift", "der", "Geist", "zu", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wahrer Seligkeiten.", "tokens": ["Wah\u00b7rer", "Se\u00b7lig\u00