{"dta.poem.8636": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00fcber Gang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Am Strand des Lebens irr' ich, starre d\u00fcster", "tokens": ["Am", "Strand", "des", "Le\u00b7bens", "irr'", "ich", ",", "star\u00b7re", "d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ins Todesmeer, umh\u00fcllt von Nebelflor;", "tokens": ["Ins", "To\u00b7des\u00b7meer", ",", "um\u00b7h\u00fcllt", "von", "Ne\u00b7bel\u00b7flor", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und immer wird der Strand des Lebens w\u00fcster,", "tokens": ["Und", "im\u00b7mer", "wird", "der", "Strand", "des", "Le\u00b7bens", "w\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00f6her schl\u00e4gt die Fluth an ihm empor. \u2014", "tokens": ["Und", "h\u00f6\u00b7her", "schl\u00e4gt", "die", "Fluth", "an", "ihm", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O str\u00f6mt, ihr Thr\u00e4nen, str\u00f6mt! \u2014 im Weiterirren", "tokens": ["O", "str\u00f6mt", ",", "ihr", "Thr\u00e4\u00b7nen", ",", "str\u00f6mt", "!", "im", "Wei\u00b7te\u00b7rir\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$.", "$(", "APPRART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Seh' ich die l\u00e4ngstverlornen Minnestunden,", "tokens": ["Seh'", "ich", "die", "l\u00e4ngst\u00b7ver\u00b7lor\u00b7nen", "Min\u00b7nes\u00b7tun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein neckend Schattenvolk, vor\u00fcberschwirren,", "tokens": ["Ein", "ne\u00b7ckend", "Schat\u00b7ten\u00b7volk", ",", "vor\u00b7\u00fc\u00b7bersc\u00b7hwir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "VVIZU", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Und neuer Schmerz durchgl\u00fcht die alten Wunden.", "tokens": ["Und", "neu\u00b7er", "Schmerz", "durch\u00b7gl\u00fcht", "die", "al\u00b7ten", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Asche meiner Hoffnungen, die Kr\u00e4nze", "tokens": ["Die", "A\u00b7sche", "mei\u00b7ner", "Hoff\u00b7nun\u00b7gen", ",", "die", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Geliebter Todten flattern mir vor\u00fcber,", "tokens": ["Ge\u00b7lieb\u00b7ter", "Tod\u00b7ten", "flat\u00b7tern", "mir", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Gerissen in des Sturmes wilde T\u00e4nze,", "tokens": ["Ge\u00b7ris\u00b7sen", "in", "des", "Stur\u00b7mes", "wil\u00b7de", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und immer wird's in meiner Seele tr\u00fcber. \u2014", "tokens": ["Und", "im\u00b7mer", "wird's", "in", "mei\u00b7ner", "See\u00b7le", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das Christuskreuz, vor dem in sch\u00f6nen Tagen", "tokens": ["Das", "Chris\u00b7tus\u00b7kreuz", ",", "vor", "dem", "in", "sch\u00f6\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ein Kind ich, selig weinend, oft gekniet,", "tokens": ["Ein", "Kind", "ich", ",", "se\u00b7lig", "wei\u00b7nend", ",", "oft", "ge\u00b7kniet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ADJD", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Es h\u00e4ngt hinab vom Strande nun, zerschlagen,", "tokens": ["Es", "h\u00e4ngt", "hin\u00b7ab", "vom", "Stran\u00b7de", "nun", ",", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Dar\u00fcber hin die Todeswelle zieht. \u2014", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "hin", "die", "To\u00b7des\u00b7wel\u00b7le", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Seltsame Stimmen mein' ich nun zu h\u00f6ren:", "tokens": ["Selt\u00b7sa\u00b7me", "Stim\u00b7men", "mein'", "ich", "nun", "zu", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.18": {"text": "Ein wirres Plaudern bald kommt's meinem Lauschen", "tokens": ["Ein", "wir\u00b7res", "Plau\u00b7dern", "bald", "kommt's", "mei\u00b7nem", "Lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Meer\u00fcber her, bald t\u00f6nt's in leisen Ch\u00f6ren,", "tokens": ["Meer\u00b7\u00fc\u00b7ber", "her", ",", "bald", "t\u00f6nt's", "in", "lei\u00b7sen", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.20": {"text": "Dann wieder schweigt's, und nur die Wellen rauschen. \u2014", "tokens": ["Dann", "wie\u00b7der", "schweigt's", ",", "und", "nur", "die", "Wel\u00b7len", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "KON", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ein ernster Freund, mein einziges Geleite", "tokens": ["Ein", "erns\u00b7ter", "Freund", ",", "mein", "ein\u00b7zi\u00b7ges", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Weist stumm hinunter in die dunkle Fluth;", "tokens": ["Weist", "stumm", "hin\u00b7un\u00b7ter", "in", "die", "dunk\u00b7le", "Fluth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Stets enger dr\u00e4ngt er sich an meine Seite:", "tokens": ["Stets", "en\u00b7ger", "dr\u00e4ngt", "er", "sich", "an", "mei\u00b7ne", "Sei\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Umarme mich, du stiller Todesmuth!", "tokens": ["Um\u00b7ar\u00b7me", "mich", ",", "du", "stil\u00b7ler", "To\u00b7des\u00b7muth", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}