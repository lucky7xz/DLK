{"textgrid.poem.55837": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Martyrinnen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Martyrin ist sie. Und als harten Falls", "tokens": ["Mar\u00b7ty\u00b7rin", "ist", "sie", ".", "Und", "als", "har\u00b7ten", "Falls"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "$.", "KON", "KOUS", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "mit einem Ruck", "tokens": ["mit", "ei\u00b7nem", "Ruck"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "das Beil durch ihre kurze Jugend ging,", "tokens": ["das", "Beil", "durch", "ih\u00b7re", "kur\u00b7ze", "Ju\u00b7gend", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "da legte sich der feine rote Ring", "tokens": ["da", "leg\u00b7te", "sich", "der", "fei\u00b7ne", "ro\u00b7te", "Ring"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "um ihren Hals, und war der erste Schmuck,", "tokens": ["um", "ih\u00b7ren", "Hals", ",", "und", "war", "der", "ers\u00b7te", "Schmuck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "den sie mit einem fremden L\u00e4cheln nahm;", "tokens": ["den", "sie", "mit", "ei\u00b7nem", "frem\u00b7den", "L\u00e4\u00b7cheln", "nahm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "aber auch den ertr\u00e4gt sie nur mit Scham.", "tokens": ["a\u00b7ber", "auch", "den", "er\u00b7tr\u00e4gt", "sie", "nur", "mit", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Und wenn sie schl\u00e4ft, mu\u00df ihre junge Schwester", "tokens": ["Und", "wenn", "sie", "schl\u00e4ft", ",", "mu\u00df", "ih\u00b7re", "jun\u00b7ge", "Schwes\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "(die, kindisch noch, sich mit der Wunde schm\u00fcckt", "tokens": ["(", "die", ",", "kin\u00b7disch", "noch", ",", "sich", "mit", "der", "Wun\u00b7de", "schm\u00fcckt"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "$,", "ADJD", "ADV", "$,", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "von jenem Stein, der ihr die Stirn erdr\u00fcckt)", "tokens": ["von", "je\u00b7nem", "Stein", ",", "der", "ihr", "die", "Stirn", "er\u00b7dr\u00fcckt", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "die harten Arme um den Hals ihr halten,", "tokens": ["die", "har\u00b7ten", "Ar\u00b7me", "um", "den", "Hals", "ihr", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "und oft im Traume fleht die andre: Fester, fester.", "tokens": ["und", "oft", "im", "Trau\u00b7me", "fleht", "die", "and\u00b7re", ":", "Fes\u00b7ter", ",", "fes\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "ADJA", "$.", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und da f\u00e4llt es dem Kinde manchmal ein,", "tokens": ["Und", "da", "f\u00e4llt", "es", "dem", "Kin\u00b7de", "manch\u00b7mal", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "die Stirne mit dem Bild von jenem Stein", "tokens": ["die", "Stir\u00b7ne", "mit", "dem", "Bild", "von", "je\u00b7nem", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "zu bergen in des sanften Nachtgewandes Falten,", "tokens": ["zu", "ber\u00b7gen", "in", "des", "sanf\u00b7ten", "Nacht\u00b7ge\u00b7wan\u00b7des", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.16": {"text": "das von der Schwester Atmen hell sich hebt,", "tokens": ["das", "von", "der", "Schwes\u00b7ter", "At\u00b7men", "hell", "sich", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "voll wie ein Segel, das vom Winde lebt.", "tokens": ["voll", "wie", "ein", "Se\u00b7gel", ",", "das", "vom", "Win\u00b7de", "lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das ist die Stunde, da sie heilig sind,", "tokens": ["Das", "ist", "die", "Stun\u00b7de", ",", "da", "sie", "hei\u00b7lig", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die stille Jungfrau und das blasse Kind.", "tokens": ["die", "stil\u00b7le", "Jung\u00b7frau", "und", "das", "blas\u00b7se", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da sind sie wieder wie vor allem Leide", "tokens": ["Da", "sind", "sie", "wie\u00b7der", "wie", "vor", "al\u00b7lem", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KOKOM", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und schlafen arm und haben keinen Ruhm,", "tokens": ["und", "schla\u00b7fen", "arm", "und", "ha\u00b7ben", "kei\u00b7nen", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und ihre Seelen sind wie wei\u00dfe Seide,", "tokens": ["und", "ih\u00b7re", "See\u00b7len", "sind", "wie", "wei\u00b7\u00dfe", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und von derselben Sehnsucht beben beide", "tokens": ["und", "von", "der\u00b7sel\u00b7ben", "Sehn\u00b7sucht", "be\u00b7ben", "bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und f\u00fcrchten sich vor ihrem Heldentum.", "tokens": ["und", "f\u00fcrch\u00b7ten", "sich", "vor", "ih\u00b7rem", "Hel\u00b7den\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und du kannst meinen: wenn sie aus den Betten", "tokens": ["Und", "du", "kannst", "mei\u00b7nen", ":", "wenn", "sie", "aus", "den", "Bet\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "$.", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aufst\u00fcnden bei dem n\u00e4chsten Morgenlichte", "tokens": ["auf\u00b7st\u00fcn\u00b7den", "bei", "dem", "n\u00e4chs\u00b7ten", "Mor\u00b7gen\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und, mit demselben tr\u00e4umenden Gesichte,", "tokens": ["und", ",", "mit", "dem\u00b7sel\u00b7ben", "tr\u00e4u\u00b7men\u00b7den", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die Gassen k\u00e4men in den kleinen St\u00e4dten, \u2013", "tokens": ["die", "Gas\u00b7sen", "k\u00e4\u00b7men", "in", "den", "klei\u00b7nen", "St\u00e4d\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "es bliebe keiner hinter ihnen staunen,", "tokens": ["es", "blie\u00b7be", "kei\u00b7ner", "hin\u00b7ter", "ih\u00b7nen", "stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "kein Fenster klirrte an den H\u00e4userreihn,", "tokens": ["kein", "Fens\u00b7ter", "klirr\u00b7te", "an", "den", "H\u00e4u\u00b7ser\u00b7reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und nirgends bei den Frauen ging ein Raunen,", "tokens": ["und", "nir\u00b7gends", "bei", "den", "Frau\u00b7en", "ging", "ein", "Rau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "und keines von den Kindern w\u00fcrde schrein.", "tokens": ["und", "kei\u00b7nes", "von", "den", "Kin\u00b7dern", "w\u00fcr\u00b7de", "schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Sie schritten durch die Stille in den Hemden", "tokens": ["Sie", "schrit\u00b7ten", "durch", "die", "Stil\u00b7le", "in", "den", "Hem\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "(die flachen Falten geben keinen Glanz)", "tokens": ["(", "die", "fla\u00b7chen", "Fal\u00b7ten", "ge\u00b7ben", "kei\u00b7nen", "Glanz", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "so fremd, und dennoch keinem zum Befremden,", "tokens": ["so", "fremd", ",", "und", "den\u00b7noch", "kei\u00b7nem", "zum", "Be\u00b7frem\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KON", "ADV", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "so wie zu Festen, aber ohne Kranz.", "tokens": ["so", "wie", "zu", "Fes\u00b7ten", ",", "a\u00b7ber", "oh\u00b7ne", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Martyrin ist sie. Und als harten Falls", "tokens": ["Mar\u00b7ty\u00b7rin", "ist", "sie", ".", "Und", "als", "har\u00b7ten", "Falls"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER"