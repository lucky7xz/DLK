{"dta.poem.9333": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n Quadrain/  \n so sich von M\u00e4nnlichen anf\u00e4ht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Jungfern gleuben wil/ mu\u00df pfl\u00fcgen auff der", "tokens": ["Wer", "Jung\u00b7fern", "gleu\u00b7ben", "wil", "/", "mu\u00df", "pfl\u00fc\u00b7gen", "auff", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VVINF", "VMFIN", "$(", "VMFIN", "VVINF", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "See/", "tokens": ["See", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "und seen in den Sand: wer jhren worten trauet;", "tokens": ["und", "seen", "in", "den", "Sand", ":", "wer", "jhren", "wor\u00b7ten", "trau\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$.", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "helt mit der hand die lust/ auf kemen grund er bauet;", "tokens": ["helt", "mit", "der", "hand", "die", "lust", "/", "auf", "ke\u00b7men", "grund", "er", "bau\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$(", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "wie glaa\u00df ist Jhre gunst/ dr\u00fcmb pack dich Galatee.", "tokens": ["wie", "glaa\u00df", "ist", "Ih\u00b7re", "gunst", "/", "dr\u00fcmb", "pack", "dich", "Ga\u00b