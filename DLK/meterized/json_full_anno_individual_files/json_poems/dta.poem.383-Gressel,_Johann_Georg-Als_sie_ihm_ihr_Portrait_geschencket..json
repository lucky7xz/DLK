{"dta.poem.383": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihm ihr  Portrait  geschencket.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll dein Schatten-Bild annehmliche Lysette?", "tokens": ["Was", "soll", "dein", "Schat\u00b7ten\u00b7Bild", "an\u00b7nehm\u00b7li\u00b7che", "Ly\u00b7set\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Deiner holden Sch\u00f6nheit-Zier", "tokens": ["Dei\u00b7ner", "hol\u00b7den", "Sch\u00f6n\u00b7heit\u00b7Zier"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stelt kein schlechter Pinsul f\u00fcr.", "tokens": ["Stelt", "kein", "schlech\u00b7ter", "Pin\u00b7sul", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Es stehet zwar das Bild recht unvergleichlich nette/", "tokens": ["Es", "ste\u00b7het", "zwar", "das", "Bild", "recht", "un\u00b7ver\u00b7gleich\u00b7lich", "net\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "ADJD", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch das ", "tokens": ["Doch", "das"], "token_info": ["word", "word"], "pos": ["KON", "PDS"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Stelle mir dasselbe zu/", "tokens": ["Stel\u00b7le", "mir", "das\u00b7sel\u00b7be", "zu", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So vermehrst du meine Ruh/", "tokens": ["So", "ver\u00b7mehrst", "du", "mei\u00b7ne", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die sich auf deine Gunst als ihren Ancker gr\u00fcndet.", "tokens": ["Die", "sich", "auf", "dei\u00b7ne", "Gunst", "als", "ih\u00b7ren", "An\u00b7cker", "gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}