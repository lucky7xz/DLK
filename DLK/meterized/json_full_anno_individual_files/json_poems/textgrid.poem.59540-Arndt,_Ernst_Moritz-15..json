{"textgrid.poem.59540": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der muntre Tag ist wieder still,", "tokens": ["Der", "mun\u00b7tre", "Tag", "ist", "wie\u00b7der", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alles schlafen gehen will,", "tokens": ["Und", "al\u00b7les", "schla\u00b7fen", "ge\u00b7hen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Wild auf weichen Mooses Flaum,", "tokens": ["Das", "Wild", "auf", "wei\u00b7chen", "Moo\u00b7ses", "Flaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Vogel auf den gr\u00fcnen Baum,", "tokens": ["Der", "Vo\u00b7gel", "auf", "den", "gr\u00fc\u00b7nen", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Mensch in seine stille Kammer,", "tokens": ["Der", "Mensch", "in", "sei\u00b7ne", "stil\u00b7le", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich auszuruhn von M\u00fch' und Jammer.", "tokens": ["Sich", "aus\u00b7zu\u00b7ruhn", "von", "M\u00fch'", "und", "Jam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVIZU", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch tritt er aus der H\u00fcttent\u00fcr", "tokens": ["Doch", "tritt", "er", "aus", "der", "H\u00fct\u00b7ten\u00b7t\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zuvor noch in die Nacht herf\u00fcr,", "tokens": ["Zu\u00b7vor", "noch", "in", "die", "Nacht", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich christlich erst bereiten mu\u00df", "tokens": ["Sich", "christ\u00b7lich", "erst", "be\u00b7rei\u00b7ten", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Liebesdank und Liebesgru\u00df,", "tokens": ["Mit", "Lie\u00b7bes\u00b7dank", "und", "Lie\u00b7bes\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mu\u00df sehen, wie die Sterne blinken,", "tokens": ["Mu\u00df", "se\u00b7hen", ",", "wie", "die", "Ster\u00b7ne", "blin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und noch den Odem Gottes trinken.", "tokens": ["Und", "noch", "den", "O\u00b7dem", "Got\u00b7tes", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du, der von oben Wache h\u00e4lt,", "tokens": ["Du", ",", "der", "von", "o\u00b7ben", "Wa\u00b7che", "h\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du milder Vater aller Welt,", "tokens": ["Du", "mil\u00b7der", "Va\u00b7ter", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vernimm mein stammelndes Gebet,", "tokens": ["Ver\u00b7nimm", "mein", "stam\u00b7meln\u00b7des", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das zu den hellen Sternen geht,", "tokens": ["Das", "zu", "den", "hel\u00b7len", "Ster\u00b7nen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wollst mich von deinen Sonnenkreisen", "tokens": ["Wollst", "mich", "von", "dei\u00b7nen", "Son\u00b7nen\u00b7krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im rechten Beten unterweisen.", "tokens": ["Im", "rech\u00b7ten", "Be\u00b7ten", "un\u00b7ter\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich war den Tag in deiner Hut,", "tokens": ["Ich", "war", "den", "Tag", "in", "dei\u00b7ner", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beh\u00fct' auch heint mich, Vater gut,", "tokens": ["Be\u00b7h\u00fct'", "auch", "heint", "mich", ",", "Va\u00b7ter", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PPER", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch deine milde Freundlichkeit", "tokens": ["Durch", "dei\u00b7ne", "mil\u00b7de", "Freund\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vorm b\u00f6sen Feind und seinem Neid;", "tokens": ["Vorm", "b\u00f6\u00b7sen", "Feind", "und", "sei\u00b7nem", "Neid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn was den Leib mir mag befallen,", "tokens": ["Denn", "was", "den", "Leib", "mir", "mag", "be\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist das kleinste Leid von allen.", "tokens": ["Das", "ist", "das", "kleins\u00b7te", "Leid", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O sende von dem Strahlenschein", "tokens": ["O", "sen\u00b7de", "von", "dem", "Strah\u00b7len\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den liebsten Engel zu mir ein", "tokens": ["Den", "liebs\u00b7ten", "En\u00b7gel", "zu", "mir", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Friedensboten unters Dach,", "tokens": ["Als", "Frie\u00b7dens\u00b7bo\u00b7ten", "un\u00b7ters", "Dach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als W\u00e4chter in mein Schlafgemach,", "tokens": ["Als", "W\u00e4ch\u00b7ter", "in", "mein", "Schlaf\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df Herz und Sinne und Gedanken", "tokens": ["Da\u00df", "Herz", "und", "Sin\u00b7ne", "und", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich fest um deinen Himmel ranken.", "tokens": ["Sich", "fest", "um", "dei\u00b7nen", "Him\u00b7mel", "ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann geht der Tag so lustig fort,", "tokens": ["Dann", "geht", "der", "Tag", "so", "lus\u00b7tig", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann klingt die Nacht ein Liebeswort,", "tokens": ["Dann", "klingt", "die", "Nacht", "ein", "Lie\u00b7bes\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann ist der Morgen Engelgru\u00df,", "tokens": ["Dann", "ist", "der", "Mor\u00b7gen", "En\u00b7gel\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem alles B\u00f6se weichen mu\u00df,", "tokens": ["Dem", "al\u00b7les", "B\u00f6\u00b7se", "wei\u00b7chen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wir hienieden schon auf Erden", "tokens": ["Und", "wir", "hien\u00b7ie\u00b7den", "schon", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie helle Kinder Gottes werden.", "tokens": ["Wie", "hel\u00b7le", "Kin\u00b7der", "Got\u00b7tes", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und f\u00e4llt der letzte Abendschein", "tokens": ["Und", "f\u00e4llt", "der", "letz\u00b7te", "A\u00b7bend\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einst in das m\u00fcde Aug' hinein,", "tokens": ["Einst", "in", "das", "m\u00fc\u00b7de", "Aug'", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sehnt meine Seele sich hinauf", "tokens": ["Sehnt", "mei\u00b7ne", "See\u00b7le", "sich", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "ADV"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum ewig sel'gen Sonnenlauf,", "tokens": ["Zum", "e\u00b7wig", "sel'\u00b7gen", "Son\u00b7nen\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So werden alle Engel kommen", "tokens": ["So", "wer\u00b7den", "al\u00b7le", "En\u00b7gel", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mich heimzuholen zu den Frommen.", "tokens": ["Mich", "heim\u00b7zu\u00b7ho\u00b7len", "zu", "den", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der muntre Tag ist wieder still,", "tokens": ["Der", "mun\u00b7tre", "Tag", "ist", "wie\u00b7der", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alles schlafen gehen will,", "tokens": ["Und", "al\u00b7les", "schla\u00b7fen", "ge\u00b7hen", "will", ","], "token_info": ["word", "word", "word", "word",