ne.3": {"text": "Vernimm mein stammelndes Gebet,", "tokens": ["Ver\u00b7nimm", "mein", "stam\u00b7meln\u00b7des", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das zu den hellen Sternen geht,", "tokens": ["Das", "zu", "den", "hel\u00b7len", "Ster\u00b7nen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wollst mich von deinen Sonnenkreisen", "tokens": ["Wollst", "mich", "von", "dei\u00b7nen", "Son\u00b7nen\u00b7krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im rechten Beten unterweisen.", "tokens": ["Im", "rech\u00b7ten", "Be\u00b7ten", "un\u00b7ter\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich war den Tag in deiner Hut,", "tokens": ["Ich", "war", "den", "Tag", "in", "dei\u00b7ner", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beh\u00fct' auch heint mich, Vater gut,", "tokens": ["Be\u00b7h\u00fct'", "auch", "heint", "mich", ",", "Va\u00b7ter", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PPER", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch deine milde Freundlichkeit", "tokens": ["Durch", "dei\u00b7ne", "mil\u00b7d