{"textgrid.poem.46121": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Seiner liebsten lob", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vil sch\u00f6ner, dan der sonnenglanz,", "tokens": ["Vil", "sch\u00f6\u00b7ner", ",", "dan", "der", "son\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vil s\u00fc\u00dfer, dan ein blumenkranz", "tokens": ["vil", "s\u00fc\u00b7\u00dfer", ",", "dan", "ein", "blu\u00b7men\u00b7kranz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ist meine Myrta anzuschauen;", "tokens": ["ist", "mei\u00b7ne", "Myr\u00b7ta", "an\u00b7zu\u00b7schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sie ist ein tag selbs voll klarheit,", "tokens": ["sie", "ist", "ein", "tag", "selbs", "voll", "klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "sie ist der ruhm selbs der sch\u00f6nheit,", "tokens": ["sie", "ist", "der", "ruhm", "selbs", "der", "sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "die sch\u00f6nste under den jungfrauen.", "