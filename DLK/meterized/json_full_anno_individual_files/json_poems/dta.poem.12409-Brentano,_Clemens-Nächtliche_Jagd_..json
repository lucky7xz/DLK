{"dta.poem.12409": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4chtliche Jagd .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Lust th\u00e4t ich ausreiten               ", "tokens": ["Mit", "Lust", "th\u00e4t", "ich", "aus\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch einen gr\u00fcnen Wald,", "tokens": ["Durch", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darin da h\u00f6rt ich singen,", "tokens": ["Da\u00b7rin", "da", "h\u00f6rt", "ich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drey V\u00f6glein wohlgestalt.", "tokens": ["Drey", "V\u00f6\u00b7glein", "wohl\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sind es nicht drey V\u00f6gelein,", "tokens": ["Und", "sind", "es", "nicht", "drey", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So sind's drey Fr\u00e4ulein fein;", "tokens": ["So", "sin\u00b7d's", "drey", "Fr\u00e4u\u00b7lein", "fein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "CARD", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Soll mir das ein nicht werden,", "tokens": ["Soll", "mir", "das", "ein", "nicht", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "ART", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So gilts das Leben mein.", "tokens": ["So", "gilts", "das", "Le\u00b7ben", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Abendstrahlen breiten", "tokens": ["Die", "A\u00b7bend\u00b7strah\u00b7len", "brei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Goldnetz \u00fcbern Wald,", "tokens": ["Das", "Gold\u00b7netz", "\u00fc\u00b7bern", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ihm entgegen streiten", "tokens": ["Und", "ihm", "ent\u00b7ge\u00b7gen", "strei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKVZ", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die V\u00f6glein, da\u00df es schallt;", "tokens": ["Die", "V\u00f6\u00b7glein", ",", "da\u00df", "es", "schallt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich stehe auf der Lauer,", "tokens": ["Ich", "ste\u00b7he", "auf", "der", "Lau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich harr auf dunkle Nacht,", "tokens": ["Ich", "harr", "auf", "dunk\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es hat der Abendschauer", "tokens": ["Es", "hat", "der", "A\u00b7bend\u00b7schau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Herz wohl weich gemacht.", "tokens": ["Ihr", "Herz", "wohl", "weich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ins Jubelhorn ich stosse,", "tokens": ["Ins", "Ju\u00b7bel\u00b7horn", "ich", "stos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Firmament wird klar,", "tokens": ["Das", "Fir\u00b7ma\u00b7ment", "wird", "klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich steige von dem Rosse", "tokens": ["Ich", "stei\u00b7ge", "von", "dem", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und z\u00e4hl die V\u00f6gelschaar.", "tokens": ["Und", "z\u00e4hl", "die", "V\u00f6\u00b7gel\u00b7schaar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die ein ist schwarzbraun Anne,", "tokens": ["Die", "ein", "ist", "schwarz\u00b7braun", "An\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VAFIN", "ADJD", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die andre B\u00e4rbelein,", "tokens": ["Die", "and\u00b7re", "B\u00e4r\u00b7be\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die dritt hat keinen Namen,", "tokens": ["Die", "dritt", "hat", "kei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die soll des J\u00e4gers seyn.", "tokens": ["Die", "soll", "des", "J\u00e4\u00b7gers", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da dr\u00fcben auf jenem Berge,", "tokens": ["Da", "dr\u00fc\u00b7ben", "auf", "je\u00b7nem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da steht der rothe Mond,", "tokens": ["Da", "steht", "der", "ro\u00b7the", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier h\u00fcben in diesem Thale,", "tokens": ["Hier", "h\u00fc\u00b7ben", "in", "die\u00b7sem", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein feines Liebchen wohnt.", "tokens": ["Mein", "fei\u00b7nes", "Lieb\u00b7chen", "wohnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kehr dich Feinslieb herumme,", "tokens": ["Kehr", "dich", "Feins\u00b7lieb", "her\u00b7um\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Beu ihm den rothen Mund,", "tokens": ["Beu", "ihm", "den", "ro\u00b7then", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Sonst ist die Nacht schon umme,", "tokens": ["Sonst", "ist", "die", "Nacht", "schon", "um\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Es schl\u00e4gt schon an der Hund.", "tokens": ["Es", "schl\u00e4gt", "schon", "an", "der", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}