{"dta.poem.5343": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Blum ist sch\u00f6n und riecht; du denckest;", "tokens": ["Die", "Blum", "ist", "sch\u00f6n", "und", "riecht", ";", "du", "den\u00b7ckest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADJD", "KON", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ach, da\u00df du dem, der alle Pracht", "tokens": ["Ach", ",", "da\u00df", "du", "dem", ",", "der", "al\u00b7le", "Pracht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ART", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Creatur hervorgebracht,", "tokens": ["Der", "Crea\u00b7tur", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jm Danck und Preise seiner Macht,", "tokens": ["Jm", "Danck", "und", "Prei\u00b7se", "sei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn kein vergn\u00fcgtes Dencken schenckest?", "tokens": ["Denn", "kein", "ver\u00b7gn\u00fcg\u00b7tes", "Den\u00b7cken", "schen\u00b7ckest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sey (weil du doch auf andre Weise", "tokens": ["Sey", "(", "weil", "du", "doch", "auf", "and\u00b7re", "Wei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$(", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nichts wircken kanst zu seinem Preise)", "tokens": ["Nichts", "wir\u00b7cken", "kanst", "zu", "sei\u00b7nem", "Prei\u00b7se", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu deines grossen Sch\u00f6pfers Ruhme", "tokens": ["Zu", "dei\u00b7nes", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nicht weniger, als eine Blume!", "tokens": ["Nicht", "we\u00b7ni\u00b7ger", ",", "als", "ei\u00b7ne", "Blu\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}