{"dta.poem.15985": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Sch\u00f6ne stammet her vom Schonen, es ist zart,", "tokens": ["Das", "Sch\u00f6\u00b7ne", "stam\u00b7met", "her", "vom", "Scho\u00b7nen", ",", "es", "ist", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und will behandelt seyn wie Blumen edler Art;", "tokens": ["Und", "will", "be\u00b7han\u00b7delt", "seyn", "wie", "Blu\u00b7men", "ed\u00b7ler", "Art", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVPP", "VAINF", "KOKOM", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie Blumen vor dem Frost und rauher St\u00fcrme Drohen", "tokens": ["Wie", "Blu\u00b7men", "vor", "dem", "Frost", "und", "rau\u00b7her", "St\u00fcr\u00b7me", "Dro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will es geschonet seyn, verschont von allem Rohen.", "tokens": ["Will", "es", "ge\u00b7scho\u00b7net", "seyn", ",", "ver\u00b7schont", "von", "al\u00b7lem", "Ro\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$,", "VVPP", "APPR", "PIS", "NN", "$."], "meter": "+--+-+-+-+-+-",