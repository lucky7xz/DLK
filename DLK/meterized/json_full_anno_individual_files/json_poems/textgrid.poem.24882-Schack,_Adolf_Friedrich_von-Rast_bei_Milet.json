{"textgrid.poem.24882": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Rast bei Milet", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun f\u00fcllt die Becher mit funkelndem Wein!", "tokens": ["Nun", "f\u00fcllt", "die", "Be\u00b7cher", "mit", "fun\u00b7keln\u00b7dem", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sanft rastet sich's hier, wo in langen Reihn", "tokens": ["Sanft", "ras\u00b7tet", "sich's", "hier", ",", "wo", "in", "lan\u00b7gen", "Reihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "ADV", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Gebrochene S\u00e4ulen ragen;", "tokens": ["Ge\u00b7bro\u00b7che\u00b7ne", "S\u00e4u\u00b7len", "ra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dar\u00fcber hinweg das blauende Meer", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "hin\u00b7weg", "das", "blau\u00b7en\u00b7de", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APZR", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und die Quadern des Tempeldaches umher", "tokens": ["Und", "die", "Qua\u00b7dern", "des", "Tem\u00b7pel\u00b7da\u00b7ches", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "PTKVZ"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Mit den Riesen, die es getragen.", "tokens": ["Mit", "den", "Rie\u00b7sen", ",", "die", "es", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "In Schutt gesunken das hohe Milet!", "tokens": ["In", "Schutt", "ge\u00b7sun\u00b7ken", "das", "ho\u00b7he", "Mi\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Asche der Helden und Weisen verweht,", "tokens": ["Die", "A\u00b7sche", "der", "Hel\u00b7den", "und", "Wei\u00b7sen", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Name \u00bbHellenen\u00ab verklungen!", "tokens": ["Der", "Na\u00b7me", "\u00bb", "Hel\u00b7le\u00b7nen", "\u00ab", "ver\u00b7klun\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Um Tr\u00fcmmer nun t\u00f6nt der Wogen Geroll,", "tokens": ["Um", "Tr\u00fcm\u00b7mer", "nun", "t\u00f6nt", "der", "Wo\u00b7gen", "Ge\u00b7roll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ADV", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Des Schakals Heulen, wo einst dem Apoll", "tokens": ["Des", "Scha\u00b7kals", "Heu\u00b7len", ",", "wo", "einst", "dem", "A\u00b7poll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Die Dichter Hymnen gesungen.", "tokens": ["Die", "Dich\u00b7ter", "Hym\u00b7nen", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch, ob der Glanz der V\u00f6lker erlischt,", "tokens": ["Doch", ",", "ob", "der", "Glanz", "der", "V\u00f6l\u00b7ker", "er\u00b7lischt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ob allen Winden ihr Staub sich mischt,", "tokens": ["Ob", "al\u00b7len", "Win\u00b7den", "ihr", "Staub", "sich", "mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Kommenden bleibt ihr Verm\u00e4chtnis;", "tokens": ["Den", "Kom\u00b7men\u00b7den", "bleibt", "ihr", "Ver\u00b7m\u00e4cht\u00b7nis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und was sie geschaffen in That und Wort,", "tokens": ["Und", "was", "sie", "ge\u00b7schaf\u00b7fen", "in", "That", "und", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Lebt herrlich und hoch noch fort und fort", "tokens": ["Lebt", "herr\u00b7lich", "und", "hoch", "noch", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ADV", "PTKVZ", "KON", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In sp\u00e4tester Enkel Ged\u00e4chtnis.", "tokens": ["In", "sp\u00e4\u00b7tes\u00b7ter", "En\u00b7kel", "Ge\u00b7d\u00e4cht\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Gl\u00fcckselig, wer Gro\u00dfes auf Erden vollbracht!", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", ",", "wer", "Gro\u00b7\u00dfes", "auf", "Er\u00b7den", "voll\u00b7bracht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nicht bangt ihm, wenn sie in ewiger Nacht", "tokens": ["Nicht", "bangt", "ihm", ",", "wenn", "sie", "in", "e\u00b7wi\u00b7ger", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dort unten die Gruft ihm bereiten;", "tokens": ["Dort", "un\u00b7ten", "die", "Gruft", "ihm", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Er wei\u00df, solange die Sonne kreist,", "tokens": ["Er", "wei\u00df", ",", "so\u00b7lan\u00b7ge", "die", "Son\u00b7ne", "kreist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wird leuchtend von Jahre zu Jahre sein Geist", "tokens": ["Wird", "leuch\u00b7tend", "von", "Jah\u00b7re", "zu", "Jah\u00b7re", "sein", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der Menschen Geschlechter durchschreiten.", "tokens": ["Der", "Men\u00b7schen", "Ge\u00b7schlech\u00b7ter", "durch\u00b7schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Auf, Freunde! Noch strahlt uns der Lebenstag;", "tokens": ["Auf", ",", "Freun\u00b7de", "!", "Noch", "strahlt", "uns", "der", "Le\u00b7bens\u00b7tag", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auch uns, da\u00df man unser gedenken mag,", "tokens": ["Auch", "uns", ",", "da\u00df", "man", "un\u00b7ser", "ge\u00b7den\u00b7ken", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PIS", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "La\u00dft wirken und streben und ringen!", "tokens": ["La\u00dft", "wir\u00b7ken", "und", "stre\u00b7ben", "und", "rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sto\u00dft an auf den Ruhm, der nimmer vergeht,", "tokens"