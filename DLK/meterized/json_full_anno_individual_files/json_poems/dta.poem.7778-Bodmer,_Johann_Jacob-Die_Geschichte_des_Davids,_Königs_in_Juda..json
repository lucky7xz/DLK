{"dta.poem.7778": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich heb die Augen auf aus allen Tr\u00fcbsalsgr\u00fcnden", "tokens": ["Ich", "heb", "die", "Au\u00b7gen", "auf", "aus", "al\u00b7len", "Tr\u00fcb\u00b7sals\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hin zu dem hohen Berg, von wannen H\u00fclff zu finden,", "tokens": ["Hin", "zu", "dem", "ho\u00b7hen", "Berg", ",", "von", "wan\u00b7nen", "H\u00fclff", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein H\u00fclffe kommt von Gott, vom Herren kommt Genad,", "tokens": ["Mein", "H\u00fclf\u00b7fe", "kommt", "von", "Gott", ",", "vom", "Her\u00b7ren", "kommt", "Ge\u00b7nad", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,", "APPRART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Himmel und die Erd so sch\u00f6n gemachet hat.", "tokens": ["Der", "Him\u00b7mel", "und", "die", "Erd", "so", "sch\u00f6n", "ge\u00b7ma\u00b7chet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Herr wird deinen Fu\u00df niemahlen gleiten lassen,", "tokens": ["Der", "Herr", "wird", "dei\u00b7nen", "Fu\u00df", "nie\u00b7mah\u00b7len", "glei\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein Schlaff den trifft, der dich beh\u00fctet, allermassen", "tokens": ["Kein", "Schlaff", "den", "trifft", ",", "der", "dich", "be\u00b7h\u00fc\u00b7tet", ",", "al\u00b7ler\u00b7mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "ART", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "XY"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der H\u00fcter Jsrael ja schl\u00e4ft noch schlummert nicht.", "tokens": ["Der", "H\u00fc\u00b7ter", "Js\u00b7rael", "ja", "schl\u00e4ft", "noch", "schlum\u00b7mert", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "VVFIN", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Dein Schatten ist der Herr, da\u00df dir nicht was gebricht:", "tokens": ["Dein", "Schat\u00b7ten", "ist", "der", "Herr", ",", "da\u00df", "dir", "nicht", "was", "ge\u00b7bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df dich bey Tag die Hitz der Sonnen nicht kan stechen,", "tokens": ["Da\u00df", "dich", "bey", "Tag", "die", "Hitz", "der", "Son\u00b7nen", "nicht", "kan", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Noch bey der Nacht der Mond, kein Schrecken mu\u00df dich schw\u00e4-", "tokens": ["Noch", "bey", "der", "Nacht", "der", "Mond", ",", "kein", "Schre\u00b7cken", "mu\u00df", "dich", "schw\u00e4"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "PIAT", "NN", "VMFIN", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dann deine Seel der Herr f\u00fcr allem Leyd befreyt,", "tokens": ["Dann", "dei\u00b7ne", "Seel", "der", "Herr", "f\u00fcr", "al\u00b7lem", "Leyd", "be\u00b7freyt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "PIS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "(chen,", "tokens": ["(", "chen", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "NE", "$,"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Den Ein- und Ausgang er beh\u00fct in Ewigkeit.", "tokens": ["Den", "Ein", "und", "Aus\u00b7gang", "er", "be\u00b7h\u00fct", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}