{"textgrid.poem.36603": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Frau Karschin", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundin, keine Sorgen pfl\u00fcgen", "tokens": ["Freun\u00b7din", ",", "kei\u00b7ne", "Sor\u00b7gen", "pfl\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Furchen in mein Angesicht,", "tokens": ["Fur\u00b7chen", "in", "mein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich bek\u00fcmmre mich um F\u00fcrsten", "tokens": ["Ich", "be\u00b7k\u00fcmm\u00b7re", "mich", "um", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und um Gold und Silber nicht!", "tokens": ["Und", "um", "Gold", "und", "Sil\u00b7ber", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber, wenn der Winter weichet,", "tokens": ["A\u00b7ber", ",", "wenn", "der", "Win\u00b7ter", "wei\u00b7chet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Fr\u00fchling Blumen bringt,", "tokens": ["Und", "der", "Fr\u00fch\u00b7ling", "Blu\u00b7men", "bringt", ","], "token_info": ["word", "word", "word", "wor