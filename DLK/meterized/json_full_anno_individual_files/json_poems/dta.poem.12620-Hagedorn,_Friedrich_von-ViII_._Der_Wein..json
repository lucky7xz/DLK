{"dta.poem.12620": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "ViII .  \n  Der Wein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus den Reben", "tokens": ["Aus", "den", "Re\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Fleu\u00dft das Leben:", "tokens": ["Fleu\u00dft", "das", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Das ist offenbar.", "tokens": ["Das", "ist", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Jhr, der Trauben Kenner!", "tokens": ["Ihr", ",", "der", "Trau\u00b7ben", "Ken\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Weingelehrte M\u00e4nner!", "tokens": ["Wein\u00b7ge\u00b7lehr\u00b7te", "M\u00e4n\u00b7ner", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Macht die\u00df Sprichwort wahr.", "tokens": ["Macht", "die\u00df", "Sprich\u00b7wort", "wahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Niemals gl\u00fchten", "tokens": ["Nie\u00b7mals", "gl\u00fch\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Rechabiten,", "tokens": ["Rec\u00b7ha\u00b7bi\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Edler Most! von dir.", "tokens": ["Ed\u00b7ler", "Most", "!", "von", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Aber, Wein-Erfinder,", "tokens": ["A\u00b7ber", ",", "Wein\u00b7Er\u00b7fin\u00b7der", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["KON", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Noah! deine Kinder", "tokens": ["Noah", "!", "dei\u00b7ne", "Kin\u00b7der"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Zechten so, wie wir.", "tokens": ["Zech\u00b7ten", "so", ",", "wie", "wir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ueberzogen", "tokens": ["Ue\u00b7ber\u00b7zo\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Regenbogen", "tokens": ["Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Gleich das Firmament:", "tokens": ["Gleich", "das", "Fir\u00b7ma\u00b7ment", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "So ward deiner Freude", "tokens": ["So", "ward", "dei\u00b7ner", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mehr als Augenweide,", "tokens": ["Mehr", "als", "Au\u00b7gen\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Jhr ward Wein geg\u00f6nnt.", "tokens": ["Ihr", "ward", "Wein", "ge\u00b7g\u00f6nnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Deinentwegen", "tokens": ["Dei\u00b7nent\u00b7we\u00b7gen"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Kam der Segen,", "tokens": ["Kam", "der", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wuchs der erste Wein.", "tokens": ["Wuchs", "der", "ers\u00b7te", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Nach den Wasser-Fluthen", "tokens": ["Nach", "den", "Was\u00b7ser\u00b7Flu\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Konnte nichts den Guten", "tokens": ["Konn\u00b7te", "nichts", "den", "Gu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Gr\u00f6ssern Trost verleihn.", "tokens": ["Gr\u00f6s\u00b7sern", "Trost", "ver\u00b7leihn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}