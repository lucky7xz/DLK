{"dta.poem.5251": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich gegen den Angrif eines  \n Freundes mit verschiedenen Einf\u00e4llen geweh-  \n ret hatte, und bald darauf ein wilder Schweins-  \n kopf auf die Tafel gesetzet wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Des Waldes Thiere sind dem L\u00f6wen unterthan;", "tokens": ["Des", "Wal\u00b7des", "Thie\u00b7re", "sind", "dem", "L\u00f6\u00b7wen", "un\u00b7ter\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Eber sch\u00e4umt, und droht mit gro\u00df gewachsnem", "tokens": ["Der", "E\u00b7ber", "sch\u00e4umt", ",", "und", "droht", "mit", "gro\u00df", "ge\u00b7wachs\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zahn", "tokens": ["Zahn"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Des J\u00e4gers stark gewordnen Gliedern:", "tokens": ["Des", "J\u00e4\u00b7gers", "stark", "ge\u00b7word\u00b7nen", "Glie\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin ein schwaches Weib, und wehre mich mit", "tokens": ["Ich", "bin", "ein", "schwa\u00b7ches", "Weib", ",", "und", "weh\u00b7re", "mich", "mit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PRF", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Liedern.", "tokens": ["Lie\u00b7dern", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}