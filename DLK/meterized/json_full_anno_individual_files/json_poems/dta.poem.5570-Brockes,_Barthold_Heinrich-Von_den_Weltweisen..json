{"dta.poem.5570": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um aus so dunckler Nacht sich nun zu retten; frugen", "tokens": ["Um", "aus", "so", "dunck\u00b7ler", "Nacht", "sich", "nun", "zu", "ret\u00b7ten", ";", "fru\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUI", "APPR", "ADV", "ADJA", "NN", "PRF", "ADV", "PTKZU", "VVINF", "$.", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die, so in Griechen-Land der Weisen Nahmen trugen,", "tokens": ["Die", ",", "so", "in", "Grie\u00b7chen\u00b7Land", "der", "Wei\u00b7sen", "Nah\u00b7men", "tru\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "APPR", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sich immer: Wo man doch den ersten Stoff der Welt", "tokens": ["Sich", "im\u00b7mer", ":", "Wo", "man", "doch", "den", "ers\u00b7ten", "Stoff", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "$.", "PWAV", "PIS", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu allererst gesch\u00f6pfft? Und wer doch der wol sey,", "tokens": ["Zu", "al\u00b7le\u00b7rerst", "ge\u00b7sch\u00f6pfft", "?", "Und", "wer", "doch", "der", "wol", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVPP", "$.", "KON", "PWS", "ADV", "ART", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der sie zuerst bewegt? Es wurde fest gestellt,", "tokens": ["Der", "sie", "zu\u00b7erst", "be\u00b7wegt", "?", "Es", "wur\u00b7de", "fest", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$.", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Stoff m\u00fcss\u2019 ewig seyn. Sie gingen das vorbey,", "tokens": ["Der", "Stoff", "m\u00fcss'", "e\u00b7wig", "seyn", ".", "Sie", "gin\u00b7gen", "das", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VAINF", "$.", "PPER", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Woher der erste Stoff genommen.", "tokens": ["Wo\u00b7her", "der", "ers\u00b7te", "Stoff", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie kannten ausser ihm ein\u2019 ", "tokens": ["Sie", "kann\u00b7ten", "aus\u00b7ser", "ihm", "ein'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "So ihn in solchen Stand gebracht,", "tokens": ["So", "ihn", "in", "sol\u00b7chen", "Stand", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wovon er Sch\u00f6nheit, Licht und Ordnung, \u00fcberkommen.", "tokens": ["Wo\u00b7von", "er", "Sch\u00f6n\u00b7heit", ",", "Licht", "und", "Ord\u00b7nung", ",", "\u00fc\u00b7ber\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "$,", "NN", "KON", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}