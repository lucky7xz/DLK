{"textgrid.poem.62334": {"metadata": {"author": {"name": "Gessner, Salomon", "birth": "N.A.", "death": "N.A."}, "title": "Daphne. Chloe", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieblich ist diese Gegend, lieblich des Abends K\u00fchlung; la\u00df uns hier verweilen.", "tokens": ["Lieb\u00b7lich", "ist", "die\u00b7se", "Ge\u00b7gend", ",", "lieb\u00b7lich", "des", "A\u00b7bends", "K\u00fch\u00b7lung", ";", "la\u00df", "uns", "hier", "ver\u00b7wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PDAT", "NN", "$,", "ADJD", "ART", "NN", "NN", "$.", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "+--+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}}, "stanza.2": {"line.1": {"text": "Seys denn, wir wollen.", "tokens": ["Seys", "denn", ",", "wir", "wol\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "$,", "PPER", "VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Kein Hirt wei\u00df die Pflege der Pflanzen wie er. Ists nicht so?", "tokens": ["Kein", "Hirt", "wei\u00df", "die", "Pfle\u00b7ge", "der", "Pflan\u00b7zen", "wie", "er", ".", "Ists", "nicht", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "KOKOM", "PPER", "$.", "NE", "PTKNEG", "ADV", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "O ja!", "tokens": ["O", "ja", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "ITJ", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Aber du lachest Chloe!", "token