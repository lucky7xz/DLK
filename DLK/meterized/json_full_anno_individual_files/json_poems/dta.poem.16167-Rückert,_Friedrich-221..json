{"dta.poem.16167": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "221.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Gutes dir gelang, warum willst du dich scheun,", "tokens": ["Wenn", "Gu\u00b7tes", "dir", "ge\u00b7lang", ",", "wa\u00b7rum", "willst", "du", "dich", "scheun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil es nicht dir entsprang, dich dessen doch zu freun?", "tokens": ["Weil", "es", "nicht", "dir", "ent\u00b7sprang", ",", "dich", "des\u00b7sen", "doch", "zu", "freun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPER", "VVFIN", "$,", "PPER", "PDS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da du so oft bereun mu\u00dft, was du schlecht gemacht,", "tokens": ["Da", "du", "so", "oft", "be\u00b7re\u00b7un", "mu\u00dft", ",", "was", "du", "schlecht", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$,", "PWS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Soll dich nicht einmal freun auch was du recht vollbracht?", "tokens": ["Soll", "dich", "nicht", "ein\u00b7mal", "freun", "auch", "was", "du", "recht", "voll\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "w