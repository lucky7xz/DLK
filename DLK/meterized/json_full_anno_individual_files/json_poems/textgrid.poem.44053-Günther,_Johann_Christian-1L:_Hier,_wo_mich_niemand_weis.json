{"textgrid.poem.44053": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier, wo mich niemand weis", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier, wo mich niemand weis", "tokens": ["Hier", ",", "wo", "mich", "nie\u00b7mand", "weis"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "PIS", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als Gott und meine Noth,", "tokens": ["Als", "Gott", "und", "mei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergie\u00df ich Blut vor Schwei\u00df", "tokens": ["Ver\u00b7gie\u00df", "ich", "Blut", "vor", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und e\u00dfe Kraut vor Brodt", "tokens": ["Und", "e\u00b7\u00dfe", "Kraut", "vor", "Brodt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und dencke bey dem Schmerzen", "tokens": ["Und", "den\u00b7cke", "bey", "dem", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit h\u00f6chst betr\u00fcbtem Herzen", "tokens": ["Mit", "h\u00f6chst", "be\u00b7tr\u00fcb\u00b7tem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "An meine Vaterstadt,", "tokens": ["An", "mei\u00b7ne", "Va\u00b7ter\u00b7stadt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die unter Asch und Kohlen", "tokens": ["Die", "un\u00b7ter", "Asch", "und", "Koh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Mir alles Gut gestohlen", "tokens": ["Mir", "al\u00b7les", "Gut", "ge\u00b7stoh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PIS", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und mich, ihr Kind, dazu noch gar verworfen hat.", "tokens": ["Und", "mich", ",", "ihr", "Kind", ",", "da\u00b7zu", "noch", "gar", "ver\u00b7wor\u00b7fen", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "PAV", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich denck auch wohl an dich,", "tokens": ["Ich", "denck", "auch", "wohl", "an", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du allerliebstes Kind.", "tokens": ["Du", "al\u00b7ler\u00b7liebs\u00b7tes", "Kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Zeiten \u00e4ndern sich,", "tokens": ["Die", "Zei\u00b7ten", "\u00e4n\u00b7dern", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ach, aber zu geschwind.", "tokens": ["Ach", ",", "a\u00b7ber", "zu", "ge\u00b7schwind", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie schlecht ist unserm Lieben", "tokens": ["Wie", "schlecht", "ist", "un\u00b7serm", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Abschiedswuntsch beklieben,", "tokens": ["Der", "Ab\u00b7schieds\u00b7wunt\u00b7sch", "be\u00b7klie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der Gott so z\u00e4rtlich bat.", "tokens": ["Der", "Gott", "so", "z\u00e4rt\u00b7lich", "bat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Du weintest vor Vergn\u00fcgen,", "tokens": ["Du", "wein\u00b7test", "vor", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Durch mich bald Ruhm zu kriegen;", "tokens": ["Durch", "mich", "bald", "Ruhm", "zu", "krie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Hier liegt nun meine Kunst und weis vor sich kaum Rath.", "tokens": ["Hier", "liegt", "nun", "mei\u00b7ne", "Kunst", "und", "weis", "vor", "sich", "kaum", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "KON", "PTKVZ", "APPR", "PRF", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die f\u00fcnfte Sommerlust", "tokens": ["Die", "f\u00fcnf\u00b7te", "Som\u00b7mer\u00b7lust"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fchrt jezt die Garben ein;", "tokens": ["F\u00fchrt", "jezt", "die", "Gar\u00b7ben", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lang ist meine Brust", "tokens": ["So", "lang", "ist", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Schauplaz vieler Pein.", "tokens": ["Ein", "Schau\u00b7plaz", "vie\u00b7ler", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch was ich sonst ertragen,", "tokens": ["Doch", "was", "ich", "sonst", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist gegen diese Plagen", "tokens": ["Ist", "ge\u00b7gen", "die\u00b7se", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vorwahr nur Spiel und Scherz:", "tokens": ["Vor\u00b7wahr", "nur", "Spiel", "und", "Scherz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Kein Ansehn, mich zu heben,", "tokens": ["Kein", "An\u00b7sehn", ",", "mich", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und nichts als M\u00fch im Leben;", "tokens": ["Und", "nichts", "als", "M\u00fch", "im", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "O Gott, wie dauret mich mein allzuredlich Herz.", "tokens": ["O", "Gott", ",", "wie", "dau\u00b7ret", "mich", "mein", "all\u00b7zu\u00b7red\u00b7lich", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mich wundert, da\u00df mir noch", "tokens": ["Mich", "wun\u00b7dert", ",", "da\u00df", "mir", "noch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Stock ein Lager g\u00f6nnt,", "tokens": ["Der", "Stock", "ein", "La\u00b7ger", "g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn sonst verfolgt mich doch,", "tokens": ["Denn", "sonst", "ver\u00b7folgt", "mich", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was mich nur sieht und kennt;", "tokens": ["Was", "mich", "nur", "sieht", "und", "kennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vor leichte Jugends\u00fcnden,", "tokens": ["Vor", "leich\u00b7te", "Ju\u00b7gend\u00b7s\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die doch die meisten binden,", "tokens": ["Die", "doch", "die", "meis\u00b7ten", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Trift mich allein die Last,", "tokens": ["Trift", "mich", "al\u00b7lein", "die", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Last zu schwerer Strafen,", "tokens": ["Die", "Last", "zu", "schwe\u00b7rer", "Stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da viel in F\u00fclle schlafen,", "tokens": ["Da", "viel", "in", "F\u00fcl\u00b7le", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die Bo\u00dfheit und Betrug an g\u00fcldne Stricke fast.", "tokens": ["Die", "Bo\u00df\u00b7heit", "und", "Be\u00b7trug", "an", "g\u00fcld\u00b7ne", "Stri\u00b7cke", "fast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O Phoebus, leidest du,", "tokens": ["O", "Phoe\u00b7bus", ",", "lei\u00b7dest", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df deine Kinder schreyn", "tokens": ["Da\u00df", "dei\u00b7ne", "Kin\u00b7der", "schreyn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und doch bey wenig Ruh", "tokens": ["Und", "doch", "bey", "we\u00b7nig", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den Helden dienstbahr seyn?", "tokens": ["Den", "Hel\u00b7den", "dienst\u00b7bahr", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie kommt's, da\u00df unsre Gaben", "tokens": ["Wie", "kommt's", ",", "da\u00df", "uns\u00b7re", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Fast nichts zum Vortheil haben", "tokens": ["Fast", "nichts", "zum", "Vor\u00b7theil", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als Armuth und Verdru\u00df?", "tokens": ["Als", "Ar\u00b7muth", "und", "Ver\u00b7dru\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ein Hofnarr lebt ja be\u00dfer", "tokens": ["Ein", "Hof\u00b7narr", "lebt", "ja", "be\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und lacht mit fettem Me\u00dfer,", "tokens": ["Und", "lacht", "mit", "fet\u00b7tem", "Me\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wenn unsre kluge Hand nur R\u00fcben sch\u00e4len mu\u00df.", "tokens": ["Wenn", "uns\u00b7re", "klu\u00b7ge", "Hand", "nur", "R\u00fc\u00b7ben", "sch\u00e4\u00b7len", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Im Reimen steckt wohl auch", "tokens": ["Im", "Rei\u00b7men", "steckt", "wohl", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein ganzes Wi\u00dfen nicht;", "tokens": ["Mein", "gan\u00b7zes", "Wi\u00b7\u00dfen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sorge vor den Bauch,", "tokens": ["Ich", "sor\u00b7ge", "vor", "den", "Bauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch hat mein Kopf auch Licht.", "tokens": ["Doch", "hat", "mein", "Kopf", "auch", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was fehlt mir denn? Die Mode.", "tokens": ["Was", "fehlt", "mir", "denn", "?", "Die", "Mo\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es heist, ich l\u00e4g im Sode", "tokens": ["Es", "heist", ",", "ich", "l\u00e4g", "im", "So\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und w\u00e4re nicht gewand.", "tokens": ["Und", "w\u00e4\u00b7re", "nicht", "ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "O rei\u00dft mich aus dem Kittel", "tokens": ["O", "rei\u00dft", "mich", "aus", "dem", "Kit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und gebt Erh\u00f6hungsmittel", "tokens": ["Und", "gebt", "Er\u00b7h\u00f6\u00b7hungs\u00b7mit\u00b7tel"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und seht mich wieder an: Was gilt's? Ich bin galant.", "tokens": ["Und", "seht", "mich", "wie\u00b7der", "an", ":", "Was", "gilt's", "?", "Ich", "bin", "ga\u00b7lant", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PWS", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dir,