{"textgrid.poem.34006": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Kunst", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, heil'ge Kunst, dir hab' ich mich ergeben!", "tokens": ["Dir", ",", "heil'\u00b7ge", "Kunst", ",", "dir", "hab'", "ich", "mich", "er\u00b7ge\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht dr\u00e4ngt' ich mich, du riefst mich zum Altare,", "tokens": ["Nicht", "dr\u00e4ngt'", "ich", "mich", ",", "du", "riefst", "mich", "zum", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PRF", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich rang mit dir, ob ich mich frei bewahre,", "tokens": ["Ich", "rang", "mit", "dir", ",", "ob", "ich", "mich", "frei", "be\u00b7wah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du siegtest, nimm mich denn auf Tod und Leben!", "tokens": ["Du", "sieg\u00b7test", ",", "nimm", "mich", "denn", "auf", "Tod", "und", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVIMP", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun wollen Tr\u00e4ume meinen Blick umweben,", "tokens": ["Nun", "wol\u00b7len", "Tr\u00e4u\u00b7me", "mei\u00b7nen", "Blick", "um\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich aber schau' hinab auf ernste Jahre,", "tokens": ["Ich", "a\u00b7ber", "schau'", "hin\u00b7ab", "auf", "erns\u00b7te", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch, wie sich auch zum Kampf der P\u00f6bel schaare,", "tokens": ["Doch", ",", "wie", "sich", "auch", "zum", "Kampf", "der", "P\u00f6\u00b7bel", "schaa\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PRF", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Ende siegt ein gottgebor'nes Streben.", "tokens": ["Am", "En\u00b7de", "siegt", "ein", "gott\u00b7ge\u00b7bor'\u00b7nes", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Viel trage ich, doch schl\u00e4gt mir die Entehrung", "tokens": ["Viel", "tra\u00b7ge", "ich", ",", "doch", "schl\u00e4gt", "mir", "die", "E\u00b7nteh\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Welt-Idee, auf deren Leib ich hoffe,", "tokens": ["Der", "Welt\u00b7I\u00b7dee", ",", "auf", "de\u00b7ren", "Leib", "ich", "hof\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch Puppen-Larven leicht die Todeswunde.", "tokens": ["Durch", "Pup\u00b7pen\u00b7La\u00b7rven", "leicht", "die", "To\u00b7des\u00b7wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was thut's? Die echte Zeugung ist Entleerung", "tokens": ["Was", "thut's", "?", "Die", "ech\u00b7te", "Zeu\u00b7gung", "ist", "Ent\u00b7lee\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "ART", "ADJA", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Einzelwesens von dem Weltenstoffe", "tokens": ["Des", "Ein\u00b7zel\u00b7we\u00b7sens", "von", "dem", "Wel\u00b7ten\u00b7stof\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und geht mit ihrem Vater nicht zu Grunde.", "tokens": ["Und", "geht", "mit", "ih\u00b7rem", "Va\u00b7ter", "nicht", "zu", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dir, heil'ge Kunst, dir hab' ich mich ergeben!", "tokens": ["Dir", ",", "heil'\u00b7ge", "Kunst", ",", "dir", "hab'", "ich", "mich", "er\u00b7ge\u00b7ben", "!"], "token_info": ["word", "punct"