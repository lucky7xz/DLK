{"dta.poem.1109": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Schweinitz/                             hast Du dich zum ersten                         ", "tokens": ["Mein", "Schwei\u00b7nitz", "/", "hast", "Du", "dich", "zum", "ers\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "VAFIN", "PPER", "PRF", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "mahl beflissen/                         ", "tokens": ["mahl", "be\u00b7flis\u00b7sen", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ein rohes Sch\u00e4ffer-Kind zu f\u00fchren in die Welt/", "tokens": ["Ein", "ro\u00b7hes", "Sch\u00e4f\u00b7fer\u00b7Kind", "zu", "f\u00fch\u00b7ren", "in", "die", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wiewohl es sich zu weit gewagt ins offne Feld/)", "tokens": ["Wie\u00b7wohl", "es", "sich", "zu", "weit", "ge\u00b7wagt", "ins", "off\u00b7ne", "Feld", "/", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKA", "ADJD", "VVPP", "APPRART", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wird dichs billich auch zum andern mahl be-", "tokens": ["So", "wird", "dichs", "bil\u00b7lich", "auch", "zum", "an\u00b7dern", "mahl", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "ADV", "APPRART", "ADJA", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "gr\u00fcssen.                         ", "tokens": ["gr\u00fcs\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Es kommen Blumen mit/ die sahst du theils ent-", "tokens": ["Es", "kom\u00b7men", "Blu\u00b7men", "mit", "/", "die", "sahst", "du", "theils", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "$(", "ART", "VVFIN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "sprissen;                         ", "tokens": ["spris\u00b7sen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Auch hat sich ihm anizt ein Lands-Mann zugesellt/", "tokens": ["Auch", "hat", "sich", "ihm", "a\u00b7nizt", "ein", "Lands\u00b7Mann", "zu\u00b7ge\u00b7sellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "PPER", "VVFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.10": {"text": "Wie er aus fremder Sprach ein Teutsch Sonnet", "tokens": ["Wie", "er", "aus", "frem\u00b7der", "Sprach", "ein", "Teutsch", "Son\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "gestellt.                         ", "tokens": ["ge\u00b7stellt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Wird dein ge\u00fcbter Geist den Spruch zu f\u00e4llen                             wis-", "tokens": ["Wird", "dein", "ge\u00b7\u00fcb\u00b7ter", "Geist", "den", "Spruch", "zu", "f\u00e4l\u00b7len", "wis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "sen.                         ", "tokens": ["sen."], "token_info": ["abbreviation"], "pos": ["XY"]}, "line.14": {"text": "Sie kommen nicht umsonst: Es f\u00fcrchtet unser                             Land/", "tokens": ["Sie", "kom\u00b7men", "nicht", "um\u00b7sonst", ":", "Es", "f\u00fcrch\u00b7tet", "un\u00b7ser", "Land", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wo nicht ein ", "tokens": ["Wo", "nicht", "ein"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PTKNEG", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Den wohlerworbnen Ruhm der Dicht-Kunst zu", "tokens": ["Den", "woh\u00b7ler\u00b7worb\u00b7nen", "Ruhm", "der", "Dicht\u00b7Kunst", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "verlieren:                         ", "tokens": ["ver\u00b7lie\u00b7ren", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Da\u00df du ihn retten kanst beruht in deiner Hand.", "tokens": ["Da\u00df", "du", "ihn", "ret\u00b7ten", "kanst", "be\u00b7ruht", "in", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Kan ich/ was diese schrieb/ der klugen Welt erlan", "tokens": ["Kan", "ich", "/", "was", "die\u00b7se", "schrieb", "/", "der", "klu\u00b7gen", "Welt", "er\u00b7lan"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$(", "PWS", "PDS", "VVFIN", "$(", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "gen/                         ", "tokens": ["gen", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "-", "measure": "single.down"}, "line.21": {"text": "So ist mein Hirten-Knecht gl\u00fcckselig                             ausgegangen.", "tokens": ["So", "ist", "mein", "Hir\u00b7ten\u00b7Knecht", "gl\u00fcck\u00b7se\u00b7lig", "aus\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}