{"textgrid.poem.34526": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[auf einem weissen Thierfell kugeln sich]", "genre": "verse", "period": "N.A.", "pub_year": 1893, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einem weissen Thierfell kugeln sich", "tokens": ["Auf", "ei\u00b7nem", "weis\u00b7sen", "Thier\u00b7fell", "ku\u00b7geln", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ein nacktes Kind und ein defecter Globus.", "tokens": ["ein", "nack\u00b7tes", "Kind", "und", "ein", "de\u00b7fec\u00b7ter", "Glo\u00b7bus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bed\u00e4chtig setzt die blondgelockte Kleine", "tokens": ["Be\u00b7d\u00e4ch\u00b7tig", "setzt", "die", "blond\u00b7ge\u00b7lock\u00b7te", "Klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sich oben auf des Nordpols eisige Spitze", "tokens": ["sich", "o\u00b7ben", "auf", "des", "Nord\u00b7pols", "ei\u00b7si\u00b7ge", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "und dann \u2013 Hallo! Mit Jauchzen und Beinestrampeln", "tokens": ["und", "dann", "\u2013", "Hal\u00b7lo", "!", "Mit", "Jauch\u00b7zen", "und", "Bei\u00b7nes\u00b7tram\u00b7peln"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "NE", "$.", "APPR", "NN", "KON", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "f\u00e4hrt sie hernieder in das weiche Fell!", "tokens": ["f\u00e4hrt", "sie", "her\u00b7nie\u00b7der", "in", "das", "wei\u00b7che", "Fell", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und gar nicht m\u00fcde wird das Kind des Spiels,", "tokens": ["Und", "gar", "nicht", "m\u00fc\u00b7de", "wird", "das", "Kind", "des", "Spiels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "es ist ganz ausser sich vor toller Freude,", "tokens": ["es", "ist", "ganz", "aus\u00b7ser", "sich", "vor", "tol\u00b7ler", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "dass Gott der Herr die Welt so rund geschaffen ...", "tokens": ["dass", "Gott", "der", "Herr", "die", "Welt", "so", "rund", "ge\u00b7schaf\u00b7fen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie herrlich l\u00e4sst mit dieser Welt sich spielen!", "tokens": ["Wie", "herr\u00b7lich", "l\u00e4sst", "mit", "die\u00b7ser", "Welt", "sich", "spie\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN"