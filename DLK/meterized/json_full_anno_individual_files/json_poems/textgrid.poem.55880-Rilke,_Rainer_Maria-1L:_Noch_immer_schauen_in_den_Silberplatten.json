{"textgrid.poem.55880": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch immer schauen in den Silberplatten", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch immer schauen in den Silberplatten", "tokens": ["Noch", "im\u00b7mer", "schau\u00b7en", "in", "den", "Sil\u00b7ber\u00b7plat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wie tiefe Frauenaugen die Saphire,", "tokens": ["wie", "tie\u00b7fe", "Frau\u00b7en\u00b7au\u00b7gen", "die", "Sa\u00b7phi\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Goldranken schlingen sich wie schlanke Tiere,", "tokens": ["Gold\u00b7ran\u00b7ken", "schlin\u00b7gen", "sich", "wie", "schlan\u00b7ke", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die sich im Glanze ihrer Br\u00fcnste gatten,", "tokens": ["die", "sich", "im", "Glan\u00b7ze", "ih\u00b7rer", "Br\u00fcns\u00b7te", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und sanfte Perlen warten in dem Schatten", "tokens": ["und", "sanf\u00b7te", "Per\u00b7len", "war\u00b7ten", "in", "dem", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "wilder Gebilde, da\u00df ein Schimmer ihre", "tokens": ["wil\u00b7der", "Ge\u00b7bil\u00b7de", ",", "da\u00df", "ein", "Schim\u00b7mer", "ih\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "ART", "NN", "PPOSAT"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "stillen Gesichter finde und verliere.", "tokens": ["stil\u00b7len", "Ge\u00b7sich\u00b7ter", "fin\u00b7de", "und", "ver\u00b7lie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Und das ist Mantel, Strahlenkranz und Land,", "tokens": ["Und", "das", "ist", "Man\u00b7tel", ",", "Strah\u00b7len\u00b7kranz", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "und ein Bewegen geht von Rand zu Rand,", "tokens": ["und", "ein", "Be\u00b7we\u00b7gen", "geht", "von", "Rand", "zu", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "wie Korn im Wind und wie ein Flu\u00df im Tale,", "tokens": ["wie", "Korn", "im", "Wind", "und", "wie", "ein", "Flu\u00df", "im", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "KON", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "so gl\u00e4nzt es wechselnd durch die Rahmenwand.", "tokens": ["so", "gl\u00e4nzt", "es", "wech\u00b7selnd", "durch", "die", "Rah\u00b7men\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In ihrer Sonne dunkeln drei Ovale:", "tokens": ["In", "ih\u00b7rer", "Son\u00b7ne", "dun\u00b7keln", "drei", "O\u00b7va\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das gro\u00dfe giebt dem Mutterantlitz Raum,", "tokens": ["das", "gro\u00b7\u00dfe", "giebt", "dem", "Mut\u00b7ter\u00b7ant\u00b7litz", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und rechts und links hebt eine mandelschmale", "tokens": ["und", "rechts", "und", "links", "hebt", "ei\u00b7ne", "man\u00b7del\u00b7schma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jungfrauenhand sich aus dem Silbersaum.", "tokens": ["Jung\u00b7frau\u00b7en\u00b7hand", "sich", "aus", "dem", "Sil\u00b7ber\u00b7saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die beiden H\u00e4nde, seltsam still und braun,", "tokens": ["Die", "bei\u00b7den", "H\u00e4n\u00b7de", ",", "selt\u00b7sam", "still", "und", "braun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ADJD", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "verk\u00fcnden, da\u00df im k\u00f6stlichen Ikone", "tokens": ["ver\u00b7k\u00fcn\u00b7den", ",", "da\u00df", "im", "k\u00f6st\u00b7li\u00b7chen", "I\u00b7ko\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "die K\u00f6nigliche wie im Kloster wohne,", "tokens": ["die", "K\u00f6\u00b7nig\u00b7li\u00b7che", "wie", "im", "Klos\u00b7ter", "woh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "die \u00fcberflie\u00dfen wird von jenem Sohne,", "tokens": ["die", "\u00fc\u00b7berf\u00b7lie\u00b7\u00dfen", "wird", "von", "je\u00b7nem", "Soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "von jenem Tropfen, drinnen wolkenohne", "tokens": ["von", "je\u00b7nem", "Trop\u00b7fen", ",", "drin\u00b7nen", "wol\u00b7ken\u00b7oh\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "die niegehofften Himmel blaun.", "tokens": ["die", "nie\u00b7ge\u00b7hoff\u00b7ten", "Him\u00b7mel", "blaun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die H\u00e4nde zeugen noch daf\u00fcr;", "tokens": ["Die", "H\u00e4n\u00b7de", "zeu\u00b7gen", "noch", "da\u00b7f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "aber das Antlitz ist wie eine T\u00fcr", "tokens": ["a\u00b7ber", "das", "Ant\u00b7litz", "ist", "wie", "ei\u00b7ne", "T\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "KOKOM", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "in warme D\u00e4mmerungen aufgegangen,", "tokens": ["in", "war\u00b7me", "D\u00e4m\u00b7me\u00b7run\u00b7gen", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in die das L\u00e4cheln von den Gnadenwangen", "tokens": ["in", "die", "das", "L\u00e4\u00b7cheln", "von", "den", "Gna\u00b7den\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "mit seinem Lichte irrend, sich verlor.", "tokens": ["mit", "sei\u00b7nem", "Lich\u00b7te", "ir\u00b7rend", ",", "sich", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da neigt sich tief der Zar davor und spricht:", "tokens": ["Da", "neigt", "sich", "tief", "der", "Zar", "da\u00b7vor", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ART", "NN", "PAV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "F\u00fchltest Du nicht, wie sehr wir in Dich drangen", "tokens": ["F\u00fchl\u00b7test", "Du", "nicht", ",", "wie", "sehr", "wir", "in", "Dich", "dran\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ADV", "PPER", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit allem F\u00fchlen, F\u00fcrchten und Verlangen:", "tokens": ["mit", "al\u00b7lem", "F\u00fch\u00b7len", ",", "F\u00fcrch\u00b7ten", "und", "Ver\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wir warten auf Dein liebes Angesicht,", "tokens": ["wir", "war\u00b7ten", "auf", "Dein", "lie\u00b7bes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "das uns vergangen ist; wohin vergangen?:", "tokens": ["das", "uns", "ver\u00b7gan\u00b7gen", "ist", ";", "wo\u00b7hin", "ver\u00b7gan\u00b7gen", "?", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$.", "PWAV", "VVPP", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Den gro\u00dfen Heiligen vergeht es nicht.", "tokens": ["Den", "gro\u00b7\u00dfen", "Hei\u00b7li\u00b7gen", "ver\u00b7geht", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Er bebte tief in seinem steifen Kleid,", "tokens": ["Er", "beb\u00b7te", "tief", "in", "sei\u00b7nem", "stei\u00b7fen", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das strahlend stand. Er wu\u00dfte nicht, wie weit", "tokens": ["das", "strah\u00b7lend", "stand", ".", "Er", "wu\u00df\u00b7te", "nicht", ",", "wie", "weit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "er schon von allem war, und ihrem Segnen", "tokens": ["er", "schon", "von", "al\u00b7lem", "war", ",", "und", "ih\u00b7rem", "Seg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PIS", "VAFIN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wie selig nah in seiner Einsamkeit.", "tokens": ["wie", "se\u00b7lig", "nah", "in", "sei\u00b7ner", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Noch sinnt und sinnt der blasse Gossudar.", "tokens": ["Noch", "sinnt", "und", "sinnt", "der", "blas\u00b7se", "Gos\u00b7su\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sein Gesicht, das unterm kranken Haar", "tokens": ["Und", "sein", "Ge\u00b7sicht", ",", "das", "un\u00b7term", "kran\u00b7ken", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,"