{"textgrid.poem.65643": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich war ein kleiner Knabe, stand fest kaum auf dem Bein,", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein kleiner Knabe, stand fest kaum auf dem Bein,", "tokens": ["Ich", "war", "ein", "klei\u00b7ner", "Kna\u00b7be", ",", "stand", "fest", "kaum", "auf", "dem", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Da nahm mich schon mein Vater mit in das Meer hinein,", "tokens": ["Da", "nahm", "mich", "schon", "mein", "Va\u00b7ter", "mit", "in", "das", "Meer", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und lehrte leicht mich schwimmen an seiner sichern Hand,", "tokens": ["Und", "lehr\u00b7te", "leicht", "mich", "schwim\u00b7men", "an", "sei\u00b7ner", "si\u00b7chern", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und in die Fluthen tauchen bis nieder auf den Sand.", "tokens": ["Und", "in", "die", "Flut\u00b7hen", "tau\u00b7chen", "bis", "nie\u00b7der", "auf", "den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ein Silberst\u00fcckchen warf er dreimal in's Meer hinab,", "tokens": ["Ein", "Sil\u00b7ber\u00b7st\u00fcck\u00b7chen", "warf", "er", "drei\u00b7mal", "in's", "Meer", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und dreimal mu\u00dft' ich's holen, eh' er's zum Lohn mir gab.", "tokens": ["Und", "drei\u00b7mal", "mu\u00dft'", "ich's", "ho\u00b7len", ",", "eh'", "er's", "zum", "Lohn", "mir", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "VVINF", "$,", "KOUS", "PIS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Dann reicht' er mir ein Ruder, hie\u00df in ein Boot mich gehn,", "tokens": ["Dann", "reicht'", "er", "mir", "ein", "Ru\u00b7der", ",", "hie\u00df", "in", "ein", "Boot", "mich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Er selber blieb zur Seite mir unverdrossen stehn,", "tokens": ["Er", "sel\u00b7ber", "blieb", "zur", "Sei\u00b7te", "mir", "un\u00b7ver\u00b7dros\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Wies mir, wie man die Woge mit scharfem Schlage bricht,", "tokens": ["Wies", "mir", ",", "wie", "man", "die", "Wo\u00b7ge", "mit", "schar\u00b7fem", "Schla\u00b7ge", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "PWAV", "PIS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Wie man die Wirbel meidet und mit der Brandung ficht.", "tokens": ["Wie", "man", "die", "Wir\u00b7bel", "mei\u00b7det", "und", "mit", "der", "Bran\u00b7dung", "ficht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Und von dem kleinen Kahne ging's flugs in's gro\u00dfe Schiff,", "tokens": ["Und", "von", "dem", "klei\u00b7nen", "Kah\u00b7ne", "ging's", "flugs", "in's", "gro\u00b7\u00dfe", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Es trieben uns die St\u00fcrme um manches Felsenriff.", "tokens": ["Es", "trie\u00b7ben", "uns", "die", "St\u00fcr\u00b7me", "um", "man\u00b7ches", "Fel\u00b7sen\u00b7riff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Ich sa\u00df auf hohem Maste, schaut' \u00fcber Meer und Land,", "tokens": ["Ich", "sa\u00df", "auf", "ho\u00b7hem", "Mas\u00b7te", ",", "schaut'", "\u00fc\u00b7ber", "Meer", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Es schwebten Berg' und Th\u00fcrme vor\u00fcber mit dem Strand.", "tokens": ["Es", "schweb\u00b7ten", "Ber\u00b7g'", "und", "Th\u00fcr\u00b7me", "vor\u00b7\u00fc\u00b7ber", "mit", "dem", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Der Vater hie\u00df mich merken auf jedes Vogels Flug,", "tokens": ["Der", "Va\u00b7ter", "hie\u00df", "mich", "mer\u00b7ken", "auf", "je\u00b7des", "Vo\u00b7gels", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Auf aller Winde Wehen, auf aller Wolken Zug;", "tokens": ["Auf", "al\u00b7ler", "Win\u00b7de", "We\u00b7hen", ",", "auf", "al\u00b7ler", "Wol\u00b7ken", "Zug", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Und bogen dann die St\u00fcrme den Mast bis in die Fluth,", "tokens": ["Und", "bo\u00b7gen", "dann", "die", "St\u00fcr\u00b7me", "den", "Mast", "bis", "in", "die", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Und spritzten dann die Wogen hoch \u00fcber meinen Hut,", "tokens": ["Und", "spritz\u00b7ten", "dann", "die", "Wo\u00b7gen", "hoch", "\u00fc\u00b7ber", "mei\u00b7nen", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Da sah der Vater pr\u00fcfend mir in das Angesicht \u2013", "tokens": ["Da", "sah", "der", "Va\u00b7ter", "pr\u00fc\u00b7fend", "mir", "in", "das", "An\u00b7ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Ich sa\u00df in meinem Korbe und r\u00fcttelte mich nicht \u2013", "tokens": ["Ich", "sa\u00df", "in", "mei\u00b7nem", "Kor\u00b7be", "und", "r\u00fct\u00b7tel\u00b7te", "mich", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Da sprach er, und die Wange ward ihm, wie Blut, so roth:", "tokens": ["Da", "sprach", "er", ",", "und", "die", "Wan\u00b7ge", "ward", "ihm", ",", "wie", "Blut", ",", "so", "roth", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "VAFIN", "PPER", "$,", "PWAV", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Gl\u00fcck zu, auf deinem Maste, du kleiner Hydriot! \u2013", "tokens": ["Gl\u00fcck", "zu", ",", "auf", "dei\u00b7nem", "Mas\u00b7te", ",", "du", "klei\u00b7ner", "Hyd\u00b7riot", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.23": {"text": "Und heute gab der Vater ein Schwert mir in die Hand,", "tokens": ["Und", "heu\u00b7te", "gab", "der", "Va\u00b7ter", "ein", "Schwert", "mir", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und weihte mich zum K\u00e4mpfer f\u00fcr Gott und Vaterland.", "tokens": ["Und", "weih\u00b7te", "mich", "zum", "K\u00e4mp\u00b7fer", "f\u00fcr", "Gott", "und", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Er ma\u00df mich mit den Blicken vom Kopf bis zu den Zehn,", "tokens": ["Er", "ma\u00df", "mich", "mit", "den", "Bli\u00b7cken", "vom", "Kopf", "bis", "zu", "den", "Zehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "APPR", "APPR", "ART", "CARD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Mir war's, als th\u00e4t' sein Auge hinab in's Herz mir sehn.", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "th\u00e4t'", "sein", "Au\u00b7ge", "hin\u00b7ab", "in's", "Herz", "mir", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "----+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Ich hielt mein Schwert gen Himmel, und schaut' ihn sicher an,", "tokens": ["Ich", "hielt", "mein", "Schwert", "gen", "Him\u00b7mel", ",", "und", "schaut'", "ihn", "si\u00b7cher", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Und d\u00e4uchte mich zur Stunde nicht schlechter, als ein Mann.", "tokens": ["Und", "d\u00e4uch\u00b7te", "mich", "zur", "Stun\u00b7de", "nicht", "schlech\u00b7ter", ",", "als", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "PTKNEG", "ADJD", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Da sprach er, und die Wange ward ihm, wie Blut, so roth:", "tokens": ["Da", "sprach", "er", ",", "und", "die", "Wan\u00b7ge", "ward", "ihm", ",", "wie", "Blut", ",", "so", "roth", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "VAFIN", "PPER", "$,", "PWAV", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Gl\u00fcck zu, mit deinem Schwerte, du kleiner Hydriot!", "tokens": ["Gl\u00fcck", "zu", ",", "mit", "dei\u00b7nem", "Schwer\u00b7te", ",", "du", "klei\u00b7ner", "Hyd\u00b7riot", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Ich war ein kleiner Knabe, stand fest kaum auf dem Bein,", "tokens": ["Ich", "war", "ein", "klei\u00b7ner", "Kna\u00b7be", ",", "stand", "fest", "kaum", "auf", "dem", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Da nahm mich schon mein Vater mit in das Meer hinein,", "tokens": ["Da", "nahm", "mich", "schon", "mein", "Va\u00b7ter", "mit", "in", "das", "Meer", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und lehrte leicht mich schwimmen an seiner sichern Hand,", "tokens": ["Und", "lehr\u00b7te", "leicht", "mich", "schwim\u00b7men", "an", "sei\u00b7ner", "si\u00b7chern", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und in die Fluthen tauchen bis nieder auf den Sand.", "tokens": ["Und", "in", "die", "Flut\u00b7hen", "tau\u00b7chen", "bis", "nie\u00b7der", "auf", "den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ein Silberst\u00fcckchen warf er dreimal in's Meer hinab,", "tokens": ["Ein", "Sil\u00b7ber\u00b7st\u00fcck\u00b7chen", "warf", "er", "drei\u00b7mal", "in's", "Meer", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und dreimal mu\u00dft' ich's holen, eh' er's zum Lohn mir gab.", "tokens": ["Und", "drei\u00b7mal", "mu\u00dft'", "ich's", "ho\u00b7len", ",", "eh'", "er's", "zum", "Lohn", "mir", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "VVINF", "$,", "KOUS", "PIS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Dann reicht' er mir ein Ruder, hie\u00df in ein Boot mich gehn,", "tokens": ["Dann", "reicht'", "er", "mir", "ein", "Ru\u00b7der", ",", "hie\u00df", "in", "ein", "Boot", "mich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Er selber blieb zur Seite mir unverdrossen stehn,", "tokens": ["Er", "sel\u00b7ber", "blieb", "zur", "Sei\u00b7te", "mir", "un\u00b7ver\u00b7dros\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Wies mir, wie man die Woge mit scharfem Schlage bricht,", "tokens": ["Wies", "mir", ",", "wie", "man", "die", "Wo\u00b7ge", "mit", "schar\u00b7fem", "Schla\u00b7ge", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "PWAV", "PIS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Wie man die Wirbel meidet und mit der Brandung ficht.", "tokens": ["Wie", "man", "die", "Wir\u00b7bel", "mei\u00b7det", "und", "mit", "der", "Bran\u00b7dung", "ficht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Und von dem kleinen Kahne ging's flugs in's gro\u00dfe Schiff,", "tokens": ["Und", "von", "dem", "klei\u00b7nen", "Kah\u00b7ne", "ging's", "flugs", "in's", "gro\u00b7\u00dfe", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Es trieben uns die St\u00fcrme um manches Felsenriff.", "tokens": ["Es", "trie\u00b7ben", "uns", "die", "St\u00fcr\u00b7me", "um", "man\u00b7ches", "Fel\u00b7sen\u00b7riff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Ich sa\u00df auf hohem Maste, schaut' \u00fcber Meer und Land,", "tokens": ["Ich", "sa\u00df", "auf", "ho\u00b7hem", "Mas\u00b7te", ",", "schaut'", "\u00fc\u00b7ber", "Meer", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Es schwebten Berg' und Th\u00fcrme vor\u00fcber mit dem Strand.", "tokens": ["Es", "schweb\u00b7ten", "Ber\u00b7g'", "und", "Th\u00fcr\u00b7me", "vor\u00b7\u00fc\u00b7ber", "mit", "dem", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Der Vater hie\u00df mich merken auf jedes Vogels Flug,", "tokens": ["Der", "Va\u00b7ter", "hie\u00df", "mich", "mer\u00b7ken", "auf", "je\u00b7des", "Vo\u00b7gels", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Auf aller Winde Wehen, auf aller Wolken Zug;", "tokens": ["Auf", "al\u00b7ler", "Win\u00b7de", "We\u00b7hen", ",", "auf", "al\u00b7ler", "Wol\u00b7ken", "Zug", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Und bogen dann die St\u00fcrme den Mast bis in die Fluth,", "tokens": ["Und", "bo\u00b7gen", "dann", "die", "St\u00fcr\u00b7me", "den", "Mast", "bis", "in", "die", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Und spritzten dann die Wogen hoch \u00fcber meinen Hut,", "tokens": ["Und", "spritz\u00b7ten", "dann", "die", "Wo\u00b7gen", "hoch", "\u00fc\u00b7ber", "mei\u00b7nen", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Da sah der Vater pr\u00fcfend mir in das Angesicht \u2013", "tokens": ["Da", "sah", "der", "Va\u00b7ter", "pr\u00fc\u00b7fend", "mir", "in", "das", "An\u00b7ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Ich sa\u00df in meinem Korbe und r\u00fcttelte mich nicht \u2013", "tokens": ["Ich", "sa\u00df", "in", "mei\u00b7nem", "Kor\u00b7be", "und", "r\u00fct\u00b7tel\u00b7te", "mich", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Da sprach er, und die Wange ward ihm, wie Blut, so roth:", "tokens": ["Da", "sprach", "er", ",", "und", "die", "Wan\u00b7ge", "ward", "ihm", ",", "wie", "Blut", ",", "so", "roth", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ART", "NN", "VAFIN", "PPER", "$,", "PWAV", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Gl\u00fcck zu, auf deinem Maste, du kleiner Hydriot! \u2013", "tokens": ["Gl\u00fcck", "zu", ",", "auf", "dei\u00b7nem", "Mas\u00b7te", ",", "du", "klei\u00b7ner", "Hyd\u00b7riot", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.23": {"text": "Und heute gab der Vater ein Schwert mir in die Hand,", "tokens": ["Und", "heu\u00b7te", "gab", "der", "Va\u00b7ter", "ein", "Schwert", "mir", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und weihte mich zum K\u00e4mpfer f\u00fcr Gott und Vaterland.", "tokens": ["Und", "weih\u00b7te", "mich", "zum", "K\u00e4mp\u00b7fer", "f\u00fcr", "Gott", "und", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Er ma\u00df mich mit den Blicken vom Kopf bis zu den Zehn,", "tokens": ["Er", "ma\u00df", "mich", "mit", "den", "Bli\u00b7cken", "vom", "Kopf", "bis", "zu", "den", "Zehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "APPR", "APPR", "ART", "CARD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Mir war's, als th\u00e4t' sein Auge hinab in's Herz mir sehn.", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "th\u00e4t'", "sein", "Au\u00b7ge", "hin\u00b7ab", "in's", "Herz", "mir", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "----+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Ich hielt mein Schwert gen Himmel, und schaut' ihn sicher an,", "tokens": ["Ich", "hielt", "mein", "Schwert", "gen", "Him\u00b7mel", ",", "und", "schaut'", "ihn", "si\u00b7cher", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Und d\u00e4uchte mich zur Stunde nicht schlechter, als ein Mann.", "tokens": ["Und", "d\u00e4uch\u00b7te", "mich", "zur", "Stun\u00b7de", "nicht", "schlech\u00b7ter", ",", "als", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "PTKNEG", "ADJD", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Da sprach er, und die Wange ward ihm, wie Blut, so roth:", "tokens": ["Da", "sprach", "er", ",", "und", "die", "Wan\u00b7ge", "ward", "ihm", ",", "wie", "Blut", ",", "so", "roth", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER