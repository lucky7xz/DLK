{"dta.poem.708": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1. Satz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Wje die Erden schmacht vnd brennet ", "tokens": ["Wie", "die", "Er\u00b7den", "schmacht", "vnd", "bren\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Blume sinckt vnd f\u00e4llt/", "tokens": ["Wie", "die", "Blu\u00b7me", "sinckt", "vnd", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Garten sich verstellt/", "tokens": ["Wie", "der", "Gar\u00b7ten", "sich", "ver\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVPP", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wie die Wiese sich verkennet/", "tokens": ["Wie", "die", "Wie\u00b7se", "sich", "ver\u00b7ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die erhitzte Sonn mit jhrem Mittags flammen", "tokens": ["Wenn", "die", "er\u00b7hitz\u00b7te", "Sonn", "mit", "jhrem", "Mit\u00b7tags", "flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Den Krey\u00df der welt ansteckt.", "tokens": ["Den", "Krey\u00df", "der", "welt", "an\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So/ wenn de\u00df H\u00f6chsten zorn wil t\u00f6dten vnd verdammen. ", "tokens": ["So", "/", "wenn", "de\u00df", "H\u00f6chs\u00b7ten", "zorn", "wil", "t\u00f6d\u00b7ten", "vnd", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ART", "ADJA", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn vn\u00df die Angst erschreckt:", "tokens": ["Wenn", "vn\u00df", "die", "Angst", "er\u00b7schreckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wenn vn\u00df die heisse Noth verzehret ", "tokens": ["Wenn", "vn\u00df", "die", "heis\u00b7se", "Noth", "ver\u00b7zeh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn vn\u00df die bange Furcht beschweret:", "tokens": ["Wenn", "vn\u00df", "die", "ban\u00b7ge", "Furcht", "be\u00b7schwe\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Denn wil vn\u00df krafft vnd Muth verschwinden:", "tokens": ["Denn", "wil", "vn\u00df", "krafft", "vnd", "Muth", "ver\u00b7schwin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Denn ist kein Hertz in vn\u00df zu finden.", "tokens": ["Denn", "ist", "kein", "Hertz", "in", "vn\u00df", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}