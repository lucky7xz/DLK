{"dta.poem.15913": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "118.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Versteh mich, liebes Kind! sowenig als mir nun", "tokens": ["Ver\u00b7steh", "mich", ",", "lie\u00b7bes", "Kind", "!", "so\u00b7we\u00b7nig", "als", "mir", "nun"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "ADV", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich jung zu machen ziemt, ziemt dir schon alt zu thun.", "tokens": ["Mich", "jung", "zu", "ma\u00b7chen", "ziemt", ",", "ziemt", "dir", "schon", "alt", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch reine Freude la\u00df uns aneinander haben,", "tokens": ["Doch", "rei\u00b7ne", "Freu\u00b7de", "la\u00df", "uns", "an\u00b7ein\u00b7an\u00b7der", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVIMP", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du lieb' an mir den Greis, ich lieb' an dir den Knaben.", "tokens": ["Du", "lieb'", "an", "mir", "den", "Greis", ",", "ich", "lieb'", "an", "dir", "den", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Erbauen magst du dich an meinem Weisheitspruch,", "tokens": ["Er\u00b7bau\u00b7en", "magst", "du", "dich", "an", "mei\u00b7nem", "Weis\u00b7heits\u00b7pruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch mich erquicken soll dein frischer Lenzgeruch.", "tokens": ["Doch", "mich", "er\u00b7qui\u00b7cken", "soll", "dein", "fri\u00b7scher", "Lenz\u00b7ge\u00b7ruch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und eher m\u00f6chte mir ein Liebeslied entspringen", "tokens": ["Und", "e\u00b7her", "m\u00f6ch\u00b7te", "mir", "ein", "Lie\u00b7bes\u00b7lied", "ent\u00b7sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch jetzt, als jetzt schon dir ein Lehrgedicht gelingen.", "tokens": ["Noch", "jetzt", ",", "als", "jetzt", "schon", "dir", "ein", "Lehr\u00b7ge\u00b7dicht", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADV", "ADV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}