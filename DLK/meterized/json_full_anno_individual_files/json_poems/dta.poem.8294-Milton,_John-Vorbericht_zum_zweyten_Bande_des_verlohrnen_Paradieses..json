{"dta.poem.8294": {"metadata": {"author": {"name": "Milton, John", "birth": "N.A.", "death": "N.A."}, "title": "Vorbericht  \n  zum zweyten Bande  \n des  \n verlohrnen Paradieses.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1763", "urn": "urn:nbn:de:kobv:b4-200905199734", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als diese Welt noch nicht geschaffen war,", "tokens": ["Als", "die\u00b7se", "Welt", "noch", "nicht", "ge\u00b7schaf\u00b7fen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und w\u00fcst und wild das Chaos da regierte,", "tokens": ["Und", "w\u00fcst", "und", "wild", "das", "Chaos", "da", "re\u00b7gier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "ADJD", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo itzt voll Pracht sich diese Himmel vollen,", "tokens": ["Wo", "itzt", "voll", "Pracht", "sich", "die\u00b7se", "Him\u00b7mel", "vol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "NN", "PRF", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wo die Erd\u2019 auf ihrem Mittelpunkt", "tokens": ["Und", "wo", "die", "Erd'", "auf", "ih\u00b7rem", "Mit\u00b7tel\u00b7punkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gegr\u00fcndet ruht; da wars an einem Tage,", "tokens": ["Ge\u00b7gr\u00fcn\u00b7det", "ruht", ";", "da", "wars", "an", "ei\u00b7nem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$.", "ADV", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch die Bewegung alles, was geschieht", "tokens": ["Durch", "die", "Be\u00b7we\u00b7gung", "al\u00b7les", ",", "was", "ge\u00b7schieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "PIS", "$,", "PWS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit dem Vergangnen, Gegenw\u00e4rtigen,", "tokens": ["Mit", "dem", "Ver\u00b7gang\u00b7nen", ",", "Ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und dem Zuk\u00fcnftgen) an solch einem Tage,", "tokens": ["Und", "dem", "Zu\u00b7k\u00fcnft\u00b7gen", ")", "an", "solch", "ei\u00b7nem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wie ihn das gro\u00dfe Jahr des Himmels zeugt,", "tokens": ["Wie", "ihn", "das", "gro\u00b7\u00dfe", "Jahr", "des", "Him\u00b7mels", "zeugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Erschien, gefodert durch Befehl von Gott", "tokens": ["Er\u00b7schien", ",", "ge\u00b7fo\u00b7dert", "durch", "Be\u00b7fehl", "von", "Gott"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVPP", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das ganze Heer der Engel vor dem Throne", "tokens": ["Das", "gan\u00b7ze", "Heer", "der", "En\u00b7gel", "vor", "dem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Des Ewigen; unz\u00e4hlbar; eingetheilt", "tokens": ["Des", "E\u00b7wi\u00b7gen", ";", "un\u00b7z\u00e4hl\u00b7bar", ";", "ein\u00b7ge\u00b7theilt"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJA", "$.", "ADJD", "$.", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "In ihre Hierarchien und Ordnungen;", "tokens": ["In", "ih\u00b7re", "Hier\u00b7ar\u00b7chi\u00b7en", "und", "Ord\u00b7nun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Zehntausend tausend Fahnen und Standarten,", "tokens": ["Zehn\u00b7tau\u00b7send", "tau\u00b7send", "Fah\u00b7nen", "und", "Stand\u00b7ar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und stralende Paniere, hoch erh\u00f6ht,", "tokens": ["Und", "stra\u00b7len\u00b7de", "Pa\u00b7nie\u00b7re", ",", "hoch", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Durchschimmerten im Vor- und Nachtrapp weit", "tokens": ["Durch\u00b7schim\u00b7mer\u00b7ten", "im", "Vor", "und", "Nach\u00b7trapp", "weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "TRUNC", "KON", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die Luft; und dieneten zum Unterschied", "tokens": ["Die", "Luft", ";", "und", "die\u00b7ne\u00b7ten", "zum", "Un\u00b7ter\u00b7schied"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.18": {"text": "F\u00fcr Hierarchien und Ordnungen und Stufen.", "tokens": ["F\u00fcr", "Hier\u00b7ar\u00b7chi\u00b7en", "und", "Ord\u00b7nun\u00b7gen", "und", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.19": {"text": "In ihren hellen Stoff war manche That", "tokens": ["In", "ih\u00b7ren", "hel\u00b7len", "Stoff", "war", "man\u00b7che", "That"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Von Lieb und heilgem Eifer eingewebt.", "tokens": ["Von", "Lieb", "und", "heil\u00b7gem", "Ei\u00b7fer", "ein\u00b7ge\u00b7webt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Inde\u00df, da\u00df Myriad an Myriade,", "tokens": ["In\u00b7de\u00df", ",", "da\u00df", "My\u00b7ri\u00b7ad", "an", "My\u00b7ria\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Und Kreis in Kreis, sich unabsehlig dr\u00e4ngt,", "tokens": ["Und", "Kreis", "in", "Kreis", ",", "sich", "un\u00b7ab\u00b7seh\u00b7lig", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Enth\u00fcllte sich dem Blick der flammende", "tokens": ["Ent\u00b7h\u00fcll\u00b7te", "sich", "dem", "Blick", "der", "flam\u00b7men\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.24": {"text": "Lichtklare H\u00fcgel, dessen obrer Gipfel", "tokens": ["Licht\u00b7kla\u00b7re", "H\u00fc\u00b7gel", ",", "des\u00b7sen", "ob\u00b7rer", "Gip\u00b7fel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.25": {"text": "Unsichtbar war vor Herrlichkeit, die ihn", "tokens": ["Un\u00b7sicht\u00b7bar", "war", "vor", "Herr\u00b7lich\u00b7keit", ",", "die", "ihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Bedeckte. Auf ihm sa\u00df der Ewige,", "tokens": ["Be\u00b7deck\u00b7te", ".", "Auf", "ihm", "sa\u00df", "der", "E\u00b7wi\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "APPR", "PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.27": {"text": "Und neben ihm in gleicher Herrlichkeit", "tokens": ["Und", "ne\u00b7ben", "ihm", "in", "glei\u00b7cher", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Der Sohn; indem die Stimme Gottes sprach:", "tokens": ["Der", "Sohn", ";", "in\u00b7dem", "die", "Stim\u00b7me", "Got\u00b7tes", "sprach", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KOUS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt, all ihr Engel, ihr, des Lichts Geschlecht,", "tokens": ["H\u00f6rt", ",", "all", "ihr", "En\u00b7gel", ",", "ihr", ",", "des", "Lichts", "Ge\u00b7schlecht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PIAT", "PPOSAT", "NN", "$,", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jhr Thronen, F\u00fcrsten, Kr\u00e4fte, Tugenden,", "tokens": ["Ihr", "Thro\u00b7nen", ",", "F\u00fcrs\u00b7ten", ",", "Kr\u00e4f\u00b7te", ",", "Tu\u00b7gen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "H\u00f6rt meinen Rathschlu\u00df, der unwiederruflich", "tokens": ["H\u00f6rt", "mei\u00b7nen", "Rath\u00b7schlu\u00df", ",", "der", "un\u00b7wie\u00b7der\u00b7ruf\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Bestehn soll! Heute hab ich meinen Sohn", "tokens": ["Be\u00b7stehn", "soll", "!", "Heu\u00b7te", "hab", "ich", "mei\u00b7nen", "Sohn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVINF", "VMFIN", "$.", "ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gezeugt, und ihn gesalbt auf diesem H\u00fcgel,", "tokens": ["Ge\u00b7zeugt", ",", "und", "ihn", "ge\u00b7salbt", "auf", "die\u00b7sem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "PPER", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo ihr ihn seht zu meiner rechten Hand.", "tokens": ["Wo", "ihr", "ihn", "seht", "zu", "mei\u00b7ner", "rech\u00b7ten", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich setz ihn euch zu eurem Oberhaupt", "tokens": ["Ich", "setz", "ihn", "euch", "zu", "eu\u00b7rem", "O\u00b7ber\u00b7haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und K\u00f6nig; und ich habe bey mir selbst", "tokens": ["Und", "K\u00f6\u00b7nig", ";", "und", "ich", "ha\u00b7be", "bey", "mir", "selbst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$.", "KON", "PPER", "VAFIN", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Geschworen, aller Knie soll sich vor ihm", "tokens": ["Ge\u00b7schwo\u00b7ren", ",", "al\u00b7ler", "Knie", "soll", "sich", "vor", "ihm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PIAT", "NN", "VMFIN", "PRF", "APPR", "PPER"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Jm Himmel beugen, und ihn f\u00fcr den Herrn", "tokens": ["Jm", "Him\u00b7mel", "beu\u00b7gen", ",", "und", "ihn", "f\u00fcr", "den", "Herrn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "$,", "KON", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Erkennen! Unter ihm und seinem Reich", "tokens": ["Er\u00b7ken\u00b7nen", "!", "Un\u00b7ter", "ihm", "und", "sei\u00b7nem", "Reich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Seyd, als wie Eine Seele stets vereint,", "tokens": ["Seyd", ",", "als", "wie", "Ei\u00b7ne", "See\u00b7le", "stets", "ver\u00b7eint", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "$,", "KOUS", "KOKOM", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Auf ewig gl\u00fccklich. Wer ihm den Gehorsam", "tokens": ["Auf", "e\u00b7wig", "gl\u00fcck\u00b7lich", ".", "Wer", "ihm", "den", "Ge\u00b7hor\u00b7sam"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJD", "$.", "PWS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Versagt, versagt ihn mir; zerrei\u00dft das Band", "tokens": ["Ver\u00b7sagt", ",", "ver\u00b7sagt", "ihn", "mir", ";", "zer\u00b7rei\u00dft", "das", "Band"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "PPER", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Einigkeit, und soll noch diesen Tag", "tokens": ["Der", "Ei\u00b7nig\u00b7keit", ",", "und", "soll", "noch", "die\u00b7sen", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VMFIN", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Von Gott verbannt, von seinem Anschaun fern,", "tokens": ["Von", "Gott", "ver\u00b7bannt", ",", "von", "sei\u00b7nem", "An\u00b7schaun", "fern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Herunterst\u00fcrzen in die \u00e4u\u00dferste", "tokens": ["Her\u00b7un\u00b7ter\u00b7st\u00fcr\u00b7zen", "in", "die", "\u00e4u\u00b7\u00dfers\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.18": {"text": "Furchtbare Finsterni\u00df, den Ort der Quaal,", "tokens": ["Furcht\u00b7ba\u00b7re", "Fins\u00b7ter\u00b7ni\u00df", ",", "den", "Ort", "der", "Qua\u00b7al", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.19": {"text": "F\u00fcr ihn bestimmt, ohn End, und ohn\u2019 Erl\u00f6sung.", "tokens": ["F\u00fcr", "ihn", "be\u00b7stimmt", ",", "ohn", "End", ",", "und", "ohn'", "Er\u00b7l\u00f6\u00b7sung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "KOUI", "NN", "$,", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}