{"dta.poem.20657": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Das Lob des Essens .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Lob des Rebensaftes ward                 ", "tokens": ["Das", "Lob", "des", "Re\u00b7bens\u00b7af\u00b7tes", "ward"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von keinem Dichter je vergessen,", "tokens": ["Von", "kei\u00b7nem", "Dich\u00b7ter", "je", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch keiner sang mit gleicher Art", "tokens": ["Doch", "kei\u00b7ner", "sang", "mit", "glei\u00b7cher", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lob vom guten Essen.", "tokens": ["Das", "Lob", "vom", "gu\u00b7ten", "Es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O, wenn wir von dem Hunger stark", "tokens": ["O", ",", "wenn", "wir", "von", "dem", "Hun\u00b7ger", "stark"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Getrieben sind zum vollen Tische,", "tokens": ["Ge\u00b7trie\u00b7ben", "sind", "zum", "vol\u00b7len", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erregt alsdann des Rindes Mark,", "tokens": ["Er\u00b7regt", "als\u00b7dann", "des", "Rin\u00b7des", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Brustkern, und die Fische,", "tokens": ["Der", "Brust\u00b7kern", ",", "und", "die", "Fi\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Feldhuhn, oder von dem Reh", "tokens": ["Das", "Feld\u00b7huhn", ",", "o\u00b7der", "von", "dem", "Reh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wohlgebratne zarte R\u00fccken,", "tokens": ["Der", "wohl\u00b7ge\u00b7brat\u00b7ne", "zar\u00b7te", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und selbst der Hummer aus der See,", "tokens": ["Und", "selbst", "der", "Hum\u00b7mer", "aus", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Gaumen kein Entz\u00fccken?", "tokens": ["Dem", "Gau\u00b7men", "kein", "Ent\u00b7z\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie? w\u00e4re nicht aus Calekut", "tokens": ["Wie", "?", "w\u00e4\u00b7re", "nicht", "aus", "Ca\u00b7le\u00b7kut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VAFIN", "PTKNEG", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Hahn, und eines Hammels Lende", "tokens": ["Der", "Hahn", ",", "und", "ei\u00b7nes", "Ham\u00b7mels", "Len\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So liederwerth, als Traubenblut,", "tokens": ["So", "lie\u00b7der\u00b7werth", ",", "als", "Trau\u00b7ben\u00b7blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ich vortrefflich f\u00e4nde?", "tokens": ["Das", "ich", "vor\u00b7treff\u00b7lich", "f\u00e4n\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sprich, Quintus! wenn Du m\u00fcd und matt", "tokens": ["Sprich", ",", "Quin\u00b7tus", "!", "wenn", "Du", "m\u00fcd", "und", "matt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "NE", "$.", "KOUS", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Lager kamst von Kriegesthaten,", "tokens": ["Ins", "La\u00b7ger", "kamst", "von", "Krie\u00b7gest\u00b7ha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie reizte Dich das Schulterblatt", "tokens": ["Wie", "reiz\u00b7te", "Dich", "das", "Schul\u00b7ter\u00b7blatt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Ebers frisch gebraten!", "tokens": ["Des", "E\u00b7bers", "frisch", "ge\u00b7bra\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mit welcher Wollust des Geschmacks", "tokens": ["Mit", "wel\u00b7cher", "Wol\u00b7lust", "des", "Ge\u00b7schmacks"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verzehrtest Du, statt der Melonen", "tokens": ["Ver\u00b7zehr\u00b7test", "Du", ",", "statt", "der", "Me\u00b7lo\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUI", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und Pfirsichen, den trocknen Lachs", "tokens": ["Und", "Pfir\u00b7si\u00b7chen", ",", "den", "trock\u00b7nen", "Lachs"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Betr\u00e4ufelt von Citronen!", "tokens": ["Be\u00b7tr\u00e4u\u00b7felt", "von", "Cit\u00b7ro\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wenn Dir noch anjezt ", "tokens": ["Und", "wenn", "Dir", "noch", "an\u00b7jezt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Nichts darf verbieten, nichts befehlen,", "tokens": ["Nichts", "darf", "ver\u00b7bie\u00b7ten", ",", "nichts", "be\u00b7feh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "$,", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Siehst Du mit Lust die Sch\u00fcsseln stehn", "tokens": ["Siehst", "Du", "mit", "Lust", "die", "Sch\u00fcs\u00b7seln", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lobst sie vor Pok\u00e4len.", "tokens": ["Und", "lobst", "sie", "vor", "Po\u00b7k\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}