{"textgrid.poem.55065": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ein andres", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh! gehorche meinen Winken,", "tokens": ["Geh", "!", "ge\u00b7hor\u00b7che", "mei\u00b7nen", "Win\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nutze deine jungen Tage,", "tokens": ["Nut\u00b7ze", "dei\u00b7ne", "jun\u00b7gen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lerne zeitig kl\u00fcger sein:", "tokens": ["Ler\u00b7ne", "zei\u00b7tig", "kl\u00fc\u00b7ger", "sein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des Gl\u00fcckes gro\u00dfer Waage", "tokens": ["Auf", "des", "Gl\u00fc\u00b7ckes", "gro\u00b7\u00dfer", "Waa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Steht die Zunge selten ein;", "tokens": ["Steht", "die", "Zun\u00b7ge", "sel\u00b7ten", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du mu\u00dft steigen oder sinken,", "tokens": ["Du", "mu\u00dft", "stei\u00b7gen", "o\u00b7der", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "VVINF"