{"dta.poem.19824": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich ging einmal nach Gra\u00dfdorf nein,               ", "tokens": ["Ich", "ging", "ein\u00b7mal", "nach", "Gra\u00df\u00b7dorf", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kam ich vor die Schenke,", "tokens": ["Da", "kam", "ich", "vor", "die", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und da ich vor die Schenke kam,", "tokens": ["Und", "da", "ich", "vor", "die", "Schen\u00b7ke", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da fing mich an zu dursten.", "tokens": ["Da", "fing", "mich", "an", "zu", "durs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Wirth der sezt mich oben an,", "tokens": ["Der", "Wirth", "der", "sezt", "mich", "o\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er dacht ich w\u00e4r der beste.", "tokens": ["Er", "dacht", "ich", "w\u00e4r", "der", "bes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ey Mutter Gottes ja", "tokens": ["Ey", "Mut\u00b7ter", "Got\u00b7tes", "ja"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Maynbl\u00fcmlein bla,", "tokens": ["Mayn\u00b7bl\u00fcm\u00b7lein", "bla", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wie lachten die andern G\u00e4ste.", "tokens": ["Wie", "lach\u00b7ten", "die", "an\u00b7dern", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und weil ich nun gegessen hatt'", "tokens": ["Und", "weil", "ich", "nun", "ge\u00b7ges\u00b7sen", "hatt'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sollt' ich auch bezahlen,", "tokens": ["Da", "sollt'", "ich", "auch", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da fragt ich, was die Mahlzeit kost',", "tokens": ["Da", "fragt", "ich", ",", "was", "die", "Mahl\u00b7zeit", "kost'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sprach der Wirth ein Thaler,", "tokens": ["Da", "sprach", "der", "Wirth", "ein", "Tha\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ey Mutter Gottes ja", "tokens": ["Ey", "Mut\u00b7ter", "Got\u00b7tes", "ja"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Maynbl\u00fcmlein bla,", "tokens": ["Mayn\u00b7bl\u00fcm\u00b7lein", "bla", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da hatt' ich keinen Thaler.", "tokens": ["Da", "hatt'", "ich", "kei\u00b7nen", "Tha\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Wirth der zog mein R\u00f6ckle aus,", "tokens": ["Der", "Wirth", "der", "zog", "mein", "R\u00f6ck\u00b7le", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jagt mich in die Scheune,", "tokens": ["Und", "jagt", "mich", "in", "die", "Scheu\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ey Mutter Gottes ja, Maynbl\u00fcmlein bla,", "tokens": ["Ey", "Mut\u00b7ter", "Got\u00b7tes", "ja", ",", "Mayn\u00b7bl\u00fcm\u00b7lein", "bla", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "PTKANT", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie lang war mir die Weile.", "tokens": ["Wie", "lang", "war", "mir", "die", "Wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und als es gegen Morgen kam,", "tokens": ["Und", "als", "es", "ge\u00b7gen", "Mor\u00b7gen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da tr\u00e4ufelts von dem Dache,", "tokens": ["Da", "tr\u00e4u\u00b7felts", "von", "dem", "Da\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.7": {"text": "Ey Mutter Gottes ja", "tokens": ["Ey", "Mut\u00b7ter", "Got\u00b7tes", "ja"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Maynbl\u00fcmlein bla,", "tokens": ["Mayn\u00b7bl\u00fcm\u00b7lein", "bla", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Da mu\u00dft ich selber lachen.", "tokens": ["Da", "mu\u00dft", "ich", "sel\u00b7ber", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als es gegen Mittag kam,", "tokens": ["Und", "als", "es", "ge\u00b7gen", "Mit\u00b7tag", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da zog der Wirth mir's K\u00e4pple aus,", "tokens": ["Da", "zog", "der", "Wirth", "mir's", "K\u00e4pp\u00b7le", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jagt mich auf die Stra\u00dfe.", "tokens": ["Und", "jagt", "mich", "auf", "die", "Stra\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und als ich auf die Stra\u00dfe kam,", "tokens": ["Und", "als", "ich", "auf", "die", "Stra\u00b7\u00dfe", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Schuh warn sehr zerbrochen,", "tokens": ["Die", "Schuh", "warn", "sehr", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ey Mutter Gottes ja", "tokens": ["Ey", "Mut\u00b7ter", "Got\u00b7tes", "ja"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Maynbl\u00fcmlein bla,", "tokens": ["Mayn\u00b7bl\u00fcm\u00b7lein", "bla", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Da lief ich auf den Socken.", "tokens": ["Da", "lief", "ich", "auf", "den", "So\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}