{"textgrid.poem.48319": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2. Vom Fehrbelliner Schlachtfeld", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blumen, o Freundin, dir mitzubringen", "tokens": ["Blu\u00b7men", ",", "o", "Freun\u00b7din", ",", "dir", "mit\u00b7zu\u00b7brin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "FM", "NN", "$,", "PPER", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Von diesem Feld, es wollt' nicht gelingen.", "tokens": ["Von", "die\u00b7sem", "Feld", ",", "es", "wollt'", "nicht", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Hafer nur, soweit ich sah,", "tokens": ["Ha\u00b7fer", "nur", ",", "so\u00b7weit", "ich", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hafer, Hafer nur war da.", "tokens": ["Ha\u00b7fer", ",", "Ha\u00b7fer", "nur", "war", "da", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADV", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00e4rkische Rosse gewannen die Schlacht,", "tokens": ["M\u00e4r\u00b7ki\u00b7sche", "Ros\u00b7se", "ge\u00b7wan\u00b7nen", "die", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measur