chein", "tokens": ["So", "\u00fc\u00b7ber\u00b7gl\u00e4nzt", "mit", "Pur\u00b7pur\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Liebe nun mein ganzes Sein,", "tokens": ["Die", "Lie\u00b7be", "nun", "mein", "gan\u00b7zes", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gie\u00dft goldne Feuer nieder", "tokens": ["Gie\u00dft", "gold\u00b7ne", "Feu\u00b7er", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und wirbt um neue Lieder ...", "tokens": ["Und", "wirbt", "um", "neu\u00b7e", "Lie\u00b7der", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und nah und ferne quellen", "tokens": ["Und", "nah", "und", "fer\u00b7ne", "quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Blitzende Welten empor", "tokens": ["Blit\u00b7zen\u00b7de", "Wel\u00b7ten", "em\u00b7por"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "PTKVZ"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "An meinem Lebenshorizont", "tokens": ["An", "mei\u00b7nem", "Le\u00b7bens\u00b7ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Dunst und Wolkenflor!", "tokens": ["Aus", "Dunst", "und", "Wol\u00b7ken\u00b7flor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gedanken, die mir nie genaht,", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "die", "mir", "nie", "ge\u00b7naht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Pfade, die ich nie betrat,", "tokens": ["Und", "Pfa\u00b7de", ",", "die", "ich", "nie", "be\u00b7trat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Entsteigen verborgenen Gr\u00fcnden,", "tokens": ["Ent\u00b7stei\u00b7gen", "ver\u00b7bor\u00b7ge\u00b7nen", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Heilige Kraft zu entz\u00fcnden!", "tokens": ["Hei\u00b7li\u00b7ge", "Kraft", "zu", "ent\u00b7z\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Die leise schon verklungen,", "tokens": ["Die", "lei\u00b7se", "schon", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die m\u00fcde schon vergl\u00fcht:", "tokens": ["Die", "m\u00fc\u00b7de", "schon", "ver\u00b7gl\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wild ist sie wieder aufgeflammt,", "tokens": ["Wild", "ist", "sie", "wie\u00b7der", "auf\u00b7ge\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Lenzsturm stark erbl\u00fcht!", "tokens": ["Im", "Lenz\u00b7sturm", "stark", "er\u00b7bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und lag ich wieder staubbedeckt,", "tokens": ["Und", "lag", "ich", "wie\u00b7der", "staub\u00b7be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So hab' ich mich nun aufgereckt,", "tokens": ["So", "hab'", "ich", "mich", "nun", "auf\u00b7ge\u00b7reckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und die Gedanken schweifen", "tokens": ["Und", "die", "Ge\u00b7dan\u00b7ken", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In gro\u00dfem Weltbegreifen!", "tokens": ["In", "gro\u00b7\u00dfem", "Welt\u00b7be\u00b7grei\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die m\u00fcde schon vergl\u00fchte,", "tokens": ["Die", "m\u00fc\u00b7de", "schon", "ver\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die leise schon verklang,", "tokens": ["Die", "lei\u00b7