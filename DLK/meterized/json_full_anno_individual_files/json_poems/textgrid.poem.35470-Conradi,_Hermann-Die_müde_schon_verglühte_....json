{"textgrid.poem.35470": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die m\u00fcde schon vergl\u00fchte ...", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die m\u00fcde schon vergl\u00fchte,", "tokens": ["Die", "m\u00fc\u00b7de", "schon", "ver\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die leise schon verklang,", "tokens": ["Die", "lei\u00b7se", "schon", "ver\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jach ist sie wieder aufgeflammt", "tokens": ["Jach", "ist", "sie", "wie\u00b7der", "auf\u00b7ge\u00b7flammt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In jauchzendem Gesang!", "tokens": ["In", "jauch\u00b7zen\u00b7dem", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie Zymbelton, wie Lautenschlag", "tokens": ["Wie", "Zym\u00b7bel\u00b7ton", ",", "wie", "Lau\u00b7ten\u00b7schlag"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "NE", "$,", "PWAV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ward meine Liebe wieder wach,", "tokens": ["Ward", "mei\u00b7ne", "Lie\u00b7be", "wie\u00b7der", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die m\u00fcde schon vergl\u00fchte,", "tokens": ["Die", "m\u00fc\u00b7de", "schon", "ver\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die leise schon verklang ...", "tokens": ["Die", "lei\u00b7se", "schon", "ver\u00b7klang", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und heller t\u00f6nt ihr Rauschen,", "tokens": ["Und", "hel\u00b7ler", "t\u00f6nt", "ihr", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie junger Fr\u00fchlingswind,", "tokens": ["Wie", "jun\u00b7ger", "Fr\u00fch\u00b7lings\u00b7wind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn er in hei\u00dfem Sch\u00f6pferdrang", "tokens": ["Wenn", "er", "in", "hei\u00b7\u00dfem", "Sch\u00f6p\u00b7fer\u00b7drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Welt dem Licht gewinnt", "tokens": ["Die", "Welt", "dem", "Licht", "ge\u00b7winnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und das Prophetenwort erl\u00e4\u00dft,", "tokens": ["Und", "das", "Pro\u00b7phe\u00b7ten\u00b7wort", "er\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df nun