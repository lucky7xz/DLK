{"textgrid.poem.46326": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[o Gott, wan dein lob h\u00f6ren mag]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gott, wan dein lob h\u00f6ren mag,", "tokens": ["O", "Gott", ",", "wan", "dein", "lob", "h\u00f6\u00b7ren", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein trost ist vnermessen:", "tokens": ["Mein", "trost", "ist", "vner\u00b7mes\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hingegen ist mein h\u00f6chste klag,", "tokens": ["Hin\u00b7ge\u00b7gen", "ist", "mein", "h\u00f6chs\u00b7te", "klag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Wan deiner wird vergessen:", "tokens": ["Wan", "dei\u00b7ner", "wird", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wan ich die wunder dein betracht", "tokens": ["Wan", "ich", "die", "wun\u00b7der", "dein", "be\u00b7tracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Die krafft, vnd macht bedencke,", "tokens": ["Die", "krafft", ",", "vnd", "macht", "be\u00b7den\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erstarr ich, Herr, ob solchem pracht,", "tokens": ["Er\u00b7starr", "ich", ",", "Herr", ",", "ob", "sol\u00b7chem", "pracht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "KOUS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd mich zur erden sencke.", "tokens": ["Vnd", "mich", "zur", "er\u00b7den", "sen\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, ach, seind wir nit erden staub?", "tokens": ["Ach", ",", "ach", ",", "seind", "wir", "nit", "er\u00b7den", "staub", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$,", "VAFIN", "PPER", "PTKNEG", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "So gar vor dir verschwinden,", "tokens": ["So", "gar", "vor", "dir", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd fliegen hin wie d\u00fcrres laub,", "tokens": ["Vnd", "flie\u00b7gen", "hin", "wie", "d\u00fcr\u00b7res", "laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das triben wird von winden.", "tokens": ["Das", "tri\u00b7ben", "wird", "von", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VAFIN", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur dir allein, \u00f4 m\u00e4chtig Gott,", "tokens": ["Nur", "dir", "al\u00b7lein", ",", "\u00f4", "m\u00e4ch\u00b7tig", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "$,", "FM", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df alles vnter-ligen;", "tokens": ["Mu\u00df", "al\u00b7les", "vn\u00b7ter\u00b7li\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Teuffel auch sampt ihrer rott", "tokens": ["Die", "Teuf\u00b7fel", "auch", "sampt", "ih\u00b7rer", "rott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vor dir sich m\u00fcssen biegen.", "tokens": ["Vor", "dir", "sich", "m\u00fcs\u00b7sen", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Cherubim in vollem brand,", "tokens": ["Die", "Che\u00b7ru\u00b7bim", "in", "vol\u00b7lem", "brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seind deine Ro\u00df vnd Wagen:", "tokens": ["Seind", "dei\u00b7ne", "Ro\u00df", "vnd", "Wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die starcke wind gehn dir zu hand,", "tokens": ["Die", "star\u00b7cke", "wind", "gehn", "dir", "zu", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd dich auff h\u00e4nden tragen.", "tokens": ["Vnd", "dich", "auff", "h\u00e4n\u00b7den", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auff ihren fl\u00fcttig manigfalt", "tokens": ["Auff", "ih\u00b7ren", "fl\u00fct\u00b7tig", "ma\u00b7nig\u00b7falt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit macht kompst anher tretten,", "tokens": ["Mit", "macht", "kompst", "an\u00b7her", "tret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die wolcken reissest mit gewalt;", "tokens": ["Die", "wol\u00b7cken", "reis\u00b7sest", "mit", "ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "All welt mu\u00df dich anbetten.", "tokens": ["All", "welt", "mu\u00df", "dich", "an\u00b7bet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Fewr, kolen, blitz, seind deine pfeil,", "tokens": ["Fewr", ",", "ko\u00b7len", ",", "blitz", ",", "seind", "dei\u00b7ne", "pfeil", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVIMP", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der himmel ist dein bogen:", "tokens": ["Der", "him\u00b7mel", "ist", "dein", "bo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da fahren ab die donnerkeil,", "tokens": ["Da", "fah\u00b7ren", "ab", "die", "don\u00b7ner\u00b7keil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wan du kompst auffgezogen.", "tokens": ["Wan", "du", "kompst", "auff\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "VVIZU", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Das wilde meer: thut w\u00fcten sehr,", "tokens": ["Das", "wil\u00b7de", "meer", ":", "thut", "w\u00fc\u00b7ten", "sehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So bald dein stimm erschollen,", "tokens": ["So", "bald", "dein", "stimm", "er\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geht hoch so fehrr: als wan es wer", "tokens": ["Geht", "hoch", "so", "fehrr", ":", "als", "wan", "es", "wer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "ADJD", "$.", "KON", "PWAV", "PPER", "PWS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zun wolcken auffgeschwollen.", "tokens": ["Zun", "wol\u00b7cken", "auff\u00b7ge\u00b7schwol\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die wellen toben immerdar,", "tokens": ["Die", "wel\u00b7len", "to\u00b7ben", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd gehn die wasser-wogen.", "tokens": ["Vnd", "gehn", "die", "was\u00b7ser\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die berg, vnd felsen allegar", "tokens": ["Die", "berg", ",", "vnd", "fel\u00b7sen", "al\u00b7le\u00b7gar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor dir stehn krum gebogen.", "tokens": ["Vor", "dir", "stehn", "krum", "ge\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die erden rund: mu\u00df gleich zur stund", "tokens": ["Die", "er\u00b7den", "rund", ":", "mu\u00df", "gleich", "zur", "stund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$.", "VMFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von sch\u00fcttlen sich entdecken:", "tokens": ["Von", "sch\u00fctt\u00b7len", "sich", "ent\u00b7de\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der hellen schlund: mu\u00df tieff in grund", "tokens": ["Der", "hel\u00b7len", "schlund", ":", "mu\u00df", "tieff", "in", "grund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "$.", "VMFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ob deiner macht erschrecken.", "tokens": ["Ob", "dei\u00b7ner", "macht", "er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Gott, von deiner glory gro\u00df,", "tokens": ["O", "Gott", ",", "von", "dei\u00b7ner", "glo\u00b7ry", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu reden mu\u00df verzagen;", "tokens": ["Zu", "re\u00b7den", "mu\u00df", "ver\u00b7za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ewig wort in deinem scho\u00df,", "tokens": ["Das", "e\u00b7wig", "wort", "in", "dei\u00b7nem", "scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "NN", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allein es recht mag sagen.", "tokens": ["Al\u00b7lein", "es", "recht", "mag", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein herrlichkeit ist ohne ma\u00df,", "tokens": ["Dein", "herr\u00b7lich\u00b7keit", "ist", "oh\u00b7ne", "ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein ma\u00df ist auch der g\u00fcte.", "tokens": ["Kein", "ma\u00df", "ist", "auch", "der", "g\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drumb dich will ich noch loben ba\u00df,", "tokens": ["Drumb", "dich", "will", "ich", "noch", "lo\u00b7ben", "ba\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VMFIN", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Darnach steht mein gem\u00fcte.", "tokens": ["Dar\u00b7nach", "steht", "mein", "ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O Gott, wan dein lob h\u00f6ren mag,", "tokens": ["O", "Gott", ",", "wan", "dein", "lob", "h\u00f6\u00b7ren", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein trost ist vnermessen:", "tokens": ["Mein", "trost", "ist", "vner\u00b7mes\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hingegen ist mein h\u00f6chste klag,", "tokens": ["Hin\u00b7ge\u00b7gen", "ist", "mein", "h\u00f6chs\u00b7te", "klag", ","], "token_info": ["word", "word", "word", "word", "wor