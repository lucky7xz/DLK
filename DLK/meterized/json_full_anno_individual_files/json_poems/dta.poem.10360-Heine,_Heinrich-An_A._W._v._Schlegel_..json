{"dta.poem.10360": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An A. W. v. Schlegel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Im Reifrockputz, mit Blumen reich verzieret,", "tokens": ["Im", "Rei\u00b7frock\u00b7putz", ",", "mit", "Blu\u00b7men", "reich", "ver\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sch\u00f6npfl\u00e4sterchen auf den geschminkten Wangen,", "tokens": ["Sch\u00f6n\u00b7pfl\u00e4s\u00b7ter\u00b7chen", "auf", "den", "ge\u00b7schmink\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mit Schnabelschuh'n, mit Stickerei'n behangen,", "tokens": ["Mit", "Schna\u00b7bel\u00b7schuh'n", ",", "mit", "Sti\u00b7cke\u00b7rei'n", "be\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Thurmfrisur, und wespengleich geschn\u00fcret;", "tokens": ["Mit", "Thurm\u00b7fri\u00b7sur", ",", "und", "wes\u00b7pen\u00b7gleich", "ge\u00b7schn\u00fc\u00b7ret", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So war die Aftermuse ausstaffiret,", "tokens": ["So", "war", "die", "Af\u00b7ter\u00b7mu\u00b7se", "aus\u00b7staf\u00b7fi\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als sie einst kam, dich liebend zu umfangen.", "tokens": ["Als", "sie", "einst", "kam", ",", "dich", "lie\u00b7bend", "zu", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du bist ihr aber aus dem Weg gegangen,", "tokens": ["Du", "bist", "ihr", "a\u00b7ber", "aus", "dem", "Weg", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und irrtest fort von dunkelm Trieb gef\u00fchret.", "tokens": ["Und", "irr\u00b7test", "fort", "von", "dun\u00b7kelm", "Trieb", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da fandest du ein Schlo\u00df in alter Wildni\u00df,", "tokens": ["Da", "fan\u00b7dest", "du", "ein", "Schlo\u00df", "in", "al\u00b7ter", "Wild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und drinnen lag, wie'n holdes Marmorbildni\u00df,", "tokens": ["Und", "drin\u00b7nen", "lag", ",", "wie'n", "hol\u00b7des", "Mar\u00b7mor\u00b7bild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die sch\u00f6nste Maid in Zauberschlaf versunken.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Maid", "in", "Zau\u00b7ber\u00b7schlaf", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Doch wich der Zauber bald, bei deinem Gru\u00dfe", "tokens": ["Doch", "wich", "der", "Zau\u00b7ber", "bald", ",", "bei", "dei\u00b7nem", "Gru\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Aufwachte l\u00e4chelnd Deutschlands \u00e4chte Muse,", "tokens": ["Auf\u00b7wach\u00b7te", "l\u00e4\u00b7chelnd", "Deutschlands", "\u00e4ch\u00b7te", "Mu\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Und sank in deine Arme liebestrunken.", "tokens": ["Und", "sank", "in", "dei\u00b7ne", "Ar\u00b7me", "lie\u00b7be\u00b7strun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}