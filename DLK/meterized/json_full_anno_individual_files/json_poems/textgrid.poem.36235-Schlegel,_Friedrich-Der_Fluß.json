{"textgrid.poem.36235": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Flu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie rein Gesang sich windet", "tokens": ["Wie", "rein", "Ge\u00b7sang", "sich", "win\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch wunderbarer Saitenspiele Rauschen,", "tokens": ["Durch", "wun\u00b7der\u00b7ba\u00b7rer", "Sai\u00b7ten\u00b7spie\u00b7le", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er selbst sich wieder findet,", "tokens": ["Er", "selbst", "sich", "wie\u00b7der", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie auch die Weisen tauschen,", "tokens": ["Wie", "auch", "die", "Wei\u00b7sen", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df neu entz\u00fcckt die H\u00f6rer ewig lauschen;", "tokens": ["Da\u00df", "neu", "ent\u00b7z\u00fcckt", "die", "H\u00f6\u00b7rer", "e\u00b7wig", "lau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So flie\u00dfet mir gediegen", "tokens": ["So", "flie\u00b7\u00dfet", "mir", "ge\u00b7die\u00b7gen"], "t