{"textgrid.poem.39545": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sanct Obesus sa\u00df nach Tische,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sanct Obesus sa\u00df nach Tische,", "tokens": ["Sanct", "O\u00b7be\u00b7sus", "sa\u00df", "nach", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Alle Westenkn\u00f6pfe los,", "tokens": ["Al\u00b7le", "Wes\u00b7ten\u00b7kn\u00f6p\u00b7fe", "los", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Gartensales Frische,", "tokens": ["In", "des", "Gar\u00b7ten\u00b7sa\u00b7les", "Fri\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des Sopha's weichem Schoo\u00df.", "tokens": ["Auf", "des", "So\u00b7pha's", "wei\u00b7chem", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit den runden Armen hielt er", "tokens": ["Mit", "den", "run\u00b7den", "Ar\u00b7men", "hielt", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen Bauch gelind' umspannt,", "tokens": ["Sei\u00b7nen", "Bauch", "ge\u00b7lind'", "um\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Einem Daume spielt' er", "tokens": ["Und", "mit", "Ei\u00b7nem", "Dau\u00b7me", "spielt'", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um den Daum der andern Hand.", "tokens": ["Um", "den", "Daum", "der", "an\u00b7dern", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schlummernd fast, doch geistig wacht' er", "tokens": ["Schlum\u00b7mernd", "fast", ",", "doch", "geis\u00b7tig", "wacht'", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "ADV", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In beschaulich sel'ger Ruh;", "tokens": ["In", "be\u00b7schau\u00b7lich", "sel'\u00b7ger", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Mahlzeit \u00fcberdacht' er,", "tokens": ["Sei\u00b7ne", "Mahl\u00b7zeit", "\u00fc\u00b7ber\u00b7dacht'", "er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manchen guten Trunk dazu.", "tokens": ["Man\u00b7chen", "gu\u00b7ten", "Trunk", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbschmackhaft sind die Gottesgaben,", "tokens": ["\u00bb", "schmack\u00b7haft", "sind", "die", "Got\u00b7tes\u00b7ga\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00e4rkend einem frommen Mann.", "tokens": ["St\u00e4r\u00b7kend", "ei\u00b7nem", "from\u00b7men", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie nicht die S\u00fcnder laben,", "tokens": ["Da\u00df", "sie", "nicht", "die", "S\u00fcn\u00b7der", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nehm' ich ein, soviel ich kann.\u00ab", "tokens": ["Nehm'", "ich", "ein", ",", "so\u00b7viel", "ich", "kann", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "VMFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "M\u00fchsam ist es, erst zu kauen,", "tokens": ["M\u00fch\u00b7sam", "ist", "es", ",", "erst", "zu", "kau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leicht ist auch das Schl\u00fcrfen nicht;", "tokens": ["Leicht", "ist", "auch", "das", "Schl\u00fcr\u00b7fen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fchsam ist es, zu verdauen:", "tokens": ["M\u00fch\u00b7sam", "ist", "es", ",", "zu", "ver\u00b7dau\u00b7en", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ich halt' auf meine Pflicht.", "tokens": ["Doch", "ich", "halt'", "auf", "mei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Siede den Caffee nun, Grete!", "tokens": ["Sie\u00b7de", "den", "Caf\u00b7fee", "nun", ",", "Gre\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Zwar des Mahomet Geschenk.", "tokens": ["Zwar", "des", "Ma\u00b7ho\u00b7met", "Ge\u00b7schenk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "L\u00fcgenhaft war der Prophete,", "tokens": ["L\u00fc\u00b7gen\u00b7haft", "war", "der", "Pro\u00b7phe\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Aber gut ist sein Getr\u00e4nk.", "tokens": ["A\u00b7ber", "gut", "ist", "sein", "Ge\u00b7tr\u00e4nk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach! die Welt liegt ganz im Argen;", "tokens": ["Ach", "!", "die", "Welt", "liegt", "ganz", "im", "Ar\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seelentrost bedarf ich sehr.", "tokens": ["See\u00b7lent\u00b7rost", "be\u00b7darf", "ich", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mahnen mich doch gar die Kargen,", "tokens": ["Mah\u00b7nen", "mich", "doch", "gar", "die", "Kar\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollen mir nicht borgen mehr.", "tokens": ["Wol\u00b7len", "mir", "nicht", "bor\u00b7gen", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wir, die Heil'gen sollten leben", "tokens": ["Wir", ",", "die", "Heil'\u00b7gen", "soll\u00b7ten", "le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der andern Schwei\u00df und Gut.", "tokens": ["Von", "der", "an\u00b7dern", "Schwei\u00df", "und", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nichts umsonst will geben", "tokens": ["A\u00b7ber", "nichts", "um\u00b7sonst", "will", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese aufgekl\u00e4rte Brut.", "tokens": ["Die\u00b7se", "auf\u00b7ge\u00b7kl\u00e4r\u00b7te", "Brut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Waffne dich mit Ungewittern,", "tokens": ["Waff\u00b7ne", "dich", "mit", "Un\u00b7ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herr, zu strengem Strafgericht!", "tokens": ["Herr", ",", "zu", "stren\u00b7gem", "Straf\u00b7ge\u00b7richt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df das freche Weltkind zittern,", "tokens": ["La\u00df", "das", "fre\u00b7che", "Welt\u00b7kind", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das von Schuld und Zahlung spricht!", "tokens": ["Das", "von", "Schuld", "und", "Zah\u00b7lung", "spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Jeder Schuldschein ist zerri\u00dfen,", "tokens": ["Je\u00b7der", "Schuld\u00b7schein", "ist", "zer\u00b7ri\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann der j\u00fcngste Tag erscheint;", "tokens": ["Wann", "der", "j\u00fcngs\u00b7te", "Tag", "er\u00b7scheint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er kommt, das sollt ihr wi\u00dfen,", "tokens": ["Und", "er", "kommt", ",", "das", "sollt", "ihr", "wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "B\u00f6se Gl\u00e4ub'ger, eh' ihr's meint.", "tokens": ["B\u00f6\u00b7se", "Gl\u00e4ub'\u00b7ger", ",", "eh'", "ih\u00b7r's", "meint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Innerlich erleuchtet, wei\u00df ich,", "tokens": ["In\u00b7ner\u00b7lich", "er\u00b7leuch\u00b7tet", ",", "wei\u00df", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was in Zukunft wird geschehn.", "tokens": ["Was", "in", "Zu\u00b7kunft", "wird", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Achtzehnhundert drei und drei\u00dfig", "tokens": ["Acht\u00b7zehn\u00b7hun\u00b7dert", "drei", "und", "drei\u00b7\u00dfig"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "CARD", "KON", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird die Welt in Brand vergehn.", "tokens": ["Wird", "die", "Welt", "in", "Brand", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Dann beginnt das Reich, das tausend", "tokens": ["Dann", "be\u00b7ginnt", "das", "Reich", ",", "das", "tau\u00b7send"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jahr' in Freuden wird bestehn.", "tokens": ["Jahr'", "in", "Freu\u00b7den", "wird", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Auserw\u00e4hlten schmausend", "tokens": ["Mit", "den", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "schmau\u00b7send"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich dieses Fest begehn.", "tokens": ["Will", "ich", "die\u00b7ses", "Fest", "be\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Sanct Obesus sa\u00df nach Tische,", "tokens": ["Sanct", "O\u00b7be\u00b7sus", "sa\u00df", "nach", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Alle Westenkn\u00f6pfe los,", "tokens": ["Al\u00b7le", "Wes\u00b7ten\u00b7kn\u00f6p\u00b7fe", "los", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Gartensales Frische,", "tokens": ["In", "des", "Gar\u00b7ten\u00b7sa\u00b7les", "Fri\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des Sopha's weichem Schoo\u00df.", "tokens": ["Auf", "des", "So\u00b7pha's", "wei\u00b7chem", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Mit den runden Armen hielt er", "tokens": ["Mit", "den", "run\u00b7den", "Ar\u00b7men", "hielt", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen Bauch gelind' umspannt,", "tokens": ["Sei\u00b7nen", "Bauch", "ge\u00b7lind'", "um\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Einem Daume spielt' er", "tokens": ["Und", "mit", "Ei\u00b7nem", "Dau\u00b7me", "spielt'", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um den Daum der andern Hand.", "tokens": ["Um", "den", "Daum", "der", "an\u00b7dern", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Schlummernd fast, doch geistig wacht' er", "tokens": ["Schlum\u00b7mernd", "fast", ",", "doch", "geis\u00b7tig", "wacht'", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "ADV", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In beschaulich sel'ger Ruh;", "tokens": ["In", "be\u00b7schau\u00b7lich", "sel'\u00b7ger", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Mahlzeit \u00fcberdacht' er,", "tokens": ["Sei\u00b7ne", "Mahl\u00b7zeit", "\u00fc\u00b7ber\u00b7dacht'", "er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manchen guten Trunk dazu.", "tokens": ["Man\u00b7chen", "gu\u00b7ten", "Trunk", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "\u00bbschmackhaft sind die Gottesgaben,", "tokens": ["\u00bb", "schmack\u00b7haft", "sind", "die", "Got\u00b7tes\u00b7ga\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00e4rkend einem frommen Mann.", "tokens": ["St\u00e4r\u00b7kend", "ei\u00b7nem", "from\u00b7men", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie nicht die S\u00fcnder laben,", "tokens": ["Da\u00df", "sie", "nicht", "die", "S\u00fcn\u00b7der", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nehm' ich ein, soviel ich kann.\u00ab", "tokens": ["Nehm'", "ich", "ein", ",", "so\u00b7viel", "ich", "kann", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "VMFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "M\u00fchsam ist es, erst zu kauen,", "tokens": ["M\u00fch\u00b7sam", "ist", "es", ",", "erst", "zu", "kau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leicht ist auch das Schl\u00fcrfen nicht;", "tokens": ["Leicht", "ist", "auch", "das", "Schl\u00fcr\u00b7fen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fchsam ist es, zu verdauen:", "tokens": ["M\u00fch\u00b7sam", "ist", "es", ",", "zu", "ver\u00b7dau\u00b7en", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ich halt' auf meine Pflicht.", "tokens": ["Doch", "ich", "halt'", "auf", "mei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Siede den Caffee nun, Grete!", "tokens": ["Sie\u00b7de", "den", "Caf\u00b7fee", "nun", ",", "Gre\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,", "NE", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Zwar des Mahomet Geschenk.", "tokens": ["Zwar", "des", "Ma\u00b7ho\u00b7met", "Ge\u00b7schenk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "L\u00fcgenhaft war der Prophete,", "tokens": ["L\u00fc\u00b7gen\u00b7haft", "war", "der", "Pro\u00b7phe\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Aber gut ist sein Getr\u00e4nk.", "tokens": ["A\u00b7ber", "gut", "ist", "sein", "Ge\u00b7tr\u00e4nk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Ach! die Welt liegt ganz im Argen;", "tokens": ["Ach", "!", "die", "Welt", "liegt", "ganz", "im", "Ar\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seelentrost bedarf ich sehr.", "tokens": ["See\u00b7lent\u00b7rost", "be\u00b7darf", "ich", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mahnen mich doch gar die Kargen,", "tokens": ["Mah\u00b7nen", "mich", "doch", "gar", "die", "Kar\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollen mir nicht borgen mehr.", "tokens": ["Wol\u00b7len", "mir", "nicht", "bor\u00b7gen", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Wir, die Heil'gen sollten leben", "tokens": ["Wir", ",", "die", "Heil'\u00b7gen", "soll\u00b7ten", "le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der andern Schwei\u00df und Gut.", "tokens": ["Von", "der", "an\u00b7dern", "Schwei\u00df", "und", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nichts umsonst will geben", "tokens": ["A\u00b7ber", "nichts", "um\u00b7sonst", "will", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese aufgekl\u00e4rte Brut.", "tokens": ["Die\u00b7se", "auf\u00b7ge\u00b7kl\u00e4r\u00b7te", "Brut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Waffne dich mit Ungewittern,", "tokens": ["Waff\u00b7ne", "dich", "mit", "Un\u00b7ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herr, zu strengem Strafgericht!", "tokens": ["Herr", ",", "zu", "stren\u00b7gem", "Straf\u00b7ge\u00b7richt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df das freche Weltkind zittern,", "tokens": ["La\u00df", "das", "fre\u00b7che", "Welt\u00b7kind", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das von Schuld und Zahlung spricht!", "tokens": ["Das", "von", "Schuld", "und", "Zah\u00b7lung", "spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Jeder Schuldschein ist zerri\u00dfen,", "tokens": ["Je\u00b7der", "Schuld\u00b7schein", "ist", "zer\u00b7ri\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann der j\u00fcngste Tag erscheint;", "tokens": ["Wann", "der", "j\u00fcngs\u00b7te", "Tag", "er\u00b7scheint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er kommt, das sollt ihr wi\u00dfen,", "tokens": ["Und", "er", "kommt", ",", "das", "sollt", "ihr", "wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "B\u00f6se Gl\u00e4ub'ger, eh' ihr's meint.", "tokens": ["B\u00f6\u00b7se", "Gl\u00e4ub'\u00b7ger", ",", "eh'", "ih\u00b7r's", "meint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.23": {"line.1": {"text": "Innerlich erleuchtet, wei\u00df ich,", "tokens": ["In\u00b7ner\u00b7lich", "er\u00b7leuch\u00b7tet", ",", "wei\u00df", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was in Zukunft wird geschehn.", "tokens": ["Was", "in", "Zu\u00b7kunft", "wird", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Achtzehnhundert drei und drei\u00dfig", "tokens": ["Acht\u00b7zehn\u00b7hun\u00b7dert", "drei", "und", "drei\u00b7\u00dfig"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "CARD", "KON", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird die Welt in Brand vergehn.", "tokens": ["Wird", "die", "Welt", "in", "Brand", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Dann beginnt das Reich, das tausend", "tokens": ["Dann", "be\u00b7ginnt", "das", "Reich", ",", "das", "tau\u00b7send"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jahr' in Freuden wird bestehn.", "tokens": ["Jahr'", "in", "Freu\u00b7den", "wird", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Auserw\u00e4hlten schmausend", "tokens": ["Mit", "den", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "schmau\u00b7send"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich dieses Fest begehn.", "tokens": ["Will", "ich", "die\u00b7ses", "Fest", "be\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}