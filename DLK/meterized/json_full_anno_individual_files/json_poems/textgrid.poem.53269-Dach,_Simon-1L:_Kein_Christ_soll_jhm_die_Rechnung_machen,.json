{"textgrid.poem.53269": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kein Christ soll jhm die Rechnung machen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Christ soll jhm die Rechnung machen,", "tokens": ["Kein", "Christ", "soll", "jhm", "die", "Rech\u00b7nung", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df lauter Sonnenschein", "tokens": ["Da\u00df", "lau\u00b7ter", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hie vmb jhn werde seyn,", "tokens": ["Hie", "vmb", "jhn", "wer\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vnd er nur schertzen m\u00fcss' vnd lachen;", "tokens": ["Vnd", "er", "nur", "schert\u00b7zen", "m\u00fcss'", "vnd", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVINF", "VMFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir haben keinen Rosen-Garten", "tokens": ["Wir", "ha\u00b7ben", "kei\u00b7nen", "Ro\u00b7sen\u00b7Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hie zu gewarten.", "tokens": ["Hie", "zu", "ge\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wer dort mit Christo hofft zu erben,", "tokens": ["Wer", "dort", "mit", "Chris\u00b7to", "hofft", "zu", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NE", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gedenck' auch f\u00fcr vnd f\u00fcr", "tokens": ["Ge\u00b7denck", "auch", "f\u00fcr", "vnd", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "KON", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In dieser Welt allhier", "tokens": ["In", "die\u00b7ser", "Welt", "all\u00b7hier"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit Ihm zu leiden vnd zu sterben.", "tokens": ["Mit", "Ihm", "zu", "lei\u00b7den", "vnd", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hie wird, was Gott vns dort erkohren,", "tokens": ["Hie", "wird", ",", "was", "Gott", "vns", "dort", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PRELS", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Creutz gebohren.", "tokens": ["Durch", "Creutz", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Was muste Christus selbst au\u00dfstehen!", "tokens": ["Was", "mus\u00b7te", "Chris\u00b7tus", "selbst", "au\u00df\u00b7ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er muste ja durch Noht", "tokens": ["Er", "mus\u00b7te", "ja", "durch", "Noht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd j\u00e4mmerlichen Tod", "tokens": ["Vnd", "j\u00e4m\u00b7mer\u00b7li\u00b7chen", "Tod"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu seiner Herrligkeit eingehen;", "tokens": ["Zu", "sei\u00b7ner", "Herr\u00b7lig\u00b7keit", "ein\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd du vermeinst mit Recht zu klagen", "tokens": ["Vnd", "du", "ver\u00b7meinst", "mit", "Recht", "zu", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In b\u00f6sen Tagen!", "tokens": ["In", "b\u00f6\u00b7sen", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der Wein mu\u00df erst gek\u00e4ltert werden,", "tokens": ["Der", "Wein", "mu\u00df", "erst", "ge\u00b7k\u00e4l\u00b7tert", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eh' als sein s\u00fcsser Safft", "tokens": ["Eh'", "als", "sein", "s\u00fcs\u00b7ser", "Safft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Trawren von vns rafft:", "tokens": ["Das", "Traw\u00b7ren", "von", "vns", "rafft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Weitzen, so vns st\u00e4rckt auff Erden,", "tokens": ["Der", "Weit\u00b7zen", ",", "so", "vns", "st\u00e4rckt", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wird durch das Mahlen vnd durch Hitze", "tokens": ["Wird", "durch", "das", "Mah\u00b7len", "vnd", "durch", "Hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vns erst zu n\u00fctze.", "tokens": ["Vns", "erst", "zu", "n\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Gold, Silber, vnd viel ander wesen", "tokens": ["Gold", ",", "Sil\u00b7ber", ",", "vnd", "viel", "an\u00b7der", "we\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "KON", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df auch durchs Fewer gehn", "tokens": ["Mu\u00df", "auch", "durchs", "Fe\u00b7wer", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eh' als es kan bestehn;", "tokens": ["Eh'", "als", "es", "kan", "be\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Krancker, will er recht genesen,", "tokens": ["Ein", "Kran\u00b7cker", ",", "will", "er", "recht", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wird vber den Artzney-Getr\u00e4ncken", "tokens": ["Wird", "vber", "den", "Artz\u00b7ney\u00b7Ge\u00b7tr\u00e4n\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sich nicht viel kr\u00e4ncken.", "tokens": ["Sich", "nicht", "viel", "kr\u00e4n\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wer hat den Sieges Krantz getragen,", "tokens": ["Wer", "hat", "den", "Sie\u00b7ges", "Krantz", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der nicht vom Vbermuth", "tokens": ["Der", "nicht", "vom", "Vber\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der Feind' in Schwei\u00df vnd Bluth", "tokens": ["Der", "Feind'", "in", "Schwei\u00df", "vnd", "Bluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vnd Kummer hat gewust zu sagen?", "tokens": ["Vnd", "Kum\u00b7mer", "hat", "ge\u00b7wust", "zu", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer wird das Ziehl im Wetterennen", "tokens": ["Wer", "wird", "das", "Ziehl", "im", "Wet\u00b7te\u00b7ren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ohn Staub erkennen?", "tokens": ["Ohn", "Staub", "er\u00b7ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ist noch so viel vns wiederfahren,", "tokens": ["Ist", "noch", "so", "viel", "vns", "wie\u00b7der\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So ist doch dieses Leid", "tokens": ["So", "ist", "doch", "die\u00b7ses", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht wehrt der Herrligkeit", "tokens": ["Nicht", "wehrt", "der", "Herr\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Gott an vns wil offenbahren,", "tokens": ["Die", "Gott", "an", "vns", "wil", "of\u00b7fen\u00b7bah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weil sie nach diesen kurtzen Zehren", "tokens": ["Weil", "sie", "nach", "die\u00b7sen", "kurt\u00b7zen", "Zeh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sol ewig wehren.", "tokens": ["Sol", "e\u00b7wig", "weh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Kein Christ soll jhm die Rechnung machen,", "tokens": ["Kein", "Christ", "soll", "jhm", "die", "Rech\u00b7nung", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df lauter Sonnenschein", "tokens": ["Da\u00df", "lau\u00b7ter", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hie vmb jhn werde seyn,", "tokens": ["Hie", "vmb", "jhn", "wer\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vnd er nur schertzen m\u00fcss' vnd lachen;", "tokens": ["Vnd", "er", "nur", "schert\u00b7zen", "m\u00fcss'", "vnd", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVINF", "VMFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir haben keinen Rosen-Garten", "tokens": ["Wir", "ha\u00b7ben", "kei\u00b7nen", "Ro\u00b7sen\u00b7Gar\u00b7ten"], "