{"dta.poem.21315": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eda\u00df ich keine reine Jungfer bin,", "tokens": ["\u201e", "da\u00df", "ich", "kei\u00b7ne", "rei\u00b7ne", "Jung\u00b7fer", "bin", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PIAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die Franzosen wissen es besser,", "tokens": ["Die", "Fran\u00b7zo\u00b7sen", "wis\u00b7sen", "es", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sie haben mit meinem Wasser so oft", "tokens": ["Sie", "ha\u00b7ben", "mit", "mei\u00b7nem", "Was\u00b7ser", "so", "oft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vermischt ihr Siegergew\u00e4sser.", "tokens": ["Ver\u00b7mischt", "ihr", "Sie\u00b7ger\u00b7ge\u00b7w\u00e4s\u00b7ser", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201edas dumme Lied und der dumme Kerl!", "tokens": ["\u201e", "das", "dum\u00b7me", "Lied", "und", "der", "dum\u00b7me", "Kerl", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er hat mich schm\u00e4hlich blamiret,", "tokens": ["Er", "hat", "mich", "schm\u00e4h\u00b7lich", "bla\u00b7mi\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewisserma\u00dfen hat er mich auch", "tokens": ["Ge\u00b7wis\u00b7ser\u00b7ma\u00b7\u00dfen", "hat", "er", "mich", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Politisch kompromittiret.", "tokens": ["Po\u00b7li\u00b7tisch", "kom\u00b7pro\u00b7mit\u00b7ti\u00b7ret", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "\u201edenn kehren jetzt die Franzosen zur\u00fcck,", "tokens": ["\u201e", "denn", "keh\u00b7ren", "jetzt", "die", "Fran\u00b7zo\u00b7sen", "zu\u00b7r\u00fcck", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So mu\u00df ich vor ihnen err\u00f6then,", "tokens": ["So", "mu\u00df", "ich", "vor", "ih\u00b7nen", "er\u00b7r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich, der um ihre R\u00fcckkehr so oft", "tokens": ["Ich", ",", "der", "um", "ih\u00b7re", "R\u00fcck\u00b7kehr", "so", "oft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit Thr\u00e4nen zum Himmel gebeten.", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "zum", "Him\u00b7mel", "ge\u00b7be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "\u201eich habe sie immer so lieb gehabt,", "tokens": ["\u201e", "ich", "ha\u00b7be", "sie", "im\u00b7mer", "so", "lieb", "ge\u00b7habt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die lieben kleinen Franz\u00f6schen \u2013", "tokens": ["Die", "lie\u00b7ben", "klei\u00b7nen", "Fran\u00b7z\u00f6\u00b7schen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Singen und springen sie noch wie sonst?", "tokens": ["Sin\u00b7gen", "und", "sprin\u00b7gen", "sie", "noch", "wie", "sonst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADV", "KOKOM", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Tragen noch wei\u00dfe H\u00f6schen?", "tokens": ["Tra\u00b7gen", "noch", "wei\u00b7\u00dfe", "H\u00f6\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}