{"dta.poem.15654": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wozu sind all die Stern' am Himmel nur gemacht?", "tokens": ["Wo\u00b7zu", "sind", "all", "die", "Stern'", "am", "Him\u00b7mel", "nur", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIAT", "ART", "NN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit goldnem Flitter wol zu schm\u00fccken unsre Nacht?", "tokens": ["Mit", "gold\u00b7nem", "Flit\u00b7ter", "wol", "zu", "schm\u00fc\u00b7cken", "uns\u00b7re", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dazu sind sie gemacht, doch nur dem Kindersinn.", "tokens": ["Da\u00b7zu", "sind", "sie", "ge\u00b7macht", ",", "doch", "nur", "dem", "Kin\u00b7der\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVPP", "$,", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was hat des Manns Verstand von ihnen f\u00fcr Gewinn?", "tokens": ["Was", "hat", "des", "Manns", "Ver\u00b7stand", "von", "ih\u00b7nen", "f\u00fcr", "Ge\u00b7winn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er h\u00e4tte, scheints, genug an Sonn' und Mond allein,", "tokens": ["Er", "h\u00e4t\u00b7te", ",", "scheints", ",", "ge\u00b7nug", "an", "Sonn'", "und", "Mond", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "$,", "ADV", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Licht im Erdenhaus, und brauchte nicht den Schein.", "tokens": ["Zum", "Licht", "im", "Er\u00b7den\u00b7haus", ",", "und", "brauch\u00b7te", "nicht", "den", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Statt m\u00fc\u00dfig aufzuschaun in zahllos fremde Welten,", "tokens": ["Statt", "m\u00fc\u00b7\u00dfig", "auf\u00b7zu\u00b7schaun", "in", "zahl\u00b7los", "frem\u00b7de", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVIZU", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00e4r' es nicht besser da\u00df die eigne wir bestellten?", "tokens": ["W\u00e4r'", "es", "nicht", "bes\u00b7ser", "da\u00df", "die", "eig\u00b7ne", "wir", "be\u00b7stell\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "KOUS", "ART", "ADJA", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Doch grade da\u00df bestellt die eigne richtig sei,", "tokens": ["Doch", "gra\u00b7de", "da\u00df", "be\u00b7stellt", "die", "eig\u00b7ne", "rich\u00b7tig", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "VVFIN", "ART", "ADJA", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In jene fremden tr\u00e4gt dazu der Ausblick bei.", "tokens": ["In", "je\u00b7ne", "frem\u00b7den", "tr\u00e4gt", "da\u00b7zu", "der", "Aus\u00b7blick", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "VVFIN", "PAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du kannst Mondsonnenlauf, der ewig wechselnd geht,", "tokens": ["Du", "kannst", "Mond\u00b7son\u00b7nen\u00b7lauf", ",", "der", "e\u00b7wig", "wech\u00b7selnd", "geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$,", "PRELS", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An Etwas messen nur, das unbeweglich steht.", "tokens": ["An", "Et\u00b7was", "mes\u00b7sen", "nur", ",", "das", "un\u00b7be\u00b7weg\u00b7lich", "steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ADV", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Als Wendepunkte stehn dazu die Himmelsterne,", "tokens": ["Als", "Wen\u00b7de\u00b7punk\u00b7te", "stehn", "da\u00b7zu", "die", "Him\u00b7mels\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df man daran den Gang des Erdhaushaltes lerne.", "tokens": ["Da\u00df", "man", "da\u00b7ran", "den", "Gang", "des", "Erd\u00b7haus\u00b7hal\u00b7tes", "ler\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Aufs Gro\u00dfe mu\u00df man sehn, um sich zu freun am Kleinen;", "tokens": ["Aufs", "Gro\u00b7\u00dfe", "mu\u00df", "man", "sehn", ",", "um", "sich", "zu", "freun", "am", "Klei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VMFIN", "PIS", "VVINF", "$,", "KOUI", "PRF", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Einzelne wird nur erkannt am Allgemeinen.", "tokens": ["Das", "Ein\u00b7zel\u00b7ne", "wird", "nur", "er\u00b7kannt", "am", "All\u00b7ge\u00b7mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "VVPP", "APPRART", "ADJA", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}}}}