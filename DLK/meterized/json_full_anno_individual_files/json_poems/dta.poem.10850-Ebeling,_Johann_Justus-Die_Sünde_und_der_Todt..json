{"dta.poem.10850": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n S\u00fcnde und der Todt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Uebel folgt alzeit dem andern auf dem\nFu\u00df;", "tokens": ["Ein", "Ue\u00b7bel", "folgt", "al\u00b7zeit", "dem", "an\u00b7dern", "auf", "dem", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil eins nat\u00fcrlich stets, aus andern", "tokens": ["Weil", "eins", "na\u00b7t\u00fcr\u00b7lich", "stets", ",", "aus", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADV", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das gr\u00f6\u00dfte Uebel ist, ohn Zweiffel ja", "tokens": ["Das", "gr\u00f6\u00df\u00b7te", "Ue\u00b7bel", "ist", ",", "ohn", "Zweif\u00b7fel", "ja"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "KOUI", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Man siehet dieses klar an ihren b\u00f6sen Kin-", "tokens": ["Man", "sie\u00b7het", "die\u00b7ses", "klar", "an", "ih\u00b7ren", "b\u00f6\u00b7sen", "Kin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PDAT", "ADJD", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was sie gebohren hat; Und dieses heist der Todt,", "tokens": ["Was", "sie", "ge\u00b7boh\u00b7ren", "hat", ";", "Und", "die\u00b7ses", "heist", "der", "Todt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$.", "KON", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Wort das in sich fast die allergr\u00f6ste Noth.", "tokens": ["Ein", "Wort", "das", "in", "sich", "fast", "die", "al\u00b7ler\u00b7gr\u00f6s\u00b7te", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die S\u00fcnde und der Todt sind allemahl Gesel-", "tokens": ["Die", "S\u00fcn\u00b7de", "und", "der", "Todt", "sind", "al\u00b7le\u00b7mahl", "Ge\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "O m\u00f6chte doch der Mensch sich dieses stets vorstel-", "tokens": ["O", "m\u00f6ch\u00b7te", "doch", "der", "Mensch", "sich", "die\u00b7ses", "stets", "vors\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADV", "ART", "NN", "PRF", "PDAT", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wir fliehen all den Todt; und wer ihn wil ent-", "tokens": ["Wir", "flie\u00b7hen", "all", "den", "Todt", ";", "und", "wer", "ihn", "wil", "ent"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ART", "NN", "$.", "KON", "PWS", "PPER", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der mu\u00df sich durch die Krafft des Geistes auch be-", "tokens": ["Der", "mu\u00df", "sich", "durch", "die", "Krafft", "des", "Geis\u00b7tes", "auch", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der S\u00fcnde zu entgehn. Wer will die Folgen has-", "tokens": ["Der", "S\u00fcn\u00b7de", "zu", "ent\u00b7gehn", ".", "Wer", "will", "die", "Fol\u00b7gen", "has"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$.", "PWS", "VMFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der mu\u00df die Ursach auch daraus sie kommen, las-", "tokens": ["Der", "mu\u00df", "die", "Ur\u00b7sach", "auch", "da\u00b7raus", "sie", "kom\u00b7men", ",", "las"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADV", "PAV", "PPER", "VVINF", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}