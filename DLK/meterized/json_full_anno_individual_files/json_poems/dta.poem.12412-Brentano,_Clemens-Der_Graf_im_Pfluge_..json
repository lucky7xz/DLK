{"dta.poem.12412": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Graf im Pfluge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich verk\u00fcnd euch neue Mehre,               ", "tokens": ["Ich", "ver\u00b7k\u00fcnd", "euch", "neu\u00b7e", "Meh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halt Frieden bei der Kann.", "tokens": ["Halt", "Frie\u00b7den", "bei", "der", "Kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Rom da sa\u00df ein Herre,", "tokens": ["Zu", "Rom", "da", "sa\u00df", "ein", "Her\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Graf gar wohlgethan,", "tokens": ["Ein", "Graf", "gar", "wohl\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der war von reicher Habe,", "tokens": ["Der", "war", "von", "rei\u00b7cher", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War mild und tugendhaft,", "tokens": ["War", "mild", "und", "tu\u00b7gend\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wollt ziehen zum heiligen Grabe,", "tokens": ["Wollt", "zie\u00b7hen", "zum", "hei\u00b7li\u00b7gen", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Nach Ehren und Ritterschaft.", "tokens": ["Nach", "Eh\u00b7ren", "und", "Rit\u00b7ter\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sein Frau erschrack der Mehre,", "tokens": ["Sein", "Frau", "er\u00b7schrack", "der", "Meh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie blickt den Grafen an:", "tokens": ["Sie", "blickt", "den", "Gra\u00b7fen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201egnad mir edler Herre,", "tokens": ["\u201e", "gnad", "mir", "ed\u00b7ler", "Her\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "\u201edazu mein ehelich Mann,", "tokens": ["\u201e", "da\u00b7zu", "mein", "e\u00b7he\u00b7lich", "Mann", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "\u201emich nimmt Wunder sehre,", "tokens": ["\u201e", "mich", "nimmt", "Wun\u00b7der", "seh\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "\u201ewas euch die Ritterschaft soll,", "tokens": ["\u201e", "was", "euch", "die", "Rit\u00b7ter\u00b7schaft", "soll", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "ART", "NN", "VMFIN", "$,"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "\u201ehabt ihr doch Gut und Ehre,", "tokens": ["\u201e", "habt", "ihr", "doch", "Gut", "und", "Eh\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u201eund alles, was ihr wollt.\u201c", "tokens": ["\u201e", "und", "al\u00b7les", ",", "was", "ihr", "wollt", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PIS", "$,", "PWS", "PPER", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er sprach zu seiner Frauen:", "tokens": ["Er", "sprach", "zu", "sei\u00b7ner", "Frau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201enun spar dich Gott gesund,", "tokens": ["\u201e", "nun", "spar", "dich", "Gott", "ge\u00b7sund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201ealles will ich dir vertrauen,", "tokens": ["\u201e", "al\u00b7les", "will", "ich", "dir", "ver\u00b7trau\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201eallhie zu dieser Stund.\u201c", "tokens": ["\u201e", "all\u00b7hie", "zu", "die\u00b7ser", "Stund", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Also schied er von dannen,", "tokens": ["Al\u00b7so", "schied", "er", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Der edle Graf so hart,", "tokens": ["Der", "ed\u00b7le", "Graf", "so", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gro\u00df Kummer stand ihm zu handen,", "tokens": ["Gro\u00df", "Kum\u00b7mer", "stand", "ihm", "zu", "han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Eins K\u00f6nigs Gefangner er ward.", "tokens": ["Eins", "K\u00f6\u00b7nigs", "Ge\u00b7fang\u00b7ner", "er", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VAFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Er mocht ihm nicht entfliehen,", "tokens": ["Er", "mocht", "ihm", "nicht", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das war sein gr\u00f6ste Klag,", "tokens": ["Das", "war", "sein", "gr\u00f6s\u00b7te", "Klag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Pflug da must er ziehen,", "tokens": ["Im", "Pflug", "da", "must", "er", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel l\u00e4nger denn Jahr und Tag,", "tokens": ["Viel", "l\u00e4n\u00b7ger", "denn", "Jahr", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Erlitt viel Hunger, und schwere", "tokens": ["Er\u00b7litt", "viel", "Hun\u00b7ger", ",", "und", "schwe\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$,", "KON", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ward ihm die gro\u00dfe Bu\u00df.", "tokens": ["Ward", "ihm", "die", "gro\u00b7\u00dfe", "Bu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der K\u00f6nig reit vor ihm here,", "tokens": ["Der", "K\u00f6\u00b7nig", "reit", "vor", "ihm", "he\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Der Graf fiel ihm zu Fu\u00df.", "tokens": ["Der", "Graf", "fiel", "ihm", "zu", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der K\u00f6nig sprach:\u201eMit nichten\u201c", "tokens": ["Der", "K\u00f6\u00b7nig", "sprach", ":", "\u201e", "Mit", "nich\u00b7ten", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "APPR", "PIS", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach noch dem Grafen Hohn:", "tokens": ["Sprach", "noch", "dem", "Gra\u00b7fen", "Hohn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201ees hilft dir doch kein Bitten,", "tokens": ["\u201e", "es", "hilft", "dir", "doch", "kein", "Bit\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eschw\u00f6r ich bey meiner Kron;", "tokens": ["\u201e", "schw\u00f6r", "ich", "bey", "mei\u00b7ner", "Kron", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "\u201eund fielest du alle Morgen,", "tokens": ["\u201e", "und", "fie\u00b7lest", "du", "al\u00b7le", "Mor\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "\u201et\u00e4glich auf deine Knie,", "tokens": ["\u201e", "t\u00e4g\u00b7lich", "auf", "dei\u00b7ne", "Knie", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "\u201edu m\u00f6chtest nicht ledig werden,", "tokens": ["\u201e", "du", "m\u00f6ch\u00b7test", "nicht", "le\u00b7dig", "wer\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "\u201edenn deine Frau w\u00e4r hie.\u201c", "tokens": ["\u201e", "denn", "dei\u00b7ne", "Frau", "w\u00e4r", "hie", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Graf erschrack der Mehre,", "tokens": ["Der", "Graf", "er\u00b7schrack", "der", "Meh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gro\u00df Leid er ihm gedacht:", "tokens": ["Gro\u00df", "Leid", "er", "ihm", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201ebring ich mein Frauen here,", "tokens": ["\u201e", "bring", "ich", "mein", "Frau\u00b7en", "he\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eso wird sie mir geschw\u00e4cht,", "tokens": ["\u201e", "so", "wird", "sie", "mir", "ge\u00b7schw\u00e4cht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eund soll ich hier noch bleiben,", "tokens": ["\u201e", "und", "soll", "ich", "hier", "noch", "blei\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201eso gilt es meinen Leib,", "tokens": ["\u201e", "so", "gilt", "es", "mei\u00b7nen", "Leib", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u201edarauf so will ich schreiben,", "tokens": ["\u201e", "da\u00b7rauf", "so", "will", "ich", "schrei\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u201ewill schicken nach meinem Weib.\u201c", "tokens": ["\u201e", "will", "schi\u00b7cken", "nach", "mei\u00b7nem", "Weib", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "VVINF", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Einer der war an dem Hofe,", "tokens": ["Ei\u00b7ner", "der", "war", "an", "dem", "Ho\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der hat die Gefangen in Hut,", "tokens": ["Der", "hat", "die", "Ge\u00b7fan\u00b7gen", "in", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Dem \u00fcbertrugs der Grafe,", "tokens": ["Dem", "\u00fc\u00b7bert\u00b7rugs", "der", "Gra\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verhie\u00df ihm Hab und Gut,", "tokens": ["Ver\u00b7hie\u00df", "ihm", "Hab", "und", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Brief schreibt der behende,", "tokens": ["Ein", "Brief", "schreibt", "der", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Macht seiner Frauen klar,", "tokens": ["Macht", "sei\u00b7ner", "Frau\u00b7en", "klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein Kummer m\u00f6cht niemand wenden,", "tokens": ["Sein", "Kum\u00b7mer", "m\u00f6cht", "nie\u00b7mand", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Denn sie k\u00e4m selber dar.", "tokens": ["Denn", "sie", "k\u00e4m", "sel\u00b7ber", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Der Bote zog ohne Trauern,", "tokens": ["Der", "Bo\u00b7te", "zog", "oh\u00b7ne", "Trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wohl \u00fcber das wilde Meer,", "tokens": ["Wohl", "\u00fc\u00b7ber", "das", "wil\u00b7de", "Meer", ","], "token_info": ["word", "word", "word", "word", "w