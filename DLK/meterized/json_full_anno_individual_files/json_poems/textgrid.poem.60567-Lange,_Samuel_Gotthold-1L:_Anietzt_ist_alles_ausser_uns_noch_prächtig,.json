{"textgrid.poem.60567": {"metadata": {"author": {"name": "Lange, Samuel Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Anietzt ist alles ausser uns noch pr\u00e4chtig,", "genre": "verse", "period": "N.A.", "pub_year": 1746, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Anietzt ist alles ausser uns noch pr\u00e4chtig,", "tokens": ["An\u00b7ietzt", "ist", "al\u00b7les", "aus\u00b7ser", "uns", "noch", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein giftger Dampf aus uns benebelt alles,", "tokens": ["Ein", "gift\u00b7ger", "Dampf", "aus", "uns", "be\u00b7ne\u00b7belt", "al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Es bleibt der Mensch in seinem Eigenthume", "tokens": ["Es", "bleibt", "der", "Mensch", "in", "sei\u00b7nem", "Ei\u00b7gen\u00b7thu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fl\u00fcchtig und fremde.", "tokens": ["Fl\u00fcch\u00b7tig", "und", "frem\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Dich, Gottheit, ehr ich mit ehrf\u00fcrchtgem Schweigen,", "tokens": ["Dich", ",", "Got\u00b7theit", ",", "ehr", "ich", "mit", "ehr\u00b7f\u00fcrcht\u00b7gem", "Schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du die der Rache feuerrothes Blitzen", "tokens": ["Du", "die", "der", "Ra\u00b7che", "feu\u00b7er\u00b7ro\u00b7thes", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit eigenem unschuldgen Blut gel\u00f6schet;", "tokens": ["Mit", "ei\u00b7ge\u00b7nem", "un\u00b7schuld\u00b7gen", "Blut", "ge\u00b7l\u00f6\u00b7schet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die\u00df sing ich einst dort.", "tokens": ["Die\u00df", "sing", "ich", "einst", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Hier bin ich fremd, da ist mein Geist zu Hause;", "tokens": ["Hier", "bin", "ich", "fremd", ",", "da", "ist", "mein", "Geist", "zu", "Hau\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch f\u00fchrt die Tugend auf verwehrten Wegen", "tokens": ["Doch", "f\u00fchrt", "die", "Tu\u00b7gend", "auf", "ver\u00b7wehr\u00b7ten", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich oft ins Heiligthum, da hol ich Labsal", "tokens": ["Mich", "oft", "ins", "Hei\u00b7lig\u00b7thum", ",", "da", "hol", "ich", "Lab\u00b7sal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr den irdschen Gram.", "tokens": ["F\u00fcr", "den", "ird\u00b7schen", "Gram", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Nebst dem lehrt mich der Dichtkunst heilge Leyer,", "tokens": ["Nebst", "dem", "lehrt", "mich", "der", "Dicht\u00b7kunst", "heil\u00b7ge", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit H\u00fclf der Wahrheit tief in meinem Hertzen", "tokens": ["Mit", "H\u00fclf", "der", "Wahr\u00b7heit", "tief", "in", "mei\u00b7nem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die st\u00f6rrsche Lasterbrut zu fesseln, und der Tugend", "tokens": ["Die", "st\u00f6rrsc\u00b7he", "Las\u00b7ter\u00b7brut", "zu", "fes\u00b7seln", ",", "und", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "W\u00fcrdige Lieder.", "tokens": ["W\u00fcr\u00b7di\u00b7ge", "Lie\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Und ausser mir empfind ich Doris Liebe,", "tokens": ["Und", "aus\u00b7ser", "mir", "emp\u00b7find", "ich", "Do\u00b7ris", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ihren Arm um meinen Nacken schlinget,", "tokens": ["Die", "ih\u00b7ren", "Arm", "um", "mei\u00b7nen", "Na\u00b7cken", "schlin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich k\u00fcssend fa\u00dft, in erster Unschuld lachet,", "tokens": ["Mich", "k\u00fcs\u00b7send", "fa\u00dft", ",", "in", "ers\u00b7ter", "Un\u00b7schuld", "la\u00b7chet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie die heitre Luft.", "tokens": ["Wie", "die", "heit\u00b7re", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Du Thirsis, oder besser, andrer Damon,", "tokens": ["Du", "Thir\u00b7sis", ",", "o\u00b7der", "bes\u00b7ser", ",", "an\u00b7drer", "Da\u00b7mon", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "KON", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein tr\u00fcgendes Geschick hat uns vereinet.", "tokens": ["Kein", "tr\u00fc\u00b7gen\u00b7des", "Ge\u00b7schick", "hat", "uns", "ver\u00b7ei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Blick von Doris und von dir ein Schreiben", "tokens": ["Ein", "Blick", "von", "Do\u00b7ris", "und", "von", "dir", "ein", "Schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "KON", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Macht mich entz\u00fccket.", "tokens": ["Macht", "mich", "ent\u00b7z\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Umringt vom tollen Schwarm des dummen P\u00f6bels,", "tokens": ["Um\u00b7ringt", "vom", "tol\u00b7len", "Schwarm", "des", "dum\u00b7men", "P\u00f6\u00b7bels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und vom gedrehten Garn des falschen Neides", "tokens": ["Und", "vom", "ge\u00b7dreh\u00b7ten", "Garn", "des", "fal\u00b7schen", "Nei\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umstricket, irrt mit Sehnsucht meine Seele,", "tokens": ["Um\u00b7stri\u00b7cket", ",", "irrt", "mit", "Sehn\u00b7sucht", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis sie an dich denckt.", "tokens": ["Bis", "sie", "an", "dich", "denckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "O welch ein heilger Taumel! Welche Gegend,", "tokens": ["O", "welch", "ein", "heil\u00b7ger", "Tau\u00b7mel", "!", "Wel\u00b7che", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "ADJA", "NN", "$.", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Welch angenehmes griechsches Thal! Die Musen", "tokens": ["Welch", "an\u00b7ge\u00b7neh\u00b7mes", "griech\u00b7sches", "Thal", "!", "Die", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vermischen mit den Sch\u00e4fern heilge Reihen!", "tokens": ["Ver\u00b7mi\u00b7schen", "mit", "den", "Sch\u00e4\u00b7fern", "heil\u00b7ge", "Rei\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was schallet dort her?", "tokens": ["Was", "schal\u00b7let", "dort", "her", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Mit Ph\u00f6bus Glantz geziert, trittst du voll Ruhe", "tokens": ["Mit", "Ph\u00f6\u00b7bus", "Glantz", "ge\u00b7ziert", ",", "trittst", "du", "voll", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tr\u00e4gst in deiner Hand die lesbsche Leyer;", "tokens": ["Und", "tr\u00e4gst", "in", "dei\u00b7ner", "Hand", "die", "lesb\u00b7sche", "Le\u00b7yer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Das scheuche Wild vergi\u00dft die Furcht, und h\u00f6ret", "tokens": ["Das", "scheu\u00b7che", "Wild", "ver\u00b7gi\u00dft", "die", "Furcht", ",", "und", "h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den g\u00f6ttlichen Klang.", "tokens": ["Den", "g\u00f6tt\u00b7li\u00b7chen", "Klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Ich h\u00f6re lauschend auf der Lieder Innhalt,", "tokens": ["Ich", "h\u00f6\u00b7re", "lau\u00b7schend", "auf", "der", "Lie\u00b7der", "Inn\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Z\u00e4rtlichkeit r\u00fchrt meine Brust. Ich f\u00fchle", "tokens": ["Die", "Z\u00e4rt\u00b7lich\u00b7keit", "r\u00fchrt", "mei\u00b7ne", "Brust", ".", "Ich", "f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mich selbst. Die Sehnsucht zittert in den Saiten.", "tokens": ["Mich", "selbst", ".", "Die", "Sehn\u00b7sucht", "zit\u00b7tert", "in", "den", "Sai\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$.", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du denckest an mich!", "tokens": ["Du", "den\u00b7ckest", "an", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Was hinderts noch, da\u00df dich mein Arm umschlinge?", "tokens": ["Was", "hin\u00b7derts", "noch", ",", "da\u00df", "dich", "mein", "Arm", "um\u00b7schlin\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O still, mein Thirsis, h\u00f6re auf das Rollen,", "tokens": ["O", "still", ",", "mein", "Thir\u00b7sis", ",", "h\u00f6\u00b7re", "auf", "das", "Rol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Wagen n\u00e4hert sich, der mich und Doris", "tokens": ["Der", "Wa\u00b7gen", "n\u00e4\u00b7hert", "sich", ",", "der", "mich", "und", "Do\u00b7ris"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "PRELS", "PPER", "KON", "NE"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dir \u00fcberbringet.", "tokens": ["Dir", "\u00fc\u00b7berb\u00b7rin\u00b7get", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Anietzt ist alles ausser uns noch pr\u00e4chtig,", "tokens": ["An\u00b7ietzt", "ist", "al\u00b7les", "aus\u00b7ser", "uns", "noch", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein giftger Dampf aus uns benebelt alles,", "tokens": ["Ein", "gift\u00b7ger", "Dampf", "aus", "uns", "be\u00b7ne\u00b7belt", "al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Es bleibt der Mensch in seinem Eigenthume", "tokens": ["Es", "bleibt", "der", "Mensch", "in", "sei\u00b7nem", "Ei\u00b7gen\u00b7thu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fl\u00fcchtig und fremde.", "tokens": ["Fl\u00fcch\u00b7tig", "und", "frem\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Dich, Gottheit, ehr ich mit ehrf\u00fcrchtgem Schweigen,", "tokens": ["Dich", ",", "Got\u00b7theit", ",", "ehr", "ich", "mit", "ehr\u00b7f\u00fcrcht\u00b7gem", "Schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du die der Rache feuerrothes Blitzen", "tokens": ["Du", "die", "der", "Ra\u00b7che", "feu\u00b7er