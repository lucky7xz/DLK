{"dta.poem.15598": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du m\u00f6chtest seyn wie der und jener, doch dabei", "tokens": ["Du", "m\u00f6ch\u00b7test", "seyn", "wie", "der", "und", "je\u00b7ner", ",", "doch", "da\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "VAINF", "KOKOM", "ART", "KON", "PDS", "$,", "ADV", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch bleiben, der du bist, alsob das m\u00f6glich sei.", "tokens": ["Auch", "blei\u00b7ben", ",", "der", "du", "bist", ",", "al\u00b7sob", "das", "m\u00f6g\u00b7lich", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PRELS", "PPER", "VAFIN", "$,", "KOUS", "ART", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So m\u00f6chtest du im Herbst des Fr\u00fchlings Bl\u00fcten haben,", "tokens": ["So", "m\u00f6ch\u00b7test", "du", "im", "Herbst", "des", "Fr\u00fch\u00b7lings", "Bl\u00fc\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch drum der Fr\u00fcchte nicht entbehren, die dich laben.", "tokens": ["Doch", "drum", "der", "Fr\u00fcch\u00b7te", "nicht", "ent\u00b7beh\u00b7ren", ",", "die", "dich", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "PTKNEG", "VVINF", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dazu sind eben W\u00fcnsch' und Tr\u00e4ume dir verliehn,", "tokens": ["Da\u00b7zu", "sind", "e\u00b7ben", "W\u00fcn\u00b7sch'", "und", "Tr\u00e4u\u00b7me", "dir", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Um alles, was dir fehlt, in deinen Kreis zu ziehn.", "tokens": ["Um", "al\u00b7les", ",", "was", "dir", "fehlt", ",", "in", "dei\u00b7nen", "Kreis", "zu", "ziehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}