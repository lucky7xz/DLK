{"textgrid.poem.41397": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein vorgeladner Abt fragt einen klugen Alten:", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein vorgeladner Abt fragt einen klugen Alten:", "tokens": ["Ein", "vor\u00b7ge\u00b7lad\u00b7ner", "Abt", "fragt", "ei\u00b7nen", "klu\u00b7gen", "Al\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr kennt das ganze Recht; mich r\u00fcgt ein B\u00f6sewicht;", "tokens": ["Ihr", "kennt", "das", "gan\u00b7ze", "Recht", ";", "mich", "r\u00fcgt", "ein", "B\u00f6\u00b7se\u00b7wicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Schriften bring' ich mit; gebt mir doch Unterricht:", "tokens": ["Die", "Schrif\u00b7ten", "bring'", "ich", "mit", ";", "gebt", "mir", "doch", "Un\u00b7ter\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie soll ich mich dabei verhalten?", "tokens": ["Wie", "soll", "ich", "mich", "da\u00b7bei", "ver\u00b7hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.