{"textgrid.poem.44059": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mag es doch die Welt verdrie\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mag es doch die Welt verdrie\u00dfen,", "tokens": ["Mag", "es", "doch", "die", "Welt", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn mein ungebundner Geist", "tokens": ["Wenn", "mein", "un\u00b7ge\u00b7bund\u00b7ner", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich mit unschuldsvollen K\u00fc\u00dfen", "tokens": ["Sich", "mit", "un\u00b7schulds\u00b7vol\u00b7len", "K\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bey galanten M\u00e4gdgen speist;", "tokens": ["Bey", "ga\u00b7lan\u00b7ten", "M\u00e4gd\u00b7gen", "speist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn dergleichen sch\u00f6ne S\u00fcnden,", "tokens": ["Denn", "derg\u00b7lei\u00b7chen", "sch\u00f6\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die der P\u00f6bel hoch verflucht,", "tokens": ["Die", "der", "P\u00f6\u00b7bel", "hoch", "ver\u00b7flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Werden da wohl Ablas finden,", "tokens": ["Wer\u00b7den", "da", "wohl", "Ab\u00b7las", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo die Treu ein Urtheil sucht.", "tokens": ["Wo", "die", "Treu", "ein", "Ur\u00b7theil", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Freylich weis ich, da\u00df viel Leuten", "tokens": ["Frey\u00b7lich", "weis", "ich", ",", "da\u00df", "viel", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "PPER", "$,", "KOUS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solche Mode nicht gef\u00e4llt,", "tokens": ["Sol\u00b7che", "Mo\u00b7de", "nicht", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil man doch auf allen Seiten", "tokens": ["Weil", "man", "doch", "auf", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch der Unschuld Neze stellt;", "tokens": ["Auch", "der", "Un\u00b7schuld", "Ne\u00b7ze", "stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unterde\u00dfen wird mein Herze", "tokens": ["Un\u00b7ter\u00b7de\u00b7\u00dfen", "wird", "mein", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch die Thorheit nicht begehn,", "tokens": ["Doch", "die", "Thor\u00b7heit", "nicht", "be\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von dem unschuldsvollen Scherze", "tokens": ["Von", "dem", "un\u00b7schulds\u00b7vol\u00b7len", "Scher\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Treuer Seelen abzustehn.", "tokens": ["Treu\u00b7er", "See\u00b7len", "ab\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist doch noch kein Mensch gebohren,", "tokens": ["Ist", "doch", "noch", "kein", "Mensch", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der es allen recht gemacht;", "tokens": ["Der", "es", "al\u00b7len", "recht", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werd ich nur von Blind- und Thoren", "tokens": ["Werd", "ich", "nur", "von", "Blin\u00b7d", "und", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "TRUNC", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und der Misgunst ausgelacht,", "tokens": ["Und", "der", "Mis\u00b7gunst", "aus\u00b7ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O so schlag ich alle Grillen", "tokens": ["O", "so", "schlag", "ich", "al\u00b7le", "Gril\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihrer L\u00e4strung in den Wind,", "tokens": ["Ih\u00b7rer", "L\u00e4st\u00b7rung", "in", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn mein Geist nur stets im Stillen", "tokens": ["Wenn", "mein", "Geist", "nur", "stets", "im", "Stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seine Selbstvergn\u00fcgung findt.", "tokens": ["Sei\u00b7ne", "Selbst\u00b7ver\u00b7gn\u00fc\u00b7gung", "findt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mag sich doch ein jeder wehlen,", "tokens": ["Mag", "sich", "doch", "ein", "je\u00b7der", "weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ihm Aug und Herz entz\u00fcckt!", "tokens": ["Was", "ihm", "Aug", "und", "Herz", "ent\u00b7z\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird er trefen oder fehlen,", "tokens": ["Wird", "er", "tre\u00b7fen", "o\u00b7der", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd ich dadurch nicht gedr\u00fcckt.", "tokens": ["Werd", "ich", "da\u00b7durch", "nicht", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum begehr ich auch mit Rechte,", "tokens": ["Drum", "be\u00b7gehr", "ich", "auch", "mit", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df man mir die Lust verg\u00f6nnt,", "tokens": ["Da\u00df", "man", "mir", "die", "Lust", "ver\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die das z\u00e4rtliche Geschlechte", "tokens": ["Die", "das", "z\u00e4rt\u00b7li\u00b7che", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vor ihr h\u00f6chstes Gut erkennt.", "tokens": ["Vor", "ihr", "h\u00f6chs\u00b7tes", "Gut", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Also sez ich mein Vergn\u00fcgen", "tokens": ["Al\u00b7so", "sez", "ich", "mein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Gram und Zweifel fort.", "tokens": ["Oh\u00b7ne", "Gram", "und", "Zwei\u00b7fel", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will es nun der Himmel f\u00fcgen,", "tokens": ["Will", "es", "nun", "der", "Him\u00b7mel", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Find ich auch den rechten Ort,", "tokens": ["Find", "ich", "auch", "den", "rech\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die Klugheit wie die Liebe", "tokens": ["Wo", "die", "Klug\u00b7heit", "wie", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir bereits zu schmeicheln scheint", "tokens": ["Mir", "be\u00b7reits", "zu", "schmei\u00b7cheln", "scheint"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und auf hofnungsvolle Triebe", "tokens": ["Und", "auf", "hof\u00b7nungs\u00b7vol\u00b7le", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich wohl nicht zu teuschen meint.", "tokens": ["Mich", "wohl", "nicht", "zu", "teu\u00b7schen", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Teuschen mich die holden Blicke", "tokens": ["Teu\u00b7schen", "mich", "die", "hol\u00b7den", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und des Mundes H\u00f6fligkeit,", "tokens": ["Und", "des", "Mun\u00b7des", "H\u00f6f\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Halt ich dies schon vor ein Gl\u00fccke,", "tokens": ["Halt", "ich", "dies", "schon", "vor", "ein", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn mich auch dein Scherz erfreut.", "tokens": ["Wenn", "mich", "auch", "dein", "Scherz", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weis ich doch nicht, wen ich nenne.", "tokens": ["Weis", "ich", "doch", "nicht", ",", "wen", "ich", "nen\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch genug, es ist ein Bild,", "tokens": ["Doch", "ge\u00b7nug", ",", "es", "ist", "ein", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das, so lang ich leb und brenne,", "tokens": ["Das", ",", "so", "lang", "ich", "leb", "und", "bren\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "ADJD", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir allein das Herze stillt.", "tokens": ["Mir", "al\u00b7lein", "das", "Her\u00b7ze", "stillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Mag es doch die Welt verdrie\u00dfen,", "tokens": ["Mag", "es", "doch", "die", "Welt", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn mein ungebundner Geist", "tokens": ["Wenn", "mein", "un\u00b7ge\u00b7bund\u00b7ner", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich mit unschuldsvollen K\u00fc\u00dfen", "tokens": ["Sich", "mit", "un\u00b7schulds\u00b7vol\u00b7len", "K\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bey galanten M\u00e4gdgen speist;", "tokens": ["Bey", "ga\u00b7lan\u00b7ten", "M\u00e4gd\u00b7gen", "speist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn dergleichen sch\u00f6ne S\u00fcnden,", "tokens": ["Denn", "derg\u00b7lei\u00b7chen", "sch\u00f6\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die der P\u00f6bel hoch verflucht,", "tokens": ["Die", "der", "P\u00f6\u00b7bel", "hoch", "ver\u00b7flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Werden da wohl Ablas finden,", "tokens": ["Wer\u00b7den", "da", "wohl", "Ab\u00b7las", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo die Treu ein Urtheil sucht.", "tokens": ["Wo", "die", "Treu", "ein", "Ur\u00b7theil", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Freylich weis ich, da\u00df viel Leuten", "tokens": ["Frey\u00b7lich", "weis", "ich", ",", "da\u00df", "viel", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "PPER", "$,", "KOUS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solche Mode nicht gef\u00e4llt,", "tokens": ["Sol\u00b7che", "Mo\u00b7de", "nicht", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil man doch auf allen Seiten", "tokens": ["Weil", "man", "doch", "auf", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch der Unschuld Neze stellt;", "tokens": ["Auch", "der", "Un\u00b7schuld", "Ne\u00b7ze", "stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unterde\u00dfen wird mein Herze", "tokens": ["Un\u00b7ter\u00b7de\u00b7\u00dfen", "wird", "mein", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch die Thorheit nicht begehn,", "tokens": ["Doch", "die", "Thor\u00b7heit", "nicht", "be\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von dem unschuldsvollen Scherze", "tokens": ["Von", "dem", "un\u00b7schulds\u00b7vol\u00b7len", "Scher\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Treuer Seelen abzustehn.", "tokens": ["Treu\u00b7er", "See\u00b7len", "ab\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ist doch noch kein Mensch gebohren,", "tokens": ["Ist", "doch", "noch", "kein", "Mensch", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der es allen recht gemacht;", "tokens": ["Der", "es", "al\u00b7len", "recht", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werd ich nur von Blind- und Thoren", "tokens": ["Werd", "ich", "nur", "von", "Blin\u00b7d", "und", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "TRUNC", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und der Misgunst ausgelacht,", "tokens": ["Und", "der", "Mis\u00b7gunst", "aus\u00b7ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O so schlag ich alle Grillen", "tokens": ["O", "so", "schlag", "ich", "al\u00b7le", "Gril\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihrer L\u00e4strung in den Wind,", "tokens": ["Ih\u00b7rer", "L\u00e4st\u00b7rung", "in", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn mein Geist nur stets im Stillen", "tokens": ["Wenn", "mein", "Geist", "nur", "stets", "im", "Stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seine Selbstvergn\u00fcgung findt.", "tokens": ["Sei\u00b7ne", "Selbst\u00b7ver\u00b7gn\u00fc\u00b7gung", "findt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Mag sich doch ein jeder wehlen,", "tokens": ["Mag", "sich", "doch", "ein", "je\u00b7der", "weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ihm Aug und Herz entz\u00fcckt!", "tokens": ["Was", "ihm", "Aug", "und", "Herz", "ent\u00b7z\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird er trefen oder fehlen,", "tokens": ["Wird", "er", "tre\u00b7fen", "o\u00b7der", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd ich dadurch nicht gedr\u00fcckt.", "tokens": ["Werd", "ich", "da\u00b7durch", "nicht", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum begehr ich auch mit Rechte,", "tokens": ["Drum", "be\u00b7gehr", "ich", "auch", "mit", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df man mir die Lust verg\u00f6nnt,", "tokens": ["Da\u00df", "man", "mir", "die", "Lust", "ver\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die das z\u00e4rtliche Geschlechte", "tokens": ["Die", "das", "z\u00e4rt\u00b7li\u00b7che", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vor ihr h\u00f6chstes Gut erkennt.", "tokens": ["Vor", "ihr", "h\u00f6chs\u00b7tes", "Gut", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Also sez ich mein Vergn\u00fcgen", "tokens": ["Al\u00b7so", "sez", "ich", "mein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Gram und Zweifel fort.", "tokens": ["Oh\u00b7ne", "Gram", "und", "Zwei\u00b7fel", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will es nun der Himmel f\u00fcgen,", "tokens": ["Will", "es", "nun", "der", "Him\u00b7mel", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Find ich auch den rechten Ort,", "tokens": ["Find", "ich", "auch", "den", "rech\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die Klugheit wie die Liebe", "tokens": ["Wo", "die", "Klug\u00b7heit", "wie", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir bereits zu schmeicheln scheint", "tokens": ["Mir", "be\u00b7reits", "zu", "schmei\u00b7cheln", "scheint"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und auf hofnungsvolle Triebe", "tokens": ["Und", "auf", "hof\u00b7nungs\u00b7vol\u00b7le", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich wohl nicht zu teuschen meint.", "tokens": ["Mich", "wohl", "nicht", "zu", "teu\u00b7schen", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Teuschen mich die holden Blicke", "tokens": ["Teu\u00b7schen", "mich", "die", "hol\u00b7den", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und des Mundes H\u00f6fligkeit,", "tokens": ["Und", "des", "Mun\u00b7des", "H\u00f6f\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Halt ich dies schon vor ein Gl\u00fccke,", "tokens": ["Halt", "ich", "dies", "schon", "vor", "ein", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn mich auch dein Scherz erfreut.", "tokens": ["Wenn", "mich", "auch", "dein", "Scherz", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "w