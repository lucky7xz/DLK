{"textgrid.poem.49746": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Bedenken", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selber liebe ich den Sport,", "tokens": ["Sel\u00b7ber", "lie\u00b7be", "ich", "den", "Sport", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die Frage qu\u00e4lt mich immer:", "tokens": ["Doch", "die", "Fra\u00b7ge", "qu\u00e4lt", "mich", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ist er passend, schickt er sich", "tokens": ["Ist", "er", "pas\u00b7send", ",", "schickt", "er", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr ein junges Frauenzimmer?", "tokens": ["F\u00fcr", "ein", "jun\u00b7ges", "Frau\u00b7en\u00b7zim\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fr\u00fcher war es allgemein", "tokens": ["Fr\u00fc\u00b7her", "war", "es", "all\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit Heftigkeit bestritten;", "tokens": ["Und", "mit", "Hef\u00b7tig\u00b7keit", "be\u00b7strit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Man verlangte sehr bestimmt", "tokens": ["Man", "ver\u00b7lang\u00b7te", "sehr", "be\u00b7stimmt"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00fcchtigkeit und milde Sitten.", "tokens": ["Z\u00fcch\u00b7tig\u00b7keit", "und", "mil\u00b7de", "Sit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Erstens war man \u00fcberzeugt,", "tokens": ["Ers\u00b7tens", "war", "man", "\u00fc\u00b7berz\u00b7eugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df es selten Gutes br\u00e4chte,", "tokens": ["Da\u00df", "es", "sel\u00b7ten", "Gu\u00b7tes", "br\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ein M\u00e4dchen viel verkehrt", "tokens": ["Wenn", "ein", "M\u00e4d\u00b7chen", "viel", "ver\u00b7kehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem anderen Geschlechte.", "tokens": ["Mit", "dem", "an\u00b7de\u00b7ren", "Ge\u00b7schlech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Au\u00dferdem und nebenbei", "tokens": ["Au\u00b7\u00dfer\u00b7dem", "und", "ne\u00b7ben\u00b7bei"], "token_info": ["word", "word", "word"], "pos": ["PAV", "KON", "APPR"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ist es ja dem Sporte eigen,", "tokens": ["Ist", "es", "ja", "dem", "Spor\u00b7te", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Spieles wilder Lust", "tokens": ["In", "des", "Spie\u00b7les", "wil\u00b7der", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Manches unverh\u00fcllt zu zeigen.", "tokens": ["Man\u00b7ches", "un\u00b7ver\u00b7h\u00fcllt", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Es verschiebt sich oft ein Rock,", "tokens": ["Es", "ver\u00b7schiebt", "sich", "oft", "ein", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder fliegt vom Wind gehoben,", "tokens": ["O\u00b7der", "fliegt", "vom", "Wind", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und man sieht das Wadenbein", "tokens": ["Und", "man", "sieht", "das", "Wa\u00b7den\u00b7bein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiter unten oder oben.", "tokens": ["Wei\u00b7ter", "un\u00b7ten", "o\u00b7der", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ein J\u00fcngling, der es sieht,", "tokens": ["Und", "ein", "J\u00fcng\u00b7ling", ",", "der", "es", "sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fa\u00dft verwerfliche Gedanken;", "tokens": ["Fa\u00dft", "ver\u00b7werf\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, er \u00fcbersteigt sofort", "tokens": ["Ja", ",", "er", "\u00fc\u00b7bers\u00b7teigt", "so\u00b7fort"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Innerlich der Sitte Schranken.", "tokens": ["In\u00b7ner\u00b7lich", "der", "Sit\u00b7te", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sei es, da\u00df es ihm gelingt,", "tokens": ["Sei", "es", ",", "da\u00df", "es", "ihm", "ge\u00b7lingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder nicht, man soll bedenken:", "tokens": ["O\u00b7der", "nicht", ",", "man", "soll", "be\u00b7den\u00b7ken", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PIS", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Besser ist es, nicht den Blick", "tokens": ["Bes\u00b7ser", "ist", "es", ",", "nicht", "den", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Wunsch darauf zu lenken.", "tokens": ["Und", "den", "Wunsch", "da\u00b7rauf", "zu", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Andrerseits ist wiederum", "tokens": ["A\u00b7ndrer\u00b7seits", "ist", "wie\u00b7de\u00b7rum"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses eine zu bemerken:", "tokens": ["Die\u00b7ses", "ei\u00b7ne", "zu", "be\u00b7mer\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die M\u00e4dchen durch den Sport", "tokens": ["Da\u00df", "die", "M\u00e4d\u00b7chen", "durch", "den", "Sport"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Wadenbeine st\u00e4rken.", "tokens": ["Ih\u00b7re", "Wa\u00b7den\u00b7bei\u00b7ne", "st\u00e4r\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Freilich blieb' es au\u00dfer Acht,", "tokens": ["Frei\u00b7lich", "blieb'", "es", "au\u00b7\u00dfer", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00e4lt es nur, der Lust zu fr\u00f6nen,", "tokens": ["G\u00e4lt", "es", "nur", ",", "der", "Lust", "zu", "fr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die Rassenbildung will,", "tokens": ["Doch", "die", "Ras\u00b7sen\u00b7bil\u00b7dung", "will", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df wir uns damit vers\u00f6hnen!", "tokens": ["Da\u00df", "wir", "uns", "da\u00b7mit", "ver\u00b7s\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und ich sage ohne Scheu,", "tokens": ["Und", "ich", "sa\u00b7ge", "oh\u00b7ne", "Scheu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fc\u00dft' ich selber mich verm\u00e4hlen,", "tokens": ["M\u00fc\u00dft'", "ich", "sel\u00b7ber", "mich", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrd' ich pflanzungsvorbedacht", "tokens": ["W\u00fcrd'", "ich", "pflan\u00b7zungs\u00b7vor\u00b7be\u00b7dacht"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine gut Gebeinte w\u00e4hlen.", "tokens": ["Ei\u00b7ne", "gut", "Ge\u00b7bein\u00b7te", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Selber liebe ich den Sport,", "tokens": ["Sel\u00b7ber", "lie\u00b7be", "ich", "den", "Sport", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch die Frage qu\u00e4lt mich immer:", "tokens": ["Doch", "die", "Fra\u00b7ge", "qu\u00e4lt", "mich", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ist er passend, schickt er sich", "tokens": ["Ist", "er", "pas\u00b7send", ",", "schickt", "er", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr ein junges Frauenzimmer?", "tokens": ["F\u00fcr", "ein", "jun\u00b7ges", "Frau\u00b7en\u00b7zim\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Fr\u00fcher war es allgemein", "tokens": ["Fr\u00fc\u00b7her", "war", "es", "all\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit Heftigkeit bestritten;", "tokens": ["Und", "mit", "Hef\u00b7tig\u00b7keit", "be\u00b7strit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.te