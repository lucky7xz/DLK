{"textgrid.poem.41968": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm, nach Arkadien wollen wir ziehn, in's bl\u00fchende Tempe,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, nach Arkadien wollen wir ziehn, in's bl\u00fchende Tempe,", "tokens": ["Komm", ",", "nach", "Ar\u00b7ka\u00b7di\u00b7en", "wol\u00b7len", "wir", "ziehn", ",", "in's", "bl\u00fc\u00b7hen\u00b7de", "Tem\u00b7pe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NE", "VMFIN", "PPER", "VVINF", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sieh, schon d\u00e4mmert der Tag; Liebchen, o gieb mir die Hand!", "tokens": ["Sieh", ",", "schon", "d\u00e4m\u00b7mert", "der", "Tag", ";", "Lieb\u00b7chen", ",", "o", "gieb", "mir", "die", "Hand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "$.", "NN", "$,", "FM", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Weit ist die Wandrung zwar, doch Liebende sch\u00fctzen die G\u00f6tter", "tokens": ["Weit", "ist", "die", "Wand\u00b7rung", "zwar", ",", "doch", "Lie\u00b7ben\u00b7de", "sch\u00fct\u00b7zen", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "$,", "KON", "NN", "VVFIN", "ART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Willig; im ganzen Olymp herrschet Idalias Sohn.", "tokens": ["Wil\u00b7lig", ";", "im", "gan\u00b7zen", "O\u00b7lymp", "herr\u00b7schet", "I\u00b7da\u00b7li\u00b7as", "Sohn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "APPRART", "ADJA", "NN", "VVFIN", "NE", "NN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.5": {"text": "Aber begleitet uns auch der Fl\u00fcchtige? Kannst du noch zweifeln?", "tokens": ["A\u00b7ber", "be\u00b7glei\u00b7tet", "uns", "auch", "der", "Fl\u00fcch\u00b7ti\u00b7ge", "?", "Kannst", "du", "noch", "zwei\u00b7feln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Bandest du ihn nicht j\u00fcngst Grazie, da du im Hain", "tokens": ["Ban\u00b7dest", "du", "ihn", "nicht", "j\u00fcngst", "Gra\u00b7zie", ",", "da", "du", "im", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "NN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Schlummernd auf duftigen Bl\u00fcthen ihn sahst? Jetzt dient er dir ewig,", "tokens": ["Schlum\u00b7mernd", "auf", "duf\u00b7ti\u00b7gen", "Bl\u00fc\u00b7then", "ihn", "sahst", "?", "Jetzt", "dient", "er", "dir", "e\u00b7wig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Nach der Idalischen Flur sehnt er sich nimmer zur\u00fcck.", "tokens": ["Nach", "der", "I\u00b7da\u00b7li\u00b7schen", "Flur", "sehnt", "er", "sich", "nim\u00b7mer", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+---+--+", "measure": "iambic.hexa.chol"}, "line.9": {"text": "Ach, kein schmeichelndes Wort der Erzeugerin kann ihn hinwegziehn;", "tokens": ["Ach", ",", "kein", "schmei\u00b7cheln\u00b7des", "Wort", "der", "Er\u00b7zeu\u00b7ge\u00b7rin", "kann", "ihn", "hin\u00b7weg\u00b7ziehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "ADJA", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Huldg\u00f6ttinnen, umsonst locket ihr kosend den Freund!", "tokens": ["Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", ",", "um\u00b7sonst", "lo\u00b7cket", "ihr", "ko\u00b7send", "den", "Freund", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Gern wohl leiht er uns jetzt den Taubenwagen, und dienstbar", "tokens": ["Gern", "wohl", "leiht", "er", "uns", "jetzt", "den", "Tau\u00b7ben\u00b7wa\u00b7gen", ",", "und", "dienst\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "$,", "KON", "ADJD"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "Lenkt er, wohin du befiehlst, selber das leichte Gespann.", "tokens": ["Lenkt", "er", ",", "wo\u00b7hin", "du", "be\u00b7fiehlst", ",", "sel\u00b7ber", "das", "leich\u00b7te", "Ge\u00b7spann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "F\u00fchr' uns nach Tempe jetzt, du Freundlicher! Siehe der Winter", "tokens": ["F\u00fchr'", "uns", "nach", "Tem\u00b7pe", "jetzt", ",", "du", "Freund\u00b7li\u00b7cher", "!", "Sie\u00b7he", "der", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NE", "ADV", "$,", "PPER", "NN", "$.", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Nahete schon, und rauh sch\u00fcttelt die Haine der Sturm.", "tokens": ["Na\u00b7he\u00b7te", "schon", ",", "und", "rauh", "sch\u00fct\u00b7telt", "die", "Hai\u00b7ne", "der", "Sturm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "L\u00e4ngst schon sanken die Blumen dahin, Cythereens Geschmeide,", "tokens": ["L\u00e4ngst", "schon", "san\u00b7ken", "die", "Blu\u00b7men", "da\u00b7hin", ",", "Cy\u00b7ther\u00b7eens", "Ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PAV", "$,", "NE", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Und mit den Blumen entfloh jeglicher heitere Gott.", "tokens": ["Und", "mit", "den", "Blu\u00b7men", "ent\u00b7floh", "jeg\u00b7li\u00b7cher", "hei\u00b7te\u00b7re", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Frost erstarret das Herz, Frost ist der Liebe Verderben,", "tokens": ["Frost", "er\u00b7star\u00b7ret", "das", "Herz", ",", "Frost", "ist", "der", "Lie\u00b7be", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+--++--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Zephyrus Schwingen allein duldet das z\u00e4rtliche Kind.", "tokens": ["Ze\u00b7phy\u00b7rus", "Schwin\u00b7gen", "al\u00b7lein", "dul\u00b7det", "das", "z\u00e4rt\u00b7li\u00b7che", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "W\u00e4rme nur hebt die Knospen der Flur; im lauen Ges\u00e4usel.", "tokens": ["W\u00e4r\u00b7me", "nur", "hebt", "die", "Knos\u00b7pen", "der", "Flur", ";", "im", "lau\u00b7en", "Ge\u00b7s\u00e4u\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$.", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Schleichet die Sehnsucht sich leis' in's verlangende Herz.", "tokens": ["Schlei\u00b7chet", "die", "Sehn\u00b7sucht", "sich", "leis'", "in's", "ver\u00b7lan\u00b7gen\u00b7de", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.21": {"text": "W\u00e4rme nur \u00f6ffnet der Bl\u00fcthe den Kelch; beim schmeichelnden Lenzhauch", "tokens": ["W\u00e4r\u00b7me", "nur", "\u00f6ff\u00b7net", "der", "Bl\u00fc\u00b7the", "den", "Kelch", ";", "beim", "schmei\u00b7cheln\u00b7den", "Lenz\u00b7hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Schlie\u00dft dem zarten Gef\u00fchl willig der Busen sich auf.", "tokens": ["Schlie\u00dft", "dem", "zar\u00b7ten", "Ge\u00b7f\u00fchl", "wil\u00b7lig", "der", "Bu\u00b7sen", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Ach, mich verlangt in ein sch\u00f6neres Land, wo ewiger Fr\u00fchling", "tokens": ["Ach", ",", "mich", "ver\u00b7langt", "in", "ein", "sch\u00f6\u00b7ne\u00b7res", "Land", ",", "wo", "e\u00b7wi\u00b7ger", "Fr\u00fch\u00b7ling"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Unter dem schattigen Dach fl\u00fcsternder Bl\u00fcthen verweilt!", "tokens": ["Un\u00b7ter", "dem", "schat\u00b7ti\u00b7gen", "Dach", "fl\u00fcs\u00b7tern\u00b7der", "Bl\u00fc\u00b7then", "ver\u00b7weilt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Komm, nach Arkadien wollen wir ziehn, in's bl\u00fchende Tempe,", "tokens": ["Komm", ",", "nach", "Ar\u00b7ka\u00b7di\u00b7en", "wol\u00b7len", "wir", "ziehn", ",", "in's", "bl\u00fc\u00b7hen\u00b7de", "Tem\u00b7pe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NE", "VMFIN", "PPER", "VVINF", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Sieh, schon d\u00e4mmert der Tag, Liebchen, o gieb mir die Hand.", "tokens": ["Sieh", ",", "schon", "d\u00e4m\u00b7mert", "der", "Tag", ",", "Lieb\u00b7chen", ",", "o", "gieb", "mir", "die", "Hand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "NN", "$,", "FM", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Komm, nach Arkadien wollen wir ziehn, in's bl\u00fchende Tempe,", "tokens": ["Komm", ",", "nach", "Ar\u00b7ka\u00b7di\u00b7en", "wol\u00b7len", "wir", "ziehn", ",", "in's", "bl\u00fc\u00b7hen\u00b7de", "Tem\u00b7pe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NE", "VMFIN", "PPER", "VVINF", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sieh, schon d\u00e4mmert der Tag; Liebchen, o gieb mir die Hand!", "tokens": ["Sieh", ",", "schon", "d\u00e4m\u00b7mert", "der", "Tag", ";", "Lieb\u00b7chen", ",", "o", "gieb", "mir", "die", "Hand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "$.", "NN", "$,", "FM", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Weit ist die Wandrung zwar, doch Liebende sch\u00fctz