meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bricht demselben wieder ab.", "tokens": ["Bricht", "dem\u00b7sel\u00b7ben", "wie\u00b7der", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}