{"textgrid.poem.66972": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Wahrheit nach unten und oben", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist denn einer \u00fcber allen,", "tokens": ["Ist", "denn", "ei\u00b7ner", "\u00fc\u00b7ber", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "APPR", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die Wahrheit nicht vertr\u00e4gt?", "tokens": ["Der", "die", "Wahr\u00b7heit", "nicht", "ver\u00b7tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun, so sind wir nur Vasallen", "tokens": ["Nun", ",", "so", "sind", "wir", "nur", "Va\u00b7sal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und versklaven \u00bbunentwegt\u00ab.", "tokens": ["Und", "ver\u00b7skla\u00b7ven", "\u00bb", "un\u00b7ent\u00b7wegt", "\u00ab", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$(", "ADJD", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freies Wort nach allen Seiten,", "tokens": ["Frei\u00b7es", "Wort", "nach", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zur Trib\u00fcne wie zum Thron!", "tokens": ["Zur", "Tri\u00b7b\u00fc\u00b7ne", "wie", "zum", "Thron", "!"], "token_info": ["word