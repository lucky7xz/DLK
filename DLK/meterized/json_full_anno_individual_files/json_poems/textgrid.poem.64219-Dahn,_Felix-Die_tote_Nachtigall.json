{"textgrid.poem.64219": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die tote Nachtigall", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, da\u00df am Fu\u00df der duft'gen Linde,", "tokens": ["Ach", ",", "da\u00df", "am", "Fu\u00df", "der", "duft'\u00b7gen", "Lin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "APPRART", "NN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die oft dein wonnig Lied durchdrang,", "tokens": ["Die", "oft", "dein", "won\u00b7nig", "Lied", "durch\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich tot dich, gl\u00fch'nder S\u00e4nger, finde!", "tokens": ["Ich", "tot", "dich", ",", "gl\u00fch'n\u00b7der", "S\u00e4n\u00b7ger", ",", "fin\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob dir vor Drang das Herz zersprang?", "tokens": ["Ob", "dir", "vor", "Drang", "das", "Herz", "zer\u00b7sprang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Oft liegt Verderben im Gesang!", "tokens": ["Oft", "liegt", "Ver\u00b7der\u00b7ben", "im", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem S\u00e4nger Heil, des hei\u00dfe Jugend", "tokens": ["Dem", "S\u00e4n\u00b7ger", "Heil", ",", "des", "hei\u00b7\u00dfe", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kraft ge\u00fcbt hat, nicht entweiht,", "tokens": ["Die", "Kraft", "ge\u00b7\u00fcbt", "hat", ",", "nicht", "ent\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ihm der Dichtung h\u00f6chste Tugend,", "tokens": ["Da\u00df", "ihm", "der", "Dich\u00b7tung", "h\u00f6chs\u00b7te", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Ma\u00dfes stille Heiligkeit,", "tokens": ["Des", "Ma\u00b7\u00dfes", "stil\u00b7le", "Hei\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun vollgereift das Alter leiht.", "tokens": ["Nun", "voll\u00b7ge\u00b7reift", "das", "Al\u00b7ter", "leiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Oft denk' ich dein wildfeurig Singen,", "tokens": ["Oft", "denk'", "ich", "dein", "wild\u00b7feu\u00b7rig", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du allzu k\u00fchner Spielgeno\u00df,", "tokens": ["Du", "all\u00b7zu", "k\u00fch\u00b7ner", "Spiel\u00b7ge\u00b7no\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Heinrich, du von Ofterdingen:", "tokens": ["O", "Hein\u00b7rich", ",", "du", "von", "Of\u00b7ter\u00b7din\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann voll das Lied vom Mund dir flo\u00df,", "tokens": ["Wann", "voll", "das", "Lied", "vom", "Mund", "dir", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie hei\u00df dein Blick dann Flammen scho\u00df!", "tokens": ["Wie", "hei\u00df", "dein", "Blick", "dann", "Flam\u00b7men", "scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohin hat dich der Sturm vertragen,", "tokens": ["Wo\u00b7hin", "hat", "dich", "der", "Sturm", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du heller, stolzer, junger Stern?", "tokens": ["Du", "hel\u00b7ler", ",", "stol\u00b7zer", ",", "jun\u00b7ger", "Stern", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verlodert bist du und zerschlagen,", "tokens": ["Ver\u00b7lo\u00b7dert", "bist", "du", "und", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eh' voll gefestigt war dein Kern. \u2013", "tokens": ["Eh'", "voll", "ge\u00b7fes\u00b7tigt", "war", "dein", "Kern", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "VVPP", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wem's besser ward, der dankt's dem Herrn.", "tokens": ["Wem's", "bes\u00b7ser", "ward", ",", "der", "dankt's", "dem", "Herrn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "$,", "PRELS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hei\u00dfherzig, kleines Singeseelchen,", "tokens": ["Hei\u00df\u00b7her\u00b7zig", ",", "klei\u00b7nes", "Sin\u00b7ge\u00b7see\u00b7lchen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich bett' ich hier nach Waldesbrauch", "tokens": ["Dich", "bett'", "ich", "hier", "nach", "Wal\u00b7des\u00b7brauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In gr\u00fcnem Moos \u2013: da singt Rotkehlchen", "tokens": ["In", "gr\u00fc\u00b7nem", "Moos", "\u2013", ":", "da", "singt", "Rot\u00b7kehl\u00b7chen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "$.", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Grablied dir vom Rosenstrauch,", "tokens": ["Das", "Grab\u00b7lied", "dir", "vom", "Ro\u00b7sen\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und \u00fcber dir Sang, Duft und Hauch. \u2013", "tokens": ["Und", "\u00fc\u00b7ber", "dir", "Sang", ",", "Duft", "und", "Hauch", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "NN", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wo wirst einst du wohl schlummern, Walther?", "tokens": ["Wo", "wirst", "einst", "du", "wohl", "schlum\u00b7mern", ",", "Walt\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPER", "ADV", "VVINF", "$,", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "O legt mich in den Domhof nicht,", "tokens": ["O", "legt", "mich", "in", "den", "Dom\u00b7hof", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo mir ein Marbelstein, ein kalter,", "tokens": ["Wo", "mir", "ein", "Mar\u00b7bel\u00b7stein", ",", "ein", "kal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ruht auf der Brust mit Lastgewicht,", "tokens": ["Ruht", "auf", "der", "Brust", "mit", "Last\u00b7ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Absperrend Himmel, Luft und Licht.", "tokens": ["Ab\u00b7sper\u00b7rend", "Him\u00b7mel", ",", "Luft", "und", "Licht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nein! In den Wald sollt ihr mich tragen", "tokens": ["Nein", "!", "In", "den", "Wald", "sollt", "ihr", "mich", "tra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "APPR", "ART", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und betten unter'm Moose gr\u00fcn,", "tokens": ["Und", "bet\u00b7ten", "un\u00b7ter'm", "Moo\u00b7se", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Nachtigallen um mich schlagen,", "tokens": ["Da\u00df", "Nach\u00b7ti\u00b7gal\u00b7len", "um", "mich", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wilde Rosen um mich bl\u00fchn:", "tokens": ["Und", "wil\u00b7de", "Ro\u00b7sen", "um", "mich", "bl\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, wann des Winters Flocken spr\u00fchn,", "tokens": ["Und", ",", "wann", "des", "Win\u00b7ters", "Flo\u00b7cken", "spr\u00fchn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf meinem schneebefreiten Grabe", "tokens": ["Auf", "mei\u00b7nem", "schnee\u00b7be\u00b7frei\u00b7ten", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sollt ihr den V\u00f6glein Futter streun,", "tokens": ["Sollt", "ihr", "den", "V\u00f6\u00b7glein", "Fut\u00b7ter", "streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sie an ihres Freundes Gabe,", "tokens": ["Da\u00df", "sie", "an", "ih\u00b7res", "Freun\u00b7des", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Wann Frost und Hunger sie bedr\u00e4un,", "tokens": ["Wann", "Frost", "und", "Hun\u00b7ger", "sie", "be\u00b7dr\u00e4un", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch lang nach seinem Tod sich freun.", "tokens": ["Noch", "lang", "nach", "sei\u00b7nem", "Tod", "sich", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ob dann wohl in der Sterne Hallen", "tokens": ["Ob", "dann", "wohl", "in", "der", "Ster\u00b7ne", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "ART", "NN", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mein Saitenspiel aufs neue klingt?", "tokens": ["Mein", "Sai\u00b7ten\u00b7spiel", "aufs", "neu\u00b7e", "klingt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob, gleich der Brust der Nachtigallen,", "tokens": ["Ob", ",", "gleich", "der", "Brust", "der", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Saite, die im Herzen schwingt,", "tokens": ["Die", "Sai\u00b7te", ",", "die", "im", "Her\u00b7zen", "schwingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr immerdar im Tode springt?", "tokens": ["F\u00fcr", "im\u00b7mer\u00b7dar", "im", "To\u00b7de", "springt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wer wei\u00df es! \u2013 Walther, sei zufrieden", "tokens": ["Wer", "wei\u00df", "es", "!", "\u2013", "Walt\u00b7her", ",", "sei", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "$(", "NE", "$,", "VAFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dem, was dir auf Erden ward:", "tokens": ["Mit", "dem", ",", "was", "dir", "auf", "Er\u00b7den", "ward", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wem das Sch\u00f6ne ward beschieden,", "tokens": ["Denn", "wem", "das", "Sch\u00f6\u00b7ne", "ward", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der hat \u2013 ihm ist der Tod nicht hart \u2013", "tokens": ["Der", "hat", "\u2013", "ihm", "ist", "der", "Tod", "nicht", "hart", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Ewigkeit in Gegenwart.", "tokens": ["Die", "E\u00b7wig\u00b7keit", "in", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ach, da\u00df am Fu\u00df der duft'gen Linde,", "tokens": ["Ach", ",", "da\u00df", "am", "Fu\u00df", "der", "duft'\u00b7gen", "Lin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "APPRART", "NN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die oft dein wonnig Lied durchdrang,", "tokens": ["Die", "oft", "dein", "won\u00b7nig", "Lied", "durch\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"},