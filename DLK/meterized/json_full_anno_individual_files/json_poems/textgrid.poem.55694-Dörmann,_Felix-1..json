{"textgrid.poem.55694": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und leise trat ich an Dein Sterbelager,", "tokens": ["Und", "lei\u00b7se", "trat", "ich", "an", "Dein", "Ster\u00b7be\u00b7la\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du kaum erschlossne, schnell verwelkte Bl\u00fcte.", "tokens": ["Du", "kaum", "er\u00b7schloss\u00b7ne", ",", "schnell", "ver\u00b7welk\u00b7te", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u2013 \u2013 Mir war ja heimlich zugefl\u00fcstert worden,", "tokens": ["\u2013", "\u2013", "Mir", "war", "ja", "heim\u00b7lich", "zu\u00b7ge\u00b7fl\u00fcs\u00b7tert", "wor\u00b7den", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPER", "VAFIN", "ADV", "ADJD", "VVPP", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du h\u00e4ttest ganz im Stillen mich geliebt. \u2013 \u2013", "tokens": ["Du", "h\u00e4t\u00b7test", "ganz", "im", "Stil\u00b7len", "mich", "ge\u00b7liebt", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "PPER", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch einmal hoben sich die schweren Lider", "tokens": ["Noch", "ein\u00b7mal", "ho\u00b7ben", "sich", "die", "schwe\u00b7ren", "Li\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und aus den todesmatten Augen brach", "tokens": ["Und", "aus", "den", "to\u00b7des\u00b7mat\u00b7ten", "Au\u00b7gen", "brach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein letzter \u2013 langer \u2013 tiefer Blick der Liebe,", "tokens": ["Ein", "letz\u00b7ter", "\u2013", "lan\u00b7ger", "\u2013", "tie\u00b7fer", "Blick", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ADJD", "$(", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Deine abgezehrte, kleine Hand,", "tokens": ["Und", "Dei\u00b7ne", "ab\u00b7ge\u00b7zehr\u00b7te", ",", "klei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch einmal legte sie sich fest in meine,", "tokens": ["Noch", "ein\u00b7mal", "leg\u00b7te", "sie", "sich", "fest", "in", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dann war Alles, Alles, Alles aus.", "tokens": ["Und", "dann", "war", "Al\u00b7les", ",", "Al\u00b7les", ",", "Al\u00b7les", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"