{"dta.poem.13983": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "IiI .  \n  Jagdlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Durch schwankende Wipfel", "tokens": ["Durch", "schwan\u00b7ken\u00b7de", "Wip\u00b7fel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schie\u00dft g\u00fcldener Strahl,", "tokens": ["Schie\u00dft", "g\u00fcl\u00b7de\u00b7ner", "Strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Tief unter den Gipfeln", "tokens": ["Tief", "un\u00b7ter", "den", "Gip\u00b7feln"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das neblichte Thal.", "tokens": ["Das", "neb\u00b7lich\u00b7te", "Thal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Fern hallt es am Schlosse,", "tokens": ["Fern", "hallt", "es", "am", "Schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Das Waldhorn ruft,", "tokens": ["Das", "Wald\u00b7horn", "ruft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Es wiehern die Rosse,", "tokens": ["Es", "wie\u00b7hern", "die", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "In die Luft, in die Luft!", "tokens": ["In", "die", "Luft", ",", "in", "die", "Luft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}