{"dta.poem.7363": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wie d\u00e4mmerschaurig ist der Wald", "tokens": ["Wie", "d\u00e4m\u00b7mer\u00b7schau\u00b7rig", "ist", "der", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An neblichten Novembertagen,", "tokens": ["An", "neb\u00b7lich\u00b7ten", "No\u00b7vem\u00b7ber\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wie wunderlich die Wildni\u00df hallt", "tokens": ["Wie", "wun\u00b7der\u00b7lich", "die", "Wild\u00b7ni\u00df", "hallt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Astgest\u00f6hn und Windesklagen!", "tokens": ["Von", "Ast\u00b7ge\u00b7st\u00f6hn", "und", "Win\u00b7des\u00b7kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u201ehorch, Knabe, war das Waffenklang?\u201c \u2014", "tokens": ["\u201e", "horch", ",", "Kna\u00b7be", ",", "war", "das", "Waf\u00b7fen\u00b7klang", "?", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVIMP", "$,", "NN", "$,", "VAFIN", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201enein, gn\u00e4d'ger Herr! ein Vogel sang,", "tokens": ["\u201e", "nein", ",", "gn\u00e4d'\u00b7ger", "Herr", "!", "ein", "Vo\u00b7gel", "sang", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Sturmesfl\u00fcgeln hergetragen.\u201c \u2014", "tokens": ["Von", "Stur\u00b7mes\u00b7fl\u00fc\u00b7geln", "her\u00b7ge\u00b7tra\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Fort trabt der m\u00e4chtige Pr\u00e4lat,", "tokens": ["Fort", "trabt", "der", "m\u00e4ch\u00b7ti\u00b7ge", "Pr\u00e4\u00b7lat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der k\u00fchne Erzbischof von C\u00f6llen,", "tokens": ["Der", "k\u00fch\u00b7ne", "Erz\u00b7bi\u00b7schof", "von", "C\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er, den der Kaiser sich zum Rath", "tokens": ["Er", ",", "den", "der", "Kai\u00b7ser", "sich", "zum", "Rath"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Reichsverweser mochte stellen,", "tokens": ["Und", "Reichs\u00b7ver\u00b7we\u00b7ser", "moch\u00b7te", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ehrne Hand der Clerisey, \u2014", "tokens": ["Die", "ehr\u00b7ne", "Hand", "der", "Cle\u00b7ri\u00b7sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NE", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zwei Edelknaben, Reis'ger zwei,", "tokens": ["Zwei", "E\u00b7del\u00b7kna\u00b7ben", ",", "Reis'\u00b7ger", "zwei", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "NN", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und noch drei Aebte als Gesellen.", "tokens": ["Und", "noch", "drei", "A\u00b7eb\u00b7te", "als", "Ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "CARD", "NN", "KOUS", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Gelassen trabt er fort, im Traum", "tokens": ["Ge\u00b7las\u00b7sen", "trabt", "er", "fort", ",", "im", "Traum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von eines Wunderdomes Sch\u00f6ne,", "tokens": ["Von", "ei\u00b7nes", "Wun\u00b7der\u00b7do\u00b7mes", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf seines Rosses Hals den Zaum,", "tokens": ["Auf", "sei\u00b7nes", "Ros\u00b7ses", "Hals", "den", "Zaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er streicht ihm sanft die dichte M\u00e4hne,", "tokens": ["Er", "streicht", "ihm", "sanft", "die", "dich\u00b7te", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Windesodem senkt und schwellt; \u2014", "tokens": ["Die", "Win\u00b7de\u00b7so\u00b7dem", "senkt", "und", "schwellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es schaudert, wenn ein Tropfen f\u00e4llt", "tokens": ["Es", "schau\u00b7dert", ",", "wenn", "ein", "Trop\u00b7fen", "f\u00e4llt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Ast und Laub, des Nebels Thr\u00e4ne.", "tokens": ["Von", "Ast", "und", "Laub", ",", "des", "Ne\u00b7bels", "Thr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schon schwindelnd steigt das Kirchenschiff,", "tokens": ["Schon", "schwin\u00b7delnd", "steigt", "das", "Kir\u00b7chen\u00b7schiff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon bilden sich die krausen Zacken \u2014", "tokens": ["Schon", "bil\u00b7den", "sich", "die", "krau\u00b7sen", "Za\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da, horch, ein Pfiff und hui, ein Griff,", "tokens": ["Da", ",", "horch", ",", "ein", "Pfiff", "und", "hui", ",", "ein", "Griff", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "VVIMP", "$,", "ART", "NN", "KON", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Helmbusch hier, ein Arm im Nacken!", "tokens": ["Ein", "Helm\u00b7busch", "hier", ",", "ein", "Arm", "im", "Na\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie Schwarzwildrudel bricht's heran,", "tokens": ["Wie", "Schwarz\u00b7wild\u00b7ru\u00b7del", "bricht's", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Aebte fliehn wie Spreu, und dann", "tokens": ["Die", "A\u00b7eb\u00b7te", "fliehn", "wie", "Spreu", ",", "und", "dann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "KOKOM", "NE", "$,", "KON", "ADV"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Mit Reisigen sich Reis'ge packen.", "tokens": ["Mit", "Rei\u00b7si\u00b7gen", "sich", "Reis'\u00b7ge", "pa\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ha, schn\u00f6der Straus! zwei gegen zehn!", "tokens": ["Ha", ",", "schn\u00f6\u00b7der", "Straus", "!", "zwei", "ge\u00b7gen", "zehn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$.", "CARD", "APPR", "CARD", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Doch hat der F\u00fcrst sich losgerungen,", "tokens": ["Doch", "hat", "der", "F\u00fcrst", "sich", "los\u00b7ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er peitscht sein Thier und mit Gest\u00f6hn", "tokens": ["Er", "peitscht", "sein", "Thier", "und", "mit", "Ge\u00b7st\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat's \u00fcber'n Hohlweg sich geschwungen;", "tokens": ["Hat's", "\u00fc\u00b7ber'n", "Hohl\u00b7weg", "sich", "ge\u00b7schwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PRF", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die Gerte pfeift \u2014 \u201eWeh, Rinkerad!\u201c \u2014", "tokens": ["Die", "Ger\u00b7te", "pfeift", "\u201e", "Weh", ",", "Rin\u00b7ke\u00b7rad", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "$(", "NN", "$,", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vom Rosse gleitet der Pr\u00e4lat", "tokens": ["Vom", "Ros\u00b7se", "glei\u00b7tet", "der", "Pr\u00e4\u00b7lat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ist in's Dickicht dann gedrungen.", "tokens": ["Und", "ist", "in's", "Di\u00b7ckicht", "dann", "ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u201ehussah, hussah, erschlagt den Hund,", "tokens": ["\u201e", "hus\u00b7sah", ",", "hus\u00b7sah", ",", "er\u00b7schlagt", "den", "Hund", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den stolzen Hund!\u201c und eine Meute", "tokens": ["Den", "stol\u00b7zen", "Hund", "!", "\u201c", "und", "ei\u00b7ne", "Meu\u00b7te"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00e4hrt's in den Wald, es schlie\u00dft ein Rund,", "tokens": ["F\u00e4hrt's", "in", "den", "Wald", ",", "es", "schlie\u00dft", "ein", "Rund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "ADJD", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann vor \u2014 und r\u00fcckw\u00e4rts und zur Seite;", "tokens": ["Dann", "vor", "und", "r\u00fcck\u00b7w\u00e4rts", "und", "zur", "Sei\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "$(", "KON", "ADV", "KON", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Die Zweige krachen \u2014 ha es naht \u2014", "tokens": ["Die", "Zwei\u00b7ge", "kra\u00b7chen", "ha", "es", "naht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ITJ", "PPER", "VVFIN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Am Buchenstamm steht der Pr\u00e4lat", "tokens": ["Am", "Bu\u00b7chen\u00b7stamm", "steht", "der", "Pr\u00e4\u00b7lat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie ein gestellter Eber heute.", "tokens": ["Wie", "ein", "ge\u00b7stell\u00b7ter", "E\u00b7ber", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er blickt verzweifelnd auf sein Schwert,", "tokens": ["Er", "blickt", "ver\u00b7zwei\u00b7felnd", "auf", "sein", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er l\u00f6st die kurze breite Klinge,", "tokens": ["Er", "l\u00f6st", "die", "kur\u00b7ze", "brei\u00b7te", "Klin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann pr\u00fcfend unter'n Mantel f\u00e4hrt", "tokens": ["Dann", "pr\u00fc\u00b7fend", "un\u00b7ter'n", "Man\u00b7tel", "f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Linke nach dem Panzerringe;", "tokens": ["Die", "Lin\u00b7ke", "nach", "dem", "Pan\u00b7zer\u00b7rin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und nun wohlan, er ist bereit,", "tokens": ["Und", "nun", "wo\u00b7hlan", ",", "er", "ist", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja m\u00e4nnlich focht der Priester heut,", "tokens": ["Ja", "m\u00e4nn\u00b7lich", "focht", "der", "Pries\u00b7ter", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Streich war eine Flammenschwinge.", "tokens": ["Sein", "Streich", "war", "ei\u00b7ne", "Flam\u00b7men\u00b7schwin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Das schwirrt und klingelt durch den Wald,", "tokens": ["Das", "schwirrt", "und", "klin\u00b7gelt", "durch", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Bl\u00e4tter st\u00e4uben von den Eichen,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "st\u00e4u\u00b7ben", "von", "den", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber Arm und Sch\u00e4del bald", "tokens": ["Und", "\u00fc\u00b7ber", "Arm", "und", "Sch\u00e4\u00b7del", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blutrothe Rinnen tr\u00f6pfe