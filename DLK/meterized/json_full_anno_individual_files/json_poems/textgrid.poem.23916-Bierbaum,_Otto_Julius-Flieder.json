{"textgrid.poem.23916": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Flieder", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stille, tr\u00e4umende Fr\u00fchlingsnacht ...", "tokens": ["Stil\u00b7le", ",", "tr\u00e4u\u00b7men\u00b7de", "Fr\u00fch\u00b7lings\u00b7nacht", "..."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Die Sterne am Himmel blinzelten mild,", "tokens": ["Die", "Ster\u00b7ne", "am", "Him\u00b7mel", "blin\u00b7zel\u00b7ten", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Breit stand der Mond wie ein silberner Schild,", "tokens": ["Breit", "stand", "der", "Mond", "wie", "ein", "sil\u00b7ber\u00b7ner", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "In den Zweigen rauschte es sacht.", "tokens": ["In", "den", "Zwei\u00b7gen", "rauschte", "es", "sacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Arm in Arm und wie in Tr\u00e4umen", "tokens": ["Arm", "in", "Arm", "und", "wie", "in", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter duften