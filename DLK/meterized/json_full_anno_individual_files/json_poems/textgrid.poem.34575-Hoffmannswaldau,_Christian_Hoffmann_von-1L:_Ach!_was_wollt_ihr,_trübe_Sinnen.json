unct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und bei tr\u00fcbem Geist und Sinnen", "tokens": ["Und", "bei", "tr\u00fc\u00b7bem", "Geist", "und", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Scherzen k\u00f6nnen,", "tokens": ["Scher\u00b7zen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ist ein Schatz der klugen Welt.", "tokens": ["Ist", "ein", "Schatz", "der", "klu\u00b7gen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Ueber das Verh\u00e4ngni\u00df klagen,", "tokens": ["Ue\u00b7ber", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehrt die Plagen", "tokens": ["Mehrt", "die", "Pla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und verr\u00e4th die Ungeduld;", "tokens": ["Und", "ver\u00b7r\u00e4\u00b7th", "die", "Un\u00b7ge\u00b7duld", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Solchem, der mit gleichem Herzen", "tokens": ["Sol\u00b7chem", ",", "der", "mit", "glei\u00b7chem", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "po