trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dornenreiches Ungel\u00fccke,", "tokens": ["Dor\u00b7nen\u00b7rei\u00b7ches", "Un\u00b7ge\u00b7l\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Donnerblicke", "tokens": ["Don\u00b7ner\u00b7bli\u00b7cke"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und des Himmels H\u00e4rtigkeit", "tokens": ["Und", "des", "Him\u00b7mels", "H\u00e4r\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird kein Kummer linder machen;", "tokens": ["Wird", "kein", "Kum\u00b7mer", "lin\u00b7der", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Sachen", "tokens": ["Al\u00b7le", "Sa\u00b7chen"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.6": {"text": "Werden anders mit der Zeit.", "tokens": ["Wer\u00b7den", "an\u00b7ders", "mit", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sich in tausend Thr\u00e4nen baden,", "tokens": ["Sich", "in", "tau\u00b7send", "Thr\u00e4\u00b7nen", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt nur Schaden", "tokens": ["Bringt", "nur", "Scha\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und verl\u00f6scht der Jugend Licht.", "tokens": ["Und", "ver\u00b7l\u00f6scht", "der", "Ju\u00b7gend", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Seufzen wird zum Winde;", "tokens": ["Un\u00b7ser", "Seuf\u00b7zen", "wird", "zum", "Win\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie geschwinde", "tokens": ["Wie", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word"], "pos": ["PWAV", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Aendert sich der Himmel nicht!", "tokens": ["A\u00b7en\u00b7dert", "sich", "der", "Him\u00b7mel", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Heute will er Hagel streuen,", "tokens": ["Heu\u00b7te", "will", "er", "Ha\u00b7gel", "streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feuer dr\u00e4uen;", "tokens": ["Feu\u00b7er", "dr\u00e4u\u00b7en", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Bald gew\u00e4hrt er Sonnenschein;", "tokens": ["Bald", "ge\u00b7w\u00e4hrt", "er", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Manches Irrlicht voller Sorgen", "tokens": ["Man\u00b7ches", "Irr\u00b7licht", "vol\u00b7ler", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird uns morgen", "tokens": ["Wird", "uns", "mor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ein bequemer Leitstern sein.", "tokens": ["Ein", "be\u00b7que\u00b7mer", "Leits\u00b7tern", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bei verkehrtem Spiele singen,", "tokens": ["Bei", "ver\u00b7kehr\u00b7tem", "Spie\u00b7le", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich bezwingen,", "tokens": ["Sich", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Reden, was uns nicht gef\u00e4llt,", "tokens": ["Re\u00b7den", ",", "was", "uns", "nicht", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und bei tr\u00fcbem Geist und Sinnen", "tokens": ["Und", "bei", "tr\u00fc\u00b7bem", "Geist", "und", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Scherzen k\u00f6nnen,", "tokens": ["Scher\u00b7zen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ist ein Schatz der klugen Welt.", "tokens": ["Ist", "ein", "Schatz", "der", "klu\u00b7gen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ueber das Verh\u00e4ngni\u00df klagen,", "tokens": ["Ue\u00b7ber", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehrt die Plagen", "tokens": ["Mehrt", "die", "Pla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und verr\u00e4th die Ungeduld;", "tokens": ["Und", "ver\u00b7r\u00e4\u00b7th", "die", "Un\u00b7ge\u00b7duld", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Solchem, der mit gleichem Herzen", "tokens": ["Sol\u00b7chem", ",", "der", "mit", "glei\u00b7chem", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00e4gt die Schmerzen,", "tokens": ["Tr\u00e4gt", "die", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Wird der Himmel endlich hold.", "tokens": ["Wird", "der", "Him\u00b7mel", "end\u00b7lich", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf, o Seele, du mu\u00dft lernen,", "tokens": ["Auf", ",", "o", "See\u00b7le", ",", "du", "mu\u00dft", "ler\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "FM", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Sternen,", "tokens": ["Oh\u00b7ne", "Ster\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wenn das Wetter tobt und bricht,", "tokens": ["Wenn", "das", "Wet\u00b7ter", "tobt", "und", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der N\u00e4chte schwarze Decken", "tokens": ["Wenn", "der", "N\u00e4ch\u00b7te", "schwar\u00b7ze", "De\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Uns erschrecken,", "tokens": ["Uns", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Dir zu sein dein eigen Licht.", "tokens": ["Dir", "zu", "sein", "dein", "ei\u00b7gen", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VAINF", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Du mu\u00dft dich in dir erg\u00f6tzen", "tokens": ["Du", "mu\u00dft", "dich", "in", "dir", "er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Sch\u00e4tzen,", "tokens": ["Mit", "den", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die kein Feind zu nichte macht", "tokens": ["Die", "kein", "Feind", "zu", "nich\u00b7te", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "PIS", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kein falscher Freund kann kr\u00e4nken", "tokens": ["Und", "kein", "fal\u00b7scher", "Freund", "kann", "kr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit den R\u00e4nken,", "tokens": ["Mit", "den", "R\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Die sein leichter Sinn erdacht.", "tokens": ["Die", "sein", "leich\u00b7ter", "Sinn", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Von der s\u00fc\u00dfen Kost zu scheiden", "tokens": ["Von", "der", "s\u00fc\u00b7\u00dfen", "Kost", "zu", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zu meiden,", "tokens": ["Und", "zu", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Was des Geistes Trieb begehrt,", "tokens": ["Was", "des", "Geis\u00b7tes", "Trieb", "be\u0