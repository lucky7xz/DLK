{"dta.poem.270": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie sehnet sich nach ihrem abwesenden  \n Liebsten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Fliegt ihr Seuffzer/ gleich dem Winde/", "tokens": ["Fliegt", "ihr", "Seuff\u00b7zer", "/", "gleich", "dem", "Win\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch zu meinem Schatze hin/", "tokens": ["Doch", "zu", "mei\u00b7nem", "Schat\u00b7ze", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eilet fort/ ach! eilt geschwinde/", "tokens": ["Ei\u00b7let", "fort", "/", "ach", "!", "eilt", "ge\u00b7schwin\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$(", "XY", "$.", "VVFIN", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und beschreibt ihm meinen Sinn;", "tokens": ["Und", "be\u00b7schreibt", "ihm", "mei\u00b7nen", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Saget ihm/ da\u00df ich mein Leben", "tokens": ["Sa\u00b7get", "ihm", "/", "da\u00df", "ich", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seinem Leben \u00fcbergeben.", "tokens": ["Sei\u00b7nem", "Le\u00b7ben", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tausend Stunden sind gez\u00e4hlet/", "tokens": ["Tau\u00b7send", "Stun\u00b7den", "sind", "ge\u00b7z\u00e4h\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um ihn wiederum zu sehn/", "tokens": ["Um", "ihn", "wie\u00b7de\u00b7rum", "zu", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber/ ach! die rechte fehlet/", "tokens": ["A\u00b7ber", "/", "ach", "!", "die", "rech\u00b7te", "feh\u00b7let", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "XY", "$.", "ART", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Vergn\u00fcgung anzugehn", "tokens": ["Die", "Ver\u00b7gn\u00fc\u00b7gung", "an\u00b7zu\u00b7gehn"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVIZU"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meinen Liebsten auf den Wangen", "tokens": ["Mei\u00b7nen", "Liebs\u00b7ten", "auf", "den", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebreich k\u00fcssend zu umfangen.", "tokens": ["Lieb\u00b7reich", "k\u00fcs\u00b7send", "zu", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mahlt die Gr\u00f6sse meiner Liebe", "tokens": ["Mahlt", "die", "Gr\u00f6s\u00b7se", "mei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jhm als einen Atlas f\u00fcr/", "tokens": ["Jhm", "als", "ei\u00b7nen", "At\u00b7las", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "NN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt ihm/ wie ich mich betr\u00fcbe/", "tokens": ["Sagt", "ihm", "/", "wie", "ich", "mich", "be\u00b7tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PWAV", "PPER", "PRF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringet ihn doch her zu mir/", "tokens": ["Brin\u00b7get", "ihn", "doch", "her", "zu", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df die Seele meiner Seelen", "tokens": ["Da\u00df", "die", "See\u00b7le", "mei\u00b7ner", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hemme meines Hertzens-Qu\u00e4len.", "tokens": ["Hem\u00b7me", "mei\u00b7nes", "Hert\u00b7zens\u00b7Qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}