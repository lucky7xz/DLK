{"textgrid.poem.55303": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Gegenseitig", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sitzt mir das Liebchen?", "tokens": ["Wie", "sitzt", "mir", "das", "Lieb\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Was freut sie so gro\u00df?", "tokens": ["Was", "freut", "sie", "so", "gro\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Den Fernen, sie wiegt ihn,", "tokens": ["Den", "Fer\u00b7nen", ",", "sie", "wiegt", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sie hat ihn im Scho\u00df;", "tokens": ["Sie", "hat", "ihn", "im", "Scho\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Im zierlichen K\u00e4fig", "tokens": ["Im", "zier\u00b7li\u00b7chen", "K\u00e4\u00b7fig"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein V\u00f6glein sie h\u00e4lt,", "tokens": ["Ein", "V\u00f6\u00b7glein", "sie", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie l\u00e4\u00dft es herau\u00dfer,", "tokens": ["Sie", "l\u00e4\u00dft", "es", "her\u00b7au\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So wie's ihr gef\u00e4llt.", "tokens": ["So", "wie's", "ihr", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Hat's Picken dem Finger,", "tokens": ["Hat's", "Pi\u00b7cken", "dem", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den Lippen getan,", "tokens": ["Den", "Lip\u00b7pen", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es flieget und flattert,", "tokens": ["Es", "flie\u00b7get", "und", "flat\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und wieder heran.", "tokens": ["Und", "wie\u00b7der", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "So eile zur Heimat,", "tokens": ["So", "ei\u00b7le", "zur", "Hei\u00b7mat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das ist nun der Brauch,", "tokens": ["Das", "ist", "nun", "der", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS",