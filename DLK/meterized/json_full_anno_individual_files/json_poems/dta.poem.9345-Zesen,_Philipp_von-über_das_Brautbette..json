{"dta.poem.9345": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "\u00fcber das Brautbette.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Djs ist das zelt/ wo Lieb und Lust sich letzet/", "tokens": ["Djs", "ist", "das", "zelt", "/", "wo", "Lieb", "und", "Lust", "sich", "let\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PDS", "VVFIN", "$(", "PWAV", "NN", "KON", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dis ist das grab der sch\u00f6nen Jun gferschafft/", "tokens": ["dis", "ist", "das", "grab", "der", "sch\u00f6\u00b7nen", "Jun", "gfer\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die durch den pfeil der Liebe weggerafft;", "tokens": ["die", "durch", "den", "pfeil", "der", "Lie\u00b7be", "weg\u00b7ge\u00b7rafft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dis ist das feld/ wo Venus jagt und hetzet.", "tokens": ["Dis", "ist", "das", "feld", "/", "wo", "Ve\u00b7nus", "jagt", "und", "het\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "PWAV", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}}}}