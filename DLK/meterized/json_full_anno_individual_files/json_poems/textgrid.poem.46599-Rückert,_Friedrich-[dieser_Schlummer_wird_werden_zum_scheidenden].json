{"textgrid.poem.46599": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[dieser Schlummer wird werden zum scheidenden]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dieser Schlummer wird werden zum scheidenden", "tokens": ["Die\u00b7ser", "Schlum\u00b7mer", "wird", "wer\u00b7den", "zum", "schei\u00b7den\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "VAINF", "APPRART", "ADJA"], "meter": "+-+-++--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Zwischen dem Leiden und unserem Leidenden;", "tokens": ["Zwi\u00b7schen", "dem", "Lei\u00b7den", "und", "un\u00b7se\u00b7rem", "Lei\u00b7den\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.3": {"text": "Sei es nun, da\u00df er hin\u00fcber ihn f\u00e4chele,", "tokens": ["Sei", "es", "nun", ",", "da\u00df", "er", "hin\u00b7\u00fc\u00b7ber", "ihn", "f\u00e4\u00b7che\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sei's, da\u00df er wiedererwachend uns l\u00e4chele,", "tokens": ["Sei's", ",", "da\u00df", "er", "wie\u00b7de\u00b7rer\u00b7wa\u00b7chend", "uns", "l\u00e4\u00b7che\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "VVPP", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.5": {"text": "Was auch vom s\u00e4uselnden werde beschieden,", "tokens": ["Was", "auch", "vom", "s\u00e4u\u00b7seln\u00b7den", "wer\u00b7de", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gott ist im S\u00e4useln, \u2013 wir harren in Frieden.", "tokens": ["Gott", "ist", "im", "S\u00e4u\u00b7seln", ",", "\u2013", "wir", "har\u00b7ren", "in", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "$,", "$(", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Dieser Schlummer wird werden zum scheidenden", "tokens": ["Die\u00b7ser", "Schlum\u00b7mer", "wird", "wer\u00b7den", "zum", "schei\u00b7den\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "VAINF", "APPRART", "ADJA"], "meter": "+-+-++--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Zwischen dem Leiden und unserem Leidenden;", "tokens": ["Zwi\u00b7schen", "dem", "Lei\u00b7den", "und", "un\u00b7se\u00b7rem", "Lei\u00b7den\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.3": {"text": "Sei es nun, da\u00df er hin\u00fcber ihn f\u00e4chele,", "tokens": ["Sei", "es", "nun", ",", "da\u00df", "er", "hin\u00b7\u00fc\u00b7ber", "ihn", "f\u00e4\u00b7che\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sei's, da\u00df er wiedererwachend uns l\u00e4chele,", "tokens