{"dta.poem.20606": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Verg\u00e4nglichkeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Er het der Pfn\u00fcsel! Seig doch nit so", "tokens": ["Er", "het", "der", "Pf\u00b7n\u00fc\u00b7sel", "!", "Seig", "doch", "nit", "so"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NE", "ADV", "PTKNEG", "ADV"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "n\u00e4rsch!", "tokens": ["n\u00e4rsch", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "H\u00fcst Laubi, Merz! &#8212; und lo\u00df die Tod-               ", "tokens": ["H\u00fcst", "Lau\u00b7bi", ",", "Merz", "!", "&#8212;", "und", "lo\u00df", "die", "To\u00b7d"], "token_info": ["word", "word", "punct", "word", "punct", "XML_entity", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "$,", "NN", "$.", "$(", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "te go,", "tokens": ["te", "go", ","], "token_info": ["word", "word", "punct"], "pos": ["FM", "FM", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "\u2019s sin Nare-Posse! \u2014 Je, was hani gseit?", "tokens": ["'s", "sin", "Na\u00b7re\u00b7Pos\u00b7se", "!", "Je", ",", "was", "ha\u00b7ni", "gseit", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$.", "$(", "ADV", "$,", "PWS", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Vo Basel, a\u00df es au e mol verfallt. \u2014", "tokens": ["Vo", "Ba\u00b7sel", ",", "a\u00df", "es", "au", "e", "mol", "ver\u00b7fallt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "PPER", "NE", "NE", "NE", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und goht in langer Zit e Wanders-Ma", "tokens": ["Und", "goht", "in", "lan\u00b7ger", "Zit", "e", "Wan\u00b7der\u00b7sMa"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "ne halbi Stund, e Stund wit dra verbey", "tokens": ["ne", "hal\u00b7bi", "Stund", ",", "e", "Stund", "wit", "dra", "ver\u00b7bey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.9": {"text": "se luegt er dure, lit ke Nebel druf,", "tokens": ["se", "luegt", "er", "du\u00b7re", ",", "lit", "ke", "Ne\u00b7bel", "druf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "ADJD", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "und seit si\u2019m Camerad, wo mittem goht:", "tokens": ["und", "seit", "si'm", "Ca\u00b7me\u00b7rad", ",", "wo", "mit\u00b7tem", "goht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NE", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "\u201elueg, d\u00f6rt isch Basel gstande! Selle Thurn", "tokens": ["\u201e", "lu\u00b7eg", ",", "d\u00f6rt", "isch", "Ba\u00b7sel", "gstan\u00b7de", "!", "Sel\u00b7le", "Thurn"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "ADJD", "NN", "ADJA", "$.", "NE", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "\u201eisch d\u2019 Peters-Chilche gsi, \u2019s isch schad", "tokens": ["\u201e", "isch", "d'", "Pe\u00b7ter\u00b7sChil\u00b7che", "gsi", ",", "'s", "isch", "schad"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "FM", "FM", "FM", "FM", "$,", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "derf\u00fcr!\u201c", "tokens": ["der\u00b7f\u00fcr", "!", "\u201c"], "token_info": ["word", "punct", "punct"], "pos": ["PAV", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}}}}