{"dta.poem.8675": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Des Himmels frohes Antlitz brannte", "tokens": ["Des", "Him\u00b7mels", "fro\u00b7hes", "Ant\u00b7litz", "brann\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon von des Tages erstem Ku\u00df,", "tokens": ["Schon", "von", "des", "Ta\u00b7ges", "ers\u00b7tem", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und durch das Morgensternlein sandte", "tokens": ["Und", "durch", "das", "Mor\u00b7gens\u00b7tern\u00b7lein", "sand\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Die Nacht mir ihren Scheidegru\u00df:", "tokens": ["Die", "Nacht", "mir", "ih\u00b7ren", "Schei\u00b7de\u00b7gru\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da griff ich nach dem Wanderstabe,", "tokens": ["Da", "griff", "ich", "nach", "dem", "Wan\u00b7der\u00b7sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach meinem Wirthe: \u201eGott vergelt'", "tokens": ["Sprach", "mei\u00b7nem", "Wirt\u00b7he", ":", "\u201e", "Gott", "ver\u00b7gelt'"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$.", "$(", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ruhestatt, die milde Labe!\u201c", "tokens": ["Die", "Ru\u00b7he\u00b7statt", ",", "die", "mil\u00b7de", "La\u00b7be", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zog lustig weiter in die Welt.", "tokens": ["Zog", "lus\u00b7tig", "wei\u00b7ter", "in", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}