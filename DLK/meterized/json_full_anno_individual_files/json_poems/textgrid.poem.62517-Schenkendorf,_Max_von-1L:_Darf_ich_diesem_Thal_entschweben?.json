": "Die ihr mir entgegen tragt!", "tokens": ["Die", "ihr", "mir", "ent\u00b7ge\u00b7gen", "tragt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob ich auch im Himmel wohne,", "tokens": ["Ob", "ich", "auch", "im", "Him\u00b7mel", "woh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig bin ich Gottes Magd.", "tokens": ["E\u00b7wig", "bin", "ich", "Got\u00b7tes", "Magd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Weht und spielt ihr s\u00fc\u00dfen D\u00fcfte,", "tokens": ["Weht", "und", "spielt", "ihr", "s\u00fc\u00b7\u00dfen", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lilien, bl\u00fchet um mein Grab,", "tokens": ["Li\u00b7li\u00b7en", ",", "bl\u00fc\u00b7het", "um", "mein", "Grab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Engelsboten, Liebesl\u00fcfte,", "tokens": ["En\u00b7gels\u00b7bo\u00b7ten", ",", "Lie\u00b7bes\u00b7l\u00fcf\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traget meinen Gru\u00df hinab!", "tokens": ["Tra\u00b7get", "mei\u00b7nen", "Gru\u00df", "hin\u00b7ab", "!"], "token_info": ["