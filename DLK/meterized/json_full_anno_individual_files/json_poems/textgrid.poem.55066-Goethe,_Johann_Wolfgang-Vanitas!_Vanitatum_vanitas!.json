{"textgrid.poem.55066": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Vanitas! Vanitatum vanitas!", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab mein Sach auf Nichts gestellt.", "tokens": ["Ich", "hab", "mein", "Sach", "auf", "Nichts", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Juchhe!", "tokens": ["Juch\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Drum ist's so wohl mir in der Welt.", "tokens": ["Drum", "ist's", "so", "wohl", "mir", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "ADV", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Juchhe!", "tokens": ["Juch\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und wer will mein Kamerade sein,", "tokens": ["Und", "wer", "will", "mein", "Ka\u00b7me\u00b7ra\u00b7de", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der sto\u00dfe mit an, der stimme mit ein", "tokens": ["Der", "sto\u00b7\u00dfe", "mit", "an", ",", "der", "stim\u00b7me", "mit", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "PTKVZ", "$,", "PRELS", "VVFIN", "APPR", "ART"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Bei dieser Neige Wein.", "tokens": ["Bei", "die\u00b7ser", "Nei\u00b7ge", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich stellt mein Sach auf Geld und Gut.", "tokens": ["Ich", "stellt", "mein", "Sach", "auf", "Geld", "und", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Juchhe!", "tokens": ["Juch\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Dar\u00fcber verlor ich Freud und Mut.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "ver\u00b7lor", "ich", "Freud", "und", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O weh!", "tokens": ["O", "weh", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Die M\u00fcnze rollte hier und dort,", "tokens": ["Die", "M\u00fcn\u00b7ze", "roll\u00b7te", "hier", "und", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hascht ich sie an einem Ort,", "tokens": ["Und", "hascht", "ich", "sie", "an", "ei\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Am andern war sie fort.", "tokens": ["Am", "an\u00b7dern", "war", "sie", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf Weiber stellt ich nun mein Sach.", "tokens": ["Auf", "Wei\u00b7ber", "stellt", "ich", "nun", "mein", "Sach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Juchhe!", "tokens": ["Juch\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Daher mir kam viel Ungemach.", "tokens": ["Da\u00b7her", "mir", "kam", "viel", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O weh!", "tokens": ["O", "weh", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Die Falsche sucht' sich ein ander Teil,", "tokens": ["Die", "Fal\u00b7sche", "sucht'", "sich", "ein", "an\u00b7der", "Teil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Treue macht' mir Langeweil:", "tokens": ["Die", "Treu\u00b7e", "macht'", "mir", "Lan\u00b7ge\u00b7weil", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Beste war nicht feil.", "tokens": ["Die", "Bes\u00b7te", "war", "nicht", "feil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich stellt mein Sach auf Reis' und Fahrt.", "tokens": ["Ich", "stellt", "mein", "Sach", "auf", "Reis'", "und", "Fahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Juchhe!", "tokens": ["Juch\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und lie\u00df meine Vaterlandesart.", "tokens": ["Und", "lie\u00df", "mei\u00b7ne", "Va\u00b7ter\u00b7lan\u00b7des\u00b7art", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O weh!", "tokens": ["O", "weh", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Und mir behagt' es nirgends recht,", "tokens": ["Und", "mir", "be\u00b7hagt'", "es", "nir\u00b7gends", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Kost war fremd, das Bett war schlecht,", "tokens": ["Die", "Kost", "war", "fremd", ",", "das", "Bett", "war", "schlecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Niemand verstand mich recht.", "tokens": ["Nie\u00b7mand", "ver\u00b7stand", "mich", "recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Ich stellt mein Sach auf Ruhm und Ehr.", "tokens": ["Ich", "stellt", "mein", "Sach", "auf", "Ruhm", "und", "Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Juchhe!", "tokens": ["Juch\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und sieh! gleich hatt ein andrer mehr.", "tokens": ["Und", "sieh", "!", "gleich", "hatt", "ein", "an\u00b7drer", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ADV", "VAFIN", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O weh!", "tokens": ["O", "weh", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Wie ich mich hatt hervorgetan,", "tokens": ["Wie", "ich", "mich", "hatt", "her\u00b7vor\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da sahen die Leute scheel mich an,", "tokens": ["Da", "sa\u00b7hen", "die", "Leu\u00b7te", "scheel", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Hatte keinem recht getan.", "tokens": ["Hat\u00b7te", "kei\u00b7nem", "recht", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich setzt mein Sach auf Kampf und Krieg.", "tokens": ["Ich", "setzt", "mein", "Sach", "auf", "Kampf", "und", "Krieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Juchhe!", "tokens": ["Juch\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und uns gelang so mancher Sieg.", "tokens": ["Und", "uns", "ge\u00b7lang", "so", "man\u00b7cher", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Juchhe!", "tokens": ["Juch\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wir zogen in Feindes Land hinein,", "tokens": ["Wir", "zo\u00b7gen", "in", "Fein\u00b7des", "Land", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dem Freunde sollt's nicht viel besser sein,", "tokens": ["Dem", "Freun\u00b7de", "sollt's", "nicht", "viel", "bes\u00b7ser", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und ich verlor ein Bein.", "tokens": ["Und", "ich", "ver\u00b7lor", "ein", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nun hab ich mein Sach auf Nichts gestellt.", "tokens": ["Nun", "hab", "ich", "mein", "Sach", "auf", "Nichts", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Juchhe!", "tokens": ["Juch\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und mein geh\u00f6rt die ganze Welt.", "tokens": ["Und", "mein", "ge\u00b7h\u00f6rt", "die", "gan\u00b7ze", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Juchhe!", "tokens": ["Juch\u00b7he", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Zu Ende geht nun Sang und Schmaus.", "tokens": ["Zu", "En\u00b7de", "geht", "nun", "Sang", "und", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur trinkt mir alle Neigen aus;", "tokens": ["Nur", "trinkt", "mir", "al\u00b7le", "Nei\u00b7gen", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die letzte mu\u00df heraus!", "tokens": ["Die", "letz\u00b7te", "mu\u00df", "he\u00b7raus", "!"], "token_info": ["word", "wo