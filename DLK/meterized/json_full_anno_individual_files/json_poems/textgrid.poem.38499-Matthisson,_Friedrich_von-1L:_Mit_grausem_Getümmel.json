": ["Der", "Schmet\u00b7ter\u00b7ling", "trinkt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.13": {"line.1": {"text": "Die Freude flog wieder", "tokens": ["Die", "Freu\u00b7de", "flog", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf buntem Gefieder", "tokens": ["Auf", "bun\u00b7tem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Den Sterblichen zu;", "tokens": ["Den", "Sterb\u00b7li\u00b7chen", "zu", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ihr himmlisches Walten", "tokens": ["Ihr", "himm\u00b7li\u00b7sches", "Wal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Verwischte die Falten", "tokens": ["Ver\u00b7wischte", "die", "Fal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Der Stirnen im Nu!", "tokens": ["Der", "Stir\u00b7nen", "im", "Nu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.14": {"line.1": {"text": "Vom einsamen R\u00e4dchen", "tokens": ["Vom", "ein\u00b7sa\u00b7men", "R\u00e4d\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Entf\u00fchrt sie die M\u00e4dchen", "tokens": ["Ent\u00b7f\u00fchrt", "sie", "die", "M\u00e4d\u00b7chen"], "token_info": ["