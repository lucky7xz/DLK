{"dta.poem.2626": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ehren-Stern  \n Hn. M. R. v. L. d. R. in B. den 9. Mar-  \n tii 1670.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die ihr das grosse Rund durch GOttes Arm geschaffen/", "tokens": ["Die", "ihr", "das", "gros\u00b7se", "Rund", "durch", "Got\u00b7tes", "Arm", "ge\u00b7schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In jedem Augenblick verwundernd schauet an/", "tokens": ["In", "je\u00b7dem", "Au\u00b7gen\u00b7blick", "ver\u00b7wun\u00b7dernd", "schau\u00b7et", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sagt/ ob es thulich sey hierin sich zu vergaffen/", "tokens": ["Sagt", "/", "ob", "es", "thu\u00b7lich", "sey", "hie\u00b7rin", "sich", "zu", "ver\u00b7gaf\u00b7fen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "ADV", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und/ ob nicht r\u00fchmlicher zu gehn die Himmels-Bahn?", "tokens": ["Und", "/", "ob", "nicht", "r\u00fchm\u00b7li\u00b7cher", "zu", "gehn", "die", "Him\u00b7mels\u00b7Bahn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PTKNEG", "ADJD", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier findet ihr Verdru\u00df und tausend Eitelkeiten", "tokens": ["Hier", "fin\u00b7det", "ihr", "Ver\u00b7dru\u00df", "und", "tau\u00b7send", "Ei\u00b7tel\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo nichts als Tand und Wahn/ und ein verlarvtes Spiel,", "tokens": ["Wo", "nichts", "als", "Tand", "und", "Wahn", "/", "und", "ein", "ver\u00b7larv\u00b7tes", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "KOKOM", "NN", "KON", "NN", "$(", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dort gehen stetig auff gew\u00fcnschte Freuden-Zeiten", "tokens": ["Dort", "ge\u00b7hen", "ste\u00b7tig", "auff", "ge\u00b7w\u00fcnschte", "Freu\u00b7den\u00b7Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Wo die Unendlichkeit sich setzet zu dem Ziel;", "tokens": ["Wo", "die", "Un\u00b7end\u00b7lich\u00b7keit", "sich", "set\u00b7zet", "zu", "dem", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hir glimmt ein tunckel Tacht und wird gar bald verzehret/", "tokens": ["Hir", "glimmt", "ein", "tun\u00b7ckel", "Tacht", "und", "wird", "gar", "bald", "ver\u00b7zeh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "KON", "VAFIN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Flammen rothe Gluth ist ein verlodernd Schein;", "tokens": ["Der", "Flam\u00b7men", "ro\u00b7the", "Gluth", "ist", "ein", "ver\u00b7lo\u00b7dernd", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ART", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dort wird zur Opfferung ein reines Feur gewehret/", "tokens": ["Dort", "wird", "zur", "Opf\u00b7fe\u00b7rung", "ein", "rei\u00b7nes", "Feur", "ge\u00b7weh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und mu\u00df an hellem Glantz den Sternen gleiche seyn.", "tokens": ["Und", "mu\u00df", "an", "hel\u00b7lem", "Glantz", "den", "Ster\u00b7nen", "glei\u00b7che", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN", "ART", "NN", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Werfft ab die S\u00fcnden-Last und irrdische Gedancken/", "tokens": ["Werfft", "ab", "die", "S\u00fcn\u00b7den\u00b7Last", "und", "irr\u00b7di\u00b7sche", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die euren Sinn gar leicht vom Himmel abwerts zichn/", "tokens": ["Die", "eu\u00b7ren", "Sinn", "gar", "leicht", "vom", "Him\u00b7mel", "ab\u00b7werts", "zichn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "APPRART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Seht auff und \u00fcber euch/ wie in gesetztem Schrancken", "tokens": ["Seht", "auff", "und", "\u00fc\u00b7ber", "euch", "/", "wie", "in", "ge\u00b7setz\u00b7tem", "Schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "APPR", "PPER", "$(", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sonn\u2019/ Mond und Sterne sich in ihrem Lauff bem\u00fchn/", "tokens": ["Sonn'", "/", "Mond", "und", "Ster\u00b7ne", "sich", "in", "ih\u00b7rem", "Lauff", "be\u00b7m\u00fchn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Jhr werdet alsobald die W\u00fcrckungen empfinden", "tokens": ["Ihr", "wer\u00b7det", "al\u00b7so\u00b7bald", "die", "W\u00fcr\u00b7ckun\u00b7gen", "emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Und was vor Eigenschafft in eu\u2019ren C\u00f6rpern sey;", "tokens": ["Und", "was", "vor", "Ei\u00b7gen\u00b7schafft", "in", "eu'\u00b7ren", "C\u00f6r\u00b7pern", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch d\u00f6rffen wir hier nicht voll Aberwitz ergrunden", "tokens": ["Doch", "d\u00f6rf\u00b7fen", "wir", "hier", "nicht", "voll", "A\u00b7ber\u00b7witz", "er\u00b7grun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und Epicurus Schlu\u00df unschl\u00fc\u00dflich stimmen bey;", "tokens": ["Und", "E\u00b7pi\u00b7cu\u00b7rus", "Schlu\u00df", "un\u00b7schl\u00fc\u00df\u00b7lich", "stim\u00b7men", "bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es sey des Menschen Seel vom Sch\u00f6pffer so gebohren/", "tokens": ["Es", "sey", "des", "Men\u00b7schen", "Seel", "vom", "Sch\u00f6pf\u00b7fer", "so", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "APPRART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df ein besonder Stern mit der Geburt auffgeh/", "tokens": ["Da\u00df", "ein", "be\u00b7son\u00b7der", "Stern", "mit", "der", "Ge\u00b7burt", "auff\u00b7geh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wem Gold und G\u00fcter hier und Reichthum auserkohren/", "tokens": ["Wem", "Gold", "und", "G\u00fc\u00b7ter", "hier", "und", "Reicht\u00b7hum", "au\u00b7ser\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "ADV", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dem sey ein sch\u00f6ner Stern gesetzet in die H\u00f6h/", "tokens": ["Dem", "sey", "ein", "sch\u00f6\u00b7ner", "Stern", "ge\u00b7set\u00b7zet", "in", "die", "H\u00f6h", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein Armer habe nur ein kleines Licht zu schauen/", "tokens": ["Ein", "Ar\u00b7mer", "ha\u00b7be", "nur", "ein", "klei\u00b7nes", "Licht", "zu", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Noth und Mangel sp\u00fcrt/ seh\u2019 tunckles glimmen an;", "tokens": ["Der", "Noth", "und", "Man\u00b7gel", "sp\u00fcrt", "/", "seh'", "tunck\u00b7les", "glim\u00b7men", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$(", "VVFIN", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wem aber wolte nicht vor solcher Narrheit grauen", "tokens": ["Wem", "a\u00b7ber", "wol\u00b7te", "nicht", "vor", "sol\u00b7cher", "Nar\u00b7rheit", "grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VMFIN", "PTKNEG", "APPR", "PIAT", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die nur mit L\u00e4sterung beschimpfft des Himmels Bahn.", "tokens": ["Die", "nur", "mit", "L\u00e4s\u00b7te\u00b7rung", "be\u00b7schimpfft", "des", "Him\u00b7mels", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wir/ die des Vatern Glantz und helles Wort erleuchtet", "tokens": ["Wir", "/", "die", "des", "Va\u00b7tern", "Glantz", "und", "hel\u00b7les", "Wort", "er\u00b7leuch\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "ART", "ART", "NN", "NN", "KON", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Verstehen aus der Schrifft/ da\u00df die den Sternen gleich/", "tokens": ["Ver\u00b7ste\u00b7hen", "aus", "der", "Schrifft", "/", "da\u00df", "die", "den", "Ster\u00b7nen", "gleich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "KOUS", "ART", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So mit Gerechtigkeit gleich einem Strom befeuchtet/", "tokens": ["So", "mit", "Ge\u00b7rech\u00b7tig\u00b7keit", "gleich", "ei\u00b7nem", "Strom", "be\u00b7feuch\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die werden stehen dort in GOttes Gnaden-Reich.", "tokens": ["Die", "wer\u00b7den", "ste\u00b7hen", "dort", "in", "Got\u00b7tes", "Gna\u00b7den\u00b7Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAINF", "VVFIN", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Hier gl\u00e4ntzt ", "tokens": ["Hier", "gl\u00e4ntzt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.34": {"text": "Die Straalen schiessen schon vom Firmament herab/", "tokens": ["Die", "Straa\u00b7len", "schies\u00b7sen", "schon", "vom", "Fir\u00b7ma\u00b7ment", "her\u00b7ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ob bey der Reinigung man gleich die Wangen netzet/", "tokens": ["Ob", "bey", "der", "Rei\u00b7ni\u00b7gung", "man", "gleich", "die", "Wan\u00b7gen", "net\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PIS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und den unreinen Leib verscharret in das Grab.", "tokens": ["Und", "den", "un\u00b7rei\u00b7nen", "Leib", "ver\u00b7schar\u00b7ret", "in", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So bald der Morgen-Stern im Hertzen auffgegangen", "tokens": ["So", "bald", "der", "Mor\u00b7gen\u00b7S\u00b7tern", "im", "Hert\u00b7zen", "auff\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und dieses Tage-Licht die zarte Frucht gesehn/", "tokens": ["Und", "die\u00b7ses", "Ta\u00b7ge\u00b7Licht", "die", "zar\u00b7te", "Frucht", "ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So bald trug Vater-Treu/ und Mutter-Lust Verlangen", "tokens": ["So", "bald", "trug", "Va\u00b7ter\u00b7Treu", "/", "und", "Mut\u00b7ter\u00b7Lust", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "NN", "$(", "KON", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und w\u00fcnschten Freud und Lust/ die ihnen auch geschehn;", "tokens": ["Und", "w\u00fcnschten", "Freud", "und", "Lust", "/", "die", "ih\u00b7nen", "auch", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$(", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.41": {"text": "Worauff als der Verstand der Jahre sich genahet", "tokens": ["Wo\u00b7rauff", "als", "der", "Ver\u00b7stand", "der", "Jah\u00b7re", "sich", "ge\u00b7na\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "KOUS", "ART", "NN", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und Klugheit ihren Sitz zu suchen sich bem\u00fcht/", "tokens": ["Und", "Klug\u00b7heit", "ih\u00b7ren", "Sitz", "zu", "su\u00b7chen", "sich", "be\u00b7m\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "PTKZU", "VVFIN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Da wurde gleich hierauff von beyderseits bejahet", "tokens":