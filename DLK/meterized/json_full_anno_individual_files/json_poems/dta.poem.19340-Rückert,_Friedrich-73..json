{"dta.poem.19340": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So sprach zum Adlichen, der mit den Ahnen prahlte,", "tokens": ["So", "sprach", "zum", "Ad\u00b7li\u00b7chen", ",", "der", "mit", "den", "Ah\u00b7nen", "prahl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der B\u00fcrgerliche, der mit seinem Werthe zahlte:", "tokens": ["Der", "B\u00fcr\u00b7ger\u00b7li\u00b7che", ",", "der", "mit", "sei\u00b7nem", "Wert\u00b7he", "zahl\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn du Vorrechte hast, so sei derselben werth;", "tokens": ["Wenn", "du", "Vor\u00b7rech\u00b7te", "hast", ",", "so", "sei", "der\u00b7sel\u00b7ben", "werth", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$,", "ADV", "VAFIN", "PDAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steck' ein die Zung' und zieh f\u00fcrs Vaterland das Schwert.", "tokens": ["Steck'", "ein", "die", "Zung'", "und", "zieh", "f\u00fcrs", "Va\u00b7ter\u00b7land", "das", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn deine V\u00e4ter all gut waren, sei nicht schlechter;", "tokens": ["Wenn", "dei\u00b7ne", "V\u00e4\u00b7ter", "all", "gut", "wa\u00b7ren", ",", "sei", "nicht", "schlech\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIAT", "ADJD", "VAFIN", "$,", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sind sie ungerecht gewesen, sei gerechter.", "tokens": ["Und", "sind", "sie", "un\u00b7ge\u00b7recht", "ge\u00b7we\u00b7sen", ",", "sei", "ge\u00b7rech\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VAPP", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn Raub villeicht und Blut klebt am ererbten Gut,", "tokens": ["Wenn", "Raub", "vil\u00b7leicht", "und", "Blut", "klebt", "am", "er\u00b7erb\u00b7ten", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "KON", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mache durch Gebrauch das schlechterworbne gut.", "tokens": ["So", "ma\u00b7che", "durch", "Ge\u00b7brauch", "das", "schlech\u00b7ter\u00b7worb\u00b7ne", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Hab' ich nicht Ahnen auch? nur sind sie ungenannt;", "tokens": ["Hab'", "ich", "nicht", "Ah\u00b7nen", "auch", "?", "nur", "sind", "sie", "un\u00b7ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "NN", "ADV", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von deinen mancher w\u00e4r' auch besser ungekannt.", "tokens": ["Von", "dei\u00b7nen", "man\u00b7cher", "w\u00e4r'", "auch", "bes\u00b7ser", "un\u00b7ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PIAT", "VAFIN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die deinen konnten dir Erworbnes nur vererben;", "tokens": ["Die", "dei\u00b7nen", "konn\u00b7ten", "dir", "Er\u00b7worb\u00b7nes", "nur", "ver\u00b7er\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "VMFIN", "PPER", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die meinen lie\u00dfen Lust und Kraft mir, zu erwerben.", "tokens": ["Die", "mei\u00b7nen", "lie\u00b7\u00dfen", "Lust", "und", "Kraft", "mir", ",", "zu", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "KON", "NN", "PPER", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}