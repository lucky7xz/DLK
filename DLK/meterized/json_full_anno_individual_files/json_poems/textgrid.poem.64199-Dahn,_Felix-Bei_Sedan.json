{"textgrid.poem.64199": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Bei Sedan", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei Bazeilles, bei Balan hin und her,", "tokens": ["Bei", "Ba\u00b7zeil\u00b7les", ",", "bei", "Ba\u00b7lan", "hin", "und", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wie rangen doch meine Bayern schwer!", "tokens": ["Wie", "ran\u00b7gen", "doch", "mei\u00b7ne", "Bay\u00b7ern", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Da traf ich am Graben, im Sch\u00fctzenkampf, \u2013", "tokens": ["Da", "traf", "ich", "am", "Gra\u00b7ben", ",", "im", "Sch\u00fct\u00b7zen\u00b7kampf", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u2013 Kaum sah man die Br\u00fccke vor grauem Dampf \u2013", "tokens": ["\u2013", "Kaum", "sah", "man", "die", "Br\u00fc\u00b7cke", "vor", "grau\u00b7em", "Dampf", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIS", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Am zerschoss'nen Zaun, von dem Park nicht weit,", "tokens": ["Am", "zer\u00b7schoss'\u00b7nen", "Zaun", ",", "von", "dem", "Park", "nicht", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Den Hauptmann, den Freund aus der Jugendzeit!", "tokens": ["Den", "Haupt\u00b7mann", ",", "den", "Freund", "aus", "der", "Ju\u00b7gend\u00b7zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "\u00bbfreund Felix, du hast dein altes Gl\u00fcck!", "tokens": ["\u00bb", "freund", "Fe\u00b7lix", ",", "du", "hast", "dein", "al\u00b7tes", "Gl\u00fcck", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NE", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heut' schaust du des Krieges sch\u00f6nstes St\u00fcck!", "tokens": ["Heut'", "schaust", "du", "des", "Krie\u00b7ges", "sch\u00f6ns\u00b7tes", "St\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Die Sachsen, so hei\u00dft es, sind schon ganz nah: \u2013", "tokens": ["Die", "Sach\u00b7sen", ",", "so", "hei\u00dft", "es", ",", "sind", "schon", "ganz", "nah", ":", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.