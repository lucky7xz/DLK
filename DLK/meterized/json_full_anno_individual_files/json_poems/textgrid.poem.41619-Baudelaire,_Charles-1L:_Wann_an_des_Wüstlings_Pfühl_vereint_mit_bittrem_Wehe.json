{"textgrid.poem.41619": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann an des W\u00fcstlings Pf\u00fchl vereint mit bittrem Wehe", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann an des W\u00fcstlings Pf\u00fchl vereint mit bittrem Wehe", "tokens": ["Wann", "an", "des", "W\u00fcst\u00b7lings", "Pf\u00fchl", "ver\u00b7eint", "mit", "bitt\u00b7rem", "We\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der rosig-wei\u00dfe Schein der Fr\u00fche neu erwacht,", "tokens": ["Der", "ro\u00b7sig\u00b7wei\u00b7\u00dfe", "Schein", "der", "Fr\u00fc\u00b7he", "neu", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ists, als ob, geweckt durch r\u00e4cherische Macht,", "tokens": ["So", "ists", ",", "als", "ob", ",", "ge\u00b7weckt", "durch", "r\u00e4\u00b7che\u00b7ri\u00b7sche", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Engel wundersam im satten Tier erstehe.", "tokens": ["Ein", "En\u00b7gel", "wun\u00b7der\u00b7sam", "im", "sat\u00b7ten", "Tier", "er\u00b7ste\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Geahnter Himmel Zelt in fernentr\u00fccktem Blau", "tokens": ["Ge\u00b7ahn\u00b7ter", "Him\u00b7mel", "Zelt", "in", "fer\u00b7nen\u00b7tr\u00fcck\u00b7tem", "Blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vertieft sich und verlockt wie eines Abgrunds Schatten", "tokens": ["Ver\u00b7tieft", "sich", "und", "ver\u00b7lockt", "wie", "ei\u00b7nes", "Ab\u00b7grunds", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Menschen, der noch tr\u00e4umt in leidendem Ermatten.", "tokens": ["Den", "Men\u00b7schen", ",", "der", "noch", "tr\u00e4umt", "in", "lei\u00b7den\u00b7dem", "Er\u00b7mat\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So, g\u00f6ttlich Wesen, du, lichthelle, zarte Frau,", "tokens": ["So", ",", "g\u00f6tt\u00b7lich", "We\u00b7sen", ",", "du", ",", "licht\u00b7hel\u00b7le", ",", "zar\u00b7te", "Frau", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "NN", "$,", "PPER", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Schwebt auf der dumpfen Lust zerfallnen grauen Tr\u00fcmmern", "tokens": ["Schwebt", "auf", "der", "dum\u00b7pfen", "Lust", "zer\u00b7fall\u00b7nen", "grau\u00b7en", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor meinen Blicken, die sich weiten, immerdar", "tokens": ["Vor", "mei\u00b7nen", "Bli\u00b7cken", ",", "die", "sich", "wei\u00b7ten", ",", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein hold Gedenken, rosig, mild und klar.", "tokens": ["Dein", "hold", "Ge\u00b7den\u00b7ken", ",", "ro\u00b7sig", ",", "mild", "und", "klar", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Sonne Feuer schw\u00e4rzt der Kerzen n\u00e4chtig Flimmern;", "tokens": ["Der", "Son\u00b7ne", "Feu\u00b7er", "schw\u00e4rzt", "der", "Ker\u00b7zen", "n\u00e4ch\u00b7tig", "Flim\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So, lichte Seele, ist, verkl\u00e4rt und strahlenreich,", "tokens": ["So", ",", "lich\u00b7te", "See\u00b7le", ",", "ist", ",", "ver\u00b7kl\u00e4rt", "und", "strah\u00b7len\u00b7reich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VAFIN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein siegge