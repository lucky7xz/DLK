{"dta.poem.1170": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Mein allein/ oder la\u00df es gar seyn?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Beliebe mich f\u00fcr andern zu erwehlen/ ", "tokens": ["Be\u00b7lie\u00b7be", "mich", "f\u00fcr", "an\u00b7dern", "zu", "er\u00b7weh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Hertze giebt sich gantz zu eigen dir.", "tokens": ["Mein", "Hert\u00b7ze", "giebt", "sich", "gantz", "zu", "ei\u00b7gen", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "PTKA", "ADJD", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wo du dir ein Fremdes wirst verm\u00e4hlen/", "tokens": ["Doch", "wo", "du", "dir", "ein", "Frem\u00b7des", "wirst", "ver\u00b7m\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "ART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nehm ich das Mein hinwieder auch zu mir.", "tokens": ["Nehm", "ich", "das", "Mein", "hin\u00b7wie\u00b7der", "auch", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PPOSAT", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie sehr mich ie Gel\u00fcck und Himmel hasset/", "tokens": ["Wie", "sehr", "mich", "ie", "Ge\u00b7l\u00fcck", "und", "Him\u00b7mel", "has\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bleibt doch mein Hertz und meine Treue rein;", "tokens": ["Bleibt", "doch", "mein", "Hertz", "und", "mei\u00b7ne", "Treu\u00b7e", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wann aber dich ein fremdes Joch umfasset/", "tokens": ["Wann", "a\u00b7ber", "dich", "ein", "frem\u00b7des", "Joch", "um\u00b7fas\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Soll mir dein Strick der Weg zur Freyheit seyn.", "tokens": ["Soll", "mir", "dein", "Strick", "der", "Weg", "zur", "Frey\u00b7heit", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}