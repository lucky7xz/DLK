{"textgrid.poem.39042": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Berg, der den Florentinern", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Berg, der den Florentinern", "tokens": ["Den", "Berg", ",", "der", "den", "Flo\u00b7ren\u00b7ti\u00b7nern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Immerdar vor Augen schwebt,", "tokens": ["Im\u00b7mer\u00b7dar", "vor", "Au\u00b7gen", "schwebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind wir heut erstiegen,", "tokens": ["Sind", "wir", "heut", "er\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Das alte Fiesole zu besuchen.", "tokens": ["Das", "al\u00b7te", "Fie\u00b7so\u00b7le", "zu", "be\u00b7su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "In dem Kloster dort erlabten uns Gebilde", "tokens": ["In", "dem", "Klos\u00b7ter", "dort", "er\u00b7lab\u00b7ten", "uns", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Von Giotto und dem frommen Johann,", "tokens": ["Von", "Giot\u00b7to", "und", "dem", "from\u00b7men", "Jo\u00b7hann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "In der B\u00fccher Pracht.", "tokens": ["In", "der", "B\u00fc\u00b7cher", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.8": {"text": "Doch endlich sind wir h\u00f6her geklimmt,", "tokens": ["Doch", "end\u00b7lich", "sind", "wir", "h\u00f6\u00b7her", "ge\u00b7klimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Zur Spitze hinauf,", "tokens": ["Zur", "Spit\u00b7ze", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Wo unter Cypressen", "tokens": ["Wo", "un\u00b7ter", "Cyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.11": {"text": "Einsam das Kloster der Franziskaner ruht.", "tokens": ["Ein\u00b7sam", "das", "Klos\u00b7ter", "der", "Fran\u00b7zis\u00b7ka\u00b7ner", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ein kalter Wind durchsaust die Berge,", "tokens": ["Ein", "kal\u00b7ter", "Wind", "durch\u00b7saust", "die", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach dem Gewitter ist die Gegend tr\u00fcbe,", "tokens": ["Nach", "dem", "Ge\u00b7wit\u00b7ter", "ist", "die", "Ge\u00b7gend", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weit umher ergeht sich hier der Blick", "tokens": ["Weit", "um\u00b7her", "er\u00b7geht", "sich", "hier", "der", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ueber Felsen weg durch Th\u00e4ler hin,", "tokens": ["Ue\u00b7ber", "Fel\u00b7sen", "weg", "durch", "Th\u00e4\u00b7ler", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und zu den F\u00fc\u00dfen liegt Fiesole und Florenz.", "tokens": ["Und", "zu", "den", "F\u00fc\u00b7\u00dfen", "liegt", "Fie\u00b7so\u00b7le", "und", "Flo\u00b7renz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie wir mit den M\u00f6nchen gespeist,", "tokens": ["Wie", "wir", "mit", "den", "M\u00f6n\u00b7chen", "ge\u00b7speist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Erbietet man sich zu unserm Ergetzen,", "tokens": ["Er\u00b7bie\u00b7tet", "man", "sich", "zu", "un\u00b7serm", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da das Wetter rauh und unfreundlich,", "tokens": ["Da", "das", "Wet\u00b7ter", "rauh", "und", "un\u00b7freund\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+--", "measure": "anapaest.init"}, "line.4": {"text": "Mit uns Schach zu spielen.", "tokens": ["Mit", "uns", "Schach", "zu", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Meine Gef\u00e4hrten treten besch\u00e4mt zur\u00fcck,", "tokens": ["Mei\u00b7ne", "Ge\u00b7f\u00e4hr\u00b7ten", "tre\u00b7ten", "be\u00b7sch\u00e4mt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Und ich, \u00fcberrascht, als der Einzige", "tokens": ["Und", "ich", ",", "\u00fc\u00b7berr\u00b7ascht", ",", "als", "der", "Ein\u00b7zi\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "VVPP", "$,", "KOUS", "ART", "ADJA"], "meter": "--+--+-+--", "measure": "anapaest.di.plus"}, "line.7": {"text": "Der die Kunst versteht und \u00fcbt,", "tokens": ["Der", "die", "Kunst", "ver\u00b7steht", "und", "\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Erbiete mich, der Landsmannschaft Ehre zu retten.", "tokens": ["Er\u00b7bie\u00b7te", "mich", ",", "der", "Lands\u00b7mann\u00b7schaft", "Eh\u00b7re", "zu", "ret\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Doch selbst seit der Kindheit", "tokens": ["Doch", "selbst", "seit", "der", "Kind\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Hab' ich kaum den Stein ber\u00fchrt,", "tokens": ["Hab'", "ich", "kaum", "den", "Stein", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und nie hab' ich mehr von der Weisen Erg\u00f6tzung", "tokens": ["Und", "nie", "hab'", "ich", "mehr", "von", "der", "Wei\u00b7sen", "Er\u00b7g\u00f6t\u00b7zung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.12": {"text": "Gefa\u00dft als nur die Z\u00fcge.", "tokens": ["Ge\u00b7fa\u00dft", "als", "nur", "die", "Z\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Der kl\u00fcgste, gewandteste Pater wird mir", "tokens": ["Der", "kl\u00fcgs\u00b7te", ",", "ge\u00b7wand\u00b7tes\u00b7te", "Pa\u00b7ter", "wird", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VAFIN", "PPER"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Als Feldherr gegen\u00fcber gestellt,", "tokens": ["Als", "Feld\u00b7herr", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPO", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Ein feiner Kopf, so freundlich-sch\u00f6n", "tokens": ["Ein", "fei\u00b7ner", "Kopf", ",", "so", "freund\u00b7lich\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wie man ihn wohl auf alten Bildern sieht.", "tokens": ["Wie", "man", "ihn", "wohl", "auf", "al\u00b7ten", "Bil\u00b7dern", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Kampf beginnt: \u2013", "tokens": ["Der", "Kampf", "be\u00b7ginnt", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und ich, nur in Aengsten,", "tokens": ["Und", "ich", ",", "nur", "in", "A\u00b7engs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "APPR", "NE", "$,"], "meter": "-+++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Nicht gleich die schlimmsten Bl\u00f6\u00dfen zu geben,", "tokens": ["Nicht", "gleich", "die", "schlimms\u00b7ten", "Bl\u00f6\u00b7\u00dfen", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ziehe, im Zagen mit zaudernden Unwissen,", "tokens": ["Zie\u00b7he", ",", "im", "Za\u00b7gen", "mit", "zau\u00b7dern\u00b7den", "Un\u00b7wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--++-", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Und rings die andern,", "tokens": ["Und", "rings", "die", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Alte wie Junge", "tokens": ["Al\u00b7te", "wie", "Jun\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "KOKOM", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Erwundern mein kluges, feines Spiel,", "tokens": ["Er\u00b7wun\u00b7dern", "mein", "klu\u00b7ges", ",", "fei\u00b7nes", "Spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Feldherr selber", "tokens": ["Der", "Feld\u00b7herr", "sel\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Wei\u00df kaum sich zu wehren,", "tokens": ["Wei\u00df", "kaum", "sich", "zu", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Und ich verstehe selbst von meinen Listen nichts.", "tokens": ["Und", "ich", "ver\u00b7ste\u00b7he", "selbst", "von", "mei\u00b7nen", "Lis\u00b7ten", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Lob auf Lob, Bewundrung, laute,", "tokens": ["Lob", "auf", "Lob", ",", "Be\u00b7wund\u00b7rung", ",", "lau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ermuthigt mich, und trunken, erhitzt", "tokens": ["Er\u00b7mut\u00b7higt", "mich", ",", "und", "trun\u00b7ken", ",", "er\u00b7hitzt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Such' ich mir eines Planes bewu\u00dft zu werden.", "tokens": ["Such'", "ich", "mir", "ei\u00b7nes", "Pla\u00b7nes", "be\u00b7wu\u00dft", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Schon giebt man den Pater verlohren,", "tokens": ["Schon", "giebt", "man", "den", "Pa\u00b7ter", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "Und ich strebe tantalisch vergeblich", "tokens": ["Und", "ich", "stre\u00b7be", "tan\u00b7ta\u00b7lisch", "ver\u00b7geb\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Zu sehn, die Einsicht nur etwas zu gewinnen,", "tokens": ["Zu", "sehn", ",", "die", "Ein\u00b7sicht", "nur", "et\u00b7was", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ART", "NN", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Doch nur mechanisch r\u00fcckt der Finger die Holzgestalten.", "tokens": ["Doch", "nur", "me\u00b7cha\u00b7nisch", "r\u00fcckt", "der", "Fin\u00b7ger", "die", "Holz\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Man sagt, in drei Z\u00fcgen sei ich der Meister,", "tokens": ["Man", "sagt", ",", "in", "drei", "Z\u00fc\u00b7gen", "sei", "ich", "der", "Meis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "APPR", "CARD", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da verl\u00e4\u00dft pl\u00f6tzlich der Genius den Blinden,", "tokens": ["Da", "ver\u00b7l\u00e4\u00dft", "pl\u00f6tz\u00b7lich", "der", "Ge\u00b7nius", "den", "Blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und lautes Gel\u00e4chter statt der Ehrfurcht", "tokens": ["Und", "lau\u00b7tes", "Ge\u00b7l\u00e4ch\u00b7ter", "statt", "der", "Ehr\u00b7furcht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Umschallt und besch\u00e4mt mich,", "tokens": ["Um\u00b7schallt", "und", "be\u00b7sch\u00e4mt", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Denn wie ich r\u00fccke spiel' ich mich selber", "tokens": ["Denn", "wie", "ich", "r\u00fc\u00b7cke", "spiel'", "ich", "mich", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "VVFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In wenigen Z\u00fcgen matt,", "tokens": ["In", "we\u00b7ni\u00b7gen", "Z\u00fc\u00b7gen", "matt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und rings die Versammlung", "tokens": ["Und", "rings", "die", "Ver\u00b7samm\u00b7lung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Begreift so wenig", "tokens": ["Be\u00b7greift", "so", "we\u00b7nig"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Die hohe List, wie jetzt die Einfalt.", "tokens": ["Die", "ho\u00b7he", "List", ",", "wie", "jetzt", "die", "Ein\u00b7falt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So erz\u00e4hlt man, da\u00df der gro\u00dfe Cond\u00e9", "tokens": ["So", "er\u00b7z\u00e4hlt", "man", ",", "da\u00df", "der", "gro\u00b7\u00dfe", "Con\u00b7d\u00e9"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als Meister begann", "tokens": ["Als", "Meis\u00b7ter", "be\u00b7gann"], "