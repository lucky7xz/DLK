"Von", "Giot\u00b7to", "und", "dem", "from\u00b7men", "Jo\u00b7hann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "In der B\u00fccher Pracht.", "tokens": ["In", "der", "B\u00fc\u00b7cher", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.8": {"text": "Doch endlich sind wir h\u00f6her geklimmt,", "tokens": ["Doch", "end\u00b7lich", "sind", "wir", "h\u00f6\u00b7her", "ge\u00b7klimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Zur Spitze hinauf,", "tokens": ["Zur", "Spit\u00b7ze", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Wo unter Cypressen", "tokens": ["Wo", "un\u00b7ter", "Cyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.11": {"text": "Einsam das Kloster der Franziskaner ruht.", "tokens": ["Ein\u00b7sam", "das", "Klos\u00b7ter", "der", "Fran\u00b7zis\u00b7ka\u00b7ner", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Ein kalter Wind durchsaust die Berge,", "tokens": ["Ein", "kal\u00b7ter", "Wind", "durch\u00b7saust", "die", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach dem Gewitter ist die Gegend tr\u00fcbe,", "tokens": ["Nach", "dem", "Ge\u00b7wit\u00b7ter", "ist", "die", "Ge\u00b7gend", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weit umher ergeht sich hier der Blick", "tokens": ["Weit", "um\u00b7her", "er\u00b7geht", "sich", "hier", "der", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ueber Felsen weg durch Th\u00e4ler hin,", "tokens": ["Ue\u00b7ber", "Fel\u00b7sen", "weg", "durch", "Th\u00e4\u00b7ler", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und zu den F\u00fc\u00dfen liegt Fiesole und Florenz.", "tokens": ["Und", "zu", "den", "F\u00fc\u00b7\u00dfen", "liegt", "Fie\u00b7so\u00b7le", "und", "Flo\u00b7renz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Wie wir mit den M\u00f6nchen gespeist,", "tokens": ["Wie", "wir", "mit", "den", "M\u00f6n\u00b7chen", "ge\u00b7speist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Erbietet man sich zu unserm Ergetzen,", "tokens": ["Er\u00b7bie\u00b7tet", "man", "sich", "zu", "un\u00b7serm", "Er\u00b7get\u00b7zen", ","], "token_info":