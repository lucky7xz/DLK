asure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Lob auf Lob, Bewundrung, laute,", "tokens": ["Lob", "auf", "Lob", ",", "Be\u00b7wund\u00b7rung", ",", "lau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ermuthigt mich, und trunken, erhitzt", "tokens": ["Er\u00b7mut\u00b7higt", "mich", ",", "und", "trun\u00b7ken", ",", "er\u00b7hitzt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Such' ich mir eines Planes bewu\u00dft zu werden.", "tokens": ["Such'", "ich", "mir", "ei\u00b7nes", "Pla\u00b7nes", "be\u00b7wu\u00dft", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Schon giebt man den Pater verlohren,", "tokens": ["Schon", "giebt", "man", "den", "Pa\u00b7ter", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "Und ich strebe tantalisch vergeblich", "tokens": ["Und", "ich", "stre\u00b7be", "tan\u00b7ta\u00b7lisch", "ver\u00b7geb\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Zu sehn, die Einsicht nur etwas zu gewinnen,", "tokens": ["Zu", "sehn", ",", "die", "Ein\u00b7sicht", "nur", "et\u00b7was", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "punc