{"textgrid.poem.46672": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[unglaublich, wie ertr\u00e4gt ein Herz]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unglaublich, wie ertr\u00e4gt ein Herz,", "tokens": ["Un\u00b7glaub\u00b7lich", ",", "wie", "er\u00b7tr\u00e4gt", "ein", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was schon zu denken unertr\u00e4glich!", "tokens": ["Was", "schon", "zu", "den\u00b7ken", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hinhalten Hoffnungen den Schmerz,", "tokens": ["Hin\u00b7hal\u00b7ten", "Hoff\u00b7nun\u00b7gen", "den", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn brechend, den sie steigern t\u00e4glich.", "tokens": ["Ihn", "bre\u00b7chend", ",", "den", "sie", "stei\u00b7gern", "t\u00e4g\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man hofft und hofft, bis hoffnungslos", "tokens": ["Man", "hofft", "und", "hofft", ",", "bis", "hoff\u00b7nungs\u00b7los"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "$,", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geworden das geliebte Leben,", "tokens": ["Ge\u00b7wor\u00b7den", "das", "ge\u00b7lieb\u00b7te", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann gibt man auf die Hoffnung blos,", "tokens": ["Dann", "gibt", "man", "auf", "die", "Hoff\u00b7nung", "blos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Leben war schon aufgegeben.", "tokens": ["Das", "Le\u00b7ben", "war", "schon", "auf\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Unglaublich, wie ertr\u00e4gt ein Herz,", "tokens": ["Un\u00b7glaub\u00b7lich", ",", "wie", "er\u00b7tr\u00e4gt", "ein", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was schon zu denken unertr\u00e4glich!", "tokens": ["Was", "schon", "zu", "den\u00b7ken", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hinhalten Hoffnungen den Schmerz,", "tokens": ["Hin\u00b7hal\u00b7ten", "Hoff\u00b7nun\u00b7gen", "den", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn brechend, den sie steigern t\u00e4glich.", "tokens": ["Ihn", "bre\u00b7chend", ",", "den", "