{"textgrid.poem.25209": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "Das bist du", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn mit Dunkel und mit Schweigen", "tokens": ["Wenn", "mit", "Dun\u00b7kel", "und", "mit", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mutter Nacht dein Bett umh\u00fcllt,", "tokens": ["Mut\u00b7ter", "Nacht", "dein", "Bett", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lausche, wie mein Zaubergeigen", "tokens": ["Lau\u00b7sche", ",", "wie", "mein", "Zau\u00b7berg\u00b7ei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOKOM", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimlich dir die Kammer f\u00fcllt.", "tokens": ["Heim\u00b7lich", "dir", "die", "Kam\u00b7mer", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lausche, wie dich Wunderglocken", "tokens": ["Lau\u00b7sche", ",", "wie", "dich", "Wun\u00b7der\u00b7glo\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fromm zur heilgen Tiefe locken.", "tokens": ["Fromm", "zur", "heil\u00b7gen", "Tie\u00b7fe", "lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In der Tiefe wohnt die Ruh,", "tokens": ["In", "der", "Tie\u00b7fe", "wohnt", "die", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Tiefe/ das bist du.", "tokens": ["Und", "die", "Tie\u00b7fe", "/", "das", "bist", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "PDS", "VAFIN", "PPER", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Frieden ihm, so dir zur Seiten", "tokens": ["Frie\u00b7den", "ihm", ",", "so", "dir", "zur", "Sei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "ADV", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Atmend ruht; er ist dein Schild.", "tokens": ["At\u00b7mend", "ruht", ";", "er", "ist", "dein", "Schild", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frieden allen Erdenbreiten,", "tokens": ["Frie\u00b7den", "al\u00b7len", "Er\u00b7den\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedem Gottesebenbild!", "tokens": ["Je\u00b7dem", "Got\u00b7te\u00b7se\u00b7ben\u00b7bild", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gib den H\u00fctten dein Erbarmen", "tokens": ["Gib", "den", "H\u00fct\u00b7ten", "dein", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und dem Gl\u00fcck ein froh Umarmen.", "tokens": ["Und", "dem", "Gl\u00fcck", "ein", "froh", "Um\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne G\u00fcte keine Ruh.", "tokens": ["Oh\u00b7ne", "G\u00fc\u00b7te", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Jedes Antlitz/ das bist du.", "tokens": ["Je\u00b7des", "Ant\u00b7litz", "/", "das", "bist", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PDS", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Engel, heitre Lichtgestalten,", "tokens": ["En\u00b7gel", ",", "heit\u00b7re", "Licht\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigen aus dem dunkeln Land", "tokens": ["Stei\u00b7gen", "aus", "dem", "dun\u00b7keln", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in deine H\u00e4nde falten", "tokens": ["Und", "in", "dei\u00b7ne", "H\u00e4n\u00b7de", "fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kosend sie die Kinderhand.", "tokens": ["Ko\u00b7send", "sie", "die", "Kin\u00b7der\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh doch, deine toten Lieben", "tokens": ["Sieh", "doch", ",", "dei\u00b7ne", "to\u00b7ten", "Lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind dir alle treu geblieben;", "tokens": ["Sind", "dir", "al\u00b7le", "treu", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mutterherz hei\u00dft ihre Ruh.", "tokens": ["Mut\u00b7ter\u00b7herz", "hei\u00dft", "ih\u00b7re", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Deine Kinder/ das bist du.", "tokens": ["Dei\u00b7ne", "Kin\u00b7der", "/", "das", "bist", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PDS", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sp\u00fcrst du auch, wie auf dein Gr\u00fc\u00dfen", "tokens": ["Sp\u00fcrst", "du", "auch", ",", "wie", "auf", "dein", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Harrt ein treuer Paladin?", "tokens": ["Harrt", "ein", "treu\u00b7er", "Pa\u00b7la\u00b7din", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Aus der Ferne dir zu F\u00fc\u00dfen", "tokens": ["Aus", "der", "Fer\u00b7ne", "dir", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann ihn deine Sehnsucht ziehn.", "tokens": ["Kann", "ihn", "dei\u00b7ne", "Sehn\u00b7sucht", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gib dein Auge seinem Auge;", "tokens": ["Gib", "dein", "Au\u00b7ge", "sei\u00b7nem", "Au\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eins im andern sauge, sauge", "tokens": ["Eins", "im", "an\u00b7dern", "sau\u00b7ge", ",", "sau\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPRART", "ADJA", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heimatswonne, Heimatsruh.", "tokens": ["Hei\u00b7mats\u00b7won\u00b7ne", ",", "Hei\u00b7mats\u00b7ruh", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Du bist ich, und ich bin du.", "tokens": ["Du", "bist", "ich", ",", "und", "ich", "bin", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KON", "PPER", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Horch, mein Lieb, die Zaubergeigen", "tokens": ["Horch", ",", "mein", "Lieb", ",", "die", "Zau\u00b7berg\u00b7ei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singen Hochzeitsmelodein,", "tokens": ["Sin\u00b7gen", "Hoch\u00b7zeits\u00b7me\u00b7lo\u00b7dein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und der bunte Sternenreigen", "tokens": ["Und", "der", "bun\u00b7te", "Ster\u00b7nen\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stimmt und funkelt \u00fcppig drein.", "tokens": ["Stimmt", "und", "fun\u00b7kelt", "\u00fcp\u00b7pig", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welten schw\u00e4rmen dort bei Welten,", "tokens": ["Wel\u00b7ten", "schw\u00e4r\u00b7men", "dort", "bei", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wiegen sich in blauen Zelten,", "tokens": ["Wie\u00b7gen", "sich", "in", "blau\u00b7en", "Zel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Summen uns in selge Ruh ...", "tokens": ["Sum\u00b7men", "uns", "in", "sel\u00b7ge", "Ruh", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich bin Stern, und Stern bist du.", "tokens": ["Ich", "bin", "Stern", ",", "und", "