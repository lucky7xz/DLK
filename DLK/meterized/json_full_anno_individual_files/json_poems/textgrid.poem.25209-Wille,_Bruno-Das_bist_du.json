, "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh doch, deine toten Lieben", "tokens": ["Sieh", "doch", ",", "dei\u00b7ne", "to\u00b7ten", "Lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind dir alle treu geblieben;", "tokens": ["Sind", "dir", "al\u00b7le", "treu", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mutterherz hei\u00dft ihre Ruh.", "tokens": ["Mut\u00b7ter\u00b7herz", "hei\u00dft", "ih\u00b7re", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Deine Kinder/ das bist du.", "tokens": ["Dei\u00b7ne", "Kin\u00b7der", "/", "das", "bist", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PDS", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sp\u00fcrst du auch, wie auf dein Gr\u00fc\u00dfen", "tokens": ["Sp\u00fcrst", "du", "auch", ",", "wie", "auf", "dein", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Harrt ein treuer Paladin?", "tokens": ["Harrt", "ein", "treu\u00b7er", "Pa\u00b7la\u00b7din", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Aus der Ferne dir zu F\u00fc\u00dfen", "tokens": ["Aus", "der", "Fer\u00b7ne", "dir", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann ihn deine Sehnsucht ziehn.", "tokens": ["Kann", "ihn", "dei\u00b7ne", "Sehn\u00b7sucht", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gib dein Auge seinem Auge;", "tokens": ["Gib", "dein", "Au\u00b7ge", "sei\u00b7nem", "Au\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eins im andern sauge, sauge", "tokens": ["Eins", "im", "an\u00b7dern", "sau\u00b7ge", ",", "sau\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPRART", "ADJA", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heimatswonne, Heimatsruh.", "tokens": ["Hei\u00b7mats\u00b7won\u00b7ne", ",", "Hei\u00b7mats\u00b7ruh", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Du bist ich, und ich bin du.", "tokens": ["Du", "bist", "ich", ",", "und", "ich", "bin", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KON", "PPER", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Horch, mein Lieb, die Zaubergeigen", "tokens": ["Horch", ",", "mein", "Lieb", ",", "die", "Zau\u00b7berg\u00b7ei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singen Hochzeitsmelodein,", "tokens": ["Sin\u00b7gen", "Hoch\u00b7zeits\u00b7me\u00b7lo\u00b7dein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und der bunte Sternenreigen", "tokens": ["Und", "der", "bun\u00b7te", "Ster\u00b7nen\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stimmt und funkelt \u00fcppig drein.", "tokens": ["Stimmt", "und", "fun\u00b7kelt", "\u00fcp\u00b7pig", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welten schw\u00e4rmen dort bei Welten,", "tokens": ["Wel\u00b7ten", "schw\u00e4r\u00b7men", "dort", "bei", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wiegen sich in blauen Zelten,", "tokens": ["Wie\u00b7gen", "sich", "in", "blau\u00b7en", "Zel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Summen uns in selge Ruh ...", "tokens": ["Sum\u00b7men", "uns", "in", "sel\u00b7ge", "Ruh", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich bin Stern, und Stern bist du.", "tokens": ["Ich", "bin", "Stern", ",", "und", "Stern", "bist", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "KON", "NN", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}