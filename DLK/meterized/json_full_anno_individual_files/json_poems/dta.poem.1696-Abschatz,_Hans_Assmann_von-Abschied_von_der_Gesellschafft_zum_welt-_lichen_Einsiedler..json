{"dta.poem.1696": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von der Gesellschafft zum welt-  \n lichen Einsiedler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich mu\u00df nun von dir scheiden/ ", "tokens": ["Ich", "mu\u00df", "nun", "von", "dir", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bi\u00dfher geliebtes Hau\u00df/", "tokens": ["Bi\u00df\u00b7her", "ge\u00b7lieb\u00b7tes", "Hau\u00df", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein angenehmes Leiden", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mes", "Lei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Treibt mich von hinnen aus.", "tokens": ["Treibt", "mich", "von", "hin\u00b7nen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bezwungner Andacht Brunst", "tokens": ["Be\u00b7zwung\u00b7ner", "An\u00b7dacht", "Brunst"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "L\u00e4st schlechte Glutt versp\u00fcren/", "tokens": ["L\u00e4st", "schlech\u00b7te", "Glutt", "ver\u00b7sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel st\u00e4rcker Feuer f\u00fchren", "tokens": ["Viel", "st\u00e4r\u00b7cker", "Feu\u00b7er", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Freywillge Gunst und Gegen-Gunst.", "tokens": ["Frey\u00b7will\u00b7ge", "Gunst", "und", "Ge\u00b7gen\u00b7Gunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Was man befohlen thut", "tokens": ["Was", "man", "be\u00b7foh\u00b7len", "thut"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PIS", "VVPP", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Geht nimmer so von statten/", "tokens": ["Geht", "nim\u00b7mer", "so", "von", "stat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als was ein freyer Mutt", "tokens": ["Als", "was", "ein", "frey\u00b7er", "Mutt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Uns pfleget einzurathen.", "tokens": ["Uns", "pfle\u00b7get", "ein\u00b7zu\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Viel lieber will ich seyn", "tokens": ["Viel", "lie\u00b7ber", "will", "ich", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Verbunden bi\u00df zur Bahre/", "tokens": ["Ver\u00b7bun\u00b7den", "bi\u00df", "zur", "Bah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als auff gewisse Jahre", "tokens": ["Als", "auff", "ge\u00b7wis\u00b7se", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umsonst in Dienst mich lassen ein.", "tokens": ["Um\u00b7sonst", "in", "Dienst", "mich", "las\u00b7sen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie kan ein einig Ku\u00df", "tokens": ["Wie", "kan", "ein", "ei\u00b7nig", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir voll Vergn\u00fcgen geben/", "tokens": ["Mir", "voll", "Ver\u00b7gn\u00fc\u00b7gen", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn andrer Uberflu\u00df", "tokens": ["Wenn", "an\u00b7drer", "U\u00b7ber\u00b7flu\u00df"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mir mu\u00df f\u00fcr Augen schweben.", "tokens": ["Mir", "mu\u00df", "f\u00fcr", "Au\u00b7gen", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn andre ziehen ein", "tokens": ["Wenn", "and\u00b7re", "zie\u00b7hen", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit ungez\u00e4hlten Zahlen/", "tokens": ["Mit", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "Zah\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was mir zu vielen mahlen", "tokens": ["Was", "mir", "zu", "vie\u00b7len", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kaum einfach will verg\u00fcnstigt seyn.", "tokens": ["Kaum", "ein\u00b7fach", "will", "ver\u00b7g\u00fcns\u00b7tigt", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mag unsrer Schl\u00fcsse Pflicht", "tokens": ["Mag", "uns\u00b7rer", "Schl\u00fcs\u00b7se", "Pflicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Celind und andre brechen/", "tokens": ["Ce\u00b7lind", "und", "and\u00b7re", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die mir erlauben nicht", "tokens": ["Die", "mir", "er\u00b7lau\u00b7ben", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie k\u00fcssend anzusprechen.", "tokens": ["Sie", "k\u00fcs\u00b7send", "an\u00b7zu\u00b7spre\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie will man mich allein", "tokens": ["Wie", "will", "man", "mich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PIS", "PRF", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zur Straff und Schuld verb\u00fcnden/", "tokens": ["Zur", "Straff", "und", "Schuld", "ver\u00b7b\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da andrer ihre S\u00fcnden", "tokens": ["Da", "an\u00b7drer", "ih\u00b7re", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch frey und ungeb\u00fcsset seyn?", "tokens": ["Noch", "frey", "und", "un\u00b7ge\u00b7b\u00fcs\u00b7set", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wer wird Gel\u00fcbd erf\u00fcllen/", "tokens": ["Wer", "wird", "Ge\u00b7l\u00fcbd", "er\u00b7f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die man nicht halten kan?", "tokens": ["Die", "man", "nicht", "hal\u00b7ten", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Man nimmt den gutten Willen", "tokens": ["Man", "nimmt", "den", "gut\u00b7ten", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier vor die W\u00fcrckung an.", "tokens": ["Hier", "vor", "die", "W\u00fcr\u00b7ckung", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Zu leben gantz f\u00fcr sich/", "tokens": ["Zu", "le\u00b7ben", "gantz", "f\u00fcr", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "APPR", "PRF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ohn Tisch- und Bettgenossen", "tokens": ["Ohn", "Tisch", "und", "Bett\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seyn einsam eingeschlossen/", "tokens": ["Seyn", "ein\u00b7sam", "ein\u00b7ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist ein zu schweres Thun f\u00fcr mich.", "tokens": ["Ist", "ein", "zu", "schwe\u00b7res", "Thun", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PTKZU", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Will man gleich meinen Sinn", "tokens": ["Will", "man", "gleich", "mei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Voll Wanckelmutt beschreiben/", "tokens": ["Voll", "Wan\u00b7ckel\u00b7mutt", "be\u00b7schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich werde/ wie ich bin/", "tokens": ["Ich", "wer\u00b7de", "/", "wie", "ich", "bin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PWAV", "PPER", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch hier ein Diener bleiben.", "tokens": ["Doch", "hier", "ein", "Die\u00b7ner", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Man lasse mit der Zeit", "tokens": ["Man", "las\u00b7se", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Amaranthe sagen/", "tokens": ["Mein", "A\u00b7ma\u00b7ran\u00b7the", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob sie wird k\u00f6nnen klagen", "tokens": ["Ob", "sie", "wird", "k\u00f6n\u00b7nen", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von meiner Unbest\u00e4ndigkeit.", "tokens": ["Von", "mei\u00b7ner", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}