{"textgrid.poem.43969": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: So soll mich auch durchaus nichts kr\u00e4ncken,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So soll mich auch durchaus nichts kr\u00e4ncken,", "tokens": ["So", "soll", "mich", "auch", "durc\u00b7haus", "nichts", "kr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es geh mir noch so wunderlich;", "tokens": ["Es", "geh", "mir", "noch", "so", "wun\u00b7der\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00df in aller Noth gedencken:", "tokens": ["Ich", "mu\u00df", "in", "al\u00b7ler", "Noth", "ge\u00b7den\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Himmel thut's und pr\u00fcfet dich;", "tokens": ["Der", "Him\u00b7mel", "thut's", "und", "pr\u00fc\u00b7fet", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er ist ein Vater, der zwar schl\u00e4gt,", "tokens": ["Er", "ist", "ein", "Va\u00b7ter", ",", "der", "zwar", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch auch nicht ewig Ruthen tr\u00e4gt.", "tokens": ["Doch", "auch", "nicht", "e\u00b7wig", "Ru\u00b7then", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir Menschen k\u00f6nnen unser Gl\u00fccke", "tokens": ["Wir", "Men\u00b7schen", "k\u00f6n\u00b7nen", "un\u00b7ser", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unm\u00f6glich allzeit selbst verstehn,", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "all\u00b7zeit", "selbst", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir fallen in Gefahr und Stricke,", "tokens": ["Wir", "fal\u00b7len", "in", "Ge\u00b7fahr", "und", "Stri\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So oft wir vor uns selber gehn;", "tokens": ["So", "oft", "wir", "vor", "uns", "sel\u00b7ber", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wehlen falsch und wandeln blind,", "tokens": ["Wir", "weh\u00b7len", "falsch", "und", "wan\u00b7deln", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So oft wir ohne Creuze sind.", "tokens": ["So", "oft", "wir", "oh\u00b7ne", "Creu\u00b7ze", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein ruhig Herz und rein Gewi\u00dfen", "tokens": ["Ein", "ru\u00b7hig", "Herz", "und", "rein", "Ge\u00b7wi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird doch von au\u00dfen nicht verst\u00f6rt;", "tokens": ["Wird", "doch", "von", "au\u00b7\u00dfen", "nicht", "ver\u00b7st\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df treue Seelen leiden m\u00fc\u00dfen,", "tokens": ["Da\u00df", "treu\u00b7e", "See\u00b7len", "lei\u00b7den", "m\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das wird wohl \u00fcberall geh\u00f6rt,", "tokens": ["Das", "wird", "wohl", "\u00fc\u00b7be\u00b7rall", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch da\u00df sie niemahls H\u00fclfe sehn,", "tokens": ["Doch", "da\u00df", "sie", "nie\u00b7mahls", "H\u00fcl\u00b7fe", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das sieht man nimmermehr geschehn.", "tokens": ["Das", "sieht", "man", "nim\u00b7mer\u00b7mehr", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Sp\u00f6tter thun mir freylich bange,", "tokens": ["Die", "Sp\u00f6t\u00b7ter", "thun", "mir", "frey\u00b7lich", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man sezt mir ein gefehrlich Ziel,", "tokens": ["Man", "sezt", "mir", "ein", "ge\u00b7fehr\u00b7lich", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wetter stehn so schwer als lange,", "tokens": ["Die", "Wet\u00b7ter", "stehn", "so", "schwer", "als", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Trauern\u00e4chte werden viel;", "tokens": ["Der", "Trau\u00b7er\u00b7n\u00e4ch\u00b7te", "wer\u00b7den", "viel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch Gedult, Vernunft und Zeit", "tokens": ["Je\u00b7doch", "Ge\u00b7dult", ",", "Ver\u00b7nunft", "und", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Versprechen Palmen auf den Streit.", "tokens": ["Ver\u00b7spre\u00b7chen", "Pal\u00b7men", "auf", "den", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer weis, wie bald mein Gl\u00fccke steige,", "tokens": ["Wer", "weis", ",", "wie", "bald", "mein", "Gl\u00fc\u00b7cke", "stei\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "$,", "PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es kan mir unverhoft noch bl\u00fchn;", "tokens": ["Es", "kan", "mir", "un\u00b7ver\u00b7hoft", "noch", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Gro\u00dfmuth, so ich stets bezeige,", "tokens": ["Die", "Gro\u00df\u00b7muth", ",", "so", "ich", "stets", "be\u00b7zei\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df durch Gefahr dem Feind entfliehn.", "tokens": ["Mu\u00df", "durch", "Ge\u00b7fahr", "dem", "Feind", "ent\u00b7fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein treuer Gott und kluger Mann", "tokens": ["Ein", "treu\u00b7er", "Gott", "und", "klu\u00b7ger", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Macht, da\u00df man alles dulden kan.", "tokens": ["Macht", ",", "da\u00df", "man", "al\u00b7les", "dul\u00b7den", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein Leben wird noch um das Ende", "tokens": ["Mein", "Le\u00b7ben", "wird", "noch", "um", "das", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Himmel voll Vergn\u00fcgung seyn;", "tokens": ["Ein", "Him\u00b7mel", "voll", "Ver\u00b7gn\u00fc\u00b7gung", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Hofnung reicht mir schon die H\u00e4nde,", "tokens": ["Die", "Hof\u00b7nung", "reicht", "mir", "schon", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die keusche Liebe stimmt mit ein:", "tokens": ["Die", "keu\u00b7sche", "Lie\u00b7be", "stimmt", "mit", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geht, Winde, bringt den stillen Ku\u00df", "tokens": ["Geht", ",", "Win\u00b7de", ",", "bringt", "den", "stil\u00b7len", "Ku\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem, der mir bleiben will und mu\u00df.", "tokens":