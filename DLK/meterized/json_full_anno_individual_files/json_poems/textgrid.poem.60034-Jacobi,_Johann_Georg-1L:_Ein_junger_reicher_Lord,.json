{"textgrid.poem.60034": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein junger reicher Lord,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein junger reicher Lord,", "tokens": ["Ein", "jun\u00b7ger", "rei\u00b7cher", "Lord", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der mehr als eine Welt sein treues Julchen liebte,", "tokens": ["Der", "mehr", "als", "ei\u00b7ne", "Welt", "sein", "treu\u00b7es", "Jul\u00b7chen", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und, auf ein halbgesagtes Wort,", "tokens": ["Und", ",", "auf", "ein", "halb\u00b7ge\u00b7sag\u00b7tes", "Wort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den kleinsten Wunsch von ihr sich zu errathen \u00fcbte,", "tokens": ["Den", "kleins\u00b7ten", "Wunsch", "von", "ihr", "sich", "zu", "er\u00b7ra\u00b7then", "\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gieng einst in einer Sommernacht,", "tokens": ["Gieng", "einst", "in", "ei\u00b7ner", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom heiterm Himmel angelacht,", "tokens": ["Vom", "hei\u00b7term", "Him\u00b7mel", "an\u00b7ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit ihr, f\u00fcr deren Gl\u00fcck er alles hingegeben.", "tokens": ["Mit", "ihr", ",", "f\u00fcr", "de\u00b7ren", "Gl\u00fcck", "er", "al\u00b7les", "hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PRELAT", "NN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u00bbo sieh doch\u00ab, rief das M\u00e4dchen schnell,", "tokens": ["\u00bb", "o", "sieh", "doch", "\u00ab", ",", "rief", "das", "M\u00e4d\u00b7chen", "schnell", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "ADV", "$(", "$,", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbo sieh doch, welch ein Stern, wie spielend und wie hell!", "tokens": ["\u00bb", "o", "sieh", "doch", ",", "welch", "ein", "Stern", ",", "wie", "spie\u00b7lend", "und", "wie", "hell", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "ADV", "$,", "PWAT", "ART", "NN", "$,", "PWAV", "ADJD", "KON", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der sch\u00f6nste, den ich sah in meinem ganzen Leben!\u00ab", "tokens": ["Der", "sch\u00f6ns\u00b7te", ",", "den", "ich", "sah", "in", "mei\u00b7nem", "gan\u00b7zen", "Le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie f\u00fchlt des Lieblings Hand in ihren H\u00e4nden beben;", "tokens": ["Sie", "f\u00fchlt", "des", "Lieb\u00b7lings", "Hand", "in", "ih\u00b7ren", "H\u00e4n\u00b7den", "be\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er sieht den Stern, mit traurigem Gesicht,", "tokens": ["Er", "sieht", "den", "Stern", ",", "mit", "trau\u00b7ri\u00b7gem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und dann sein M\u00e4dchen an, und spricht:", "tokens": ["Und", "dann", "sein", "M\u00e4d\u00b7chen", "an", ",", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "\u00bbach! Julchen, ach! verlang ihn nicht,", "tokens": ["\u00bb", "ach", "!", "Jul\u00b7chen", ",", "ach", "!", "ver\u00b7lang", "ihn", "nicht", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "NE", "$,", "ITJ", "$.", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ich kann ihn dir nicht geben!\u00ab", "tokens": ["Ich", "kann", "i