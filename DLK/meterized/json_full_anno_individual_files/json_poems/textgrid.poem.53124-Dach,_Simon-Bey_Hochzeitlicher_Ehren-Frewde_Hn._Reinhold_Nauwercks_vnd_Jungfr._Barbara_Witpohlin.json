{"textgrid.poem.53124": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Bey Hochzeitlicher Ehren-Frewde Hn. Reinhold Nauwercks vnd Jungfr. Barbara Witpohlin", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In seiner Liebsten Armen", "tokens": ["In", "sei\u00b7ner", "Liebs\u00b7ten", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entschlaffen vnd erwarmen,", "tokens": ["Ent\u00b7schlaf\u00b7fen", "vnd", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist, was in dieser Zeit", "tokens": ["Ist", ",", "was", "in", "die\u00b7ser", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "PRELS", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vns einig noch erfreut:", "tokens": ["Vns", "ei\u00b7nig", "noch", "er\u00b7freut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann Gn\u00fcge, Schertz vnd Lachen", "tokens": ["Wann", "Gn\u00fc\u00b7ge", ",", "Schertz", "vnd", "La\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vmb vnser Bett' her wachen,", "tokens": ["Vmb", "vn\u00b7ser", "Bett'", "her", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APZR", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd man kein Licht erkennt,", "tokens": ["Vnd", "man", "kein", "Licht", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ohn was im Hertzen brennt.", "tokens": ["Ohn", "was", "im", "Hert\u00b7zen", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kein Vngemach vnd Leiden", "tokens": ["Kein", "Vn\u00b7ge\u00b7mach", "vnd", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entsteht da zwischen beyden,", "tokens": ["Ent\u00b7steht", "da", "zwi\u00b7schen", "bey\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ohn was die Lieb' erregt,", "tokens": ["Ohn", "was", "die", "Lieb'", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die starck zum Fewer legt:", "tokens": ["Die", "starck", "zum", "Fe\u00b7wer", "legt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie bringt durch tausent Flammen", "tokens": ["Sie", "bringt", "durch", "tau\u00b7sent", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "All' jhre Krafft zusammen,", "tokens": ["All'", "jhre", "Krafft", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sucht Reitzung im Verdru\u00df,", "tokens": ["Sucht", "Reit\u00b7zung", "im", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Im Mangel Vberflu\u00df.", "tokens": ["Im", "Man\u00b7gel", "Vberf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "So ruht es sich ohn Sorgen,", "tokens": ["So", "ruht", "es", "sich", "ohn", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis vmb den lichten Morgen", "tokens": ["Bis", "vmb", "den", "lich\u00b7ten", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der helle Tages-Schein", "tokens": ["Der", "hel\u00b7le", "Ta\u00b7ges\u00b7Schein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zun Fenstern bricht herein,", "tokens": ["Zun", "Fens\u00b7tern", "bricht", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der sieht vns im Begn\u00fcgen", "tokens": ["Der", "sieht", "vns", "im", "Be\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vmbarmt zusammen liegen,", "tokens": ["Vm\u00b7barmt", "zu\u00b7sam\u00b7men", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir blasen Lieb' vnd Ruh", "tokens": ["Wir", "bla\u00b7sen", "Lieb'", "vnd", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Im Schlaf einander zu.", "tokens": ["Im", "Schlaf", "ein\u00b7an\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer jhm hat vorgenommen", "tokens": ["Wer", "jhm", "hat", "vor\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Heyraht zu entkommen,", "tokens": ["Der", "Hey\u00b7raht", "zu", "ent\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der siehet w\u00fcrdig nicht", "tokens": ["Der", "sie\u00b7het", "w\u00fcr\u00b7dig", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Sonnen guldnes Liecht,", "tokens": ["Der", "Son\u00b7nen", "guld\u00b7nes", "Liecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sonst ist ja dieses Leben", "tokens": ["Sonst", "ist", "ja", "die\u00b7ses", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Sorg' vnd Quahl vmbgeben,", "tokens": ["Mit", "Sor\u00b7g'", "vnd", "Quahl", "vmb\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn Heyraht auch entf\u00e4llt,", "tokens": ["Wenn", "Hey\u00b7raht", "auch", "ent\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Was sol vns diese Welt?", "tokens": ["Was", "sol", "vns", "die\u00b7se", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nein, lasst vns, weil wir k\u00f6nnen,", "tokens": ["Nein", ",", "lasst", "vns", ",", "weil", "wir", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der keuschen Lieb' vns g\u00f6nnen,", "tokens": ["Der", "keu\u00b7schen", "Lieb'", "vns", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch die wir s\u00e4mptlich sind,", "tokens": ["Durch", "die", "wir", "s\u00e4mpt\u00b7lich", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Lasst Venus vnd jhr Kind,", "tokens": ["Lasst", "Ve\u00b7nus", "vnd", "jhr", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Eh' als wir m\u00fcssen alten,", "tokens": ["Eh'", "als", "wir", "m\u00fcs\u00b7sen", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VMFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In vnsern Hertzen walten,", "tokens": ["In", "vn\u00b7sern", "Hert\u00b7zen", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sprecht, wie ein jedes kan,", "tokens": ["Sprecht", ",", "wie", "ein", "je\u00b7des", "kan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "PIAT", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Im Tantz einander an!", "tokens": ["Im", "Tantz", "ein\u00b7an\u00b7der", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der grosse Drang im Reyen,", "tokens": ["Der", "gros\u00b7se", "Drang", "im", "Re\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Seiten vnd Schallmeyen", "tokens": ["Die", "Sei\u00b7ten", "vnd", "Schall\u00b7me\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vnd des Get\u00fcmmels Fug", "tokens": ["Vnd", "des", "Ge\u00b7t\u00fcm\u00b7mels", "Fug"], "token_info": ["word", "word", "word", "wo