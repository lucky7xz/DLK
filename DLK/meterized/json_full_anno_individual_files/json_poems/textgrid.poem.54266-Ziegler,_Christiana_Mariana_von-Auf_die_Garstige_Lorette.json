{"textgrid.poem.54266": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Garstige Lorette", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lorettgen, nettes Kind, dein Leibgen k\u00f6mmet mir", "tokens": ["Lo\u00b7rett\u00b7gen", ",", "net\u00b7tes", "Kind", ",", "dein", "Leib\u00b7gen", "k\u00f6m\u00b7met", "mir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schlanck und wohlgestalt wie Butter-F\u00e4sser f\u00fcr,", "tokens": ["So", "schlanck", "und", "wohl\u00b7ge\u00b7stalt", "wie", "But\u00b7ter\u00b7F\u00e4s\u00b7ser", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KOKOM", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die F\u00fc\u00dfgen sind so schmal, als wie die Straussen weissen;", "tokens": ["Die", "F\u00fc\u00df\u00b7gen", "sind", "so", "schmal", ",", "als", "wie", "die", "Straus\u00b7sen", "weis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Z\u00e4hne k\u00f6nnen wir mit Recht emaille heissen;", "tokens": ["Die", "Z\u00e4h\u00b7ne", "k\u00f6n\u00b7nen", "wir", "mit", "Recht", "e\u00b7mail\u00b7le", "heis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die H\u00e4nde f\u00fchlen sich so sanfft und weichlich an,", "tokens": ["Die", "H\u00e4n\u00b7de", "f\u00fch\u00b7len", "sich", "so", "sanfft", "und", "weich\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als eine Juchten-Haut und grober Cortuan;", "tokens": ["Als", "ei\u00b7ne", "Juch\u00b7ten\u00b7Haut", "und", "gro\u00b7ber", "Cor\u00b7tu\u00b7an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Augen lichter Blitz, gleicht schimmernden Carfunckel,", "tokens": ["Der", "Au\u00b7gen", "lich\u00b7ter", "Blitz", ",", "gleicht", "schim\u00b7mern\u00b7den", "Car\u00b7fun\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du machst ein Zimmer hell und w\u00e4r es noch so dunckel.", "tokens": ["Du", "machst", "ein", "Zim\u00b7mer", "hell", "und", "w\u00e4r", "es", "noch", "so", "dun\u00b7ckel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie gl\u00fccklich und vergn\u00fcgt mu\u00df nun derjenge seyn,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "und", "ver\u00b7gn\u00fcgt", "mu\u00df", "nun", "der\u00b7jen\u00b7ge", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "VMFIN", "ADV", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dem du dein Hertze wirst durch Band und Mahlschatz weyhn.", "tokens": ["Dem", "du", "dein", "Hert\u00b7ze", "wirst", "durch", "Band", "und", "Mahl\u00b7schatz", "weyhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "VVFIN", "VAFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wahrhafftig gl\u00fccklich genug, er darff nicht Wache stehen,", "tokens": ["Wahr\u00b7haff\u00b7tig", "gl\u00fcck\u00b7lich", "ge\u00b7nug", ",", "er", "darff", "nicht", "Wa\u00b7che", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "$,", "PPER", "VMFIN", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Denn keiner wird so leicht ihm ins Gehege gehen.", "tokens": ["Denn", "kei\u00b7ner", "wird", "so", "leicht", "ihm", "ins", "Ge\u00b7he\u00b7ge", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADJD", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Lorettgen, nettes Kind, dein Leibgen k\u00f6mmet mir", "tokens": ["Lo\u00b7rett\u00b7gen", ",", "net\u00b7tes", "Kind", ",", "dein", "Leib\u00b7gen", "k\u00f6m\u00b7met", "mir"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "