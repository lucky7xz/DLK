{"textgrid.poem.32328": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "5. Nikander", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nikandern gl\u00fcckte j\u00fcngst ein trefflich Epigramm,", "tokens": ["Ni\u00b7kan\u00b7dern", "gl\u00fcck\u00b7te", "j\u00fcngst", "ein", "treff\u00b7lich", "E\u00b7pig\u00b7ramm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So fein, so scharf, als je von K\u00e4stnern eines kam.", "tokens": ["So", "fein", ",", "so", "scharf", ",", "als", "je", "von", "K\u00e4st\u00b7nern", "ei\u00b7nes", "kam", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "KOUS", "ADV", "APPR", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun schwitzt er Tag und Nacht, ein zweites auszuhecken.", "tokens": ["Nun", "schwitzt", "er", "Tag", "und", "Nacht", ",", "ein", "zwei\u00b7tes", "aus\u00b7zu\u00b7he\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,", "ART", "ADJA", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vergebens; was er macht, verdirbt.", "tokens": ["Ver\u00b7ge\u00b7bens", ";", "was", "er", "macht", ",", "ver\u00b7dirbt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$.", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra