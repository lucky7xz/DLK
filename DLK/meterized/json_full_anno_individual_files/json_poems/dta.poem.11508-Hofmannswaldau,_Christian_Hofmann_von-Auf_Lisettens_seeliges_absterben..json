{"dta.poem.11508": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Lisettens seeliges absterben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ljsette schlief so sanfft und seclig ein:", "tokens": ["Ljset\u00b7te", "schlief", "so", "sanfft", "und", "se\u00b7clig", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df, wenn es k\u00f6nte m\u00f6glich seyn,", "tokens": ["Da\u00df", ",", "wenn", "es", "k\u00f6n\u00b7te", "m\u00f6g\u00b7lich", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df auch die engel sterben solten,", "tokens": ["Da\u00df", "auch", "die", "en\u00b7gel", "ster\u00b7ben", "sol\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie gantz gewi\u00df nicht anders sterben wolten.", "tokens": ["Sie", "gantz", "ge\u00b7wi\u00df", "nicht", "an\u00b7ders", "ster\u00b7ben", "wol\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}