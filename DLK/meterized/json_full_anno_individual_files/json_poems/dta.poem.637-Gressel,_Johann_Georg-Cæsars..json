{"dta.poem.637": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "C\u00e6sars.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich stegte/ wenn ich kahm/ ja wenn man mich nur sahe", "tokens": ["Ich", "steg\u00b7te", "/", "wenn", "ich", "kahm", "/", "ja", "wenn", "man", "mich", "nur", "sa\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "VVFIN", "$(", "ADV", "KOUS", "PIS", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So war der Feind schon fort. Rom ehrte mich darum/", "tokens": ["So", "war", "der", "Feind", "schon", "fort", ".", "Rom", "ehr\u00b7te", "mich", "da\u00b7rum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PTKVZ", "$.", "NE", "VVFIN", "PRF", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und gab mir seinen Thron/ allein dadurch geschahe", "tokens": ["Und", "gab", "mir", "sei\u00b7nen", "Thron", "/", "al\u00b7lein", "da\u00b7durch", "ge\u00b7scha\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "ADV", "PAV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df eine Laster-Brut mich rasend brachte um.", "tokens": ["Da\u00df", "ei\u00b7ne", "Las\u00b7ter\u00b7Brut", "mich", "ra\u00b7send", "brach\u00b7te", "um", "."], "token_info": ["word", "word"