{"dta.poem.10575": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n  Der Ph\u00f6nix.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Am Mastbaum gelehnt, auf dem hohen Verdeck,", "tokens": ["Am", "Mast\u00b7baum", "ge\u00b7lehnt", ",", "auf", "dem", "ho\u00b7hen", "Ver\u00b7deck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Stand ich und h\u00f6rt' ich des Vogels Gesang.", "tokens": ["Stand", "ich", "und", "h\u00f6rt'", "ich", "des", "Vo\u00b7gels", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wie schwarzgr\u00fcne Rosse mit silbernen M\u00e4hnen,", "tokens": ["Wie", "schwarz\u00b7gr\u00fc\u00b7ne", "Ros\u00b7se", "mit", "sil\u00b7ber\u00b7nen", "M\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sprangen die wei\u00dfgekr\u00e4uselten Wellen,", "tokens": ["Spran\u00b7gen", "die", "wei\u00df\u00b7ge\u00b7kr\u00e4u\u00b7sel\u00b7ten", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Wie Schw\u00e4nenz\u00fcge schifften vor\u00fcber,", "tokens": ["Wie", "Schw\u00e4\u00b7nen\u00b7z\u00fc\u00b7ge", "schiff\u00b7ten", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit schimmernden Segeln, die Helgolander,", "tokens": ["Mit", "schim\u00b7mern\u00b7den", "Se\u00b7geln", ",", "die", "Hel\u00b7go\u00b7lan\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Die kecken Nomaden der Nordsee;", "tokens": ["Die", "ke\u00b7cken", "No\u00b7ma\u00b7den", "der", "Nord\u00b7see", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Ueber mein Haupt, im ewigen Blau,", "tokens": ["Ue\u00b7ber", "mein", "Haupt", ",", "im", "e\u00b7wi\u00b7gen", "Blau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Hinflatterte wei\u00dfes Gew\u00f6lk", "tokens": ["Hin\u00b7flat\u00b7ter\u00b7te", "wei\u00b7\u00dfes", "Ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Und prangte die ewige Sonne,", "tokens": ["Und", "prang\u00b7te", "die", "e\u00b7wi\u00b7ge", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Die Rose des Himmels, die feuerbl\u00fchende,", "tokens": ["Die", "Ro\u00b7se", "des", "Him\u00b7mels", ",", "die", "feu\u00b7er\u00b7bl\u00fc\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Die freudvoll sich im Meer bespiegelte;", "tokens": ["Die", "freud\u00b7voll", "sich", "im", "Meer", "be\u00b7spie\u00b7gel\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und Himmel und Meer und mein eignes Herz", "tokens": ["Und", "Him\u00b7mel", "und", "Meer", "und", "mein", "eig\u00b7nes", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Ert\u00f6nten im Nachhall:", "tokens": ["Er\u00b7t\u00f6n\u00b7ten", "im", "Nach\u00b7hall", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Sie liebt ihn! sie liebt ihn!", "tokens": ["Sie", "liebt", "ihn", "!", "sie", "liebt", "ihn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}