{"textgrid.poem.24523": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Monsieur Schenckens Musicalia auf der Viole de gambe", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kunst und Erfindung sind hier unvergleichlich sch\u00f6n/", "tokens": ["Kunst", "und", "Er\u00b7fin\u00b7dung", "sind", "hier", "un\u00b7ver\u00b7gleich\u00b7lich", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADV", "ADJD", "ADJD", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Nur etwas mangelt noch: Wilst du gedoppelt leben/", "tokens": ["Nur", "et\u00b7was", "man\u00b7gelt", "noch", ":", "Wilst", "du", "ge\u00b7dop\u00b7pelt", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "$.", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So la\u00df uns deiner Kunst die Ehr' und Anmuth geben/", "tokens": ["So", "la\u00df", "uns", "dei\u00b7ner", "Kunst", "die", "Ehr'", "und", "An\u00b7muth", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "ART", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die wir/ wenn du sie spielst/ im h\u00f6chsten Grade sehn.", "tokens": ["Die", "wir", "/", "wenn", "du", "sie", "spielst", "/", "im", "h\u00f6chs\u00b7ten", "Gra\u00b7de", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "P