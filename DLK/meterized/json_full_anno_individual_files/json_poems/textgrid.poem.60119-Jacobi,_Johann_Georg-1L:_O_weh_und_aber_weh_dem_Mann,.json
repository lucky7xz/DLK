{"textgrid.poem.60119": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: O weh und aber weh dem Mann,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O weh und aber weh dem Mann,", "tokens": ["O", "weh", "und", "a\u00b7ber", "weh", "dem", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sch\u00f6nes nicht auf Erden liebt,", "tokens": ["Der", "Sch\u00f6\u00b7nes", "nicht", "auf", "Er\u00b7den", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich keines Dings erfreuen kann,", "tokens": ["Sich", "kei\u00b7nes", "Dings", "er\u00b7freu\u00b7en", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein volles Herz an keins ergiebt!", "tokens": ["Sein", "vol\u00b7les", "Herz", "an", "keins", "er\u00b7giebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O wehe, wer sich nie vereint", "tokens": ["O", "we\u00b7he", ",", "wer", "sich", "nie", "ver\u00b7eint"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "PWS", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Wies' und Quell und Bl\u00fcthenast,", "tokens": ["Mit", "Wies'", "und", "Quell", "und", "Bl\u00fc\u00b7the\u00b7nast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sein M\u00e4dchen auch und seinen Freund", "tokens": ["Sein", "M\u00e4d\u00b7chen", "auch", "und", "sei\u00b7nen", "Freund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit halber Seele nur umfa\u00dft!", "tokens": ["Mit", "hal\u00b7ber", "See\u00b7le", "nur", "um\u00b7fa\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wieder wehe, weh dem Mann,", "tokens": ["Und", "wie\u00b7der", "we\u00b7he", ",", "weh", "dem", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Liebe zieht, den Liebe dr\u00e4ngt!", "tokens": ["Den", "Lie\u00b7be", "zieht", ",", "den", "Lie\u00b7be", "dr\u00e4ngt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sch\u00f6nes sucht, und fest daran", "tokens": ["Der", "Sch\u00f6\u00b7nes", "sucht", ",", "und", "fest", "da\u00b7ran"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJD", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein ganzes Herz auf immer h\u00e4ngt!", "tokens": ["Sein", "gan\u00b7zes", "Herz", "auf", "im\u00b7mer", "h\u00e4ngt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Erd' es tr\u00e4gt, verschwindets bald.", "tokens": ["Wenn", "Erd'", "es", "tr\u00e4gt", ",", "ver\u00b7schwin\u00b7dets", "bald", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Bl\u00fcthenast am Quell verdirbt:", "tokens": ["Der", "Bl\u00fc\u00b7the\u00b7nast", "am", "Quell", "ver\u00b7dirbt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Freundesbusen wird es kalt;", "tokens": ["Im", "Freun\u00b7des\u00b7bu\u00b7sen", "wird", "es", "kalt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ach! das treue M\u00e4dchen stirbt.", "tokens": ["Und", "ach", "!", "das", "treu\u00b7e", "M\u00e4d\u00b7chen", "stirbt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mag lieben denn, mag lieben nicht!", "tokens": ["Mag", "lie\u00b7ben", "denn", ",", "mag", "lie\u00b7ben", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KON", "$,", "VMFIN", "VVINF", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O weh und aber wehe mir!", "tokens": ["O", "weh", "und", "a\u00b7ber", "we\u00b7he", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Liebe strahlt das Sonnenlicht,", "tokens": ["In", "Lie\u00b7be", "strahlt", "das", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00e4llt auf lauter Gr\u00e4ber hier.", "tokens": ["Und", "f\u00e4llt", "auf", "lau\u00b7ter", "Gr\u00e4\u00b7ber", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was einst ich an mein Herz gedr\u00fcckt,", "tokens": ["Was", "einst", "ich", "an", "mein", "Herz", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist Asche nun und Todtenbein;", "tokens": ["Ist", "A\u00b7sche", "nun", "und", "Tod\u00b7ten\u00b7bein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es sank, wo ich die Gruft geschm\u00fcckt;", "tokens": ["Es", "sank", ",", "wo", "ich", "die", "Gruft", "ge\u00b7schm\u00fcckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm sinket nach der Leichenstein.", "tokens": ["Ihm", "sin\u00b7ket", "nach", "der", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohin, wohin? Denn Lieb' ist Noth,", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", "?", "Denn", "Lieb'", "ist", "Noth", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$.", "KON", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Alles wankt, und Alles weicht;", "tokens": ["Und", "Al\u00b7les", "wankt", ",", "und", "Al\u00b7les", "weicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gebohren wird's und geht in Tod:", "tokens": ["Ge\u00b7boh\u00b7ren", "wird's", "und", "geht", "in", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin, so weit der Himmel reicht?", "tokens": ["Wo\u00b7hin", ",", "so", "weit", "der", "Him\u00b7mel", "reicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu dir hinauf, du Gotteskraft,", "tokens": ["Zu", "dir", "hin\u00b7auf", ",", "du", "Got\u00b7tes\u00b7kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Baum und Wiesenquell erneut,", "tokens": ["Die", "Baum", "und", "Wie\u00b7sen\u00b7quell", "er\u00b7neut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ohn' Ende wirkt, ohn' Ende schafft,", "tokens": ["Ohn'", "En\u00b7de", "wirkt", ",", "ohn'", "En\u00b7de", "schafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und noch das Grab voll Blumen streut!", "tokens": ["Und", "noch", "das", "Grab", "voll", "Blu\u00b7men", "streut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O du, dein Athem ists allein,", "tokens": ["O", "du", ",", "dein", "A\u00b7them", "ists", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der allen Staub lebendig weht;", "tokens": ["Der", "al\u00b7len", "Staub", "le\u00b7ben\u00b7dig", "weht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du gabst den Sternen ihren Schein,", "tokens": ["Du", "gabst", "den", "Ster\u00b7nen", "ih\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bleibst, wenn Erd und Meer vergeht.", "tokens": ["Und", "bleibst", ",", "wenn", "Erd", "und", "Meer", "ver\u00b7geht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu dir hinauf erhebe mich,", "tokens": ["Zu", "dir", "hin\u00b7auf", "er\u00b7he\u00b7be", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu deiner unsichtbaren Welt!", "tokens": ["Zu", "dei\u00b7ner", "un\u00b7sicht\u00b7ba\u00b7ren", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da lebt und liebt's, und ewiglich", "tokens": ["Da", "lebt", "und", "liebt's", ",", "und", "e\u00b7wig\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird bleiben, was an dir sich h\u00e4lt.", "tokens": ["Wird", "blei\u00b7ben", ",", "was", "an", "dir", "sich", "h\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,", "PRELS", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O weh und aber weh dem Mann,", "tokens": ["O", "weh", "und", "a\u00b7ber", "weh", "dem", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sch\u00f6nes nicht auf Erden liebt,", "tokens": ["Der", "Sch\u00f6\u00b7nes", "nicht", "auf", "Er\u00b7den", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "NN", "VVFIN