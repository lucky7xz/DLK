{"dta.poem.21217": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Abend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Senke dich von Purpurwolken,", "tokens": ["Sen\u00b7ke", "dich", "von", "Pur\u00b7pur\u00b7wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Holder Abend, sanft herab!", "tokens": ["Hol\u00b7der", "A\u00b7bend", ",", "sanft", "her\u00b7ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hauche reine frische L\u00fcfte!", "tokens": ["Hau\u00b7che", "rei\u00b7ne", "fri\u00b7sche", "L\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcttle Thau, und Rosend\u00fcfte,", "tokens": ["Sch\u00fctt\u00b7le", "Thau", ",", "und", "Ro\u00b7sen\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von den feuchten Schwingen ab!", "tokens": ["Von", "den", "feuch\u00b7ten", "Schwin\u00b7gen", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Wald steht dunkelgr\u00fcn; von langen Matten", "tokens": ["Der", "Wald", "steht", "dun\u00b7kel\u00b7gr\u00fcn", ";", "von", "lan\u00b7gen", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhebet sich der k\u00fchle Thau.", "tokens": ["Er\u00b7he\u00b7bet", "sich", "der", "k\u00fch\u00b7le", "Thau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Abendwind erquickt, bey k\u00fchlem Schatten.", "tokens": ["Der", "Ab\u00b7end\u00b7wind", "er\u00b7quickt", ",", "bey", "k\u00fch\u00b7lem", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das stille Thal, die Au.", "tokens": ["Das", "stil\u00b7le", "Thal", ",", "die", "Au", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt rauscht der Busch, ietzt wallen die Gefilde;", "tokens": ["Jetzt", "rauscht", "der", "Busch", ",", "ietzt", "wal\u00b7len", "die", "Ge\u00b7fil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der laute Bach rinnt hell und milde", "tokens": ["Der", "lau\u00b7te", "Bach", "rinnt", "hell", "und", "mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von Felsen ab, und alles f\u00e4llt vergn\u00fcgt", "tokens": ["Von", "Fel\u00b7sen", "ab", ",", "und", "al\u00b7les", "f\u00e4llt", "ver\u00b7gn\u00fcgt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "$,", "KON", "PIS", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In Schlaf und Traum, vom Westwind eingewiegt.", "tokens": ["In", "Schlaf", "und", "Traum", ",", "vom", "West\u00b7wind", "ein\u00b7ge\u00b7wiegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Komm, holder Schlaf! die matten Augen", "tokens": ["Komm", ",", "hol\u00b7der", "Schlaf", "!", "die", "mat\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sinken,\nDie g\u00fcldnen Sterne winken\nZur s\u00fc\u00dfen Ruh.\nNichts kan des Frommen Schlummer st\u00f6ren,\nEr wird besch\u00fctzt von starker Engel Heeren;\nDer Himmel deckt ihn zu.", "tokens": ["sin\u00b7ken", ",", "Die", "g\u00fcld\u00b7nen", "Ster\u00b7ne", "win\u00b7ken", "Zur", "s\u00fc\u00b7\u00dfen", "Ruh", ".", "Nichts", "kan", "des", "From\u00b7men", "Schlum\u00b7mer", "st\u00f6\u00b7ren", ",", "Er", "wird", "be\u00b7sch\u00fctzt", "von", "star\u00b7ker", "En\u00b7gel", "Hee\u00b7ren", ";", "Der", "Him\u00b7mel", "deckt", "ihn", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$.", "PIS", "VMFIN", "ART", "NN", "NN", "VVINF", "$,", "PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+--+-+-+-+-+-+--+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.invert"}}}}}