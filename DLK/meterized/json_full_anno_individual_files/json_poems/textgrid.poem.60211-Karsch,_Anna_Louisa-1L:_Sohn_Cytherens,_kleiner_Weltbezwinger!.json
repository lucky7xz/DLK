, "NN", "APPR", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Schwarzen Augen, die mich t\u00f6dten wollen", "tokens": ["Schwar\u00b7zen", "Au\u00b7gen", ",", "die", "mich", "t\u00f6d\u00b7ten", "wol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und mit einem Munde rosenweich,", "tokens": ["Und", "mit", "ei\u00b7nem", "Mun\u00b7de", "ro\u00b7sen\u00b7weich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Findet Wollust in der Kunst zu qu\u00e4len.", "tokens": ["Fin\u00b7det", "Wol\u00b7lust", "in", "der", "Kunst", "zu", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Zw\u00f6lf betr\u00fcbte Tage mu\u00df ich z\u00e4hlen", "tokens": ["Zw\u00f6lf", "be\u00b7tr\u00fcb\u00b7te", "Ta\u00b7ge", "mu\u00df", "ich", "z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Jeder ist den Erndte-Tagen gleich.", "tokens": ["Je\u00b7der", "ist", "den", "Ernd\u00b7te\u00b7Ta\u00b7gen", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "O du kennst die Th\u00e4ler, wo er gehet,", "tokens": ["O", "du", "kennst", "die", "Th\u00e4\u00b7ler", ",", "wo", "er", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dort, wo deiner Mutter Bildni\u00df stehet", "tokens": ["Dort", ",", "wo", "dei\u00b7ner", "Mut\u00b7ter", "Bild\u00b7ni\u00df", "ste\u00b7het"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In dem Palmen-Hayn, da wandelt er!", "tokens": ["In", "dem", "Pal\u00b7men\u00b7Hayn", ",", "da", "wan\u00b7delt", "er", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Such ihn unter dickbelaubten Eichen,", "tokens": ["Such", "ihn", "un\u00b7ter", "dick\u00b7be\u00b7laub\u00b7ten", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und will er zu Rosenhecken weichen,", "tokens": ["Und", "will", "er", "zu", "Ro\u00b7sen\u00b7he\u00b7cken", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Flattre um ihn, wie ein Vogel her,", "tokens": ["Flatt\u00b7re", "um", "ihn", ",", "wie", "ein", "Vo\u00b7gel", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PWAV", "ART", "NN", "PTKVZ", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}}, "stanza.13": {"line.1": {"text": "Hurtig ist er, gleich den jungen Rehen!", "tokens": ["Hur\u00b7tig", "ist", "er", ",", "gleich", "den", "jun\u00b7gen", "Re\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber bleibt er an dem Wasser stehen,", "tokens": ["A\u00b7ber", "bleibt", "er", "an", "dem", "Was\u00b7ser", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo der weiche Klee am Ufer gr\u00fcnt;", "tokens": ["Wo", "der", "wei\u00b7che", "Klee", "am", "U\u00b7fer", "gr\u00fcnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dann erinnre dich, was ich gelitten,", "tokens": ["Dann", "e\u00b7rinn\u00b7re", "dich", ",", "was", "ich", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Spann den Bogen, fa\u00df ihn in der Mitten,", "tokens": ["Spann", "den", "Bo\u00b7gen", ",", "fa\u00df", "ihn", "in", "der", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Triff die Stelle, die den Pfeil verdient!", "tokens": ["Triff", "die", "Stel\u00b7le", ",", "die", "den", "Pfeil", "ver\u00b7dient", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "In sein Herz, noch k\u00e4lter als die Schollen,", "tokens": ["In", "sein", "Herz", ",", "noch", "k\u00e4l\u00b7ter", "als", "die", "Schol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die dem Blick der Sonne trotzen wollen,", "tokens": ["Die", "dem", "Blick", "der", "Son\u00b7ne", "trot\u00b7zen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Amor, in sein Herze ziele du.", "tokens": ["A\u00b7mor", ",", "in", "sein", "Her\u00b7ze", "zie\u00b7le", "du", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dann wird ihm die tiefe Wunde schmerzen,", "tokens": ["Dann", "wird", "ihm", "die", "tie\u00b7fe", "Wun\u00b7de", "schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und er eilt mit halb zerschmolznem Herzen", "tokens": ["Und", "er", "eilt", "mit", "halb", "zer\u00b7schmolz\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.