{"dta.poem.211": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als ihn die sch\u00f6ne  Secantis  nach langen Ab-  \n wesen wieder umarmete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So hat mein Trauren doch zu Lachen werden m\u00fcssen/", "tokens": ["So", "hat", "mein", "Trau\u00b7ren", "doch", "zu", "La\u00b7chen", "wer\u00b7den", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "VAINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da dich das holde Gl\u00fcck mir in die Arme giebt/", "tokens": ["Da", "dich", "das", "hol\u00b7de", "Gl\u00fcck", "mir", "in", "die", "Ar\u00b7me", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach schwartzer Kummer-Nacht kan ich den Morgen gr\u00fcssen/", "tokens": ["Nach", "schwart\u00b7zer", "Kum\u00b7mer\u00b7Nacht", "kan", "ich", "den", "Mor\u00b7gen", "gr\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil mich dein Abseyn nun mit Angst nicht mehr betr\u00fcbt.", "tokens": ["Weil", "mich", "dein", "Ab\u00b7seyn", "nun", "mit", "Angst", "nicht", "mehr", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "APPR", "NN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Egyptens K\u00e4rcker wird ins K\u00f6nigs Sahl verkehret/", "tokens": ["E\u00b7gyp\u00b7tens", "K\u00e4r\u00b7cker", "wird", "ins", "K\u00f6\u00b7nigs", "Sahl", "ver\u00b7keh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "APPRART", "NN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nachdem mich meine Sonn mit ihrem Strahl anlacht/", "tokens": ["Nach\u00b7dem", "mich", "mei\u00b7ne", "Sonn", "mit", "ih\u00b7rem", "Strahl", "an\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den sie der bittern See/ sonst eine Zeit gewehret ", "tokens": ["Den", "sie", "der", "bit\u00b7tern", "See", "/", "sonst", "ei\u00b7ne", "Zeit", "ge\u00b7weh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "$(", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und mir die Nacht dadurch der ", "tokens": ["Und", "mir", "die", "Nacht", "da\u00b7durch", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "PAV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nun aber l\u00e4st mein Geist die langgewehrten Fasten/", "tokens": ["Nun", "a\u00b7ber", "l\u00e4st", "mein", "Geist", "die", "lang\u00b7ge\u00b7wehr\u00b7ten", "Fas\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und hebet h\u00f6chst-vergn\u00fcgt die Oster-Woche an/", "tokens": ["Und", "he\u00b7bet", "h\u00f6chst\u00b7ver\u00b7gn\u00fcgt", "die", "Os\u00b7ter\u00b7Wo\u00b7che", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein Froh-seyn l\u00e4sset mich nicht l\u00e4nger traurig rasten/", "tokens": ["Mein", "Froh\u00b7seyn", "l\u00e4s\u00b7set", "mich", "nicht", "l\u00e4n\u00b7ger", "trau\u00b7rig", "ras\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da ich mein Leben jetzt vergn\u00fcgt umfassen kan.", "tokens": ["Da", "ich", "mein", "Le\u00b7ben", "jetzt", "ver\u00b7gn\u00fcgt", "um\u00b7fas\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Sommer meiner Lust mit sch\u00f6nen Blumen spielet/", "tokens": ["Der", "Som\u00b7mer", "mei\u00b7ner", "Lust", "mit", "sch\u00f6\u00b7nen", "Blu\u00b7men", "spie\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die in dem Angesicht und auf der Br\u00fcste-Feld/", "tokens": ["Die", "in", "dem", "An\u00b7ge\u00b7sicht", "und", "auf", "der", "Br\u00fcs\u00b7te\u00b7Feld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die g\u00fcnstige Natur mit grossem Flei\u00df erziehlet/", "tokens": ["Die", "g\u00fcns\u00b7ti\u00b7ge", "Na\u00b7tur", "mit", "gros\u00b7sem", "Flei\u00df", "er\u00b7zieh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und zum Genusse mir in ihrer Pracht erh\u00e4lt.", "tokens": ["Und", "zum", "Ge\u00b7nus\u00b7se", "mir", "in", "ih\u00b7rer", "Pracht", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Stirn-", "tokens": ["Die", "Stirn"], "token_info": ["word", "word"], "pos": ["ART", "TRUNC"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Die Wangen-Rose frist kein sch\u00e4dlich Raupe ab/", "tokens": ["Die", "Wan\u00b7gen\u00b7Ro\u00b7se", "frist", "kein", "sch\u00e4d\u00b7lich", "Rau\u00b7pe", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ADJD", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Schnee erschw\u00e4rtzet schier vor deiner Br\u00fcste-Liljen/", "tokens": ["Der", "Schnee", "er\u00b7schw\u00e4rt\u00b7zet", "schier", "vor", "dei\u00b7ner", "Br\u00fcs\u00b7te\u00b7Lil\u00b7jen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Vor deinen Adern geht die blau Viol ins Grab.", "tokens": ["Vor", "dei\u00b7nen", "A\u00b7dern", "geht", "die", "blau", "Vi\u00b7ol", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJD", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Bl\u00fchte des ", "tokens": ["Die", "Bl\u00fch\u00b7te", "des"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Der Balsam Indiens kan nicht so st\u00e4rckend seyn/", "tokens": ["Der", "Bal\u00b7sam", "In\u00b7di\u00b7ens", "kan", "nicht", "so", "st\u00e4r\u00b7ckend", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VMFIN", "PTKNEG", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als wie dein Athem-Dufft erquicket was erblichen/", "tokens": ["Als", "wie", "dein", "A\u00b7them\u00b7Dufft", "er\u00b7quic\u00b7ket", "was", "er\u00b7bli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPOSAT", "NN", "VVFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und wie dein Lippen-Oehl fl\u00f6\u00dft Lebens-Geister ein.", "tokens": ["Und", "wie", "dein", "Lip\u00b7pen\u00b7Oehl", "fl\u00f6\u00dft", "Le\u00b7bens\u00b7Geis\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich selber kan hiervon ein lebhafft Beyspiel geben/", "tokens": ["Ich", "sel\u00b7ber", "kan", "hier\u00b7von", "ein", "leb\u00b7hafft", "Bey\u00b7spiel", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PAV", "PTKVZ", "VVFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Denn da ich halb entseelt von vielen Sorgen war/", "tokens": ["Denn", "da", "ich", "halb", "ent\u00b7seelt", "von", "vie\u00b7len", "Sor\u00b7gen", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVPP", "APPR", "PIAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Bracht deine Gegenwart und Ku\u00df mir neues Leben.", "tokens": ["Bracht", "dei\u00b7ne", "Ge\u00b7gen\u00b7wart", "und", "Ku\u00df", "mir", "neu\u00b7es", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So da\u00df mich deine Gunst stellt wieder lebend dar.", "tokens": ["So", "da\u00df", "mich", "dei\u00b7ne", "Gunst", "stellt", "wie\u00b7der", "le\u00b7bend", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}