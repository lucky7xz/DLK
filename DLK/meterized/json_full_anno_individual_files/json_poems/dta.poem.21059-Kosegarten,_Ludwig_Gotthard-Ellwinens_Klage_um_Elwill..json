{"dta.poem.21059": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Ellwinens Klage um Elwill.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Elwill, Elwill, du mein Gram, mein Sehnen,               ", "tokens": ["El\u00b7will", ",", "El\u00b7will", ",", "du", "mein", "Gram", ",", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mein erw\u00e4hlter, mein verlorner Freund,", "tokens": ["Mein", "er\u00b7w\u00e4hl\u00b7ter", ",", "mein", "ver\u00b7lor\u00b7ner", "Freund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Siehst du auch die tausend Jammerthr\u00e4nen,", "tokens": ["Siehst", "du", "auch", "die", "tau\u00b7send", "Jam\u00b7mert\u00b7hr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die um dich dein traurend M\u00e4dchen weint?", "tokens": ["Die", "um", "dich", "dein", "trau\u00b7rend", "M\u00e4d\u00b7chen", "weint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "H\u00f6rst du auch mein mittern\u00e4chtlich St\u00f6hnen?", "tokens": ["H\u00f6rst", "du", "auch", "mein", "mit\u00b7ter\u00b7n\u00e4cht\u00b7lich", "St\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Weisst um meines Herzens kranken Schlag?", "tokens": ["Weisst", "um", "mei\u00b7nes", "Her\u00b7zens", "kran\u00b7ken", "Schlag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Oder schweigt den Hallelujaht\u00f6nen", "tokens": ["O\u00b7der", "schweigt", "den", "Hal\u00b7le\u00b7lu\u00b7jah\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Deines Edens jedes Erdenach?", "tokens": ["Dei\u00b7nes", "E\u00b7dens", "je\u00b7des", "Er\u00b7de\u00b7nach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ach verloren, Elwill, ach verlassen", "tokens": ["Ach", "ver\u00b7lo\u00b7ren", ",", "El\u00b7will", ",", "ach", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ITJ", "VVPP", "$,", "NE", "$,", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hast du mich in diesen W\u00fcsteneyn.", "tokens": ["Hast", "du", "mich", "in", "die\u00b7sen", "W\u00fcs\u00b7te\u00b7neyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf der Welt get\u00fcmmelvollen Strassen", "tokens": ["Auf", "der", "Welt", "ge\u00b7t\u00fcm\u00b7mel\u00b7vol\u00b7len", "Stras\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wandl' ich k\u00fcnftig einsam und allein.", "tokens": ["Wandl'", "ich", "k\u00fcnf\u00b7tig", "ein\u00b7sam", "und", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADJD", "KON", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Leichenfeyer d\u00fcnkt mich ihr Gepr\u00e4nge.", "tokens": ["Lei\u00b7chen\u00b7fe\u00b7yer", "d\u00fcnkt", "mich", "ihr", "Ge\u00b7pr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Ihre Bl\u00fcthe haucht mir Gr\u00e4bergraus,", "tokens": ["Ih\u00b7re", "Bl\u00fc\u00b7the", "haucht", "mir", "Gr\u00e4\u00b7ber\u00b7graus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und des Lebens f\u00fcrchterliche L\u00e4nge", "tokens": ["Und", "des", "Le\u00b7bens", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "L\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Dehnt sich mir zu Ewigkeiten aus.", "tokens": ["Dehnt", "sich", "mir", "zu", "E\u00b7wig\u00b7kei\u00b7ten", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Elwill, Elwill, wo sind nun die Stunden,", "tokens": ["El\u00b7will", ",", "El\u00b7will", ",", "wo", "sind", "nun", "die", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PWAV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die ich dir am Busen selig lag?", "tokens": ["Die", "ich", "dir", "am", "Bu\u00b7sen", "se\u00b7lig", "lag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ein Morgentraum sind sie verschwunden,", "tokens": ["Wie", "ein", "Mor\u00b7gen\u00b7traum", "sind", "sie", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sind verrieselt, wie ein Regenbach.", "tokens": ["Sind", "ver\u00b7rie\u00b7selt", ",", "wie", "ein", "Re\u00b7gen\u00b7bach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Elwill, Elwill, wo sind nun die Wonnen,", "tokens": ["El\u00b7will", ",", "El\u00b7will", ",", "wo", "sind", "nun", "die", "Won\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PWAV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die ich mir in deinem Arm verhiess?", "tokens": ["Die", "ich", "mir", "in", "dei\u00b7nem", "Arm", "ver\u00b7hiess", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Fr\u00fch verflattert sind sie, schnell verronnen,", "tokens": ["Fr\u00fch", "ver\u00b7flat\u00b7tert", "sind", "sie", ",", "schnell", "ver\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "PPER", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wie ein Dunstbild, das der Sturm zerriss.", "tokens": ["Wie", "ein", "Dunst\u00b7bild", ",", "das", "der", "Sturm", "zer\u00b7riss", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "All get\u00e4uscht ist nun mein irdisch Hoffen.", "tokens": ["All", "ge\u00b7t\u00e4uscht", "ist", "nun", "mein", "ir\u00b7disch", "Hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meine Saaten sind im Keim erstickt.", "tokens": ["Mei\u00b7ne", "Saa\u00b7ten", "sind", "im", "Keim", "er\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Bl\u00fcthen sind vom Hagelschlag getroffen;", "tokens": ["Mei\u00b7ne", "Bl\u00fc\u00b7then", "sind", "vom", "Ha\u00b7gel\u00b7schlag", "ge\u00b7trof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Meine Halme hat der Nord geknickt.", "tokens": ["Mei\u00b7ne", "Hal\u00b7me", "hat", "der", "Nord", "ge\u00b7knickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wie die Rebe, die der Ulm umranket,", "tokens": ["Wie", "die", "Re\u00b7be", ",", "die", "der", "Ulm", "um\u00b7ran\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wenn der Blitz den hohen Ulm zerbrach,", "tokens": ["Wenn", "der", "Blitz", "den", "ho\u00b7hen", "Ulm", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "St\u00fctzelos mit ihm zu Boden schwanket,", "tokens": ["St\u00fct\u00b7ze\u00b7los", "mit", "ihm", "zu", "Bo\u00b7den", "schwan\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Schwank' ich dir, du Fr\u00fchgefallner, nach.", "tokens": ["Schwank'", "ich", "dir", ",", "du", "Fr\u00fch\u00b7ge\u00b7fall\u00b7ner", ",", "nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPER", "NN", "$,", "APPR", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Hochverrath bed\u00fcnkt mich Erdenfreude;", "tokens": ["Hoch\u00b7ver\u00b7rath", "be\u00b7d\u00fcnkt", "mich", "Er\u00b7den\u00b7freu\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Frecher Frevel d\u00e4ucht mich froher Scherz.", "tokens": ["Fre\u00b7cher", "Fre\u00b7vel", "d\u00e4ucht", "mich", "fro\u00b7her", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Welkes Laub ist meine Augenweide;", "tokens": ["Wel\u00b7kes", "Laub", "ist", "mei\u00b7ne", "Au\u00b7gen\u00b7wei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "D\u00fcrres Bl\u00e4tterrascheln labt mein Herz.", "tokens": ["D\u00fcr\u00b7res", "Bl\u00e4t\u00b7ter\u00b7ra\u00b7scheln", "labt", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Meinem Gram hab' ich den Ring gegeben,", "tokens": ["Mei\u00b7nem", "Gram", "hab'", "ich", "den", "Ring", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Den ich dir zu geben am Altar", "tokens": ["Den", "ich", "dir", "zu", "ge\u00b7ben", "am", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PPER", "PTKZU", "VVINF", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und mein Leben mit dir durchzuleben,", "tokens": ["Und", "mein", "Le\u00b7ben", "mit", "dir", "durch\u00b7zu\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Tr\u00e4umt', und tr\u00e4umend, ach, so selig war!", "tokens": ["Tr\u00e4umt'", ",", "und", "tr\u00e4u\u00b7mend", ",", "ach", ",", "so", "se\u00b7lig", "war", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVPP", "$,", "ITJ", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Wenn der Morgen meine W\u00e4nde r\u00f6thet,", "tokens": ["Wenn", "der", "Mor\u00b7gen", "mei\u00b7ne", "W\u00e4n\u00b7de", "r\u00f6\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gr\u00fcss' ich ihn mit thr\u00e4nendunkelm Blick.", "tokens": ["Gr\u00fcss'", "ich", "ihn", "mit", "thr\u00e4\u00b7nen\u00b7dun\u00b7kelm", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn das Sp\u00e4troth in mein Fenster fl\u00f6tet,", "tokens": ["Wenn", "das", "Sp\u00e4t\u00b7roth", "in", "mein", "Fens\u00b7ter", "fl\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Tr\u00e4um' ich in die Vorzeit mich zur\u00fcck.", "tokens": ["Tr\u00e4um'", "ich", "in", "die", "Vor\u00b7zeit", "mich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn der Mitternacht wehm\u00fcthge H\u00fclle", "tokens": ["Wenn", "der", "Mit\u00b7ter\u00b7nacht", "weh\u00b7m\u00fcth\u00b7ge", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mich auf meinem Thr\u00e4nenlager h\u00fcllt,", "tokens": ["Mich", "auf", "mei\u00b7nem", "Thr\u00e4\u00b7nen\u00b7la\u00b7ger", "h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Fantasier' ich mich in jene Stille,", "tokens": ["Fan\u00b7ta\u00b7sier'", "ich", "mich", "in", "je\u00b7ne", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "In die Nacht hinunter, die dich h\u00fcllt.", "tokens": ["In", "die", "Nacht", "hin\u00b7un\u00b7ter", ",", "die", "dich", "h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Schlummre sanft in deiner Schlummerst\u00e4tte,", "tokens": ["Schlumm\u00b7re", "sanft", "in", "dei\u00b7ner", "Schlum\u00b7mer\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mein Geliebter, schlummre sanft und s\u00fcss,", "tokens": ["Mein", "Ge\u00b7lieb\u00b7ter", ",", "schlumm\u00b7re", "sanft", "und", "s\u00fcss", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bis die gr\u00e4bersprengende Drommete", "tokens": ["Bis", "die", "gr\u00e4\u00b7ber\u00b7spren\u00b7gen\u00b7de", "Drom\u00b7me\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--++-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dich entruft der tiefsten Finsterniss.", "tokens": ["Dich", "ent\u00b7ruft", "der", "tiefs\u00b7ten", "Fins\u00b7ter\u00b7niss", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Dein vergessen werd' ich nimmer, nimmer,", "tokens": ["Dein", "ver\u00b7ges\u00b7sen", "werd'", "ich", "nim\u00b7mer", ",", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "VVPP", "VAFIN", "PPER", "ADV", "$,", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bis mein Geist die Schale Lethens trinkt;", "tokens": ["Bis", "mein", "Geist", "die", "Scha\u00b7le", "Le\u00b7thens", "trinkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Dein gedenken werd' ich immer, immer,", "tokens": ["Dein", "ge\u00b7den\u00b7ken", "werd'", "ich", "im\u00b7mer", ",", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VAFIN", "PPER", "ADV", "$,", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Bis dein Schatten meinen zu sich winkt.", "tokens": ["Bis", "dein", "Schat\u00b7ten", "mei\u00b7nen", "zu", "sich", "winkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Hoffnung, ach, des Wiederauferstehens", "tokens": ["Hoff\u00b7nung", ",", "ach", ",", "des", "Wie\u00b7der\u00b7au\u00b7fer\u00b7ste\u00b7hens"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ITJ", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "An der Allvollendung grossem Tag,", "tokens": ["An", "der", "All\u00b7vol\u00b7len\u00b7dung", "gros\u00b7sem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "S\u00fcsse Hoffnung jenes Wiedersehens,", "tokens": ["S\u00fcs\u00b7se", "Hoff\u00b7nung", "je\u00b7nes", "Wie\u00b7der\u00b7se\u00b7hens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Nie verbittert durch der Trennung Ach \u2014", "tokens": ["Nie", "ver\u00b7bit\u00b7tert", "durch", "der", "Tren\u00b7nung", "Ach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Reich' mir, Sel'ge, deinen Lilienstengel", "tokens": ["Reich'", "mir", ",", "Sel'\u00b7ge", ",", "dei\u00b7nen", "Li\u00b7li\u00b7ens\u00b7ten\u00b7gel"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Auf des Lebens schw\u00fclem Wallertag,", "tokens": ["Auf", "des", "Le\u0