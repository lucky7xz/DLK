{"dta.poem.23537": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von und aus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Burgen sa\u00dfen Edelleute,", "tokens": ["Auf", "Bur\u00b7gen", "sa\u00b7\u00dfen", "E\u00b7del\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo aber sind die Burgen heute?", "tokens": ["Wo", "a\u00b7ber", "sind", "die", "Bur\u00b7gen", "heu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es wohnt oft ohne Hab' und Gut", "tokens": ["Es", "wohnt", "oft", "oh\u00b7ne", "Hab'", "und", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Thale manches adlich Blut.", "tokens": ["Im", "Tha\u00b7le", "man\u00b7ches", "ad\u00b7lich", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und von den G\u00fctern ihrer Lieben", "tokens": ["Und", "von", "den", "G\u00fc\u00b7tern", "ih\u00b7rer", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist ihnen nur ein ", "tokens": ["Ist", "ih\u00b7nen", "nur", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Des alten Namens Herrlichkeit", "tokens": ["Des", "al\u00b7ten", "Na\u00b7mens", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blieb manchem nur in unsrer Zeit.", "tokens": ["Blieb", "man\u00b7chem", "nur", "in", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So bin auch ", "tokens": ["So", "bin", "auch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Wer wird ein ", "tokens": ["Wer", "wird", "ein"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VAFIN", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Ich bin nur ", "tokens": ["Ich", "bin", "nur"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Jetzt hab' ich weder Hof noch Haus.", "tokens": ["Jetzt", "hab'", "ich", "we\u00b7der", "Hof", "noch", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}