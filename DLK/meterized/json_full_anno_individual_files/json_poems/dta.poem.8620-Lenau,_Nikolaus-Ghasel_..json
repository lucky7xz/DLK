{"dta.poem.8620": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Ghasel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Du, sch\u00f6ne Stunde, warst mir hold, so hold, wie keine noch,", "tokens": ["Du", ",", "sch\u00f6\u00b7ne", "Stun\u00b7de", ",", "warst", "mir", "hold", ",", "so", "hold", ",", "wie", "kei\u00b7ne", "noch", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VAFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "$,", "PWAV", "PIAT", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ich seh' dein Angesicht ergl\u00fch'n im Rosenscheine noch;", "tokens": ["Ich", "seh'", "dein", "An\u00b7ge\u00b7sicht", "er\u00b7gl\u00fch'n", "im", "Ro\u00b7sen\u00b7schei\u00b7ne", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "So sah den Engel Gottes einst mit Wangen freudenroth", "tokens": ["So", "sah", "den", "En\u00b7gel", "Got\u00b7tes", "einst", "mit", "Wan\u00b7gen", "freu\u00b7den\u00b7roth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Im Paradiese l\u00e4chelnd nah'n der Mensch, der reine noch.", "tokens": ["Im", "Pa\u00b7ra\u00b7die\u00b7se", "l\u00e4\u00b7chelnd", "nah'n", "der", "Mensch", ",", "der", "rei\u00b7ne", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPR", "ART", "NN", "$,", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Du kamst mit ", "tokens": ["Du", "kamst", "mit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Versehnt' ich manchen tr\u00fcben Tag in jenem Haine noch,", "tokens": ["Ver\u00b7sehnt'", "ich", "man\u00b7chen", "tr\u00fc\u00b7ben", "Tag", "in", "je\u00b7nem", "Hai\u00b7ne", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "APPR", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Und fragte weinend mein Geschick: \u201ebewahrst in deinem Schatz", "tokens": ["Und", "frag\u00b7te", "wei\u00b7nend", "mein", "Ge\u00b7schick", ":", "\u201e", "be\u00b7wahrst", "in", "dei\u00b7nem", "Schatz"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$.", "$(", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "So holde Stunde du f\u00fcr mich nicht eine, eine noch?\u201c", "tokens": ["So", "hol\u00b7de", "Stun\u00b7de", "du", "f\u00fcr", "mich", "nicht", "ei\u00b7ne", ",", "ei\u00b7ne", "noch", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "NN", "PPER", "APPR", "PPER", "PTKNEG", "ART", "$,", "ART", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Dort mocht' ich lauschen sp\u00e4t und fr\u00fch: wohl fl\u00fcsterts im", "tokens": ["Dort", "mocht'", "ich", "lau\u00b7schen", "sp\u00e4t", "und", "fr\u00fch", ":", "wohl", "fl\u00fcs\u00b7terts", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "ADJD", "KON", "ADJD", "$.", "ADV", "ADV", "APPRART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gezweig',", "tokens": ["Ge\u00b7zweig'", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Doch immer schweigt noch mein Geschick \u2014 ich lausch' und", "tokens": ["Doch", "im\u00b7mer", "schweigt", "noch", "mein", "Ge\u00b7schick", "ich", "lausch'", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$(", "PPER", "ADJD", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "weine noch.", "tokens": ["wei\u00b7ne", "noch", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}