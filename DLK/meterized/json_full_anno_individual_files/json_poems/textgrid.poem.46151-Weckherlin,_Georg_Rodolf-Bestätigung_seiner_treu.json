{"textgrid.poem.46151": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Best\u00e4tigung seiner treu", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, was betr\u00fcbt ihr mich so sehr,", "tokens": ["Ach", ",", "was", "be\u00b7tr\u00fcbt", "ihr", "mich", "so", "sehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df ihr mich wankelm\u00fctig nennet?", "tokens": ["da\u00df", "ihr", "mich", "wan\u00b7kel\u00b7m\u00fc\u00b7tig", "nen\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ich wei\u00df, da\u00df Amor mich vilmehr,", "tokens": ["ich", "wei\u00df", ",", "da\u00df", "A\u00b7mor", "mich", "vil\u00b7mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NE", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "dan euch, so sch\u00f6n, standhaft bekennet.", "tokens": ["dan", "euch", ",", "so", "sch\u00f6n", ",", "stand\u00b7haft", "be\u00b7ken\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "ADJD", "$,", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Wie euers leibs vollkommenheit", "tokens": ["Wie", "eu\u00b7ers", "leibs", "voll\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN"], "meter":