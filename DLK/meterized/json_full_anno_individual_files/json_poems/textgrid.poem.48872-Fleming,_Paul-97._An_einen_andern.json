{"textgrid.poem.48872": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "97. An einen andern", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei willig, edler Ring, mich willig zu gelosen", "tokens": ["Sei", "wil\u00b7lig", ",", "ed\u00b7ler", "Ring", ",", "mich", "wil\u00b7lig", "zu", "ge\u00b7lo\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "ADJA", "NN", "$,", "PRF", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und einer sch\u00f6nern Hand forthin geschenkt zu sein,", "tokens": ["und", "ei\u00b7ner", "sch\u00f6\u00b7nern", "Hand", "for\u00b7thin", "ge\u00b7schenkt", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die zwar nicht edler macht ein mehr als edler Stein.", "tokens": ["die", "zwar", "nicht", "ed\u00b7ler", "macht", "ein", "mehr", "als", "ed\u00b7ler", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ADJA", "VVFIN", "ART", "PIAT", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nein, darum send' ich ihr versetzt in dieser Rosen", "tokens": ["Nein", ",", "da\u00b7rum", "send'", "ich", "ihr", "ver\u00b7setzt", "in", "die\u00b7ser", "Ro\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PAV", "VVFIN", "PPER", "PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "die angenehme Nacht der g\u00fcnstigen T\u00fcrkosen,", "tokens": ["die", "an\u00b7ge\u00b7neh\u00b7me", "Nacht", "der", "g\u00fcns\u00b7ti\u00b7gen", "T\u00fcr\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "darmit sie nicht soll sehn des Scheines Widerschein,", "tokens": ["dar\u00b7mit", "sie", "nicht", "soll", "sehn", "des", "Schei\u00b7nes", "Wi\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der aus der Stirnen blinkt und auch den Stein nimmt ein,", "tokens": ["der", "aus", "der", "Stir\u00b7nen", "blinkt", "und", "auch", "den", "Stein", "nimmt", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "KON", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df er von ihrer Zier ihr gleichsam lieb mu\u00df kosen.", "tokens": ["da\u00df", "er", "von", "ih\u00b7rer", "Zier", "ihr", "gleich\u00b7sam", "lieb", "mu\u00df", "ko\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPER", "ADJD", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der lichte Diamant blitzt, wie ihr Antlitz tut,", "tokens": ["Der", "lich\u00b7te", "Di\u00b7a\u00b7mant", "blitzt", ",", "wie", "ihr", 