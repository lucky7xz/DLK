{"dta.poem.14375": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .   Geistliche Gedichte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Andre haben andre Schwingen,", "tokens": ["And\u00b7re", "ha\u00b7ben", "and\u00b7re", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber wir, mein fr\u00f6hlich Herz,", "tokens": ["A\u00b7ber", "wir", ",", "mein", "fr\u00f6h\u00b7lich", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollen grad' hinauf uns singen,", "tokens": ["Wol\u00b7len", "grad'", "hin\u00b7auf", "uns", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Fr\u00fchling himmelw\u00e4rts!", "tokens": ["Aus", "dem", "Fr\u00fch\u00b7ling", "him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}}}}