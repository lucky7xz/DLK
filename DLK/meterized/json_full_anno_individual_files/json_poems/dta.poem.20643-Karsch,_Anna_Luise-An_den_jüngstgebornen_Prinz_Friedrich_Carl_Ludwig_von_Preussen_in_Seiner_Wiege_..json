{"dta.poem.20643": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den j\u00fcngstgebornen Prinz  \n Friedrich Carl Ludwig von Preussen  \n  in Seiner Wiege .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Friederika, sey gegr\u00fc\u00dft!                 ", "tokens": ["Frie\u00b7de\u00b7ri\u00b7ka", ",", "sey", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welchem Helden soll ich dich vergleichen,", "tokens": ["Wel\u00b7chem", "Hel\u00b7den", "soll", "ich", "dich", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Welchen jungen Gott umschlie\u00dft", "tokens": ["Wel\u00b7chen", "jun\u00b7gen", "Gott", "um\u00b7schlie\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Adern zart Gewebe,", "tokens": ["Die\u00b7ser", "A\u00b7dern", "zart", "Ge\u00b7we\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese sammetweiche Haut?", "tokens": ["Die\u00b7se", "sam\u00b7met\u00b7wei\u00b7che", "Haut", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bist du nicht der milden Ph\u00f6be", "tokens": ["Bist", "du", "nicht", "der", "mil\u00b7den", "Ph\u00f6\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sch\u00f6nem Bruder gleich gebaut?", "tokens": ["Sch\u00f6\u00b7nem", "Bru\u00b7der", "gleich", "ge\u00b7baut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ha! Du blickest wie der ", "tokens": ["Ha", "!", "Du", "bli\u00b7ckest", "wie", "der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "KOKOM", "ART"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gro\u00dfe Vater Friederich. &#8212;                 ", "tokens": ["Gro\u00b7\u00dfe", "Va\u00b7ter", "Frie\u00b7de\u00b7rich", ".", "&#8212;"], "token_info": ["word", "word", "word", "punct", "XML_entity"], "pos": ["ADJA", "NN", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey der Tafel liegst du einst dem ", "tokens": ["Bey", "der", "Ta\u00b7fel", "liegst", "du", "einst", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der Brust, und j\u00fcngerlich", "tokens": ["An", "der", "Brust", ",", "und", "j\u00fcn\u00b7ger\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4ngst Du an Ihn mit den Augen,", "tokens": ["H\u00e4ngst", "Du", "an", "Ihn", "mit", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4ssest Deinen Nektar stehn,", "tokens": ["L\u00e4s\u00b7sest", "Dei\u00b7nen", "Nek\u00b7tar", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Um die Reden einzusaugen", "tokens": ["Um", "die", "Re\u00b7den", "ein\u00b7zu\u00b7sau\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die aus Seinem Munde gehn.", "tokens": ["Die", "aus", "Sei\u00b7nem", "Mun\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "L\u00e4chle Ihm und ", "tokens": ["L\u00e4ch\u00b7le", "Ihm", "und"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "KON"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Deiner ", "tokens": ["Dei\u00b7ner"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "La\u00df uns nicht f\u00fcr Deine Tage zittern.", "tokens": ["La\u00df", "uns", "nicht", "f\u00fcr", "Dei\u00b7ne", "Ta\u00b7ge", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Diesen Gliederbau bezogen,", "tokens": ["Die\u00b7sen", "Glie\u00b7der\u00b7bau", "be\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist schon mancher junge F\u00fcrst", "tokens": ["Ist", "schon", "man\u00b7cher", "jun\u00b7ge", "F\u00fcrst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von dem Throne fortgeflogen,", "tokens": ["Von", "dem", "Thro\u00b7ne", "fort\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den Du mitbesch\u00fctzen wirst. \u2014", "tokens": ["Den", "Du", "mit\u00b7be\u00b7sch\u00fct\u00b7zen", "wirst", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "VVINF", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Theurer Prinz bleib auf der Erde,                 ", "tokens": ["Theu\u00b7rer", "Prinz", "bleib", "auf", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo Du sehr willkommen bist;", "tokens": ["Wo", "Du", "sehr", "will\u00b7kom\u00b7men", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleib bey Deinem Bruder hier, und werde", "tokens": ["Bleib", "bey", "Dei\u00b7nem", "Bru\u00b7der", "hier", ",", "und", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADV", "$,", "KON", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihm dereinst, was ", "tokens": ["Ihm", "de\u00b7reinst", ",", "was"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "PWS"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Seinem Bruder unserm ", "tokens": ["Sei\u00b7nem", "Bru\u00b7der", "un\u00b7serm"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Unserm v\u00e4terlichen Freund,", "tokens": ["Un\u00b7serm", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Freund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dem ein Seculum zu wenig", "tokens": ["Dem", "ein", "Se\u00b7cu\u00b7lum", "zu", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "APPR", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zeit zum Thatenraume scheint.", "tokens": ["Zeit", "zum", "Tha\u00b7ten\u00b7rau\u00b7me", "scheint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}