{"dta.poem.18511": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Den von Abraham seinem GOTT  \n aufgeopferten Jsaac/ an dem exem-  \n pel Hrn. Wolff Abraham  \n Platzens/  Jur. Utr.  \n Candidati.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hochverdientes haupt/ du andrer Abraham/", "tokens": ["Du", "hoch\u00b7ver\u00b7dien\u00b7tes", "haupt", "/", "du", "an\u00b7drer", "Ab\u00b7ra\u00b7ham", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "ADJA", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So l\u00e4st dein GOtt dich schon die scharffe stimme h\u00f6ren:", "tokens": ["So", "l\u00e4st", "dein", "Gott", "dich", "schon", "die", "scharf\u00b7fe", "stim\u00b7me", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nimm deinen sohn/ der einst aus deinen lenden kam/", "tokens": ["Nimm", "dei\u00b7nen", "sohn", "/", "der", "einst", "aus", "dei\u00b7nen", "len\u00b7den", "kam", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$(", "ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "La\u00df ihn die grosse zahl der todten-opfer mehren.", "tokens": ["La\u00df", "ihn", "die", "gros\u00b7se", "zahl", "der", "tod\u00b7ten\u00b7op\u00b7fer", "meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nimm deinen einigen und vielgeliebten sohn/", "tokens": ["Nimm", "dei\u00b7nen", "ei\u00b7ni\u00b7gen", "und", "viel\u00b7ge\u00b7lieb\u00b7ten", "sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "PIAT", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den meine g\u00fcte dir geschenckt zu einen erben.", "tokens": ["Den", "mei\u00b7ne", "g\u00fc\u00b7te", "dir", "ge\u00b7schenckt", "zu", "ei\u00b7nen", "er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVPP", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das messer ist gewetzt/ das feuer brennet schon/", "tokens": ["Das", "mes\u00b7ser", "ist", "ge\u00b7wetzt", "/", "das", "feu\u00b7er", "bren\u00b7net", "schon", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "VVPP", "$(", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gib deinen Jsaac her/ er soll und mu\u00df itzt sterben.", "tokens": ["Gib", "dei\u00b7nen", "Jsaac", "her", "/", "er", "soll", "und", "mu\u00df", "itzt", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$(", "PPER", "VMFIN", "KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ach donner-gleiches wort! wie wehe wird doch dir!", "tokens": ["Ach", "don\u00b7ner\u00b7glei\u00b7ches", "wort", "!", "wie", "we\u00b7he", "wird", "doch", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "PWAV", "ADJD", "VAFIN", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie schnell und unverhofft f\u00e4llt deine st\u00fctze nieder!", "tokens": ["Wie", "schnell", "und", "un\u00b7ver\u00b7hofft", "f\u00e4llt", "dei\u00b7ne", "st\u00fct\u00b7ze", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVFIN", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ist denn kein opfer-schaf statt deines sohnes hier?", "tokens": ["Ist", "denn", "kein", "op\u00b7fer\u00b7schaf", "statt", "dei\u00b7nes", "soh\u00b7nes", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verhindert dich kein ruf? Er scheinet dir kein wieder?", "tokens": ["Ver\u00b7hin\u00b7dert", "dich", "kein", "ruf", "?", "Er", "schei\u00b7net", "dir", "kein", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$.", "PPER", "VVFIN", "PPER", "PIAT", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach nein! es bleibt dabey. Das urtheil gehet fort.", "tokens": ["Ach", "nein", "!", "es", "bleibt", "da\u00b7bey", ".", "Das", "ur\u00b7theil", "ge\u00b7het", "fort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "PPER", "VVFIN", "PAV", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du aber l\u00e4\u00dft hierbey uns viel gehorsam sehen.", "tokens": ["Du", "a\u00b7ber", "l\u00e4\u00dft", "hier\u00b7bey", "uns", "viel", "ge\u00b7hor\u00b7sam", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du redst wie Abraham kein ungedultig wort/", "tokens": ["Du", "redst", "wie", "Ab\u00b7ra\u00b7ham", "kein", "un\u00b7ge\u00b7dul\u00b7tig", "wort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NE", "PIAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und sprichst: Hier bin ich Herr/ dein wille soll geschehen.", "tokens": ["Und", "sprichst", ":", "Hier", "bin", "ich", "Herr", "/", "dein", "wil\u00b7le", "soll", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "NN", "$(", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die nachwelt tadelt noch des Jephth\u00e4 frevelthat/", "tokens": ["Die", "nach\u00b7welt", "ta\u00b7delt", "noch", "des", "Je\u00b7phth\u00e4", "fre\u00b7vel\u00b7that", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der einst/ wie er gelobt/ die einge tochter schlachte.", "tokens": ["Der", "einst", "/", "wie", "er", "ge\u00b7lobt", "/", "die", "ein\u00b7ge", "toch\u00b7ter", "schlach\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$(", "PWAV", "PPER", "VVPP", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dergleichen schlimmen ruhm auch Agamemnon hat/", "tokens": ["Derg\u00b7lei\u00b7chen", "schlim\u00b7men", "ruhm", "auch", "A\u00b7ga\u00b7mem\u00b7non", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "ADV", "NE", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Jphigeniam/ sein kind/ zum opfer brachte.", "tokens": ["Der", "Jphi\u00b7ge\u00b7ni\u00b7am", "/", "sein", "kind", "/", "zum", "op\u00b7fer", "brach\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "PPOSAT", "NN", "$(", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Allein du bringst zwar auch dem h\u00f6chsten deinen sohn/", "tokens": ["Al\u00b7lein", "du", "bringst", "zwar", "auch", "dem", "h\u00f6chs\u00b7ten", "dei\u00b7nen", "sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Doch nur wie Abraham nach GOttes schlu\u00df und willen.", "tokens": ["Doch", "nur", "wie", "Ab\u00b7ra\u00b7ham", "nach", "Got\u00b7tes", "schlu\u00df", "und", "wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "NE", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der herr ist ja dein schild und dein sehr grosser lohn/", "tokens": ["Der", "herr", "ist", "ja", "dein", "schild", "und", "dein", "sehr", "gros\u00b7ser", "lohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "ADJD", "KON", "PPOSAT", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Drum bist du so bereit sein w\u00fcnschen zu erf\u00fcllen.", "tokens": ["Drum", "bist", "du", "so", "be\u00b7reit", "sein", "w\u00fcn\u00b7schen", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADJD", "VAINF", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dein Jsaac willigt selbst in dieses opfer ein/", "tokens": ["Dein", "Jsaac", "wil\u00b7ligt", "selbst", "in", "die\u00b7ses", "op\u00b7fer", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PDAT", "NN", "ART", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.26": {"text": "Mein vater/ ruft er aus/ hier di\u00df mein sterbe-bette/", "tokens": ["Mein", "va\u00b7ter", "/", "ruft", "er", "aus", "/", "hier", "di\u00df", "mein", "ster\u00b7be\u00b7bet\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PPER", "PTKVZ", "$(", "ADV", "PDS", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Kan ein bequemer platz zum brand-altare seyn/", "tokens": ["Kan", "ein", "be\u00b7que\u00b7mer", "platz", "zum", "bran\u00b7dal\u00b7ta\u00b7re", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der lieb\u2019 und kranckheit glut brennt hier schon in die wette.", "tokens": ["Der", "lieb'", "und", "kran\u00b7ck\u00b7heit", "glut", "brennt", "hier", "schon", "in", "die", "wet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "NN", "ADJD", "VVFIN", "ADV", "ADV", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Wohlan! dein sohn ist hin. Das opfer ist vollbracht/", "tokens": ["Wo\u00b7hlan", "!", "dein", "sohn", "ist", "hin", ".", "Das", "op\u00b7fer", "ist", "voll\u00b7bracht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "$.", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sein todter c\u00f6rper wird bereits zu asch und erde.", "tokens": ["Sein", "tod\u00b7ter", "c\u00f6r\u00b7per", "wird", "be\u00b7reits", "zu", "asch", "und", "er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Was aber hat hierbey dich so behertzt gemacht?", "tokens": ["Was", "a\u00b7ber", "hat", "hier\u00b7bey", "dich", "so", "be\u00b7hertzt", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ADV", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der glaube/ da\u00df ihn GOtt einst auferwecken werde.", "tokens": ["Der", "glau\u00b7be", "/", "da\u00df", "ihn", "Gott", "einst", "au\u00b7fer\u00b7we\u00b7cken", "wer\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "KOUS", "PPER", "NN", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es fordert dir der HErr dein liebstes kleinod ab;", "tokens": ["Es", "for\u00b7dert", "dir", "der", "Herr", "dein", "liebs\u00b7tes", "klei\u00b7nod", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Du sprichst: Der himmel wird ihm zum beh\u00e4ltnis dienen.", "tokens": ["Du", "sprichst", ":", "Der", "him\u00b7mel", "wird", "ihm", "zum", "be\u00b7h\u00e4lt\u00b7nis", "die\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Des h\u00f6chsten arm zerbricht dir deines alters stab;", "tokens": ["Des", "h\u00f6chs\u00b7ten", "arm", "zer\u00b7bricht", "dir", "dei\u00b7nes", "al\u00b7ters", "stab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVFIN", "PPER", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Du sprichst: er wird doch einst wie Aarons stecken gr\u00fcnen.", "tokens": ["Du", "sprichst", ":", "er", "wird", "doch", "einst", "wie", "Aa\u00b7rons", "ste\u00b7cken", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADV", "KOKOM", "NE", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Das werthe mutter-hertz hat gleichen glaubens-grund/", "tokens": ["Das", "wert\u00b7he", "mut\u00b7ter\u00b7hertz", "hat", "glei\u00b7chen", "glau\u00b7bens\u00b7grund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das opfer/ welches itzt ihr Abraham gegeben/", "tokens": ["Das", "op\u00b7fer", "/", "wel\u00b7ches", "itzt", "ihr", "Ab\u00b7ra\u00b7ham", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWS", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ist zwar bey ihr weit mehr als dort bey Sara kund:", "tokens": ["Ist", "zwar", "bey", "ihr", "weit", "mehr", "als", "dort", "bey", "Sa\u00b7ra", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "ADJD", "PIAT", "KOKOM", "ADV", "APPR", "NE", "PTKVZ", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Allein dies ist ihr trost: mein sohn wird wieder leben.", "tokens": ["Al\u00b7lein", "dies", "ist", "ihr", "trost", ":", "mein", "sohn", "wird", "wie\u00b7der", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "