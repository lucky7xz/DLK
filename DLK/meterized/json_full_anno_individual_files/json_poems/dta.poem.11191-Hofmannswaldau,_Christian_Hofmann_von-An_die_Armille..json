{"dta.poem.11191": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An die Armille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Christ-nacht gab dir zwar den zettel in die hand,", "tokens": ["Die", "Christ\u00b7nacht", "gab", "dir", "zwar", "den", "zet\u00b7tel", "in", "die", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darauf mein nahme stund; wie ist es denn bewandt,", "tokens": ["Da\u00b7rauf", "mein", "nah\u00b7me", "stund", ";", "wie", "ist", "es", "denn", "be\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "$.", "PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df dich ein andrer hat zu seiner braut erlesen?", "tokens": ["Da\u00df", "dich", "ein", "an\u00b7drer", "hat", "zu", "sei\u00b7ner", "braut", "er\u00b7le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VAFIN", "APPR", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das machts, mein zettel ist von lesch-papier gewesen.", "tokens": ["Das", "machts", ",", "mein", "zet\u00b7tel", "ist", "von", "le\u00b7schpa\u00b7pier", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "APPR", "NE", "VAPP", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}}}}