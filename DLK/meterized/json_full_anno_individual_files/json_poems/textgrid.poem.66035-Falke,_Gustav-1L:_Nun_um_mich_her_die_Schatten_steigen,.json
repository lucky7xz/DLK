{"textgrid.poem.66035": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun um mich her die Schatten steigen,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun um mich her die Schatten steigen,", "tokens": ["Nun", "um", "mich", "her", "die", "Schat\u00b7ten", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "APZR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stellst du dich ein, willkommnes Schweigen,", "tokens": ["Stellst", "du", "dich", "ein", ",", "will\u00b7komm\u00b7nes", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du, aller tiefsten Sehnsucht wert.", "tokens": ["Du", ",", "al\u00b7ler", "tiefs\u00b7ten", "Sehn\u00b7sucht", "wert", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sehr hab ich unter L\u00e4rm und Last", "tokens": ["Sehr", "hab", "ich", "un\u00b7ter", "L\u00e4rm", "und", "Last"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Tags nach dir, du scheuer Gast,", "tokens": ["Des", "Tags", "nach", "dir", ",", "du", "scheu\u00b7er", "Gast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "mea