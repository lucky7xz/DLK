{"dta.poem.19442": {"metadata": {"author": {"name": "Stolberg-Stolberg, Friedrich Leopold zu", "birth": "N.A.", "death": "N.A."}, "title": "Der Gesang.  \n An Sch\u00f6nborn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1779", "urn": "urn:nbn:de:kobv:b4-20589-5", "language": ["de:0.99"], "booktitle": "Stolberg-Stolberg, Christian zu; Stolberg-Stolberg, Friedrich Leopold zu: Gedichte. Leipzig, 1779."}, "poem": {"stanza.1": {"line.1": {"text": "Und oft schwebt er vom Himmel herab! den", "tokens": ["Und", "oft", "schwebt", "er", "vom", "Him\u00b7mel", "her\u00b7ab", "!", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$.", "ART"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "nahenden f\u00fchl' ich,", "tokens": ["na\u00b7hen\u00b7den", "f\u00fchl'", "ich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Meine Seel\u2019 erhebet sich dann in steigender Wal-", "tokens": ["Mei\u00b7ne", "Seel'", "er\u00b7he\u00b7bet", "sich", "dann", "in", "stei\u00b7gen\u00b7der", "Wa\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "TRUNC"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "lung,", "tokens": ["lung", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Wie das Meer sich erhebt in der K\u00fchle des pur-", "tokens": ["Wie", "das", "Meer", "sich", "er\u00b7hebt", "in", "der", "K\u00fch\u00b7le", "des", "pur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "purnen Abends.", "tokens": ["pur\u00b7nen", "A\u00b7bends", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Neue Bilder schweben um ihn und junge Gedanken,", "tokens": ["Neu\u00b7e", "Bil\u00b7der", "schwe\u00b7ben", "um", "ihn", "und", "jun\u00b7ge", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPER", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Wie mit zahllosen Blumen der Lenz die Erde", "tokens": ["Wie", "mit", "zahl\u00b7lo\u00b7sen", "Blu\u00b7men", "der", "Lenz", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "NN", "ART", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "besuchet,", "tokens": ["be\u00b7su\u00b7chet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Und mit taufend S\u00e4ngern des Hains in bl\u00fchen-", "tokens": ["Und", "mit", "tau\u00b7fend", "S\u00e4n\u00b7gern", "des", "Hains", "in", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN", "ART", "NN", "APPR", "TRUNC"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.11": {"text": "den Stauden!", "tokens": ["den", "Stau\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Hohe Gedanken schweben um ihn, wie rund um", "tokens": ["Ho\u00b7he", "Ge\u00b7dan\u00b7ken", "schwe\u00b7ben", "um", "ihn", ",", "wie", "rund", "um"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPER", "$,", "PWAV", "ADJD", "APPR"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.13": {"text": "den Himmel", "tokens": ["den", "Him\u00b7mel"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Flammende Sonnen mit gr\u00fcngelockten Erden", "tokens": ["Flam\u00b7men\u00b7de", "Son\u00b7nen", "mit", "gr\u00fcn\u00b7ge\u00b7lock\u00b7ten", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.15": {"text": "umkr\u00e4nzet,", "tokens": ["um\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Und mit Silberwangigen Monden! Mondschein-", "tokens": ["Und", "mit", "Sil\u00b7ber\u00b7wan\u00b7gi\u00b7gen", "Mon\u00b7den", "!", "Mond\u00b7schein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "NN", "NN", "$.", "TRUNC"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "\u00e4hnlich", "tokens": ["\u00e4hn\u00b7lich"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Leuchtet er manchmal sanft und entlocket z\u00e4rtliche", "tokens": ["Leuch\u00b7tet", "er", "manch\u00b7mal", "sanft", "und", "ent\u00b7lo\u00b7cket", "z\u00e4rt\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KON", "VVFIN", "ADJA"], "meter": "+--+-+--+-+--", "measure": "iambic.penta.invert"}, "line.19": {"text": "Thr\u00e4nen;", "tokens": ["Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Und dann eilt er mit Flammen umg\u00fcrtet, gleich", "tokens": ["Und", "dann", "eilt", "er", "mit", "Flam\u00b7men", "um\u00b7g\u00fcr\u00b7tet", ",", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "VVPP", "$,", "ADV"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.21": {"text": "dem Kometen,", "tokens": ["dem", "Ko\u00b7me\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.22": {"text": "Wann er von Himmel zu Himmel im feurigen", "tokens": ["Wann", "er", "von", "Him\u00b7mel", "zu", "Him\u00b7mel", "im", "feu\u00b7ri\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "APPR", "NN", "APPRART", "ADJA"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Wagen daherrollt!", "tokens": ["Wa\u00b7gen", "da\u00b7her\u00b7rollt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}