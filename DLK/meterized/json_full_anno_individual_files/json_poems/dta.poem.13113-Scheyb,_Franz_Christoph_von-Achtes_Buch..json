{"dta.poem.13113": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas mach ich? Stim\u0303 und Mund mu\u00df zu der Absicht dienen:", "tokens": ["\u201e", "was", "mach", "ich", "?", "Stim\u0303", "und", "Mund", "mu\u00df", "zu", "der", "Ab\u00b7sicht", "die\u00b7nen", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$.", "NE", "KON", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edas Herz bricht mit Begier, mit zitterndem Erk\u00fchnen", "tokens": ["\u201e", "das", "Herz", "bricht", "mit", "Be\u00b7gier", ",", "mit", "zit\u00b7tern\u00b7dem", "Er\u00b7k\u00fch\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "465\u201dIn frohes Singen aus. Ist aber die\u00df zu schwach,", "tokens": ["\"", "In", "fro\u00b7hes", "Sin\u00b7gen", "aus", ".", "Ist", "a\u00b7ber", "die\u00df", "zu", "schwach", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "PTKVZ", "$.", "VAFIN", "ADV", "PDS", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eso folgt der Cither Klang, die Leyr, die Fl\u00f6te nach,", "tokens": ["\u201e", "so", "folgt", "der", "Cit\u00b7her", "Klang", ",", "die", "Leyr", ",", "die", "Fl\u00f6\u00b7te", "nach", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eja was den Herzens-Trieb der Welt kann offenbaren:", "tokens": ["\u201e", "ja", "was", "den", "Her\u00b7zens\u00b7Trieb", "der", "Welt", "kann", "of\u00b7fen\u00b7ba\u00b7ren", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PWS", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edann hei\u00dft es weder Kunst, noch Thon noch Stimme spahren.", "tokens": ["\u201e", "dann", "hei\u00dft", "es", "we\u00b7der", "Kunst", ",", "noch", "Thon", "noch", "Stim\u00b7me", "spah\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "KON", "NN", "$,", "ADV", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edie Seel erwacht und f\u00fchlt die Macht, den Trieb, den Geist,", "tokens": ["\u201e", "die", "Seel", "er\u00b7wacht", "und", "f\u00fchlt", "die", "Macht", ",", "den", "Trieb", ",", "den", "Geist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVPP", "KON", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "470\u201dDer sie fast von ihr selbst in diese Freude rei\u00dft.", "tokens": ["\"", "Der", "sie", "fast", "von", "ihr", "selbst", "in", "die\u00b7se", "Freu\u00b7de", "rei\u00dft", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PPER", "ADV", "APPR", "PPER", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eso will sie durch den Mund, gleich einer Flamme, dringen,", "tokens": ["\u201e", "so", "will", "sie", "durch", "den", "Mund", ",", "gleich", "ei\u00b7ner", "Flam\u00b7me", ",", "drin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201evon ihren Regungen, von dieser Lust zu singen.", "tokens": ["\u201e", "von", "ih\u00b7ren", "Re\u00b7gun\u00b7gen", ",", "von", "die\u00b7ser", "Lust", "zu", "sin\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}