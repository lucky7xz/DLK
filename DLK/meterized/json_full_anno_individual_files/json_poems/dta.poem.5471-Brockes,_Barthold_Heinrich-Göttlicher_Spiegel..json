{"dta.poem.5471": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6ttlicher Spiegel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein vom Sch\u00f6pfer, durch sein Werck und von deren", "tokens": ["Ein", "vom", "Sch\u00f6p\u00b7fer", ",", "durch", "sein", "Werck", "und", "von", "de\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "$,", "APPR", "PPOSAT", "NN", "KON", "APPR", "PRELAT"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wunder-Schein,", "tokens": ["Wun\u00b7der\u00b7Schein", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Angef\u00fclletes Gem\u00fcthe", "tokens": ["An\u00b7ge\u00b7f\u00fcl\u00b7le\u00b7tes", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheinet gleichsam wie ein Spiegel f\u00fcr den Sch\u00f6pfer selbst", "tokens": ["Schei\u00b7net", "gleich\u00b7sam", "wie", "ein", "Spie\u00b7gel", "f\u00fcr", "den", "Sch\u00f6p\u00b7fer", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "zu seyn,", "tokens": ["zu", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VAINF", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Worinn er, sein Werck vergeistert und mit Danck und", "tokens": ["Wo\u00b7rinn", "er", ",", "sein", "Werck", "ver\u00b7geis\u00b7tert", "und", "mit", "Danck", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "PPOSAT", "NN", "VVPP", "KON", "APPR", "NN", "KON"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Lust geschm\u00fcckt,", "tokens": ["Lust", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Lauter Weisheit, Allmacht, G\u00fcte; ja sich gleichsam", "tokens": ["Lau\u00b7ter", "Weis\u00b7heit", ",", "All\u00b7macht", ",", "G\u00fc\u00b7te", ";", "ja", "sich", "gleich\u00b7sam"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN", "$.", "ADV", "PRF", "ADJD"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "selbst erblickt.", "tokens": ["selbst", "er\u00b7blickt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}