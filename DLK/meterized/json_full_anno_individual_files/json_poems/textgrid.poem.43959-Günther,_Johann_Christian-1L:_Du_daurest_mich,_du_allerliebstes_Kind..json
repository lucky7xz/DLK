{"textgrid.poem.43959": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du daurest mich, du allerliebstes Kind.", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du daurest mich, du allerliebstes Kind.", "tokens": ["Du", "dau\u00b7rest", "mich", ",", "du", "al\u00b7ler\u00b7liebs\u00b7tes", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du f\u00fchlst mein Weh, ich leide deine Schmerzen,", "tokens": ["Du", "f\u00fchlst", "mein", "Weh", ",", "ich", "lei\u00b7de", "dei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da Gl\u00fcck und Zeit so lange grausam sind", "tokens": ["Da", "Gl\u00fcck", "und", "Zeit", "so", "lan\u00b7ge", "grau\u00b7sam", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mit dem Flehn getreuer Seelen scherzen;", "tokens": ["Und", "mit", "dem", "Flehn", "ge\u00b7treu\u00b7er", "See\u00b7len", "scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du leidest viel, doch gieb der Treu Geh\u00f6r:", "tokens": ["Du", "lei\u00b7dest", "viel", ",", "doch", "gieb", "der", "Treu", "Ge\u00b7h\u00f6r", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "VVIMP", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich leide mehr.", "tokens": ["Ich", "lei\u00b7de", "mehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich leide mehr, als jemand kan und glaubt,", "tokens": ["Ich", "lei\u00b7de", "mehr", ",", "als", "je\u00b7mand", "kan", "und", "glaubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PIS", "VMFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich mu\u00df von dir, der Ri\u00df macht schwere Plagen;", "tokens": ["Ich", "mu\u00df", "von", "dir", ",", "der", "Ri\u00df", "macht", "schwe\u00b7re", "Pla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "$,", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich seh den Trost, den dir mein Abschied raubt,", "tokens": ["Ich", "seh", "den", "Trost", ",", "den", "dir", "mein", "Ab\u00b7schied", "raubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wird mein Herz auch zweyfach wund geschlagen,", "tokens": ["So", "wird", "mein", "Herz", "auch", "zwey\u00b7fach", "wund", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du liebest mich so wohl getreu als klug,", "tokens": ["Du", "lie\u00b7best", "mich", "so", "wohl", "ge\u00b7treu", "als", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das ist genug.", "tokens": ["Das", "ist", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Das ist genug, die Unruh zu verstehn,", "tokens": ["Das", "ist", "ge\u00b7nug", ",", "die", "Un\u00b7ruh", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Lorchen kr\u00e4nckt und mich in ihr verzehret;", "tokens": ["Die", "Lor\u00b7chen", "kr\u00e4nckt", "und", "mich", "in", "ihr", "ver\u00b7zeh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach, sollt ich bald davor zu Grabe gehn,", "tokens": ["Ach", ",", "sollt", "ich", "bald", "da\u00b7vor", "zu", "Gra\u00b7be", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "ADV", "PAV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich w\u00fcrde wohl so heftig nicht beschweret.", "tokens": ["Ich", "w\u00fcr\u00b7de", "wohl", "so", "hef\u00b7tig", "nicht", "be\u00b7schwe\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer weis, was kommt! Vielleicht beschliest der Tod", "tokens": ["Wer", "weis", ",", "was", "kommt", "!", "Viel\u00b7leicht", "be\u00b7schliest", "der", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PTKVZ", "$,", "PWS", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die lange Noth.", "tokens": ["Die", "lan\u00b7ge", "Noth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Die lange Noth ist dennoch nicht so starck,", "tokens": ["Die", "lan\u00b7ge", "Noth", "ist", "den\u00b7noch", "nicht", "so", "starck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Uns, werther Schaz, dem Geiste nach zu trennen.", "tokens": ["Uns", ",", "wert\u00b7her", "Schaz", ",", "dem", "Geis\u00b7te", "nach", "zu", "tren\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erwarth ich mir statt deiner Schoos den Sarg,", "tokens": ["Er\u00b7warth", "ich", "mir", "statt", "dei\u00b7ner", "Schoos", "den", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So soll mir doch der Neid den Nachruhm g\u00f6nnen,", "tokens": ["So", "soll", "mir", "doch", "der", "Neid", "den", "Nach\u00b7ruhm", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df leicht kein Mensch so rein als ich geliebt,", "tokens": ["Da\u00df", "leicht", "kein", "Mensch", "so", "rein", "als", "ich", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PIAT", "NN", "ADV", "ADJD", "KOKOM", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Obgleich betr\u00fcbt.", "tokens": ["Ob\u00b7gleich", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Obgleich betr\u00fcbt, jedennoch unverzagt.", "tokens": ["Ob\u00b7gleich", "be\u00b7tr\u00fcbt", ",", "je\u00b7den\u00b7noch", "un\u00b7ver\u00b7zagt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Himmel z\u00fcrnt, wer will mit diesem zancken?", "tokens": ["Der", "Him\u00b7mel", "z\u00fcrnt", ",", "wer", "will", "mit", "die\u00b7sem", "zan\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWS", "VMFIN", "APPR", "PDAT", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohin mich auch mein hart Verh\u00e4ngn\u00fc\u00df jagt,", "tokens": ["Wo\u00b7hin", "mich", "auch", "mein", "hart", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da bleibest du ein Trostbild der Gedancken;", "tokens": ["Da", "blei\u00b7best", "du", "ein", "Trost\u00b7bild", "der", "Ge\u00b7dan\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wirst du mir nicht, so ha\u00df ich Lieb und Eh.", "tokens": ["Wirst", "du", "mir", "nicht", ",", "so", "ha\u00df", "ich", "Lieb", "und", "Eh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nun, Kind, ich geh.", "tokens": ["Nun", ",", "Kind", ",", "ich", "geh."], "token_info": ["word", "punct", "word", "punct", "word", "abbreviation"], "pos": ["ADV", "$,", "NN", "$,", "PPER", "NE"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Nun, Kind, ich geh. Geh auch und nimm den Ku\u00df,", "tokens": ["Nun", ",", "Kind", ",", "ich", "geh.", "Geh", "auch", "und", "nimm", "den", "Ku\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "abbreviation", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "PPER", "NE", "NE", "ADV", "KON", "VVIMP", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir martern nur einander durch dies Lezen.", "tokens": ["Wir", "mar\u00b7tern", "nur", "ein\u00b7an\u00b7der", "durch", "dies", "Le\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich zwinge mich, den ungewi\u00dfen Fu\u00df,", "tokens": ["Ich", "zwin\u00b7ge", "mich", ",", "den", "un\u00b7ge\u00b7wi\u00b7\u00dfen", "Fu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den du verweilst, Gott weis wohin zu sezen;", "tokens": ["Den", "du", "ver\u00b7weilst", ",", "Gott", "weis", "wo\u00b7hin", "zu", "se\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "NN", "PTKVZ", "PWAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Ungl\u00fcck st\u00fcrmt, die L\u00e4strer stimmen ein,", "tokens": ["Das", "Un\u00b7gl\u00fcck", "st\u00fcrmt", ",", "die", "L\u00e4st\u00b7rer", "stim\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ergieb dich drein!", "tokens": ["Er\u00b7gieb", "dich", "drein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ergieb dich drein! Es bliz auch nah und fern;", "tokens": ["Er\u00b7gieb", "dich", "drein", "!", "Es", "bliz", "auch", "nah", "und", "fern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein schneller Wind kan leicht das Wetter \u00e4ndern.", "tokens": ["Ein", "schnel\u00b7ler", "Wind", "kan", "leicht", "das", "Wet\u00b7ter", "\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Vaterland versagt mir Gl\u00fcck und Stern,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", "ver\u00b7sagt", "mir", "Gl\u00fcck", "und", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dies bl\u00fcht vielleicht in unbekandten L\u00e4ndern.", "tokens": ["Dies", "bl\u00fcht", "viel\u00b7leicht", "in", "un\u00b7be\u00b7kand\u00b7ten", "L\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Flei\u00df ist froh, nur dich noch zu erh\u00f6hn,", "tokens": ["Mein", "Flei\u00df", "ist", "froh", ",", "nur", "dich", "noch", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADV", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Viel auszustehn.", "tokens": ["Viel", "aus\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Viel auszustehn und gleichwohl frey zu seyn,", "tokens": ["Viel", "aus\u00b7zu\u00b7stehn", "und", "gleich\u00b7wohl", "frey", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vermag kein Geist, den Lieb und Ruhm nicht st\u00e4rcken.", "tokens": ["Ver\u00b7mag", "kein", "Geist", ",", "den", "Lieb", "und", "Ruhm", "nicht", "st\u00e4r\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "ART", "NN", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kind, gute Nacht! Mein Anblick mehrt die Pein,", "tokens": ["Kind", ",", "gu\u00b7te", "Nacht", "!", "Mein", "An\u00b7blick", "mehrt", "die", "Pein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich kan die Angst an Farb und Sprache mercken.", "tokens": ["Ich", "kan", "die", "Angst", "an", "Farb", "und", "Spra\u00b7che", "mer\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sieh mich noch an und lebe wohl und sprich:", "tokens": ["Sieh", "mich", "noch", "an", "und", "le\u00b7be", "wohl", "und", "sprich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du daurest mich.", "tokens": ["Du", "dau\u00b7rest", "mich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Du daurest mich, du allerliebstes Kind.", "tokens": ["Du", "dau\u00b7rest", "mich", ",", "du", "al\u00b7ler\u00b7liebs\u00b7tes", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du f\u00fchlst mein Weh, ich leide deine Schmerzen,", "tokens": ["Du", "f\u00fchlst", "mein", "Weh", ",", "ich", "lei\u00b7de", "dei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da Gl\u00fcck und Zeit so lange grausam sind", "tokens": ["Da", "Gl\u00fcck", "und", "Zeit", "so", "lan\u00b7ge", "grau\u00b7sam", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mit dem Flehn getreuer Seelen scherzen;", "tokens": ["Und", "mit", "dem", "Flehn", "ge\u00b7treu\u00b7er", "See\u00b7len", "scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du leidest viel, doch gieb der Treu Geh\u00f6r:", "tokens": ["Du", "lei\u00b7dest", "viel", ",", "doch", "gieb", "der", "Treu", "Ge\u00b7h\u00f6r", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "VVIMP", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich leide mehr.", "tokens": ["Ich", "lei\u00b7de", "mehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Ich leide mehr, als jemand kan und glaubt,", "tokens": ["Ich", "lei\u00b7de", "mehr", ",", "als", "je\u00b7mand", "kan", "und", "glaubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PIS", "VMFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich mu\u00df von dir, der Ri\u00df macht schwere Plagen;", "tokens": ["Ich", "mu\u00df", "von", "dir", ",", "der", "Ri\u00df", "macht", "schwe\u00b7re", "Pla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "$,", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich seh den Trost, den dir mein Abschied raubt,", "tokens": ["Ich", "seh", "den", "Trost", ",", "den", "dir", "mein", "Ab\u00b7schied", "raubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wird mein Herz auch zweyfach wund geschlagen,", "tokens": ["So", "wird", "mein", "Herz", "auch", "zwey\u00b7fach", "wund", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du liebest mich so wohl getreu als klug,", "tokens": ["Du", "lie\u00b7best", "mich", "so", "wohl", "ge\u00b7treu", "als", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das ist genug.", "tokens": ["Das", "ist", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Das ist genug, die Unruh zu verstehn,", "tokens": ["Das", "ist", "ge\u00b7nug", ",", "die", "Un\u00b7ruh", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Lorchen kr\u00e4nckt und mich in ihr verzehret;", "tokens": ["Die", "Lor\u00b7chen", "kr\u00e4nckt", "und", "mich", "in", "ihr", "ver\u00b7zeh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach, sollt ich bald davor zu Grabe gehn,", "tokens": ["Ach", ",", "sollt", "ich", "bald", "da\u00b7vor", "zu", "Gra\u00b7be", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "ADV", "PAV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich w\u00fcrde wohl so heftig nicht beschweret.", "tokens": ["Ich", "w\u00fcr\u00b7de", "wohl", "so", "hef\u00b7tig", "nicht", "be\u00b7schwe\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer weis, was kommt! Vielleicht beschliest der Tod", "tokens": ["Wer", "weis", ",", "was", "kommt", "!", "Viel\u00b7leicht", "be\u00b7schliest", "der", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PTKVZ", "$,", "PWS", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die lange Noth.", "tokens": ["Die", "lan\u00b7ge", "Noth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Die lange Noth ist dennoch nicht so starck,", "tokens": ["Die", "lan\u00b7ge", "Noth", "ist", "den\u00b7noch", "nicht", "so", "starck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Uns, werther Schaz, dem Geiste nach zu trennen.", "tokens": ["Uns", ",", "wert\u00b7her", "Schaz", ",", "dem", "Geis\u00b7te", "nach", "zu", "tren\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erwarth ich mir statt deiner Schoos den Sarg,", "tokens": ["Er\u00b7warth", "ich", "mir", "statt", "dei\u00b7ner", "Schoos", "den", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So soll mir doch der Neid den Nachruhm g\u00f6nnen,", "tokens": ["So", "soll", "mir", "doch", "der", "Neid", "den", "Nach\u00b7ruhm", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df leicht kein Mensch so rein als ich geliebt,", "tokens": ["Da\u00df", "leicht", "kein", "Mensch", "so", "rein", "als", "ich", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PIAT", "NN", "ADV", "ADJD", "KOKOM", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Obgleich betr\u00fcbt.", "tokens": ["Ob\u00b7gleich", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.13": {"line.1": {"text": "Obgleich betr\u00fcbt, jedennoch unverzagt.", "tokens": ["Ob\u00b7gleich", "be\u00b7tr\u00fcbt", ",", "je\u00b7den\u00b7noch", "un\u00b7ver\u00b7zagt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Himmel z\u00fcrnt, wer will mit diesem zancken?", "tokens": ["Der", "Him\u00b7mel", "z\u00fcrnt", ",", "wer", "will", "mit", "die\u00b7sem", "zan\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWS", "VMFIN", "APPR", "PDAT", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohin mich auch mein hart Verh\u00e4ngn\u00fc\u00df jagt,", "tokens": ["Wo\u00b7hin", "mich", "auch", "mein", "hart", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da bleibest du ein Trostbild der Gedancken;", "tokens": ["Da", "blei\u00b7best", "du", "ein", "Trost\u00b7bild", "der", "Ge\u00b7dan\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wirst du mir nicht, so ha\u00df ich Lieb und Eh.", "tokens": ["Wirst", "du", "mir", "nicht", ",", "so", "ha\u00df", "ich", "Lieb", "und", "Eh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nun, Kind, ich geh.", "tokens": ["Nun", ",", "Kind", ",", "ich", "geh."], "token_info": ["word", "punct", "word", "punct", "word", "abbreviation"], "pos": ["ADV", "$,", "NN", "$,", "PPER", "NE"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.14": {"line.1": {"text": "Nun, Kind, ich geh. Geh auch und nimm den Ku\u00df,", "tokens": ["Nun", ",", "Kind", ",", "ich", "geh.", "Geh", "auch", "und", "nimm", "den", "Ku\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "abbreviation", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "PPER", "NE", "NE", "ADV", "KON", "VVIMP", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir martern nur einander durch dies Lezen.", "tokens": ["Wir", "mar\u00b7tern", "nur", "ein\u00b7an\u00b7der", "durch", "dies", "Le\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich zwinge mich, den ungewi\u00dfen Fu\u00df,", "tokens": ["Ich", "zwin\u00b7ge", "mich", ",", "den", "un\u00b7ge\u00b7wi\u00b7\u00dfen", "Fu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den du verweilst, Gott weis wohin zu sezen;", "tokens": ["Den", "du", "ver\u00b7weilst", ",", "Gott", "weis", "wo\u00b7hin", "zu", "se\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "NN", "PTKVZ", "PWAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Ungl\u00fcck st\u00fcrmt, die L\u00e4strer stimmen ein,", "tokens": ["Das", "Un\u00b7gl\u00fcck", "st\u00fcrmt", ",", "die", "L\u00e4st\u00b7rer", "stim\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ergieb dich drein!", "tokens": ["Er\u00b7gieb", "dich", "drein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.15": {"line.1": {"text": "Ergieb dich drein! Es bliz auch nah und fern;", "tokens": ["Er\u00b7gieb", "dich", "drein", "!", "Es", "bliz", "auch", "nah", "und", "fern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein schneller Wind kan leicht das Wetter \u00e4ndern.", "tokens": ["Ein", "schnel\u00b7ler", "Wind", "kan", "leicht", "das", "Wet\u00b7ter", "\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Vaterland versagt mir Gl\u00fcck und Stern,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", "ver\u00b7sagt", "mir", "Gl\u00fcck", "und", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dies bl\u00fcht vielleicht in unbekandten L\u00e4ndern.", "tokens": ["Dies", "bl\u00fcht", "viel\u00b7leicht", "in", "un\u00b7be\u00b7kand\u00b7ten", "L\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Flei\u00df ist froh, nur dich noch zu erh\u00f6hn,", "tokens": ["Mein", "Flei\u00df", "ist", "froh", ",", "nur", "dich", "noch", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADV", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Viel auszustehn.", "tokens": ["Viel", "aus\u00b7zu\u00b7stehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.16": {"line.1": {"text": "Viel auszustehn und gleichwohl frey zu seyn,", "tokens": ["Viel", "aus\u00b7zu\u00b7stehn", "und", "gleich\u00b7wohl", "frey", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word"