{"dta.poem.7663": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kann aus meinem Haus nicht auf- noch abwerts schreiten,", "tokens": ["Ich", "kann", "aus", "mei\u00b7nem", "Haus", "nicht", "auf", "noch", "ab\u00b7werts", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "TRUNC", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nicht mich Kinder zwei verlorene begleiten.", "tokens": ["Da\u00df", "nicht", "mich", "Kin\u00b7der", "zwei", "ver\u00b7lo\u00b7re\u00b7ne", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPER", "NN", "CARD", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Denn aufwerts liegt vom Haus ein Graben, den mein Fu\u00df", "tokens": ["Denn", "auf\u00b7werts", "liegt", "vom", "Haus", "ein", "Gra\u00b7ben", ",", "den", "mein", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beschreitet niemals, da\u00df ich nicht gedenken mu\u00df,", "tokens": ["Be\u00b7schrei\u00b7tet", "nie\u00b7mals", ",", "da\u00df", "ich", "nicht", "ge\u00b7den\u00b7ken", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie ich das letztemal sie dieses Weges f\u00fchrte,", "tokens": ["Wie", "ich", "das", "letz\u00b7te\u00b7mal", "sie", "die\u00b7ses", "We\u00b7ges", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDS", "ADV", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als heimlich Todesglut in ihnen schon sich sch\u00fcrte.", "tokens": ["Als", "heim\u00b7lich", "To\u00b7des\u00b7glut", "in", "ih\u00b7nen", "schon", "sich", "sch\u00fcr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "APPR", "PPER", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den kleinen Schrittchen war der Graben da zu breit,", "tokens": ["Den", "klei\u00b7nen", "Schritt\u00b7chen", "war", "der", "Gra\u00b7ben", "da", "zu", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch sie vertrauten auf mein v\u00e4terlich Geleit.", "tokens": ["Doch", "sie", "ver\u00b7trau\u00b7ten", "auf", "mein", "v\u00e4\u00b7ter\u00b7lich", "Ge\u00b7leit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dar\u00fcber hob ich sie, und dachte sie zu haben", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "hob", "ich", "sie", ",", "und", "dach\u00b7te", "sie", "zu", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "$,", "KON", "VVFIN", "PPER", "PTKZU", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gebracht, wie \u00fcber den, schon \u00fcber jeden Graben.", "tokens": ["Ge\u00b7bracht", ",", "wie", "\u00fc\u00b7ber", "den", ",", "schon", "\u00fc\u00b7ber", "je\u00b7den", "Gra\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "APPR", "ART", "$,", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nicht bei dem Graben fiel mir damals ein das Grab;", "tokens": ["Nicht", "bei", "dem", "Gra\u00b7ben", "fiel", "mir", "da\u00b7mals", "ein", "das", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jetzt f\u00e4llt mirs immer ein, seitdem ich ihm sie gab.", "tokens": ["Jetzt", "f\u00e4llt", "mirs", "im\u00b7mer", "ein", ",", "seit\u00b7dem", "ich", "ihm", "sie", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch abwerts von dem Haus wenn ich mich wenden wollte,", "tokens": ["Doch", "ab\u00b7werts", "von", "dem", "Haus", "wenn", "ich", "mich", "wen\u00b7den", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ist das Pflaster wo der Leichenwagen rollte.", "tokens": ["Da", "ist", "das", "Pflas\u00b7ter", "wo", "der", "Lei\u00b7chen\u00b7wa\u00b7gen", "roll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Sein Rollen h\u00f6r' ich noch, und glaube noch die Spur", "tokens": ["Sein", "Rol\u00b7len", "h\u00f6r'", "ich", "noch", ",", "und", "glau\u00b7be", "noch", "die", "Spur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu sehn, wie auch inde\u00df manch andres dr\u00fcber fuhr.", "tokens": ["Zu", "sehn", ",", "wie", "auch", "in\u00b7de\u00df", "manch", "and\u00b7res", "dr\u00fc\u00b7ber", "fuhr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "ADV", "ADV", "PIAT", "PIS", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Was auch dar\u00fcber fuhr, nie hat's die Spur verwischt,", "tokens": ["Was", "auch", "da\u00b7r\u00fc\u00b7ber", "fuhr", ",", "nie", "hat's", "die", "Spur", "ver\u00b7wischt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PAV", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und stets auf dieser Spur geh' ich, die nie erlischt.", "tokens": ["Und", "stets", "auf", "die\u00b7ser", "Spur", "geh'", "ich", ",", "die", "nie", "er\u00b7lischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}