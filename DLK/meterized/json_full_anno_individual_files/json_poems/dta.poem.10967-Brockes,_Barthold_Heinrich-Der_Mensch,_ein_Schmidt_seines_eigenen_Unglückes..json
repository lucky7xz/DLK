{"dta.poem.10967": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Mensch,  \n ein Schmidt seines eigenen Ungl\u00fcckes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir nicht selber Menschen w\u00e4ren,", "tokens": ["Wenn", "wir", "nicht", "sel\u00b7ber", "Men\u00b7schen", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sollten wo von einem Wesen, das ", "tokens": ["Und", "soll\u00b7ten", "wo", "von", "ei\u00b7nem", "We\u00b7sen", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VMFIN", "PWAV", "APPR", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das hei\u00dft, sich selbst Gedanken zeugen; wie gl\u00fccklich w\u00fcrd", "tokens": ["Das", "hei\u00dft", ",", "sich", "selbst", "Ge\u00b7dan\u00b7ken", "zeu\u00b7gen", ";", "wie", "gl\u00fcck\u00b7lich", "w\u00fcrd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PRF", "ADV", "NN", "VVFIN", "$.", "PWAV", "ADJD", "VAFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von uns nicht angesehen seyn! Zu seinem eignen Heil und Gl\u00fcck", "tokens": ["Von", "uns", "nicht", "an\u00b7ge\u00b7se\u00b7hen", "seyn", "!", "Zu", "sei\u00b7nem", "eig\u00b7nen", "Heil", "und", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKNEG", "VVPP", "VAINF", "$.", "APPR", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Hat jeder, (d\u00e4chten wir,) den Schl\u00fcssel; er wird in jedem Au-", "tokens": ["Hat", "je\u00b7der", ",", "(", "d\u00e4ch\u00b7ten", "wir", ",", ")", "den", "Schl\u00fcs\u00b7sel", ";", "er", "wird", "in", "je\u00b7dem", "Au"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "ART", "NN", "$.", "PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Zu seinem eigenen Vergn\u00fcgen, sich frohe Vorw\u00fcrf selbst erlesen,", "tokens": ["Zu", "sei\u00b7nem", "ei\u00b7ge\u00b7nen", "Ver\u00b7gn\u00fc\u00b7gen", ",", "sich", "fro\u00b7he", "Vor\u00b7w\u00fcrf", "selbst", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PRF", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Und durch ein stets auf seiner Lust nur blo\u00df allein gerichtet", "tokens": ["Und", "durch", "ein", "stets", "auf", "sei\u00b7ner", "Lust", "nur", "blo\u00df", "al\u00b7lein", "ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADV", "APPR", "PPOSAT", "NN", "ADV", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "In immer fr\u00f6lichen Gedanken, sich selber neue Freude schenken.", "tokens": ["In", "im\u00b7mer", "fr\u00f6\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken", ",", "sich", "sel\u00b7ber", "neu\u00b7e", "Freu\u00b7de", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "PRF", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Allein, wie gehet es denn zu, da\u00df wir, die wir ja denken k\u00f6nnen,", "tokens": ["Al\u00b7lein", ",", "wie", "ge\u00b7het", "es", "denn", "zu", ",", "da\u00df", "wir", ",", "die", "wir", "ja", "den\u00b7ken", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "$,", "PRELS", "PPER", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+--+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Und zwar mit einem freyen Willen, uns selbst so wenig Gu-", "tokens": ["Und", "zwar", "mit", "ei\u00b7nem", "frey\u00b7en", "Wil\u00b7len", ",", "uns", "selbst", "so", "we\u00b7nig", "Gu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Wir brauchen die\u00df so gro\u00dfe Gut, das beste Theil von unsrer", "tokens": ["Wir", "brau\u00b7chen", "die\u00df", "so", "gro\u00b7\u00dfe", "Gut", ",", "das", "bes\u00b7te", "Theil", "von", "uns\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "ADV", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Ja fast zu nichts, als uns dadurch fast unaufh\u00f6rlich selbst zu", "tokens": ["Ja", "fast", "zu", "nichts", ",", "als", "uns", "da\u00b7durch", "fast", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "selbst", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "PTKA", "PIS", "$,", "KOUS", "PPER", "PAV", "ADV", "ADJD", "ADV", "APPR"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Der Sch\u00f6pfer schenkt uns, in f\u00fcnf Sinnen, f\u00fcnf immer-", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "schenkt", "uns", ",", "in", "f\u00fcnf", "Sin\u00b7nen", ",", "f\u00fcnf", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "APPR", "CARD", "NN", "$,", "CARD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch welche Millionen Vorw\u00fcrf in unsern Geist sich selber", "tokens": ["Durch", "wel\u00b7che", "Mil\u00b7lion\u00b7en", "Vor\u00b7w\u00fcrf", "in", "un\u00b7sern", "Geist", "sich", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "NN", "APPR", "PPOSAT", "NN", "PRF", "ADV"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Es steht bey uns, von ihnen allen, ", "tokens": ["Es", "steht", "bey", "uns", ",", "von", "ih\u00b7nen", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "APPR", "PPER", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Diejenigen, die uns gefallen,", "tokens": ["Die\u00b7je\u00b7ni\u00b7gen", ",", "die", "uns", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Es sey an geistigen Jdeen, es sey an leiblichen Gestalten,", "tokens": ["Es", "sey", "an", "geis\u00b7ti\u00b7gen", "Jdeen", ",", "es", "sey", "an", "leib\u00b7li\u00b7chen", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "Durch ein auf sie gerichtet Denken, uns zuzueignen, zu erhalten.", "tokens": ["Durch", "ein", "auf", "sie", "ge\u00b7rich\u00b7tet", "Den\u00b7ken", ",", "uns", "zu\u00b7zu\u00b7eig\u00b7nen", ",", "zu", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PPER", "VVPP", "NN", "$,", "PPER", "VVIZU", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.19": {"text": "Wir k\u00f6nnen, wenn wir selber wollen, durch eigne Kr\u00e4fte unsrer", "tokens": ["Wir", "k\u00f6n\u00b7nen", ",", "wenn", "wir", "sel\u00b7ber", "wol\u00b7len", ",", "durch", "eig\u00b7ne", "Kr\u00e4f\u00b7te", "uns\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "ADV", "VMFIN", "$,", "APPR", "ADJA", "NN", "PPOSAT"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Jdeen, die uns widrig sind, vertreiben, angenehme w\u00e4hlen.", "tokens": ["Jdeen", ",", "die", "uns", "wid\u00b7rig", "sind", ",", "ver\u00b7trei\u00b7ben", ",", "an\u00b7ge\u00b7neh\u00b7me", "w\u00e4h\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,", "VVINF", "$,", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.21": {"text": "Wie k\u00f6mmt es, da\u00df wir dieser Kraft, die wir wahrhaftig in", "tokens": ["Wie", "k\u00f6mmt", "es", ",", "da\u00df", "wir", "die\u00b7ser", "Kraft", ",", "die", "wir", "wahr\u00b7haf\u00b7tig", "in"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PDAT", "NN", "$,", "PRELS", "PPER", "ADJD", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "Nicht, als uns selbst zu plagen, brauchen? vergn\u00fcgte schnell", "tokens": ["Nicht", ",", "als", "uns", "selbst", "zu", "pla\u00b7gen", ",", "brau\u00b7chen", "?", "ver\u00b7gn\u00fcg\u00b7te", "schnell"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "ADV", "PTKZU", "VVFIN", "$,", "VVFIN", "$.", "VVFIN", "ADJD"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Und die, so uns mit Bitterkeit und Gram erf\u00fcllen, fertig fassen,", "tokens": ["Und", "die", ",", "so", "uns", "mit", "Bit\u00b7ter\u00b7keit", "und", "Gram", "er\u00b7f\u00fcl\u00b7len", ",", "fer\u00b7tig", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ADV", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.24": {"text": "Best\u00e4ndig uns mit ihnen schleppen u. plagen, ja mit allen Kr\u00e4ften", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "uns", "mit", "ih\u00b7nen", "schlep\u00b7pen", "u.", "pla\u00b7gen", ",", "ja", "mit", "al\u00b7len", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "APPR", "PPER", "VVINF", "KON", "VVFIN", "$,", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.25": {"text": "Die regen Kinder unsrer Seelen an widerw\u00e4rtge Vorw\u00fcrf heften?", "tokens": ["Die", "re\u00b7gen", "Kin\u00b7der", "uns\u00b7rer", "See\u00b7len", "an", "wi\u00b7der\u00b7w\u00e4rt\u00b7ge", "Vor\u00b7w\u00fcrf", "hef\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.26": {"text": "Sprich nicht: Es stehet dieses ja, zu \u00e4ndern, nicht in un-", "tokens": ["Sprich", "nicht", ":", "Es", "ste\u00b7het", "die\u00b7ses", "ja", ",", "zu", "\u00e4n\u00b7dern", ",", "nicht", "in", "un"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "$.", "PPER", "VVFIN", "PDS", "ADV", "$,", "PTKZU", "VVINF", "$,", "PTKNEG", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.27": {"text": "Wer kann doch die Gedanken zwingen? Denn, wo die Men-", "tokens": ["Wer", "kann", "doch", "die", "Ge\u00b7dan\u00b7ken", "zwin\u00b7gen", "?", "Denn", ",", "wo", "die", "Men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "VVINF", "$.", "KON", "$,", "PWAV", "ART", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.28": {"text": "So k\u00f6nnen sie nicht s\u00fcndigen; und was sie freyen Willen nennen,", "tokens": ["So", "k\u00f6n\u00b7nen", "sie", "nicht", "s\u00fcn\u00b7di\u00b7gen", ";", "und", "was", "sie", "frey\u00b7en", "Wil\u00b7len", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "KON", "PWS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.29": {"text": "F\u00e4llt alles weg; wofern sie nicht dasjenige, so sie gedacht,", "tokens": ["F\u00e4llt", "al\u00b7les", "weg", ";", "wo\u00b7fern", "sie", "nicht", "das\u00b7je\u00b7ni\u00b7ge", ",", "so", "sie", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$.", "KOUS", "PPER", "PTKNEG", "PDS", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.30": {"text": "Durch Denken selbst zu \u00e4ndern f\u00e4hig, und die\u00df Verm\u00f6gen nicht", "tokens": ["Durch", "Den\u00b7ken", "selbst", "zu", "\u00e4n\u00b7dern", "f\u00e4\u00b7hig", ",", "und", "die\u00df", "Ver\u00b7m\u00f6\u00b7gen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "PTKZU", "VVINF", "ADJD", "$,", "KON", "PDS", "NN", "PTKNEG"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.31": {"text": "Da\u00df aber es so leicht nicht ist, kann zur Entschuldigung", "tokens": ["Da\u00df", "a\u00b7ber", "es", "so", "leicht", "nicht", "ist", ",", "kann", "zur", "Ent\u00b7schul\u00b7di\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ADV", "ADJD", "PTKNEG", "VAFIN", "$,", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-+++-+--", "measure": "unknown.measure.septa"}, "line.32": {"text": "Weil wir viel Dinge lernen m\u00fcssen, nur blo\u00df ums Brodt, die", "tokens": ["Weil", "wir", "viel", "Din\u00b7ge", "ler\u00b7nen", "m\u00fcs\u00b7sen", ",", "nur", "blo\u00df", "ums", "Brodt", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVINF", "VMINF", "$,", "ADV", "ADV", "APPRART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Liegt alles menschliche Vergn\u00fcgen denn in dem Denken blo\u00df", "tokens": ["Liegt", "al\u00b7les", "menschli\u00b7che", "Ver\u00b7gn\u00fc\u00b7gen", "denn", "in", "dem", "Den\u00b7ken", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "KON", "APPR", "ART", "NN", "ADV"], "meter": "-+-+--+-++-+-+", "measure": "iambic.septa.relaxed"}, "line.34": {"text": "Wie es wahrhaftig blo\u00df nur liegt: So la\u00dft uns doch, zu Got-", "tokens": ["Wie", "es", "wahr\u00b7haf\u00b7tig", "blo\u00df", "nur", "liegt", ":", "So", "la\u00dft", "uns", "doch", ",", "zu", "Got"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "ADV", "VVFIN", "$.", "ADV", "VVIMP", "PPER", "ADV", "$,", "APPR", "TRUNC"], "meter": "-++--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.35": {"text": "Und uns zum Besten, k\u00fcnftighin von unserm Unsinn uns ent-", "tokens": ["Und", "uns", "zum", "Bes\u00b7ten", ",", "k\u00fcnf\u00b7tig\u00b7hin", "von", "un\u00b7serm", "Un\u00b7sinn", "uns", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "$,", "ADV", "APPR", "PPOSAT", "NN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.36": {"text": "Und beym vern\u00fcnftigen Empfinden, beym Riechen, Schme-", "tokens": ["Und", "beym", "ver\u00b7n\u00fcnf\u00b7ti\u00b7gen", "Emp\u00b7fin\u00b7den", ",", "beym", "Rie\u00b7chen", ",", "Schme"], "token