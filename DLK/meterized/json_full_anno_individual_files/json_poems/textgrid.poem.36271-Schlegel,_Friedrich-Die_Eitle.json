ct"], "pos": ["ART", "PPER", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein zu werden sei ihr Sein.", "tokens": ["Sein", "zu", "wer\u00b7den", "sei", "ihr", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PTKZU", "VAINF", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keiner darf der Eine sein;", "tokens": ["Kei\u00b7ner", "darf", "der", "Ei\u00b7ne", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ART", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So kann schweben", "tokens": ["So", "kann", "schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Reizend sch\u00f6n der Liebe Streben.", "tokens": ["Rei\u00b7zend", "sch\u00f6n", "der", "Lie\u00b7be", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Reizte Sch\u00f6nheit keinem eigen,", "tokens": ["Reiz\u00b7te", "Sch\u00f6n\u00b7heit", "kei\u00b7nem", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4r' das Leben", "tokens": ["W\u00e4r'", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Reizend sch\u00f6n, ein lieblich Streben.", "tokens": ["Rei\u00b7zend", "sch\u00f6n", ",", "ein", "lieb\u00b7lich", "Stre\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "AR