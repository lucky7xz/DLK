{"dta.poem.1661": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ein Pflugeisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch/ ein Erdenklo\u00df/ soll billig Erde pfl\u00fcgen;", "tokens": ["Der", "Mensch", "/", "ein", "Er\u00b7den\u00b7klo\u00df", "/", "soll", "bil\u00b7lig", "Er\u00b7de", "pfl\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "VMFIN", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo er gewachsen ist/ da wird er wieder liegen.", "tokens": ["Wo", "er", "ge\u00b7wach\u00b7sen", "ist", "/", "da", "wird", "er", "wie\u00b7der", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}