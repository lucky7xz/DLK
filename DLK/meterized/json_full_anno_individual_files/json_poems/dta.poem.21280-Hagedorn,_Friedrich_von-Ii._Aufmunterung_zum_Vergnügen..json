{"dta.poem.21280": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n Aufmunterung zum Vergn\u00fcgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1752", "urn": "urn:nbn:de:kobv:b4-200905199117", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erlernt von muntern Herzen", "tokens": ["Er\u00b7lernt", "von", "mun\u00b7tern", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Kunst begl\u00fcckt zu scherzen,", "tokens": ["Die", "Kunst", "be\u00b7gl\u00fcckt", "zu", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Kunst vergn\u00fcgt zu seyn.", "tokens": ["Die", "Kunst", "ver\u00b7gn\u00fcgt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Versucht es. Lasst uns singen,", "tokens": ["Ver\u00b7sucht", "es", ".", "Lasst", "uns", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Alter zu verj\u00fcngen,", "tokens": ["Das", "Al\u00b7ter", "zu", "ver\u00b7j\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Jugend zu erfreun.", "tokens": ["Die", "Ju\u00b7gend", "zu", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Macht neue Freundschafts-Schl\u00fcsse!", "tokens": ["Macht", "neu\u00b7e", "Freund\u00b7schafts\u00b7Sch\u00b7l\u00fcs\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Jhr Kinder, gebt euch K\u00fcsse!", "tokens": ["Ihr", "Kin\u00b7der", ",", "gebt", "euch", "K\u00fcs\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Jhr V\u00e4ter, gebt euch Wein!", "tokens": ["Ihr", "V\u00e4\u00b7ter", ",", "gebt", "euch", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}