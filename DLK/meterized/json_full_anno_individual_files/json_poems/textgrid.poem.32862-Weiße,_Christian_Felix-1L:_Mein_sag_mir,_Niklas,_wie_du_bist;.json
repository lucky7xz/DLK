{"textgrid.poem.32862": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein sag mir, Niklas, wie du bist;", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein sag mir, Niklas, wie du bist;", "tokens": ["Mein", "sag", "mir", ",", "Nik\u00b7las", ",", "wie", "du", "bist", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "$,", "NE", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da weinst du, weil ich dir erz\u00e4hlet,", "tokens": ["Da", "weinst", "du", ",", "weil", "ich", "dir", "er\u00b7z\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df mich die Mutter ausgeschm\u00e4let,", "tokens": ["Da\u00df", "mich", "die", "Mut\u00b7ter", "aus\u00b7ge\u00b7schm\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du mich, weist du wohl, gek\u00fc\u00dft.", "tokens": ["Da\u00df", "du", "mich", ",", "weist", "du", "wohl", ",", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$,", "VVFIN", "PPER", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du Narr, wie schm\u00e4lt sie denn auf dich?", "tokens": ["Du", "Narr", ",", "wie", "schm\u00e4lt", "sie", "denn", "auf", "dich", "?"], "token_info