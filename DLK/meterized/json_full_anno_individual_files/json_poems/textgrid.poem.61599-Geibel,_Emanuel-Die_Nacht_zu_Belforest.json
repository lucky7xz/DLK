{"textgrid.poem.61599": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Die Nacht zu Belforest", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsagt's dem K\u00f6nig, meinem Herrn,", "tokens": ["\u00bb", "sagt's", "dem", "K\u00f6\u00b7nig", ",", "mei\u00b7nem", "Herrn", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df der einz'ge Sohn und Erbe", "tokens": ["Da\u00df", "der", "einz'\u00b7ge", "Sohn", "und", "Er\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seines weiland Seneschalls,", "tokens": ["Sei\u00b7nes", "wei\u00b7land", "Se\u00b7ne\u00b7schalls", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt's ihm, da\u00df er schuldlos sterbe!", "tokens": ["Sagt's", "ihm", ",", "da\u00df", "er", "schuld\u00b7los", "ster\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Niemals hab' ich mit dem Feind", "tokens": ["Nie\u00b7mals", "hab'", "ich", "mit", "dem", "Feind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00e4nkevoll Verkehr gepflogen;", "tokens": ["R\u00e4n\u00b7ke\u00b7voll", "Ver\u00b7kehr", "ge\u00b7pflo\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die's dem K\u00f6nig hinterbracht,", "tokens": ["Die's", "dem", "K\u00f6\u00b7nig", "hin\u00b7ter\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier beschw\u00f6r' ich's, da\u00df sie logen.", "tokens": ["Hier", "be\u00b7schw\u00f6r'", "ich's", ",", "da\u00df", "sie", "lo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbdoch ich f\u00fcrcht', er glaubt' es gern,", "tokens": ["\u00bb", "doch", "ich", "f\u00fcrcht'", ",", "er", "glaubt'", "es", "gern", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn nach unsern Lehn und Landen,", "tokens": ["Denn", "nach", "un\u00b7sern", "Lehn", "und", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Schlo\u00df von Belforest", "tokens": ["Nach", "dem", "Schlo\u00df", "von", "Bel\u00b7fo\u00b7rest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Hat ihm l\u00e4ngst der Sinn gestanden.\u00ab", "tokens": ["Hat", "ihm", "l\u00e4ngst", "der", "Sinn", "ge\u00b7stan\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Also spricht Graf Aimery,", "tokens": ["Al\u00b7so", "spricht", "Graf", "Ai\u00b7me\u00b7ry", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als er niederkniet am Blocke;", "tokens": ["Als", "er", "nie\u00b7der\u00b7kniet", "am", "Blo\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blitzend f\u00e4hrt herab das Beil,", "tokens": ["Blit\u00b7zend", "f\u00e4hrt", "her\u00b7ab", "das", "Beil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es schallt die Totenglocke.", "tokens": ["Und", "es", "schallt", "die", "To\u00b7ten\u00b7glo\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch wer wagt's, des Grafen Wort", "tokens": ["Doch", "wer", "wagt's", ",", "des", "Gra\u00b7fen", "Wort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "$,", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor des K\u00f6nigs Ohr zu tragen!", "tokens": ["Vor", "des", "K\u00f6\u00b7nigs", "Ohr", "zu", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Forsten von Poitou", "tokens": ["In", "den", "Fors\u00b7ten", "von", "Poi\u00b7tou"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Schweift er schon, den Hirsch zu jagen.", "tokens": ["Schweift", "er", "schon", ",", "den", "Hirsch", "zu", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dort von edler Spur verlockt", "tokens": ["Dort", "von", "ed\u00b7ler", "Spur", "ver\u00b7lockt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Irrt er nachts im Waldesgrunde;", "tokens": ["Irrt", "er", "nachts", "im", "Wal\u00b7des\u00b7grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor das Schlo\u00df von Belforest", "tokens": ["Vor", "das", "Schlo\u00df", "von", "Bel\u00b7fo\u00b7rest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Kommt er um die zw\u00f6lfte Stunde.", "tokens": ["Kommt", "er", "um", "die", "zw\u00f6lf\u00b7te", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Langsam, wie er st\u00f6\u00dft ins Horn,", "tokens": ["Lang\u00b7sam", ",", "wie", "er", "st\u00f6\u00dft", "ins", "Horn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinkt vor ihm die Br\u00fccke nieder,", "tokens": ["Sinkt", "vor", "ihm", "die", "Br\u00fc\u00b7cke", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Langsam in den Angeln dreht", "tokens": ["Lang\u00b7sam", "in", "den", "An\u00b7geln", "dreht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich das Tor und schlie\u00dft sich wieder.", "tokens": ["Sich", "das", "Tor", "und", "schlie\u00dft", "sich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "KON", "VVFIN", "PRF", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch kein Diener l\u00e4\u00dft sich schaun;", "tokens": ["Doch", "kein", "Die\u00b7ner", "l\u00e4\u00dft", "sich", "schaun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Nur des Monds ged\u00e4mpfter Schimmer", "tokens": ["Nur", "des", "Monds", "ge\u00b7d\u00e4mpf\u00b7ter", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leuchtet ihm zum Ahnensaal", "tokens": ["Leuch\u00b7tet", "ihm", "zum", "Ah\u00b7nen\u00b7saal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die ausgestorbnen Zimmer.", "tokens": ["Durch", "die", "aus\u00b7ge\u00b7storb\u00b7nen", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aber dort im Steinkamin", "tokens": ["A\u00b7ber", "dort", "im", "Stein\u00b7ka\u00b7min"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht er rot ein Feuer blitzen,", "tokens": ["Sieht", "er", "rot", "ein", "Feu\u00b7er", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht den toten Seneschall", "tokens": ["Sieht", "den", "to\u00b7ten", "Se\u00b7ne\u00b7schall"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An der Glut im Lehnstuhl sitzen.", "tokens": ["An", "der", "Glut", "im", "Lehn\u00b7stuhl", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Der erhebt sich vor dem Gast,", "tokens": ["Der", "er\u00b7hebt", "sich", "vor", "dem", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit halberloschnem Klange", "tokens": ["Und", "mit", "hal\u00b7ber\u00b7lo\u00b7schnem", "Klan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Spricht er: \u00bbKommt ihr endlich, Sire?", "tokens": ["Spricht", "er", ":", "\u00bb", "Kommt", "ihr", "end\u00b7lich", ",", "Si\u00b7re", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "VVIMP", "PPER", "ADV", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch erwartet hab' ich lange.", "tokens": ["Euch", "er\u00b7war\u00b7tet", "hab'", "ich", "lan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbnur um eins Euch kundzutun,", "tokens": ["\u00bb", "nur", "um", "eins", "Euch", "kund\u00b7zu\u00b7tun", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "PIS", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stieg ich aus der Gruft der V\u00e4ter,", "tokens": ["Stieg", "ich", "aus", "der", "Gruft", "der", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df vom Stamm der Belforest", "tokens": ["Da\u00df", "vom", "Stamm", "der", "Bel\u00b7fo\u00b7rest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Nie gezeugt ward ein Verr\u00e4ter.\u00ab \u2013", "tokens": ["Nie", "ge\u00b7zeugt", "ward", "ein", "Ver\u00b7r\u00e4\u00b7ter", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Als der K\u00f6nig das vernahm,", "tokens": ["Als", "der", "K\u00f6\u00b7nig", "das", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Warf ihn tiefes Grausen nieder;", "tokens": ["Warf", "ihn", "tie\u00b7fes", "Grau\u00b7sen", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sinnberaubt am Morgen fand", "tokens": ["Sinn\u00b7be\u00b7raubt", "am", "Mor\u00b7gen", "fand"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein Gefolg' im Saal ihn wieder.", "tokens": ["Sein", "Ge\u00b7fol\u00b7g'", "im", "Saal", "ihn", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PPER", "ADV", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.13": {"line.1": {"text": "Sieches Leid beschlich seitdem,", "tokens": ["Sie\u00b7ches", "Leid", "be\u00b7schlich", "seit\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefer Tr\u00fcbsinn all sein Wesen;", "tokens": ["Tie\u00b7fer", "Tr\u00fcb\u00b7sinn", "all", "sein", "We\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Nacht z