{"textgrid.poem.25364": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erst war grenzenloser Durst,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erst war grenzenloser Durst,", "tokens": ["Erst", "war", "gren\u00b7zen\u00b7lo\u00b7ser", "Durst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ausholend Gl\u00fcck, schamvolles Sichbeschauen,", "tokens": ["aus\u00b7ho\u00b7lend", "Gl\u00fcck", ",", "scham\u00b7vol\u00b7les", "Sich\u00b7be\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Abends in der Jungenstube, wenn die Lampe ausgieng,", "tokens": ["A\u00b7bends", "in", "der", "Jun\u00b7gen\u00b7stu\u00b7be", ",", "wenn", "die", "Lam\u00b7pe", "aus\u00b7gieng", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "Z\u00e4rtlichkeiten \u00fcberschw\u00e4nglich hingestr\u00f6mt", "tokens": ["Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", "\u00fc\u00b7bersc\u00b7hw\u00e4ng\u00b7lich", "hin\u00b7ge\u00b7str\u00f6mt"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVPP"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "an traumerschaffne Frauen,", "tokens": ["an", "trau\u00b7mer\u00b7schaff\u00b7ne", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verz\u00fcckte Worte ins Leere gesprochen", "tokens": ["Ver\u00b7z\u00fcck\u00b7te", "Wor\u00b7te", "ins", "Lee\u00b7re", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "und im Blut der irre Brand \u2013", "tokens": ["und", "im", "Blut", "der", "ir\u00b7re", "Brand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis man sich eines Nachts", "tokens": ["Bis", "man", "sich", "ei\u00b7nes", "Nachts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PRF", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "in einem schalen Zimmer wiederfand,", "tokens": ["in", "ei\u00b7nem", "scha\u00b7len", "Zim\u00b7mer", "wie\u00b7der\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "St\u00f6hnend, dumpf, und seine Sehnsucht \u00fcber einen tr\u00fcben,", "tokens": ["St\u00f6h\u00b7nend", ",", "dumpf", ",", "und", "sei\u00b7ne", "Sehn\u00b7sucht", "\u00fc\u00b7ber", "ei\u00b7nen", "tr\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "KON", "PPOSAT", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "eingesunknen K\u00f6rper leerte,", "tokens": ["ein\u00b7ge\u00b7sun\u00b7knen", "K\u00f6r\u00b7per", "leer\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sich auf die Z\u00e4hne bi\u00df und wu\u00dfte:", "tokens": ["Sich", "auf", "die", "Z\u00e4h\u00b7ne", "bi\u00df", "und", "wu\u00df\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "APPR", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "dieses sei das Leben, dem man sich bekehrte.", "tokens": ["die\u00b7ses", "sei", "das", "Le\u00b7ben", ",", "dem", "man", "sich", "be\u00b7kehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Ein ganzer blondverkl\u00e4rter Knabenhimmel", "tokens": ["Ein", "gan\u00b7zer", "blond\u00b7ver\u00b7kl\u00e4r\u00b7ter", "Kna\u00b7ben\u00b7him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "stand in Flammen \u2013", "tokens": ["stand", "in", "Flam\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Damals st\u00fcrzte G\u00f6ttliches zusammen ...", "tokens": ["Da\u00b7mals", "st\u00fcrz\u00b7te", "G\u00f6tt\u00b7li\u00b7ches", "zu\u00b7sam\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PTKVZ", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Aber Seele h\u00fcllte g\u00fctig enge Kammer,", "tokens": ["A\u00b7ber", "See\u00b7le", "h\u00fcll\u00b7te", "g\u00fc\u00b7tig", "en\u00b7ge", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "welken Leib und Scham und Ekel ein,", "tokens": ["wel\u00b7ken", "Leib", "und", "Scham", "und", "E\u00b7kel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Und niemals wieder war Liebe so sanft,", "tokens": ["Und", "nie\u00b7mals", "wie\u00b7der", "war", "Lie\u00b7be", "so", "sanft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "dem\u00fctig und rein,", "tokens": ["de\u00b7m\u00fc\u00b7tig", "und", "rein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "So voller Musik wie da ...", "tokens": ["So", "vol\u00b7ler", "Mu\u00b7sik", "wie", "da", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KOKOM", "KOUS", "$("], "meter": "-+----+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Dann sind Jahre hingegangen", "tokens": ["Dann", "sind", "Jah\u00b7re", "hin\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und haben ihren Zoll gezahlt.", "tokens": ["und", "ha\u00b7ben", "ih\u00b7ren", "Zoll", "ge\u00b7zahlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus ihrem Flu\u00df manch' eine Liebesstunde", "tokens": ["Aus", "ih\u00b7rem", "Flu\u00df", "man\u00b7ch'", "ei\u00b7ne", "Lie\u00b7bess\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "wie eine Mondwelle aufstrahlt.", "tokens": ["wie", "ei\u00b7ne", "Mond\u00b7wel\u00b7le", "auf\u00b7strahlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Aber Wunder wich zur\u00fcck, wie sch\u00f6ne hohe Kirchen", "tokens": ["A\u00b7ber", "Wun\u00b7der", "wich", "zu\u00b7r\u00fcck", ",", "wie", "sch\u00f6\u00b7ne", "ho\u00b7he", "Kir\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$,", "PWAV", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Sommers vor der D\u00e4mmerung in die Schatten weichen.", "tokens": ["Som\u00b7mers", "vor", "der", "D\u00e4m\u00b7me\u00b7rung", "in", "die", "Schat\u00b7ten", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Eine Goldspur wehte \u00fcbern Abendhimmel hin:", "tokens": ["Ei\u00b7ne", "Gold\u00b7spur", "weh\u00b7te", "\u00fc\u00b7bern", "A\u00b7bend\u00b7him\u00b7mel", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "nichts konnte sie erreichen.", "tokens": ["nichts", "konn\u00b7te", "sie", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Seele blieb verlassen,", "tokens": ["See\u00b7le", "blieb", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Sehnsucht kam mit leeren Armen heim,", "tokens": ["Sehn\u00b7sucht", "kam", "mit", "lee\u00b7ren", "Ar\u00b7men", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "so oft ich sie hinausgeschickt,", "tokens": ["so", "oft", "ich", "sie", "hin\u00b7aus\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wenn ich im Dunkel, nach Erf\u00fcllung rang,", "tokens": ["Wenn", "ich", "im", "Dun\u00b7kel", ",", "nach", "Er\u00b7f\u00fcl\u00b7lung", "rang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "in Hauch und Haar geliebter Frau'n verstrickt.", "tokens": ["in", "Hauch", "und", "Haar", "ge\u00b7lieb\u00b7ter", "Frau'n", "ver\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Denn immer griffen meine H\u00e4nde", "tokens": ["Denn", "im\u00b7mer", "grif\u00b7fen", "mei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "nach dem fernen bunten Ding,", "tokens": ["nach", "dem", "fer\u00b7nen", "bun\u00b7ten", "Ding", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Das einmal", "tokens": ["Das", "ein\u00b7mal"], "token_info": ["word", "word"], "pos": ["PDS", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "\u00fcber meinem Knabenhimmel hieng.", "tokens": ["\u00fc\u00b7ber", "mei\u00b7nem", "Kna\u00b7ben\u00b7him\u00b7mel", "hieng", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Und immer rief mein Kiel nach Sturm \u2013", "tokens": ["Und", "im\u00b7mer", "rief", "mein", "Kiel", "nach", "Sturm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "doch jeder Sturm hat mich ans Land geschwemmt,", "tokens": ["doch", "je\u00b7der", "Sturm", "hat", "mich", "ans", "Land", "ge\u00b7schwemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Sterne brachen, und die Flut zerfiel,", "tokens": ["Ster\u00b7ne", "bra\u00b7chen", ",", "und", "die", "Flut", "zer\u00b7fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "in Schlick und Sand verschl\u00e4mmt ...", "tokens": ["in", "Schlick", "und", "Sand", "ver\u00b7schl\u00e4mmt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Daran mu\u00dft' ich heute denken,", "tokens": ["Da\u00b7ran", "mu\u00dft'", "ich", "heu\u00b7te", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "und es fiel mir ein,", "tokens": ["und", "es", "fiel", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.24": {"text": "Da\u00df alles das umsonst,", "tokens": ["Da\u00df", "al\u00b7les", "das", "um\u00b7sonst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "und da\u00df es anders m\u00fcsse sein,", "tokens": ["und", "da\u00df", "es", "an\u00b7ders", "m\u00fcs\u00b7se", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und da\u00df vielleicht die Liebe nichts", "tokens": ["Und", "da\u00df", "viel\u00b7leicht", "die", "Lie\u00b7be", "nichts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "als schweigen,", "tokens": ["als", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.28": {"text": "Mit einer Frau am Meeresufer stehn", "tokens": ["Mit", "ei\u00b7ner", "Frau", "am", "Mee\u00b7res\u00b7u\u00b7fer", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "und durch die D\u00fcnen horchen,", "tokens": ["und", "durch", "die", "D\u00fc\u00b7nen", "hor\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "wie von fern die Wasser steigen.", "tokens": ["wie", "von", "fern", "die", "Was\u00b7ser", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Erst war grenzenloser Durst,", "tokens": ["Erst", "war", "gren\u00b7zen\u00b7lo\u00b7ser", "Durst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ausholend Gl\u00fcck, schamvolles Sichbeschauen,", "tokens": ["aus\u00b7ho\u00b7lend", "Gl\u00fcck", ",", "scham\u00b7vol\u00b7les", "Sich\u00b7be\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Abends in der Jungenstube, wenn die Lampe ausgieng,", "tokens": ["A\u00b7bends", "in", "der", "Jun\u00b7gen\u00b7stu\u00b7be", ",", "wenn", "die", "Lam\u00b7pe", "aus\u00b7gieng", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "Z\u00e4rtlichkeiten \u00fcberschw\u00e4nglich hingestr\u00f6mt", "tokens": ["Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", "\u00fc