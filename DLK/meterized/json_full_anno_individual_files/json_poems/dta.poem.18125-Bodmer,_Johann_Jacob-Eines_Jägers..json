{"dta.poem.18125": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Eines J\u00e4gers.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jn dieser holen Klufft gesuchet hat sein L\u00e4ger,", "tokens": ["In", "die\u00b7ser", "ho\u00b7len", "Klufft", "ge\u00b7su\u00b7chet", "hat", "sein", "L\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein grausamer Tyrann und Feind der wilden Thier,", "tokens": ["Ein", "grau\u00b7sa\u00b7mer", "Ty\u00b7rann", "und", "Feind", "der", "wil\u00b7den", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jezt hat er wiederum auch seinen Lohn darf\u00fcr.", "tokens": ["Jezt", "hat", "er", "wie\u00b7de\u00b7rum", "auch", "sei\u00b7nen", "Lohn", "dar\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Tod der war sein Hund, die Kranckheit war sein J\u00e4ger,", "tokens": ["Der", "Tod", "der", "war", "sein", "Hund", ",", "die", "Kran\u00b7ck\u00b7heit", "war", "sein", "J\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "So ist der J\u00e4ger nun, wie k\u00fchn er sey und starck,", "tokens": ["So", "ist", "der", "J\u00e4\u00b7ger", "nun", ",", "wie", "k\u00fchn", "er", "sey", "und", "starck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gejaget durch den Tod hier unter diesen Sarck.", "tokens": ["Ge\u00b7ja\u00b7get", "durch", "den", "Tod", "hier", "un\u00b7ter", "die\u00b7sen", "Sarck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}