{"dta.poem.21415": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sehnte mich nach dem blauen Rauch,", "tokens": ["Ich", "sehn\u00b7te", "mich", "nach", "dem", "blau\u00b7en", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der aufsteigt aus deutschen Schornsteinen,", "tokens": ["Der", "auf\u00b7steigt", "aus", "deut\u00b7schen", "Schorn\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nach nieders\u00e4chsischen Nachtigall\u2019n,", "tokens": ["Nach", "nie\u00b7der\u00b7s\u00e4ch\u00b7si\u00b7schen", "Nach\u00b7ti\u00b7gall'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nach stillen Buchenhainen.", "tokens": ["Nach", "stil\u00b7len", "Bu\u00b7chen\u00b7hai\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sehnte mich nach den Pl\u00e4tzen sogar,", "tokens": ["Ich", "sehn\u00b7te", "mich", "nach", "den", "Pl\u00e4t\u00b7zen", "so\u00b7gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Nach jenen Leidensstazionen,", "tokens": ["Nach", "je\u00b7nen", "Lei\u00b7dens\u00b7sta\u00b7zi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich geschleppt das Jugendkreuz", "tokens": ["Wo", "ich", "ge\u00b7schleppt", "das", "Ju\u00b7gend\u00b7kreuz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Dornenkronen.", "tokens": ["Und", "mei\u00b7ne", "Dor\u00b7nen\u00b7kro\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich wollte weinen wo ich einst", "tokens": ["Ich", "woll\u00b7te", "wei\u00b7nen", "wo", "ich", "einst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geweint die bittersten Thr\u00e4nen \u2013", "tokens": ["Ge\u00b7weint", "die", "bit\u00b7ters\u00b7ten", "Thr\u00e4\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich glaube Vaterlandsliebe nennt", "tokens": ["Ich", "glau\u00b7be", "Va\u00b7ter\u00b7lands\u00b7lie\u00b7be", "nennt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Man dieses th\u00f6rigte Sehnen.", "tokens": ["Man", "die\u00b7ses", "th\u00f6\u00b7rig\u00b7te", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich spreche nicht gern davon; es ist", "tokens": ["Ich", "spre\u00b7che", "nicht", "gern", "da\u00b7von", ";", "es", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PAV", "$.", "PPER", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur eine Krankheit im Grunde.", "tokens": ["Nur", "ei\u00b7ne", "Krank\u00b7heit", "im", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Versch\u00e4mten Gem\u00fcthes, verberge ich stets", "tokens": ["Ver\u00b7sch\u00e4m\u00b7ten", "Ge\u00b7m\u00fc\u00b7thes", ",", "ver\u00b7ber\u00b7ge", "ich", "stets"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dem Publiko meine Wunde.", "tokens": ["Dem", "Pub\u00b7li\u00b7ko", "mei\u00b7ne", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}