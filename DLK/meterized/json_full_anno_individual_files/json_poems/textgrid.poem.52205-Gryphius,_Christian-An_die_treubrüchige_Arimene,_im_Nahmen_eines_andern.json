{"textgrid.poem.52205": {"metadata": {"author": {"name": "Gryphius, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An die treubr\u00fcchige Arimene, im Nahmen eines andern", "genre": "verse", "period": "N.A.", "pub_year": 1677, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fort, Arimene, fort, du liederliche Dirne,", "tokens": ["Fort", ",", "A\u00b7ri\u00b7me\u00b7ne", ",", "fort", ",", "du", "lie\u00b7der\u00b7li\u00b7che", "Dir\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich brauche nichts also; die anders lieben kan,", "tokens": ["Ich", "brau\u00b7che", "nichts", "al\u00b7so", ";", "die", "an\u00b7ders", "lie\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$.", "ART", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als ihrer Treu geziemt, und die mit frecher Stirne", "tokens": ["Als", "ih\u00b7rer", "Treu", "ge\u00b7ziemt", ",", "und", "die", "mit", "fre\u00b7cher", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$,", "KON", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Laden offen h\u00e4lt, steht keinen Hirten an.", "tokens": ["Den", "La\u00b7den", "of\u00b7fen", "h\u00e4lt", ",", "steht", "kei\u00b7nen", "Hir\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein andrer diene dir, mir wils nicht ins Gehirne,", "tokens": ["Ein", "an\u00b7drer", "die\u00b7ne", "dir", ",", "mir", "wils", "nicht", "ins", "Ge\u00b7hir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df wer so untreu ist, sich auf was gutts besan;", "tokens": ["Da\u00df", "wer", "so", "un\u00b7treu", "ist", ",", "sich", "auf", "was", "gutts", "be\u00b7san", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "ADV", "ADJD", "VAFIN", "$,", "PRF", "APPR", "PRELS", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich liebe kein Gespinst von derogleichen Zwirne,", "tokens": ["Ich", "lie\u00b7be", "kein", "Ge\u00b7spinst", "von", "de\u00b7ro\u00b7glei\u00b7chen", "Zwir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und schwer' izt alles ab, was ich zuvor gethan.", "tokens": ["Und", "schwer'", "izt", "al\u00b7les", "ab", ",", "was", "ich", "zu\u00b7vor", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "PTKVZ", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer ein verloschnes Feur und fast verrauchte Strahlen,", "tokens": ["Wer", "ein", "ver\u00b7lo\u00b7schnes", "Feur", "und", "fast", "ver\u00b7rauch\u00b7te", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "KON", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer ein durchl\u00f6chert Tuch, und ausgeleerte Schalen,", "tokens": ["Wer", "ein", "durch\u00b7l\u00f6\u00b7chert", "Tuch", ",", "und", "aus\u00b7ge\u00b7leer\u00b7te", "Scha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "VVPP", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer faule Fische liebt, und einen Krantz von Stroh,", "tokens": ["Wer", "fau\u00b7le", "Fi\u00b7sche", "liebt", ",", "und", "ei\u00b7nen", "Krantz", "von", "Stroh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,", "KON", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der mag mit deiner Gunst, o Arimene, prangen,", "tokens": ["Der", "mag", "mit", "dei\u00b7ner", "Gunst", ",", "o", "A\u00b7ri\u00b7me\u00b7ne", ",", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPOSAT", "NN", "$,", "FM", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mich st\u00f6st ein Eckel an, ich trage kein Verlangen", "tokens": ["Mich", "st\u00f6st", "ein", "E\u00b7ckel", "an", ",", "ich", "tra\u00b7ge", "kein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nach fast verlegner Wahr', und brauche nichts also.", "tokens": ["Nach", "fast", "ver\u00b7leg\u00b7ner", "Wahr'", ",", "und", "brau\u00b7che", "nichts", "al\u00b7so", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "KON", "VVFIN", "PIS", "ADV", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Fort, Arimene, fort, du liederliche Dirne,", "tokens": ["Fort", ",", "A\u00b7ri\u00b7me\u00b7ne", ",", "fort", ",", "du", "lie\u00b7der\u00b7li\u00b7che", "Dir\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich brauche nichts also; die anders lieben kan,", "tokens": ["Ich", "brau\u00b7che", "nichts", "al\u00b7so", ";", "die", "an\u00b7ders", "lie\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$.", "ART", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als ihrer Treu geziemt, und die mit frecher Stirne", "tokens": ["Als", "ih\u00b7rer", "Treu", "ge\u00b7ziemt", ",", "und", "die", "mit", "fre\u00b7cher", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$,", "KON", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Laden offen h\u00e4lt, steht keinen Hirten an.", "tokens": ["Den", "La\u00b7den", "of\u00b7fen", "h\u00e4lt", ",", "steht", "kei\u00b7nen", "Hir