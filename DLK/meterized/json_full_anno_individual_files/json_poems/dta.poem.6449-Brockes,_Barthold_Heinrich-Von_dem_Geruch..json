{"dta.poem.6449": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df alles k\u00f6nnen wir bey Gastereyen finden,", "tokens": ["Die\u00df", "al\u00b7les", "k\u00f6n\u00b7nen", "wir", "bey", "Gas\u00b7te\u00b7re\u00b7yen", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Woselbst sich Kunst und Uberflu\u00df verbinden.", "tokens": ["Wo\u00b7selbst", "sich", "Kunst", "und", "U\u00b7ber\u00b7flu\u00df", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die niedliche und leckerhaffte Speisen,", "tokens": ["Die", "nied\u00b7li\u00b7che", "und", "le\u00b7cker\u00b7haff\u00b7te", "Spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die ihre Krafft schon im Geruch erweisen,", "tokens": ["Die", "ih\u00b7re", "Krafft", "schon", "im", "Ge\u00b7ruch", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sind f\u00e4hig, eine Lust den G\u00e4sten zu erwecken,", "tokens": ["Sind", "f\u00e4\u00b7hig", ",", "ei\u00b7ne", "Lust", "den", "G\u00e4s\u00b7ten", "zu", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie zu geniessen und zu schmecken.", "tokens": ["Sie", "zu", "ge\u00b7nies\u00b7sen", "und", "zu", "schme\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}