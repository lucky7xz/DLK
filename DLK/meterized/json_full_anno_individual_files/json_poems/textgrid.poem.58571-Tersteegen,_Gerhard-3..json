{"textgrid.poem.58571": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt nun, ihr vom Hause Jakob, la\u00dft uns wandeln im Lichte des Herrn! Jes. 2, 5", "tokens": ["Kommt", "nun", ",", "ihr", "vom", "Hau\u00b7se", "Ja\u00b7kob", ",", "la\u00dft", "uns", "wan\u00b7deln", "im", "Lich\u00b7te", "des", "Herrn", "!", "Jes", ".", "2", ",", "5"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["VVFIN", "ADV", "$,", "PPER", "APPRART", "NN", "NE", "$,", "VVIMP", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "--+-+--+--+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Kommt, Jakobs Kinder, die ihr gern zu Hause bleibet,", "tokens": ["Kommt", ",", "Ja\u00b7kobs", "Kin\u00b7der", ",", "die", "ihr", "gern", "zu", "Hau\u00b7se", "blei\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Esau l\u00e4uft und rennt, da\u00df er ein Wildbret kriegt!", "tokens": ["Wenn", "E\u00b7sau", "l\u00e4uft", "und", "rennt", ",", "da\u00df", "er", "ein", "Wild\u00b7bret", "kriegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"te