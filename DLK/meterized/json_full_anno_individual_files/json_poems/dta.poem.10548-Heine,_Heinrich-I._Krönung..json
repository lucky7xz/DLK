{"dta.poem.10548": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  Kr\u00f6nung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Auf, auf! und wappnet Euch!", "tokens": ["Auf", ",", "auf", "!", "und", "wapp\u00b7net", "Euch", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$.", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "La\u00dft die Trompeten klingen,", "tokens": ["La\u00dft", "die", "Trom\u00b7pe\u00b7ten", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "--++-+-", "measure": "anapaest.init"}, "line.3": {"text": "Und hebt mir auf den Schild", "tokens": ["Und", "hebt", "mir", "auf", "den", "Schild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dies junge M\u00e4dchen,", "tokens": ["Dies", "jun\u00b7ge", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Das jetzt mein ganzes Herz", "tokens": ["Das", "jetzt", "mein", "gan\u00b7zes", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Beherrschen soll, als K\u00f6nigin.", "tokens": ["Be\u00b7herr\u00b7schen", "soll", ",", "als", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Heil dir! du junge K\u00f6nigin!", "tokens": ["Heil", "dir", "!", "du", "jun\u00b7ge", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von der Sonne droben", "tokens": ["Von", "der", "Son\u00b7ne", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Rei\u00df' ich das strahlend rothe Gold,", "tokens": ["Rei\u00df'", "ich", "das", "strah\u00b7lend", "ro\u00b7the", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und webe d'raus ein Diadem", "tokens": ["Und", "we\u00b7be", "d'raus", "ein", "Di\u00b7a\u00b7dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00fcr dein geweihtes Haupt,", "tokens": ["F\u00fcr", "dein", "ge\u00b7weih\u00b7tes", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von der flatternd blauseid'nen Himmelsdecke,", "tokens": ["Von", "der", "flat\u00b7ternd", "blaus\u00b7ei\u00b7d'\u00b7nen", "Him\u00b7mels\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Worin die Nachtdiamanten blitzen,", "tokens": ["Wo\u00b7rin", "die", "Nacht\u00b7di\u00b7a\u00b7man\u00b7ten", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schneid' ich ein kostbar St\u00fcck,", "tokens": ["Schneid'", "ich", "ein", "kost\u00b7bar", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und h\u00e4ng' es dir, als Kr\u00f6nungsmantel,", "tokens": ["Und", "h\u00e4ng'", "es", "dir", ",", "als", "Kr\u00f6\u00b7nungs\u00b7man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Um deine k\u00f6nigliche Schulter.", "tokens": ["Um", "dei\u00b7ne", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Schul\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich gebe dir einen Hofstaat", "tokens": ["Ich", "ge\u00b7be", "dir", "ei\u00b7nen", "Hof\u00b7staat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Von steifgeputzten Sonetten,", "tokens": ["Von", "steif\u00b7ge\u00b7putz\u00b7ten", "So\u00b7net\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Stolzen Terzinen und h\u00f6flichen Stanzen;", "tokens": ["Stol\u00b7zen", "Ter\u00b7zi\u00b7nen", "und", "h\u00f6f\u00b7li\u00b7chen", "Stan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Als L\u00e4ufer diene dir mein Witz,", "tokens": ["Als", "L\u00e4u\u00b7fer", "die\u00b7ne", "dir", "mein", "Witz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Als Hofnarr meine Phantasie,", "tokens": ["Als", "Hof\u00b7narr", "mei\u00b7ne", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Als Herold die lachende Thr\u00e4ne im Wappen,", "tokens": ["Als", "He\u00b7rold", "die", "la\u00b7chen\u00b7de", "Thr\u00e4\u00b7ne", "im", "Wap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.12": {"text": "Diene dir mein Humor.", "tokens": ["Die\u00b7ne", "dir", "mein", "Hu\u00b7mor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Aber ich selber, K\u00f6nigin,", "tokens": ["A\u00b7ber", "ich", "sel\u00b7ber", ",", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Und huld'gend, auf rothem Sammetkissen,", "tokens": ["Und", "huld'\u00b7gend", ",", "auf", "ro\u00b7them", "Sam\u00b7met\u00b7kis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Ueberreiche ich Dir", "tokens": ["Ue\u00b7ber\u00b7rei\u00b7che", "ich", "Dir"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "PPER"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Das bischen Verstand,", "tokens": ["Das", "bi\u00b7schen", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Das mir aus Mitleid noch gelassen hat", "tokens": ["Das", "mir", "aus", "Mit\u00b7leid", "noch", "ge\u00b7las\u00b7sen", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "NN", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Deine Vorg\u00e4ngerin im Reich.", "tokens": ["Dei\u00b7ne", "Vor\u00b7g\u00e4n\u00b7ge\u00b7rin", "im", "Reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}}}}}