{"textgrid.poem.37754": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ja w\u00fc\u00dft' ein Mensch recht, wer er w\u00e4r'", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja w\u00fc\u00dft' ein Mensch recht, wer er w\u00e4r',", "tokens": ["Ja", "w\u00fc\u00dft'", "ein", "Mensch", "recht", ",", "wer", "er", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ART", "NN", "ADJD", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Sterben w\u00fcrd ihm gar nicht schwer;", "tokens": ["Das", "Ster\u00b7ben", "w\u00fcrd", "ihm", "gar", "nicht", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Leben ist nur ein Vergessen", "tokens": ["Das", "Le\u00b7ben", "ist", "nur", "ein", "Ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von dem, was wir in uns besessen;", "tokens": ["Von", "dem", ",", "was", "wir", "in", "uns", "be\u00b7ses\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Leben ist nur ein Verm\u00e4hlen", "tokens": ["Das", "Le\u00b7ben", "ist", "nur", "ein", "Ver\u00b7m\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dem, was uns will ewig qu\u00e4len;", "tokens": ["Mit", "dem", ",", "was", "uns", "will", "e\u00b7wig", "qu\u00e4\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Leben ist ein Angew\u00f6hnen", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "An\u00b7ge\u00b7w\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "An das, was uns will ewig h\u00f6hnen;", "tokens": ["An", "das", ",", "was", "uns", "will", "e\u00b7wig", "h\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das Leben ist ein Zeitverderben,", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "Zeit\u00b7ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ein seelent\u00f6dtend Flucherwerben, \u2013", "tokens": ["Ein", "see\u00b7len\u00b7t\u00f6d\u00b7tend", "Fluc\u00b7her\u00b7wer\u00b7ben", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ja, w\u00fc\u00dft ein Mensch recht, wo er w\u00e4r',", "tokens": ["Ja", ",", "w\u00fc\u00dft", "ein", "Mensch", "recht", ",", "wo", "er", "w\u00e4r'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "w