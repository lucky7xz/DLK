{"dta.poem.20732": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Denselben .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Milon, gestern war ich selig,               ", "tokens": ["Mi\u00b7lon", ",", "ge\u00b7stern", "war", "ich", "se\u00b7lig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie ein Sonnenb\u00fcrger ist:", "tokens": ["Wie", "ein", "Son\u00b7nen\u00b7b\u00fcr\u00b7ger", "ist", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach mein Auge hat unz\u00e4hlig", "tokens": ["Ach", "mein", "Au\u00b7ge", "hat", "un\u00b7z\u00e4h\u00b7lig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese Stirne sanft gek\u00fc\u00dft,", "tokens": ["Die\u00b7se", "Stir\u00b7ne", "sanft", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die der Mahler kaum so g\u00f6ttlich", "tokens": ["Die", "der", "Mah\u00b7ler", "kaum", "so", "g\u00f6tt\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mahlen wird, als du sie hast.", "tokens": ["Mah\u00b7len", "wird", ",", "als", "du", "sie", "hast", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mache mir doch k\u00fcnftig sp\u00f6ttlich", "tokens": ["Ma\u00b7che", "mir", "doch", "k\u00fcnf\u00b7tig", "sp\u00f6tt\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht die Tage mehr zur Last \u2014", "tokens": ["Nicht", "die", "Ta\u00b7ge", "mehr", "zur", "Last"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADV", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "O was hab ich ausgestanden,", "tokens": ["O", "was", "hab", "ich", "aus\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Als Zemire ward gespielt,", "tokens": ["Als", "Ze\u00b7mi\u00b7re", "ward", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Und mich deine Blicke fanden,", "tokens": ["Und", "mich", "dei\u00b7ne", "Bli\u00b7cke", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und ich nicht den Trost erhielt,", "tokens": ["Und", "ich", "nicht", "den", "Trost", "er\u00b7hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Da\u00df du in der N\u00e4he bliebest.", "tokens": ["Da\u00df", "du", "in", "der", "N\u00e4\u00b7he", "blie\u00b7best", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sage mir, warum du so", "tokens": ["Sa\u00b7ge", "mir", ",", "wa\u00b7rum", "du", "so"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PWAV", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Meiner Seele Kummer liebest?", "tokens": ["Mei\u00b7ner", "See\u00b7le", "Kum\u00b7mer", "lie\u00b7best", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sprich, warum dein Fu\u00df entfloh,", "tokens": ["Sprich", ",", "wa\u00b7rum", "dein", "Fu\u00df", "ent\u00b7floh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Da\u00df ich deiner vollen Schl\u00e4fe", "tokens": ["Da\u00df", "ich", "dei\u00b7ner", "vol\u00b7len", "Schl\u00e4\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Feine Locken nicht mehr sah?", "tokens": ["Fei\u00b7ne", "Lo\u00b7cken", "nicht", "mehr", "sah", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Denke nur, wie mir geschah,", "tokens": ["Den\u00b7ke", "nur", ",", "wie", "mir", "ge\u00b7schah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.20": {"text": "Fast als ob ein Blitz mich tr\u00e4fe,", "tokens": ["Fast", "als", "ob", "ein", "Blitz", "mich", "tr\u00e4\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Weinen wollt ich eine Fluth,", "tokens": ["Wei\u00b7nen", "wollt", "ich", "ei\u00b7ne", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Durfte nicht und musts ersticken,", "tokens": ["Durf\u00b7te", "nicht", "und", "musts", "er\u00b7sti\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "KON", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Schmerz durchflammete mein Blut,", "tokens": ["Schmerz", "durch\u00b7flam\u00b7me\u00b7te", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Wehmuth sa\u00df in meinen Blicken,", "tokens": ["Weh\u00b7muth", "sa\u00df", "in", "mei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Bis Zemirens Rose kam,", "tokens": ["Bis", "Ze\u00b7mi\u00b7rens", "Ro\u00b7se", "kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Und ich meine Rosen dachte,", "tokens": ["Und", "ich", "mei\u00b7ne", "Ro\u00b7sen", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Und der gar zu schwere Gram", "tokens": ["Und", "der", "gar", "zu", "schwe\u00b7re", "Gram"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Sich durch Thr\u00e4nen leichter machte.", "tokens": ["Sich", "durch", "Thr\u00e4\u00b7nen", "leich\u00b7ter", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}