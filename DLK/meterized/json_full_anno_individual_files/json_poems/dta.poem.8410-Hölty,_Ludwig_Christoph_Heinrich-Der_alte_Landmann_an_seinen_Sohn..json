{"dta.poem.8410": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der alte  \n Landmann an seinen Sohn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Bis an dein k\u00fchles Grab,", "tokens": ["Bis", "an", "dein", "k\u00fch\u00b7les", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und weiche keinen Finger breit", "tokens": ["Und", "wei\u00b7che", "kei\u00b7nen", "Fin\u00b7ger", "breit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Gottes Wegen ab!", "tokens": ["Von", "Got\u00b7tes", "We\u00b7gen", "ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dann wirst du, wie auf gr\u00fcnen Aun,", "tokens": ["Dann", "wirst", "du", ",", "wie", "auf", "gr\u00fc\u00b7nen", "Aun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durchs Pilgerleben gehn;", "tokens": ["Durchs", "Pil\u00b7ger\u00b7le\u00b7ben", "gehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dann kannst du sonder Furcht und Graun", "tokens": ["Dann", "kannst", "du", "son\u00b7der", "Furcht", "und", "Graun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Tod' entgegen sehn.", "tokens": ["Dem", "Tod'", "ent\u00b7ge\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dann wird die Sichel und der Pflug", "tokens": ["Dann", "wird", "die", "Si\u00b7chel", "und", "der", "Pflug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deiner Hand so leicht;", "tokens": ["In", "dei\u00b7ner", "Hand", "so", "leicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann singest du beim Wasserkrug,", "tokens": ["Dann", "sin\u00b7gest", "du", "beim", "Was\u00b7ser\u00b7krug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00e4r dir Wein gereicht.", "tokens": ["Als", "w\u00e4r", "dir", "Wein", "ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem B\u00f6sewicht wird alles schwer,", "tokens": ["Dem", "B\u00f6\u00b7se\u00b7wicht", "wird", "al\u00b7les", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er thue was er thu;", "tokens": ["Er", "thue", "was", "er", "thu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Der Teufel treibt ihn hin und her,", "tokens": ["Der", "Teu\u00b7fel", "treibt", "ihn", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und l\u00e4sst ihm keine Ruh.", "tokens": ["Und", "l\u00e4sst", "ihm", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der sch\u00f6ne Fr\u00fchling lacht ihm nicht,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Fr\u00fch\u00b7ling", "lacht", "ihm", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm lacht kein Aehrenfeld;", "tokens": ["Ihm", "lacht", "kein", "A\u00b7eh\u00b7ren\u00b7feld", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er ist auf Lug und Trug erpicht,", "tokens": ["Er", "ist", "auf", "Lug", "und", "Trug", "er\u00b7picht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcnscht sich nichts als Geld.", "tokens": ["Und", "w\u00fcnscht", "sich", "nichts", "als", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Wind im Hain, das Laub am Baum,", "tokens": ["Der", "Wind", "im", "Hain", ",", "das", "Laub", "am", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Saust ihm Entsezen zu;", "tokens": ["Saust", "ihm", "Ent\u00b7se\u00b7zen", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Er findet, nach des Lebens Raum,", "tokens": ["Er", "fin\u00b7det", ",", "nach", "des", "Le\u00b7bens", "Raum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Grabe keine Ruh.", "tokens": ["Im", "Gra\u00b7be", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann muss er in der Geisterstund'", "tokens": ["Dann", "muss", "er", "in", "der", "Geis\u00b7ter\u00b7stund'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus seinem Grabe gehn,", "tokens": ["Aus", "sei\u00b7nem", "Gra\u00b7be", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und oft als schwarzer Kettenhund", "tokens": ["Und", "oft", "als", "schwar\u00b7zer", "Ket\u00b7ten\u00b7hund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor seiner Hausth\u00fcr stehn.", "tokens": ["Vor", "sei\u00b7ner", "Hau\u00b7sth\u00fcr", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Spinnerinnen, die, das Rad", "tokens": ["Die", "Spin\u00b7ne\u00b7rin\u00b7nen", ",", "die", ",", "das", "Rad"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Arm, nach Hause gehn,", "tokens": ["Im", "Arm", ",", "nach", "Hau\u00b7se", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erzittern wie ein Espenblatt,", "tokens": ["Er\u00b7zit\u00b7tern", "wie", "ein", "Es\u00b7pen\u00b7blatt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn sie ihn liegen sehn.", "tokens": ["Wenn", "sie", "ihn", "lie\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und jede Spinnestube spricht", "tokens": ["Und", "je\u00b7de", "Spin\u00b7ne\u00b7stu\u00b7be", "spricht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von diesem Abentheur,", "tokens": ["Von", "die\u00b7sem", "A\u00b7bent\u00b7heur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00fcnscht den todten B\u00f6sewicht", "tokens": ["Und", "w\u00fcnscht", "den", "tod\u00b7ten", "B\u00f6\u00b7se\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins tiefste H\u00f6llenfeur.", "tokens": ["Ins", "tiefs\u00b7te", "H\u00f6l\u00b7len\u00b7feur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der alte Kunz war bis ans Grab", "tokens": ["Der", "al\u00b7te", "Kunz", "war", "bis", "ans", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein rechter H\u00f6llenbrand:", "tokens": ["Ein", "rech\u00b7ter", "H\u00f6l\u00b7len\u00b7brand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er pfl\u00fcgte seinem Nachbar ab,", "tokens": ["Er", "pfl\u00fcg\u00b7te", "sei\u00b7nem", "Nach\u00b7bar", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und stahl ihm vieles Land.", "tokens": ["Und", "stahl", "ihm", "vie\u00b7les", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun pfl\u00fcgt er, als ein Feuermann,", "tokens": ["Nun", "pfl\u00fcgt", "er", ",", "als", "ein", "Feu\u00b7er\u00b7mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seines Nachbarn Flur,", "tokens": ["Auf", "sei\u00b7nes", "Nach\u00b7barn", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und misst das Feld hinab hinan", "tokens": ["Und", "misst", "das", "Feld", "hin\u00b7ab", "hi\u00b7nan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einer gl\u00fchnden Schnur.", "tokens": ["Mit", "ei\u00b7ner", "gl\u00fchn\u00b7den", "Schnur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er brennet, wie ein Schober Stroh,", "tokens": ["Er", "bren\u00b7net", ",", "wie", "ein", "Scho\u00b7ber", "Stroh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem gl\u00fchnden Pfluge nach,", "tokens": ["Dem", "gl\u00fchn\u00b7den", "Pflu\u00b7ge", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und pfl\u00fcgt, und brennet lichterloh", "tokens": ["Und", "pfl\u00fcgt", ",", "und", "bren\u00b7net", "lich\u00b7ter\u00b7loh"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis an den hellen Tag.", "tokens": ["Bis", "an", "den", "hel\u00b7len", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Amtmann, der die Bauern schund,", "tokens": ["Der", "Amt\u00b7mann", ",", "der", "die", "Bau\u00b7ern", "schund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hurt', und Hirsche schoss,", "tokens": ["Und", "hurt'", ",", "und", "Hir\u00b7sche", "schoss", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trabt Nachts mit einem schwarzen Hund", "tokens": ["Trabt", "Nachts", "mit", "ei\u00b7nem", "schwar\u00b7zen", "Hund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Wald' auf gl\u00fchndem Ross.", "tokens": ["Im", "Wald'", "auf", "gl\u00fchn\u00b7dem", "Ross", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Oft geht er auch am Knotenstock", "tokens": ["Oft", "geht", "er", "auch", "am", "Kno\u00b7ten\u00b7stock"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als rauher Brummb\u00e4r um,", "tokens": ["Als", "rau\u00b7her", "Brumm\u00b7b\u00e4r", "um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und meckert oft als Ziegenbock", "tokens": ["Und", "me\u00b7ckert", "oft", "als", "Zie\u00b7gen\u00b7bock"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im ganzen Dorf herum.", "tokens": ["Im", "gan\u00b7zen", "Dorf", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Pfarrer, der aufs Tanzen schalt,", "tokens": ["Der", "Pfar\u00b7rer", ",", "der", "aufs", "Tan\u00b7zen", "schalt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Filz und Wuchrer war,", "tokens": ["Und", "Filz", "und", "Wuch\u00b7rer", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steht Nachts als schwarze Spukgestalt", "tokens": ["Steht", "Nachts", "als", "schwar\u00b7ze", "Spuk\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um zw\u00f6lf Uhr am Altar;", "tokens": ["Um", "zw\u00f6lf", "Uhr", "am", "Al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPRART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Paukt dann mit dumpfigem Geschrei", "tokens": ["Paukt", "dann", "mit", "dump\u00b7fi\u00b7gem", "Ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "