{"textgrid.poem.66526": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es sangen drei Engel einen s\u00fc\u00dfen Gesang,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sangen drei Engel einen s\u00fc\u00dfen Gesang,", "tokens": ["Es", "san\u00b7gen", "drei", "En\u00b7gel", "ei\u00b7nen", "s\u00fc\u00b7\u00dfen", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie sangen, da\u00df es weit durch den Himmel erklang.", "tokens": ["Sie", "san\u00b7gen", ",", "da\u00df", "es", "weit", "durch", "den", "Him\u00b7mel", "er\u00b7klang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und als sie kamen zu der himmlischen Th\u00fcr,", "tokens": ["Und", "als", "sie", "ka\u00b7men", "zu", "der", "himm\u00b7li\u00b7schen", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Da stand eine arme Seele gar traurig daf\u00fcr.", "tokens": ["Da", "stand", "ei\u00b7ne", "ar\u00b7me", "See\u00b7le", "gar", "trau\u00b7rig", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "PAV", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Arme Seele, arme Seele, was dr\u00fcckt dich f\u00fcr Noth?", "tokens": ["Ar\u00b7me", "See\u00b7le", ",", "ar\u00b7me", "See\u00b7le", ",", "was", "dr\u00fcckt", "dich", "f\u00fcr", "Noth", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,", "PWS", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Hast du dich vergangen wider Gottes Gebot?", "tokens": ["Hast", "du", "dich", "ver\u00b7gan\u00b7gen", "wi\u00b7der", "Got\u00b7tes", "Ge\u00b7bot", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVPP", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.4": {"line.1": {"text": "\u00bbich hab nichts begangen, bin doch gar beschwert,", "tokens": ["\u00bb", "ich", "hab", "nichts", "be\u00b7gan\u00b7gen", ",", "bin", "doch", "gar", "be\u00b7schwert", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIS", "VVPP", "$,", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Dieweil sich meine Freud hat in Unmuth verkehrt.\u00ab", "tokens": ["Die\u00b7weil", "sich", "mei\u00b7ne", "Freud", "hat", "in", "Un\u00b7muth", "ver\u00b7kehrt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Arme Seele, arme Seele, du bist noch zu jung,", "tokens": ["Ar\u00b7me", "See\u00b7le", ",", "ar\u00b7me", "See\u00b7le", ",", "du", "bist", "noch", "zu", "jung", ","], "t