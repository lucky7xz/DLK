{"dta.poem.306": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihm eine Nacht-Lust verheissen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Selige Stunden", "tokens": ["Se\u00b7li\u00b7ge", "Stun\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da ich gefunden", "tokens": ["Da", "ich", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Was mich vergn\u00fcgen kan/", "tokens": ["Was", "mich", "ver\u00b7gn\u00fc\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dunckele Schatten/", "tokens": ["Dun\u00b7cke\u00b7le", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Decket die Matten/", "tokens": ["De\u00b7cket", "die", "Mat\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und z\u00fcndt die Sternen an.", "tokens": ["Und", "z\u00fcndt", "die", "Ster\u00b7nen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bleiche ", "tokens": ["Blei\u00b7che"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Schwinge die Fahne", "tokens": ["Schwin\u00b7ge", "die", "Fah\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Des hellen Silbers aus.", "tokens": ["Des", "hel\u00b7len", "Sil\u00b7bers", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Heisse ", "tokens": ["Heis\u00b7se"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wehle zum Throne", "tokens": ["Weh\u00b7le", "zum", "Thro\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dir meiner Liebsten Haus.", "tokens": ["Dir", "mei\u00b7ner", "Liebs\u00b7ten", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "F\u00fchre die Flammen", "tokens": ["F\u00fch\u00b7re", "die", "Flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Eros zusammen", "tokens": ["E\u00b7ros", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word"], "pos": ["NE", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "In ihrer ", "tokens": ["In", "ih\u00b7rer"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "F\u00fclle die Br\u00fcste", "tokens": ["F\u00fcl\u00b7le", "die", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mit Most der L\u00fcste", "tokens": ["Mit", "Most", "der", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Aus dem gestirnten Schlo\u00df.", "tokens": ["Aus", "dem", "ge\u00b7stirn\u00b7ten", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Netze der Lippen", "tokens": ["Net\u00b7ze", "der", "Lip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Rubinen Klippen", "tokens": ["Ru\u00b7bi\u00b7nen", "Klip\u00b7pen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "---+-", "measure": "unknown.measure.single"}, "line.3": {"text": "Mit feuchten ", "tokens": ["Mit", "feuch\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "La\u00df ihre Wangen", "tokens": ["La\u00df", "ih\u00b7re", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Bepurpert prangen", "tokens": ["Be\u00b7pur\u00b7pert", "pran\u00b7gen"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Lasse die Fl\u00fcsse", "tokens": ["Las\u00b7se", "die", "Fl\u00fcs\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ambrirter K\u00fcsse", "tokens": ["Am\u00b7br\u00b7ir\u00b7ter", "K\u00fcs\u00b7se"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mehr als \u00fcberfl\u00fc\u00dfig seyn.", "tokens": ["Mehr", "als", "\u00fc\u00b7berf\u00b7l\u00fc\u00b7\u00dfig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4uffe Vergn\u00fcgen", "tokens": ["H\u00e4uf\u00b7fe", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In Liebes-Siegen", "tokens": ["In", "Lie\u00b7bes\u00b7Sie\u00b7gen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}