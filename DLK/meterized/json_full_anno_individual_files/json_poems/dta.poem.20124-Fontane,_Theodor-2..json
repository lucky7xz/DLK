{"dta.poem.20124": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Der Herbst ist da; die Lust zu jagen", "tokens": ["Der", "Herbst", "ist", "da", ";", "die", "Lust", "zu", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lockt aus der Stadt nach Windsor-Schlo\u00df,", "tokens": ["Lockt", "aus", "der", "Stadt", "nach", "Wind\u00b7sor\u00b7Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und jetzt, vorbei an Heck und Hagen", "tokens": ["Und", "jetzt", ",", "vor\u00b7bei", "an", "Heck", "und", "Ha\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "APPR", "NE", "KON", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Braust Jacob und sein J\u00e4gertro\u00df.", "tokens": ["Braust", "Ja\u00b7cob", "und", "sein", "J\u00e4\u00b7ger\u00b7tro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Welch Leben das! die Rosse sch\u00e4umen,", "tokens": ["Welch", "Le\u00b7ben", "das", "!", "die", "Ros\u00b7se", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "$.", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Meute klafft, die Pfeife gellt,", "tokens": ["Die", "Meu\u00b7te", "klafft", ",", "die", "Pfei\u00b7fe", "gellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Wald erwacht aus seinen Tr\u00e4umen,", "tokens": ["Der", "Wald", "er\u00b7wacht", "aus", "sei\u00b7nen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und schauert, wenn ein Opfer f\u00e4llt.", "tokens": ["Und", "schau\u00b7ert", ",", "wenn", "ein", "Op\u00b7fer", "f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Schon dunkelt\u2019s; doch das Blutvergeuden", "tokens": ["Schon", "dun\u00b7kelt's", ";", "doch", "das", "Blut\u00b7ver\u00b7geu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Es dauert fort bis in die Nacht,", "tokens": ["Es", "dau\u00b7ert", "fort", "bis", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Bis D\u00fcrsten nach des Mahles Freuden,", "tokens": ["Bis", "D\u00fcrs\u00b7ten", "nach", "des", "Mah\u00b7les", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dem Durst nach Blut ein Ende macht.", "tokens": ["Dem", "Durst", "nach", "Blut", "ein", "En\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Heim ruft das Horn; bald in den R\u00e4umen", "tokens": ["Heim", "ruft", "das", "Horn", ";", "bald", "in", "den", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Schlosses l\u00e4rmt man beim Bankett,", "tokens": ["Des", "Schlos\u00b7ses", "l\u00e4rmt", "man", "beim", "Ban\u00b7kett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man zecht, und statt der Rosse Sch\u00e4umen,", "tokens": ["Man", "zecht", ",", "und", "statt", "der", "Ros\u00b7se", "Sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00e4umt Wein und Freude um die Wett:", "tokens": ["Sch\u00e4umt", "Wein", "und", "Freu\u00b7de", "um", "die", "Wett", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Toaste schallen hundertt\u00f6nig,", "tokens": ["Toas\u00b7te", "schal\u00b7len", "hun\u00b7dert\u00b7t\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Wein verschwistert Alt und Jung,", "tokens": ["Der", "Wein", "ver\u00b7schwis\u00b7tert", "Alt", "und", "Jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und l\u00fcstern bringt zuletzt der K\u00f6nig", "tokens": ["Und", "l\u00fcs\u00b7tern", "bringt", "zu\u00b7letzt", "der", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Damen seine Huldigung.", "tokens": ["Den", "Da\u00b7men", "sei\u00b7ne", "Hul\u00b7di\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "\u201edie Sch\u00f6nen hoch!\u201c Der trunkne Alte", "tokens": ["\u201e", "die", "Sch\u00f6\u00b7nen", "hoch", "!", "\u201c", "Der", "trunk\u00b7ne", "Al\u00b7te"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "ADJD", "$.", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Er ruft\u2019s, und blinzelt durch den Saal,", "tokens": ["Er", "ruft's", ",", "und", "blin\u00b7zelt", "durch", "den", "Saal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie aber, der sein Hoch erschallte,", "tokens": ["Sie", "a\u00b7ber", ",", "der", "sein", "Hoch", "er\u00b7schall\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Lady Essex fehlt beim Mahl.", "tokens": ["Die", "La\u00b7dy", "Es\u00b7sex", "fehlt", "beim", "Mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}