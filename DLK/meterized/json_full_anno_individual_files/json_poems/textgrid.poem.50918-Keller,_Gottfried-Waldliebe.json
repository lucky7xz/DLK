{"textgrid.poem.50918": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Waldliebe", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht den Schuft am Waldessaum", "tokens": ["Seht", "den", "Schuft", "am", "Wal\u00b7des\u00b7saum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit gewandten Schritten fliegend,", "tokens": ["Mit", "ge\u00b7wand\u00b7ten", "Schrit\u00b7ten", "flie\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den geraubten F\u00f6hrenbaum", "tokens": ["Den", "ge\u00b7raub\u00b7ten", "F\u00f6h\u00b7ren\u00b7baum"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der jungen Schulter wiegend!", "tokens": ["Auf", "der", "jun\u00b7gen", "Schul\u00b7ter", "wie\u00b7gend", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat die Axt, die er gestohlen,", "tokens": ["Hat", "die", "Axt", ",", "die", "er", "ge\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vornen in den Stamm geschwungen,", "tokens": ["Vor\u00b7nen", "in", "den", "Stamm", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weit noch hinter seinen Sohlen", "tokens": ["Weit", "noch", "hin\u00b7ter", "sei\u00b7nen", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kommt der Wipfel nachgesprungen.", "tokens": ["Kommt", "der", "Wip\u00b7fel", "nach\u00b7ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie er heimlich lacht und singt,", "tokens": ["Wie", "er", "heim\u00b7lich", "lacht", "und", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df sein Herz im Leibe springt!", "tokens": ["Da\u00df", "sein", "Herz", "im", "Lei\u00b7be", "springt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die Dirne kommt daher", "tokens": ["Und", "die", "Dir\u00b7ne", "kommt", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit gestohlnen Birkenruten;", "tokens": ["Mit", "ge\u00b7stohl\u00b7nen", "Bir\u00b7ken\u00b7ru\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Arbeit, lang und schwer,", "tokens": ["Von", "der", "Ar\u00b7beit", ",", "lang", "und", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stehn die Wangen ihr in Gluten.", "tokens": ["Stehn", "die", "Wan\u00b7gen", "ihr", "in", "Glu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Bursche wirft die F\u00f6hre", "tokens": ["Und", "der", "Bur\u00b7sche", "wirft", "die", "F\u00f6h\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie 'ne Feder in den Graben,", "tokens": ["Wie", "'ne", "Fe\u00b7der", "in", "den", "Gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Rei\u00dft die Dirne nach, ich schw\u00f6re,", "tokens": ["Rei\u00dft", "die", "Dir\u00b7ne", "nach", ",", "ich", "schw\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df die was zusammen haben!", "tokens": ["Da\u00df", "die", "was", "zu\u00b7sam\u00b7men", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIS", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo ein kleiner Freudenquell", "tokens": ["Wo", "ein", "klei\u00b7ner", "Freu\u00b7den\u00b7quell"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief im Eschengrunde flie\u00dfet", "tokens": ["Tief", "im", "E\u00b7schen\u00b7grun\u00b7de", "flie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Silberadern hell", "tokens": ["Und", "die", "Sil\u00b7be\u00b7ra\u00b7dern", "hell"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch das samtne Moos ergie\u00dfet,", "tokens": ["Durch", "das", "samt\u00b7ne", "Moos", "er\u00b7gie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wirft der schlanke Dieb sich nieder", "tokens": ["Wirft", "der", "schlan\u00b7ke", "Dieb", "sich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PRF", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit der Dirn im braunen Arm,", "tokens": ["Mit", "der", "Dirn", "im", "brau\u00b7nen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "L\u00f6st ihr hastig Tuch und Mieder,", "tokens": ["L\u00f6st", "ihr", "has\u00b7tig", "Tuch", "und", "Mie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und er fl\u00fcstert liebewarm,", "tokens": ["Und", "er", "fl\u00fcs\u00b7tert", "lie\u00b7be\u00b7warm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df sein gl\u00fchend Herz erklingt,", "tokens": ["Da\u00df", "sein", "gl\u00fc\u00b7hend", "Herz", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie die Nu\u00df im Feuer singt:", "tokens": ["Wie", "die", "Nu\u00df", "im", "Feu\u00b7er", "singt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "\u00bbsch\u00e4tzchen, o du kommst mir just,", "tokens": ["\u00bb", "sch\u00e4tz\u00b7chen", ",", "o", "du", "kommst", "mir", "just", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "FM", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich meine Sch\u00e4tze grabe,", "tokens": ["Da\u00df", "ich", "mei\u00b7ne", "Sch\u00e4t\u00b7ze", "gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder einmal meine Lust", "tokens": ["Wie\u00b7der", "ein\u00b7mal", "mei\u00b7ne", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Am verborgnen Reichtum habe!", "tokens": ["Am", "ver\u00b7borg\u00b7nen", "Reich\u00b7tum", "ha\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich pr\u00fcfe die Juwele:", "tokens": ["Da\u00df", "ich", "pr\u00fc\u00b7fe", "die", "Ju\u00b7we\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine \u00c4ugelein voll Feuer!", "tokens": ["Dei\u00b7ne", "\u00c4u\u00b7ge\u00b7lein", "voll", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich meine Perlen z\u00e4hle,", "tokens": ["Da\u00df", "ich", "mei\u00b7ne", "Per\u00b7len", "z\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deine Z\u00e4hne blank und teuer!", "tokens": ["Dei\u00b7ne", "Z\u00e4h\u00b7ne", "blank", "und", "teu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zeig mir der Korallen Schein", "tokens": ["Zeig", "mir", "der", "Ko\u00b7ral\u00b7len", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem frischen, s\u00fc\u00dfen Munde,", "tokens": ["An", "dem", "fri\u00b7schen", ",", "s\u00fc\u00b7\u00dfen", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gib mir schnell mein Elfenbein,", "tokens": ["Gib", "mir", "schnell", "mein", "El\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "All das feingedrehte runde!", "tokens": ["All", "das", "fein\u00b7ge\u00b7dreh\u00b7te", "run\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gib mir meine Silberberge,", "tokens": ["Gib", "mir", "mei\u00b7ne", "Sil\u00b7ber\u00b7ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die mich wei\u00df und selig blenden,", "tokens": ["Die", "mich", "wei\u00df", "und", "se\u00b7lig", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Drin die tausend Liebeszwerge", "tokens": ["Drin", "die", "tau\u00b7send", "Lie\u00b7bes\u00b7zwer\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Pochen mit den kleinen H\u00e4nden!\u00ab", "tokens": ["Po\u00b7chen", "mit", "den", "klei\u00b7nen", "H\u00e4n\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie ein Has im Kohle springt", "tokens": ["Wie", "ein", "Has", "im", "Koh\u00b7le", "springt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ihm das Herz und singt und klingt!", "tokens": ["Ihm", "das", "Herz", "und", "singt", "und", "klingt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbla\u00df mich w\u00e4gen all mein Gold:", "tokens": ["\u00bb", "la\u00df", "mich", "w\u00e4\u00b7gen", "all", "mein", "Gold", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "VVFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Haares schwere G\u00fcsse!", "tokens": ["Dei\u00b7nes", "Haa\u00b7res", "schwe\u00b7re", "G\u00fcs\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich z\u00e4hlen meinen Sold:", "tokens": ["La\u00df", "mich", "z\u00e4h\u00b7len", "mei\u00b7nen", "Sold", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00e4hle mir ein Hundert K\u00fcsse", "tokens": ["Z\u00e4h\u00b7le", "mir", "ein", "Hun\u00b7dert", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Blank und bar auf meine Lippen,", "tokens": ["Blank", "und", "bar", "auf", "mei\u00b7ne", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil uns kein Verr\u00e4ter lauschet!", "tokens": ["Weil", "uns", "kein", "Ver\u00b7r\u00e4\u00b7ter", "lau\u00b7schet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df mich von dem Weine nippen,", "tokens": ["La\u00df", "mich", "von", "dem", "Wei\u00b7ne", "nip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der mich armen Schelm berauschet!", "tokens": ["Der", "mich", "ar\u00b7men", "Schelm", "be\u00b7rau\u00b7schet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nun verh\u00fcll die Herrlichkeit", "tokens": ["Nun", "ver\u00b7h\u00fcll", "die", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Lumpen, mit den Fetzen,", "tokens": ["Mit", "den", "Lum\u00b7pen", ",", "mit", "den", "Fet\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df kein Auge, ungeweiht,", "tokens": ["Da\u00df", "kein", "Au\u00b7ge", ",", "un\u00b7ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sp\u00e4hen kann nach meinen Sch\u00e4tzen!", "tokens": ["Sp\u00e4\u00b7hen", "kann", "nach", "mei\u00b7nen", "Sch\u00e4t\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Tuch um deine Haare", "tokens": ["Die\u00b7ses", "Tuch", "um", "dei\u00b7ne", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dreimal, viermal sorglich winde,", "tokens": ["Drei\u00b7mal", ",", "vier\u00b7mal", "sorg\u00b7lich", "win\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df die goldne Schimmerware", "tokens": ["Da\u00df", "die", "gold\u00b7ne", "Schim\u00b7mer\u00b7wa\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ja kein Strahl der Sonne finde!\u00ab", "tokens": ["Ja", "kein", "Strahl", "der", "Son\u00b7ne", "fin\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "PIAT", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und die Dirne ist davon", "tokens": ["Und", "die", "Dir\u00b7ne", "ist", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den dunklen Wald gesprungen;", "tokens": ["Durch", "den", "dunk\u00b7len", "Wald", "ge\u00b7sprun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder hat der Bursche schon", "tokens": ["Wie\u00b7der", "hat", "der", "Bur\u00b7sche", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine F\u00f6hre aufgeschwungen.", "tokens": ["Sei\u00b7ne", "F\u00f6h\u00b7re", "auf\u00b7ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ihn schnell die Beine tragen", "tokens": ["Wie", "ihn", "schnell", "die", "Bei\u00b7ne", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem schwanken, langen Raube!", "tokens": ["Mit", "dem", "schwan\u00b7ken", ",", "lan\u00b7gen", "Rau\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Einen gr\u00fcnen Siegeswagen,", "tokens": ["Ei\u00b7nen", "gr\u00fc\u00b7nen", "Sie\u00b7ges\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schleift die Krone er im Staube.", "tokens": ["Schleift", "die", "Kro\u00b7ne", "er", "im", "Stau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und vor innerm Lachen springt", "tokens": ["Und", "vor", "in\u00b7nerm", "La\u00b7chen", "springt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Ihm das Herz und singt und klingt!", "tokens": ["Ihm", "das", "Herz", "und", "singt", "und", "klingt", "!"], "token_