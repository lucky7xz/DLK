er seinen Sohlen", "tokens": ["Weit", "noch", "hin\u00b7ter", "sei\u00b7nen", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kommt der Wipfel nachgesprungen.", "tokens": ["Kommt", "der", "Wip\u00b7fel", "nach\u00b7ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie er heimlich lacht und singt,", "tokens": ["Wie", "er", "heim\u00b7lich", "lacht", "und", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df sein Herz im Leibe springt!", "tokens": ["Da\u00df", "sein", "Herz", "im", "Lei\u00b7be", "springt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und die Dirne kommt daher", "tokens": ["Und", "die", "Dir\u00b7ne", "kommt", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit gestohlnen Birkenruten;", "tokens": ["Mit", "ge\u00b7stohl\u00b7nen", "Bir\u00b7ken\u00b7ru\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Arbeit, lang und schwer,", "tokens": ["Von", "der", "Ar\u00b7beit", ",", "lang", "und", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "AD