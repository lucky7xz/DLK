{"textgrid.poem.24265": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ostara", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ostara, die gute G\u00f6ttin,", "tokens": ["O\u00b7sta\u00b7ra", ",", "die", "gu\u00b7te", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die aus hellen Augen lacht,", "tokens": ["Die", "aus", "hel\u00b7len", "Au\u00b7gen", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df von ihrem jungen Lichte", "tokens": ["Da\u00df", "von", "ih\u00b7rem", "jun\u00b7gen", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles Schlafende erwacht,", "tokens": ["Al\u00b7les", "Schla\u00b7fen\u00b7de", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ostara, die fr\u00fchlingsfrische", "tokens": ["O\u00b7sta\u00b7ra", ",", "die", "fr\u00fch\u00b7lings\u00b7fri\u00b7sche"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Jungfrau G\u00f6ttin, deren Mund", "tokens": ["Jung\u00b7frau", "G\u00f6t\u00b7tin", ",", "de\u00b7ren", "Mund"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Duftet wie die ersten Veilchen,", "tokens": ["Duf\u00b7tet", "wie", "die", "ers\u00b7ten", "Veil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mildgew\u00fcrzig, herbgesund,", "tokens": ["Mild\u00b7ge\u00b7w\u00fcr\u00b7zig", ",", "herb\u00b7ge\u00b7sund", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ostara, die Ungest\u00fcme,", "tokens": ["O\u00b7sta\u00b7ra", ",", "die", "Un\u00b7ge\u00b7st\u00fc\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Liebevolle, die die Welt", "tokens": ["Lie\u00b7be\u00b7vol\u00b7le", ",", "die", "die", "Welt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Bund von Maienrosen", "tokens": ["Wie", "ein", "Bund", "von", "Mai\u00b7en\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An die vollen Br\u00fcste h\u00e4lt,", "tokens": ["An", "die", "vol\u00b7len", "Br\u00fcs\u00b7te", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ostara, die Magd und F\u00fcrstin,", "tokens": ["O\u00b7sta\u00b7ra", ",", "die", "Magd", "und", "F\u00fcrs\u00b7tin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "K\u00f6niglich und b\u00e4uerlich \u2013:", "tokens": ["K\u00f6\u00b7nig\u00b7lich", "und", "b\u00e4u\u00b7er\u00b7lich", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KON", "ADJD", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Zeiten sich auch wandeln,", "tokens": ["Wie", "die", "Zei\u00b7ten", "sich", "auch", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer offenbart sie sich.", "tokens": ["Im\u00b7mer", "of\u00b7fen\u00b7bart", "sie", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihre Opferherde sanken,", "tokens": ["Ih\u00b7re", "Op\u00b7fer\u00b7her\u00b7de", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als das Kreuz sich steil erhob,", "tokens": ["Als", "das", "Kreuz", "sich", "steil", "er\u00b7hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber jedes Fr\u00fchjahr rauschen", "tokens": ["A\u00b7ber", "je\u00b7des", "Fr\u00fch\u00b7jahr", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wald und Busch ihr Dank und Lob.", "tokens": ["Wald", "und", "Busch", "ihr", "Dank", "und", "Lob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Die in W\u00e4ldern gr\u00fcn sich kr\u00e4nzten,", "tokens": ["Die", "in", "W\u00e4l\u00b7dern", "gr\u00fcn", "sich", "kr\u00e4nz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, die Deutschen wurden grau,", "tokens": ["Ach", ",", "die", "Deut\u00b7schen", "wur\u00b7den", "grau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber hell geht durch das Gr\u00fcne", "tokens": ["A\u00b7ber", "hell", "geht", "durch", "das", "Gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch die fr\u00fchlingslichte Frau,", "tokens": ["Noch", "die", "fr\u00fch\u00b7lings\u00b7lich\u00b7te", "Frau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn die Urst\u00e4nd sich erneuern,", "tokens": ["Wenn", "die", "Ur\u00b7st\u00e4nd", "sich", "er\u00b7neu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das Leben auferwacht,", "tokens": ["Wenn", "das", "Le\u00b7ben", "au\u00b7fer\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn noch immer gibt es Herzen,", "tokens": ["Denn", "noch", "im\u00b7mer", "gibt", "es", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Fr\u00fchling gl\u00e4ubig macht,", "tokens": ["Die", "der", "Fr\u00fch\u00b7ling", "gl\u00e4u\u00b7big", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Gl\u00e4ubig zu den alten G\u00f6ttern,", "tokens": ["Gl\u00e4u\u00b7big", "zu", "den", "al\u00b7ten", "G\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die der deutsche Wald gebar,", "tokens": ["Die", "der", "deut\u00b7sche", "Wald", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als er noch ein Reich von freien,", "tokens": ["Als", "er", "noch", "ein", "Reich", "von", "frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heiter k\u00fchnen M\u00e4nnern war,", "tokens": ["Hei\u00b7ter", "k\u00fch\u00b7nen", "M\u00e4n\u00b7nern", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Die in Kampf und Liebe lachten,", "tokens": ["Die", "in", "Kampf", "und", "Lie\u00b7be", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Fest aufs Eigene gestellt,", "tokens": ["Fest", "aufs", "Ei\u00b7ge\u00b7ne", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Drob in Einfalt und in Treue,", "tokens": ["Drob", "in", "Ein\u00b7falt", "und", "in", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bildner einer eignen Welt", "tokens": ["Bild\u00b7ner", "ei\u00b7ner", "eig\u00b7nen", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Voller M\u00e4rchen und voll Taten,", "tokens": ["Vol\u00b7ler", "M\u00e4r\u00b7chen", "und", "voll", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00e4tselvoll und voller Licht.", "tokens": ["R\u00e4t\u00b7sel\u00b7voll", "und", "vol\u00b7ler", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Welt ist hingesunken,", "tokens": ["Die\u00b7se", "Welt", "ist", "hin\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber ihre Sch\u00f6nheit nicht.", "tokens": ["A\u00b7ber", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Was ein Volk aus seinem Herzen", "tokens": ["Was", "ein", "Volk", "aus", "sei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich zum Bild schuf und zur Lust,", "tokens": ["Sich", "zum", "Bild", "schuf", "und", "zur", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVFIN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feiert immer wieder Urst\u00e4nd", "tokens": ["Fei\u00b7ert", "im\u00b7mer", "wie\u00b7der", "Ur\u00b7st\u00e4nd"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Selbst in schwacher Enkel Brust.", "tokens": ["Selbst", "in", "schwa\u00b7cher", "En\u00b7kel", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und so sei in diesen Tagen", "tokens": ["Und", "so", "sei", "in", "die\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Voller Glanz uns Ostara,", "tokens": ["Vol\u00b7ler", "Glanz", "uns", "O\u00b7sta\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Die die V\u00e4ter uns gedichtet,", "tokens": ["Die", "die", "V\u00e4\u00b7ter", "uns", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Huldreich voller Gnaden nah.", "tokens": ["Huld\u00b7reich", "vol\u00b7ler", "Gna\u00b7den", "nah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ostara, die Ungest\u00fcme,", "tokens": ["O\u00b7sta\u00b7ra", ",", "die", "Un\u00b7ge\u00b7st\u00fc\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Liebevolle, die die Welt", "tokens": ["Lie\u00b7be\u00b7vol\u00b7le", ",", "die", "die", "Welt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Bund von Maienrosen", "tokens": ["Wie", "ein", "Bund", "von", "Mai\u00b7en\u00b7ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An die vollen Br\u00fcste h\u00e4lt.", "tokens": ["An", "die", "vol\u00b7len", "Br\u00fcs\u00b7te", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Ostara, die gute G\u00f6ttin,", "tokens": ["O\u00b7sta\u00b7ra", ",", "die", "gu\u00b7te", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "punct", "word", "word", "wo