{"textgrid.poem.25535": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lernt' ich wie die ", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lernt' ich wie die ", "tokens": ["Lernt'", "ich", "wie", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "ART"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wendet' ich, wie sie, zu dem mich hin,", "tokens": ["Wen\u00b7det'", "ich", ",", "wie", "sie", ",", "zu", "dem", "mich", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "$,", "APPR", "PRELS", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dessen Augen, in den Sinn", "tokens": ["Des\u00b7sen", "Au\u00b7gen", ",", "in", "den", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Worte, schneller dringen,", "tokens": ["Mei\u00b7ner", "Wor\u00b7te", ",", "schnel\u00b7ler", "drin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als ich mit der Zunge bin.", "tokens": ["Als", "ich", "mit", "der", "Zun\u00b7ge", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6nnt' ich wie die ", "tokens": ["K\u00f6nnt'", "ich", "wie", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "KOKOM", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wahrlich! Helden malt' ich nicht!", "tokens": ["Wahr\u00b7lich", "!", "Hel\u00b7den", "malt'", "ich", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Deine Freuden, deine Qualen,", "tokens": ["Dei\u00b7ne", "Freu\u00b7den", ",", "dei\u00b7ne", "Qua\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Liebste Liebe! wollt' ich malen,", "tokens": ["Liebs\u00b7te", "Lie\u00b7be", "!", "wollt'", "ich", "ma\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und dein freundliches Gesicht,", "tokens": ["Und", "dein", "freund\u00b7li\u00b7ches", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Bester! sollte mich bezahlen.", "tokens": ["Bes\u00b7ter", "!", "soll\u00b7te", "mich", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Lernt' ich dich, o liebste Liebe!", "tokens": ["Lernt'", "ich", "dich", ",", "o", "liebs\u00b7te", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "So b