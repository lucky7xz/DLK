{"dta.poem.883": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "MeLANCHOLIE DES ABENDS", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "\u2014 Der Wald, der sich verstorben breitet \u2014", "tokens": ["Der", "Wald", ",", "der", "sich", "ver\u00b7stor\u00b7ben", "brei\u00b7tet"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Schatten sind um ihn, wie Hecken.", "tokens": ["Und", "Schat\u00b7ten", "sind", "um", "ihn", ",", "wie", "He\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "PPER", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Wild kommt zitternd aus Verstecken,", "tokens": ["Das", "Wild", "kommt", "zit\u00b7ternd", "aus", "Ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Indes ein Bach ganz leise gleitet", "tokens": ["In\u00b7des", "ein", "Bach", "ganz", "lei\u00b7se", "glei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und Farnen folgt und alten Steinen", "tokens": ["Und", "Far\u00b7nen", "folgt", "und", "al\u00b7ten", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und silbern gl\u00e4nzt aus Laubgewinden.", "tokens": ["Und", "sil\u00b7bern", "gl\u00e4nzt", "aus", "Laub\u00b7ge\u00b7win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man h\u00f6rt ihn bald in schwarzen Schl\u00fcnden \u2014", "tokens": ["Man", "h\u00f6rt", "ihn", "bald", "in", "schwar\u00b7zen", "Schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vielleicht, da\u00df auch schon Sterne scheinen.", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "auch", "schon", "Ster\u00b7ne", "schei\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der dunkle Plan scheint ohne Massen,", "tokens": ["Der", "dunk\u00b7le", "Plan", "scheint", "oh\u00b7ne", "Mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verstreute D\u00f6rfer, Sumpf und Weiher,", "tokens": ["Ver\u00b7streu\u00b7te", "D\u00f6r\u00b7fer", ",", "Sumpf", "und", "Wei\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und etwas t\u00e4uscht dir vor ein Feuer.", "tokens": ["Und", "et\u00b7was", "t\u00e4uscht", "dir", "vor", "ein", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein kalter Glanz huscht \u00fcber Stra\u00dfen.", "tokens": ["Ein", "kal\u00b7ter", "Glanz", "huscht", "\u00fc\u00b7ber", "Stra\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Am Himmel ahnet man Bewegung,", "tokens": ["Am", "Him\u00b7mel", "ah\u00b7net", "man", "Be\u00b7we\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Heer von wilden V\u00f6geln wandern", "tokens": ["Ein", "Heer", "von", "wil\u00b7den", "V\u00f6\u00b7geln", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nach jenen L\u00e4ndern, sch\u00f6nen, andern.", "tokens": ["Nach", "je\u00b7nen", "L\u00e4n\u00b7dern", ",", "sch\u00f6\u00b7nen", ",", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "VVFIN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es steigt und sinkt des Rohres Regung.", "tokens": ["Es", "steigt", "und", "sinkt", "des", "Roh\u00b7res", "Re\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}