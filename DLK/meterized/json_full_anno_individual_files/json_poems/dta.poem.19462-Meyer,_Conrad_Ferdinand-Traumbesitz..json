{"dta.poem.19462": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Traumbesitz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "\u201efremdling, unter diesem Schutte", "tokens": ["\u201e", "fremd\u00b7ling", ",", "un\u00b7ter", "die\u00b7sem", "Schut\u00b7te"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00f6lbt sich eine weite Halle,", "tokens": ["W\u00f6lbt", "sich", "ei\u00b7ne", "wei\u00b7te", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fcht des Inka goldner Garten,", "tokens": ["Bl\u00fcht", "des", "In\u00b7ka", "gold\u00b7ner", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Prangt der Sessel meines Ahns!", "tokens": ["Prangt", "der", "Ses\u00b7sel", "mei\u00b7nes", "Ahns", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles Laub und alle Fr\u00fcchte", "tokens": ["Al\u00b7les", "Laub", "und", "al\u00b7le", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die V\u00f6gel auf den Aesten", "tokens": ["Und", "die", "V\u00f6\u00b7gel", "auf", "den", "A\u00b7es\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+-+-+--", "measure": "anapaest.init"}, "line.3": {"text": "Und die Fischlein in den Teichen", "tokens": ["Und", "die", "Fisc\u00b7hlein", "in", "den", "Tei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind vom allerfeinsten Gold.\u201c", "tokens": ["Sind", "vom", "al\u00b7ler\u00b7feins\u00b7ten", "Gold", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u2014 \u201eKnabe, du bist zart und d\u00fcrftig,", "tokens": ["\u201e", "Kna\u00b7be", ",", "du", "bist", "zart", "und", "d\u00fcrf\u00b7tig", ","], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "NN", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine greisen Eltern darben \u2014", "tokens": ["Dei\u00b7ne", "grei\u00b7sen", "El\u00b7tern", "dar\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PAV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum gr\u00e4bst du nicht die nahen", "tokens": ["Wa\u00b7rum", "gr\u00e4bst", "du", "nicht", "die", "na\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00e4tze, die dein Erbe sind?\u201c", "tokens": ["Sch\u00e4t\u00b7ze", ",", "die", "dein", "Er\u00b7be", "sind", "?", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201esolches, Fremdling, w\u00e4re s\u00fcndlich!", "tokens": ["\u201e", "sol\u00b7ches", ",", "Fremd\u00b7ling", ",", "w\u00e4\u00b7re", "s\u00fcnd\u00b7lich", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PIS", "$,", "NN", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein, ich lasse mir gen\u00fcgen", "tokens": ["Nein", ",", "ich", "las\u00b7se", "mir", "ge\u00b7n\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An dem kleinen Waizenfelde,", "tokens": ["An", "dem", "klei\u00b7nen", "Wai\u00b7zen\u00b7fel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das mir oben \u00fcbrig blieb.", "tokens": ["Das", "mir", "o\u00b7ben", "\u00fcb\u00b7rig", "blieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Im Geheimni\u00df meines Herzens,", "tokens": ["Im", "Ge\u00b7heim\u00b7ni\u00df", "mei\u00b7nes", "Her\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Augen meines Geistes", "tokens": ["Mit", "den", "Au\u00b7gen", "mei\u00b7nes", "Geis\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwelg' ich in den lichten Wundern,", "tokens": ["Schwel\u00b7g'", "ich", "in", "den", "lich\u00b7ten", "Wun\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dem unermessnen Hort:", "tokens": ["In", "dem", "un\u00b7er\u00b7mess\u00b7nen", "Hort", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O des Glanzes! O der F\u00fclle!", "tokens": ["O", "des", "Glan\u00b7zes", "!", "O", "der", "F\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehst du dort die B\u00fcschel Maises", "tokens": ["Siehst", "du", "dort", "die", "B\u00fc\u00b7schel", "Mai\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den sch\u00f6n geformten Kolben?", "tokens": ["Mit", "den", "sch\u00f6n", "ge\u00b7form\u00b7ten", "Kol\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Siehst du dort den goldnen Thron?\u201c", "tokens": ["Siehst", "du", "dort", "den", "gold\u00b7nen", "Thron", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}