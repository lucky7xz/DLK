{"dta.poem.12723": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Cratinen.  \n J. G. M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist mit mir geschehn!", "tokens": ["Es", "ist", "mit", "mir", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein hertze mu\u00df vor gro\u00dfer gluth verbrennen,", "tokens": ["Mein", "hert\u00b7ze", "mu\u00df", "vor", "gro\u00b7\u00dfer", "gluth", "ver\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo du, Cratine! nicht wilst meine treu erkennen,", "tokens": ["Wo", "du", ",", "Cra\u00b7ti\u00b7ne", "!", "nicht", "wilst", "mei\u00b7ne", "treu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "NE", "$.", "PTKNEG", "VMFIN", "PPOSAT", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und lindern meine pein,", "tokens": ["Und", "lin\u00b7dern", "mei\u00b7ne", "pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Willt du denn grausam seyn?", "tokens": ["Willt", "du", "denn", "grau\u00b7sam", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Erwege, was der himmel thut,", "tokens": ["Er\u00b7we\u00b7ge", ",", "was", "der", "him\u00b7mel", "thut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie er mit schnee und ei\u00df die d\u00fcrren th\u00e4ler f\u00fcllet,", "tokens": ["Wie", "er", "mit", "schnee", "und", "ei\u00df", "die", "d\u00fcr\u00b7ren", "th\u00e4\u00b7ler", "f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ihren heissen brand und grosses lechzen stillet;", "tokens": ["Und", "ih\u00b7ren", "heis\u00b7sen", "brand", "und", "gros\u00b7ses", "lech\u00b7zen", "stil\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach k\u00fchle, sch\u00f6nes kind! doch auch so meine brust.", "tokens": ["Ach", "k\u00fch\u00b7le", ",", "sch\u00f6\u00b7nes", "kind", "!", "doch", "auch", "so", "mei\u00b7ne", "brust", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "ADJA", "NN", "$.", "ADV", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Erfrische meine matten sinnen,", "tokens": ["Er\u00b7fri\u00b7sche", "mei\u00b7ne", "mat\u00b7ten", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "La\u00df mich dein kaltes hertz gewinnen,", "tokens": ["La\u00df", "mich", "dein", "kal\u00b7tes", "hertz", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und geu\u00df der lippen thau auf meinen trocknen mund,", "tokens": ["Und", "geu\u00df", "der", "lip\u00b7pen", "thau", "auf", "mei\u00b7nen", "trock\u00b7nen", "mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So werd ich recht gesund.", "tokens": ["So", "werd", "ich", "recht", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Ich will die erde seyn, und dich als himmel sch\u00e4tzen,", "tokens": ["Ich", "will", "die", "er\u00b7de", "seyn", ",", "und", "dich", "als", "him\u00b7mel", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,", "KON", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nur must du, merck es wohl! desselben fl\u00e4che netzen.", "tokens": ["Nur", "must", "du", ",", "merck", "es", "wohl", "!", "des\u00b7sel\u00b7ben", "fl\u00e4\u00b7che", "net\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "$.", "PDS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}