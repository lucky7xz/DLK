{"textgrid.poem.37065": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann, Friedensbothe, der du das Paradies", "tokens": ["Wann", ",", "Frie\u00b7dens\u00b7bo\u00b7the", ",", "der", "du", "das", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "NE", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem m\u00fcden Erdenpilger entschlie\u00dfest, Tod,", "tokens": ["Dem", "m\u00fc\u00b7den", "Er\u00b7den\u00b7pil\u00b7ger", "ent\u00b7schlie\u00b7\u00dfest", ",", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wann f\u00fchrst du mich mit deinem goldnen", "tokens": ["Wann", "f\u00fchrst", "du", "mich", "mit", "dei\u00b7nem", "gold\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stabe gen Himmel, zu meiner Heymath?", "tokens": ["Sta\u00b7be", "gen", "Him\u00b7mel", ",", "zu", "mei\u00b7ner", "Hey\u00b7math", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "O Wa\u00dferblase, Leben, zerfleug nur bald!", "tokens": ["O", "Wa\u00b7\u00dfer\u00b7bla\u00b7se", ",", "Le\u00b7ben", ",", "zer\u00b7fleug", "nur", "bald", "!"], "token_info": ["word", "w