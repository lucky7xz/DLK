{"textgrid.poem.42999": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Das scheue Wort", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein scheues Wort.", "tokens": ["Es", "war", "ein", "scheu\u00b7es", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das war ausgesprochen", "tokens": ["Das", "war", "aus\u00b7ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und hatte sich sofort", "tokens": ["Und", "hat\u00b7te", "sich", "so\u00b7fort"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PRF", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Unter ein Sofa verkrochen.", "tokens": ["Un\u00b7ter", "ein", "So\u00b7fa", "ver\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Samstags, als Berta das Sofa klopfte,", "tokens": ["Sams\u00b7tags", ",", "als", "Ber\u00b7ta", "das", "So\u00b7fa", "klopf\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Flog es in das linke, verstopfte", "tokens": ["Flog", "es", "in", "das", "lin\u00b7ke", ",", "ver\u00b7stopf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "$,", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ohr von Berta. Von da aus entkam es.", "tokens": ["Ohr", "von", "Ber\u00b7ta", ".", "Von", "da", "aus", "ent\u00b7kam", "es", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$.", "APPR", "ADV", "APZR", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ein Windsto\u00df nahm es,", "tokens": ["Ein", "Wind\u00b7sto\u00df", "nahm", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Trug es weit und dann hoch empor.", "tokens": ["Trug", "es", "weit", "und", "dann", "hoch", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Wo es sich in das halbe, bange", "tokens": ["Wo", "es", "sich", "in", "das", "hal\u00b7be", ",", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "ADJA", "$,", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ged\u00e4chtnis eines Piloten verlor.", "tokens": ["Ge\u00b7d\u00e4cht\u00b7nis", "ei\u00b7nes", "Pi\u00b7lo\u00b7ten", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Fiel dann an einem Wiesenhange", "tokens": ["Fiel", "dann", "an", "ei\u00b7nem", "Wie\u00b7sen\u00b7han\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf eine umarmte Arbeiterin nieder,", "tokens": ["Auf", "ei\u00b7ne", "um\u00b7arm\u00b7te", "Ar\u00b7bei\u00b7te\u00b7rin", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+---+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Trocknete deren Augenlider.", "tokens": ["Trock\u00b7ne\u00b7te", "de\u00b7ren", "Au\u00b7gen\u00b7li\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRELAT", "NN", "$."], "meter": "+-+--+---", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Wobei ein Literat es erwischte", "tokens": ["Wo\u00b7bei", "ein", "Li\u00b7te\u00b7rat", "es", "er\u00b7wischte"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und, falsch belauscht,", "tokens": ["Und", ",", "falsch", "be\u00b7lauscht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Eitel aufgebauscht,", "tokens": ["Ei\u00b7tel", "auf\u00b7ge\u00b7bauscht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Mittags dann seichten Fressern auftischte.", "tokens": ["Mit\u00b7tags", "dann", "seich\u00b7ten", "Fres\u00b7sern", "auf\u00b7tischte", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und das arme, mi\u00dfbrauchte,", "tokens": ["Und", "das", "ar\u00b7me", ",", "mi\u00df\u00b7brauch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Zitternde scheue Wort", "tokens": ["Zit\u00b7tern\u00b7de", "scheu\u00b7e", "Wort"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wanderte weiter und tauchte", "tokens": ["Wan\u00b7der\u00b7te", "wei\u00b7ter", "und", "tauch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wieder auf, hier und dort.", "tokens": ["Wie\u00b7der", "auf", ",", "hier", "und", "dort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bis ein Dichter es sanft eintr\u00e4umte,", "tokens": ["Bis", "ein", "Dich\u00b7ter", "es", "sanft", "ein\u00b7tr\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ihm ein stilles Palais einr\u00e4umte. \u2013 \u2013", "tokens": ["Ihm", "ein", "stil\u00b7les", "Pa\u00b7lais", "ein\u00b7r\u00e4um\u00b7te", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$.", "$(", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Kam aber sehr bald ein Parodist", "tokens": ["Kam", "a\u00b7ber", "sehr", "bald", "ein", "Pa\u00b7ro\u00b7dist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit gesch\u00e4ftlich sicherem Blick,", "tokens": ["Mit", "ge\u00b7sch\u00e4ft\u00b7lich", "si\u00b7che\u00b7rem", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Tauchte das Wort mit Speichel und Mist", "tokens": ["Tauch\u00b7te", "das", "Wort", "mit", "Spei\u00b7chel", "und", "Mist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In einen Aufgu\u00df gestohlner Musik.", "tokens": ["In", "ei\u00b7nen", "Auf\u00b7gu\u00df", "ge\u00b7stohl\u00b7ner", "Mu\u00b7sik", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "So ward es publik.", "tokens": ["So", "ward", "es", "pub\u00b7lik", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "So wurde es volkst\u00fcmlich laut.", "tokens": ["So", "wur\u00b7de", "es", "volks\u00b7t\u00fcm\u00b7lich", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und doch nur sein \u00c4u\u00dferes, seine Haut,", "tokens": ["Und", "doch", "nur", "sein", "\u00c4u\u00b7\u00dfe\u00b7res", ",", "sei\u00b7ne", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das Klangliche und das Reimliche.", "tokens": ["Das", "Klang\u00b7li\u00b7che", "und", "das", "Reim\u00b7li\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Denn das Innerste, Heimliche", "tokens": ["Denn", "das", "In\u00b7ners\u00b7te", ",", "Heim\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "$,", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "An ihm war weder lauschend noch lesend", "tokens": ["An", "ihm", "war", "we\u00b7der", "lau\u00b7schend", "noch", "le\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "KON", "ADJD", "ADV", "ADJD"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Erreichbar, blieb \u00f6ffentlich abwesend.", "tokens": ["Er\u00b7reich\u00b7bar", ",", "blieb", "\u00f6f\u00b7fent\u00b7lich", "ab\u00b7we\u00b7send", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "ADJD", "VVPP", "$."], "meter": "--+-++-++-", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Es war ein scheues Wort.", "tokens": ["Es", "war", "ein", "scheu\u00b7es", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das war ausgesprochen", "tokens": ["Das", "war", "aus\u00b7ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und hatte sich sofort", "tokens": ["Und", "hat\u00b7te", "sich", "so\u00b7fort"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PRF", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Unter ein Sofa verkrochen.", "tokens": ["Un\u00b7ter", "ein", "So\u00b7fa", "ver\u00b7kro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "A