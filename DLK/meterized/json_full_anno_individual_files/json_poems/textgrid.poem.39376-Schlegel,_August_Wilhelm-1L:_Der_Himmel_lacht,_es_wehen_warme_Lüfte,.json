{"textgrid.poem.39376": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Himmel lacht, es wehen warme L\u00fcfte,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel lacht, es wehen warme L\u00fcfte,", "tokens": ["Der", "Him\u00b7mel", "lacht", ",", "es", "we\u00b7hen", "war\u00b7me", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Gauen bl\u00fch'n ringsum mit Wein und Korne.", "tokens": ["Die", "Gau\u00b7en", "bl\u00fch'n", "ring\u00b7sum", "mit", "Wein", "und", "Kor\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hier schirmen H\u00fcgel vor des Nordwinds Zorne", "tokens": ["Hier", "schir\u00b7men", "H\u00fc\u00b7gel", "vor", "des", "Nord\u00b7winds", "Zor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein kleines Thal voll frischer Wiesend\u00fcfte.", "tokens": ["Ein", "klei\u00b7nes", "Thal", "voll", "fri\u00b7scher", "Wie\u00b7sen\u00b7d\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und es ergie\u00dft der Schoo\u00df der k\u00fchlen Kl\u00fcfte", "tokens": ["Und", "es", "er\u00b7gie\u00dft", "der