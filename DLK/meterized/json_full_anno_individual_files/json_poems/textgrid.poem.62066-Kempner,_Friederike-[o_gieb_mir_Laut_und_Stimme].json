{"textgrid.poem.62066": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[o gieb mir Laut und Stimme]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O gieb mir Laut und Stimme,", "tokens": ["O", "gieb", "mir", "Laut", "und", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "APPR", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O gieb mir Wort und Sang,", "tokens": ["O", "gieb", "mir", "Wort", "und", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich ein Lied anstimme", "tokens": ["Da\u00df", "ich", "ein", "Lied", "an\u00b7stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr Dich zum Lobgesang.", "tokens": ["F\u00fcr", "Dich", "zum", "Lob\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00df mich Dein Geist durchdringen,", "tokens": ["La\u00df", "mich", "Dein", "Geist", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein hoher Gottesgeist,", "tokens": ["Dein", "ho\u00b7her", "Got\u00b7tes\u00b7geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will's den Menschen singen,", "tokens": ["Ich", "will's", "den", "Men\u00b7schen", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie man Dich, H\u00f6chster, preist!", "tokens": ["Wie", "man", "Dich", ",", "H\u00f6chs\u00b7ter", ",", "preist", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "$,", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ich will's der Menschheit singen,", "tokens": ["Ich", "will's", "der", "Menschheit", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da\u00df Du die Welten lenkst,", "tokens": ["Da\u00df", "Du", "die", "Wel\u00b7ten", "lenkst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Du das Licht erschaffen,", "tokens": ["Da\u00df", "Du", "das", "Licht", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df Du die Meere tr\u00e4nkst.", "tokens": ["Da\u00df", "Du", "die", "Mee\u00b7re", "tr\u00e4nkst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df Du im tiefsten Abgrund", "tokens": ["Da\u00df", "Du", "im", "tiefs\u00b7ten", "Ab\u00b7grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das kleinste Wesen n\u00e4hrst,", "tokens": ["Das", "kleins\u00b7te", "We\u00b7sen", "n\u00e4hrst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Du vom tiefsten Kerker", "tokens": ["Da\u00df", "Du", "vom", "tiefs\u00b7ten", "Ker\u00b7ker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den stillsten Seufzer h\u00f6rst!", "tokens": ["Den", "stills\u00b7ten", "Seuf\u00b7zer", "h\u00f6rst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df Du mit Deiner Gr\u00f6\u00dfe", "tokens": ["Da\u00df", "Du", "mit", "Dei\u00b7ner", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sonnen hast geschm\u00fcckt,", "tokens": ["Die", "Son\u00b7nen", "hast", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch auch das kleinste Bl\u00fcmlein", "tokens": ["Doch", "auch", "das", "kleins\u00b7te", "Bl\u00fcm\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Deine Brust gedr\u00fcckt;", "tokens": ["An", "Dei\u00b7ne", "Brust", "ge\u00b7dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Bevor Du es erschaffen,", "tokens": ["Be\u00b7vor", "Du", "es", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bevor Du uns es gibst,", "tokens": ["Be\u00b7vor", "Du", "uns", "es", "gibst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimmst Du die kleine Bl\u00fcte", "tokens": ["Nimmst", "Du", "die", "klei\u00b7ne", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und z\u00e4rtlich Du sie liebst!", "tokens": ["Und", "z\u00e4rt\u00b7lich", "Du", "sie", "liebst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du gibst ihr Glanz und Leben,", "tokens": ["Du", "gibst", "ihr", "Glanz", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du machst sie zart und sch\u00f6n,", "tokens": ["Du", "machst", "sie", "zart", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du gibst ihr Licht und Sonne,", "tokens": ["Du", "gibst", "ihr", "Licht", "und", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und l\u00e4\u00dft sie Sonnen seh'n.", "tokens": ["Und", "l\u00e4\u00dft", "sie", "Son\u00b7nen", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "PPER", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da\u00df Du die blauen Himmel,", "tokens": ["Da\u00df", "Du", "die", "blau\u00b7en", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die goldnen Sterne schufst,", "tokens": ["Die", "gold\u00b7nen", "Ster\u00b7ne", "schufst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Du mit Deiner Stimme", "tokens": ["Da\u00df", "Du", "mit", "Dei\u00b7ner", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Berge Echo rufst:", "tokens": ["Der", "Ber\u00b7ge", "E\u00b7cho", "rufst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Damit man endlich wisse,", "tokens": ["Da\u00b7mit", "man", "end\u00b7lich", "wis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df jeder Laut Dir kund,", "tokens": ["Da\u00df", "je\u00b7der", "Laut", "Dir", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "A