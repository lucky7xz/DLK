{"dta.poem.12269": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wassersnoth .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Koblenz auf der Br\u00fccken               ", "tokens": ["Zu", "Kob\u00b7lenz", "auf", "der", "Br\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da lag ein tiefer Schnee,", "tokens": ["Da", "lag", "ein", "tie\u00b7fer", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Schnee der ist verschmolzen,", "tokens": ["Der", "Schnee", "der", "ist", "ver\u00b7schmol\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Wasser flie\u00dft in See.", "tokens": ["Das", "Was\u00b7ser", "flie\u00dft", "in", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es flie\u00dft in Liebchens Garten,", "tokens": ["Es", "flie\u00dft", "in", "Lieb\u00b7chens", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da wohnet niemand drein,", "tokens": ["Da", "woh\u00b7net", "nie\u00b7mand", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kann da lange warten,", "tokens": ["Ich", "kann", "da", "lan\u00b7ge", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es wehn zwey B\u00e4umelein.", "tokens": ["Es", "wehn", "zwey", "B\u00e4u\u00b7me\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die sehen mit den Kronen", "tokens": ["Die", "se\u00b7hen", "mit", "den", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch aus dem Wasser gr\u00fcn,", "tokens": ["Noch", "aus", "dem", "Was\u00b7ser", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mein Liebchen mu\u00df drin wohnen,", "tokens": ["Mein", "Lieb\u00b7chen", "mu\u00df", "drin", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich kann nicht zu ihr hin.", "tokens": ["Ich", "kann", "nicht", "zu", "ihr", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn Gott mich freundlich gr\u00fc\u00dfet", "tokens": ["Wenn", "Gott", "mich", "freund\u00b7lich", "gr\u00fc\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus blauer Luft und Thal,", "tokens": ["Aus", "blau\u00b7er", "Luft", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus diesem Flusse gr\u00fc\u00dfet,", "tokens": ["Aus", "die\u00b7sem", "Flus\u00b7se", "gr\u00fc\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Liebchen mich zumal.", "tokens": ["Mein", "Lieb\u00b7chen", "mich", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie geht nicht auf der Br\u00fccken,", "tokens": ["Sie", "geht", "nicht", "auf", "der", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da gehn viel sch\u00f6ne Fraun,", "tokens": ["Da", "gehn", "viel", "sch\u00f6\u00b7ne", "Fraun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie thun mich viel anblicken,", "tokens": ["Sie", "thun", "mich", "viel", "an\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich mag die nicht anschaun.", "tokens": ["Ich", "mag", "die", "nicht", "an\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}