{"dta.poem.23513": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Stimme aus der W\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stark sei dein Muth und rein dein Herz!", "tokens": ["Stark", "sei", "dein", "Muth", "und", "rein", "dein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und t\u00f6nt's auf allen Seiten:", "tokens": ["Und", "t\u00f6nt's", "auf", "al\u00b7len", "Sei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die schlimme Zeit! die b\u00f6se Welt!", "tokens": ["Die", "schlim\u00b7me", "Zeit", "!", "die", "b\u00f6\u00b7se", "Welt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du wagst dich frisch hinaus ins Feld,", "tokens": ["Du", "wagst", "dich", "frisch", "hin\u00b7aus", "ins", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Schlechte zu bestreiten.", "tokens": ["Das", "Schlech\u00b7te", "zu", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Rein sei dein Herz und stark dein Muth!", "tokens": ["Rein", "sei", "dein", "Herz", "und", "stark", "dein", "Muth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann bist du wohl gebettet.", "tokens": ["Dann", "bist", "du", "wohl", "ge\u00b7bet\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und setzte dich der Menschen Neid", "tokens": ["Und", "setz\u00b7te", "dich", "der", "Men\u00b7schen", "Neid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinaus in Wind- und Wellenstreit \u2014", "tokens": ["Hin\u00b7aus", "in", "Win\u00b7d", "und", "Wel\u00b7len\u00b7streit"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "TRUNC", "KON", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Auch Moses ward errettet!", "tokens": ["Auch", "Mo\u00b7ses", "ward", "er\u00b7ret\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nicht unsre Zeit sei ", "tokens": ["Nicht", "uns\u00b7re", "Zeit", "sei"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Rein sei dein Herz und stark dein Muth,", "tokens": ["Rein", "sei", "dein", "Herz", "und", "stark", "dein", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Gottes Lieb' auch Wunder thut", "tokens": ["Da\u00df", "Got\u00b7tes", "Lieb'", "auch", "Wun\u00b7der", "thut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An deinem Osterfeste!", "tokens": ["An", "dei\u00b7nem", "Os\u00b7ter\u00b7fes\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}