{"dta.poem.11758": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt, geboren hat die Viper nicht die Jungen,", "tokens": ["Man", "sagt", ",", "ge\u00b7bo\u00b7ren", "hat", "die", "Vi\u00b7per", "nicht", "die", "Jun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVPP", "VAFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Mutter t\u00f6dtend sind sie ihrem Leib entsprungen.", "tokens": ["Die", "Mut\u00b7ter", "t\u00f6d\u00b7tend", "sind", "sie", "ih\u00b7rem", "Leib", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Man sagt, sie thuen dies auf ein Naturgebot,", "tokens": ["Man", "sagt", ",", "sie", "thu\u00b7en", "dies", "auf", "ein", "Na\u00b7tur\u00b7ge\u00b7bot", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VVFIN", "PDS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An ihrer Mutter so r\u00e4chend des Vaters Tod.", "tokens": ["An", "ih\u00b7rer", "Mut\u00b7ter", "so", "r\u00e4\u00b7chend", "des", "Va\u00b7ters", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Denn wenn der Schlangenmann sein Weib will z\u00fcngelnd k\u00fcssen,", "tokens": ["Denn", "wenn", "der", "Schlan\u00b7gen\u00b7mann", "sein", "Weib", "will", "z\u00fcn\u00b7gelnd", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nimmt in den Mund sie ihn und schwelgt in den Gen\u00fcssen.", "tokens": ["Nimmt", "in", "den", "Mund", "sie", "ihn", "und", "schwelgt", "in", "den", "Ge\u00b7n\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PPER", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und, obs die S\u00e4ttigung, obs ihr die Lust eingab,", "tokens": ["Und", ",", "obs", "die", "S\u00e4t\u00b7ti\u00b7gung", ",", "obs", "ihr", "die", "Lust", "ein\u00b7gab", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sie empfangen hat, bei\u00dft sie das Haupt ihm ab.", "tokens": ["Wie", "sie", "emp\u00b7fan\u00b7gen", "hat", ",", "bei\u00dft", "sie", "das", "Haupt", "ihm", "ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "PPER", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Kinder f\u00fchlen wol aus welcherlei Verderben", "tokens": ["Die", "Kin\u00b7der", "f\u00fch\u00b7len", "wol", "aus", "wel\u00b7cher\u00b7lei", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie stammen, und gehn hin den gleichen Tod zu sterben.", "tokens": ["Sie", "stam\u00b7men", ",", "und", "gehn", "hin", "den", "glei\u00b7chen", "Tod", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Die Schlangenm\u00e4nnchen gehn sich mit den Weibchen gatten,", "tokens": ["Die", "Schlan\u00b7gen\u00b7m\u00e4nn\u00b7chen", "gehn", "sich", "mit", "den", "Weib\u00b7chen", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um f\u00fcr der Mutter Tod die S\u00fchnung zu erstatten;", "tokens": ["Um", "f\u00fcr", "der", "Mut\u00b7ter", "Tod", "die", "S\u00fch\u00b7nung", "zu", "er\u00b7stat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Zu s\u00e4ttigen die Lust, die niemals kann ersatten;", "tokens": ["Zu", "s\u00e4t\u00b7ti\u00b7gen", "die", "Lust", ",", "die", "nie\u00b7mals", "kann", "er\u00b7sat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,", "PRELS", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Kann solche Unnatur in der Natur auch seyn?", "tokens": ["Kann", "sol\u00b7che", "Un\u00b7na\u00b7tur", "in", "der", "Na\u00b7tur", "auch", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "APPR", "ART", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tr\u00e4gst du, o Mensch, sie nur in die Natur hinein?", "tokens": ["Tr\u00e4gst", "du", ",", "o", "Mensch", ",", "sie", "nur", "in", "die", "Na\u00b7tur", "hin\u00b7ein", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der lautern Fantasie ist sie die Mutter mild,", "tokens": ["Der", "lau\u00b7tern", "Fan\u00b7ta\u00b7sie", "ist", "sie", "die", "Mut\u00b7ter", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und der verst\u00f6rten das verzerrte Schlangenbild.", "tokens": ["Und", "der", "ver\u00b7st\u00f6r\u00b7ten", "das", "ver\u00b7zerr\u00b7te", "Schlan\u00b7gen\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}