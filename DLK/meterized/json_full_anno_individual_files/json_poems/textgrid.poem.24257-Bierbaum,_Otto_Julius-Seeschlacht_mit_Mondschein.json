{"textgrid.poem.24257": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Seeschlacht mit Mondschein", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ba\u00dftief br\u00fcllen die Kanonen,", "tokens": ["Ba\u00df\u00b7tief", "br\u00fcl\u00b7len", "die", "Ka\u00b7no\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fistelnd zischen Torpedonen", "tokens": ["Fis\u00b7telnd", "zi\u00b7schen", "Tor\u00b7pe\u00b7do\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch des Meers bewegte Flut;", "tokens": ["Durch", "des", "Meers", "be\u00b7weg\u00b7te", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu Bellonas Orgelweisen", "tokens": ["Zu", "Bel\u00b7lo\u00b7nas", "Or\u00b7gel\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mu\u00df ins harte Seegras bei\u00dfen", "tokens": ["Mu\u00df", "ins", "har\u00b7te", "See\u00b7gras", "bei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPRART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manch ein Krieger hochgemut.", "tokens": ["Manch", "ein", "Krie\u00b7ger", "hoch\u00b7ge\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stahlgu\u00dfbomben, Stahlgu\u00dfplatten", "tokens": ["Stahl\u00b7gu\u00df\u00b7bom\u00b7ben", ",", "Stahl\u00b7gu\u00df\u00b7plat\u00b7ten"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieht man t\u00f6dlich sich begatten;", "tokens": ["Sieht", "man", "t\u00f6d\u00b7lich", "sich", "be\u00b7gat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "(was mit vielem L\u00e4rm geschieht,", "tokens": ["(", "was", "mit", "vie\u00b7lem", "L\u00e4rm", "ge\u00b7schieht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "PIS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Weil bei diesem Kopulieren", "tokens": ["Weil", "bei", "die\u00b7sem", "Ko\u00b7pu\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Als Trauzeugen assistieren", "tokens": ["Als", "Trau\u00b7zeu\u00b7gen", "as\u00b7sis\u00b7tie\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Dynamit und Melinit.)", "tokens": ["Dy\u00b7na\u00b7mit", "und", "Me\u00b7li\u00b7nit", ".", ")"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kessel platzen, Schiffe sinken,", "tokens": ["Kes\u00b7sel", "plat\u00b7zen", ",", "Schif\u00b7fe", "sin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niedertr\u00e4chtge Gase stinken,", "tokens": ["Nie\u00b7der\u00b7tr\u00e4cht\u00b7ge", "Ga\u00b7se", "stin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pulverdampf bedeckt das Meer,", "tokens": ["Pul\u00b7ver\u00b7dampf", "be\u00b7deckt", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Abgeri\u00dfne Arm und Beine", "tokens": ["Ab\u00b7ge\u00b7ri\u00df\u00b7ne", "Arm", "und", "Bei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwimmen still im Mondenscheine", "tokens": ["Schwim\u00b7men", "still", "im", "Mon\u00b7den\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf der salzgen Flut umher.", "tokens": ["Auf", "der", "salz\u00b7gen", "Flut", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und der biedre Vollmond zwinkert,", "tokens": ["Und", "der", "bied\u00b7re", "Voll\u00b7mond", "zwin\u00b7kert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.2": {"text": "Da\u00df es auf den Wellen blinkert,", "tokens": ["Da\u00df", "es", "auf", "den", "Wel\u00b7len", "blin\u00b7kert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er spricht: \u00bbDas ist gewi\u00df:", "tokens": ["Und", "er", "spricht", ":", "\u00bb", "Das", "ist", "ge\u00b7wi\u00df", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "PDS", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der hohen Kunst, zu morden,", "tokens": ["In", "der", "ho\u00b7hen", "Kunst", ",", "zu", "mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind geschickter sie geworden", "tokens": ["Sind", "ge\u00b7schick\u00b7ter", "sie", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "PPER", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seit der Schlacht bei Salamis.", "tokens": ["Seit", "der", "Schlacht", "bei", "Sa\u00b7la\u00b7mis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seit in seinen Mu\u00dfestunden", "tokens": ["Seit", "in", "sei\u00b7nen", "Mu\u00b7\u00dfe\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener M\u00f6nch die Kraft erfunden,", "tokens": ["Je\u00b7ner", "M\u00f6nch", "die", "Kraft", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die den Tod von weitem speit,", "tokens": ["Die", "den", "Tod", "von", "wei\u00b7tem", "speit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Brachten sies, das mu\u00df man sagen,", "tokens": ["Brach\u00b7ten", "sies", ",", "das", "mu\u00df", "man", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PDS", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Kunst, sich totzuschlagen,", "tokens": ["In", "der", "Kunst", ",", "sich", "tot\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wirklich ganz erstaunlich weit.", "tokens": ["Wirk\u00b7lich", "ganz", "er\u00b7staun\u00b7lich", "weit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Selbst die Mongolomalaien", "tokens": ["Selbst", "die", "Mon\u00b7go\u00b7lo\u00b7ma\u00b7lai\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben das Verderbenspeien", "tokens": ["Ha\u00b7ben", "das", "Ver\u00b7der\u00b7ben\u00b7spei\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den Europ\u00e4ern abgeguckt, \u2013", "tokens": ["Den", "Eu\u00b7ro\u00b7p\u00e4\u00b7ern", "ab\u00b7ge\u00b7guckt", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was gewi\u00df durchaus kein kleines,", "tokens": ["Was", "ge\u00b7wi\u00df", "durc\u00b7haus", "kein", "klei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein vielmehr ein ungemeines", "tokens": ["Nein", "viel\u00b7mehr", "ein", "un\u00b7ge\u00b7mei\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "ADJA"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Zivilisationsprodukt.", "tokens": ["Zi\u00b7vi\u00b7li\u00b7sa\u00b7ti\u00b7ons\u00b7pro\u00b7dukt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sollte mans f\u00fcr m\u00f6glich halten?", "tokens": ["Soll\u00b7te", "mans", "f\u00fcr", "m\u00f6g\u00b7lich", "hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die in nichts f\u00fcr Meister galten,", "tokens": ["Die", "in", "nichts", "f\u00fcr", "Meis\u00b7ter", "gal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als der Kunst geschliffenen Lacks,", "tokens": ["Als", "der", "Kunst", "ge\u00b7schlif\u00b7fe\u00b7nen", "Lacks", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Machten schon, wie ungeschliffen!", "tokens": ["Mach\u00b7ten", "schon", ",", "wie", "un\u00b7ge\u00b7schlif\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus armierten Russenschiffen", "tokens": ["Aus", "ar\u00b7mier\u00b7ten", "Rus\u00b7sen\u00b7schif\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "V\u00f6llig desarmierte Wracks.", "tokens": ["V\u00f6l\u00b7lig", "de\u00b7sar\u00b7mier\u00b7te", "Wracks", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Und sie schleudern Zuckerh\u00fcte", "tokens": ["Und", "sie", "schleu\u00b7dern", "Zu\u00b7cker\u00b7h\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von nicht mindrer Kraft und G\u00fcte,", "tokens": ["Von", "nicht", "mind\u00b7rer", "Kraft", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKNEG", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als der Russe schleudert; ja", "tokens": ["Als", "der", "Rus\u00b7se", "schleu\u00b7dert", ";", "ja"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$.", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Torpedoman\u00f6vrieren", "tokens": ["Im", "Tor\u00b7pe\u00b7do\u00b7ma\u00b7n\u00f6v\u00b7rie\u00b7ren"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Scheinen sie zu exzellieren,", "tokens": ["Schei\u00b7nen", "sie", "zu", "ex\u00b7zel\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ich selbst es noch nicht sah.", "tokens": ["Wie", "ich", "selbst", "es", "noch", "nicht", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Intressant, mu\u00df ich gestehen,", "tokens": ["I\u00b7ntres\u00b7sant", ",", "mu\u00df", "ich", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist es mir, das anzusehen,", "tokens": ["Ist", "es", "mir", ",", "das", "an\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "PRELS", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der ich doch sonst ", "tokens": ["Der", "ich", "doch", "sonst"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Schlie\u00dflich ", "tokens": ["Schlie\u00df\u00b7lich"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und das Reich der Wuttkianer", "tokens": ["Und", "das", "Reich", "der", "Wutt\u00b7ki\u00b7a\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird von ", "tokens": ["Wird", "von"], "token_info": ["word", "word"], "pos": ["VAFIN", "APPR"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "Welche hohe, weite, tiefe", "tokens": ["Wel\u00b7che", "ho\u00b7he", ",", "wei\u00b7te", ",", "tie\u00b7fe"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PWAT", "ADJA", "$,", "VVFIN", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wundersame Perspektive:", "tokens": ["Wun\u00b7der\u00b7sa\u00b7me", "Pers\u00b7pek\u00b7ti\u00b7ve", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Jenen knutenfrommen, biedern", "tokens": ["Je\u00b7nen", "knu\u00b7ten\u00b7from\u00b7men", ",", "bie\u00b7dern"], "token_info": ["word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und bescheidenen Moskowitern", "tokens": ["Und", "be\u00b7schei\u00b7de\u00b7nen", "Mos\u00b7ko\u00b7wi\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Das ertr\u00e4umte Parlament.\u00ab", "tokens": ["Das", "er\u00b7tr\u00e4um\u00b7te", "Par\u00b7la\u00b7ment", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Also sprach der Mond. Da krachte,", "tokens": ["Al\u00b7so", "sprach", "der", "Mond", ".", "Da", "krach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bum, ein Schu\u00df, und sachte, sachte", "tokens": ["Bum", ",", "ein", "Schu\u00df", ",", "und", "sach\u00b7te", ",", "sach\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos":