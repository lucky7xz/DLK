], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und immer mehr noch hungert ihn.", "tokens": ["Und", "im\u00b7mer", "mehr", "noch", "hun\u00b7gert", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er geht vom Ufer ab, und watet in den Bach!", "tokens": ["Er", "geht", "vom", "U\u00b7fer", "ab", ",", "und", "wa\u00b7tet", "in", "den", "Bach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gr\u00fcndlinge trifft er an; fragt aber nichts darnach;", "tokens": ["Gr\u00fcnd\u00b7lin\u00b7ge", "trifft", "er", "an", ";", "fragt", "a\u00b7ber", "nichts", "dar\u00b7nach", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "ADV", "PIS", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er l\u00e4\u00dft sie all' im Frieden schwimmen, spricht:", "tokens": ["Er", "l\u00e4\u00dft", "sie", "all'", "im", "Frie\u00b7den", "schwim\u00b7men", ",", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPRART", "NN", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Gr\u00fcdlinge fressen Reiger nicht;", "tokens": ["Gr\u00fcd\u00b7lin\u00b7ge", "fres\u00b7sen", "Rei\u00b7ger", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Nach ihnen nur einmal den Schnabel aufzuthun,", "tokens": ["Nach", "ih\u00b7nen", "nur", "ein\u00b7mal", "den", "Schna\u00b7bel", "auf\u00b7zu\u00b7thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das w\u00e4re gro\u00dfer Schimpf f\u00fcr einen Leckermund!", "tokens": ["Das", "w\u00e4\u00b7re", "gro\u00b7\u00dfer", "Schimpf", "f\u00fcr", "ei\u00b7nen", "Le\u00b7cker\u00b7mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er sagt's; indessen geht, was Fisch ist, auf den Grund;", "tokens": ["Er", "sagt's", ";", "in\u00b7des\u00b7sen", "geht", ",", "was", "Fisch", "ist", ",", "auf", "den", "Grund", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nicht einer la\u00dft sich sehn! Ei, Leckermund, wie nun?", "tokens": ["Nicht", "ei\u00b7ner", "la\u00dft", "sich", "sehn", "!", "Ei", ",", "Le\u00b7cker\u00b7mund", ",", "wie", "nun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VVFIN", "PRF", "VVINF", "$.", "NN", "$,", "NN", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Er immer hungriger, steht da mit Ungeduld,", "tokens": ["Er", "im\u00b7mer", "hung\u00b7ri\u00b7ger", ",", "steht", "da", "mit", "Un\u00b7ge\u00b7duld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "$,", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sieht sich um, und sucht, und sucht bis um Erm\u00fcden.", "tokens": ["Und", "sieht", "sich", "um", ",", "und", "sucht", ",", "und", "sucht", "bis", "um", "Er\u00b7m\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Karpfe kommt, kein Hecht! Er giebt sich selbst die Schuld,", "tokens": ["Kein", "Karp\u00b7fe", "kommt", ",", "kein", "Hecht", "!", "Er", "giebt", "sich", "selbst", "die", "Schuld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "$.", "PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ist mit einem Frosch zufrieden!", "tokens": ["Und", "ist", "mit", "ei\u00b7nem", "Frosch", "zu\u00b7frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}