{"textgrid.poem.36549": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "22. Der Fischreiger", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Ufer eines Bachs, auf einer Wiese, ging", "tokens": ["Am", "U\u00b7fer", "ei\u00b7nes", "Bachs", ",", "auf", "ei\u00b7ner", "Wie\u00b7se", ",", "ging"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein alter Reiger, still, auf langen d\u00fcrren Beinen,", "tokens": ["Ein", "al\u00b7ter", "Rei\u00b7ger", ",", "still", ",", "auf", "lan\u00b7gen", "d\u00fcr\u00b7ren", "Bei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$,", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit langem Hals, an dem ein langer Schnabel hing.", "tokens": ["Mit", "lan\u00b7gem", "Hals", ",", "an", "dem", "ein", "lan\u00b7ger", "Schna\u00b7bel", "hing", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Bachs Gew\u00e4sser flo\u00df auf harten Kieselsteinen,", "tokens": ["Des", "Bachs", "Ge\u00b7w\u00e4s\u00b7ser", "flo\u00df", "auf", "har\u00b7ten", "Kie\u00b7sel\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bergab, mit angenehmem Schall,", "tokens": ["Berg\u00b7ab", ",", "mit", "an\u00b7ge\u00b7neh\u00b7mem", "Schall", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durchsichtig wie Krystall,", "tokens": ["Durch\u00b7sich\u00b7tig", "wie", "Krys\u00b7tall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und stand dann wieder tief. Vom Himmel ohne Wolke", "tokens": ["Und", "stand", "dann", "wie\u00b7der", "tief", ".", "Vom", "Him\u00b7mel", "oh\u00b7ne", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "$.", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Fiel warmer Sonnenstrahl", "tokens": ["Fiel", "war\u00b7mer", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Auf seine Fl\u00e4che, drang zum kalten Wasservolke;", "tokens": ["Auf", "sei\u00b7ne", "Fl\u00e4\u00b7che", ",", "drang", "zum", "kal\u00b7ten", "Was\u00b7ser\u00b7vol\u00b7ke", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Lockt es herauf, in Haufen ohne Zahl;", "tokens": ["Lockt", "es", "her\u00b7auf", ",", "in", "Hau\u00b7fen", "oh\u00b7ne", "Zahl", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Es letzte sich, war guter Dinge;", "tokens": ["Es", "letz\u00b7te", "sich", ",", "war", "gu\u00b7ter", "Din\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Stieg auf und machte frohe Spr\u00fcnge", "tokens": ["Stieg", "auf", "und", "mach\u00b7te", "fro\u00b7he", "Spr\u00fcn\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Am warmen Sonnenstrahl!", "tokens": ["Am", "war\u00b7men", "Son\u00b7nen\u00b7strahl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herr Reiger, wie so faul? Schnappst du denn nicht einmal", "tokens": ["Herr", "Rei\u00b7ger", ",", "wie", "so", "faul", "?", "Schnappst", "du", "denn", "nicht", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PWAV", "ADV", "ADJD", "$.", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit deinem langen Schnabel zu,", "tokens": ["Mit", "dei\u00b7nem", "lan\u00b7gen", "Schna\u00b7bel", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu holen einen Hecht? Du Fauler, wartest du", "tokens": ["Zu", "ho\u00b7len", "ei\u00b7nen", "Hecht", "?", "Du", "Fau\u00b7ler", ",", "war\u00b7test", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$.", "PPER", "NN", "$,", "VAFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf einen Karpfen? Ei! wie wird es dich gereu'n!", "tokens": ["Auf", "ei\u00b7nen", "Karp\u00b7fen", "?", "Ei", "!", "wie", "wird", "es", "dich", "ge\u00b7reu'n", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "NN", "$.", "PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn du wirst schnappen wollen, wird kein Hecht mehr sein!", "tokens": ["Wenn", "du", "wirst", "schnap\u00b7pen", "wol\u00b7len", ",", "wird", "kein", "Hecht", "mehr", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVINF", "VMFIN", "$,", "VAFIN", "PIAT", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie ernsthaft! wie so still!", "tokens": ["Wie", "ernst\u00b7haft", "!", "wie", "so", "still", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$.", "PWAV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie dreht er seinen Hals, den er nicht brauchen will!", "tokens": ["Wie", "dreht", "er", "sei\u00b7nen", "Hals", ",", "den", "er", "nicht", "brau\u00b7chen", "will", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein kluger Freund von M\u00e4\u00dfigkeit,", "tokens": ["Ein", "klu\u00b7ger", "Freund", "von", "M\u00e4\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Besinnt er sich: Es ist noch Zeit!", "tokens": ["Be\u00b7sinnt", "er", "sich", ":", "Es", "ist", "noch", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stets essen, ist gemeiner V\u00f6gel Weise!", "tokens": ["Stets", "es\u00b7sen", ",", "ist", "ge\u00b7mei\u00b7ner", "V\u00f6\u00b7gel", "Wei\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "VAFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bald aber hungert ihn, und nun sieht er sich um,", "tokens": ["Bald", "a\u00b7ber", "hun\u00b7gert", "ihn", ",", "und", "nun", "sieht", "er", "sich", "um", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "KON", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Nach Karpfen oder Hecht;", "tokens": ["Nach", "Karp\u00b7fen", "o\u00b7der", "Hecht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Allein, verschwunden ist das ganze Fischgeschlecht;", "tokens": ["Al\u00b7lein", ",", "ver\u00b7schwun\u00b7den", "ist", "das", "gan\u00b7ze", "Fischge\u00b7schlecht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nur Schleie schwimmen noch; er aber ist nicht dumm,", "tokens": ["Nur", "Schlei\u00b7e", "schwim\u00b7men", "noch", ";", "er", "a\u00b7ber", "ist", "nicht", "dumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADV", "$.", "PPER", "ADV", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er hat Geschmack! Schlei w\u00e4re schlechte Speise,", "tokens": ["Er", "hat", "Ge\u00b7schmack", "!", "Schlei", "w\u00e4\u00b7re", "schlech\u00b7te", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "F\u00fcr einen Reiger! alle l\u00e4\u00dft er ziehn!", "tokens": ["F\u00fcr", "ei\u00b7nen", "Rei\u00b7ger", "!", "al\u00b7le", "l\u00e4\u00dft", "er", "ziehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PIS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und immer mehr noch hungert ihn.", "tokens": ["Und", "im\u00b7mer", "mehr", "noch", "hun\u00b7gert", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er geht vom Ufer ab, und watet in den Bach!", "tokens": ["Er", "geht", "vom", "U\u00b7fer", "ab", ",", "und", "wa\u00b7tet", "in", "den", "Bach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gr\u00fcndlinge trifft er an; fragt aber nichts darnach;", "tokens": ["Gr\u00fcnd\u00b7lin\u00b7ge", "trifft", "er", "an", ";", "fragt", "a\u00b7ber", "nichts", "dar\u00b7nach", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$.", "VVFIN", "ADV", "PIS", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er l\u00e4\u00dft sie all' im Frieden schwimmen, spricht:", "tokens": ["Er", "l\u00e4\u00dft", "sie", "all'", "im", "Frie\u00b7den", "schwim\u00b7men", ",", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPRART", "NN", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Gr\u00fcdlinge fressen Reiger nicht;", "tokens": ["Gr\u00fcd\u00b7lin\u00b7ge", "fres\u00b7sen", "Rei\u00b7ger", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Nach ihnen nur einmal den Schnabel aufzuthun,", "tokens": ["Nach", "ih\u00b7nen", "nur", "ein\u00b7mal", "den", "Schna\u00b7bel", "auf\u00b7zu\u00b7thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das w\u00e4re gro\u00dfer Schimpf f\u00fcr einen Leckermund!", "tokens": ["Das", "w\u00e4\u00b7re", "gro\u00b7\u00dfer", "Schimpf", "f\u00fcr", "ei\u00b7nen", "Le\u00b7cker\u00b7mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er sagt's; indessen geht, was Fisch ist, auf den Grund;", "tokens": ["Er", "sagt's", ";", "in\u00b7des\u00b7sen", "geht", ",", "was", "Fisch", "ist", ",", "auf", "den", "Grund", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nicht einer la\u00dft sich sehn! Ei, Leckermund, wie nun?", "tokens": ["Nicht", "ei\u00b7ner", "la\u00dft", "sich", "sehn", "!", "Ei", ",", "Le\u00b7cker\u00b7mund", ",", "wie", "nun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VVFIN", "PRF", "VVINF", "$.", "NN", "$,", "NN", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er immer hungriger, steht da mit Ungeduld,", "tokens": ["Er", "im\u00b7mer", "hung\u00b7ri\u00b7ger", ",", "steht", "da", "mit", "Un\u00b7ge\u00b7duld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "$,", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sieht sich um, und sucht, und sucht bis um Erm\u00fcden.", "tokens": ["Und", "sieht", "sich", "um", ",", "und", "sucht", ",", "und", "sucht", "bis", "um", "Er\u00b7m\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Karpfe kommt, kein Hecht! Er giebt sich selbst die Schuld,", "tokens": ["Kein", "Karp\u00b7fe", "kommt", ",", "kein", "Hecht", "!", "Er", "giebt", "sich", "selbst", "die", "Schuld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "$.", "PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ist mit einem Frosch zufrieden!", "tokens": ["Und", "ist", "mit", "ei\u00b7nem", "Frosch", "zu\u00b7frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Am Ufer eines Bachs, auf einer Wiese, ging", "tokens": ["Am", "U\u00b7fer", "ei\u00b7nes", "Bachs", ",", "auf", "ei\u00b7ner", "Wie\u00b7se", ",", "ging"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein alter Reiger, still, auf langen d\u00fcrren Beinen,", "tokens": ["Ein", "al\u00b7ter", "Rei\u00b7ger", ",", "still", ",", "auf", "lan\u00b7gen", "d\u00fcr\u00b7ren", "Bei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$,", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit langem Hals, an dem ein langer Schnabel hing.", "tokens": ["Mit", "lan\u00b7gem", "Hals", ",", "an", "dem", "ein", 