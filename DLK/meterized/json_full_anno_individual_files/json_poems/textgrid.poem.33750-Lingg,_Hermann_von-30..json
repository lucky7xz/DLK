{"textgrid.poem.33750": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat nicht auch dich der Irrtum m\u00fcdgepeinigt,", "tokens": ["Hat", "nicht", "auch", "dich", "der", "Irr\u00b7tum", "m\u00fcd\u00b7ge\u00b7pei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat nicht auch dich der Himmel l\u00e4ngst verlassen,", "tokens": ["Hat", "nicht", "auch", "dich", "der", "Him\u00b7mel", "l\u00e4ngst", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stehst nicht auch du allein, umringt vom Hassen,", "tokens": ["Stehst", "nicht", "auch", "du", "al\u00b7lein", ",", "um\u00b7ringt", "vom", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "PPER", "ADV", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Hohn der Welt getreten und gesteinigt?", "tokens": ["Vom", "Hohn", "der", "Welt", "ge\u00b7tre\u00b7ten", "und", "ge\u00b7stei\u00b7nigt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von Trug sind unsre Seelen nun gereinigt,", "tokens": ["Von", "Trug", "sind", "uns\u00b7re", "See\u00b7len", "nun", "ge\u00b7rei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir sehen jeden Wahn vor uns erblassen;", "tokens": ["Wir", "se\u00b7hen", "je\u00b7den", "Wahn", "vor", "uns", "er\u00b7blas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Wellen sich im Wasserfall umfassen,", "tokens": ["Wie", "Wel\u00b7len", "sich", "im", "Was\u00b7ser\u00b7fall", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00df in den Tod uns gehen, frei, vereinigt!", "tokens": ["La\u00df", "in", "den", "Tod", "uns", "ge\u00b7hen", ",", "frei", ",", "ver\u00b7ei\u00b7nigt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "PPER", "VVINF", "$,", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was hoffst du noch? Die R\u00f6te deiner Wangen", "tokens": ["Was", "hoffst", "du", "noch", "?", "Die", "R\u00f6\u00b7te", "dei\u00b7ner", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist Fieberglut, und nur noch Fiebergluten", "tokens": ["Ist", "Fie\u00b7berg\u00b7lut", ",", "und", "nur", "noch", "Fie\u00b7berg\u00b7lu\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "KON", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "N\u00e4hrt unser Hoffen, Streben und Verlangen.", "tokens": ["N\u00e4hrt", "un\u00b7ser", "Hof\u00b7fen", ",", "Stre\u00b7ben", "und", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was hoffst du noch? Ein langsam still Verbluten?", "tokens": ["Was", "hoffst", "du", "noch", "?", "Ein", "lang\u00b7sam", "still", "Ver\u00b7blu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O komm! ein d\u00fcstrer Tag ist untergangen \u2013", "tokens": ["O", "komm", "!", "ein", "d\u00fcst\u00b7rer", "Tag", "ist", "un\u00b7ter\u00b7gan\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie friedlich liegt die Nacht auf jenen Fluten!", "tokens": ["Wie", "fried\u00b7lich", "liegt", "die", "Nacht", "auf", "je\u00b7nen", "Flu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hat nicht auch dich der Irrtum m\u00fcdgepeinigt,", "tokens": ["Hat", "nicht", "auch", "dich", "der", "Irr\u00b7tum", "m\u00fcd\u00b7ge\u00b7pei\u00b7nigt", ","], "token_info": ["word", "word", "word