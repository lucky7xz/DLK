{"dta.poem.7143": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So wie die C\u00f6rper C\u00f6rper zeugen;", "tokens": ["So", "wie", "die", "C\u00f6r\u00b7per", "C\u00f6r\u00b7per", "zeu\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So ist es unsern Seelen eigen,", "tokens": ["So", "ist", "es", "un\u00b7sern", "See\u00b7len", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gedancken aus sich selbst hervor zu bringen:", "tokens": ["Ge\u00b7dan\u00b7cken", "aus", "sich", "selbst", "her\u00b7vor", "zu", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADV", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Kinder nun, die aus der Seel\u2019 entspringen,", "tokens": ["Die", "Kin\u00b7der", "nun", ",", "die", "aus", "der", "Seel'", "ent\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sind auch im Traum so gar ein Theil von unsrer Seelen,", "tokens": ["Sind", "auch", "im", "Traum", "so", "gar", "ein", "Theil", "von", "uns\u00b7rer", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie man ja dieses deutlich sp\u00fcrt,", "tokens": ["Wie", "man", "ja", "die\u00b7ses", "deut\u00b7lich", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PDAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wird unsre Seele nicht in Tr\u00e4umen starck ger\u00fchrt,", "tokens": ["Wird", "uns\u00b7re", "See\u00b7le", "nicht", "in", "Tr\u00e4u\u00b7men", "starck", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verursacht ihr ein Traum nicht ein empfindlichs Qu\u00e4len?", "tokens": ["Ver\u00b7ur\u00b7sacht", "ihr", "ein", "Traum", "nicht", "ein", "emp\u00b7find\u00b7lichs", "Qu\u00e4\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wird sie nicht auch gar offt in Traum erg\u00f6tzt?", "tokens": ["Wird", "sie", "nicht", "auch", "gar", "offt", "in", "Traum", "er\u00b7g\u00f6tzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Kann nicht ein Traum ihr Angst und Grauen, Furcht und", "tokens": ["Kann", "nicht", "ein", "Traum", "ihr", "Angst", "und", "Grau\u00b7en", ",", "Furcht", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "KON", "NN", "$,", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Schrecken,", "tokens": ["Schre\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Und solche Leidenschafft und Regungen erwecken,", "tokens": ["Und", "sol\u00b7che", "Lei\u00b7den\u00b7schafft", "und", "Re\u00b7gun\u00b7gen", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.13": {"text": "Da\u00df sie dadurch gantz aus sich selbst gesetzt,", "tokens": ["Da\u00df", "sie", "da\u00b7durch", "gantz", "aus", "sich", "selbst", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "ADV", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Das Blut in Adern treibt, Schwei\u00df aus dem C\u00f6rper pre\u00dft,", "tokens": ["Das", "Blut", "in", "A\u00b7dern", "treibt", ",", "Schwei\u00df", "aus", "dem", "C\u00f6r\u00b7per", "pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Und wahre Thr\u00e4nen fliessen l\u00e4sst.", "tokens": ["Und", "wah\u00b7re", "Thr\u00e4\u00b7nen", "flies\u00b7sen", "l\u00e4sst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}